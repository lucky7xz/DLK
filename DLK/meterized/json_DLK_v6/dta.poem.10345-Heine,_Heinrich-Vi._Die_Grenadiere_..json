{"dta.poem.10345": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Die Grenadiere .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Nach Frankreich zogen zwei Grenadier',", "tokens": ["Nach", "Fran\u00b7kreich", "zo\u00b7gen", "zwei", "Gre\u00b7na\u00b7dier'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "CARD", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die waren in Ru\u00dfland gefangen.", "tokens": ["Die", "wa\u00b7ren", "in", "Ru\u00df\u00b7land", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und als sie kamen in's deutsche Quartier,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "in's", "deut\u00b7sche", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie lie\u00dfen die K\u00f6pfe hangen.", "tokens": ["Sie", "lie\u00b7\u00dfen", "die", "K\u00f6p\u00b7fe", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da h\u00f6rten sie beide die traurige M\u00e4hr:", "tokens": ["Da", "h\u00f6r\u00b7ten", "sie", "bei\u00b7de", "die", "trau\u00b7ri\u00b7ge", "M\u00e4hr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da\u00df Frankreich verloren gegangen,", "tokens": ["Da\u00df", "Fran\u00b7kreich", "ver\u00b7lo\u00b7ren", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Besiegt und zerschlagen das tapfere Heer, \u2014", "tokens": ["Be\u00b7siegt", "und", "zer\u00b7schla\u00b7gen", "das", "tap\u00b7fe\u00b7re", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und der Kaiser, der Kaiser gefangen.", "tokens": ["Und", "der", "Kai\u00b7ser", ",", "der", "Kai\u00b7ser", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Da weinten zusammen die Grenadier'", "tokens": ["Da", "wein\u00b7ten", "zu\u00b7sam\u00b7men", "die", "Gre\u00b7na\u00b7dier'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wohl ob der kl\u00e4glichen Kunde.", "tokens": ["Wohl", "ob", "der", "kl\u00e4g\u00b7li\u00b7chen", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Eine sprach: Wie weh wird mir,", "tokens": ["Der", "Ei\u00b7ne", "sprach", ":", "Wie", "weh", "wird", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie brennt meine alte Wunde.", "tokens": ["Wie", "brennt", "mei\u00b7ne", "al\u00b7te", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Andre sprach: das Lied ist aus,", "tokens": ["Der", "And\u00b7re", "sprach", ":", "das", "Lied", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ich m\u00f6cht mit dir sterben,", "tokens": ["Auch", "ich", "m\u00f6cht", "mit", "dir", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Doch hab' ich Weib und Kind zu Haus,", "tokens": ["Doch", "hab'", "ich", "Weib", "und", "Kind", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ohne mich verderben.", "tokens": ["Die", "oh\u00b7ne", "mich", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was scheert mich Weib, was scheert mich Kind,", "tokens": ["Was", "scheert", "mich", "Weib", ",", "was", "scheert", "mich", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,", "PWS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trage weit bess'res Verlangen;", "tokens": ["Ich", "tra\u00b7ge", "weit", "bess'\u00b7res", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df sie betteln gehn, wenn sie hungrig sind, \u2014", "tokens": ["La\u00df", "sie", "bet\u00b7teln", "gehn", ",", "wenn", "sie", "hung\u00b7rig", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mein Kaiser, mein Kaiser gefangen!", "tokens": ["Mein", "Kai\u00b7ser", ",", "mein", "Kai\u00b7ser", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Gew\u00e4hr' mir Bruder eine Bitt':", "tokens": ["Ge\u00b7w\u00e4hr'", "mir", "Bru\u00b7der", "ei\u00b7ne", "Bitt'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich jetzt sterben werde,", "tokens": ["Wenn", "ich", "jetzt", "ster\u00b7ben", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So nimm meine Leiche nach Frankreich mit,", "tokens": ["So", "nimm", "mei\u00b7ne", "Lei\u00b7che", "nach", "Fran\u00b7kreich", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Begrab' mich in Frankreichs Erde.", "tokens": ["Be\u00b7grab'", "mich", "in", "Fran\u00b7kreichs", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Das Ehrenkreuz am rothen Band", "tokens": ["Das", "Eh\u00b7ren\u00b7kreuz", "am", "ro\u00b7then", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sollst du auf's Herz mir legen;", "tokens": ["Sollst", "du", "auf's", "Herz", "mir", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Flinte gieb mir in die Hand,", "tokens": ["Die", "Flin\u00b7te", "gieb", "mir", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00fcrt' mir um den Degen.", "tokens": ["Und", "g\u00fcrt'", "mir", "um", "den", "De\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So will ich liegen und horchen still,", "tokens": ["So", "will", "ich", "lie\u00b7gen", "und", "hor\u00b7chen", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie eine Schildwach, im Grabe,", "tokens": ["Wie", "ei\u00b7ne", "Schild\u00b7wach", ",", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis einst ich h\u00f6re Kanonengebr\u00fcll,", "tokens": ["Bis", "einst", "ich", "h\u00f6\u00b7re", "Ka\u00b7no\u00b7nen\u00b7ge\u00b7br\u00fcll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wiehernder Rosse Getrabe.", "tokens": ["Und", "wie\u00b7hern\u00b7der", "Ros\u00b7se", "Ge\u00b7tra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Dann reitet mein Kaiser wohl \u00fcber mein Grab,", "tokens": ["Dann", "rei\u00b7tet", "mein", "Kai\u00b7ser", "wohl", "\u00fc\u00b7ber", "mein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Viel Schwerter klirren und blitzen;", "tokens": ["Viel", "Schwer\u00b7ter", "klir\u00b7ren", "und", "blit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann steig' ich gewaffnet hervor aus dem Grab', \u2014", "tokens": ["Dann", "steig'", "ich", "ge\u00b7waff\u00b7net", "her\u00b7vor", "aus", "dem", "Grab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Kaiser, den Kaiser zu sch\u00fctzen.", "tokens": ["Den", "Kai\u00b7ser", ",", "den", "Kai\u00b7ser", "zu", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}