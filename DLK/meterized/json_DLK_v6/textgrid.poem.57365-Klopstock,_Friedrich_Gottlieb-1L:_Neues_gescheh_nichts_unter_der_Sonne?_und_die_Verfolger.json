{"textgrid.poem.57365": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Neues gescheh nichts unter der Sonne? und die Verfolger", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neues gescheh nichts unter der Sonne? und die Verfolger", "tokens": ["Neu\u00b7es", "ge\u00b7scheh", "nichts", "un\u00b7ter", "der", "Son\u00b7ne", "?", "und", "die", "Ver\u00b7fol\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$.", "KON", "ART", "NN"], "meter": "+--+-+--+-++-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Jener Freyheit, wie sie noch die Geschichte nicht kent,", "tokens": ["Je\u00b7ner", "Frey\u00b7heit", ",", "wie", "sie", "noch", "die", "Ge\u00b7schich\u00b7te", "nicht", "kent", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Feyren gleichwohl ein Siegesfest, dass die himmelgeborne", "tokens": ["Fey\u00b7ren", "gleich\u00b7wohl", "ein", "Sie\u00b7ges\u00b7fest", ",", "dass", "die", "him\u00b7mel\u00b7ge\u00b7bor\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "An der Kette, die sie sinnlos ihr ringten, verstumt,", "tokens": ["An", "der", "Ket\u00b7te", ",", "die", "sie", "sinn\u00b7los", "ihr", "ring\u00b7ten", ",", "ver\u00b7stumt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Singen, den Ton volksb\u00fchnisch, am Fest der Sansc\u00fclottiden,", "tokens": ["Sin\u00b7gen", ",", "den", "Ton", "volks\u00b7b\u00fch\u00b7nisch", ",", "am", "Fest", "der", "San\u00b7sc\u00fc\u00b7lot\u00b7ti\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJD", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+-+--", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Hottentottade: \u00bbU \u2013 amp Marat, wir beten dich an,", "tokens": ["Hot\u00b7ten\u00b7tot\u00b7ta\u00b7de", ":", "\u00bb", "U", "\u2013", "amp", "Ma\u00b7rat", ",", "wir", "be\u00b7ten", "dich", "an", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "XY", "$(", "FM.la", "FM.la", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Der du in dir die G\u00f6tter des siebenarmigen Stromes,", "tokens": ["Der", "du", "in", "dir", "die", "G\u00f6t\u00b7ter", "des", "sie\u00b7be\u00b7nar\u00b7mi\u00b7gen", "Stro\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-+-", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Diese der lehrenden Welt unsrer gelehrigen zeigst,", "tokens": ["Die\u00b7se", "der", "leh\u00b7ren\u00b7den", "Welt", "uns\u00b7rer", "ge\u00b7leh\u00b7ri\u00b7gen", "zeigst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Dich, dem Mirabau sank, und der sie alle noch wegstrahlt", "tokens": ["Dich", ",", "dem", "Mi\u00b7ra\u00b7bau", "sank", ",", "und", "der", "sie", "al\u00b7le", "noch", "weg\u00b7strahlt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PRELS", "PPER", "PIS", "ADV", "VVFIN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Aus dem Tempel, Nu \u2013 ap Marat! Marat Hir \u2013 op!", "tokens": ["Aus", "dem", "Tem\u00b7pel", ",", "Nu", "\u2013", "ap", "Ma\u00b7rat", "!", "Ma\u00b7rat", "Hir", "\u2013", "op", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "$(", "FM.la", "FM.la", "$.", "NE", "NE", "$(", "NE", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Pand\u00e4monion war der Tempel, eh, Marat, du einzogst;", "tokens": ["Pan\u00b7d\u00e4\u00b7mo\u00b7ni\u00b7on", "war", "der", "Tem\u00b7pel", ",", "eh", ",", "Ma\u00b7rat", ",", "du", "ein\u00b7zogst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KOUS", "$,", "NE", "$,", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+--+", "measure": "iambic.septa.invert"}, "line.12": {"text": "Aber du kamst! und er ward Pantheon, Marat Gha \u2013 ip!", "tokens": ["A\u00b7ber", "du", "kamst", "!", "und", "er", "ward", "Pan\u00b7the\u00b7on", ",", "Ma\u00b7rat", "Gha", "\u2013", "ip", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "KON", "PPER", "VAFIN", "NE", "$,", "NE", "NE", "$(", "NE", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.13": {"text": "Lebe die Klubbergmunizipalg\u00fcllotinoligokra-", "tokens": ["Le\u00b7be", "die", "Klub\u00b7berg\u00b7mu\u00b7ni\u00b7zi\u00b7pal\u00b7g\u00fcllo\u00b7ti\u00b7no\u00b7li\u00b7go\u00b7kra"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ART", "TRUNC"], "meter": "+--+-+-+-+-+--+", "measure": "iambic.septa.invert"}, "line.14": {"text": "Tierepublik! und Ghu \u2013 ip sch\u00fctz uns vor Hunger und Pest.\u00ab", "tokens": ["Tie\u00b7re\u00b7pub\u00b7lik", "!", "und", "Ghu", "\u2013", "ip", "sch\u00fctz", "uns", "vor", "Hun\u00b7ger", "und", "Pest", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "KON", "NE", "$(", "NE", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Auch Verw\u00fcnschungen sprechen sie aus; die Verw\u00fcnschenden br\u00fcllen:", "tokens": ["Auch", "Ver\u00b7w\u00fcn\u00b7schun\u00b7gen", "spre\u00b7chen", "sie", "aus", ";", "die", "Ver\u00b7w\u00fcn\u00b7schen\u00b7den", "br\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVINF", "$."], "meter": "--++-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "La Fayette! und ihr, Roland! la Rochefoucauld!", "tokens": ["La", "Fay\u00b7et\u00b7te", "!", "und", "ihr", ",", "Ro\u00b7land", "!", "la", "Ro\u00b7che\u00b7fou\u00b7cauld", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KON", "PPER", "$,", "NE", "$.", "FM", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Bailly! du von Etampes! Gesegnet sey uns, o Jourdan!", "tokens": ["Bail\u00b7ly", "!", "du", "von", "E\u00b7tam\u00b7pes", "!", "Ge\u00b7seg\u00b7net", "sey", "uns", ",", "o", "Jour\u00b7dan", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "APPR", "NE", "$.", "VVPP", "VAFIN", "PPER", "$,", "FM", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Sey dein Ronsin, und sey ... Aber mir sinket der Laut,", "tokens": ["Sey", "dein", "Ron\u00b7sin", ",", "und", "sey", "...", "A\u00b7ber", "mir", "sin\u00b7ket", "der", "Laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KON", "VAFIN", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Weigert sich fortzunennen. Wie viel, und welche Verbrechen", "tokens": ["Wei\u00b7gert", "sich", "fort\u00b7zu\u00b7nen\u00b7nen", ".", "Wie", "viel", ",", "und", "wel\u00b7che", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "VVINF", "$.", "PWAV", "PIS", "$,", "KON", "PWAT", "NN"], "meter": "+--+-+----+--+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Gr\u00e4bt, f\u00fcr der Nachwelt Spruch, einst die Geschieht' in ihr Erzt!", "tokens": ["Gr\u00e4bt", ",", "f\u00fcr", "der", "Nach\u00b7welt", "Spruch", ",", "einst", "die", "Ge\u00b7schieht'", "in", "ihr", "Erzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Doch die jetzige Welt ist Nachwelt, setzet sich, richtet", "tokens": ["Doch", "die", "jet\u00b7zi\u00b7ge", "Welt", "ist", "Nach\u00b7welt", ",", "set\u00b7zet", "sich", ",", "rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "NN", "$,", "VVFIN", "PRF", "$,", "VVFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Gleiches Gericht; wenn die That nakt vor das Auge sich stellt.", "tokens": ["Glei\u00b7ches", "Ge\u00b7richt", ";", "wenn", "die", "That", "nakt", "vor", "das", "Au\u00b7ge", "sich", "stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "ART", "NN", "ADJD", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Nakt steht: Herschende Buben sie brauchen, wer von der Herschsucht", "tokens": ["Nakt", "steht", ":", "Her\u00b7schen\u00b7de", "Bu\u00b7ben", "sie", "brau\u00b7chen", ",", "wer", "von", "der", "Herschsucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ADJA", "NN", "PPER", "VVFIN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Gl\u00fchet, wie sie: gebraucht, wandert er auf das Schasot.", "tokens": ["Gl\u00fc\u00b7het", ",", "wie", "sie", ":", "ge\u00b7braucht", ",", "wan\u00b7dert", "er", "auf", "das", "Scha\u00b7sot", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "$.", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jene kennen das Volk: Es will Despoten! und Schauspiel!", "tokens": ["Je\u00b7ne", "ken\u00b7nen", "das", "Volk", ":", "Es", "will", "Des\u00b7po\u00b7ten", "!", "und", "Schau\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "PPER", "VMFIN", "NN", "$.", "KON", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Fliegt zu der B\u00fchne, so bald einer den anderen w\u00fcrgt.", "tokens": ["Fliegt", "zu", "der", "B\u00fch\u00b7ne", ",", "so", "bald", "ei\u00b7ner", "den", "an\u00b7de\u00b7ren", "w\u00fcrgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ADV", "ADV", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Marat entrann dem Schafot; nun sollt' er, selbst nach dem Tode,", "tokens": ["Ma\u00b7rat", "ent\u00b7rann", "dem", "Scha\u00b7fot", ";", "nun", "sollt'", "er", ",", "selbst", "nach", "dem", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "ADV", "VMFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Ihnen noch fr\u00f6hnen: und so machten sie ihn zu 'nem Gott.", "tokens": ["Ih\u00b7nen", "noch", "fr\u00f6h\u00b7nen", ":", "und", "so", "mach\u00b7ten", "sie", "ihn", "zu", "'nem", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$.", "KON", "ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.29": {"text": "Nakt steht da die Rache an Toulons B\u00fcrger. Dem Tode", "tokens": ["Nakt", "steht", "da", "die", "Ra\u00b7che", "an", "Tou\u00b7lons", "B\u00fcr\u00b7ger", ".", "Dem", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "APPR", "NE", "NN", "$.", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.30": {"text": "Schon zum Opfer gekr\u00e4nzt, duldet' er feindlichen Schutz.", "tokens": ["Schon", "zum", "Op\u00b7fer", "ge\u00b7kr\u00e4nzt", ",", "dul\u00b7det'", "er", "feind\u00b7li\u00b7chen", "Schutz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.31": {"text": "B\u00fcrgerpflicht war nicht, dass er schlachten sich liess', und erlaubt nicht", "tokens": ["B\u00fcr\u00b7ger\u00b7pflicht", "war", "nicht", ",", "dass", "er", "schlach\u00b7ten", "sich", "liess'", ",", "und", "er\u00b7laubt", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG"], "meter": "+----+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Selbstmord: aber erlaubt Leben im rettenden Arm.", "tokens": ["Selbst\u00b7mord", ":", "a\u00b7ber", "er\u00b7laubt", "Le\u00b7ben", "im", "ret\u00b7ten\u00b7den", "Arm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Nakt steht da, was geschah: Als Stellvertreter zu Kerker", "tokens": ["Nakt", "steht", "da", ",", "was", "ge\u00b7schah", ":", "Als", "Stell\u00b7ver\u00b7tre\u00b7ter", "zu", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$,", "PWS", "VVFIN", "$.", "KOUS", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Gehen sollten! Als roth str\u00f6mte der Rhodan! Als sie,", "tokens": ["Ge\u00b7hen", "soll\u00b7ten", "!", "Als", "roth", "str\u00f6m\u00b7te", "der", "Rho\u00b7dan", "!", "Als", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$.", "KOUS", "ADJD", "VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "$,"], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "(scheusslich nakt steht dieses da, mit zischenden Schlangen-", "tokens": ["(", "scheuss\u00b7lich", "nakt", "steht", "die\u00b7ses", "da", ",", "mit", "zi\u00b7schen\u00b7den", "Schlan\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "ADJD", "VVFIN", "PDAT", "ADV", "$,", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Haaren, blauem Gesicht, sengenden Augen) als sie,", "tokens": ["Haa\u00b7ren", ",", "blau\u00b7em", "Ge\u00b7sicht", ",", "sen\u00b7gen\u00b7den", "Au\u00b7gen", ")", "als", "sie", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$(", "KOUS", "PPER", "$,"], "meter": "+-+--+-+-+-++", "measure": "trochaic.septa.relaxed"}, "line.37": {"text": "Welche Befreyung hiess, und Eroberung war, nach des sch\u00f6nsten", "tokens": ["Wel\u00b7che", "Be\u00b7fre\u00b7yung", "hiess", ",", "und", "Er\u00b7o\u00b7be\u00b7rung", "war", ",", "nach", "des", "sch\u00f6ns\u00b7ten"], "token_info": ["w