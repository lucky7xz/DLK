{"textgrid.poem.51082": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Holz und Reisig eine hohe Wand", "tokens": ["Von", "Holz", "und", "Rei\u00b7sig", "ei\u00b7ne", "ho\u00b7he", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seit langen Jahren um die Scheune stand;", "tokens": ["Seit", "lan\u00b7gen", "Jah\u00b7ren", "um", "die", "Scheu\u00b7ne", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon vieles macht' Verwittrung unbrauchbar,", "tokens": ["Schon", "vie\u00b7les", "macht'", "Ver\u00b7witt\u00b7rung", "un\u00b7brauch\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch jeder Herbst bringt neue Lasten dar.", "tokens": ["Doch", "je\u00b7der", "Herbst", "bringt", "neu\u00b7e", "Las\u00b7ten", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der letzte Winter brachte gro\u00dfe Not,", "tokens": ["Der", "letz\u00b7te", "Win\u00b7ter", "brach\u00b7te", "gro\u00b7\u00dfe", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und manche arme Witwe frierend bot", "tokens": ["Und", "man\u00b7che", "ar\u00b7me", "Wit\u00b7we", "frie\u00b7rend", "bot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr armes Geld dem Mann f\u00fcr wenig Holz \u2013", "tokens": ["Ihr", "ar\u00b7mes", "Geld", "dem", "Mann", "f\u00fcr", "we\u00b7nig", "Holz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er gab's nicht her in seinem Bauernstolz.", "tokens": ["Er", "gab's", "nicht", "her", "in", "sei\u00b7nem", "Bau\u00b7ern\u00b7stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun flammt es auf in wildem Funkenflug", "tokens": ["Nun", "flammt", "es", "auf", "in", "wil\u00b7dem", "Fun\u00b7ken\u00b7flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Scheun und Stall, Pferd, Wagen, Vieh und Pflug;", "tokens": ["Mit", "Scheun", "und", "Stall", ",", "Pferd", ",", "Wa\u00b7gen", ",", "Vieh", "und", "Pflug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die armen Weiber stehn und schaun es an,", "tokens": ["Die", "ar\u00b7men", "Wei\u00b7ber", "stehn", "und", "schaun", "es", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00e4rmen l\u00e4chelnd ihre H\u00e4nde dran.", "tokens": ["Und", "w\u00e4r\u00b7men", "l\u00e4\u00b7chelnd", "ih\u00b7re", "H\u00e4n\u00b7de", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dies L\u00e4cheln mag die bleichste Blume sein,", "tokens": ["Dies", "L\u00e4\u00b7cheln", "mag", "die", "bleichs\u00b7te", "Blu\u00b7me", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die zieren wird des Mannes Totenschrein \u2013", "tokens": ["Die", "zie\u00b7ren", "wird", "des", "Man\u00b7nes", "To\u00b7ten\u00b7schrein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weh dem, der solchen Bl\u00fctenflor ges\u00e4t,", "tokens": ["Weh", "dem", ",", "der", "sol\u00b7chen", "Bl\u00fc\u00b7ten\u00b7flor", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn einst die Saat in reifen Fr\u00fcchten steht!", "tokens": ["Wenn", "einst", "die", "Saat", "in", "rei\u00b7fen", "Fr\u00fcch\u00b7ten", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}