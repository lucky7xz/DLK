{"textgrid.poem.64861": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O glaubet nicht, da\u00df meine d\u00fcstern Klagen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O glaubet nicht, da\u00df meine d\u00fcstern Klagen", "tokens": ["O", "glau\u00b7bet", "nicht", ",", "da\u00df", "mei\u00b7ne", "d\u00fcs\u00b7tern", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Eurer Seele Mitleid wollen wecken!", "tokens": ["In", "Eu\u00b7rer", "See\u00b7le", "Mit\u00b7leid", "wol\u00b7len", "we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wollen Euch vielmehr mit Stolz entdecken,", "tokens": ["Sie", "wol\u00b7len", "Euch", "viel\u00b7mehr", "mit", "Stolz", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie viel ein Menschenherz vermag zu tragen.", "tokens": ["Wie", "viel", "ein", "Men\u00b7schen\u00b7herz", "ver\u00b7mag", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erkennt in ihnen muthigk\u00fchne Fragen,", "tokens": ["Er\u00b7kennt", "in", "ih\u00b7nen", "mut\u00b7hig\u00b7k\u00fch\u00b7ne", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erprobten Geistes kampfgest\u00e4hlte Recken,", "tokens": ["Er\u00b7prob\u00b7ten", "Geis\u00b7tes", "kampf\u00b7ge\u00b7st\u00e4hl\u00b7te", "Re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die vor dem dunklen Gegner nicht erschrecken", "tokens": ["Die", "vor", "dem", "dunk\u00b7len", "Geg\u00b7ner", "nicht", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trotzig selbst sich an das Schicksal wagen.", "tokens": ["Und", "trot\u00b7zig", "selbst", "sich", "an", "das", "Schick\u00b7sal", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Stirn, umstrahlt von fr\u00fch'rem Kronenschimmer,", "tokens": ["Die", "Stirn", ",", "um\u00b7strahlt", "von", "fr\u00fch'\u00b7rem", "Kro\u00b7nen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die niemals trug der Feigheit schmachvoll Zeichen,", "tokens": ["Die", "nie\u00b7mals", "trug", "der", "Feig\u00b7heit", "schmach\u00b7voll", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beugt sich vor eines Feindes Machtspruch nimmer!", "tokens": ["Beugt", "sich", "vor", "ei\u00b7nes", "Fein\u00b7des", "Machts\u00b7pruch", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erliegen kann ich seinen rohen Streichen;", "tokens": ["Er\u00b7lie\u00b7gen", "kann", "ich", "sei\u00b7nen", "ro\u00b7hen", "Strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch sterbend noch werd' ich es offenbaren,", "tokens": ["Doch", "ster\u00b7bend", "noch", "werd'", "ich", "es", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nur Gewalt, nicht Recht mir widerfahren.", "tokens": ["Da\u00df", "nur", "Ge\u00b7walt", ",", "nicht", "Recht", "mir", "wi\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "$,", "PTKNEG", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}