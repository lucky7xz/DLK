{"textgrid.poem.59108": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "327. Mittel zur Andacht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ganzer Sinn, ein pur Gottmeinen", "tokens": ["Ein", "gan\u00b7zer", "Sinn", ",", "ein", "pur", "Gott\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht leicht gesammelt zum Gebet;", "tokens": ["Macht", "leicht", "ge\u00b7sam\u00b7melt", "zum", "Ge\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sonst wird man nur and\u00e4chtig scheinen,", "tokens": ["Sonst", "wird", "man", "nur", "an\u00b7d\u00e4ch\u00b7tig", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man gleich gro\u00dfe Arbeit t\u00e4t'.", "tokens": ["Wenn", "man", "gleich", "gro\u00b7\u00dfe", "Ar\u00b7beit", "t\u00e4t'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}