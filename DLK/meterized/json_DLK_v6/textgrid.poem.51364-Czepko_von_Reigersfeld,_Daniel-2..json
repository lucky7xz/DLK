{"textgrid.poem.51364": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles, was ich seh an dir,", "tokens": ["Al\u00b7les", ",", "was", "ich", "seh", "an", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Stellung, Wonn und Zier:", "tokens": ["Dei\u00b7ner", "Stel\u00b7lung", ",", "Wonn", "und", "Zier", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Wangen freundlich Lachen,", "tokens": ["Dei\u00b7ner", "Wan\u00b7gen", "freund\u00b7lich", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann sie Rosengr\u00fcblein machen,", "tokens": ["Wann", "sie", "Ro\u00b7sen\u00b7gr\u00fcb\u00b7lein", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Augen Schertz und Spiel,", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "Schertz", "und", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann sie sind der Meinen Ziel:", "tokens": ["Wann", "sie", "sind", "der", "Mei\u00b7nen", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Lippen lieblich K\u00fcssen,", "tokens": ["Dei\u00b7ner", "Lip\u00b7pen", "lieb\u00b7lich", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann sie sich zusammen schliessen:", "tokens": ["Wann", "sie", "sich", "zu\u00b7sam\u00b7men", "schlies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deiner H\u00e4nde Deuteley:", "tokens": ["Dei\u00b7ner", "H\u00e4n\u00b7de", "Deu\u00b7te\u00b7ley", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Deiner F\u00fcsse Schockeley:", "tokens": ["Dei\u00b7ner", "F\u00fcs\u00b7se", "Scho\u00b7cke\u00b7ley", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Aller deiner Glieder Sitten,", "tokens": ["Al\u00b7ler", "dei\u00b7ner", "Glie\u00b7der", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn sie mich sehn dich so bitten;", "tokens": ["Wenn", "sie", "mich", "sehn", "dich", "so", "bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Nymphe, sprechen s\u00e4mmtlich Ja:", "tokens": ["Nym\u00b7phe", ",", "spre\u00b7chen", "s\u00e4mmt\u00b7lich", "Ja", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Nein, spricht blos der Mund allda.", "tokens": ["Nein", ",", "spricht", "blos", "der", "Mund", "all\u00b7da", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Wann es sol zum halten kommen,", "tokens": ["Wann", "es", "sol", "zum", "hal\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "APPRART", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df er m\u00fcste gar verstummen.", "tokens": ["Da\u00df", "er", "m\u00fcs\u00b7te", "gar", "ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}