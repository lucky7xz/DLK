{"dta.poem.7109": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O GOTT, der Du mich abermahl", "tokens": ["O", "GoTT", ",", "der", "Du", "mich", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Jahr vergn\u00fcgt beschl\u00fcssen lassen;", "tokens": ["Ein", "Jahr", "ver\u00b7gn\u00fcgt", "be\u00b7schl\u00fcs\u00b7sen", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer kan doch deiner Wunder Zahl,", "tokens": ["Wer", "kan", "doch", "dei\u00b7ner", "Wun\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Du an mir gethan, erz\u00e4hlen oder fassen?", "tokens": ["Die", "Du", "an", "mir", "ge\u00b7than", ",", "er\u00b7z\u00e4h\u00b7len", "o\u00b7der", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man zahle nicht allein die Tage, N\u00e4chte, Stunden;", "tokens": ["Man", "zah\u00b7le", "nicht", "al\u00b7lein", "die", "Ta\u00b7ge", ",", "N\u00e4ch\u00b7te", ",", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So viel Minuten und Secunden,", "tokens": ["So", "viel", "Mi\u00b7nu\u00b7ten", "und", "Se\u00b7cun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als ich in diesem Jahr erlebt; so manche Quelle", "tokens": ["Als", "ich", "in", "die\u00b7sem", "Jahr", "er\u00b7lebt", ";", "so", "man\u00b7che", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVPP", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von deiner G\u00fcter Heer ist \u00fcber mich geflossen.", "tokens": ["Von", "dei\u00b7ner", "G\u00fc\u00b7ter", "Heer", "ist", "\u00fc\u00b7ber", "mich", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja nicht allein auf mich, ein jeder Augenblick", "tokens": ["Ja", "nicht", "al\u00b7lein", "auf", "mich", ",", "ein", "je\u00b7der", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PTKNEG", "ADV", "APPR", "PPER", "$,", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hat sich zugleich, mit Heil und Gl\u00fcck,", "tokens": ["Hat", "sich", "zu\u00b7gleich", ",", "mit", "Heil", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf alle Meinigen ergossen.", "tokens": ["Auf", "al\u00b7le", "Mei\u00b7ni\u00b7gen", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Wie mancherley Gefahr, wie viele Ungl\u00fcs-F\u00e4lle", "tokens": ["Wie", "man\u00b7cher\u00b7ley", "Ge\u00b7fahr", ",", "wie", "vie\u00b7le", "Un\u00b7gl\u00fcs\u00b7F\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die jeden Augenblick uns h\u00e4tten st\u00fcrtzen k\u00f6nnen,", "tokens": ["Die", "je\u00b7den", "Au\u00b7gen\u00b7blick", "uns", "h\u00e4t\u00b7ten", "st\u00fcrt\u00b7zen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hat Deine Gnaden-Hand,", "tokens": ["Hat", "Dei\u00b7ne", "Gna\u00b7den\u00b7Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "All Augenblick, HERR! von uns abgewandt.", "tokens": ["All", "Au\u00b7gen\u00b7blick", ",", "HeRR", "!", "von", "uns", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$.", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Wer kan die Plagen alle nennen,", "tokens": ["Wer", "kan", "die", "Pla\u00b7gen", "al\u00b7le", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die \u00fcber unserm Haupt best\u00e4ndig stehn,", "tokens": ["Die", "\u00fc\u00b7ber", "un\u00b7serm", "Haupt", "be\u00b7st\u00e4n\u00b7dig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Dadurch ohnfehlbar wir sonst m\u00fcsten untergehn,", "tokens": ["Da\u00b7durch", "ohn\u00b7fehl\u00b7bar", "wir", "sonst", "m\u00fcs\u00b7ten", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aufs wenigste verletzt, geplagt, gekr\u00e4ncket werden.", "tokens": ["Aufs", "we\u00b7nigs\u00b7te", "ver\u00b7letzt", ",", "ge\u00b7plagt", ",", "ge\u00b7kr\u00e4n\u00b7cket", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVPP", "$,", "VVPP", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}