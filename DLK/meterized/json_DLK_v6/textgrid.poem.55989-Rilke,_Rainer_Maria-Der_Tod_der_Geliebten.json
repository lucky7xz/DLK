{"textgrid.poem.55989": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod der Geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er wu\u00dfte nur vom Tod was alle wissen:", "tokens": ["Er", "wu\u00df\u00b7te", "nur", "vom", "Tod", "was", "al\u00b7le", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "PWS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df er uns nimmt und in das Stumme st\u00f6\u00dft.", "tokens": ["da\u00df", "er", "uns", "nimmt", "und", "in", "das", "Stum\u00b7me", "st\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als aber sie, nicht von ihm fortgerissen,", "tokens": ["Als", "a\u00b7ber", "sie", ",", "nicht", "von", "ihm", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nein, leis aus seinen Augen ausgel\u00f6st,", "tokens": ["nein", ",", "leis", "aus", "sei\u00b7nen", "Au\u00b7gen", "aus\u00b7ge\u00b7l\u00f6st", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "hin\u00fcberglitt zu unbekannten Schatten,", "tokens": ["hin\u00b7\u00fc\u00b7berg\u00b7litt", "zu", "un\u00b7be\u00b7kann\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und als er f\u00fchlte, da\u00df sie dr\u00fcben nun", "tokens": ["und", "als", "er", "f\u00fchl\u00b7te", ",", "da\u00df", "sie", "dr\u00fc\u00b7ben", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wie einen Mond ihr M\u00e4dchenl\u00e4cheln hatten", "tokens": ["wie", "ei\u00b7nen", "Mond", "ihr", "M\u00e4d\u00b7chen\u00b7l\u00e4\u00b7cheln", "hat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und ihre Weise wohlzutun:", "tokens": ["und", "ih\u00b7re", "Wei\u00b7se", "wohl\u00b7zu\u00b7tun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "da wurden ihm die Toten so bekannt,", "tokens": ["da", "wur\u00b7den", "ihm", "die", "To\u00b7ten", "so", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als w\u00e4re er durch sie mit einem jeden", "tokens": ["als", "w\u00e4\u00b7re", "er", "durch", "sie", "mit", "ei\u00b7nem", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PPER", "APPR", "ART", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ganz nah verwandt; er lie\u00df die andern reden", "tokens": ["ganz", "nah", "ver\u00b7wandt", ";", "er", "lie\u00df", "die", "an\u00b7dern", "re\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$.", "PPER", "VVFIN", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "und glaubte nicht und nannte jenes Land", "tokens": ["und", "glaub\u00b7te", "nicht", "und", "nann\u00b7te", "je\u00b7nes", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das gutgelegene, das immers\u00fc\u00dfe \u2013", "tokens": ["das", "gut\u00b7ge\u00b7le\u00b7ge\u00b7ne", ",", "das", "im\u00b7mer\u00b7s\u00fc\u00b7\u00dfe", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und tastete es ab f\u00fcr ihre F\u00fc\u00dfe.", "tokens": ["Und", "tas\u00b7te\u00b7te", "es", "ab", "f\u00fcr", "ih\u00b7re", "F\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}