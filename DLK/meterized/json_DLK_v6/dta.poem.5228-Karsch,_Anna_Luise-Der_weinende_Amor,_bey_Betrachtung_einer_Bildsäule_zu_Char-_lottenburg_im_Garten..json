{"dta.poem.5228": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der weinende Amor,  \n bey Betrachtung einer Bilds\u00e4ule zu Char-  \n lottenburg im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Die Thr\u00e4ne redet von der rechten Backen", "tokens": ["Die", "Thr\u00e4\u00b7ne", "re\u00b7det", "von", "der", "rech\u00b7ten", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In stummer Sprache, Schmerz herab!", "tokens": ["In", "stum\u00b7mer", "Spra\u00b7che", ",", "Schmerz", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Pfeilen leer h\u00e4ngt ihm der K\u00f6cher auf dem Nacken;", "tokens": ["Von", "Pfei\u00b7len", "leer", "h\u00e4ngt", "ihm", "der", "K\u00f6\u00b7cher", "auf", "dem", "Na\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die grosse Venus gab", "tokens": ["Die", "gros\u00b7se", "Ve\u00b7nus", "gab"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}