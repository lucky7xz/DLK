{"textgrid.poem.34713": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es erklingen alle B\u00e4ume,", "tokens": ["Es", "er\u00b7klin\u00b7gen", "al\u00b7le", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es singen alle Nester \u2013", "tokens": ["Und", "es", "sin\u00b7gen", "al\u00b7le", "Nes\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist der Kapellenmeister", "tokens": ["Wer", "ist", "der", "Ka\u00b7pel\u00b7len\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In dem gr\u00fcnen Waldorchester?", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "Wal\u00b7dor\u00b7ches\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es dort der graue Kiebitz,", "tokens": ["Ist", "es", "dort", "der", "grau\u00b7e", "Kie\u00b7bitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der best\u00e4ndig nickt so wichtig?", "tokens": ["Der", "be\u00b7st\u00e4n\u00b7dig", "nickt", "so", "wich\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder der Pedant, der dorten", "tokens": ["O\u00b7der", "der", "Pe\u00b7dant", ",", "der", "dor\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer kuckuckt, zeitma\u00dfrichtig?", "tokens": ["Im\u00b7mer", "ku\u00b7ckuckt", ",", "zeit\u00b7ma\u00df\u00b7rich\u00b7tig", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist es jener Storch, der ernsthaft,", "tokens": ["Ist", "es", "je\u00b7ner", "Storch", ",", "der", "ernst\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und als ob er dirigieret',", "tokens": ["Und", "als", "ob", "er", "di\u00b7ri\u00b7gie\u00b7ret'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit dem langen Streckbein klappert,", "tokens": ["Mit", "dem", "lan\u00b7gen", "Streck\u00b7bein", "klap\u00b7pert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.4": {"text": "W\u00e4hrend alles musizieret?", "tokens": ["W\u00e4h\u00b7rend", "al\u00b7les", "mu\u00b7si\u00b7zie\u00b7ret", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein, in meinem eignen Herzen", "tokens": ["Nein", ",", "in", "mei\u00b7nem", "eig\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt des Walds Kapellenmeister,", "tokens": ["Sitzt", "des", "Walds", "Ka\u00b7pel\u00b7len\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich f\u00fchl, wie er den Takt schl\u00e4gt,", "tokens": ["Und", "ich", "f\u00fchl", ",", "wie", "er", "den", "Takt", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und ich glaube, Amor hei\u00dft er.", "tokens": ["Und", "ich", "glau\u00b7be", ",", "A\u00b7mor", "hei\u00dft", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NE", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}