{"dta.poem.7125": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist es nun nicht ein Gl\u00fcck, ein grosses Gl\u00fcck zu nennen,", "tokens": ["Ist", "es", "nun", "nicht", "ein", "Gl\u00fcck", ",", "ein", "gros\u00b7ses", "Gl\u00fcck", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein solches Ungl\u00fcck nicht zu kennen?", "tokens": ["Ein", "sol\u00b7ches", "Un\u00b7gl\u00fcck", "nicht", "zu", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja gar, an dessen statt, durch ein geruhig Schlaffen,", "tokens": ["Ja", "gar", ",", "an", "des\u00b7sen", "statt", ",", "durch", "ein", "ge\u00b7ru\u00b7hig", "Schlaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "APPR", "PDS", "PTKVZ", "$,", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem C\u00f6rper und dem Geist aufs neue Krafft zu schaffen.", "tokens": ["Dem", "C\u00f6r\u00b7per", "und", "dem", "Geist", "aufs", "neu\u00b7e", "Krafft", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach so gedencke doch ein jeder,", "tokens": ["Ach", "so", "ge\u00b7den\u00b7cke", "doch", "ein", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ADV", "ART", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der alle Nacht so s\u00fc\u00df, so sanffte ruht,", "tokens": ["Der", "al\u00b7le", "Nacht", "so", "s\u00fc\u00df", ",", "so", "sanff\u00b7te", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und singe Dem, der ihm die Gnade thut,", "tokens": ["Und", "sin\u00b7ge", "Dem", ",", "der", "ihm", "die", "Gna\u00b7de", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In Ehrsurcht Danck-und Freuden-Lieder.", "tokens": ["In", "Ehr\u00b7surcht", "Dan\u00b7ck\u00b7\u00b7und", "Freu\u00b7den\u00b7Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}