{"textgrid.poem.66552": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel vermag ein Menschenherz zu tragen,", "tokens": ["Viel", "ver\u00b7mag", "ein", "Men\u00b7schen\u00b7herz", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sorg', Entbehrung, harter Pflicht Gebot;", "tokens": ["Sor\u00b7g'", ",", "Ent\u00b7beh\u00b7rung", ",", "har\u00b7ter", "Pflicht", "Ge\u00b7bot", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um der Lieben und der Freunde Tod", "tokens": ["Um", "der", "Lie\u00b7ben", "und", "der", "Freun\u00b7de", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "KON", "ART", "NN", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Tr\u00e4gt es Leid, und will doch nicht verzagen.", "tokens": ["Tr\u00e4gt", "es", "Leid", ",", "und", "will", "doch", "nicht", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Von Betrug und T\u00e4uschung selbst geschlagen", "tokens": ["Von", "Be\u00b7trug", "und", "T\u00e4u\u00b7schung", "selbst", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebt es unter Wunden ungez\u00e4hlt,", "tokens": ["Liebt", "es", "un\u00b7ter", "Wun\u00b7den", "un\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ja, es liebt, was seine Liebe qu\u00e4lt,", "tokens": ["Ja", ",", "es", "liebt", ",", "was", "sei\u00b7ne", "Lie\u00b7be", "qu\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Um die Qual gleichwie ein Gl\u00fcck zu tragen!", "tokens": ["Um", "die", "Qual", "gleich\u00b7wie", "ein", "Gl\u00fcck", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}