{"textgrid.poem.61412": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilig acht' ich den Wein, und immer, sobald er die Lippen", "tokens": ["Hei\u00b7lig", "acht'", "ich", "den", "Wein", ",", "und", "im\u00b7mer", ",", "so\u00b7bald", "er", "die", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Herzerfreuend mir netzt, denk' ich des Lebens dabei.", "tokens": ["Her\u00b7zer\u00b7freu\u00b7end", "mir", "netzt", ",", "denk'", "ich", "des", "Le\u00b7bens", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn vom Lichte gezeugt und der alles ern\u00e4hrenden Erde", "tokens": ["Denn", "vom", "Lich\u00b7te", "ge\u00b7zeugt", "und", "der", "al\u00b7les", "er\u00b7n\u00e4h\u00b7ren\u00b7den", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "KON", "ART", "PIAT", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Gr\u00fc\u00dft in des Lenzes Beginn sch\u00fcchtern die Rebe den Tag;", "tokens": ["Gr\u00fc\u00dft", "in", "des", "Len\u00b7zes", "Be\u00b7ginn", "sch\u00fcch\u00b7tern", "die", "Re\u00b7be", "den", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Und dann k\u00fc\u00dft sie der Strahl, da weint sie. Aber die Z\u00e4hren", "tokens": ["Und", "dann", "k\u00fc\u00dft", "sie", "der", "Strahl", ",", "da", "weint", "sie", ".", "A\u00b7ber", "die", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$.", "KON", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Sind noch s\u00fc\u00df und allein quellenden Lebens Symbol;", "tokens": ["Sind", "noch", "s\u00fc\u00df", "und", "al\u00b7lein", "quel\u00b7len\u00b7den", "Le\u00b7bens", "Sym\u00b7bol", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADV", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Bald auch schie\u00dfen die Bl\u00e4tter heraus in gr\u00fcnender Jugend,", "tokens": ["Bald", "auch", "schie\u00b7\u00dfen", "die", "Bl\u00e4t\u00b7ter", "he\u00b7raus", "in", "gr\u00fc\u00b7nen\u00b7der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und allm\u00e4hlich am Stock dr\u00e4ngt sich die Traube hervor.", "tokens": ["Und", "all\u00b7m\u00e4h\u00b7lich", "am", "Stock", "dr\u00e4ngt", "sich", "die", "Trau\u00b7be", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-++--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Langsam reift sie, vom Glanze ges\u00e4ugt, bis endlich im Herbste", "tokens": ["Lang\u00b7sam", "reift", "sie", ",", "vom", "Glan\u00b7ze", "ge\u00b7s\u00e4ugt", ",", "bis", "end\u00b7lich", "im", "Herbs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,", "KON", "ADV", "APPRART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Voll s\u00fc\u00dfschwellenden Safts purpurn den Winzer sie lockt.", "tokens": ["Voll", "s\u00fc\u00df\u00b7schwel\u00b7len\u00b7den", "Safts", "pur\u00b7purn", "den", "Win\u00b7zer", "sie", "lockt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NE", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wenn sich das Laub dann senkt und den Tod vorahnend noch einmal", "tokens": ["Wenn", "sich", "das", "Laub", "dann", "senkt", "und", "den", "Tod", "vor\u00b7ah\u00b7nend", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVFIN", "KON", "ART", "NN", "VVPP", "ADV", "ADV"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Pr\u00e4chtig in Farben ergl\u00fcht, naht er mit blinkendem Erz;", "tokens": ["Pr\u00e4ch\u00b7tig", "in", "Far\u00b7ben", "er\u00b7gl\u00fcht", ",", "naht", "er", "mit", "blin\u00b7ken\u00b7dem", "Erz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Und vom Stamme gel\u00f6st und gel\u00f6st von der n\u00e4hrenden Mutter", "tokens": ["Und", "vom", "Stam\u00b7me", "ge\u00b7l\u00f6st", "und", "ge\u00b7l\u00f6st", "von", "der", "n\u00e4h\u00b7ren\u00b7den", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "KON", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Wird die gezeitigte Frucht unter die Kelter getan.", "tokens": ["Wird", "die", "ge\u00b7zei\u00b7tig\u00b7te", "Frucht", "un\u00b7ter", "die", "Kel\u00b7ter", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ach, dann duldet sie viel; der Geburt urspr\u00fcngliche Reinheit", "tokens": ["Ach", ",", "dann", "dul\u00b7det", "sie", "viel", ";", "der", "Ge\u00b7burt", "ur\u00b7spr\u00fcng\u00b7li\u00b7che", "Rein\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Geht ihr verloren, sie weint blutige Tr\u00e4nen des Leids.", "tokens": ["Geht", "ihr", "ver\u00b7lo\u00b7ren", ",", "sie", "weint", "blu\u00b7ti\u00b7ge", "Tr\u00e4\u00b7nen", "des", "Leids", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Aber das Fremde bew\u00e4ltigt sie nicht, und die Strahlen der Sonne,", "tokens": ["A\u00b7ber", "das", "Frem\u00b7de", "be\u00b7w\u00e4l\u00b7tigt", "sie", "nicht", ",", "und", "die", "Strah\u00b7len", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Die sie als Kind einsog, regen sich m\u00e4chtig in ihr,", "tokens": ["Die", "sie", "als", "Kind", "ein\u00b7sog", ",", "re\u00b7gen", "sich", "m\u00e4ch\u00b7tig", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVFIN", "$,", "ADJA", "PRF", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.19": {"text": "Bis sie in g\u00e4rendem Kampf die gemeineren Stoffe bezwungen", "tokens": ["Bis", "sie", "in", "g\u00e4\u00b7ren\u00b7dem", "Kampf", "die", "ge\u00b7mei\u00b7ne\u00b7ren", "Stof\u00b7fe", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und als Feuer und Geist wiedergeboren erscheint.", "tokens": ["Und", "als", "Feu\u00b7er", "und", "Geist", "wie\u00b7der\u00b7ge\u00b7bo\u00b7ren", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "VVIZU", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Seht, da fasset der Priester den Wein in goldene Schalen,", "tokens": ["Seht", ",", "da", "fas\u00b7set", "der", "Pries\u00b7ter", "den", "Wein", "in", "gol\u00b7de\u00b7ne", "Scha\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Und ein gel\u00e4utert Geschenk bringt er den G\u00f6ttern ihn dar.", "tokens": ["Und", "ein", "ge\u00b7l\u00e4u\u00b7tert", "Ge\u00b7schenk", "bringt", "er", "den", "G\u00f6t\u00b7tern", "ihn", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVPP", "NN", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}