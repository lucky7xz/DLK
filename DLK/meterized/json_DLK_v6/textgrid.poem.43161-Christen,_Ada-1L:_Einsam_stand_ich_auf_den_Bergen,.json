{"textgrid.poem.43161": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam stand ich auf den Bergen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam stand ich auf den Bergen,", "tokens": ["Ein\u00b7sam", "stand", "ich", "auf", "den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Falke kreischend flog,", "tokens": ["Wo", "der", "Fal\u00b7ke", "krei\u00b7schend", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber schneebedecktem Gipfel", "tokens": ["\u00dc\u00b7ber", "schnee\u00b7be\u00b7deck\u00b7tem", "Gip\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine stillen Kreise zog.", "tokens": ["Sei\u00b7ne", "stil\u00b7len", "Krei\u00b7se", "zog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einsam lag ich auf der Haide", "tokens": ["Ein\u00b7sam", "lag", "ich", "auf", "der", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Sonne untersank,", "tokens": ["Wenn", "die", "Son\u00b7ne", "un\u00b7ter\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der d\u00fcrre gl\u00fch'nde Boden", "tokens": ["Und", "der", "d\u00fcr\u00b7re", "gl\u00fch'n\u00b7de", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gierig feuchte Nebel trank.", "tokens": ["Gie\u00b7rig", "feuch\u00b7te", "Ne\u00b7bel", "trank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einsam sa\u00df ich oft am Meere,", "tokens": ["Ein\u00b7sam", "sa\u00df", "ich", "oft", "am", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen alter Klaggesang", "tokens": ["Des\u00b7sen", "al\u00b7ter", "Klag\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald wild-zornig, bald s\u00fc\u00df-traurig,", "tokens": ["Bald", "wild\u00b7zor\u00b7nig", ",", "bald", "s\u00fc\u00df\u00b7trau\u00b7rig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bald wie dumpfes Schluchzen klang.", "tokens": ["Bald", "wie", "dum\u00b7pfes", "Schluch\u00b7zen", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einsam irrt ich durch die W\u00e4lder,", "tokens": ["Ein\u00b7sam", "irrt", "ich", "durch", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur die Eul' am Felsenriff", "tokens": ["Nur", "die", "Eul'", "am", "Fel\u00b7sen\u00b7riff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War mein kr\u00e4chzender Gef\u00e4hrte", "tokens": ["War", "mein", "kr\u00e4ch\u00b7zen\u00b7der", "Ge\u00b7f\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Wind, der wimmernd pfiff.", "tokens": ["Und", "der", "Wind", ",", "der", "wim\u00b7mernd", "pfiff", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Einsam litt ich \u2013 aber tr\u00f6stend", "tokens": ["Ein\u00b7sam", "litt", "ich", "\u2013", "a\u00b7ber", "tr\u00f6s\u00b7tend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$(", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War die hehre Einsamkeit \u2013", "tokens": ["War", "die", "heh\u00b7re", "Ein\u00b7sam\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht allein trug ich mein Elend,", "tokens": ["Nicht", "al\u00b7lein", "trug", "ich", "mein", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Natur verstand mein Leid!", "tokens": ["Die", "Na\u00b7tur", "ver\u00b7stand", "mein", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch allein \u2013 so ganz alleine \u2013", "tokens": ["Doch", "al\u00b7lein", "\u2013", "so", "ganz", "al\u00b7lei\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "ADV", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abgrundtief von Euch entfernt,", "tokens": ["Ab\u00b7grund\u00b7tief", "von", "Euch", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand ich mich in Euren S\u00e4len \u2013", "tokens": ["Fand", "ich", "mich", "in", "Eu\u00b7ren", "S\u00e4\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich Euch versteh'n gelernt!", "tokens": ["Als", "ich", "Euch", "ver\u00b7steh'n", "ge\u00b7lernt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}