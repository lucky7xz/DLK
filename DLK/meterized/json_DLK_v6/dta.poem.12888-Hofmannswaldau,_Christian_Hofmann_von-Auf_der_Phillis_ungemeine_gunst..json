{"dta.poem.12888": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Phillis ungemeine gunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So offt ich mund und arm auf Phillis achseln lege,", "tokens": ["So", "offt", "ich", "mund", "und", "arm", "auf", "Phil\u00b7lis", "ach\u00b7seln", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "KON", "ADJD", "APPR", "NE", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "So mu\u00df ich wol gestehn, wenn ich es recht erwege,", "tokens": ["So", "mu\u00df", "ich", "wol", "ge\u00b7stehn", ",", "wenn", "ich", "es", "recht", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df dieses holde kind mit mir zu sch\u00f6ne thut,", "tokens": ["Da\u00df", "die\u00b7ses", "hol\u00b7de", "kind", "mit", "mir", "zu", "sch\u00f6\u00b7ne", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "APPR", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil selbst der himmel nur auf harten ", "tokens": ["Weil", "selbst", "der", "him\u00b7mel", "nur", "auf", "har\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}