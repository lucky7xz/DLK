{"dta.poem.14157": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeits\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Fernher zieh'n wir durch die Gassen,", "tokens": ["Fern\u00b7her", "zieh'n", "wir", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steh'n im Regen und im Wind,", "tokens": ["Steh'n", "im", "Re\u00b7gen", "und", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl von aller Welt verlassen", "tokens": ["Wohl", "von", "al\u00b7ler", "Welt", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme Musikanten sind.", "tokens": ["Ar\u00b7me", "Mu\u00b7si\u00b7kan\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den Fenstern Geigen klingen,", "tokens": ["Aus", "den", "Fens\u00b7tern", "Gei\u00b7gen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleift und dreht sich's bunt und laut,", "tokens": ["Schleift", "und", "dreht", "sich's", "bunt", "und", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir Musikanten singen", "tokens": ["Und", "wir", "Mu\u00b7si\u00b7kan\u00b7ten", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drau\u00dfen da der reichen Braut.", "tokens": ["Drau\u00b7\u00dfen", "da", "der", "rei\u00b7chen", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}