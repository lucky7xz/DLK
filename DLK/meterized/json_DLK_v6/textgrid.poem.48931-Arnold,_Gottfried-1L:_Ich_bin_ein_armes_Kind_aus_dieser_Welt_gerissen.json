{"textgrid.poem.48931": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin ein armes Kind/ aus dieser Welt gerissen/", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein armes Kind/ aus dieser Welt gerissen/", "tokens": ["Ich", "bin", "ein", "ar\u00b7mes", "Kind", "/", "aus", "die\u00b7ser", "Welt", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von mir selbst entbl\u00f6st/ von allen abgethan/", "tokens": ["Und", "von", "mir", "selbst", "ent\u00b7bl\u00f6st", "/", "von", "al\u00b7len", "ab\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVPP", "$(", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich hab und liebe nichts/ ich kan und will nichts wissen/", "tokens": ["Ich", "hab", "und", "lie\u00b7be", "nichts", "/", "ich", "kan", "und", "will", "nichts", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "PIS", "$(", "PPER", "VMFIN", "KON", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bekenne/ da\u00df ich mir im Grund nichts helffen kan.", "tokens": ["Be\u00b7ken\u00b7ne", "/", "da\u00df", "ich", "mir", "im", "Grund", "nichts", "helf\u00b7fen", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PPER", "APPRART", "NN", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So arm und blo\u00df bin ich. Wo aber soll ich finden/", "tokens": ["So", "arm", "und", "blo\u00df", "bin", "ich", ".", "Wo", "a\u00b7ber", "soll", "ich", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VAFIN", "PPER", "$.", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was mich in Armuth reich/ im Elend herrlich macht.", "tokens": ["Was", "mich", "in", "Ar\u00b7muth", "reich", "/", "im", "E\u00b7lend", "herr\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADJD", "$(", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich wehle was ich will/ so mu\u00df mirs bald verschwinden/", "tokens": ["Ich", "weh\u00b7le", "was", "ich", "will", "/", "so", "mu\u00df", "mirs", "bald", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$(", "ADV", "VMFIN", "NE", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die gantze Creatur hat mir nichts zugebracht.", "tokens": ["Die", "gant\u00b7ze", "Crea\u00b7tur", "hat", "mir", "nichts", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die Welt die gibt mir nichts/ sie hat mich abgetrennet/", "tokens": ["Die", "Welt", "die", "gibt", "mir", "nichts", "/", "sie", "hat", "mich", "ab\u00b7ge\u00b7tren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PPER", "PIS", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Frommen sind selbst arm/ sie haben nichts vor sich/", "tokens": ["Die", "From\u00b7men", "sind", "selbst", "arm", "/", "sie", "ha\u00b7ben", "nichts", "vor", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "PPER", "VAFIN", "PIS", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein jeder bettelt selbst/ was er sein eigen nennet.", "tokens": ["Ein", "je\u00b7der", "bet\u00b7telt", "selbst", "/", "was", "er", "sein", "ei\u00b7gen", "nen\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$(", "PWS", "PPER", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So gar stehts ausser Gott umb uns sehr j\u00e4mmerlich/", "tokens": ["So", "gar", "stehts", "aus\u00b7ser", "Gott", "umb", "uns", "sehr", "j\u00e4m\u00b7mer\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "APPR", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier steh ich/ Herr/ vor dir; Entbl\u00f6\u00df und f\u00fcll zugleich/", "tokens": ["Hier", "steh", "ich", "/", "Herr", "/", "vor", "dir", ";", "Ent\u00b7bl\u00f6\u00df", "und", "f\u00fcll", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "$(", "APPR", "PPER", "$.", "NN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein au\u00dfgeleertes Hertz mit deinem Himmel-Reich.", "tokens": ["Mein", "au\u00df\u00b7ge\u00b7leer\u00b7tes", "Hertz", "mit", "dei\u00b7nem", "Him\u00b7mel\u00b7Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}