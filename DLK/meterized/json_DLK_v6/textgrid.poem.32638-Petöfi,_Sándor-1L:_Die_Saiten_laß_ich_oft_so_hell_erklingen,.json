{"textgrid.poem.32638": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Saiten la\u00df ich oft so hell erklingen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Saiten la\u00df ich oft so hell erklingen,", "tokens": ["Die", "Sai\u00b7ten", "la\u00df", "ich", "oft", "so", "hell", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das liebe Erdendasein zu besingen,", "tokens": ["Das", "lie\u00b7be", "Er\u00b7den\u00b7da\u00b7sein", "zu", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Leben lieb' ich, ob's auch Wunden schl\u00e4gt,", "tokens": ["Das", "Le\u00b7ben", "lieb'", "ich", ",", "ob's", "auch", "Wun\u00b7den", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meist uns nur durch \u00f6de Wildnis tr\u00e4gt.", "tokens": ["Und", "meist", "uns", "nur", "durch", "\u00f6\u00b7de", "Wild\u00b7nis", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie hoch indes ich mit dem Sang auch strebe,", "tokens": ["Wie", "hoch", "in\u00b7des", "ich", "mit", "dem", "Sang", "auch", "stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie reich ihn mit des Frohsinns Glanz durchwebe,", "tokens": ["Wie", "reich", "ihn", "mit", "des", "Froh\u00b7sinns", "Glanz", "durch\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das End' ist doch, da\u00df freudlos es verklingt,", "tokens": ["Das", "End'", "ist", "doch", ",", "da\u00df", "freud\u00b7los", "es", "ver\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es, wie alles, in die Grube sinkt.", "tokens": ["Da\u00df", "es", ",", "wie", "al\u00b7les", ",", "in", "die", "Gru\u00b7be", "sinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PIS", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O, wie erquick' ich mich am Reiz des Maien,", "tokens": ["O", ",", "wie", "er\u00b7quick'", "ich", "mich", "am", "Reiz", "des", "Mai\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da Aug' und Herz an Blumen sich erfreuen!", "tokens": ["Da", "Aug'", "und", "Herz", "an", "Blu\u00b7men", "sich", "er\u00b7freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wenn mein Blick noch auf den Bl\u00fcten weilt,", "tokens": ["Doch", ",", "wenn", "mein", "Blick", "noch", "auf", "den", "Bl\u00fc\u00b7ten", "weilt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Fu\u00df schon hastend hin zum Friedhof eilt!", "tokens": ["Mein", "Fu\u00df", "schon", "has\u00b7tend", "hin", "zum", "Fried\u00b7hof", "eilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Friedhof, den nur Gram und Leid ums\u00e4umen,", "tokens": ["Im", "Fried\u00b7hof", ",", "den", "nur", "Gram", "und", "Leid", "um\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versinken Seel' und Herz in ernstes Tr\u00e4umen,", "tokens": ["Ver\u00b7sin\u00b7ken", "Seel'", "und", "Herz", "in", "erns\u00b7tes", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier tr\u00e4umen, scheint mir, die Akazien auch,", "tokens": ["Hier", "tr\u00e4u\u00b7men", ",", "scheint", "mir", ",", "die", "A\u00b7ka\u00b7zi\u00b7en", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Geschaukelt von des Abendwindes Hauch.", "tokens": ["Ge\u00b7schau\u00b7kelt", "von", "des", "Ab\u00b7end\u00b7win\u00b7des", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie heil'ge Zeichen Gottes mir erscheinen", "tokens": ["Wie", "heil'\u00b7ge", "Zei\u00b7chen", "Got\u00b7tes", "mir", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lettern auf den Kreuzlein und den Steinen,", "tokens": ["Die", "Let\u00b7tern", "auf", "den", "Kreuz\u00b7lein", "und", "den", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie halten mich in seinem hehren Bann,", "tokens": ["Sie", "hal\u00b7ten", "mich", "in", "sei\u00b7nem", "heh\u00b7ren", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie f\u00fchren mich auf Gr\u00e4bern himmelan!", "tokens": ["Sie", "f\u00fch\u00b7ren", "mich", "auf", "Gr\u00e4\u00b7bern", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dumpf-traurig aus dem Dorf das Gl\u00f6cklein l\u00e4utet, \u2013", "tokens": ["Dumpf\u00b7trau\u00b7rig", "aus", "dem", "Dorf", "das", "Gl\u00f6c\u00b7klein", "l\u00e4u\u00b7tet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch dort wird jetzt ein frisches Grab bereitet,", "tokens": ["Auch", "dort", "wird", "jetzt", "ein", "fri\u00b7sches", "Grab", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Voll Andacht tr\u00e4gt den Toten man hinaus,", "tokens": ["Voll", "An\u00b7dacht", "tr\u00e4gt", "den", "To\u00b7ten", "man", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Senkt ihn zur Ruh' in der Verwesung Haus.", "tokens": ["Senkt", "ihn", "zur", "Ruh'", "in", "der", "Ver\u00b7we\u00b7sung", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schon ahnt mein Herz \u2013 kaum wird die Ahnung tr\u00fcgen \u2013,", "tokens": ["Schon", "ahnt", "mein", "Herz", "\u2013", "kaum", "wird", "die", "Ah\u00b7nung", "tr\u00fc\u00b7gen", "\u2013", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "ART", "NN", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald werd' auch ich im Grabe unten liegen,", "tokens": ["Bald", "werd'", "auch", "ich", "im", "Gra\u00b7be", "un\u00b7ten", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald bettet man auch mir zur ew'gen Ruh',", "tokens": ["Bald", "bet\u00b7tet", "man", "auch", "mir", "zur", "ew'\u00b7gen", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schlie\u00dft ew'ger Traum auch mir die Augen zu.", "tokens": ["Schlie\u00dft", "ew'\u00b7ger", "Traum", "auch", "mir", "die", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nun denn, mit Gott! Mag mir die Stunde schlagen,", "tokens": ["Nun", "denn", ",", "mit", "Gott", "!", "Mag", "mir", "die", "Stun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "NN", "$.", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die meine Laufbahn schlie\u00dft, ich will nicht klagen,", "tokens": ["Die", "mei\u00b7ne", "Lauf\u00b7bahn", "schlie\u00dft", ",", "ich", "will", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerrei\u00dfen mag, was mich ans Dasein h\u00e4lt,", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "mag", ",", "was", "mich", "ans", "Da\u00b7sein", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Abschiedsku\u00df, dann lebe wohl, du Welt!", "tokens": ["Ein", "Ab\u00b7schieds\u00b7ku\u00df", ",", "dann", "le\u00b7be", "wohl", ",", "du", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ADV", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und was das Schicksal grausam mir verwehrte,", "tokens": ["Und", "was", "das", "Schick\u00b7sal", "grau\u00b7sam", "mir", "ver\u00b7wehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Obgleich es meine Sehnsucht hei\u00df begehrte:", "tokens": ["Ob\u00b7gleich", "es", "mei\u00b7ne", "Sehn\u00b7sucht", "hei\u00df", "be\u00b7gehr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fahr' hin, o ", "tokens": ["Fahr'", "hin", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "PTKVZ", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Du Himmelsblume \u00bb", "tokens": ["Du", "Him\u00b7mels\u00b7blu\u00b7me", "\u00bb"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}