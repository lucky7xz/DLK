{"dta.poem.8552": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alle tausend Jahre", "tokens": ["Al\u00b7le", "tau\u00b7send", "Jah\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "wachsen mir Fl\u00fcgel.", "tokens": ["wach\u00b7sen", "mir", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Alle tausend Jahre", "tokens": ["Al\u00b7le", "tau\u00b7send", "Jah\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "saust mein purpurner Drachenleib", "tokens": ["saust", "mein", "pur\u00b7pur\u00b7ner", "Dra\u00b7chen\u00b7leib"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "durch die Finsterniss.", "tokens": ["durch", "die", "Fins\u00b7ter\u00b7niss", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "In entseelte Himmel", "tokens": ["In", "ent\u00b7seel\u00b7te", "Him\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "spei ich", "tokens": ["spei", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Myriaden Sterne!", "tokens": ["My\u00b7ria\u00b7den", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Am Bach,", "tokens": ["Am", "Bach", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "unter Weiden,", "tokens": ["un\u00b7ter", "Wei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "sitz ich dann, flechte mein langes Goldhaar, singe", "tokens": ["sitz", "ich", "dann", ",", "flech\u00b7te", "mein", "lan\u00b7ges", "Gold\u00b7haar", ",", "sin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "und freue mich, wie sie Oben glitzern.", "tokens": ["und", "freu\u00b7e", "mich", ",", "wie", "sie", "O\u00b7ben", "glit\u00b7zern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}