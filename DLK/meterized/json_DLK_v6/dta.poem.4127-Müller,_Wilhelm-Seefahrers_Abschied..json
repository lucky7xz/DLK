{"dta.poem.4127": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Seefahrers Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kleine Schwalbe, komm herab,", "tokens": ["Klei\u00b7ne", "Schwal\u00b7be", ",", "komm", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ich dir ein Wort im Stillen", "tokens": ["Weil", "ich", "dir", "ein", "Wort", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unten zu vertrauen hab'.", "tokens": ["Un\u00b7ten", "zu", "ver\u00b7trau\u00b7en", "hab'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst mir eine Feder schenken", "tokens": ["Sollst", "mir", "ei\u00b7ne", "Fe\u00b7der", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den schwarzen Fl\u00fcgeln dein,", "tokens": ["Aus", "den", "schwar\u00b7zen", "Fl\u00fc\u00b7geln", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will an meine Liebe schreiben:", "tokens": ["Will", "an", "mei\u00b7ne", "Lie\u00b7be", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herz, es mu\u00df geschieden sein!", "tokens": ["Herz", ",", "es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Morgen fahr' ich auf dem Meere,", "tokens": ["Mor\u00b7gen", "fahr'", "ich", "auf", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wind und Woge wei\u00df wohin,", "tokens": ["Wind", "und", "Wo\u00b7ge", "wei\u00df", "wo\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es fragen mich die Freunde,", "tokens": ["Und", "es", "fra\u00b7gen", "mich", "die", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich doch so traurig bin.", "tokens": ["Was", "ich", "doch", "so", "trau\u00b7rig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber Wind und Woge sprechen", "tokens": ["A\u00b7ber", "Wind", "und", "Wo\u00b7ge", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Viel von Unbest\u00e4ndigkeit,", "tokens": ["Viel", "von", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Sklave singt zum Ruder:", "tokens": ["Und", "der", "Skla\u00b7ve", "singt", "zum", "Ru\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00e4chtig, m\u00e4chtig ist die Zeit!", "tokens": ["M\u00e4ch\u00b7tig", ",", "m\u00e4ch\u00b7tig", "ist", "die", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott, und soll ich untergehen,", "tokens": ["Gott", ",", "und", "soll", "ich", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei es in dem tiefen Meer,", "tokens": ["Sei", "es", "in", "dem", "tie\u00b7fen", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur nicht in der Liebsten Herzen,", "tokens": ["Nur", "nicht", "in", "der", "Liebs\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo ich gern geborgen w\u00e4r'.", "tokens": ["Wo", "ich", "gern", "ge\u00b7bor\u00b7gen", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem stillen klaren Spiegel", "tokens": ["In", "dem", "stil\u00b7len", "kla\u00b7ren", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Male sich mein treues Bild,", "tokens": ["Ma\u00b7le", "sich", "mein", "treu\u00b7es", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann um mich in Ungewittern", "tokens": ["Wann", "um", "mich", "in", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "KOUI", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die emp\u00f6rte Woge schwillt.", "tokens": ["Die", "em\u00b7p\u00f6r\u00b7te", "Wo\u00b7ge", "schwillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebe, sieh, wie Well' auf Welle", "tokens": ["Lie\u00b7be", ",", "sieh", ",", "wie", "Well'", "auf", "Wel\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PWAV", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringt nach dem ersehnten Strand:", "tokens": ["Ringt", "nach", "dem", "er\u00b7sehn\u00b7ten", "Strand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber manche wird verschlungen,", "tokens": ["A\u00b7ber", "man\u00b7che", "wird", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh' sie k\u00fc\u00dft das gr\u00fcne Land.", "tokens": ["Eh'", "sie", "k\u00fc\u00dft", "das", "gr\u00fc\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn du an dem Ufer wandelst,", "tokens": ["Wenn", "du", "an", "dem", "U\u00b7fer", "wan\u00b7delst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00fcpft die Fluth nach deinem Fu\u00df:", "tokens": ["H\u00fcpft", "die", "Fluth", "nach", "dei\u00b7nem", "Fu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wogen hab' ich nur und Winde,", "tokens": ["Wo\u00b7gen", "hab'", "ich", "nur", "und", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir zu schicken meinen Gru\u00df.", "tokens": ["Dir", "zu", "schi\u00b7cken", "mei\u00b7nen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wann die fernen H\u00f6hen d\u00e4mmern,", "tokens": ["Wann", "die", "fer\u00b7nen", "H\u00f6\u00b7hen", "d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jauchzet Alles nach dem Land:", "tokens": ["Jauch\u00b7zet", "Al\u00b7les", "nach", "dem", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zwei m\u00fcde Augen bleiben", "tokens": ["Nur", "zwei", "m\u00fc\u00b7de", "Au\u00b7gen", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still dem Meere zugewandt.", "tokens": ["Still", "dem", "Mee\u00b7re", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die Segel wieder gl\u00e4nzen,", "tokens": ["Wann", "die", "Se\u00b7gel", "wie\u00b7der", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann die Winde heimw\u00e4rts wehn,", "tokens": ["Wann", "die", "Win\u00b7de", "heim\u00b7w\u00e4rts", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00dft mich auf dem Maste sitzen:", "tokens": ["La\u00dft", "mich", "auf", "dem", "Mas\u00b7te", "sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebe kann durch Wolken sehn.", "tokens": ["Lie\u00b7be", "kann", "durch", "Wol\u00b7ken", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}