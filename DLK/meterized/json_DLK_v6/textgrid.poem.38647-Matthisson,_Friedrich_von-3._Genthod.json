{"textgrid.poem.38647": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3. Genthod", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00f6rflich erschein' ich dem Auge des Wanderers: aber seit Bonnet", "tokens": ["D\u00f6rf\u00b7lich", "er\u00b7schein'", "ich", "dem", "Au\u00b7ge", "des", "Wan\u00b7de\u00b7rers", ":", "a\u00b7ber", "seit", "Bon\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "ADV", "APPR", "NE"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Hier ein Sabinum bewohnt, hab' ich mit London den Rang.", "tokens": ["Hier", "ein", "Sa\u00b7bi\u00b7num", "be\u00b7wohnt", ",", "hab'", "ich", "mit", "Lon\u00b7don", "den", "Rang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVPP", "$,", "VAFIN", "PPER", "APPR", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Nah' dich dem Greise voll Andacht, o Fremdling! der g\u00f6ttlich zu leben", "tokens": ["Nah'", "dich", "dem", "Grei\u00b7se", "voll", "An\u00b7dacht", ",", "o", "Fremd\u00b7ling", "!", "der", "g\u00f6tt\u00b7lich", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "NN", "ADJD", "NN", "$,", "FM", "NN", "$.", "ART", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+--++-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und was die Vorsicht verh\u00e4ngt, still zu erwarten dich lehrt.", "tokens": ["Und", "was", "die", "Vor\u00b7sicht", "ver\u00b7h\u00e4ngt", ",", "still", "zu", "er\u00b7war\u00b7ten", "dich", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "ADJD", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}