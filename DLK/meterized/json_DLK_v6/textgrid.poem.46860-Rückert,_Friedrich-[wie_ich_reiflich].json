{"textgrid.poem.46860": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie ich reiflich]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich reiflich", "tokens": ["Wie", "ich", "reif\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wog mein Leid,", "tokens": ["Wog", "mein", "Leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Es ist doch mir unbegreiflich,", "tokens": ["Es", "ist", "doch", "mir", "un\u00b7be\u00b7greif\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ihr mir verloren seid.", "tokens": ["Da\u00df", "ihr", "mir", "ver\u00b7lo\u00b7ren", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sah ich nicht die Todtenbahre,", "tokens": ["Sah", "ich", "nicht", "die", "Tod\u00b7ten\u00b7bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den dunkeln Kranz im Haare", "tokens": ["Und", "den", "dun\u00b7keln", "Kranz", "im", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinem sch\u00f6nen Kinderpaar?", "tokens": ["Mei\u00b7nem", "sch\u00f6\u00b7nen", "Kin\u00b7der\u00b7paar", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch bezweifl' ich", "tokens": ["Doch", "be\u00b7zweifl'", "ich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Noch mein Leid,", "tokens": ["Noch", "mein", "Leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Es ist doch mir unbegreiflich,", "tokens": ["Es", "ist", "doch", "mir", "un\u00b7be\u00b7greif\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da\u00df ihr mir verloren seid.", "tokens": ["Da\u00df", "ihr", "mir", "ver\u00b7lo\u00b7ren", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leugn' ich ab das Offenbare,", "tokens": ["Leugn'", "ich", "ab", "das", "Of\u00b7fen\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es sei nicht wahr das Wahre?", "tokens": ["Und", "es", "sei", "nicht", "wahr", "das", "Wah\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch an meinem Hals das klare", "tokens": ["Doch", "an", "mei\u00b7nem", "Hals", "das", "kla\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fehlt handgreiflich,", "tokens": ["Fehlt", "hand\u00b7greif\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Das Geschmeid;", "tokens": ["Das", "Ge\u00b7schmeid", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und das Weh ist unabstreiflich,", "tokens": ["Und", "das", "Weh", "ist", "un\u00b7ab\u00b7streif\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Da\u00df ihr mir verloren seid.", "tokens": ["Da\u00df", "ihr", "mir", "ver\u00b7lo\u00b7ren", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kommen nun und gehen Jahre,", "tokens": ["Kom\u00b7men", "nun", "und", "ge\u00b7hen", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Natur am Brautaltare", "tokens": ["Und", "Na\u00b7tur", "am", "Brau\u00b7tal\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald und bald auch an der Bahre", "tokens": ["Bald", "und", "bald", "auch", "an", "der", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wechsl' umschweiflich", "tokens": ["Wechsl'", "um\u00b7schwei\u00b7flich"], "token_info": ["word", "word"], "pos": ["XY", "XY"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Kleid um Kleid!", "tokens": ["Kleid", "um", "Kleid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Diese Schart' ist unauschleiflich,", "tokens": ["Die\u00b7se", "Schart'", "ist", "un\u00b7au\u00b7schleif\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ihr mir verloren seid.", "tokens": ["Da\u00df", "ihr", "mir", "ver\u00b7lo\u00b7ren", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}