{"dta.poem.10899": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch Rascheln unterbricht der Stille so lang hier ungest\u00f6r-\nten Sitz?", "tokens": ["Welch", "Ra\u00b7scheln", "un\u00b7ter\u00b7bricht", "der", "Stil\u00b7le", "so", "lang", "hier", "un\u00b7ge\u00b7st\u00f6r", "ten", "Sitz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADV", "TRUNC", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Es rauschen B\u00fcsch, es krachen Str\u00e4ucher. Was fliegt daher, als", "tokens": ["Es", "rau\u00b7schen", "B\u00fcsch", ",", "es", "kra\u00b7chen", "Str\u00e4u\u00b7cher", ".", "Was", "fliegt", "da\u00b7her", ",", "als"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "ADJA", "NN", "$.", "PWS", "VVFIN", "PAV", "$,", "KOUS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es war ein angeschweister Hirsch: Er ist vorbey und fort", "tokens": ["Es", "war", "ein", "an\u00b7ge\u00b7schweis\u00b7ter", "Hirsch", ":", "Er", "ist", "vor\u00b7bey", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wie! wird er in der Flucht zu Stein?", "tokens": ["Wie", "!", "wird", "er", "in", "der", "Flucht", "zu", "Stein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er fleucht, und bleibt auf einer Stelle. Die\u00df ist nun keine", "tokens": ["Er", "fleucht", ",", "und", "bleibt", "auf", "ei\u00b7ner", "Stel\u00b7le", ".", "Die\u00df", "ist", "nun", "kei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$.", "PDS", "VAFIN", "ADV", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Doch ist es eine schwarze Kunst. Damit hier lange sichtbar sey,", "tokens": ["Doch", "ist", "es", "ei\u00b7ne", "schwar\u00b7ze", "Kunst", ".", "Da\u00b7mit", "hier", "lan\u00b7ge", "sicht\u00b7bar", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "NN", "$.", "PAV", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Was sonst die Schnelligkeit uns raubet: Kann man, an diesem", "tokens": ["Was", "sonst", "die", "Schnel\u00b7lig\u00b7keit", "uns", "rau\u00b7bet", ":", "Kann", "man", ",", "an", "die\u00b7sem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "PPER", "VVFIN", "$.", "VMFIN", "PIS", "$,", "APPR", "PDAT"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Entsetzen, Unmuth, Grimm und Gram, Furcht, W\u00fcten,", "tokens": ["Ent\u00b7set\u00b7zen", ",", "Un\u00b7muth", ",", "Grimm", "und", "Gram", ",", "Furcht", ",", "W\u00fc\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht in den Augen nur allein, in allen seinen Gliedern, schauen.", "tokens": ["Nicht", "in", "den", "Au\u00b7gen", "nur", "al\u00b7lein", ",", "in", "al\u00b7len", "sei\u00b7nen", "Glie\u00b7dern", ",", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "ADV", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "Die Muskeln raffen sich zusammen; die strammen Nerven", "tokens": ["Die", "Mus\u00b7keln", "raf\u00b7fen", "sich", "zu\u00b7sam\u00b7men", ";", "die", "stram\u00b7men", "Ner\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Hier seh ich nun zwar eine Probe, wie weit des Menschen", "tokens": ["Hier", "seh", "ich", "nun", "zwar", "ei\u00b7ne", "Pro\u00b7be", ",", "wie", "weit", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Auch in den allerdicksten W\u00e4ldern, die Herrschaft \u00fcber Thie-", "tokens": ["Auch", "in", "den", "al\u00b7ler\u00b7dicks\u00b7ten", "W\u00e4l\u00b7dern", ",", "die", "Herr\u00b7schaft", "\u00fc\u00b7ber", "Thie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Doch seh ichs ohne Mitleid kaum. Wesfalls ich eilig meine", "tokens": ["Doch", "seh", "ichs", "oh\u00b7ne", "Mit\u00b7leid", "kaum", ".", "Wes\u00b7falls", "ich", "ei\u00b7lig", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "APPR", "NN", "ADV", "$.", "KOUS", "PPER", "ADJD", "PPOSAT"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Um mich zu tr\u00f6sten, in den Wald, in die beb\u00fcschte Ferne schicke.", "tokens": ["Um", "mich", "zu", "tr\u00f6s\u00b7ten", ",", "in", "den", "Wald", ",", "in", "die", "be\u00b7b\u00fcschte", "Fer\u00b7ne", "schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Ich senke mich, mit stiller Lust, in das verwachsene Gefilde;", "tokens": ["Ich", "sen\u00b7ke", "mich", ",", "mit", "stil\u00b7ler", "Lust", ",", "in", "das", "ver\u00b7wach\u00b7se\u00b7ne", "Ge\u00b7fil\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.16": {"text": "Und wenn ich, in dem sch\u00f6nen Wald, mich satt und doch nicht", "tokens": ["Und", "wenn", "ich", ",", "in", "dem", "sch\u00f6\u00b7nen", "Wald", ",", "mich", "satt", "und", "doch", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "ADJD", "KON", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "Weil man stets neue Sch\u00f6nheit sp\u00fcret: So ruf ich: Jeder", "tokens": ["Weil", "man", "stets", "neu\u00b7e", "Sch\u00f6n\u00b7heit", "sp\u00fc\u00b7ret", ":", "So", "ruf", "ich", ":", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "VVFIN", "$.", "ADV", "NN", "PPER", "$.", "PIS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Da\u00df hier die bildende Natur, durch Ridinger, sich selbsten bilde.", "tokens": ["Da\u00df", "hier", "die", "bil\u00b7den\u00b7de", "Na\u00b7tur", ",", "durch", "Ri\u00b7din\u00b7ger", ",", "sich", "selbs\u00b7ten", "bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "NN", "$,", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}