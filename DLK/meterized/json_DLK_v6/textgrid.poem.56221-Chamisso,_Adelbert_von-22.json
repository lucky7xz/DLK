{"textgrid.poem.56221": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "22", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bestreut mit Eichenlaub die Bahre dort \u2013 \u2013", "tokens": ["Be\u00b7streut", "mit", "Ei\u00b7chen\u00b7laub", "die", "Bah\u00b7re", "dort", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "ADV", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O meine Kinder! so wird hergetragen,", "tokens": ["O", "mei\u00b7ne", "Kin\u00b7der", "!", "so", "wird", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der unser Vater war und unser Hort,", "tokens": ["Der", "un\u00b7ser", "Va\u00b7ter", "war", "und", "un\u00b7ser", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Herz hat ausgeschlagen.", "tokens": ["Sein", "Herz", "hat", "aus\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heb auf das Tuch, du bist sein einz'ger Sohn,", "tokens": ["Heb", "auf", "das", "Tuch", ",", "du", "bist", "sein", "einz'\u00b7ger", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Sohne wird die Wunde dieses Helden,", "tokens": ["Dem", "Soh\u00b7ne", "wird", "die", "Wun\u00b7de", "die\u00b7ses", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Mannestugend sei, und was ihr Lohn,", "tokens": ["Was", "Man\u00b7nes\u00b7tu\u00b7gend", "sei", ",", "und", "was", "ihr", "Lohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "KON", "PWS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gar unverge\u00dflich melden.", "tokens": ["Gar", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Namens Erbe, den er sich erwarb,", "tokens": ["Des", "Na\u00b7mens", "Er\u00b7be", ",", "den", "er", "sich", "er\u00b7warb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sollst trachten du dereinst nach gleichem Adel,", "tokens": ["Sollst", "trach\u00b7ten", "du", "de\u00b7reinst", "nach", "glei\u00b7chem", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sterben, mu\u00df es sein, so wie er starb,", "tokens": ["Und", "ster\u00b7ben", ",", "mu\u00df", "es", "sein", ",", "so", "wie", "er", "starb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VMFIN", "PPER", "VAINF", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stets ohne Furcht und Tadel.", "tokens": ["Stets", "oh\u00b7ne", "Furcht", "und", "Ta\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du, Auge meiner Freude, fielest zu,", "tokens": ["Du", ",", "Au\u00b7ge", "mei\u00b7ner", "Freu\u00b7de", ",", "fie\u00b7lest", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich, s\u00fc\u00dfer Mund, erschlie\u00dfet nicht mein Sehnen, \u2013", "tokens": ["Dich", ",", "s\u00fc\u00b7\u00dfer", "Mund", ",", "er\u00b7schlie\u00b7\u00dfet", "nicht", "mein", "Seh\u00b7nen", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, weine, meine Tochter, weine du,", "tokens": ["Ja", ",", "wei\u00b7ne", ",", "mei\u00b7ne", "Toch\u00b7ter", ",", "wei\u00b7ne", "du", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich habe keine Tr\u00e4nen.", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}