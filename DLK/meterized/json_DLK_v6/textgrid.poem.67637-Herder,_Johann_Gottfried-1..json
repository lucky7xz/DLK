{"textgrid.poem.67637": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie liebe lange Nacht,\u00ab", "tokens": ["\u00bb", "die", "lie\u00b7be", "lan\u00b7ge", "Nacht", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sprach Petrus einst, \u00bbhab' ich mit saurem Schwei\u00df durchwacht", "tokens": ["Sprach", "Pet\u00b7rus", "einst", ",", "\u00bb", "hab'", "ich", "mit", "sau\u00b7rem", "Schwei\u00df", "durch\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "FM", "ADV", "$,", "$(", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und leider nichts gefangen.\u00ab", "tokens": ["Und", "lei\u00b7der", "nichts", "ge\u00b7fan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PIS", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbnicht Alles l\u00e4\u00dft sich auch durch sauern Schwei\u00df erlangen,\u00ab", "tokens": ["\u00bb", "nicht", "Al\u00b7les", "l\u00e4\u00dft", "sich", "auch", "durch", "sau\u00b7ern", "Schwei\u00df", "er\u00b7lan\u00b7gen", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKNEG", "PIS", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sprach unser Herr; \u00bbfahr auf die H\u00f6h", "tokens": ["Sprach", "un\u00b7ser", "Herr", ";", "\u00bb", "fahr", "auf", "die", "H\u00f6h"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "$(", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit gutem Muth", "tokens": ["Mit", "gu\u00b7tem", "Muth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und thu noch ", "tokens": ["Und", "thu", "noch"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Er that's und rief: \u00bbAch, Herr, genug!", "tokens": ["Er", "that's", "und", "rief", ":", "\u00bb", "Ach", ",", "Herr", ",", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "ITJ", "$,", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Netz zerri\u00df; wir k\u00f6nnen mehr nicht fangen.\u00ab", "tokens": ["Das", "Netz", "zer\u00b7ri\u00df", ";", "wir", "k\u00f6n\u00b7nen", "mehr", "nicht", "fan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kleingl\u00e4ubiger, heg' immer guten Muth!", "tokens": ["Klein\u00b7gl\u00e4u\u00b7bi\u00b7ger", ",", "heg'", "im\u00b7mer", "gu\u00b7ten", "Muth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Noch wird das Ende gut.", "tokens": ["Noch", "wird", "das", "En\u00b7de", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Wunder der Natur gelang durch Schwei\u00df und M\u00fch;", "tokens": ["Kein", "Wun\u00b7der", "der", "Na\u00b7tur", "ge\u00b7lang", "durch", "Schwei\u00df", "und", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf Gottes Wink gelingen sie.", "tokens": ["Auf", "Got\u00b7tes", "Wink", "ge\u00b7lin\u00b7gen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}