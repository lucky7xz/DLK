{"dta.poem.813": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "L.  \n  ELIAS.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der Flammen aus der Brust der Mutter hat gesogen; ", "tokens": ["Der", "Flam\u00b7men", "aus", "der", "Brust", "der", "Mut\u00b7ter", "hat", "ge\u00b7so\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der von der heilgen Flam des eyvers hei\u00df entbrandt/", "tokens": ["Der", "von", "der", "heil\u00b7gen", "Flam", "des", "ey\u00b7vers", "hei\u00df", "ent\u00b7brandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df F\u00fcrsten grim verlacht/ vnd dem verf\u00fchrten Land", "tokens": ["De\u00df", "F\u00fcrs\u00b7ten", "grim", "ver\u00b7lacht", "/", "vnd", "dem", "ver\u00b7f\u00fchr\u00b7ten", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch flammen hat entdeckt/ wie Kron vnd Hau\u00df betrogen:", "tokens": ["Durch", "flam\u00b7men", "hat", "ent\u00b7deckt", "/", "wie", "Kron", "vnd", "Hau\u00df", "be\u00b7tro\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VAFIN", "VVPP", "$(", "KOKOM", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Mann/ auff dessen wortt die flammen abgeflogen", "tokens": ["Der", "Mann", "/", "auff", "des\u00b7sen", "wortt", "die", "flam\u00b7men", "ab\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "APPR", "PRELAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die erhitzte Lufft/ vnd die der K\u00f6nig sandt", "tokens": ["Durch", "die", "er\u00b7hitz\u00b7te", "Lufft", "/", "vnd", "die", "der", "K\u00f6\u00b7nig", "sandt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "KON", "ART", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit schneller glutt verzehrt; ist als jhn GOTT entbandt", "tokens": ["Mit", "schnel\u00b7ler", "glutt", "ver\u00b7zehrt", ";", "ist", "als", "jhn", "GoTT", "ent\u00b7bandt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJD", "VVPP", "$.", "VAFIN", "KOKOM", "PPER", "NE", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch in dem Fewr\u2019gen Sturm aus dieser welt gezogen.", "tokens": ["Auch", "in", "dem", "Fe\u00b7wr'\u00b7gen", "Sturm", "aus", "die\u00b7ser", "welt", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Er fehrt/ doch vnversehrt/ kein fewrig Ro\u00df vnd Wagen", "tokens": ["Er", "fehrt", "/", "doch", "vn\u00b7ver\u00b7sehrt", "/", "kein", "few\u00b7rig", "Ro\u00df", "vnd", "Wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADJD", "$(", "PIAT", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Letzt den/ der Fewr im Mund vnd Hertzen pflag zu tragen", "tokens": ["Letzt", "den", "/", "der", "Fewr", "im", "Mund", "vnd", "Hert\u00b7zen", "pflag", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$(", "ART", "NN", "APPRART", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Mit dem Er Hertzen/ mehr de\u00f1 Marmorhart/ zusprengt", "tokens": ["Mit", "dem", "Er", "Hert\u00b7zen", "/", "mehr", "de\u00f1", "Mar\u00b7mor\u00b7hart", "/", "zu\u00b7sprengt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "NN", "$(", "ADV", "NE", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der gantz von Fewer war/ mu\u00df mit dem Fewr hinscheiden:", "tokens": ["Der", "gantz", "von", "Fe\u00b7wer", "war", "/", "mu\u00df", "mit", "dem", "Fewr", "hin\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VAFIN", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fragt jhr warumb sein kleidt nichts kan von flammen leid\u1ebd?", "tokens": ["Fragt", "jhr", "wa\u00b7rumb", "sein", "kleidt", "nichts", "kan", "von", "flam\u00b7men", "lei\u00b7d\u1ebd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "PPOSAT", "ADJD", "PIS", "VMFIN", "APPR", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Mich wundert/ da\u00df es nicht weil er es trug/ verseng\u2019t.", "tokens": ["Mich", "wun\u00b7dert", "/", "da\u00df", "es", "nicht", "weil", "er", "es", "trug", "/", "ver\u00b7seng'", "t."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "KOUS", "PPER", "PPER", "VVFIN", "$(", "XY", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}