{"textgrid.poem.34580": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der schwarze Fl\u00fcgel tr\u00fcber Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der schwarze Fl\u00fcgel tr\u00fcber Nacht", "tokens": ["Der", "schwar\u00b7ze", "Fl\u00fc\u00b7gel", "tr\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will Alles \u00fcberdecken;", "tokens": ["Will", "Al\u00b7les", "\u00fc\u00b7berd\u00b7e\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch dies, was Gottes Finger macht,", "tokens": ["Doch", "dies", ",", "was", "Got\u00b7tes", "Fin\u00b7ger", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt mir geringen Schrecken.", "tokens": ["Bringt", "mir", "ge\u00b7rin\u00b7gen", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ist das Aufgebot zur Ruh,", "tokens": ["Es", "ist", "das", "Auf\u00b7ge\u00b7bot", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Abschied vieler Sorgen,", "tokens": ["Der", "Ab\u00b7schied", "vie\u00b7ler", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gar in einem kurzen Nu", "tokens": ["Und", "gar", "in", "ei\u00b7nem", "kur\u00b7zen", "Nu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erscheint ein neuer Morgen.", "tokens": ["Er\u00b7scheint", "ein", "neu\u00b7er", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Jesu, bleib mein klares Licht!", "tokens": ["Mein", "Je\u00b7su", ",", "bleib", "mein", "kla\u00b7res", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entz\u00fcnd' in meinem Herzen,", "tokens": ["Ent\u00b7z\u00fcnd'", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mir der Sonne Glanz gebricht,", "tokens": ["Wenn", "mir", "der", "Son\u00b7ne", "Glanz", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Andacht reine Kerzen.", "tokens": ["Der", "An\u00b7dacht", "rei\u00b7ne", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Besch\u00fctze meinen Leib und Geist", "tokens": ["Be\u00b7sch\u00fct\u00b7ze", "mei\u00b7nen", "Leib", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch deines Heeres Wache,", "tokens": ["Durch", "dei\u00b7nes", "Hee\u00b7res", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, was da Feind und Teufel hei\u00dft,", "tokens": ["Da\u00df", ",", "was", "da", "Feind", "und", "Teu\u00b7fel", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nicht zu Schanden mache.", "tokens": ["Mich", "nicht", "zu", "Schan\u00b7den", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df gegen mich nicht Schlaf und Tod", "tokens": ["La\u00df", "ge\u00b7gen", "mich", "nicht", "Schlaf", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPER", "PTKNEG", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammen sich verbinden;", "tokens": ["Zu\u00b7sam\u00b7men", "sich", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df keine Krankheit, Angst und Noth", "tokens": ["La\u00df", "kei\u00b7ne", "Krank\u00b7heit", ",", "Angst", "und", "Noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich um mein Lager finden.", "tokens": ["Sich", "um", "mein", "La\u00b7ger", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hilf, da\u00df die weiche Lagerstatt", "tokens": ["Hilf", ",", "da\u00df", "die", "wei\u00b7che", "La\u00b7ger\u00b7statt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nicht zu Dornen mache.", "tokens": ["Sich", "nicht", "zu", "Dor\u00b7nen", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl dem, der diesen Machtspruch hat:", "tokens": ["Wohl", "dem", ",", "der", "die\u00b7sen", "Machts\u00b7pruch", "hat", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herr f\u00fchre meine Sache!", "tokens": ["Herr", "f\u00fch\u00b7re", "mei\u00b7ne", "Sa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df durch die Ruh sich neue Kraft", "tokens": ["La\u00df", "durch", "die", "Ruh", "sich", "neu\u00b7e", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Geist und Adern r\u00fchren", "tokens": ["In", "Geist", "und", "A\u00b7dern", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und deines Segens Eigenschaft", "tokens": ["Und", "dei\u00b7nes", "Se\u00b7gens", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich auch im Schlafe sp\u00fcren!", "tokens": ["Mich", "auch", "im", "Schla\u00b7fe", "sp\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch la\u00df den Schlaf zu rechter Zeit", "tokens": ["Doch", "la\u00df", "den", "Schlaf", "zu", "rech\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch, wie die Nacht, verschwinden", "tokens": ["Auch", ",", "wie", "die", "Nacht", ",", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mich in reiner Freudigkeit", "tokens": ["Und", "mich", "in", "rei\u00b7ner", "Freu\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das neue Licht empfinden!", "tokens": ["Das", "neu\u00b7e", "Licht", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So will ich mich, so viel ich kann,", "tokens": ["So", "will", "ich", "mich", ",", "so", "viel", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erden stets entrei\u00dfen,", "tokens": ["Der", "Er\u00b7den", "stets", "ent\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich ehren und auch Jedermann", "tokens": ["Dich", "eh\u00b7ren", "und", "auch", "Je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dienen mich beflei\u00dfen.", "tokens": ["Zu", "die\u00b7nen", "mich", "be\u00b7flei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mein Herze soll dein Weihrauch sein;", "tokens": ["Mein", "Her\u00b7ze", "soll", "dein", "Weih\u00b7rauch", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will es dir verbrennen", "tokens": ["Ich", "will", "es", "dir", "ver\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ohne Heuchelei und Schein", "tokens": ["Und", "oh\u00b7ne", "Heu\u00b7che\u00b7lei", "und", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich Herr, mich Diener nennen.", "tokens": ["Dich", "Herr", ",", "mich", "Die\u00b7ner", "nen\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}