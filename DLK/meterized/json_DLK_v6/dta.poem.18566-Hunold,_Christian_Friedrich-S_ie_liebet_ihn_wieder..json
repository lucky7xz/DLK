{"dta.poem.18566": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "S ie liebet ihn wieder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gl\u00fccke will mich freundlich k\u00fcssen/", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "will", "mich", "freund\u00b7lich", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Wermuth gleichen Schmertz durch Nectar-Wein", "tokens": ["Und", "Wer\u00b7muth", "glei\u00b7chen", "Schmertz", "durch", "Nec\u00b7ta\u00b7rWein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "vers\u00fcssen/", "tokens": ["ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Himmel rinnt von Perlen-Thau/", "tokens": ["Der", "Him\u00b7mel", "rinnt", "von", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er \u00f6fnet meiner Brust den Uberflu\u00df von Sch\u00e4tzen/", "tokens": ["Er", "\u00f6f\u00b7net", "mei\u00b7ner", "Brust", "den", "U\u00b7ber\u00b7flu\u00df", "von", "Sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und will die Seel auf keinen Bau/", "tokens": ["Und", "will", "die", "Seel", "auf", "kei\u00b7nen", "Bau", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von irdischer Vergn\u00fcgung setzen.", "tokens": ["Von", "ir\u00b7di\u00b7scher", "Ver\u00b7gn\u00fc\u00b7gung", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df mit Noht und Leid zu schertzen.", "tokens": ["Ich", "wei\u00df", "mit", "Noht", "und", "Leid", "zu", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es brennen nun in mir wohl tausend Freuden-Kertzen.", "tokens": ["Es", "bren\u00b7nen", "nun", "in", "mir", "wohl", "tau\u00b7send", "Freu\u00b7den\u00b7Kert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich geh' auf einer Rosen-Bahn.", "tokens": ["Ich", "geh'", "auf", "ei\u00b7ner", "Ro\u00b7sen\u00b7Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach schwartzer Nacht mu\u00df mir die Sonne kl\u00e4rer scheinen/", "tokens": ["Nach", "schwart\u00b7zer", "Nacht", "mu\u00df", "mir", "die", "Son\u00b7ne", "kl\u00e4\u00b7rer", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der s\u00fcsse Morgen bricht nun an/", "tokens": ["Der", "s\u00fcs\u00b7se", "Mor\u00b7gen", "bricht", "nun", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Lachen folget nach dem Weinen.", "tokens": ["Und", "La\u00b7chen", "fol\u00b7get", "nach", "dem", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein Centner kan die Schmertzen wiegen/", "tokens": ["Kein", "Cent\u00b7ner", "kan", "die", "Schmert\u00b7zen", "wie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die erst auf meiner Brust durch Zweiffel musten liegen/", "tokens": ["Die", "erst", "auf", "mei\u00b7ner", "Brust", "durch", "Zweif\u00b7fel", "mus\u00b7ten", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun fliehen sie wie Staub in Wind:", "tokens": ["Nun", "flie\u00b7hen", "sie", "wie", "Staub", "in", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df machet/ deine Hand kan lindern und auch dr\u00fccken/", "tokens": ["Da\u00df", "ma\u00b7chet", "/", "dei\u00b7ne", "Hand", "kan", "lin\u00b7dern", "und", "auch", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "$(", "PPOSAT", "NN", "VMFIN", "VVINF", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wie der Treue Wercke sind/", "tokens": ["Und", "wie", "der", "Treu\u00b7e", "Wer\u00b7cke", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach der Erkentni\u00df auch erquicken.", "tokens": ["Nach", "der", "Er\u00b7kent\u00b7ni\u00df", "auch", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Und darff ich mir gleich nicht versprechen/", "tokens": ["Und", "darff", "ich", "mir", "gleich", "nicht", "ver\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Gl\u00fccke werde nun die Knospen g\u00e4ntzlich brechen/", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "wer\u00b7de", "nun", "die", "Knos\u00b7pen", "g\u00e4ntz\u00b7lich", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vielleicht da\u00df es doch bald geschieht.", "tokens": ["Viel\u00b7leicht", "da\u00df", "es", "doch", "bald", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Maulbeer-Baum ist offt die Freude zu vergleichen/", "tokens": ["Dem", "Maul\u00b7beer\u00b7Baum", "ist", "offt", "die", "Freu\u00b7de", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df beydes nur am letzten bl\u00fcht/", "tokens": ["Da\u00df", "bey\u00b7des", "nur", "am", "letz\u00b7ten", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um auch zu erst die Frucht zu reichen.", "tokens": ["Um", "auch", "zu", "erst", "die", "Frucht", "zu", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df ich dir stets getreu gewesen/", "tokens": ["Da\u00df", "ich", "dir", "stets", "ge\u00b7treu", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kanst du aus der Gedult in Marter Wochen lesen/", "tokens": ["Kanst", "du", "aus", "der", "Ge\u00b7dult", "in", "Mar\u00b7ter", "Wo\u00b7chen", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Drum g\u00f6nne mir dein Freuden-Fest/", "tokens": ["Drum", "g\u00f6n\u00b7ne", "mir", "dein", "Freu\u00b7den\u00b7Fest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lasse mir geneigt nach Sturm und Jammer-Winden/", "tokens": ["Und", "las\u00b7se", "mir", "ge\u00b7neigt", "nach", "Sturm", "und", "Jam\u00b7mer\u00b7Win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch deinen s\u00fcssen Gnaden-West/", "tokens": ["Auch", "dei\u00b7nen", "s\u00fcs\u00b7sen", "Gna\u00b7den\u00b7West", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und meiner Sehnsucht Uffer finden.", "tokens": ["Und", "mei\u00b7ner", "Sehn\u00b7sucht", "Uf\u00b7fer", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Engel nimm auf Brust und Wangen", "tokens": ["Mein", "En\u00b7gel", "nimm", "auf", "Brust", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun meinen keuschen Ku\u00df und brennendes Verlangen/", "tokens": ["Nun", "mei\u00b7nen", "keu\u00b7schen", "Ku\u00df", "und", "bren\u00b7nen\u00b7des", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will dein treuer ", "tokens": ["Ich", "will", "dein", "treu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Weil du der Helena in allen zu vergleichen/", "tokens": ["Weil", "du", "der", "He\u00b7le\u00b7na", "in", "al\u00b7len", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "APPR", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum lasse mir zum Hafen ein", "tokens": ["Drum", "las\u00b7se", "mir", "zum", "Ha\u00b7fen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seegel reiner Liebe streichen.", "tokens": ["Die", "See\u00b7gel", "rei\u00b7ner", "Lie\u00b7be", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}