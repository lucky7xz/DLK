{"textgrid.poem.61022": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein, wohin so schnell?", "tokens": ["V\u00f6\u00b7glein", ",", "wo\u00b7hin", "so", "schnell", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbnach Norden, nach Norden!", "tokens": ["\u00bb", "nach", "Nor\u00b7den", ",", "nach", "Nor\u00b7den", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dort scheint die Sonne nun so hell,", "tokens": ["Dort", "scheint", "die", "Son\u00b7ne", "nun", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort ist's nun Fr\u00fchling worden.\u00ab", "tokens": ["Dort", "ist's", "nun", "Fr\u00fch\u00b7ling", "wor\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O V\u00f6glein mit den Fl\u00fcgeln bunt,", "tokens": ["O", "V\u00f6\u00b7glein", "mit", "den", "Fl\u00fc\u00b7geln", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn du kommst zum Lindengrund,", "tokens": ["Und", "wenn", "du", "kommst", "zum", "Lin\u00b7den\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Hause meiner Lieben,", "tokens": ["Zum", "Hau\u00b7se", "mei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann sag' ihr, da\u00df ich Tag und Nacht", "tokens": ["Dann", "sag'", "ihr", ",", "da\u00df", "ich", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von ihr getr\u00e4umt, an sie gedacht,", "tokens": ["Von", "ihr", "ge\u00b7tr\u00e4umt", ",", "an", "sie", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und da\u00df ich treu geblieben.", "tokens": ["Und", "da\u00df", "ich", "treu", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und die Blumen im Tal", "tokens": ["Und", "die", "Blu\u00b7men", "im", "Tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gr\u00fc\u00df' tausend, tausendmal!", "tokens": ["Gr\u00fc\u00df'", "tau\u00b7send", ",", "tau\u00b7send\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}