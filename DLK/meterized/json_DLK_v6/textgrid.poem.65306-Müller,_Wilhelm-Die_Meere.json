{"textgrid.poem.65306": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Meere", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Winde schlafen", "tokens": ["Al\u00b7le", "Win\u00b7de", "schla\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf dem Spiegel der Fluth;", "tokens": ["Auf", "dem", "Spie\u00b7gel", "der", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "K\u00fchle Schatten des Abends", "tokens": ["K\u00fch\u00b7le", "Schat\u00b7ten", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Decken die m\u00fcden zu.", "tokens": ["De\u00b7cken", "die", "m\u00fc\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Luna h\u00e4ngt sich Schleier", "tokens": ["Lu\u00b7na", "h\u00e4ngt", "sich", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u00dcber ihr Gesicht,", "tokens": ["\u00dc\u00b7ber", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schwebt in d\u00e4mmernden Tr\u00e4umen", "tokens": ["Schwebt", "in", "d\u00e4m\u00b7mern\u00b7den", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00dcber die Wasser hin.", "tokens": ["\u00dc\u00b7ber", "die", "Was\u00b7ser", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Alles, Alles stille", "tokens": ["Al\u00b7les", ",", "Al\u00b7les", "stil\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["PIS", "$,", "PIS", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf dem weiten Meer!", "tokens": ["Auf", "dem", "wei\u00b7ten", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur mein Herz will nimmer", "tokens": ["Nur", "mein", "Herz", "will", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit zur Ruhe gehn.", "tokens": ["Mit", "zur", "Ru\u00b7he", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In der Liebe Fluthen", "tokens": ["In", "der", "Lie\u00b7be", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Treibt es her und hin,", "tokens": ["Treibt", "es", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo die St\u00fcrme nicht ruhen,", "tokens": ["Wo", "die", "St\u00fcr\u00b7me", "nicht", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bis der Nachen sinkt.", "tokens": ["Bis", "der", "Na\u00b7chen", "sinkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}