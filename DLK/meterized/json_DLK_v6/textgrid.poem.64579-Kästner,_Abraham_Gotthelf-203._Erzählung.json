{"textgrid.poem.64579": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "203. Erz\u00e4hlung", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Sternthurm mu\u00dft' ein J\u00fcngling oft besteigen,", "tokens": ["Den", "Stern\u00b7thurm", "mu\u00dft'", "ein", "J\u00fcng\u00b7ling", "oft", "be\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Lehrer wollt' ihm da die Venus zeigen,", "tokens": ["Sein", "Leh\u00b7rer", "wollt'", "ihm", "da", "die", "Ve\u00b7nus", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und das bey hellem Sonnenschein.", "tokens": ["Und", "das", "bey", "hel\u00b7lem", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Beyde manchen Weg sich nun umsonst gemacht,", "tokens": ["Als", "Bey\u00b7de", "man\u00b7chen", "Weg", "sich", "nun", "um\u00b7sonst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "NN", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Fand, ohne Lehrer, ganz allein,", "tokens": ["Fand", ",", "oh\u00b7ne", "Leh\u00b7rer", ",", "ganz", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der J\u00fcngling sie bey Nacht.", "tokens": ["Der", "J\u00fcng\u00b7ling", "sie", "bey", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}