{"dta.poem.1778": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df sind wahre Freunde nicht/ welche gutte Worte geben/", "tokens": ["Di\u00df", "sind", "wah\u00b7re", "Freun\u00b7de", "nicht", "/", "wel\u00b7che", "gut\u00b7te", "Wor\u00b7te", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "PTKNEG", "$(", "PWAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und/ was von dem Freunde kommt/ alles in den Himmel", "tokens": ["Und", "/", "was", "von", "dem", "Freun\u00b7de", "kommt", "/", "al\u00b7les", "in", "den", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "PWS", "APPR", "ART", "NN", "VVFIN", "$(", "PIS", "APPR", "ART", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "heben.", "tokens": ["he\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}