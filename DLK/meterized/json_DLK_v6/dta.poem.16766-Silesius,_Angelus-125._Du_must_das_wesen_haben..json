{"dta.poem.16766": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "125.  Du must das wesen haben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott selbst ists Him\u0304elreich: wiltu in Himmel kom\u0304en/", "tokens": ["Gott", "selbst", "ists", "Him\u0304el\u00b7reich", ":", "wil\u00b7tu", "in", "Him\u00b7mel", "kom\u0304en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADJD", "$.", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mu\u00df GOttes wesenheit in dir seyn angeglommen.", "tokens": ["Mu\u00df", "Got\u00b7tes", "we\u00b7sen\u00b7heit", "in", "dir", "seyn", "an\u00b7ge\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "APPR", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}