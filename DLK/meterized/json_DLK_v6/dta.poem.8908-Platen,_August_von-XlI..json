{"dta.poem.8908": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Das mich bef\u00e4llt in deiner lieben N\u00e4he?", "tokens": ["Das", "mich", "be\u00b7f\u00e4llt", "in", "dei\u00b7ner", "lie\u00b7ben", "N\u00e4\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin ich geh' und mit den Blicken sp\u00e4he,", "tokens": ["Wo\u00b7hin", "ich", "geh'", "und", "mit", "den", "Bli\u00b7cken", "sp\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da hoff' ich dich und f\u00fcrchte dich zu finden.", "tokens": ["Da", "hoff'", "ich", "dich", "und", "f\u00fcrch\u00b7te", "dich", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie kann ich Furcht vor dir, o Freund, empfinden,", "tokens": ["Wie", "kann", "ich", "Furcht", "vor", "dir", ",", "o", "Freund", ",", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "APPR", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den ich so gern an meinem Busen s\u00e4he?", "tokens": ["Den", "ich", "so", "gern", "an", "mei\u00b7nem", "Bu\u00b7sen", "s\u00e4\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erkl\u00e4re du mir, was so schnell und j\u00e4he", "tokens": ["Er\u00b7kl\u00e4\u00b7re", "du", "mir", ",", "was", "so", "schnell", "und", "j\u00e4\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PRELS", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Blut mir hemmt, den Geist vermag zu binden?", "tokens": ["Das", "Blut", "mir", "hemmt", ",", "den", "Geist", "ver\u00b7mag", "zu", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ist es die Sorge, da\u00df dein Herz mir schweiget,", "tokens": ["Ist", "es", "die", "Sor\u00b7ge", ",", "da\u00df", "dein", "Herz", "mir", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich an Klippen deines Stolzes strande,", "tokens": ["Da\u00df", "ich", "an", "Klip\u00b7pen", "dei\u00b7nes", "Stol\u00b7zes", "stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der als der Liebe gr\u00f6\u00dfter Feind sich zeiget?", "tokens": ["Der", "als", "der", "Lie\u00b7be", "gr\u00f6\u00df\u00b7ter", "Feind", "sich", "zei\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ist es die G\u00f6ttlichkeit so s\u00fc\u00dfer Bande,", "tokens": ["Ist", "es", "die", "G\u00f6tt\u00b7lich\u00b7keit", "so", "s\u00fc\u00b7\u00dfer", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da stets die Liebe, wie vor Gott, sich neiget", "tokens": ["Da", "stets", "die", "Lie\u00b7be", ",", "wie", "vor", "Gott", ",", "sich", "nei\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PWAV", "APPR", "NN", "$,", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit heil'ger Furcht vor ihrem Gegenstande?", "tokens": ["Mit", "heil'\u00b7ger", "Furcht", "vor", "ih\u00b7rem", "Ge\u00b7gen\u00b7stan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}