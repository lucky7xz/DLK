{"textgrid.poem.54893": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Fr\u00fchling meiner Zeit und Anfang erster Tage", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling meiner Zeit und Anfang erster Tage", "tokens": ["Der", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Zeit", "und", "An\u00b7fang", "ers\u00b7ter", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschwand in Angst und Ach und rauher Traurigkeit/", "tokens": ["Ver\u00b7schwand", "in", "Angst", "und", "Ach", "und", "rau\u00b7her", "Trau\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Weinen und Verstand bejammerte die Plage/", "tokens": ["Mein", "Wei\u00b7nen", "und", "Ver\u00b7stand", "be\u00b7jam\u00b7mer\u00b7te", "die", "Pla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die mir auf dieser Welt die rauhe Noth bereit.", "tokens": ["Die", "mir", "auf", "die\u00b7ser", "Welt", "die", "rau\u00b7he", "Noth", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So bald sich die Vernunft fand in ein besser Wesen/", "tokens": ["So", "bald", "sich", "die", "Ver\u00b7nunft", "fand", "in", "ein", "bes\u00b7ser", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und der gezierte Leib zu etwas Kr\u00e4fften kam/", "tokens": ["Und", "der", "ge\u00b7zier\u00b7te", "Leib", "zu", "et\u00b7was", "Kr\u00e4ff\u00b7ten", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lernt ich der Menschen Leid aus fremden Unfall lesen/", "tokens": ["Lernt", "ich", "der", "Men\u00b7schen", "Leid", "aus", "frem\u00b7den", "Un\u00b7fall", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus dem ich eigne Furcht und Hoffnung an mich nahm/", "tokens": ["Aus", "dem", "ich", "eig\u00b7ne", "Furcht", "und", "Hoff\u00b7nung", "an", "mich", "nahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bald brach der Jammer an mit ungeheurem Leiden/", "tokens": ["Bald", "brach", "der", "Jam\u00b7mer", "an", "mit", "un\u00b7ge\u00b7heu\u00b7rem", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das schnelle Wetter fiel auf mich noch zarte Blum/", "tokens": ["Das", "schnel\u00b7le", "Wet\u00b7ter", "fiel", "auf", "mich", "noch", "zar\u00b7te", "Blum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ADV", "ADJA", "NE", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Man zwang von Grab und Haus der Eltern mich zu scheiden/", "tokens": ["Man", "zwang", "von", "Grab", "und", "Haus", "der", "El\u00b7tern", "mich", "zu", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und gab in fremde Macht mein freyes Eigenthum.", "tokens": ["Und", "gab", "in", "frem\u00b7de", "Macht", "mein", "frey\u00b7es", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da hab ich Welt und Tod bey zweyer M\u00e4nner Leichen/", "tokens": ["Da", "hab", "ich", "Welt", "und", "Tod", "bey", "zwey\u00b7er", "M\u00e4n\u00b7ner", "Lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und in der Einsamkeit der Freunde Treu erkennt.", "tokens": ["Und", "in", "der", "Ein\u00b7sam\u00b7keit", "der", "Freun\u00b7de", "Treu", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach Menschen! eure Gunst stirbt eh als wir erbleichen/", "tokens": ["Ach", "Men\u00b7schen", "!", "eu\u00b7re", "Gunst", "stirbt", "eh", "als", "wir", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPOSAT", "NN", "VVFIN", "KOUS", "KOUS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gleich wie der Thau verraucht wenn nun der Mittag brennt.", "tokens": ["Gleich", "wie", "der", "Thau", "ver\u00b7raucht", "wenn", "nun", "der", "Mit\u00b7tag", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Schmertzen grimme Qual/ des Vaterlandes Aschen/", "tokens": ["Der", "Schmert\u00b7zen", "grim\u00b7me", "Qual", "/", "des", "Va\u00b7ter\u00b7lan\u00b7des", "A\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dieselbe raubten mir die treffliche Gestalt/", "tokens": ["Die\u00b7sel\u00b7be", "raub\u00b7ten", "mir", "die", "treff\u00b7li\u00b7che", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Indem ich stets mich must aus heissen Thr\u00e4nen waschen/", "tokens": ["In\u00b7dem", "ich", "stets", "mich", "must", "aus", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "wa\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Verdorret ich und ward vor meinem Alter alt.", "tokens": ["Ver\u00b7dor\u00b7ret", "ich", "und", "ward", "vor", "mei\u00b7nem", "Al\u00b7ter", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier ruh ich dann die hier kaum eine Ruh genossen/", "tokens": ["Hier", "ruh", "ich", "dann", "die", "hier", "kaum", "ei\u00b7ne", "Ruh", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und finde was umsonst die tr\u00fcbe Welt begehrt:", "tokens": ["Und", "fin\u00b7de", "was", "um\u00b7sonst", "die", "tr\u00fc\u00b7be", "Welt", "be\u00b7gehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das Leben h\u00e4tt ich wohl noch viel zu fr\u00fch beschlossen/", "tokens": ["Das", "Le\u00b7ben", "h\u00e4tt", "ich", "wohl", "noch", "viel", "zu", "fr\u00fch", "be\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn Gott ein bessers mir dort oben nicht beschert.", "tokens": ["Wenn", "Gott", "ein", "bes\u00b7sers", "mir", "dort", "o\u00b7ben", "nicht", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "PPER", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}