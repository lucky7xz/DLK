{"textgrid.poem.47594": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[durstig sind wir, lieber Wirth]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durstig sind wir, lieber Wirth,", "tokens": ["Durs\u00b7tig", "sind", "wir", ",", "lie\u00b7ber", "Wirth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch im Bankerutte;", "tokens": ["Doch", "im", "Ban\u00b7ke\u00b7rut\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nimm, o nimm f\u00fcr baares Geld", "tokens": ["Nimm", ",", "o", "nimm", "f\u00fcr", "baa\u00b7res", "Geld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "FM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre blaue Kutte!", "tokens": ["Uns\u00b7re", "blau\u00b7e", "Kut\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bu\u00dfe, Contemplation,", "tokens": ["Bu\u00b7\u00dfe", ",", "Con\u00b7tem\u00b7pla\u00b7ti\u00b7on", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kanzel und Katheder,", "tokens": ["Kan\u00b7zel", "und", "Ka\u00b7the\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Lieber Gott, was sind sie werth?", "tokens": ["Lie\u00b7ber", "Gott", ",", "was", "sind", "sie", "werth", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PWS", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Hagebutte.", "tokens": ["Kei\u00b7ne", "Ha\u00b7ge\u00b7but\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber hoch gepriesen sei", "tokens": ["A\u00b7ber", "hoch", "ge\u00b7prie\u00b7sen", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rebe mir und Winzer,", "tokens": ["Re\u00b7be", "mir", "und", "Win\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kelterfa\u00df und Kelterer", "tokens": ["Kel\u00b7ter\u00b7fa\u00df", "und", "Kel\u00b7te\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die volle Butte!", "tokens": ["Und", "die", "vol\u00b7le", "But\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Das reale Resultat", "tokens": ["Das", "re\u00b7a\u00b7le", "Re\u00b7sul\u00b7tat"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser edlen M\u00fchen,", "tokens": ["Die\u00b7ser", "ed\u00b7len", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Es gedeih' uns fr\u00fch und spat,", "tokens": ["Es", "ge\u00b7deih'", "uns", "fr\u00fch", "und", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dem Kind die Dutte!", "tokens": ["Wie", "dem", "Kind", "die", "Dut\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdas Geb\u00e4ude deines Seins", "tokens": ["\u00bb", "das", "Ge\u00b7b\u00e4u\u00b7de", "dei\u00b7nes", "Seins"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzet es!\u00ab Es sei so!", "tokens": ["St\u00fcr\u00b7zet", "es", "!", "\u00ab", "Es", "sei", "so", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Einen sch\u00f6nen Schatz vielleicht", "tokens": ["Ei\u00b7nen", "sch\u00f6\u00b7nen", "Schatz", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Find' ich in dem Schutte.", "tokens": ["Find'", "ich", "in", "dem", "Schut\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}