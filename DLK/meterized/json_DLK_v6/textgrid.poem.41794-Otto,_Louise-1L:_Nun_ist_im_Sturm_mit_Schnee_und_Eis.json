{"textgrid.poem.41794": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun ist im Sturm mit Schnee und Eis", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist im Sturm mit Schnee und Eis", "tokens": ["Nun", "ist", "im", "Sturm", "mit", "Schnee", "und", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Winter angekommen,", "tokens": ["Der", "Win\u00b7ter", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat auf tyrannisches Gehei\u00df", "tokens": ["Hat", "auf", "ty\u00b7ran\u00b7ni\u00b7sches", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00fcten all genommen.", "tokens": ["Die", "Bl\u00fc\u00b7ten", "all", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sind dahin mit einem mal", "tokens": ["Sie", "sind", "da\u00b7hin", "mit", "ei\u00b7nem", "mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "APPR", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4ngen welk hernieder,", "tokens": ["Und", "h\u00e4n\u00b7gen", "welk", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es weckt kein milder Sonnenstrahl", "tokens": ["Es", "weckt", "kein", "mil\u00b7der", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frostgetroffnen wieder.", "tokens": ["Die", "Frost\u00b7ge\u00b7troff\u00b7nen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Glanz, ihr Duft, ihr Leben schwand", "tokens": ["Ihr", "Glanz", ",", "ihr", "Duft", ",", "ihr", "Le\u00b7ben", "schwand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00f6d' sind Flur und Garten,", "tokens": ["Und", "\u00f6d'", "sind", "Flur", "und", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur wei\u00dfen W\u00fcste ward das Land,", "tokens": ["Zur", "wei\u00b7\u00dfen", "W\u00fcs\u00b7te", "ward", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fl\u00fcsse selbst erstarrten.", "tokens": ["Die", "Fl\u00fcs\u00b7se", "selbst", "er\u00b7starr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So sinken in die kalte Gruft", "tokens": ["So", "sin\u00b7ken", "in", "die", "kal\u00b7te", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die letzten Blumenleichen,", "tokens": ["Die", "letz\u00b7ten", "Blu\u00b7men\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und harren bis der Lenz sie ruft", "tokens": ["Und", "har\u00b7ren", "bis", "der", "Lenz", "sie", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihrem Grab zu steigen.", "tokens": ["Aus", "ih\u00b7rem", "Grab", "zu", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch kann der Blumeugeister Schar", "tokens": ["Doch", "kann", "der", "Blu\u00b7meu\u00b7geis\u00b7ter", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl n\u00e4chtlich um noch gehen \u2013", "tokens": ["Wohl", "n\u00e4cht\u00b7lich", "um", "noch", "ge\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In kalter Mondnacht, hell und klar", "tokens": ["In", "kal\u00b7ter", "Mond\u00b7nacht", ",", "hell", "und", "klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind sie gar oft zu sehen.", "tokens": ["Sind", "sie", "gar", "oft", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie kommen aus dem Grab hervor", "tokens": ["Sie", "kom\u00b7men", "aus", "dem", "Grab", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie neckende Gespenster,", "tokens": ["Wie", "ne\u00b7cken\u00b7de", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und bl\u00fchen \u2013 ein krystall'ner Flor \u2013", "tokens": ["Und", "bl\u00fc\u00b7hen", "\u2013", "ein", "kry\u00b7stall'\u00b7ner", "Flor", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "An dem gefrornen Fenster.", "tokens": ["An", "dem", "ge\u00b7fror\u00b7nen", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und rufen die Erinnrung wach", "tokens": ["Und", "ru\u00b7fen", "die", "E\u00b7rinn\u00b7rung", "wach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "An alle Sommerstunden,", "tokens": ["An", "al\u00b7le", "Som\u00b7mer\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Menschenhand die Bl\u00fcmlein brach", "tokens": ["Wo", "Men\u00b7schen\u00b7hand", "die", "Bl\u00fcm\u00b7lein", "brach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie zum Kranz gewunden \u2013", "tokens": ["Und", "sie", "zum", "Kranz", "ge\u00b7wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VAPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wo Menschenfu\u00df sie gar zertrat,", "tokens": ["Wo", "Men\u00b7schen\u00b7fu\u00df", "sie", "gar", "zer\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht achtend auf ihr Flehen \u2013", "tokens": ["Nicht", "ach\u00b7tend", "auf", "ihr", "Fle\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es l\u00e4\u00dft zu r\u00e4chen solche That,", "tokens": ["Es", "l\u00e4\u00dft", "zu", "r\u00e4\u00b7chen", "sol\u00b7che", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Geisterschar sich sehen.", "tokens": ["Die", "Geis\u00b7ter\u00b7schar", "sich", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und mahnt mit gl\u00e4nzend heller Schrift:", "tokens": ["Und", "mahnt", "mit", "gl\u00e4n\u00b7zend", "hel\u00b7ler", "Schrift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdein eignes Thun bewache,", "tokens": ["\u00bb", "dein", "eig\u00b7nes", "Thun", "be\u00b7wa\u00b7che", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit dich nicht im Winter trifft", "tokens": ["Da\u00b7mit", "dich", "nicht", "im", "Win\u00b7ter", "trifft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blumengeister Rache!\u00ab", "tokens": ["Der", "Blu\u00b7men\u00b7geis\u00b7ter", "Ra\u00b7che", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}