{"textgrid.poem.40778": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herbstlich \u00fcber Asperns Fluren schien die Sonne m\u00fcd' und lau,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herbstlich \u00fcber Asperns Fluren schien die Sonne m\u00fcd' und lau,", "tokens": ["Herbst\u00b7lich", "\u00fc\u00b7ber", "As\u00b7perns", "Flu\u00b7ren", "schien", "die", "Son\u00b7ne", "m\u00fcd'", "und", "lau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "St\u00f6rche schifften schon nach S\u00fcden durch der L\u00fcfte ruhig Blau,", "tokens": ["St\u00f6r\u00b7che", "schiff\u00b7ten", "schon", "nach", "S\u00fc\u00b7den", "durch", "der", "L\u00fcf\u00b7te", "ru\u00b7hig", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ueber stille weite Felder schritt ich einsam, unbelauscht,", "tokens": ["Ue\u00b7ber", "stil\u00b7le", "wei\u00b7te", "Fel\u00b7der", "schritt", "ich", "ein\u00b7sam", ",", "un\u00b7be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und mit mir ein kalter Herbstwind, der durch fahle Stoppeln rauscht.", "tokens": ["Und", "mit", "mir", "ein", "kal\u00b7ter", "Herbst\u00b7wind", ",", "der", "durch", "fah\u00b7le", "Stop\u00b7peln", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Dachte dessen j\u00fcngst der Landmann, als er hier die Garben wand,", "tokens": ["Dach\u00b7te", "des\u00b7sen", "j\u00fcngst", "der", "Land\u00b7mann", ",", "als", "er", "hier", "die", "Gar\u00b7ben", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df in einem Menschenherzen manche ihrer Wurzeln stand?", "tokens": ["Da\u00df", "in", "ei\u00b7nem", "Men\u00b7schen\u00b7her\u00b7zen", "man\u00b7che", "ih\u00b7rer", "Wur\u00b7zeln", "stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denkt der St\u00e4dter, wenn beim Mahle er sein wei\u00dfes Brod genie\u00dft,", "tokens": ["Denkt", "der", "St\u00e4d\u00b7ter", ",", "wenn", "beim", "Mah\u00b7le", "er", "sein", "wei\u00b7\u00dfes", "Brod", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "APPRART", "NN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df ged\u00fcngt es mit dem Blute eines Heldenbruders ist?", "tokens": ["Da\u00df", "ge\u00b7d\u00fcngt", "es", "mit", "dem", "Blu\u00b7te", "ei\u00b7nes", "Hel\u00b7den\u00b7bru\u00b7ders", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Aus der Lava, die einst gl\u00fchend vom Vesuv herniederquoll,", "tokens": ["Aus", "der", "La\u00b7va", ",", "die", "einst", "gl\u00fc\u00b7hend", "vom", "Ve\u00b7suv", "her\u00b7nie\u00b7der\u00b7quoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Bl\u00fch'n, wie Leben aus dem Tode, saft'ge Reben, gr\u00fcn und voll;", "tokens": ["Bl\u00fch'n", ",", "wie", "Le\u00b7ben", "aus", "dem", "To\u00b7de", ",", "saft'\u00b7ge", "Re\u00b7ben", ",", "gr\u00fcn", "und", "voll", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch die ihren Wein einst trinken unter k\u00fchlem Laubendach,", "tokens": ["Doch", "die", "ih\u00b7ren", "Wein", "einst", "trin\u00b7ken", "un\u00b7ter", "k\u00fch\u00b7lem", "Lau\u00b7ben\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dem Vesuv und seinen Schrecken sinnen sie wohl schwerlich nach!", "tokens": ["Dem", "Ve\u00b7suv", "und", "sei\u00b7nen", "Schre\u00b7cken", "sin\u00b7nen", "sie", "wohl", "schwer\u00b7lich", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Hier auch hat all' seine Schrecken ausgetobt einst ein Vulkan,", "tokens": ["Hier", "auch", "hat", "all'", "sei\u00b7ne", "Schre\u00b7cken", "aus\u00b7ge\u00b7tobt", "einst", "ein", "Vul\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NE", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Blut'ge, gl\u00fch'nde Lavafluthen \u00fcberstr\u00f6mten rings den Plan,", "tokens": ["Blut'\u00b7ge", ",", "gl\u00fch'n\u00b7de", "La\u00b7va\u00b7flut\u00b7hen", "\u00fc\u00b7ber\u00b7str\u00f6m\u00b7ten", "rings", "den", "Plan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schwarzer Rauch und Nachtgew\u00f6lke h\u00fcllte tief den Himmel ein,", "tokens": ["Schwar\u00b7zer", "Rauch", "und", "Nacht\u00b7ge\u00b7w\u00f6l\u00b7ke", "h\u00fcll\u00b7te", "tief", "den", "Him\u00b7mel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wetterschl\u00e4ge krachten donnernd, Blitze zuckten flammend drein!", "tokens": ["Wet\u00b7ter\u00b7schl\u00e4\u00b7ge", "krach\u00b7ten", "don\u00b7nernd", ",", "Blit\u00b7ze", "zuck\u00b7ten", "flam\u00b7mend", "drein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wie dort am Vesuv die Lava einst manch' heitre Stadt verschlang,", "tokens": ["Wie", "dort", "am", "Ve\u00b7suv", "die", "La\u00b7va", "einst", "man\u00b7ch'", "heit\u00b7re", "Stadt", "ver\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "So begrub sie viel der Edlen hier die weite Flur entlang;", "tokens": ["So", "be\u00b7grub", "sie", "viel", "der", "Ed\u00b7len", "hier", "die", "wei\u00b7te", "Flur", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hundert St\u00e4dte zu beleben, reichte wahrlich ihre Zahl,", "tokens": ["Hun\u00b7dert", "St\u00e4d\u00b7te", "zu", "be\u00b7le\u00b7ben", ",", "reich\u00b7te", "wahr\u00b7lich", "ih\u00b7re", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und nicht minder sch\u00f6n glomm ihnen noch des Lebens sonn'ger Strahl!", "tokens": ["Und", "nicht", "min\u00b7der", "sch\u00f6n", "glomm", "ih\u00b7nen", "noch", "des", "Le\u00b7bens", "sonn'\u00b7ger", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Gleich an frommer Kraft und Weisheit jenem edlen Plinius,", "tokens": ["Gleich", "an", "from\u00b7mer", "Kraft", "und", "Weis\u00b7heit", "je\u00b7nem", "ed\u00b7len", "Pli\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN", "PDAT", "ADJA", "NE", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Der dort rettend seine Mutter trug durch Nacht und Lavagu\u00df,", "tokens": ["Der", "dort", "ret\u00b7tend", "sei\u00b7ne", "Mut\u00b7ter", "trug", "durch", "Nacht", "und", "La\u00b7va\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Also Carl, du hoher Sieger, trugst du k\u00fchn und glorreich da", "tokens": ["Al\u00b7so", "Carl", ",", "du", "ho\u00b7her", "Sie\u00b7ger", ",", "trugst", "du", "k\u00fchn", "und", "glor\u00b7reich", "da"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aus den Flammen und den Schrecken deine Mutter Austria!", "tokens": ["Aus", "den", "Flam\u00b7men", "und", "den", "Schre\u00b7cken", "dei\u00b7ne", "Mut\u00b7ter", "Aust\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+---", "measure": "unknown.measure.hexa"}}, "stanza.7": {"line.1": {"text": "Manch' gewaltiges Jahrhundert schritt schon am Vesuv vorbei;", "tokens": ["Man\u00b7ch'", "ge\u00b7wal\u00b7ti\u00b7ges", "Jahr\u00b7hun\u00b7dert", "schritt", "schon", "am", "Ve\u00b7suv", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Sieh, der fernsten Enkel Spaten schl\u00e4gt der Lava Krust' entzwei,", "tokens": ["Sieh", ",", "der", "ferns\u00b7ten", "En\u00b7kel", "Spa\u00b7ten", "schl\u00e4gt", "der", "La\u00b7va", "Krust'", "ent\u00b7zwei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und es steigt aus Schutt und Asche eine heitre Stadt ans Licht,", "tokens": ["Und", "es", "steigt", "aus", "Schutt", "und", "A\u00b7sche", "ei\u00b7ne", "heit\u00b7re", "Stadt", "ans", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Manch' ein G\u00f6tterbild und Tempel, manch' unsterbliches Gedicht!", "tokens": ["Man\u00b7ch'", "ein", "G\u00f6t\u00b7ter\u00b7bild", "und", "Tem\u00b7pel", ",", "man\u00b7ch'", "uns\u00b7terb\u00b7li\u00b7ches", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "KON", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Oestreichs Herkulanum nenn' ich, ihr Gefilde Asperns, euch!", "tokens": ["O\u00b7e\u00b7streichs", "Her\u00b7ku\u00b7la\u00b7num", "nenn'", "ich", ",", "ihr", "Ge\u00b7fil\u00b7de", "As\u00b7perns", ",", "euch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "NE", "$,", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "W\u00e4r' an edlen heil'gen Sch\u00e4tzen euer Schoo\u00df wohl minder reich?", "tokens": ["W\u00e4r'", "an", "ed\u00b7len", "heil'\u00b7gen", "Sch\u00e4t\u00b7zen", "eu\u00b7er", "Schoo\u00df", "wohl", "min\u00b7der", "reich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "ADJA", "NN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wahrlich, stieg' in eure Tiefen rechten Sinns der rechte Mann,", "tokens": ["Wahr\u00b7lich", ",", "stieg'", "in", "eu\u00b7re", "Tie\u00b7fen", "rech\u00b7ten", "Sinns", "der", "rech\u00b7te", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bald das G\u00f6tterbild der Freiheit br\u00e4cht' er uns ans Licht hinan! \u2013", "tokens": ["Bald", "das", "G\u00f6t\u00b7ter\u00b7bild", "der", "Frei\u00b7heit", "br\u00e4cht'", "er", "uns", "ans", "Licht", "hi\u00b7nan", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Wallt dann wieder einst durchs weite reiche Saatgefild mein Fu\u00df,", "tokens": ["Wallt", "dann", "wie\u00b7der", "einst", "durchs", "wei\u00b7te", "rei\u00b7che", "Saat\u00b7ge\u00b7fild", "mein", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "APPRART", "ADJA", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "O dann nickt wohl jede Aehre mit dem Haupt mir heitren Gru\u00df;", "tokens": ["O", "dann", "nickt", "wohl", "je\u00b7de", "A\u00b7eh\u00b7re", "mit", "dem", "Haupt", "mir", "hei\u00b7tren", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Und wie Geisterharfen s\u00e4uselt's aus den goldnen Halmen leis:", "tokens": ["Und", "wie", "Geis\u00b7ter\u00b7har\u00b7fen", "s\u00e4u\u00b7selt's", "aus", "den", "gold\u00b7nen", "Hal\u00b7men", "leis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "\u00bbnicht umsonst flo\u00df unser Herzblut, denn es trug euch sch\u00f6nen Preis!\u00ab", "tokens": ["\u00bb", "nicht", "um\u00b7sonst", "flo\u00df", "un\u00b7ser", 