{"dta.poem.10553": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Poseidon .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonnenlichter spielten", "tokens": ["Die", "Son\u00b7nen\u00b7lich\u00b7ter", "spiel\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ueber das weithinrollende Meer;", "tokens": ["Ue\u00b7ber", "das", "weit\u00b7hin\u00b7rol\u00b7len\u00b7de", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Fern' auf der Rhede gl\u00e4nzte das Schiff,", "tokens": ["Fern'", "auf", "der", "Rhe\u00b7de", "gl\u00e4nz\u00b7te", "das", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das mich zur Heimath tragen sollte;", "tokens": ["Das", "mich", "zur", "Hei\u00b7math", "tra\u00b7gen", "soll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aber es fehlte an gutem Fahrwind,", "tokens": ["A\u00b7ber", "es", "fehl\u00b7te", "an", "gu\u00b7tem", "Fahr\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und ich sa\u00df noch ruhig auf wei\u00dfer D\u00fchne,", "tokens": ["Und", "ich", "sa\u00df", "noch", "ru\u00b7hig", "auf", "wei\u00b7\u00dfer", "D\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Am einsamen Strand,", "tokens": ["Am", "ein\u00b7sa\u00b7men", "Strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und ich las das Lied vom Odysseus,", "tokens": ["Und", "ich", "las", "das", "Lied", "vom", "O\u00b7dys\u00b7seus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Das alte, das ewig junge Lied,", "tokens": ["Das", "al\u00b7te", ",", "das", "e\u00b7wig", "jun\u00b7ge", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Aus dessen meerdurchrauschten Bl\u00e4ttern", "tokens": ["Aus", "des\u00b7sen", "meer\u00b7durc\u00b7hrauschten", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Mir freudig entgegenstieg", "tokens": ["Mir", "freu\u00b7dig", "ent\u00b7ge\u00b7gen\u00b7stieg"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Der Athem der G\u00f6tter,", "tokens": ["Der", "A\u00b7them", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und der leuchtende Menschenfr\u00fchling,", "tokens": ["Und", "der", "leuch\u00b7ten\u00b7de", "Men\u00b7schen\u00b7fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Und der bl\u00fchende Himmel von Hellas.", "tokens": ["Und", "der", "bl\u00fc\u00b7hen\u00b7de", "Him\u00b7mel", "von", "Hel\u00b7las", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Mein edles Herz begleitete treulich", "tokens": ["Mein", "ed\u00b7les", "Herz", "be\u00b7glei\u00b7te\u00b7te", "treu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Sohn des Laertes, in Irrfahrt und Drangsal,", "tokens": ["Den", "Sohn", "des", "Laer\u00b7tes", ",", "in", "Irr\u00b7fahrt", "und", "Dran\u00b7gsal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Setzte sich mit ihm, seelenbek\u00fcmmert,", "tokens": ["Setz\u00b7te", "sich", "mit", "ihm", ",", "see\u00b7len\u00b7be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$,", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "An gastliche Heerde,", "tokens": ["An", "gast\u00b7li\u00b7che", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wo K\u00f6niginnen Purpur spinnen,", "tokens": ["Wo", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "Pur\u00b7pur", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und half ihm l\u00fcgen und gl\u00fccklich entrinnen", "tokens": ["Und", "half", "ihm", "l\u00fc\u00b7gen", "und", "gl\u00fcck\u00b7lich", "ent\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aus Riesenh\u00f6hlen und Nymphenarmen,", "tokens": ["Aus", "Rie\u00b7sen\u00b7h\u00f6h\u00b7len", "und", "Nym\u00b7phen\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Folgte ihm nach in kimmerische Nacht,", "tokens": ["Folg\u00b7te", "ihm", "nach", "in", "kim\u00b7me\u00b7ri\u00b7sche", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und in Sturm und Schiffbruch,", "tokens": ["Und", "in", "Sturm", "und", "Schiff\u00b7bruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und duldete mit ihm uns\u00e4gliches Elend.", "tokens": ["Und", "dul\u00b7de\u00b7te", "mit", "ihm", "un\u00b7s\u00e4g\u00b7li\u00b7ches", "E\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Seufzend sprach ich: Du b\u00f6ser Poseidon,", "tokens": ["Seuf\u00b7zend", "sprach", "ich", ":", "Du", "b\u00f6\u00b7ser", "Po\u00b7sei\u00b7don", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dein Zorn ist furchtbar,", "tokens": ["Dein", "Zorn", "ist", "furcht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und mir selber bangt", "tokens": ["Und", "mir", "sel\u00b7ber", "bangt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ob der eignen Heimkehr.", "tokens": ["Ob", "der", "eig\u00b7nen", "Heim\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Kaum sprach ich die Worte,", "tokens": ["Kaum", "sprach", "ich", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da sch\u00e4umte das Meer,", "tokens": ["Da", "sch\u00e4um\u00b7te", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und aus den wei\u00dfen Wellen stieg", "tokens": ["Und", "aus", "den", "wei\u00b7\u00dfen", "Wel\u00b7len", "stieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das schilfbekr\u00e4nzte Haupt des Meergotts,", "tokens": ["Das", "schilf\u00b7be\u00b7kr\u00e4nz\u00b7te", "Haupt", "des", "Meer\u00b7gotts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00f6hnisch rief er:", "tokens": ["Und", "h\u00f6h\u00b7nisch", "rief", "er", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "F\u00fcrchte dich nicht, Poetlein!", "tokens": ["F\u00fcrch\u00b7te", "dich", "nicht", ",", "Poet\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ich will nicht im g'ringsten gef\u00e4hrden", "tokens": ["Ich", "will", "nicht", "im", "g'\u00b7rings\u00b7ten", "ge\u00b7f\u00e4hr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dein armes Schiffchen,", "tokens": ["Dein", "ar\u00b7mes", "Schiff\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und nicht dein liebes Leben be\u00e4ngst'gen", "tokens": ["Und", "nicht", "dein", "lie\u00b7bes", "Le\u00b7ben", "be\u00b7\u00e4ngst'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Mit allzubedenklichem Schaukeln.", "tokens": ["Mit", "all\u00b7zu\u00b7be\u00b7denk\u00b7li\u00b7chem", "Schau\u00b7keln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Denn Du, Poetlein, hast nie mich erz\u00fcrnt,", "tokens": ["Denn", "Du", ",", "Poet\u00b7lein", ",", "hast", "nie", "mich", "er\u00b7z\u00fcrnt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du hast kein einziges Th\u00fcrmchen verletzt", "tokens": ["Du", "hast", "kein", "ein\u00b7zi\u00b7ges", "Th\u00fcrm\u00b7chen", "ver\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "An Priamos heiliger Veste,", "tokens": ["An", "Pri\u00b7a\u00b7mos", "hei\u00b7li\u00b7ger", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Kein einziges H\u00e4rchen hast du versengt", "tokens": ["Kein", "ein\u00b7zi\u00b7ges", "H\u00e4r\u00b7chen", "hast", "du", "ver\u00b7sengt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Am Aug' meines Sohns Polyphemos,", "tokens": ["Am", "Aug'", "mei\u00b7nes", "Sohns", "Po\u00b7ly\u00b7phe\u00b7mos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und dich hat niemals rathend besch\u00fctzt", "tokens": ["Und", "dich", "hat", "nie\u00b7mals", "ra\u00b7thend", "be\u00b7sch\u00fctzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die G\u00f6ttin der Klugheit, Pallas Athene.", "tokens": ["Die", "G\u00f6t\u00b7tin", "der", "Klug\u00b7heit", ",", "Pal\u00b7las", "A\u00b7the\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NE", "NE", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Also rief Poseidon", "tokens": ["Al\u00b7so", "rief", "Po\u00b7sei\u00b7don"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und tauchte zur\u00fcck in's Meer;", "tokens": ["Und", "tauch\u00b7te", "zu\u00b7r\u00fcck", "in's", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und \u00fcber den groben Seemannswitz", "tokens": ["Und", "\u00fc\u00b7ber", "den", "gro\u00b7ben", "See\u00b7manns\u00b7witz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lachten unter dem Wasser", "tokens": ["Lach\u00b7ten", "un\u00b7ter", "dem", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Amphitrite, das plumpe Fischweib,", "tokens": ["Am\u00b7phit\u00b7ri\u00b7te", ",", "das", "plum\u00b7pe", "Fisc\u00b7hweib", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und die dummen T\u00f6chter des Nereus.", "tokens": ["Und", "die", "dum\u00b7men", "T\u00f6ch\u00b7ter", "des", "Ne\u00b7reus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}