{"textgrid.poem.37599": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "2. Am Schlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gern seh' ich die Namen der Freunde", "tokens": ["Gern", "seh'", "ich", "die", "Na\u00b7men", "der", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In meinem Stammbuch hier an,", "tokens": ["In", "mei\u00b7nem", "Stamm\u00b7buch", "hier", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und bete mit dieser Gemeinde.", "tokens": ["Und", "be\u00b7te", "mit", "die\u00b7ser", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dies Kirchlein ich schm\u00fccken kann:", "tokens": ["Dies", "Kirch\u00b7lein", "ich", "schm\u00fc\u00b7cken", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mit Bildern und sch\u00f6nen Zeichen,", "tokens": ["Mit", "Bil\u00b7dern", "und", "sch\u00f6\u00b7nen", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit manchem herzlichen Wort,", "tokens": ["Mit", "man\u00b7chem", "herz\u00b7li\u00b7chen", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Vor dem die Zeichen weichen,", "tokens": ["Vor", "dem", "die", "Zei\u00b7chen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und auch der einsame Ort.", "tokens": ["Und", "auch", "der", "ein\u00b7sa\u00b7me", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und bis die Augen erblinden,", "tokens": ["Und", "bis", "die", "Au\u00b7gen", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und bis der Tag mir versinkt,", "tokens": ["Und", "bis", "der", "Tag", "mir", "ver\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Soll keiner vor mir verschwinden,", "tokens": ["Soll", "kei\u00b7ner", "vor", "mir", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Der mir je freundlich gewinkt;", "tokens": ["Der", "mir", "je", "freund\u00b7lich", "ge\u00b7winkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Er mag noch wandeln und wirken,", "tokens": ["Er", "mag", "noch", "wan\u00b7deln", "und", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Und schauen das ewige Licht,", "tokens": ["Und", "schau\u00b7en", "das", "e\u00b7wi\u00b7ge", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Er mag in andern Bezirken", "tokens": ["Er", "mag", "in", "an\u00b7dern", "Be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Verh\u00fcllen das bleiche Gesicht.", "tokens": ["Ver\u00b7h\u00fcl\u00b7len", "das", "blei\u00b7che", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Da\u00df hier auf Erden die ", "tokens": ["Da\u00df", "hier", "auf", "Er\u00b7den", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ein moosiger Eichenstamm,", "tokens": ["Ein", "moo\u00b7si\u00b7ger", "Ei\u00b7chen\u00b7stamm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Braucht viele Jahre zur Weihe,", "tokens": ["Braucht", "vie\u00b7le", "Jah\u00b7re", "zur", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.20": {"text": "Und st\u00fcrzt in schneller Flamm',", "tokens": ["Und", "st\u00fcrzt", "in", "schnel\u00b7ler", "Flamm'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Die Flamme steiget zur Bl\u00e4ue,", "tokens": ["Die", "Flam\u00b7me", "stei\u00b7get", "zur", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Und \u00fcber die Bl\u00e4ue hinaus,", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Bl\u00e4u\u00b7e", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.23": {"text": "Da findet auf Sternen die Treue", "tokens": ["Da", "fin\u00b7det", "auf", "Ster\u00b7nen", "die", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Ein gl\u00e4nzend gezimmertes Haus.", "tokens": ["Ein", "gl\u00e4n\u00b7zend", "ge\u00b7zim\u00b7mer\u00b7tes", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.25": {"text": "Es treiben wohl Hirten die Heerde,", "tokens": ["Es", "trei\u00b7ben", "wohl", "Hir\u00b7ten", "die", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.26": {"text": "So weit der Himmel ist blau,", "tokens": ["So", "weit", "der", "Him\u00b7mel", "ist", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Und meinen sich eigen die Erde,", "tokens": ["Und", "mei\u00b7nen", "sich", "ei\u00b7gen", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.28": {"text": "Gl\u00e4nzt himmlisch die bl\u00fchende Au,", "tokens": ["Gl\u00e4nzt", "himm\u00b7lisch", "die", "bl\u00fc\u00b7hen\u00b7de", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.29": {"text": "Es treiben auch Fischer den Nachen,", "tokens": ["Es", "trei\u00b7ben", "auch", "Fi\u00b7scher", "den", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "So weit die Meere sind blau", "tokens": ["So", "weit", "die", "Mee\u00b7re", "sind", "blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Und spielen am Todesrachen", "tokens": ["Und", "spie\u00b7len", "am", "To\u00b7des\u00b7ra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Wie Fische in Netzes Bau;", "tokens": ["Wie", "Fi\u00b7sche", "in", "Net\u00b7zes", "Bau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Auch J\u00e4ger kennen nicht Gr\u00e4nzen,", "tokens": ["Auch", "J\u00e4\u00b7ger", "ken\u00b7nen", "nicht", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "So weit der Wald sie verbirgt,", "tokens": ["So", "weit", "der", "Wald", "sie", "ver\u00b7birgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Mit bunten Federn sich kr\u00e4nzen", "tokens": ["Mit", "bun\u00b7ten", "Fe\u00b7dern", "sich", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Von V\u00f6geln, die jubelnd erw\u00fcrgt;", "tokens": ["Von", "V\u00f6\u00b7geln", ",", "die", "ju\u00b7belnd", "er\u00b7w\u00fcrgt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.37": {"text": "Doch h\u00f6r', der Hirt ist gepf\u00e4ndet,", "tokens": ["Doch", "h\u00f6r'", ",", "der", "Hirt", "ist", "ge\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Der Fischer versenkt in das Meer,", "tokens": ["Der", "Fi\u00b7scher", "ver\u00b7senkt", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.39": {"text": "Der J\u00e4ger ist heimgesendet,", "tokens": ["Der", "J\u00e4\u00b7ger", "ist", "heim\u00b7ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Ihn dr\u00fcckte der Raub zu schwer.", "tokens": ["Ihn", "dr\u00fcck\u00b7te", "der", "Raub", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Nur treue Liebe sie dringet", "tokens": ["Nur", "treu\u00b7e", "Lie\u00b7be", "sie", "drin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Noch \u00fcber das Blau hinaus,", "tokens": ["Noch", "\u00fc\u00b7ber", "das", "Blau", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Sich \u00fcber die Meere erschwinget,", "tokens": ["Sich", "\u00fc\u00b7ber", "die", "Mee\u00b7re", "er\u00b7schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.44": {"text": "Und \u00fcber der W\u00e4lder Gebraus,", "tokens": ["Und", "\u00fc\u00b7ber", "der", "W\u00e4l\u00b7der", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.45": {"text": "Und zu den Sternen sich hebet,", "tokens": ["Und", "zu", "den", "Ster\u00b7nen", "sich", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.46": {"text": "Und freuet sich da der Welt,", "tokens": ["Und", "freu\u00b7et", "sich", "da", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "Was war, was wird, was lebet,", "tokens": ["Was", "war", ",", "was", "wird", ",", "was", "le\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWS", "VAFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Ist vor ihr ausgestellt.", "tokens": ["Ist", "vor", "ihr", "aus\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}