{"textgrid.poem.62240": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[mir tr\u00e4umte, da\u00df ich stund]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte, da\u00df ich stund", "tokens": ["Mir", "tr\u00e4um\u00b7te", ",", "da\u00df", "ich", "stund"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An eines Teiches Grund,", "tokens": ["An", "ei\u00b7nes", "Tei\u00b7ches", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sieh', mein Mutterlieb,", "tokens": ["Und", "sieh'", ",", "mein", "Mut\u00b7ter\u00b7lieb", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es schaute drinn so tr\u00fcb'!", "tokens": ["Es", "schau\u00b7te", "drinn", "so", "tr\u00fcb'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbwas machst so tr\u00fcb' Gesicht\u00ab \u2013", "tokens": ["\u00bb", "was", "machst", "so", "tr\u00fcb'", "Ge\u00b7sicht", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fragt ich, \u00bbich fass' es nicht\u00ab.", "tokens": ["Fragt", "ich", ",", "\u00bb", "ich", "fass'", "es", "nicht", "\u00ab", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbbist gut\u00ab, \u2013 sprach sie, \u2013 \u00bbmein Kind,", "tokens": ["\u00bb", "bist", "gut", "\u00ab", ",", "\u2013", "sprach", "sie", ",", "\u2013", "\u00bb", "mein", "Kind", ","], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$(", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "$(", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wei\u00dft nicht, wie b\u00f6s' sie sind;", "tokens": ["Wei\u00dft", "nicht", ",", "wie", "b\u00f6s'", "sie", "sind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du fa\u00dft das B\u00f6se nicht,", "tokens": ["Du", "fa\u00dft", "das", "B\u00f6\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "D'rum mach' ich tr\u00fcb' Gesicht\u00ab.", "tokens": ["D'\u00b7rum", "mach'", "ich", "tr\u00fcb'", "Ge\u00b7sicht", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So sprach mein Mutterlieb,", "tokens": ["So", "sprach", "mein", "Mut\u00b7ter\u00b7lieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ich erwachte tr\u00fcb'.", "tokens": ["Und", "ich", "er\u00b7wach\u00b7te", "tr\u00fcb'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}