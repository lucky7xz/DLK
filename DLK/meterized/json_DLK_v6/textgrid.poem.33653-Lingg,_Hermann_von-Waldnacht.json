{"textgrid.poem.33653": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Waldnacht", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie uralt weht's, wie l\u00e4ngst verklungen", "tokens": ["Wie", "ur\u00b7alt", "weht's", ",", "wie", "l\u00e4ngst", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "$,", "PWAV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem tiefen Waldesgr\u00fcn,", "tokens": ["In", "die\u00b7sem", "tie\u00b7fen", "Wal\u00b7des\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tr\u00e4umen voller D\u00e4mmerungen,", "tokens": ["Ein", "Tr\u00e4u\u00b7men", "vol\u00b7ler", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein dichtverschlungnes Wunderbl\u00fchn!", "tokens": ["Ein", "dicht\u00b7ver\u00b7schlung\u00b7nes", "Wun\u00b7der\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch dieser Laubgew\u00f6lbe Mitten,", "tokens": ["Durch", "die\u00b7ser", "Laub\u00b7ge\u00b7w\u00f6l\u00b7be", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprich, bist du schon auf irrer Bahn", "tokens": ["Sprich", ",", "bist", "du", "schon", "auf", "ir\u00b7rer", "Bahn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Mitternacht dahingeschritten?", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "da\u00b7hin\u00b7ge\u00b7schrit\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann hebt auch hier der Zauber an.", "tokens": ["Dann", "hebt", "auch", "hier", "der", "Zau\u00b7ber", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des Wolfs durchschossne Augen funkeln,", "tokens": ["Des", "Wolfs", "durch\u00b7schoss\u00b7ne", "Au\u00b7gen", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um schwarze Wipfel kreist der Weih,", "tokens": ["Um", "schwar\u00b7ze", "Wip\u00b7fel", "kreist", "der", "Weih", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Moor auf Felsen gl\u00fcht im Dunkeln", "tokens": ["Im", "Moor", "auf", "Fel\u00b7sen", "gl\u00fcht", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hirsche moderndes Geweih.", "tokens": ["Der", "Hir\u00b7sche", "mo\u00b7dern\u00b7des", "Ge\u00b7weih", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vor\u00fcber jagt auf Flammenhufen", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "jagt", "auf", "Flam\u00b7men\u00b7hu\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erlk\u00f6nig sein goldm\u00e4hnig Ro\u00df;", "tokens": ["Erl\u00b7k\u00f6\u00b7nig", "sein", "gold\u00b7m\u00e4h\u00b7nig", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Geige t\u00f6nt, die Fl\u00f6ten rufen,", "tokens": ["Die", "Gei\u00b7ge", "t\u00f6nt", ",", "die", "Fl\u00f6\u00b7ten", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er reitet auf sein Elfenschlo\u00df.", "tokens": ["Er", "rei\u00b7tet", "auf", "sein", "El\u00b7fen\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}