{"textgrid.poem.25603": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Verl\u00e4umdung der h\u00e4\u00dflichen Cephise", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gr\u00e4mt sie sich, da\u00df in der n\u00e4chsten Stadt", "tokens": ["Wie", "gr\u00e4mt", "sie", "sich", ",", "da\u00df", "in", "der", "n\u00e4chs\u00b7ten", "Stadt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein L\u00e4stermund ihr aufgeb\u00fcrdet hat,", "tokens": ["Ein", "L\u00e4s\u00b7ter\u00b7mund", "ihr", "auf\u00b7ge\u00b7b\u00fcr\u00b7det", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch sie sey eine Buhlerin.", "tokens": ["Auch", "sie", "sey", "ei\u00b7ne", "Buh\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie schicke nur ihr Bildni\u00df hin!", "tokens": ["Sie", "schi\u00b7cke", "nur", "ihr", "Bild\u00b7ni\u00df", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}