{"textgrid.poem.53352": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[herr, wir wallen s\u00e4mptlich dir]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, wir wallen s\u00e4mptlich dir,", "tokens": ["Herr", ",", "wir", "wal\u00b7len", "s\u00e4mpt\u00b7lich", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVINF", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil der Leib uns h\u00e4lt umbschlossen,", "tokens": ["Weil", "der", "Leib", "uns", "h\u00e4lt", "umbsc\u00b7hlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wir sind dir beydes hier", "tokens": ["Denn", "wir", "sind", "dir", "bey\u00b7des", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PIS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frembd' und Reichsgenossen,", "tokens": ["Frembd'", "und", "Reichs\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnsers kurtzen Wandels Lauff", "tokens": ["Vn\u00b7sers", "kurt\u00b7zen", "Wan\u00b7dels", "Lauff"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Steht hinauff", "tokens": ["Steht", "hin\u00b7auff"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "---", "measure": "unknown.measure.zero"}, "line.7": {"text": "Da wir her entsprossen.", "tokens": ["Da", "wir", "her", "ent\u00b7spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sey du beydes Licht und Stab", "tokens": ["Sey", "du", "bey\u00b7des", "Licht", "und", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch dein Wort auff unsern Wegen,", "tokens": ["Durch", "dein", "Wort", "auff", "un\u00b7sern", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wend der Feinde Bosheit ab,", "tokens": ["Wend", "der", "Fein\u00b7de", "Bos\u00b7heit", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die uns Stricke legen,", "tokens": ["Die", "uns", "Stri\u00b7cke", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "La\u00df sich deiner Engel Schar", "tokens": ["La\u00df", "sich", "dei\u00b7ner", "En\u00b7gel", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PRF", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Immerdar", "tokens": ["Im\u00b7mer\u00b7dar"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Trewlich umb uns regen,", "tokens": ["Trew\u00b7lich", "umb", "uns", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Bis wir selig durch den Tod", "tokens": ["Bis", "wir", "se\u00b7lig", "durch", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnsre B\u00fcrger-Stadt erreichen,", "tokens": ["Vns\u00b7re", "B\u00fcr\u00b7ger\u00b7Stadt", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da uns weder Gram noch Noht", "tokens": ["Da", "uns", "we\u00b7der", "Gram", "noch", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "NN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig wird bestreichen,", "tokens": ["E\u00b7wig", "wird", "be\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da, was hier uns immer irrt,", "tokens": ["Da", ",", "was", "hier", "uns", "im\u00b7mer", "irrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Alles wird", "tokens": ["Al\u00b7les", "wird"], "token_info": ["word", "word"], "pos": ["PIS", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Von uns m\u00fcssen weichen.", "tokens": ["Von", "uns", "m\u00fcs\u00b7sen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Vnsre Stadt, die deine Hand", "tokens": ["Vns\u00b7re", "Stadt", ",", "die", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst so herrlich auffgef\u00fchret,", "tokens": ["Selbst", "so", "herr\u00b7lich", "auff\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da man keiner Sonnen-Brand", "tokens": ["Da", "man", "kei\u00b7ner", "Son\u00b7nen\u00b7Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd kein Mond-Licht sp\u00fcret,", "tokens": ["Vnd", "kein", "Mon\u00b7dLicht", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Weil dein Herrlicheit allein,", "tokens": ["Weil", "dein", "Herr\u00b7li\u00b7cheit", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie mit Schein", "tokens": ["Sie", "mit", "Schein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Vnvergleichlich zieret.", "tokens": ["Vn\u00b7ver\u00b7gleich\u00b7lich", "zie\u00b7ret", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ihre Gassen sind Cristall,", "tokens": ["Ih\u00b7re", "Gas\u00b7sen", "sind", "Cris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Th\u00f6re zw\u00f6lff erh\u00f6het,", "tokens": ["Ih\u00b7rer", "Th\u00f6\u00b7re", "zw\u00f6lff", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "CARD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Perlen allzumal,", "tokens": ["A\u00b7ber", "Per\u00b7len", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr Geb\u00e4w bestehet", "tokens": ["Ihr", "Ge\u00b7b\u00e4w", "be\u00b7ste\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Klar aus Golde, dessen Prei\u00df", "tokens": ["Klar", "aus", "Gol\u00b7de", ",", "des\u00b7sen", "Prei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was man wei\u00df", "tokens": ["Was", "man", "wei\u00df"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Weit weit \u00fcbergehet.", "tokens": ["Weit", "weit", "\u00fc\u00b7berg\u00b7e\u00b7het", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Herr, wir sehnen da uns hin,", "tokens": ["Herr", ",", "wir", "seh\u00b7nen", "da", "uns", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "End uns dieses Pilger-Leben,", "tokens": ["End", "uns", "die\u00b7ses", "Pil\u00b7ger\u00b7Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df von hier sich unsern Sinn", "tokens": ["La\u00df", "von", "hier", "sich", "un\u00b7sern", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ADV", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets hinauff erheben,", "tokens": ["Stets", "hin\u00b7auff", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bis nach unserm Tode wir", "tokens": ["Bis", "nach", "un\u00b7serm", "To\u00b7de", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar bey Dir", "tokens": ["Gar", "bey", "Dir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ohn auffh\u00f6ren schweben.", "tokens": ["Ohn", "auff\u00b7h\u00f6\u00b7ren", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}