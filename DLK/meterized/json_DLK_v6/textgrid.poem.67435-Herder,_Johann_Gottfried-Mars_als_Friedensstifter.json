{"textgrid.poem.67435": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Mars als Friedensstifter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bringst Du selber, o Mars, in Deiner Rechte den Oelzweig", "tokens": ["Bringst", "Du", "sel\u00b7ber", ",", "o", "Mars", ",", "in", "Dei\u00b7ner", "Rech\u00b7te", "den", "O\u00b7el\u00b7zweig"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+--+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und des bl\u00fchenden Horns Freuden die F\u00fclle zur\u00fcck?", "tokens": ["Und", "des", "bl\u00fc\u00b7hen\u00b7den", "Horns", "Freu\u00b7den", "die", "F\u00fcl\u00b7le", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schild und Bogen und Spie\u00df sind Dir zu Fu\u00dfe gesunken,", "tokens": ["Schild", "und", "Bo\u00b7gen", "und", "Spie\u00df", "sind", "Dir", "zu", "Fu\u00b7\u00dfe", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Deinen umlorbeerten Helm tr\u00e4get der ruhige Arm.", "tokens": ["Dei\u00b7nen", "um\u00b7lor\u00b7beer\u00b7ten", "Helm", "tr\u00e4\u00b7get", "der", "ru\u00b7hi\u00b7ge", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Wohl Dir! Biete den Zweig der Paphischen G\u00f6ttin; sie wird Dir", "tokens": ["Wohl", "Dir", "!", "Bie\u00b7te", "den", "Zweig", "der", "Pa\u00b7phi\u00b7schen", "G\u00f6t\u00b7tin", ";", "sie", "wird", "Dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "NN", "ART", "NN", "ART", "NN", "NN", "$.", "PPER", "VAFIN", "PPER"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Lohnen im seligsten Ku\u00df, was Du im Frieden uns schenkst.", "tokens": ["Loh\u00b7nen", "im", "se\u00b7ligs\u00b7ten", "Ku\u00df", ",", "was", "Du", "im", "Frie\u00b7den", "uns", "schenkst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "PWS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}