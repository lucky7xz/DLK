{"dta.poem.3121": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "166.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie von der Sonne gehn viel Stralen erdenwerts,", "tokens": ["Wie", "von", "der", "Son\u00b7ne", "gehn", "viel", "Stra\u00b7len", "er\u00b7den\u00b7werts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So geht von Gott ein Stral in jedes Dinges Herz.", "tokens": ["So", "geht", "von", "Gott", "ein", "Stral", "in", "je\u00b7des", "Din\u00b7ges", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An diesem Strale h\u00e4ngt das Ding mit Gott zusammen,", "tokens": ["An", "die\u00b7sem", "Stra\u00b7le", "h\u00e4ngt", "das", "Ding", "mit", "Gott", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jedes f\u00fchlet sich dadurch von Gott entstammen.", "tokens": ["Und", "je\u00b7des", "f\u00fch\u00b7let", "sich", "da\u00b7durch", "von", "Gott", "ent\u00b7stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PRF", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von Ding zu Dinge geht seitwerts kein solcher Stral,", "tokens": ["Von", "Ding", "zu", "Din\u00b7ge", "geht", "seit\u00b7werts", "kein", "sol\u00b7cher", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "ADV", "PIAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur viel verworrene Streiflichter alzumal.", "tokens": ["Nur", "viel", "ver\u00b7wor\u00b7re\u00b7ne", "Streif\u00b7lich\u00b7ter", "al\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "An diesen Lichtern kanst du nie das Ding erkennen,", "tokens": ["An", "die\u00b7sen", "Lich\u00b7tern", "kanst", "du", "nie", "das", "Ding", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dunkle Scheidewand wird stets von ihm dich trennen.", "tokens": ["Die", "dunk\u00b7le", "Schei\u00b7de\u00b7wand", "wird", "stets", "von", "ihm", "dich", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "An deinem Stral vielmehr mu\u00dft du zu Gott aufsteigen,", "tokens": ["An", "dei\u00b7nem", "Stral", "viel\u00b7mehr", "mu\u00dft", "du", "zu", "Gott", "auf\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VMFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und in das Ding hinab an seinem Stral dich neigen.", "tokens": ["Und", "in", "das", "Ding", "hin\u00b7ab", "an", "sei\u00b7nem", "Stral", "dich", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dann siehest du das Ding, wie's ist, nicht wie es scheint,", "tokens": ["Dann", "sie\u00b7hest", "du", "das", "Ding", ",", "wie's", "ist", ",", "nicht", "wie", "es", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "VAFIN", "$,", "PTKNEG", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du es siehest mit dir selbst in Gott vereint.", "tokens": ["Wenn", "du", "es", "sie\u00b7hest", "mit", "dir", "selbst", "in", "Gott", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}