{"textgrid.poem.52253": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Wisperwind, der Wisperwind,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wisperwind, der Wisperwind,", "tokens": ["Der", "Wis\u00b7per\u00b7wind", ",", "der", "Wis\u00b7per\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den kennt bis \u00d6strich jedes Kind;", "tokens": ["Den", "kennt", "bis", "\u00d6st\u00b7rich", "je\u00b7des", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Morgens fr\u00fch von vier bis zehn,", "tokens": ["Des", "Mor\u00b7gens", "fr\u00fch", "von", "vier", "bis", "zehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "CARD", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sp\u00fcrt man allermeist sein Wehn!", "tokens": ["Da", "sp\u00fcrt", "man", "al\u00b7ler\u00b7meist", "sein", "Wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stromauf aus Wald und Wiesengrund", "tokens": ["Stro\u00b7mauf", "aus", "Wald", "und", "Wie\u00b7sen\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Haucht ihn der Wisper k\u00fchler Mund!", "tokens": ["Haucht", "ihn", "der", "Wis\u00b7per", "k\u00fch\u00b7ler", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, immer, immer nur stromauf", "tokens": ["Ja", ",", "im\u00b7mer", ",", "im\u00b7mer", "nur", "stro\u00b7mauf"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "$,", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4hrt er mit Pfeifen und Geschnauf':", "tokens": ["F\u00e4hrt", "er", "mit", "Pfei\u00b7fen", "und", "Ge\u00b7schnauf'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Von unten jetzt und allezeit", "tokens": ["Von", "un\u00b7ten", "jetzt", "und", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Braust er nach oben, kampfbereit;", "tokens": ["Braust", "er", "nach", "o\u00b7ben", ",", "kampf\u00b7be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "$,", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nie ", "tokens": ["Nie"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Nur ihr ", "tokens": ["Nur", "ihr"], "token_info": ["word", "word"], "pos": ["ADV", "PPOSAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Er macht sich auf, wo H\u00fctten stehn;", "tokens": ["Er", "macht", "sich", "auf", ",", "wo", "H\u00fct\u00b7ten", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo H\u00fctten stehn und M\u00fchlen gehn.", "tokens": ["Wo", "H\u00fct\u00b7ten", "stehn", "und", "M\u00fch\u00b7len", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Bauern Strohdach ohne Ruh'", "tokens": ["Des", "Bau\u00b7ern", "Stroh\u00b7dach", "oh\u00b7ne", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schickt ihn der Burg des F\u00fcrsten zu;", "tokens": ["Schickt", "ihn", "der", "Burg", "des", "F\u00fcrs\u00b7ten", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Anf\u00e4hrt er trotzig, sagt mein Ferg',", "tokens": ["An\u00b7f\u00e4hrt", "er", "trot\u00b7zig", ",", "sagt", "mein", "Fer\u00b7g'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schlo\u00df Rheinstein und Johannisberg.", "tokens": ["Schlo\u00df", "Rhein\u00b7stein", "und", "Jo\u00b7han\u00b7nis\u00b7berg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Er saust und w\u00fctet um sie her,", "tokens": ["Er", "saust", "und", "w\u00fc\u00b7tet", "um", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch und gradaus wie keiner mehr;", "tokens": ["Frisch", "und", "gra\u00b7daus", "wie", "kei\u00b7ner", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "KOKOM", "PIS", "ADV", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Er schiert den Teufel sich um Gunst,", "tokens": ["Er", "schiert", "den", "Teu\u00b7fel", "sich", "um", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er pfeift was auf den blauen Dunst,", "tokens": ["Er", "pfeift", "was", "auf", "den", "blau\u00b7en", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der tr\u00fcb um ihre Zinnen hangt \u2013", "tokens": ["Der", "tr\u00fcb", "um", "ih\u00b7re", "Zin\u00b7nen", "hangt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er pfeift, bis klar der Himmel prangt.", "tokens": ["Er", "pfeift", ",", "bis", "klar", "der", "Him\u00b7mel", "prangt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, heiter wird auf ihn der Tag;", "tokens": ["Ja", ",", "hei\u00b7ter", "wird", "auf", "ihn", "der", "Tag", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum braus' er, was er brausen mag!", "tokens": ["Drum", "braus'", "er", ",", "was", "er", "brau\u00b7sen", "mag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er selbst und ", "tokens": ["Er", "selbst", "und"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "KON"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ein neuer Tag der Welt beginnt!", "tokens": ["Ein", "neu\u00b7er", "Tag", "der", "Welt", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die H\u00e4hne kr\u00e4hn, der Wald erwacht,", "tokens": ["Die", "H\u00e4h\u00b7ne", "kr\u00e4hn", ",", "der", "Wald", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Wispern hat sich aufgemacht!", "tokens": ["Ein", "Wis\u00b7pern", "hat", "sich", "auf\u00b7ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Von unten keck nach oben auch", "tokens": ["Von", "un\u00b7ten", "keck", "nach", "o\u00b7ben", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJD", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht dieser andern Wisper Hauch;", "tokens": ["Zieht", "die\u00b7ser", "an\u00b7dern", "Wis\u00b7per", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf aus den Tiefen zu den H\u00f6hn", "tokens": ["Auf", "aus", "den", "Tie\u00b7fen", "zu", "den", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhebt sich frisch auch dieses Wehn;", "tokens": ["Er\u00b7hebt", "sich", "frisch", "auch", "die\u00b7ses", "Wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Strohdach und Werkstatt ohne Ruh'", "tokens": ["Stroh\u00b7dach", "und", "Werk\u00b7statt", "oh\u00b7ne", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schicken der F\u00fcrstenburg es zu!", "tokens": ["Schi\u00b7cken", "der", "F\u00fcrs\u00b7ten\u00b7burg", "es", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Da hangen tr\u00fcb die Nebel noch;", "tokens": ["Da", "han\u00b7gen", "tr\u00fcb", "die", "Ne\u00b7bel", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geduld nur, es verjagt sie doch!", "tokens": ["Ge\u00b7duld", "nur", ",", "es", "ver\u00b7jagt", "sie", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie zornig sie auch dr\u00e4un, wie wirr,", "tokens": ["Wie", "zor\u00b7nig", "sie", "auch", "dr\u00e4un", ",", "wie", "wirr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVINF", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es l\u00e4\u00dft nicht ab, es wird nicht irr!", "tokens": ["Es", "l\u00e4\u00dft", "nicht", "ab", ",", "es", "wird", "nicht", "irr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit kr\u00e4ft'gem Blasen, Ruck auf Ruck,", "tokens": ["Mit", "kr\u00e4ft'\u00b7gem", "Bla\u00b7sen", ",", "Ruck", "auf", "Ruck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht es zunichte Dunst und Druck!", "tokens": ["Macht", "es", "zu\u00b7nich\u00b7te", "Dunst", "und", "Druck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Hab' Dank, du frisch und freudig Wehn!", "tokens": ["Hab'", "Dank", ",", "du", "frisch", "und", "freu\u00b7dig", "Wehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' Dank, hab' Dank \u2013 o, w\u00e4r' es zehn!", "tokens": ["Hab'", "Dank", ",", "hab'", "Dank", "\u2013", "o", ",", "w\u00e4r'", "es", "zehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "APPR", "$(", "FM", "$,", "VAFIN", "PPER", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, zehn und rings der Himmel rein!", "tokens": ["Ja", ",", "zehn", "und", "rings", "der", "Him\u00b7mel", "rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "CARD", "KON", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt, mein' ich, wird es sechse sein! \u2013", "tokens": ["Jetzt", ",", "mein'", "ich", ",", "wird", "es", "sech\u00b7se", "sein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wisperwind, der Wisperwind,", "tokens": ["Der", "Wis\u00b7per\u00b7wind", ",", "der", "Wis\u00b7per\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den kennt bis \u00d6strich jedes Kind!", "tokens": ["Den", "kennt", "bis", "\u00d6st\u00b7rich", "je\u00b7des", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}