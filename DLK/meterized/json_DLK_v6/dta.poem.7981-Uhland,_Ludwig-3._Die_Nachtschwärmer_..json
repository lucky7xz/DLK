{"dta.poem.7981": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.  Die Nachtschw\u00e4rmer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf Uhr! ist der Ruf erschollen", "tokens": ["Zw\u00f6lf", "Uhr", "!", "ist", "der", "Ruf", "er\u00b7schol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$.", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und mir sinkt das Glas vom Munde.", "tokens": ["Und", "mir", "sinkt", "das", "Glas", "vom", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll ich jetzt nach Haus mich trollen", "tokens": ["Soll", "ich", "jetzt", "nach", "Haus", "mich", "trol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der schlimmen Geisterstunde,", "tokens": ["In", "der", "schlim\u00b7men", "Geis\u00b7ter\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Stunde der Patrollen?", "tokens": ["In", "der", "Stun\u00b7de", "der", "Pat\u00b7rol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und daheim zum Zeitvertreibe", "tokens": ["Und", "da\u00b7heim", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch den Zank von meinem Weibe!", "tokens": ["Noch", "den", "Zank", "von", "mei\u00b7nem", "Wei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann die Nachbarn, h\u00e4m\u2019sche Tadler! \u2014", "tokens": ["Dann", "die", "Nach\u00b7barn", ",", "h\u00e4m'\u00b7sche", "Tad\u00b7ler", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nein! ich bleib\u2019 im goldnen Adler,", "tokens": ["Nein", "!", "ich", "bleib'", "im", "gold\u00b7nen", "Ad\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sehe Jeder, wo er bleibe!", "tokens": ["Se\u00b7he", "Je\u00b7der", ",", "wo", "er", "blei\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}