{"textgrid.poem.67472": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Stern", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmlischer Stern, Du f\u00fchrst aus fernem Lande die Weisen", "tokens": ["Himm\u00b7li\u00b7scher", "Stern", ",", "Du", "f\u00fchrst", "aus", "fer\u00b7nem", "Lan\u00b7de", "die", "Wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Zu der Krippe des Sohns, der wie ein Himmlischer schl\u00e4ft;", "tokens": ["Zu", "der", "Krip\u00b7pe", "des", "Sohns", ",", "der", "wie", "ein", "Himm\u00b7li\u00b7scher", "schl\u00e4ft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und da knieen sie schon, sie bringen Myrrhen und Weihrauch", "tokens": ["Und", "da", "kni\u00b7e\u00b7en", "sie", "schon", ",", "sie", "brin\u00b7gen", "Myr\u00b7rhen", "und", "Weih\u00b7rauch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVINF", "NN", "KON", "NN"], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ihr k\u00f6stliches Gold Deinem Gesegneten dar.", "tokens": ["Und", "ihr", "k\u00f6st\u00b7li\u00b7ches", "Gold", "Dei\u00b7nem", "Ge\u00b7se\u00b7gne\u00b7ten", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Aber dort klagt Rahel untr\u00f6stbar: ihre Geliebten", "tokens": ["A\u00b7ber", "dort", "klagt", "Ra\u00b7hel", "un\u00b7tr\u00f6st\u00b7bar", ":", "ih\u00b7re", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE", "ADJD", "$.", "PPOSAT", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Sind nicht mehr, sie entrei\u00dft ihr der w\u00fcrgende Tod.", "tokens": ["Sind", "nicht", "mehr", ",", "sie", "ent\u00b7rei\u00dft", "ihr", "der", "w\u00fcr\u00b7gen\u00b7de", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Ach, der Menschenschicksale auf unsrer rollenden Erde!", "tokens": ["Ach", ",", "der", "Men\u00b7schen\u00b7schick\u00b7sa\u00b7le", "auf", "uns\u00b7rer", "rol\u00b7len\u00b7den", "Er\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Selbst der gl\u00fccklichste Stern bringet hier Freude, dort Weh.", "tokens": ["Selbst", "der", "gl\u00fcck\u00b7lichs\u00b7te", "Stern", "brin\u00b7get", "hier", "Freu\u00b7de", ",", "dort", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}