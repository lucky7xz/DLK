{"textgrid.poem.34390": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Morgens, wenn am Strande noch der Hauch der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Morgens, wenn am Strande noch der Hauch der Nacht", "tokens": ["Des", "Mor\u00b7gens", ",", "wenn", "am", "Stran\u00b7de", "noch", "der", "Hauch", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "KOUS", "APPRART", "NN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weht, wenn im Osten kaum das Fr\u00fchrothlicht erwacht,", "tokens": ["weht", ",", "wenn", "im", "Os\u00b7ten", "kaum", "das", "Fr\u00fch\u00b7roth\u00b7licht", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPRART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "wenn sich die Wellen f\u00e4rben \u2013 irr ich schon verst\u00f6rt", "tokens": ["wenn", "sich", "die", "Wel\u00b7len", "f\u00e4r\u00b7ben", "\u2013", "irr", "ich", "schon", "ver\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$(", "VVIMP", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und stehe, wo am Steine sich der Schwall emp\u00f6rt,", "tokens": ["und", "ste\u00b7he", ",", "wo", "am", "Stei\u00b7ne", "sich", "der", "Schwall", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPRART", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und schau hinaus aufs unermessne, \u00f6de Meer,", "tokens": ["und", "schau", "hin\u00b7aus", "aufs", "un\u00b7er\u00b7mess\u00b7ne", ",", "\u00f6\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "APZR", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "trocken und starr die Augen und die Brust so leer ...", "tokens": ["tro\u00b7cken", "und", "starr", "die", "Au\u00b7gen", "und", "die", "Brust", "so", "leer", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "KON", "ART", "NN", "ADV", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Allm\u00e4hlich wohl vergess ich meine Schuld und Qual.", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "wohl", "ver\u00b7gess", "ich", "mei\u00b7ne", "Schuld", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich denke dann des Tages, da zum ersten Mal", "tokens": ["Ich", "den\u00b7ke", "dann", "des", "Ta\u00b7ges", ",", "da", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ich sie gesehn, das blonde, m\u00e4rchensch\u00f6ne Kind.", "tokens": ["ich", "sie", "ge\u00b7sehn", ",", "das", "blon\u00b7de", ",", "m\u00e4r\u00b7chen\u00b7sch\u00f6\u00b7ne", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Th\u00fcr der armen H\u00fctte, drin sie sitzt und sinnt,", "tokens": ["Die", "Th\u00fcr", "der", "ar\u00b7men", "H\u00fct\u00b7te", ",", "drin", "sie", "sitzt", "und", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "steht wieder offen, wieder hemm ich meinen Fuss,", "tokens": ["steht", "wie\u00b7der", "of\u00b7fen", ",", "wie\u00b7der", "hemm", "ich", "mei\u00b7nen", "Fuss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der schon vor\u00fcbereilen wollte .. ersten Gruss", "tokens": ["der", "schon", "vor\u00b7\u00fc\u00b7be\u00b7rei\u00b7len", "woll\u00b7te", "..", "ers\u00b7ten", "Gruss"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "wagen die Augen, ihre Hand erbebt .. doch blieb", "tokens": ["wa\u00b7gen", "die", "Au\u00b7gen", ",", "ih\u00b7re", "Hand", "er\u00b7bebt", "..", "doch", "blieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVPP", "$.", "ADV", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "haften ihr Blick in meinem tief \u2013: hast du mich lieb?.. mich lieb?..", "tokens": ["haf\u00b7ten", "ihr", "Blick", "in", "mei\u00b7nem", "tief", "\u2013", ":", "hast", "du", "mich", "lieb", "?", "..", "mich", "lieb", "?", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJD", "$(", "$.", "VAFIN", "PPER", "PRF", "ADJD", "$.", "$(", "PPER", "ADJD", "$.", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.3": {"line.1": {"text": "Zur\u00fcck! Schon zischt die Welle unter dir! Die Fluth,", "tokens": ["Zu\u00b7r\u00fcck", "!", "Schon", "zischt", "die", "Wel\u00b7le", "un\u00b7ter", "dir", "!", "Die", "Fluth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sie steigt .. und wieder dr\u00e4ngt sie an mit alter Wuth!", "tokens": ["sie", "steigt", "..", "und", "wie\u00b7der", "dr\u00e4ngt", "sie", "an", "mit", "al\u00b7ter", "Wuth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}