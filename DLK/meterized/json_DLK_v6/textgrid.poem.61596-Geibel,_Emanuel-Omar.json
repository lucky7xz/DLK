{"textgrid.poem.61596": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Omar", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Inmitten seiner Turbankrieger,", "tokens": ["In\u00b7mit\u00b7ten", "sei\u00b7ner", "Tur\u00b7ban\u00b7krie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stirne voll Gewitterschein,", "tokens": ["Die", "Stir\u00b7ne", "voll", "Ge\u00b7wit\u00b7ter\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zog Omar, der Kalif, als Sieger", "tokens": ["Zog", "O\u00b7mar", ",", "der", "Ka\u00b7lif", ",", "als", "Sie\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "$,", "ART", "NN", "$,", "KOUS", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ins Tor der Ptolem\u00e4er ein.", "tokens": ["Ins", "Tor", "der", "Pto\u00b7le\u00b7m\u00e4\u00b7er", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umrauscht von Mekkas Halbmondbannern,", "tokens": ["Um\u00b7rauscht", "von", "Mek\u00b7kas", "Halb\u00b7mond\u00b7ban\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ritt langsam er dahin im Zug,", "tokens": ["Ritt", "lang\u00b7sam", "er", "da\u00b7hin", "im", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihm folgte mit den Bogenspannern", "tokens": ["Ihm", "folg\u00b7te", "mit", "den", "Bo\u00b7gen\u00b7span\u00b7nern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Negerschwarm, der Fackeln trug.", "tokens": ["Ein", "Ne\u00b7ger\u00b7schwarm", ",", "der", "Fa\u00b7ckeln", "trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie zogen durch die \u00f6den Gassen,", "tokens": ["Sie", "zo\u00b7gen", "durch", "die", "\u00f6\u00b7den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Siegestor und S\u00e4ulengang,", "tokens": ["Durch", "Sie\u00b7ge\u00b7stor", "und", "S\u00e4u\u00b7len\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drin klirrend nur der Schritt der Massen,", "tokens": ["Drin", "klir\u00b7rend", "nur", "der", "Schritt", "der", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hengste Stampfen widerklang;", "tokens": ["Der", "Hengs\u00b7te", "Stamp\u00b7fen", "wi\u00b7der\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon lenkte zu den Porphyrstufen", "tokens": ["Schon", "lenk\u00b7te", "zu", "den", "Por\u00b7phyr\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der alten Hofburg der Kalif,", "tokens": ["Der", "al\u00b7ten", "Hof\u00b7burg", "der", "Ka\u00b7lif", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da warf vor seines Rosses Hufen", "tokens": ["Da", "warf", "vor", "sei\u00b7nes", "Ros\u00b7ses", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Greis sich in den Staub und rief:", "tokens": ["Ein", "Greis", "sich", "in", "den", "Staub", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbo Herr, der Sieger warst du heute,", "tokens": ["\u00bb", "o", "Herr", ",", "der", "Sie\u00b7ger", "warst", "du", "heu\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und diese Stadt des Nils ist dein,", "tokens": ["Und", "die\u00b7se", "Stadt", "des", "Nils", "ist", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So nimm als reiche Schlachtenbeute", "tokens": ["So", "nimm", "als", "rei\u00b7che", "Schlach\u00b7ten\u00b7beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Gold und Erz und Elfenbein.", "tokens": ["Ihr", "Gold", "und", "Erz", "und", "El\u00b7fen\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die T\u00fcrme st\u00fcrz' in Schutt zusammen,", "tokens": ["Die", "T\u00fcr\u00b7me", "st\u00fcrz'", "in", "Schutt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zerbrich den Bilderschmuck des Hains,", "tokens": ["Zer\u00b7brich", "den", "Bil\u00b7der\u00b7schmuck", "des", "Hains", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Tempel selber gib den Flammen!", "tokens": ["Die", "Tem\u00b7pel", "sel\u00b7ber", "gib", "den", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur eins verschone, Herr, nur eins!", "tokens": ["Nur", "eins", "ver\u00b7scho\u00b7ne", ",", "Herr", ",", "nur", "eins", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "NN", "$,", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbsieh hin! Wo dort die Sphinxe grollen", "tokens": ["\u00bb", "sieh", "hin", "!", "Wo", "dort", "die", "Sphin\u00b7xe", "grol\u00b7len"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PTKVZ", "$.", "PWAV", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Tor, die H\u00fcter unsres Ruhms,", "tokens": ["Am", "Tor", ",", "die", "H\u00fc\u00b7ter", "uns\u00b7res", "Ruhms", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schl\u00e4ft in hunderttausend Rollen", "tokens": ["Da", "schl\u00e4ft", "in", "hun\u00b7dert\u00b7tau\u00b7send", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geisterhort des Altertums.", "tokens": ["Der", "Geis\u00b7ter\u00b7hort", "des", "Al\u00b7ter\u00b7tums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was, seit der Erdkreis aufgerichtet,", "tokens": ["Was", ",", "seit", "der", "Erd\u00b7kreis", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Tat und Wort sich offenbart,", "tokens": ["In", "Tat", "und", "Wort", "sich", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was je gedacht ward und gedichtet,", "tokens": ["Was", "je", "ge\u00b7dacht", "ward", "und", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dort liegt's der Nachwelt aufbewahrt.", "tokens": ["Dort", "liegt's", "der", "Nach\u00b7welt", "auf\u00b7be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbo gib den Schatz, aus allen Reichen", "tokens": ["\u00bb", "o", "gib", "den", "Schatz", ",", "aus", "al\u00b7len", "Rei\u00b7chen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "VVIMP", "ART", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welt geh\u00e4uft mit treuem Flei\u00df,", "tokens": ["Der", "Welt", "ge\u00b7h\u00e4uft", "mit", "treu\u00b7em", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gib dies Verm\u00e4chtnis ohnegleichen,", "tokens": ["Gib", "dies", "Ver\u00b7m\u00e4cht\u00b7nis", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Menschheit Erbteil gib nicht preis!", "tokens": ["Der", "Menschheit", "Erb\u00b7teil", "gib", "nicht", "preis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIMP", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, heilig sei auch dir die St\u00e4tte,", "tokens": ["Nein", ",", "hei\u00b7lig", "sei", "auch", "dir", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die jede Muse fromm geweiht,", "tokens": ["Die", "je\u00b7de", "Mu\u00b7se", "fromm", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Streck' dr\u00fcber deine Hand und rette", "tokens": ["Streck'", "dr\u00fc\u00b7ber", "dei\u00b7ne", "Hand", "und", "ret\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PAV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Zukunft die Vergangenheit!\u00ab", "tokens": ["Der", "Zu\u00b7kunft", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch Omar zieht die Stirn in Falten", "tokens": ["Doch", "O\u00b7mar", "zieht", "die", "Stirn", "in", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und spricht, indem sein Auge flammt:", "tokens": ["Und", "spricht", ",", "in\u00b7dem", "sein", "Au\u00b7ge", "flammt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbich bin genaht, Gericht zu halten,", "tokens": ["\u00bb", "ich", "bin", "ge\u00b7naht", ",", "Ge\u00b7richt", "zu", "hal\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dr\u00e4ngst du, Tor, dich in mein Amt?", "tokens": ["Was", "dr\u00e4ngst", "du", ",", "Tor", ",", "dich", "in", "mein", "Amt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NE", "$,", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinweg, da\u00df meines Zorns Geloder", "tokens": ["Hin\u00b7weg", ",", "da\u00df", "mei\u00b7nes", "Zorns", "Ge\u00b7lo\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht dich samt deinen Rollen trifft!", "tokens": ["Nicht", "dich", "samt", "dei\u00b7nen", "Rol\u00b7len", "trifft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die Sch\u00e4tze, die du r\u00fchmst, sind Moder,", "tokens": ["Die", "Sch\u00e4t\u00b7ze", ",", "die", "du", "r\u00fchmst", ",", "sind", "Mo\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und was du Weisheit nennst, ist Gift.", "tokens": ["Und", "was", "du", "Weis\u00b7heit", "nennst", ",", "ist", "Gift", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Schon allzulang am unfruchtbaren", "tokens": ["Schon", "all\u00b7zu\u00b7lang", "am", "un\u00b7frucht\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielwissen siecht die Welt erschlafft;", "tokens": ["Viel\u00b7wis\u00b7sen", "siecht", "die", "Welt", "er\u00b7schlafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Staub von mehr als tausend Jahren", "tokens": ["Der", "Staub", "von", "mehr", "als", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt wie ein Alp auf jeder Kraft.", "tokens": ["Liegt", "wie", "ein", "Alp", "auf", "je\u00b7der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lebens Baum lie\u00df ab zu lauben,", "tokens": ["Des", "Le\u00b7bens", "Baum", "lie\u00df", "ab", "zu", "lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seit dran der Wurm des Zweifels zehrt:", "tokens": ["Seit", "dran", "der", "Wurm", "des", "Zwei\u00b7fels", "zehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ist ein Herz noch, frisch zum Glauben!", "tokens": ["Wo", "ist", "ein", "Herz", "noch", ",", "frisch", "zum", "Glau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo ist ein Arm noch, stark zum Schwert!", "tokens": ["Wo", "ist", "ein", "Arm", "noch", ",", "stark", "zum", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbda\u00df endlich diese Dumpfheit ende,", "tokens": ["\u00bb", "da\u00df", "end\u00b7lich", "die\u00b7se", "Dum\u00b7pf\u00b7heit", "en\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bin ich gesandt, vom Herrn ein Blitz.", "tokens": ["Bin", "ich", "ge\u00b7sandt", ",", "vom", "Herrn", "ein", "Blitz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf! Schleudert denn die Feuerbr\u00e4nde", "tokens": ["Auf", "!", "Schleu\u00b7dert", "denn", "die", "Feu\u00b7er\u00b7br\u00e4n\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In der verj\u00e4hrten Krankheit Sitz!", "tokens": ["In", "der", "ver\u00b7j\u00e4hr\u00b7ten", "Krank\u00b7heit", "Sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn, umwogt vom Flammenmeere,", "tokens": ["Und", "wenn", ",", "um\u00b7wogt", "vom", "Flam\u00b7men\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der aufget\u00fcrmte Wust zergeht,", "tokens": ["Der", "auf\u00b7ge\u00b7t\u00fcrm\u00b7te", "Wust", "zer\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ruft: Gott ist gro\u00df! Ihm sei die Ehre!", "tokens": ["Ruft", ":", "Gott", "ist", "gro\u00df", "!", "Ihm", "sei", "die", "Eh\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Mahomed ist sein Prophet!\u00ab", "tokens": ["Und", "Ma\u00b7ho\u00b7med", "ist", "sein", "Pro\u00b7phet", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}