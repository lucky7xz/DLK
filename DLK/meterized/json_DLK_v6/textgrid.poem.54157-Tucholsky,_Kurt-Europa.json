{"textgrid.poem.54157": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Europa", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Rhein, da w\u00e4chst ein s\u00fcffiger Wein \u2013", "tokens": ["Am", "Rhein", ",", "da", "w\u00e4chst", "ein", "s\u00fcf\u00b7fi\u00b7ger", "Wein", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "der darf aber nicht nach England hinein \u2013", "tokens": ["der", "darf", "a\u00b7ber", "nicht", "nach", "En\u00b7gland", "hin\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "ADV", "PTKNEG", "APPR", "NE", "PTKVZ", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Buy British!", "tokens": ["Buy", "Bri\u00b7tish", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "In Wien gibt es herrliche Torten und Kuchen,", "tokens": ["In", "Wi\u00b7en", "gibt", "es", "herr\u00b7li\u00b7che", "Tor\u00b7ten", "und", "Ku\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "die haben in Schweden nichts zu suchen \u2013", "tokens": ["die", "ha\u00b7ben", "in", "Schwe\u00b7den", "nichts", "zu", "su\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "K\u00f6p svenska varor!", "tokens": ["K\u00f6p", "sven\u00b7ska", "va\u00b7ror", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "In Italien verfaulen die Apfelsinen \u2013", "tokens": ["In", "I\u00b7ta\u00b7li\u00b7en", "ver\u00b7fau\u00b7len", "die", "Ap\u00b7fel\u00b7si\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "la\u00dft die deutsche Landwirtschaft verdienen!", "tokens": ["la\u00dft", "die", "deut\u00b7sche", "Land\u00b7wirt\u00b7schaft", "ver\u00b7die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Deutsche, kauft deutsche Zitronen!", "tokens": ["Deut\u00b7sche", ",", "kauft", "deut\u00b7sche", "Zit\u00b7ro\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Und auf jedem Quadratkilometer Raum", "tokens": ["Und", "auf", "je\u00b7dem", "Quad\u00b7rat\u00b7ki\u00b7lo\u00b7me\u00b7ter", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "tr\u00e4umt einer seinen v\u00f6lkischen Traum,", "tokens": ["tr\u00e4umt", "ei\u00b7ner", "sei\u00b7nen", "v\u00f6l\u00b7ki\u00b7schen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Und leise fl\u00fcstert der Wind durch die B\u00e4ume . . .", "tokens": ["Und", "lei\u00b7se", "fl\u00fcs\u00b7tert", "der", "Wind", "durch", "die", "B\u00e4u\u00b7me", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "R\u00e4ume sind Sch\u00e4ume.", "tokens": ["R\u00e4u\u00b7me", "sind", "Sch\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da liegt Europa. Wie sieht es aus?", "tokens": ["Da", "liegt", "Eu\u00b7ro\u00b7pa", ".", "Wie", "sieht", "es", "aus", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wie ein bunt angestrichnes Irrenhaus.", "tokens": ["Wie", "ein", "bunt", "an\u00b7ge\u00b7strich\u00b7nes", "Ir\u00b7ren\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Nationen schuften auf Rekord:", "tokens": ["Die", "Na\u00b7ti\u00b7o\u00b7nen", "schuf\u00b7ten", "auf", "Re\u00b7kord", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Export! Export!", "tokens": ["Ex\u00b7port", "!", "Ex\u00b7port", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die andern! Die andern sollen kaufen!", "tokens": ["Die", "an\u00b7dern", "!", "Die", "an\u00b7dern", "sol\u00b7len", "kau\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "ART", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die andern sollen die Weine saufen!", "tokens": ["Die", "an\u00b7dern", "sol\u00b7len", "die", "Wei\u00b7ne", "sau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die andern sollen die Schiffe heuern!", "tokens": ["Die", "an\u00b7dern", "sol\u00b7len", "die", "Schif\u00b7fe", "heu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die andern sollen die Kohlen verfeuern!", "tokens": ["Die", "an\u00b7dern", "sol\u00b7len", "die", "Koh\u00b7len", "ver\u00b7feu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wir?", "tokens": ["Wir", "?"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Zollhaus, Grenzpfahl und Einfuhrschein:", "tokens": ["Zoll\u00b7haus", ",", "Grenz\u00b7pfahl", "und", "Ein\u00b7fuhr\u00b7schein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "wir lassen nicht das geringste herein.", "tokens": ["wir", "las\u00b7sen", "nicht", "das", "ge\u00b7rings\u00b7te", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wir nicht. Wir haben ein Ideal:", "tokens": ["Wir", "nicht", ".", "Wir", "ha\u00b7ben", "ein", "I\u00b7deal", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Wir hungern. Aber streng national.", "tokens": ["Wir", "hun\u00b7gern", ".", "A\u00b7ber", "streng", "na\u00b7ti\u00b7o\u00b7nal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Fahnen und Hymnen an allen Ecken.", "tokens": ["Fah\u00b7nen", "und", "Hym\u00b7nen", "an", "al\u00b7len", "E\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Europa? Europa soll doch verrecken!", "tokens": ["Eu\u00b7ro\u00b7pa", "?", "Eu\u00b7ro\u00b7pa", "soll", "doch", "ver\u00b7re\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und wenn alles der Pleite entgegentreibt:", "tokens": ["Und", "wenn", "al\u00b7les", "der", "Plei\u00b7te", "ent\u00b7ge\u00b7gen\u00b7treibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "da\u00df nur die Nation erhalten bleibt!", "tokens": ["da\u00df", "nur", "die", "Na\u00b7tion", "er\u00b7hal\u00b7ten", "bleibt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Menschen braucht es nicht mehr zu geben.", "tokens": ["Men\u00b7schen", "braucht", "es", "nicht", "mehr", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.19": {"text": "England! Polen! Italien mu\u00df leben!", "tokens": ["En\u00b7gland", "!", "Po\u00b7len", "!", "I\u00b7ta\u00b7li\u00b7en", "mu\u00df", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "VMFIN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Der Staat fri\u00dft uns auf. Ein Gespenst. Ein Begriff.", "tokens": ["Der", "Staat", "fri\u00dft", "uns", "auf", ".", "Ein", "Ge\u00b7spenst", ".", "Ein", "Be\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "Der Staat, das ist ein Ding mitm Pfiff.", "tokens": ["Der", "Staat", ",", "das", "ist", "ein", "Ding", "mitm", "Pfiff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Das Ding ragt auf bis zu den Sternen \u2013", "tokens": ["Das", "Ding", "ragt", "auf", "bis", "zu", "den", "Ster\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "von dem kann noch die Kirche was lernen.", "tokens": ["von", "dem", "kann", "noch", "die", "Kir\u00b7che", "was", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VMFIN", "ADV", "ART", "NN", "PIS", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Jeder soll kaufen. Niemand kann kaufen.", "tokens": ["Je\u00b7der", "soll", "kau\u00b7fen", ".", "Nie\u00b7mand", "kann", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$.", "PIS", "VMFIN", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Es rauchen die v\u00f6lkischen Scheiterhaufen.", "tokens": ["Es", "rau\u00b7chen", "die", "v\u00f6l\u00b7ki\u00b7schen", "Schei\u00b7ter\u00b7hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Es lodern die v\u00f6lkischen Opferfeuer:", "tokens": ["Es", "lo\u00b7dern", "die", "v\u00f6l\u00b7ki\u00b7schen", "Op\u00b7fer\u00b7feu\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Der Sinn des Lebens ist die Steuer!", "tokens": ["Der", "Sinn", "des", "Le\u00b7bens", "ist", "die", "Steu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der Himmel sei unser Konkursverwalter!", "tokens": ["Der", "Him\u00b7mel", "sei", "un\u00b7ser", "Kon\u00b7kurs\u00b7ver\u00b7wal\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Die Neuzeit tanzt als Mittelalter.", "tokens": ["Die", "Neu\u00b7zeit", "tanzt", "als", "Mit\u00b7tel\u00b7al\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Nation ist das achte Sakrament \u2013!", "tokens": ["Die", "Na\u00b7tion", "ist", "das", "ach\u00b7te", "Sak\u00b7ra\u00b7ment", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gott segne diesen Kontinent.", "tokens": ["Gott", "seg\u00b7ne", "die\u00b7sen", "Kon\u00b7ti\u00b7nent", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}