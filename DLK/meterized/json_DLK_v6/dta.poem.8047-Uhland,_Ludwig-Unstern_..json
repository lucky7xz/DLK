{"dta.poem.8047": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Unstern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Unstern, diesem guten Jungen,", "tokens": ["Uns\u00b7tern", ",", "die\u00b7sem", "gu\u00b7ten", "Jun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat es seltsam sich geschickt,", "tokens": ["Hat", "es", "selt\u00b7sam", "sich", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches w\u00e4r\u2019 ihm fast gelungen,", "tokens": ["Man\u00b7ches", "w\u00e4r'", "ihm", "fast", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manches w\u00e4r\u2019 ihm schier gegl\u00fcckt.", "tokens": ["Man\u00b7ches", "w\u00e4r'", "ihm", "schier", "ge\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Gl\u00fcckesstern\u2019 im Bunde", "tokens": ["Al\u00b7le", "Gl\u00fc\u00b7ckess\u00b7tern'", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4tten weihend ihm gelacht,", "tokens": ["H\u00e4t\u00b7ten", "wei\u00b7hend", "ihm", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn die Mutter eine Stunde", "tokens": ["Wenn", "die", "Mut\u00b7ter", "ei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fr\u00fcher ihn zur Welt gebracht.", "tokens": ["Fr\u00fc\u00b7her", "ihn", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Waffenruhm und Heldenehre", "tokens": ["Waf\u00b7fen\u00b7ruhm", "und", "Hel\u00b7de\u00b7neh\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tten zeitig ihm gebl\u00fcht,", "tokens": ["H\u00e4t\u00b7ten", "zei\u00b7tig", "ihm", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War doch in dem ganzen Heere", "tokens": ["War", "doch", "in", "dem", "gan\u00b7zen", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner so von Muth ergl\u00fcht:", "tokens": ["Kei\u00b7ner", "so", "von", "Muth", "er\u00b7gl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur als schon in wilden Wogen", "tokens": ["Nur", "als", "schon", "in", "wil\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Schaar zum Sturme drang,", "tokens": ["Sei\u00b7ne", "Schaar", "zum", "Stur\u00b7me", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kam ein Bote hergeflogen,", "tokens": ["Kam", "ein", "Bo\u00b7te", "her\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der die Friedensfahne schwang.", "tokens": ["Der", "die", "Frie\u00b7dens\u00b7fah\u00b7ne", "schwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nah ist Unsterns Hochzeitfeier,", "tokens": ["Nah", "ist", "Uns\u00b7terns", "Hoch\u00b7zeit\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hold und sittig gl\u00fcht die Braut;", "tokens": ["Hold", "und", "sit\u00b7tig", "gl\u00fcht", "die", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh! da k\u00f6mmt ein reichrer Freier,", "tokens": ["Sieh", "!", "da", "k\u00f6mmt", "ein", "reich\u00b7rer", "Frei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die Eltern ba\u00df erbaut.", "tokens": ["Der", "die", "El\u00b7tern", "ba\u00df", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch h\u00e4tte die Geraubte", "tokens": ["Den\u00b7noch", "h\u00e4t\u00b7te", "die", "Ge\u00b7raub\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn als Wittwe noch begl\u00fcckt,", "tokens": ["Ihn", "als", "Witt\u00b7we", "noch", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4re nicht der Todtgeglaubte", "tokens": ["W\u00e4\u00b7re", "nicht", "der", "Todt\u00b7ge\u00b7glaub\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pl\u00f6tzlich wieder anger\u00fcckt.", "tokens": ["Pl\u00f6tz\u00b7lich", "wie\u00b7der", "an\u00b7ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Reich w\u00e4r\u2019 Unstern noch geworden", "tokens": ["Reich", "w\u00e4r'", "Uns\u00b7tern", "noch", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "NN", "ADV", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Gut der neuen Welt,", "tokens": ["Mit", "dem", "Gut", "der", "neu\u00b7en", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte nicht ein Sturm aus Norden", "tokens": ["H\u00e4t\u00b7te", "nicht", "ein", "Sturm", "aus", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch im Port das Schiff zerschellt.", "tokens": ["Noch", "im", "Port", "das", "Schiff", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NE", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00fccklich war er selbst entschwommen,", "tokens": ["Gl\u00fcck\u00b7lich", "war", "er", "selbst", "ent\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer Planke hatt\u2019 er\u2019s Dank,", "tokens": ["Ei\u00b7ner", "Plan\u00b7ke", "hatt'", "er's", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hatte schon den Strand erklommen,", "tokens": ["Hat\u00b7te", "schon", "den", "Strand", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Glitt zur\u00fcck noch und versank.", "tokens": ["Glitt", "zu\u00b7r\u00fcck", "noch", "und", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In den Himmel, sonder Zweifel,", "tokens": ["In", "den", "Him\u00b7mel", ",", "son\u00b7der", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrd\u2019 er gleich gekommen seyn,", "tokens": ["W\u00fcrd'", "er", "gleich", "ge\u00b7kom\u00b7men", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liefe nicht ein dummer Teufel", "tokens": ["Lie\u00b7fe", "nicht", "ein", "dum\u00b7mer", "Teu\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Just ihm in den Weg hinein.", "tokens": ["Just", "ihm", "in", "den", "Weg", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Teufel meint, es sey die Seele,", "tokens": ["Teu\u00b7fel", "meint", ",", "es", "sey", "die", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die er eben holen soll,", "tokens": ["Die", "er", "e\u00b7ben", "ho\u00b7len", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Packt den Unstern an der Kehle,", "tokens": ["Packt", "den", "Uns\u00b7tern", "an", "der", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rennt mit ihm davon wie toll.", "tokens": ["Rennt", "mit", "ihm", "da\u00b7von", "wie", "toll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PAV", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da erscheint ein lichter Engel", "tokens": ["Da", "er\u00b7scheint", "ein", "lich\u00b7ter", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rettend aus dem Nebelduft,", "tokens": ["Ret\u00b7tend", "aus", "dem", "Ne\u00b7bel\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Donnert flugs den schwarzen Bengel", "tokens": ["Don\u00b7nert", "flugs", "den", "schwar\u00b7zen", "Ben\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die tiefste H\u00f6llenkluft,", "tokens": ["In", "die", "tiefs\u00b7te", "H\u00f6l\u00b7len\u00b7kluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwebt der goldnen Himmelsferne", "tokens": ["Schwebt", "der", "gold\u00b7nen", "Him\u00b7mels\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem armen Unstern zu,", "tokens": ["Mit", "dem", "ar\u00b7men", "Uns\u00b7tern", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ueber gut\u2019 und b\u00f6se Sterne", "tokens": ["Ue\u00b7ber", "gut'", "und", "b\u00f6\u00b7se", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchrt er den zur ew\u2019gen Ruh.", "tokens": ["F\u00fchrt", "er", "den", "zur", "ew'\u00b7gen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}