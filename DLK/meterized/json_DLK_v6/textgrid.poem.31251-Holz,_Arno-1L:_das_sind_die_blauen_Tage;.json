{"textgrid.poem.31251": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: das sind die blauen Tage;", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "das sind die blauen Tage;", "tokens": ["das", "sind", "die", "blau\u00b7en", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ich bin ausser mir/ verzukkt/", "tokens": ["ich", "bin", "aus\u00b7ser", "mir", "/", "ver\u00b7zukkt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$(", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ich wei\u00df nicht/ wa\u00df ich sage.", "tokens": ["ich", "wei\u00df", "nicht", "/", "wa\u00df", "ich", "sa\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Itzt ist die gantze Welt", "tokens": ["Itzt", "ist", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ein bundtes Rohsen-Feld/", "tokens": ["ein", "bund\u00b7tes", "Roh\u00b7sen\u00b7Feld", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "itzt ist es sch\u00f6n;", "tokens": ["itzt", "ist", "es", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "die schwartze Nachtigall", "tokens": ["die", "schwart\u00b7ze", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "ver\u00fcbt am Wasser-Fall", "tokens": ["ver\u00b7\u00fcbt", "am", "Was\u00b7ser\u00b7Fall"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "ihr s\u00fc\u00df Geth\u00f6n!", "tokens": ["ihr", "s\u00fc\u00df", "Ge\u00b7th\u00f6n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Zwischen Hertz-Kraut und Kamillen/", "tokens": ["Zwi\u00b7schen", "Hertz\u00b7Kraut", "und", "Ka\u00b7mil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$("], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "drau\u00df die z\u00e4hrtsten D\u00fcffte qwillen/", "tokens": ["drau\u00df", "die", "z\u00e4hrts\u00b7ten", "D\u00fcff\u00b7te", "qwil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lihgt ", "tokens": ["lihgt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "unter einem ", "tokens": ["un\u00b7ter", "ei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "der/ wenn fr\u00fch die Sonne blizzt/", "tokens": ["der", "/", "wenn", "fr\u00fch", "die", "Son\u00b7ne", "blizzt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "nichts al\u00df nur Juwelen sprizzt.", "tokens": ["nichts", "al\u00df", "nur", "Ju\u00b7we\u00b7len", "sprizzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blanck darzwischen steht betaut", "tokens": ["Blanck", "darz\u00b7wi\u00b7schen", "steht", "be\u00b7taut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PAV", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Frauen-Hahr und Wihgen-Kraut/", "tokens": ["Frau\u00b7en\u00b7Hahr", "und", "Wih\u00b7gen\u00b7Kraut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "kleine Schaafe wei\u00df wie Schnee", "tokens": ["klei\u00b7ne", "Schaa\u00b7fe", "wei\u00df", "wie", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "weiden \u00fcmb den M\u00fcmmel-See.", "tokens": ["wei\u00b7den", "\u00fcmb", "den", "M\u00fcm\u00b7mel\u00b7See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Feister Klee/ gesunder Qwendel/", "tokens": ["Feis\u00b7ter", "Klee", "/", "ge\u00b7sun\u00b7der", "Qwen\u00b7del", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "krause M\u00fcntze und Lawendel/", "tokens": ["krau\u00b7se", "M\u00fcnt\u00b7ze", "und", "La\u00b7wen\u00b7del", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$("], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Hahnen-Tau und Sonnen-Rauch/", "tokens": ["Hah\u00b7nen\u00b7Tau", "und", "Son\u00b7nen\u00b7Rauch", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "alles mu\u00df in ihren Bauch!", "tokens": ["al\u00b7les", "mu\u00df", "in", "ih\u00b7ren", "Bauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie \u00fcmb den Ulmen-Baum die Rebe/", "tokens": ["Wie", "\u00fcmb", "den", "Ul\u00b7men\u00b7Baum", "die", "Re\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "schlingt sich \u00fcmb ", "tokens": ["schlingt", "sich", "\u00fcmb"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "man siht den schilfichten ", "tokens": ["man", "siht", "den", "schil\u00b7fich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "vergn\u00fcgt bey ", "tokens": ["ver\u00b7gn\u00fcgt", "bey"], "token_info": ["word", "word"], "pos": ["VVPP", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "kikkert/ kukk mal/ wie ich bin.", "tokens": ["kik\u00b7kert", "/", "kukk", "mal", "/", "wie", "ich", "bin", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVIMP", "ADV", "$(", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "seinen blancken Harnisch weck.", "tokens": ["sei\u00b7nen", "blan\u00b7cken", "Har\u00b7nisch", "weck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Drauff so trukken beyde sich", "tokens": ["Drauff", "so", "truk\u00b7ken", "bey\u00b7de", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PIS", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "embsiglich!", "tokens": ["emb\u00b7sig\u00b7lich", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.9": {"text": "F\u00fcr nichts al\u00df Rohsen-Ketten", "tokens": ["F\u00fcr", "nichts", "al\u00df", "Roh\u00b7sen\u00b7Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "wei\u00df er sich kaum zu retten/", "tokens": ["wei\u00df", "er", "sich", "kaum", "zu", "ret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "au\u00df ihren Mund-Korallen", "tokens": ["au\u00df", "ih\u00b7ren", "Mun\u00b7dKo\u00b7ral\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "auff ihn bloh\u00df K\u00fcssckens knallen.", "tokens": ["auff", "ihn", "bloh\u00df", "K\u00fcs\u00b7sckens", "knal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "St\u00e4hts bem\u00fcht/ sich zu verpaaren/", "tokens": ["St\u00e4hts", "be\u00b7m\u00fcht", "/", "sich", "zu", "ver\u00b7paa\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PRF", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "pakkt er sie bey ihren Hahren/", "tokens": ["pakkt", "er", "sie", "bey", "ih\u00b7ren", "Hah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "s\u00fc\u00df durch seine Raserey", "tokens": ["s\u00fc\u00df", "durch", "sei\u00b7ne", "Ra\u00b7se\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "trifft sein Ohr ihr Lihbes-Schrey!", "tokens": ["trifft", "sein", "Ohr", "ihr", "Lih\u00b7bes\u00b7Schrey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rosilis lihgt auff dem R\u00fckken/", "tokens": ["Ro\u00b7si\u00b7lis", "lihgt", "auff", "dem", "R\u00fck\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nun sey bloh\u00df kein Knoten-Stokk/", "tokens": ["nun", "sey", "bloh\u00df", "kein", "Kno\u00b7ten\u00b7Stokk", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "du br\u00e4uchst dich nur nach ihr zu b\u00fckken/", "tokens": ["du", "br\u00e4uchst", "dich", "nur", "nach", "ihr", "zu", "b\u00fck\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "der Wind hebt ihren Sommer-Rokk.", "tokens": ["der", "Wind", "hebt", "ih\u00b7ren", "Som\u00b7mer\u00b7Rokk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts nicht l\u00e4sst er ohnber\u00fchrt/", "tokens": ["Nichts", "nicht", "l\u00e4sst", "er", "ohn\u00b7be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wie sich solches itzt geb\u00fchrt;", "tokens": ["wie", "sich", "sol\u00b7ches", "itzt", "ge\u00b7b\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "in den Zweigen auff und nihder", "tokens": ["in", "den", "Zwei\u00b7gen", "auff", "und", "nih\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "dantzt verbuhlt das Lufft-Gefihder!", "tokens": ["dantzt", "ver\u00b7buhlt", "das", "Lufft\u00b7Ge\u00b7fih\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gleich so nehm ich bey ihr Blazz:", "tokens": ["Gleich", "so", "nehm", "ich", "bey", "ihr", "Blazz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kindgen/ traumt dir itzt dein Schazz?"