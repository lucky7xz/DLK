{"textgrid.poem.38076": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Inkognito", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kamen drey Diebe aus Morgenland,", "tokens": ["Es", "ka\u00b7men", "drey", "Die\u00b7be", "aus", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die geben sich f\u00fcr drey Grafen aus,", "tokens": ["Die", "ge\u00b7ben", "sich", "f\u00fcr", "drey", "Gra\u00b7fen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie kamen vor der Frau Wirthin Haus:", "tokens": ["Sie", "ka\u00b7men", "vor", "der", "Frau", "Wirt\u00b7hin", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "\u00bbfrau Wirthin hat sie es diese Gewalt,", "tokens": ["\u00bb", "frau", "Wirt\u00b7hin", "hat", "sie", "es", "die\u00b7se", "Ge\u00b7walt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "VAFIN", "PPER", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da\u00df sie \u00fcber Nacht drey Grafen b'halt?\u00ab", "tokens": ["Da\u00df", "sie", "\u00fc\u00b7ber", "Nacht", "drey", "Gra\u00b7fen", "hal", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "CARD", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u00bbwenn ich es diese Gewalt nicht h\u00e4tt,", "tokens": ["\u00bb", "wenn", "ich", "es", "die\u00b7se", "Ge\u00b7walt", "nicht", "h\u00e4tt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "PDAT", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was w\u00e4r mir denn die Wirthschaft nutze?\u00ab", "tokens": ["Was", "w\u00e4r", "mir", "denn", "die", "Wirth\u00b7schaft", "nut\u00b7ze", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der erste that die Pferde in Stall,", "tokens": ["Der", "ers\u00b7te", "that", "die", "Pfer\u00b7de", "in", "Stall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Der andere schwenkt das Futter hinein,", "tokens": ["Der", "an\u00b7de\u00b7re", "schwenkt", "das", "Fut\u00b7ter", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der dritte trat zur K\u00fcche hinein,", "tokens": ["Der", "drit\u00b7te", "trat", "zur", "K\u00fc\u00b7che", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Und k\u00fc\u00dfte der Frau Wirthin ihr M\u00e4dlein,", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "der", "Frau", "Wirt\u00b7hin", "ihr", "M\u00e4d\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Oder ist es ihr getreues T\u00f6chterlein?", "tokens": ["O\u00b7der", "ist", "es", "ihr", "ge\u00b7treu\u00b7es", "T\u00f6ch\u00b7ter\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Es ist mein getreues T\u00f6chterlein,", "tokens": ["Es", "ist", "mein", "ge\u00b7treu\u00b7es", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Es soll euch zapfen Bier und Wein.", "tokens": ["Es", "soll", "euch", "zap\u00b7fen", "Bier", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Erste sprach: Das M\u00e4gdlein ist mein,", "tokens": ["Der", "Ers\u00b7te", "sprach", ":", "Das", "M\u00e4gd\u00b7lein", "ist", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Ich hab ihm gegeben ein Ringelein!", "tokens": ["Ich", "hab", "ihm", "ge\u00b7ge\u00b7ben", "ein", "Rin\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Der andere sprach: Das M\u00e4dchen ist mein,", "tokens": ["Der", "an\u00b7de\u00b7re", "sprach", ":", "Das", "M\u00e4d\u00b7chen", "ist", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Ich hab ihm gegeben ein Glas voll Wein.", "tokens": ["Ich", "hab", "ihm", "ge\u00b7ge\u00b7ben", "ein", "Glas", "voll", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Der dritte sprach: Das M\u00e4dchen w\u00e4r werth,", "tokens": ["Der", "drit\u00b7te", "sprach", ":", "Das", "M\u00e4d\u00b7chen", "w\u00e4r", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Da\u00df wir es theilten mit unserem Schwerdt.", "tokens": ["Da\u00df", "wir", "es", "theil\u00b7ten", "mit", "un\u00b7se\u00b7rem", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Sie gaben der Frau Wirthin einen s\u00fc\u00dfen Getrank,", "tokens": ["Sie", "ga\u00b7ben", "der", "Frau", "Wirt\u00b7hin", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Ge\u00b7trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.22": {"text": "Da\u00df sie vom Stuhl ins Bette hinsank.", "tokens": ["Da\u00df", "sie", "vom", "Stuhl", "ins", "Bet\u00b7te", "hin\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Das M\u00e4gdlein greift der Mutter wohl an den Mund:", "tokens": ["Das", "M\u00e4gd\u00b7lein", "greift", "der", "Mut\u00b7ter", "wohl", "an", "den", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Ach Mutter leb jetzt noch eine Stund!", "tokens": ["Ach", "Mut\u00b7ter", "leb", "jetzt", "noch", "ei\u00b7ne", "Stund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Es greift der Mutter wohl an die Brust:", "tokens": ["Es", "greift", "der", "Mut\u00b7ter", "wohl", "an", "die", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Ach Gott wenn das mein Vater wu\u00dft!", "tokens": ["Ach", "Gott", "wenn", "das", "mein", "Va\u00b7ter", "wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "KOUS", "PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Es greift der Mutter wohl an die H\u00e4nd:", "tokens": ["Es", "greift", "der", "Mut\u00b7ter", "wohl", "an", "die", "H\u00e4nd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Ach Mutter du bist am letzten End!", "tokens": ["Ach", "Mut\u00b7ter", "du", "bist", "am", "letz\u00b7ten", "End", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Es greift der Mutter wohl an die F\u00fc\u00df:", "tokens": ["Es", "greift", "der", "Mut\u00b7ter", "wohl", "an", "die", "F\u00fc\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Ach Mutter was ist der Schlaf so s\u00fc\u00df.", "tokens": ["Ach", "Mut\u00b7ter", "was", "ist", "der", "Schlaf", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PWS", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Sie legten es auf einen viereckten Tisch", "tokens": ["Sie", "leg\u00b7ten", "es", "auf", "ei\u00b7nen", "vier\u00b7eck\u00b7ten", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Und theilten es wie ein Wasserfisch,", "tokens": ["Und", "theil\u00b7ten", "es", "wie", "ein", "Was\u00b7ser\u00b7fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Und wo ein Tr\u00f6pfchen Blut hinsprang,", "tokens": ["Und", "wo", "ein", "Tr\u00f6pf\u00b7chen", "Blut", "hin\u00b7sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Da sa\u00df ein Engel ein Jahr und sang.", "tokens": ["Da", "sa\u00df", "ein", "En\u00b7gel", "ein", "Jahr", "und", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Und wo der M\u00f6rder das Schwerdt hinlegt,", "tokens": ["Und", "wo", "der", "M\u00f6r\u00b7der", "das", "Schwerdt", "hin\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Da sa\u00df ein Rabe ein Jahr und kr\u00e4ht.", "tokens": ["Da", "sa\u00df", "ein", "Ra\u00b7be", "ein", "Jahr", "und", "kr\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}