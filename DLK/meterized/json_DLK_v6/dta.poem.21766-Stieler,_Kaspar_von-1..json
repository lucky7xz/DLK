{"dta.poem.21766": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Warum ich nur von Lieben", "tokens": ["Wa\u00b7rum", "ich", "nur", "von", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die Bl\u00e4tter voll geschrieben/", "tokens": ["die", "Bl\u00e4t\u00b7ter", "voll", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "warum mein Buch verz\u00e4rtlet lacht", "tokens": ["wa\u00b7rum", "mein", "Buch", "ver\u00b7z\u00e4rt\u00b7let", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "m\u00f6cht\u2019 einer wundernd fragen.", "tokens": ["m\u00f6cht'", "ei\u00b7ner", "wun\u00b7dernd", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dr\u00fcm wil ich selber sagen", "tokens": ["Dr\u00fcm", "wil", "ich", "sel\u00b7ber", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "was mich darzu hat angebracht:", "tokens": ["was", "mich", "dar\u00b7zu", "hat", "an\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Feuer-hauch der Musen", "tokens": ["Der", "Feu\u00b7er\u00b7hauch", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "hat meinen engen Busen", "tokens": ["hat", "mei\u00b7nen", "en\u00b7gen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "mit solchen Flammen nicht ger\u00fchrt.", "tokens": ["mit", "sol\u00b7chen", "Flam\u00b7men", "nicht", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Apoll ist hier nicht Meifter/", "tokens": ["A\u00b7poll", "ist", "hier", "nicht", "Meif\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PTKNEG", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "nicht Pallas/ so die Geister", "tokens": ["nicht", "Pal\u00b7las", "/", "so", "die", "Geis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$(", "ADV", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "auff Helikons Geb\u00fcsche f\u00fchrt.", "tokens": ["auff", "He\u00b7li\u00b7kons", "Ge\u00b7b\u00fc\u00b7sche", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Lust/ die Red\u2019 und Bltkke/", "tokens": ["Die", "Lust", "/", "die", "Red'", "und", "Bltk\u00b7ke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der Glieder ihr Geschikke/", "tokens": ["der", "Glie\u00b7der", "ihr", "Ge\u00b7schik\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und was Rosillen mehr besch\u00f6nt:", "tokens": ["und", "was", "Ro\u00b7sil\u00b7len", "mehr", "be\u00b7sch\u00f6nt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr Wesen/ Kleidung/ Lachen", "tokens": ["Ihr", "We\u00b7sen", "/", "Klei\u00b7dung", "/", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$(", "NN", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Betr\u00fcbni\u00df/ Schlaf und Wachen", "tokens": ["Be\u00b7tr\u00fcb\u00b7ni\u00df", "/", "Schlaf", "und", "Wa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "hat mich mit Efeu umgekr\u00f6nt.", "tokens": ["hat", "mich", "mit", "E\u00b7feu", "um\u00b7ge\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Straks bin ich ein Poete/", "tokens": ["Straks", "bin", "ich", "ein", "Poe\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "wenn ihre Wangen-r\u00f6hte", "tokens": ["wenn", "ih\u00b7re", "Wan\u00b7gen\u00b7r\u00f6h\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "im weissem Alabaster blikkt.", "tokens": ["im", "weis\u00b7sem", "A\u00b7la\u00b7bas\u00b7ter", "blikkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn in die g\u00f6ldne Seiten", "tokens": ["Wenn", "in", "die", "g\u00f6ld\u00b7ne", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "wil ihre Kehle streiten/", "tokens": ["wil", "ih\u00b7re", "Keh\u00b7le", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ist wo ihr Leib entbl\u00f6sset:", "tokens": ["Ist", "wo", "ihr", "Leib", "ent\u00b7bl\u00f6s\u00b7set", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit Wasser au\u00df dem Pferde-Gu\u00df.", "tokens": ["mit", "Was\u00b7ser", "au\u00df", "dem", "Pfer\u00b7de\u00b7Gu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff ihr Bewegen/ regen/", "tokens": ["Auff", "ihr", "Be\u00b7we\u00b7gen", "/", "re\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "w\u00e4chst mir geschwind entgegen", "tokens": ["w\u00e4chst", "mir", "ge\u00b7schwind", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "ein Buch/ das Troja trozzen mu\u00df.", "tokens": ["ein", "Buch", "/", "das", "Tro\u00b7ja", "troz\u00b7zen", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der mag die Tugend melden", "tokens": ["Der", "mag", "die", "Tu\u00b7gend", "mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und der die alten Helden", "tokens": ["und", "der", "die", "al\u00b7ten", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "au\u00df Teutschland tragen zu Papier/", "tokens": ["au\u00df", "Teutschland", "tra\u00b7gen", "zu", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "der hohe Sachen schreiben:", "tokens": ["der", "ho\u00b7he", "Sa\u00b7chen", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wil die Liebe treiben", "tokens": ["Ich", "wil", "die", "Lie\u00b7be", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und wie Rosille mir komt f\u00fcr.", "tokens": ["und", "wie", "Ro\u00b7sil\u00b7le", "mir", "komt", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "PPER", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Schiffer schwazzt von St\u00fcrmen/", "tokens": ["Der", "Schif\u00b7fer", "schwazzt", "von", "St\u00fcr\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Krieger praalt von T\u00fcrmen", "tokens": ["Der", "Krie\u00b7ger", "praalt", "von", "T\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "die er so oft erstiegen hat/", "tokens": ["die", "er", "so", "oft", "er\u00b7stie\u00b7gen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Bauer lobt die Felder/", "tokens": ["der", "Bau\u00b7er", "lobt", "die", "Fel\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "der J\u00e4ger Wild und W\u00e4lder/", "tokens": ["der", "J\u00e4\u00b7ger", "Wild", "und", "W\u00e4l\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der Reisender so manche Stat:", "tokens": ["der", "Rei\u00b7sen\u00b7der", "so", "man\u00b7che", "Stat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich bin ein Jungfer-lieber/", "tokens": ["Ich", "bin", "ein", "Jung\u00b7fer\u00b7lie\u00b7ber", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die Zunge geht mir \u00fcber", "tokens": ["die", "Zun\u00b7ge", "geht", "mir", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "von dehm/ was au\u00df dem Hertzen quillt.", "tokens": ["von", "dehm", "/", "was", "au\u00df", "dem", "Hert\u00b7zen", "quillt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "PWS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer mich hierum wil schelten/", "tokens": ["Wer", "mich", "hie\u00b7rum", "wil", "schel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "der fluche den Gew\u00e4lten/", "tokens": ["der", "flu\u00b7che", "den", "Ge\u00b7w\u00e4l\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die ob uns hat ein Weibes-Bild.", "tokens": ["die", "ob", "uns", "hat", "ein", "Wei\u00b7bes\u00b7Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}