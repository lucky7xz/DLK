{"textgrid.poem.59945": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der kluge Mann h\u00e4lt sich zur\u00fcck", "tokens": ["Der", "klu\u00b7ge", "Mann", "h\u00e4lt", "sich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und streift im Fluge nur das Gl\u00fcck;", "tokens": ["Und", "streift", "im", "Flu\u00b7ge", "nur", "das", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es immer zu ersch\u00f6pfen", "tokens": ["Es", "im\u00b7mer", "zu", "er\u00b7sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ziemt nur den hohlen K\u00f6pfen,", "tokens": ["Ziemt", "nur", "den", "hoh\u00b7len", "K\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die glauben, da\u00df dem Hochgenu\u00df", "tokens": ["Die", "glau\u00b7ben", ",", "da\u00df", "dem", "Hoch\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein tiefer Fall stets folgen mu\u00df.", "tokens": ["Ein", "tie\u00b7fer", "Fall", "stets", "fol\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Biene gleiche, die sich labt", "tokens": ["Der", "Bie\u00b7ne", "glei\u00b7che", ",", "die", "sich", "labt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An holden Blumen duftbegabt:", "tokens": ["An", "hol\u00b7den", "Blu\u00b7men", "duft\u00b7be\u00b7gabt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sagt auf ihrem Wandern", "tokens": ["Sie", "sagt", "auf", "ih\u00b7rem", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht einer von der andern.", "tokens": ["Nicht", "ei\u00b7ner", "von", "der", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}