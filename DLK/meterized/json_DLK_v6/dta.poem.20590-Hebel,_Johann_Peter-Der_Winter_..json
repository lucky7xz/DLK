{"dta.poem.20590": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.85", "nl:0.14"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Isch echt do obe Bauwele feil?               ", "tokens": ["Isch", "echt", "do", "o\u00b7be", "Bau\u00b7we\u00b7le", "feil", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sie sch\u00fctten eim e redli Theil", "tokens": ["Sie", "sch\u00fct\u00b7ten", "eim", "e", "red\u00b7li", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "FM", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in d\u2019 G\u00e4rten aben und ufs Hus;", "tokens": ["in", "d'", "G\u00e4r\u00b7ten", "a\u00b7ben", "und", "ufs", "Hus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "KON", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "es schneit doch au, es e isch Gruus;", "tokens": ["es", "schneit", "doch", "au", ",", "es", "e", "isch", "Gru\u00b7us", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und \u2019s hangt no menge Wage voll", "tokens": ["und", "'s", "hangt", "no", "men\u00b7ge", "Wa\u00b7ge", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NE", "ADJA", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "am Himmel obe, merki wol.", "tokens": ["am", "Him\u00b7mel", "o\u00b7be", ",", "mer\u00b7ki", "wol", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wo ne Ma vo witem lauft,", "tokens": ["Und", "wo", "ne", "Ma", "vo", "wi\u00b7tem", "lauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "FM", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so het er vo der Bauwele gchauft;", "tokens": ["so", "het", "er", "vo", "der", "Bau\u00b7we\u00b7le", "gchauft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "er treit sie uf der Achsle no,", "tokens": ["er", "treit", "sie", "uf", "der", "Achs\u00b7le", "no", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und uffem Hut, und lauft dervo.", "tokens": ["und", "uf\u00b7fem", "Hut", ",", "und", "lauft", "der\u00b7vo", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was laufsch denn so, du n\u00e4rsche Ma?", "tokens": ["Was", "lauf\u00b7sch", "denn", "so", ",", "du", "n\u00e4r\u00b7sche", "Ma", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "De wirsch sie doch nit gstole ha?", "tokens": ["De", "wirsch", "sie", "doch", "nit", "gs\u00b7to\u00b7le", "ha", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "ADV", "PTKNEG", "NE", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und G\u00e4rten ab, und G\u00e4rten uf,", "tokens": ["Und", "G\u00e4r\u00b7ten", "ab", ",", "und", "G\u00e4r\u00b7ten", "uf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hen alli Scheie Ch\u00e4pli uf;", "tokens": ["hen", "al\u00b7li", "Schei\u00b7e", "Ch\u00e4\u00b7pli", "uf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie st\u00f6hn wie gro\u00dfi Here do;", "tokens": ["sie", "st\u00f6hn", "wie", "gro\u00b7\u00dfi", "He\u00b7re", "do", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie meine \u2019s heigs sust niemes so.", "tokens": ["Sie", "mei\u00b7ne", "'s", "heigs", "sust", "nie\u00b7mes", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "VVFIN", "PIS", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Nu\u00dfbaum het doch au si Sach,", "tokens": ["Der", "Nu\u00df\u00b7baum", "het", "doch", "au", "si", "Sach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und \u2019s Here Hus und \u2019s Chilche-Dach.", "tokens": ["und", "'s", "He\u00b7re", "Hus", "und", "'s", "Chil\u00b7che\u00b7Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "PPER", "NN", "$."], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wo me luegt, isch Schnee uud Schnee,", "tokens": ["Und", "wo", "me", "luegt", ",", "isch", "Schnee", "u\u00b7ud", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "VVFIN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "me sieht kei Stro\u00df und Fu\u00df-Weg meh.", "tokens": ["me", "sieht", "kei", "Stro\u00df", "und", "Fu\u00df\u00b7Weg", "meh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meng Some-Ch\u00f6rnli, chlei und zart,", "tokens": ["Meng", "So\u00b7me\u00b7Ch\u00f6rn\u00b7li", ",", "chlei", "und", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "lit unterm Bode wohl verwahrt,", "tokens": ["lit", "un\u00b7term", "Bo\u00b7de", "wohl", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und schnei \u2019s, so lang es schneie mag,", "tokens": ["und", "schnei", "'s", ",", "so", "lang", "es", "schnei\u00b7e", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "es wartet uf si Ostertag.", "tokens": ["es", "war\u00b7tet", "uf", "si", "Os\u00b7ter\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Meng Summer-V\u00f6geli sch\u00f6ner Art", "tokens": ["Meng", "Sum\u00b7mer\u00b7V\u00f6\u00b7ge\u00b7li", "sch\u00f6\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "lit unterm Bode wohl verwahrt;", "tokens": ["lit", "un\u00b7term", "Bo\u00b7de", "wohl", "ver\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es het kei Chummer und kei Chlag,", "tokens": ["es", "het", "kei", "Chum\u00b7mer", "und", "kei", "Chlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wartet uf si Ostertag;", "tokens": ["und", "war\u00b7tet", "uf", "si", "Os\u00b7ter\u00b7tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und gangs au lang, er chunnt emol,", "tokens": ["und", "gangs", "au", "lang", ",", "er", "chunnt", "e\u00b7mol", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "ADJD", "$,", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und sieder schlofts, und \u2019s isch em wohl.", "tokens": ["und", "sie\u00b7der", "schlofts", ",", "und", "'s", "isch", "em", "wohl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "ADJD", "ADJA", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wenn im Fr\u00fchlig \u2019s Schw\u00e4lmli singt,", "tokens": ["Und", "wenn", "im", "Fr\u00fch\u00b7lig", "'s", "Schw\u00e4lm\u00b7li", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und d\u2019 Sunne-W\u00e4rmi abe dringt,", "tokens": ["und", "d'", "Sun\u00b7ne\u00b7W\u00e4r\u00b7mi", "a\u00b7be", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Potz tausig! wachts in iedem Grab,", "tokens": ["Potz", "tau\u00b7sig", "!", "wachts", "in", "ie\u00b7dem", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und streift si Todte-Hemdli ab.", "tokens": ["und", "streift", "si", "Tod\u00b7te\u00b7Hemd\u00b7li", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nummen au e L\u00f6chli isch,", "tokens": ["Wo", "num\u00b7men", "au", "e", "L\u00f6ch\u00b7li", "isch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "NE", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "schlieft \u2019s Leben use iung und frisch. \u2014", "tokens": ["schlieft", "'s", "Le\u00b7ben", "u\u00b7se", "i\u00b7ung", "und", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJA", "NN", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Do fliegt e hungerig Sp\u00e4tzli her!", "tokens": ["Do", "fliegt", "e", "hun\u00b7ge\u00b7rig", "Sp\u00e4tz\u00b7li", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "NE", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "e Br\u00f6sli Brod w\u00e4r si Bigehr.", "tokens": ["e", "Br\u00f6s\u00b7li", "Brod", "w\u00e4r", "si", "Bi\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es luegt ein so verb\u00e4rmtli a;", "tokens": ["Es", "luegt", "ein", "so", "ver\u00b7b\u00e4rmt\u00b7li", "a", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2019s het sieder nechte n\u00fct meh gha.", "tokens": ["'s", "het", "sie\u00b7der", "nech\u00b7te", "n\u00fct", "meh", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gell B\u00fcrstli, sel isch anderi Zit,", "tokens": ["Gell", "B\u00fcrst\u00b7li", ",", "sel", "isch", "an\u00b7de\u00b7ri", "Zit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "wenn \u2019s Chorn in alle Fure lit?", "tokens": ["wenn", "'s", "Chorn", "in", "al\u00b7le", "Fu\u00b7re", "lit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Do hesch! Lo\u00df andern au dervo!", "tokens": ["Do", "hesch", "!", "Lo\u00df", "an\u00b7dern", "au", "der\u00b7vo", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NE", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bisch hungerig, chasch wieder cho! \u2014", "tokens": ["Bisch", "hun\u00b7ge\u00b7rig", ",", "cha\u00b7sch", "wie\u00b7der", "cho", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "ADV", "NE", "$.", "$("], "meter": "++--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u2019s mu\u00df wohr sy, wie\u2019s e Spr\u00fcchli git:", "tokens": ["'s", "mu\u00df", "wohr", "sy", ",", "wie's", "e", "Spr\u00fcch\u00b7li", "git", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u201esie seihe nit, und ernde nit;", "tokens": ["\u201e", "sie", "sei\u00b7he", "nit", ",", "und", "ern\u00b7de", "nit", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "KON", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201esie hen kei Pflug, und hen kei Joch,", "tokens": ["\u201e", "sie", "hen", "kei", "Pflug", ",", "und", "hen", "kei", "Joch", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "\u201eund Gott im Himmel n\u00e4hrt sie doch.\u201c", "tokens": ["\u201e", "und", "Gott", "im", "Him\u00b7mel", "n\u00e4hrt", "sie", "doch", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}