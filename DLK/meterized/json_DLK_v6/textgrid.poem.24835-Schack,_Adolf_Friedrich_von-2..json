{"textgrid.poem.24835": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00f6d' und ausgestorben alles!", "tokens": ["Wie", "\u00f6d'", "und", "aus\u00b7ge\u00b7stor\u00b7ben", "al\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch t\u00f6nt aus jedem Gang", "tokens": ["Und", "den\u00b7noch", "t\u00f6nt", "aus", "je\u00b7dem", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fl\u00fcstern mir, ein leises Regen,", "tokens": ["Ein", "Fl\u00fcs\u00b7tern", "mir", ",", "ein", "lei\u00b7ses", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mich mit Schauer f\u00fcllt, entgegen;", "tokens": ["Das", "mich", "mit", "Schau\u00b7er", "f\u00fcllt", ",", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NE", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Echo gleitet matten Schalles", "tokens": ["Ein", "E\u00b7cho", "glei\u00b7tet", "mat\u00b7ten", "Schal\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geheimnisvoll die Wand entlang.", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "die", "Wand", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Oft flieht mein Schlaf in n\u00e4cht'gen Stunden,", "tokens": ["Oft", "flieht", "mein", "Schlaf", "in", "n\u00e4cht'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn im Kamin das Heimchen zirpt;", "tokens": ["Wenn", "im", "Ka\u00b7min", "das", "Heim\u00b7chen", "zirpt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wanduhr, die seit Jahren stumme,", "tokens": ["Die", "Wan\u00b7duhr", ",", "die", "seit", "Jah\u00b7ren", "stum\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beginnt von neuem ihr Gesumme,", "tokens": ["Be\u00b7ginnt", "von", "neu\u00b7em", "ihr", "Ge\u00b7sum\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als ob sie z\u00e4hlte die Sekunden", "tokens": ["Als", "ob", "sie", "z\u00e4hl\u00b7te", "die", "Se\u00b7kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Bett des Kranken, eh' er stirbt.", "tokens": ["Am", "Bett", "des", "Kran\u00b7ken", ",", "eh'", "er", "stirbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann rauscht es in den Vorhangfalten;", "tokens": ["Dann", "rauscht", "es", "in", "den", "Vor\u00b7hang\u00b7fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf allen Treppen wird es laut;", "tokens": ["Auf", "al\u00b7len", "Trep\u00b7pen", "wird", "es", "laut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6re Rufe, wehgebrochen,", "tokens": ["Ich", "h\u00f6\u00b7re", "Ru\u00b7fe", ",", "weh\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und an den Th\u00fcren schallt ein Pochen,", "tokens": ["Und", "an", "den", "Th\u00fc\u00b7ren", "schallt", "ein", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schimmer gleitet durch die Spalten,", "tokens": ["Ein", "Schim\u00b7mer", "glei\u00b7tet", "durch", "die", "Spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor welchem meiner Seele graut.", "tokens": ["Vor", "wel\u00b7chem", "mei\u00b7ner", "See\u00b7le", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bewegen seh' ich sich die Klinken", "tokens": ["Be\u00b7we\u00b7gen", "seh'", "ich", "sich", "die", "Klin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von H\u00e4ndedr\u00fccken, mir bekannt;", "tokens": ["Von", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ",", "mir", "be\u00b7kannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich \u00f6ffne, und im matten Lichte", "tokens": ["Ich", "\u00f6ff\u00b7ne", ",", "und", "im", "mat\u00b7ten", "Lich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schaun mit gebleichtem Angesichte", "tokens": ["Schaun", "mit", "ge\u00b7bleich\u00b7tem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich Schattenbilder an und winken", "tokens": ["Mich", "Schat\u00b7ten\u00b7bil\u00b7der", "an", "und", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur\u00fcck mir mit der wei\u00dfen Hand.", "tokens": ["Zu\u00b7r\u00fcck", "mir", "mit", "der", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hinweg! hinweg! Von allen Seiten", "tokens": ["Hin\u00b7weg", "!", "hin\u00b7weg", "!", "Von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "PTKVZ", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Starrt Schrecken hier auf mich herab!", "tokens": ["Starrt", "Schre\u00b7cken", "hier", "auf", "mich", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesem Haus erstarb das Leben;", "tokens": ["In", "die\u00b7sem", "Haus", "er\u00b7starb", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch irrend noch zur Nachtzeit schweben", "tokens": ["Doch", "ir\u00b7rend", "noch", "zur", "Nacht\u00b7zeit", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Geister der vergangnen Zeiten", "tokens": ["Die", "Geis\u00b7ter", "der", "ver\u00b7gang\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um meiner Jugendfreuden Grab.", "tokens": ["Um", "mei\u00b7ner", "Ju\u00b7gend\u00b7freu\u00b7den", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}