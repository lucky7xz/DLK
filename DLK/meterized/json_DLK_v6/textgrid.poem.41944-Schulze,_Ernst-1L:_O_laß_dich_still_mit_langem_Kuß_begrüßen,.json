{"textgrid.poem.41944": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O la\u00df dich still mit langem Ku\u00df begr\u00fc\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00df dich still mit langem Ku\u00df begr\u00fc\u00dfen,", "tokens": ["O", "la\u00df", "dich", "still", "mit", "lan\u00b7gem", "Ku\u00df", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du heil'ges Thal, mein zweytes Vaterland!", "tokens": ["Du", "heil'\u00b7ges", "Thal", ",", "mein", "zwey\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo ich zuerst die Wunderblume fand,", "tokens": ["Wo", "ich", "zu\u00b7erst", "die", "Wun\u00b7der\u00b7blu\u00b7me", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die fr\u00fcher schon die Tr\u00e4ume mir verhie\u00dfen.", "tokens": ["Die", "fr\u00fc\u00b7her", "schon", "die", "Tr\u00e4u\u00b7me", "mir", "ver\u00b7hie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie manche Thr\u00e4ne mu\u00dft' ich hier vergie\u00dfen!", "tokens": ["Wie", "man\u00b7che", "Thr\u00e4\u00b7ne", "mu\u00dft'", "ich", "hier", "ver\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie br\u00e4utlich hielt die Lust mich oft umspannt!", "tokens": ["Wie", "br\u00e4ut\u00b7lich", "hielt", "die", "Lust", "mich", "oft", "um\u00b7spannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Freud' und Schmerz, wie seyd ihr nah verwandt!", "tokens": ["O", "Freud'", "und", "Schmerz", ",", "wie", "seyd", "ihr", "nah", "ver\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie mu\u00df so oft eins aus dem andern sprie\u00dfen!", "tokens": ["Wie", "mu\u00df", "so", "oft", "eins", "aus", "dem", "an\u00b7dern", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "PIS", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du, die schon lang das dunkle Grab verh\u00fcllt,", "tokens": ["Du", ",", "die", "schon", "lang", "das", "dunk\u00b7le", "Grab", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir mu\u00df ich nahn und liebend dich umfassen,", "tokens": ["Dir", "mu\u00df", "ich", "nahn", "und", "lie\u00b7bend", "dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "KON", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mich mit Trost am bittern Gram erf\u00fcllen.", "tokens": ["Und", "mich", "mit", "Trost", "am", "bit\u00b7tern", "Gram", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn, wehe mir! das warme, bl\u00fch'nde Bild,", "tokens": ["Denn", ",", "we\u00b7he", "mir", "!", "das", "war\u00b7me", ",", "bl\u00fch'n\u00b7de", "Bild", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$.", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das einz'ge Heil, das mir dein Tod gelassen,", "tokens": ["Das", "einz'\u00b7ge", "Heil", ",", "das", "mir", "dein", "Tod", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist zu stolz, die Thr\u00e4nen mir zu stillen.", "tokens": ["Es", "ist", "zu", "stolz", ",", "die", "Thr\u00e4\u00b7nen", "mir", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}