{"textgrid.poem.61180": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Kreuzzug", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schmach und Schimpf, Europa, dir und deiner tatenlosen Ruh'!", "tokens": ["O", "Schmach", "und", "Schimpf", ",", "Eu\u00b7ro\u00b7pa", ",", "dir", "und", "dei\u00b7ner", "ta\u00b7ten\u00b7lo\u00b7sen", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "NE", "$,", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "In Flammen steht Jerusalem, und tr\u00e4ge feiernd schaust du zu;", "tokens": ["In", "Flam\u00b7men", "steht", "Je\u00b7ru\u00b7sa\u00b7lem", ",", "und", "tr\u00e4\u00b7ge", "fei\u00b7ernd", "schaust", "du", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "$,", "KON", "VVFIN", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Das Grab, darin der Heiland lag, es ward der Muselm\u00e4nner Spott,", "tokens": ["Das", "Grab", ",", "da\u00b7rin", "der", "Hei\u00b7land", "lag", ",", "es", "ward", "der", "Mu\u00b7sel\u00b7m\u00e4n\u00b7ner", "Spott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Doch du verr\u00e4tst in schn\u00f6dem Geiz noch heut wie Judas deinen Gott.", "tokens": ["Doch", "du", "ver\u00b7r\u00e4tst", "in", "schn\u00f6\u00b7dem", "Geiz", "noch", "heut", "wie", "Ju\u00b7das", "dei\u00b7nen", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ADV", "KOKOM", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' ich ein Lied so rot wie Blut und laut wie Kriegstrompetenschall,", "tokens": ["H\u00e4tt'", "ich", "ein", "Lied", "so", "rot", "wie", "Blut", "und", "laut", "wie", "Kriegs\u00b7trom\u00b7pe\u00b7ten\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KOKOM", "NN", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Zu allen Thronen sendet' ich's, bis da\u00df es f\u00e4nde Widerhall,", "tokens": ["Zu", "al\u00b7len", "Thro\u00b7nen", "sen\u00b7det'", "ich's", ",", "bis", "da\u00df", "es", "f\u00e4n\u00b7de", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "$,", "KON", "KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Von Land zu Lande sollt' es ziehn durch alles Volk des Okzidents", "tokens": ["Von", "Land", "zu", "Lan\u00b7de", "sollt'", "es", "ziehn", "durch", "al\u00b7les", "Volk", "des", "Ok\u00b7zi\u00b7dents"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und werben f\u00fcr die heil'ge Stadt, wie jener M\u00f6nch von Amienz.", "tokens": ["Und", "wer\u00b7ben", "f\u00fcr", "die", "heil'\u00b7ge", "Stadt", ",", "wie", "je\u00b7ner", "M\u00f6nch", "von", "A\u00b7mienz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PDAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Ja, rufen sollt' es aus dem Grab die Zeit, von Ruhm und Taten voll,", "tokens": ["Ja", ",", "ru\u00b7fen", "sollt'", "es", "aus", "dem", "Grab", "die", "Zeit", ",", "von", "Ruhm", "und", "Ta\u00b7ten", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVINF", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Als vor der Andacht m\u00e4cht'gem Hauch hochflatternd jedes Banner schwoll,", "tokens": ["Als", "vor", "der", "An\u00b7dacht", "m\u00e4cht'\u00b7gem", "Hauch", "hoch\u00b7flat\u00b7ternd", "je\u00b7des", "Ban\u00b7ner", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "ADJD", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Als, wo es Gottes Sache galt, der Greis der Narben nicht gedacht,", "tokens": ["Als", ",", "wo", "es", "Got\u00b7tes", "Sa\u00b7che", "galt", ",", "der", "Greis", "der", "Nar\u00b7ben", "nicht", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "NN", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und froh sein sechszehnj\u00e4hr'ges Blut der blonde Knabe dargebracht.", "tokens": ["Und", "froh", "sein", "sechs\u00b7zehn\u00b7j\u00e4hr'\u00b7ges", "Blut", "der", "blon\u00b7de", "Kna\u00b7be", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Da w\u00e4lzte sich lawinengleich durch Land und Meer der Kriegesruf,", "tokens": ["Da", "w\u00e4lz\u00b7te", "sich", "la\u00b7wi\u00b7nen\u00b7gleich", "durch", "Land", "und", "Meer", "der", "Krie\u00b7ges\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Da funkelt' hell das Christenschwert, da klang des Christenrosses Huf,", "tokens": ["Da", "fun\u00b7kelt'", "hell", "das", "Chris\u00b7ten\u00b7schwert", ",", "da", "klang", "des", "Chris\u00b7ten\u00b7ros\u00b7ses", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wie Judas Wolkens\u00e4ule zog das Kreuz den Streitern hoch voran,", "tokens": ["Wie", "Ju\u00b7das", "Wol\u00b7ken\u00b7s\u00e4u\u00b7le", "zog", "das", "Kreuz", "den", "Strei\u00b7tern", "hoch", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Bis sie vom \u00d6lberg Zions Burg im Morgenrote vor sich sahn.", "tokens": ["Bis", "sie", "vom", "\u00d6l\u00b7berg", "Zi\u00b7ons", "Burg", "im", "Mor\u00b7gen\u00b7ro\u00b7te", "vor", "sich", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NE", "NE", "APPRART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Ei, wie so anders lenkt ihr Schiff die Staatskunst jetzt in schlauer Pflicht,", "tokens": ["Ei", ",", "wie", "so", "an\u00b7ders", "lenkt", "ihr", "Schiff", "die", "Staats\u00b7kunst", "jetzt", "in", "schlau\u00b7er", "Pflicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+---+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Am Steuer sitzt der Eigennutz, und die Devis' hei\u00dft: Gleichgewicht;", "tokens": ["Am", "Steu\u00b7er", "sitzt", "der", "Ei\u00b7gen\u00b7nutz", ",", "und", "die", "De\u00b7vis'", "hei\u00dft", ":", "Gleich\u00b7ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NE", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Jetzt wird auf morschem Minarett der rost'ge Halbmond klug gest\u00fctzt,", "tokens": ["Jetzt", "wird", "auf", "mor\u00b7schem", "Mi\u00b7na\u00b7rett", "der", "rost'\u00b7ge", "Halb\u00b7mond", "klug", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und mit der Feuerschl\u00fcnde Wut des alten Erbfeinds Reich gesch\u00fctzt.", "tokens": ["Und", "mit", "der", "Feu\u00b7er\u00b7schl\u00fcn\u00b7de", "Wut", "des", "al\u00b7ten", "Erb\u00b7feinds", "Reich", "ge\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "O England, Meeresf\u00fcrstin, wird dein wei\u00dfer Fels nicht rot vor Scham,", "tokens": ["O", "En\u00b7gland", ",", "Mee\u00b7res\u00b7f\u00fcrs\u00b7tin", ",", "wird", "dein", "wei\u00b7\u00dfer", "Fels", "nicht", "rot", "vor", "Scham", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Denkst du an Richard L\u00f6wenherz, der Ehre k\u00fchnen Br\u00e4utigam?", "tokens": ["Denkst", "du", "an", "Ric\u00b7hard", "L\u00f6\u00b7wen\u00b7herz", ",", "der", "Eh\u00b7re", "k\u00fch\u00b7nen", "Br\u00e4u\u00b7ti\u00b7gam", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "O Deutschland, rauscht auf deinen H\u00f6hn der Wald nicht nach Prophetenart,", "tokens": ["O", "Deutschland", ",", "rauscht", "auf", "dei\u00b7nen", "H\u00f6hn", "der", "Wald", "nicht", "nach", "Pro\u00b7phe\u00b7ten\u00b7art", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dir zu verk\u00fcnden, wie da starb dein Kaiser mit dem roten Bart?", "tokens": ["Dir", "zu", "ver\u00b7k\u00fcn\u00b7den", ",", "wie", "da", "starb", "dein", "Kai\u00b7ser", "mit", "dem", "ro\u00b7ten", "Bart", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "O Frankreich, ist in deinem Ohr denn klanglos das Ger\u00fccht verhallt,", "tokens": ["O", "Fran\u00b7kreich", ",", "ist", "in", "dei\u00b7nem", "Ohr", "denn", "klang\u00b7los", "das", "Ge\u00b7r\u00fccht", "ver\u00b7hallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wie deiner S\u00f6hne Panzerschritt gen Sonnenaufgang einst gewallt?", "tokens": ["Wie", "dei\u00b7ner", "S\u00f6h\u00b7ne", "Pan\u00b7zer\u00b7schritt", "gen", "Son\u00b7nen\u00b7auf\u00b7gang", "einst", "ge\u00b7wallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "T\u00f6nt aus gew\u00f6lbter K\u00f6nigsgruft zu Saint-Denis um Mitternacht", "tokens": ["T\u00f6nt", "aus", "ge\u00b7w\u00f6lb\u00b7ter", "K\u00f6\u00b7nigs\u00b7gruft", "zu", "Saint\u00b7De\u00b7nis", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Des heil'gen Ludwigs Stimme nicht und ruft zur Sarazenenschlacht?", "tokens": ["Des", "heil'\u00b7gen", "Lud\u00b7wigs", "Stim\u00b7me", "nicht", "und", "ruft", "zur", "Sa\u00b7ra\u00b7ze\u00b7nen\u00b7schlacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKNEG", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Das waren Helden! Ob am Gaum der letzte Tropfen war verdorrt,", "tokens": ["Das", "wa\u00b7ren", "Hel\u00b7den", "!", "Ob", "am", "Gaum", "der", "letz\u00b7te", "Trop\u00b7fen", "war", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$.", "KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Sie achteten des Durstes nicht, sie hielten fest und k\u00e4mpften fort,", "tokens": ["Sie", "ach\u00b7te\u00b7ten", "des", "Durs\u00b7tes", "nicht", ",", "sie", "hiel\u00b7ten", "fest", "und", "k\u00e4mpf\u00b7ten", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Die W\u00fcste trank der Schlachten Blut, auf fahlen Fl\u00fcgeln kam die Pest,", "tokens": ["Die", "W\u00fcs\u00b7te", "trank", "der", "Schlach\u00b7ten", "Blut", ",", "auf", "fah\u00b7len", "Fl\u00fc\u00b7geln", "kam", "die", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Der Sandwind grub die Leichen ein - sie k\u00e4mpften fort und hielten fest.", "tokens": ["Der", "Sand\u00b7wind", "grub", "die", "Lei\u00b7chen", "ein", "sie", "k\u00e4mpf\u00b7ten", "fort", "und", "hiel\u00b7ten", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "$(", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}}, "stanza.9": {"line.1": {"text": "Jetzt gilt es nicht mehr, jahrelang die hei\u00dfen Steppen zu durchziehn,", "tokens": ["Jetzt", "gilt", "es", "nicht", "mehr", ",", "jah\u00b7re\u00b7lang", "die", "hei\u00b7\u00dfen", "Step\u00b7pen", "zu", "durch\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Nicht mehr mit braunen Reitern steht entgegen euch ein Saladin;", "tokens": ["Nicht", "mehr", "mit", "brau\u00b7nen", "Rei\u00b7tern", "steht", "ent\u00b7ge\u00b7gen", "euch", "ein", "Sa\u00b7la\u00b7din", ";"], "token_info": ["word",