{"textgrid.poem.65944": {"metadata": {"author": {"name": "H\u00f6lmann, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Christian H\u00f6lmann", "genre": "verse", "period": "N.A.", "pub_year": 1710, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der geist des alterthums schrieb den beschaumten wellen", "tokens": ["Der", "geist", "des", "al\u00b7ter\u00b7thums", "schrieb", "den", "be\u00b7schaum\u00b7ten", "wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "ART", "ADJA", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die k\u00fcnstliche Geburth der liebes-G\u00f6ttin zu /", "tokens": ["Die", "k\u00fcnst\u00b7li\u00b7che", "Ge\u00b7burth", "der", "lie\u00b7bes\u00b7G\u00f6t\u00b7tin", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ein muschelhaus auf den gesaltznen stellen", "tokens": ["Und", "da\u00df", "ein", "mu\u00b7schel\u00b7haus", "auf", "den", "ge\u00b7saltz\u00b7nen", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "ADJA", "VVINF"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sowohl zur \u00fcberfuhr als ihrer ersten ruh", "tokens": ["So\u00b7wohl", "zur", "\u00fc\u00b7ber\u00b7fuhr", "als", "ih\u00b7rer", "ers\u00b7ten", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An statt der wiege sey damals bestimmt gewesen;", "tokens": ["An", "statt", "der", "wie\u00b7ge", "sey", "da\u00b7mals", "be\u00b7stimmt", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VAFIN", "ADV", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein so wurde da die wahrheit eingeh\u00fcllt /", "tokens": ["Al\u00b7lein", "so", "wur\u00b7de", "da", "die", "wahr\u00b7heit", "ein\u00b7ge\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer ihre Perlen nun wolt' aus dem schlamme lesen", "tokens": ["Wer", "ih\u00b7re", "Per\u00b7len", "nun", "wolt'", "aus", "dem", "schlam\u00b7me", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VMFIN", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der fand sie endlich zwar / doch frembde vorgebildt.", "tokens": ["Der", "fand", "sie", "end\u00b7lich", "zwar", "/", "doch", "fremb\u00b7de", "vor\u00b7ge\u00b7bildt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$(", "ADV", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zieht jenen vorhang weg und last die fabeln schweigen;", "tokens": ["Zieht", "je\u00b7nen", "vor\u00b7hang", "weg", "und", "last", "die", "fa\u00b7beln", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was gilts die wahrheit wird / ja selbst der augen-schein", "tokens": ["Was", "gilts", "die", "wahr\u00b7heit", "wird", "/", "ja", "selbst", "der", "au\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "VAFIN", "$(", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Euch den verdeckten grund der Sache besser zeigen /", "tokens": ["Euch", "den", "ver\u00b7deck\u00b7ten", "grund", "der", "Sa\u00b7che", "bes\u00b7ser", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Da\u00df ich so Muschel / Meer als Welle m\u00fcsse seyn.", "tokens": ["Da\u00df", "ich", "so", "Mu\u00b7schel", "/", "Meer", "als", "Wel\u00b7le", "m\u00fcs\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "$(", "NN", "KOUS", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In meinen gr\u00fcnden ist die liebe ja gebohren /", "tokens": ["In", "mei\u00b7nen", "gr\u00fcn\u00b7den", "ist", "die", "lie\u00b7be", "ja", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VAFIN", "ART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich bin ihr erster Sitz / ihr Stammhau\u00df / Vaterland /", "tokens": ["Ich", "bin", "ihr", "ers\u00b7ter", "Sitz", "/", "ihr", "Stamm\u00b7hau\u00df", "/", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mich hat zu dieser See selbst die natur erkohren /", "tokens": ["Mich", "hat", "zu", "die\u00b7ser", "See", "selbst", "die", "na\u00b7tur", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "An deren ufern sich das sch\u00f6ne M\u00e4dgen fand.", "tokens": ["An", "de\u00b7ren", "u\u00b7fern", "sich", "das", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7gen", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ihr glieder m\u00f6get nun vor mir die seegel streichen /", "tokens": ["Ihr", "glie\u00b7der", "m\u00f6\u00b7get", "nun", "vor", "mir", "die", "see\u00b7gel", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil ich die G\u00f6tter selbst durch mich hervorgebracht /", "tokens": ["Weil", "ich", "die", "G\u00f6t\u00b7ter", "selbst", "durch", "mich", "her\u00b7vor\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ihr selber m\u00fcstet auch im Mutterleib' erbleichen /", "tokens": ["Ihr", "sel\u00b7ber", "m\u00fcs\u00b7tet", "auch", "im", "Mut\u00b7ter\u00b7leib'", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn nicht durch mich das Thor w\u00e4r' in die welt gemacht.", "tokens": ["Wenn", "nicht", "durch", "mich", "das", "Thor", "w\u00e4r'", "in", "die", "welt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "PPER", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es f\u00fcllet meine frucht den Himmel und die Erde /", "tokens": ["Es", "f\u00fcl\u00b7let", "mei\u00b7ne", "frucht", "den", "Him\u00b7mel", "und", "die", "Er\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich mache da\u00df der bau der wundergro\u00dfen welt", "tokens": ["Ich", "ma\u00b7che", "da\u00df", "der", "bau", "der", "wun\u00b7der\u00b7gro\u00b7\u00dfen", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nicht vor der letzten zeit zu einer w\u00fcsten werde /", "tokens": ["Nicht", "vor", "der", "letz\u00b7ten", "zeit", "zu", "ei\u00b7ner", "w\u00fcs\u00b7ten", "wer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "APPR", "PIS", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die nichts als distel-str\u00e4uch und d\u00f6rner in sich h\u00e4lt.", "tokens": ["Die", "nichts", "als", "dis\u00b7tel\u00b7str\u00e4uch", "und", "d\u00f6r\u00b7ner", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NN", "KON", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich bin das paradie\u00df / vor dem die keuschheit wachet /", "tokens": ["Ich", "bin", "das", "pa\u00b7ra\u00b7die\u00df", "/", "vor", "dem", "die", "keuschheit", "wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "APPR", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "In dessen gegenden die lebens-fr\u00fcchte bl\u00fchn /", "tokens": ["In", "des\u00b7sen", "ge\u00b7gen\u00b7den", "die", "le\u00b7bens\u00b7fr\u00fcch\u00b7te", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wo unser leben wird wie feuer angefachet /", "tokens": ["Wo", "un\u00b7ser", "le\u00b7ben", "wird", "wie", "feu\u00b7er", "an\u00b7ge\u00b7fa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVINF", "VAFIN", "KOKOM", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dabei die S\u00f6hne sich / wie Adam / gerne m\u00fchn;", "tokens": ["Da\u00b7bei", "die", "S\u00f6h\u00b7ne", "sich", "/", "wie", "A\u00b7dam", "/", "ger\u00b7ne", "m\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "$(", "KOKOM", "NN", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein Tempel / wo die glutt der liebe st\u00fcndlich brennet;", "tokens": ["Ein", "Tem\u00b7pel", "/", "wo", "die", "glutt", "der", "lie\u00b7be", "st\u00fcnd\u00b7lich", "bren\u00b7net", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ART", "ADJD", "ART", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein Opffertisch / wo milch zum opffer wird gebraucht;", "tokens": ["Ein", "Opf\u00b7fer\u00b7tisch", "/", "wo", "milch", "zum", "opf\u00b7fer", "wird", "ge\u00b7braucht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ADJD", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein heiligthum / da\u00df die f\u00fcr Priester nur erkennet /", "tokens": ["Ein", "hei\u00b7lig\u00b7thum", "/", "da\u00df", "die", "f\u00fcr", "Pries\u00b7ter", "nur", "er\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "ART", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In deren keuscher brust ein reiner weihrauch raucht;", "tokens": ["In", "de\u00b7ren", "keu\u00b7scher", "brust", "ein", "rei\u00b7ner", "weih\u00b7rauch", "raucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein gutes feld / das nur gerathne fr\u00fcchte bringet;", "tokens": ["Ein", "gu\u00b7tes", "feld", "/", "das", "nur", "ge\u00b7rath\u00b7ne", "fr\u00fcch\u00b7te", "brin\u00b7get", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PDS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein garten / den der thau der wollust \u00fcberflie\u00dft;", "tokens": ["Ein", "gar\u00b7ten", "/", "den", "der", "thau", "der", "wol\u00b7lust", "\u00fc\u00b7berf\u00b7lie\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$(", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja der die anmuth hat / die alle welt bezwinget /", "tokens": ["Ja", "der", "die", "an\u00b7muth", "hat", "/", "die", "al\u00b7le", "welt", "be\u00b7zwin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ART", "NN", "VAFIN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und dessen blumenfeld sein eigner flu\u00df begie\u00dft.", "tokens": ["Und", "des\u00b7sen", "blu\u00b7men\u00b7feld", "sein", "eig\u00b7ner", "flu\u00df", "be\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein Meer / wo Ebb' und Flutt dem Monden-lauffe gleichet;", "tokens": ["Ein", "Meer", "/", "wo", "Ebb'", "und", "Flutt", "dem", "Mon\u00b7den\u00b7lauf\u00b7fe", "glei\u00b7chet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein spiegel-glattes ei\u00df / wo auch ein Riese f\u00e4lt;", "tokens": ["Ein", "spie\u00b7gel\u00b7glat\u00b7tes", "ei\u00df", "/", "wo", "auch", "ein", "Rie\u00b7se", "f\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein hafen / den vergn\u00fcgt die Zuckerflott' erreichet;", "tokens": ["Ein", "ha\u00b7fen", "/", "den", "ver\u00b7gn\u00fcgt", "die", "Zu\u00b7cke\u00b7rflott'", "er\u00b7rei\u00b7chet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Schule / die man nur f\u00fcr junge m\u00e4nner h\u00e4lt;", "tokens": ["Die", "Schu\u00b7le", "/", "die", "man", "nur", "f\u00fcr", "jun\u00b7ge", "m\u00e4n\u00b7ner", "h\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "ADV", "APPR", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der liebe musterplatz die mannschafft auszu\u00fcben;", "tokens": ["Der", "lie\u00b7be", "mus\u00b7ter\u00b7platz", "die", "mann\u00b7schafft", "aus\u00b7zu\u00b7\u00fc\u00b7ben", ";"], "token