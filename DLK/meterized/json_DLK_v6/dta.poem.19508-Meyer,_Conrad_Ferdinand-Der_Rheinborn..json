{"dta.poem.19508": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Rheinborn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin den Rhein hinaufgezogen", "tokens": ["Ich", "bin", "den", "Rhein", "hin\u00b7auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch manches schatt'ge Felsenthor,", "tokens": ["Durch", "man\u00b7ches", "schatt'\u00b7ge", "Fel\u00b7sen\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entlang die blauen, frischen Wogen", "tokens": ["Ent\u00b7lang", "die", "blau\u00b7en", ",", "fri\u00b7schen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem hohen Quell empor.", "tokens": ["Zu", "sei\u00b7nem", "ho\u00b7hen", "Quell", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem hellsten Borne, weit und offen,", "tokens": ["Dem", "hells\u00b7ten", "Bor\u00b7ne", ",", "weit", "und", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darin ein Ruder weinumlaubt", "tokens": ["Da\u00b7rin", "ein", "Ru\u00b7der", "wei\u00b7num\u00b7laubt"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich spiegle, wie ein heiter Hoffen,", "tokens": ["Sich", "spieg\u00b7le", ",", "wie", "ein", "hei\u00b7ter", "Hof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entspring' er leicht, hatt' ich geglaubt.", "tokens": ["Ent\u00b7spring'", "er", "leicht", ",", "hatt'", "ich", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich klomm empor auf schroffen Stiegen,", "tokens": ["Ich", "klomm", "em\u00b7por", "auf", "schrof\u00b7fen", "Stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verwognen Pfaden, \u00f6d und wild,", "tokens": ["Ver\u00b7wog\u00b7nen", "Pfa\u00b7den", ",", "\u00f6d", "und", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah mir ihn zu F\u00fc\u00dfen liegen", "tokens": ["Und", "sah", "mir", "ihn", "zu", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als einen erzgego\u00dfnen Schild.", "tokens": ["Als", "ei\u00b7nen", "erz\u00b7ge\u00b7go\u00df\u00b7nen", "Schild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fernab von Heerdgel\u00e4ut und Matten", "tokens": ["Fern\u00b7ab", "von", "Heerd\u00b7ge\u00b7l\u00e4ut", "und", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lag er in eine Schlucht versenkt,", "tokens": ["Lag", "er", "in", "ei\u00b7ne", "Schlucht", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bedeckt von schweren Riesenschatten,", "tokens": ["Be\u00b7deckt", "von", "schwe\u00b7ren", "Rie\u00b7sen\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Eis und ew'gem Schnee getr\u00e4nkt \u2014", "tokens": ["Aus", "Eis", "und", "ew'\u00b7gem", "Schnee", "ge\u00b7tr\u00e4nkt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier jauchzt kein Senn, hier schallt kein Reigen.", "tokens": ["Hier", "jauchzt", "kein", "Senn", ",", "hier", "schallt", "kein", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In kurzen, dunkeln Wellchen geht", "tokens": ["In", "kur\u00b7zen", ",", "dun\u00b7keln", "Well\u00b7chen", "geht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der See. Hier wird die Welt zum Schweigen,", "tokens": ["Der", "See", ".", "Hier", "wird", "die", "Welt", "zum", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn nicht ein Stein in Fall ger\u00e4t \u2014", "tokens": ["Wenn", "nicht", "ein", "Stein", "in", "Fall", "ge\u00b7r\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Sturz! Ein Schlag! Und aus den Tiefen", "tokens": ["Ein", "Sturz", "!", "Ein", "Schlag", "!", "Und", "aus", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus den W\u00e4nden brach es los:", "tokens": ["Und", "aus", "den", "W\u00e4n\u00b7den", "brach", "es", "los", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heerwagen rollten! Stimmen riefen", "tokens": ["Heer\u00b7wa\u00b7gen", "roll\u00b7ten", "!", "Stim\u00b7men", "rie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$.", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Befehle durch ein Schlachtgetos!", "tokens": ["Be\u00b7feh\u00b7le", "durch", "ein", "Schlacht\u00b7ge\u00b7tos", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}