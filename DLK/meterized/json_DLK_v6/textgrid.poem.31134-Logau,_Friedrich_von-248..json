{"textgrid.poem.31134": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "248.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Reime, die ihr hinten steht, habt einen guten Muth!", "tokens": ["Ihr", "Rei\u00b7me", ",", "die", "ihr", "hin\u00b7ten", "steht", ",", "habt", "ei\u00b7nen", "gu\u00b7ten", "Muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Niemand k\u00fcmt zu euch letzten her, wann nicht die ersten gut;", "tokens": ["Nie\u00b7mand", "k\u00fcmt", "zu", "euch", "letz\u00b7ten", "her", ",", "wann", "nicht", "die", "ers\u00b7ten", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ADJA", "PTKVZ", "$,", "PWAV", "PTKNEG", "ART", "ADJA", "ADJD", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sind aber nur die ersten gut, so geht ihr euren Schritt,", "tokens": ["Sind", "a\u00b7ber", "nur", "die", "ers\u00b7ten", "gut", ",", "so", "geht", "ihr", "eu\u00b7ren", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Im fall ihr gleich nicht forder seyd, doch unter andren mit.", "tokens": ["Im", "fall", "ihr", "gleich", "nicht", "for\u00b7der", "seyd", ",", "doch", "un\u00b7ter", "an\u00b7dren", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,", "ADV", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}