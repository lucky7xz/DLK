{"textgrid.poem.36329": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Deutschland", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnimmer wird die deutsche Nation vergehn", "tokens": ["\u00bb", "nim\u00b7mer", "wird", "die", "deut\u00b7sche", "Na\u00b7tion", "ver\u00b7gehn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Neu vereint durch Gott einst auferstehn.\u00ab \u2013", "tokens": ["Neu", "ver\u00b7eint", "durch", "Gott", "einst", "auf\u00b7er\u00b7stehn", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "ADV", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hat ein edler K\u00f6nig k\u00fchn gesagt,", "tokens": ["Hat", "ein", "ed\u00b7ler", "K\u00f6\u00b7nig", "k\u00fchn", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Er zu dieser Zeit noch k\u00f6niglich gedacht.", "tokens": ["Er", "zu", "die\u00b7ser", "Zeit", "noch", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "An der Hoffnung haltet treu,", "tokens": ["An", "der", "Hoff\u00b7nung", "hal\u00b7tet", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser Herz schl\u00e4gt ewig frei;", "tokens": ["Un\u00b7ser", "Herz", "schl\u00e4gt", "e\u00b7wig", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Burg, die nie ein Feind bezwingt,", "tokens": ["Burg", ",", "die", "nie", "ein", "Feind", "be\u00b7zwingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn das Gl\u00fcck auch untersinkt.", "tokens": ["Wenn", "das", "Gl\u00fcck", "auch", "un\u00b7ter\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}