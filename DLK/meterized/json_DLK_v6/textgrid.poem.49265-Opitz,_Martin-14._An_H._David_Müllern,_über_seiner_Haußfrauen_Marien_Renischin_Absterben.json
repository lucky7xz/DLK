{"textgrid.poem.49265": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "14. An H. David M\u00fcllern, \u00fcber seiner Hau\u00dffrauen Marien Renischin Absterben", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit, so wir verschliessen,", "tokens": ["Die", "Zeit", ",", "so", "wir", "ver\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Pflegt als ein Strom zu fliessen", "tokens": ["Pflegt", "als", "ein", "Strom", "zu", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der keinen Halt nicht wei\u00df;", "tokens": ["Der", "kei\u00b7nen", "Halt", "nicht", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann unser Ma\u00df ist kommen", "tokens": ["Wann", "un\u00b7ser", "Ma\u00df", "ist", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gott uns hat genommen,", "tokens": ["Das", "Gott", "uns", "hat", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So fehlt der Kr\u00e4uter", "tokens": ["So", "fehlt", "der", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Kein Winden und kein Drehen,", "tokens": ["Kein", "Win\u00b7den", "und", "kein", "Dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Zehren, Angst und Flehen", "tokens": ["Kein", "Zeh\u00b7ren", ",", "Angst", "und", "Fle\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Befreyet mich und dich;", "tokens": ["Be\u00b7fre\u00b7yet", "mich", "und", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist schon der Geist verflogen", "tokens": ["Ist", "schon", "der", "Geist", "ver\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und au\u00df der Haut gezogen,", "tokens": ["Und", "au\u00df", "der", "Haut", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er k\u00f6mpt nicht hinter sich.", "tokens": ["Er", "k\u00f6mpt", "nicht", "hin\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Di\u00df hier das mein' und deine,", "tokens": ["Di\u00df", "hier", "das", "mein'", "und", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PDS", "VVFIN", "KON", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Adern hat und Beine,", "tokens": ["Was", "A\u00b7dern", "hat", "und", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das mu\u00df verweset seyn;", "tokens": ["Das", "mu\u00df", "ver\u00b7we\u00b7set", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Tod streckt seine H\u00e4nde", "tokens": ["Der", "Tod", "streckt", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An jedes Ort und Ende", "tokens": ["An", "je\u00b7des", "Ort", "und", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und greifft uns allen ein.", "tokens": ["Und", "greifft", "uns", "al\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der ersten Erden Scharen,", "tokens": ["Der", "ers\u00b7ten", "Er\u00b7den", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So Gott am nechsten wahren,", "tokens": ["So", "Gott", "am", "nechs\u00b7ten", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die rasten l\u00e4ngest schon,", "tokens": ["Die", "ras\u00b7ten", "l\u00e4n\u00b7gest", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Samen, seine Kinder,", "tokens": ["Ihr", "Sa\u00b7men", ",", "sei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Kindeskind nicht minder,", "tokens": ["Und", "Kin\u00b7des\u00b7kind", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind tausend mal darvon.", "tokens": ["Sind", "tau\u00b7send", "mal", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein Tod ist schon erkohren", "tokens": ["Dein", "Tod", "ist", "schon", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh als du bist gebohren!", "tokens": ["Eh", "als", "du", "bist", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Di\u00df ist de\u00df Lebens Pflicht.", "tokens": ["Di\u00df", "ist", "de\u00df", "Le\u00b7bens", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir k\u00f6nnen sonst in Sachen", "tokens": ["Wir", "k\u00f6n\u00b7nen", "sonst", "in", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns kein Raitung machen,", "tokens": ["Uns", "kein", "Rai\u00b7tung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Das Sterben fehlet nicht.", "tokens": ["Das", "Ster\u00b7ben", "feh\u00b7let", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Kein Mensch kan ihm verheissen,", "tokens": ["Kein", "Mensch", "kan", "ihm", "ver\u00b7heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er wolle sich entreissen", "tokens": ["Er", "wol\u00b7le", "sich", "en\u00b7treis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch nur auff einen Tag;", "tokens": ["Auch", "nur", "auff", "ei\u00b7nen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann wir am besten bl\u00fchen,", "tokens": ["Wann", "wir", "am", "bes\u00b7ten", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hei\u00dft er uns weiter ziehen,", "tokens": ["Hei\u00dft", "er", "uns", "wei\u00b7ter", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Dem nichts entkommen mag.", "tokens": ["Dem", "nichts", "ent\u00b7kom\u00b7men", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Herr M\u00fcller, wer wil sagen,", "tokens": ["Herr", "M\u00fcl\u00b7ler", ",", "wer", "wil", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr sollt nicht Kummer tragen,", "tokens": ["Ihr", "sollt", "nicht", "Kum\u00b7mer", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der mu\u00df kein Mensch nicht seyn.", "tokens": ["Der", "mu\u00df", "kein", "Mensch", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer bey zertheiltem Hertzen", "tokens": ["Wer", "bey", "zer\u00b7theil\u00b7tem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bleibt unber\u00fchrt von Schmertzen", "tokens": ["Bleibt", "un\u00b7be\u00b7r\u00fchrt", "von", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist Stahl und Marmorstein.", "tokens": ["Ist", "Stahl", "und", "Mar\u00b7mor\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die euer Liecht zu Morgen,", "tokens": ["Die", "eu\u00b7er", "Liecht", "zu", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Abend eurer Sorgen", "tokens": ["Zu", "A\u00b7bend", "eu\u00b7rer", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gew\u00fcndschte Ruhstatt war,", "tokens": ["Ge\u00b7w\u00fcnd\u00b7schte", "Ruh\u00b7statt", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Lust, der Trost, das Leben,", "tokens": ["Die", "Lust", ",", "der", "Trost", ",", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die euch kundt' alles geben", "tokens": ["Die", "euch", "kundt'", "al\u00b7les", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist hin zur meisten Schar.", "tokens": ["Ist", "hin", "zur", "meis\u00b7ten", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr Lob, Zucht, Thun und Handel,", "tokens": ["Ihr", "Lob", ",", "Zucht", ",", "Thun", "und", "Han\u00b7del", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr unbefleckter Wandel", "tokens": ["Ihr", "un\u00b7be\u00b7fleck\u00b7ter", "Wan\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt nur unabgemeyt;", "tokens": ["Bleibt", "nur", "un\u00b7ab\u00b7ge\u00b7meyt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Di\u00df ists, was sie von hinnen", "tokens": ["Di\u00df", "ists", ",", "was", "sie", "von", "hin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit sich hat nemen k\u00f6nnen,", "tokens": ["Mit", "sich", "hat", "ne\u00b7men", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Die Ziehr der Erbarkeit.", "tokens": ["Die", "Ziehr", "der", "Er\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Euch hat sie hinterlassen", "tokens": ["Euch", "hat", "sie", "hin\u00b7ter\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Leyd, ein Freudehassen", "tokens": ["Ein", "Leyd", ",", "ein", "Freu\u00b7de\u00b7has\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und eurer Heyrath Frucht,", "tokens": ["Und", "eu\u00b7rer", "Hey\u00b7rath", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Bildn\u00fc\u00df ihrer Tugend,", "tokens": ["Das", "Bild\u00b7n\u00fc\u00df", "ih\u00b7rer", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Kinder zarte Jugend,", "tokens": ["Der", "Kin\u00b7der", "zar\u00b7te", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So jetzt die Mutter sucht.", "tokens": ["So", "jetzt", "die", "Mut\u00b7ter", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Was wollt ihr euch beklagen", "tokens": ["Was", "wollt", "ihr", "euch", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und von dem Tode sagen?", "tokens": ["Und", "von", "dem", "To\u00b7de", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lebt in ihnen noch.", "tokens": ["Sie", "lebt", "in", "ih\u00b7nen", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Eh' ist zwar zertrieb