{"textgrid.poem.59348": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Herbst, in linden Tagen", "tokens": ["O", "Herbst", ",", "in", "lin\u00b7den", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie hast du rings dein Reich", "tokens": ["Wie", "hast", "du", "rings", "dein", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Phantastisch aufgeschlagen,", "tokens": ["Phan\u00b7tas\u00b7tisch", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So bunt und doch so bleich!", "tokens": ["So", "bunt", "und", "doch", "so", "bleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie \u00f6de, ohne Br\u00fcder,", "tokens": ["Wie", "\u00f6\u00b7de", ",", "oh\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Tal so weit und breit,", "tokens": ["Mein", "Tal", "so", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kenne dich kaum wieder", "tokens": ["Ich", "ken\u00b7ne", "dich", "kaum", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser Einsamkeit.", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So wunderbare Weise", "tokens": ["So", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Singt nun dein bleicher Mund,", "tokens": ["Singt", "nun", "dein", "blei\u00b7cher", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist, als \u00f6ffnet' leise", "tokens": ["Es", "ist", ",", "als", "\u00f6ff\u00b7net'", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich unter mir der Grund.", "tokens": ["Sich", "un\u00b7ter", "mir", "der", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und ich ruht' \u00fcberwoben,", "tokens": ["Und", "ich", "ruht'", "\u00fc\u00b7ber\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du s\u00e4ngest immerzu,", "tokens": ["Du", "s\u00e4n\u00b7gest", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Linde sch\u00fcttelt' oben", "tokens": ["Die", "Lin\u00b7de", "sch\u00fct\u00b7telt'", "o\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Laub und deckt' mich zu.", "tokens": ["Ihr", "Laub", "und", "deckt'", "mich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}