{"dta.poem.12715": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Einer frau an ihren hinterlassenen  \n ehe-mann.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu tausend guter nacht! ich lasse dir die welt", "tokens": ["Zu", "tau\u00b7send", "gu\u00b7ter", "nacht", "!", "ich", "las\u00b7se", "dir", "die", "welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kehr im himmel ein; was ich dir \u00fcberlassen,", "tokens": ["Und", "kehr", "im", "him\u00b7mel", "ein", ";", "was", "ich", "dir", "\u00fc\u00b7ber\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PTKVZ", "$.", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind kinder, sorg und m\u00fch, die werden dich umfassen,", "tokens": ["Sind", "kin\u00b7der", ",", "sorg", "und", "m\u00fch", ",", "die", "wer\u00b7den", "dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "KON", "ADJD", "$,", "PRELS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn einig und allein mein loos auf freude f\u00e4llt.", "tokens": ["Wenn", "ei\u00b7nig", "und", "al\u00b7lein", "mein", "loos", "auf", "freu\u00b7de", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Was meynst du wol darbey? hab ich dich \u00fcbereilet?", "tokens": ["Was", "meynst", "du", "wol", "dar\u00b7bey", "?", "hab", "ich", "dich", "\u00fc\u00b7be\u00b7rei\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PAV", "$.", "VAFIN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gott ist bey dir und mir: ist das nicht gleich getheilet?", "tokens": ["Gott", "ist", "bey", "dir", "und", "mir", ":", "ist", "das", "nicht", "gleich", "ge\u00b7thei\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "KON", "PPER", "$.", "VAFIN", "PDS", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}