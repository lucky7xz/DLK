{"textgrid.poem.45037": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Orientalischer Kongress", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Esel und der Wolf im Streit,", "tokens": ["Der", "E\u00b7sel", "und", "der", "Wolf", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie greifen zum Gewehr,", "tokens": ["Sie", "grei\u00b7fen", "zum", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da treten als Vermittler ein", "tokens": ["Da", "tre\u00b7ten", "als", "Ver\u00b7mitt\u00b7ler", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nachbarn rings umher:", "tokens": ["Die", "Nach\u00b7barn", "rings", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Stockfisch und das Murmeltier,", "tokens": ["Der", "Stock\u00b7fisch", "und", "das", "Mur\u00b7mel\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Marder und der Fuchs,", "tokens": ["Der", "Mar\u00b7der", "und", "der", "Fuchs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem Langohr fern und nah verwandt,", "tokens": ["Dem", "Lan\u00b7gohr", "fern", "und", "nah", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie bieten Hilfe flugs.", "tokens": ["Sie", "bie\u00b7ten", "Hil\u00b7fe", "flugs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Doch dreinzuschlagen, eh es not,", "tokens": ["Doch", "drein\u00b7zu\u00b7schla\u00b7gen", ",", "eh", "es", "not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "W\u00e4r eben auch zu toll;", "tokens": ["W\u00e4r", "e\u00b7ben", "auch", "zu", "toll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Man zieht dem Esel ab die Haut", "tokens": ["Man", "zieht", "dem", "E\u00b7sel", "ab", "die", "Haut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und schreibt ein Protokoll.", "tokens": ["Und", "schreibt", "ein", "Pro\u00b7to\u00b7koll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}