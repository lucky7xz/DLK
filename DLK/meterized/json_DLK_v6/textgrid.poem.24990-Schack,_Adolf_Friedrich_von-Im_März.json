{"textgrid.poem.24990": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Im M\u00e4rz", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich vor allen Monden preis' ich,", "tokens": ["Dich", "vor", "al\u00b7len", "Mon\u00b7den", "preis'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcrst des Jahres, heil'ger M\u00e4rz,", "tokens": ["F\u00fcrst", "des", "Jah\u00b7res", ",", "heil'\u00b7ger", "M\u00e4rz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn den Banden, starr und eisig,", "tokens": ["Wenn", "den", "Ban\u00b7den", ",", "starr", "und", "ei\u00b7sig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich entringt der Erde Herz!", "tokens": ["Sich", "ent\u00b7ringt", "der", "Er\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch ist Schlaf auf sie gebreitet;", "tokens": ["Noch", "ist", "Schlaf", "auf", "sie", "ge\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Aber leise, sichtbar kaum,", "tokens": ["A\u00b7ber", "lei\u00b7se", ",", "sicht\u00b7bar", "kaum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber ihre Z\u00fcge gleitet", "tokens": ["Ue\u00b7ber", "ih\u00b7re", "Z\u00fc\u00b7ge", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon vom nahen Lenz ein Traum.", "tokens": ["Schon", "vom", "na\u00b7hen", "Lenz", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sie regt sich; aus den Kammern,", "tokens": ["Und", "sie", "regt", "sich", ";", "aus", "den", "Kam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo es stockend lang geruht,", "tokens": ["Wo", "es", "sto\u00b7ckend", "lang", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flutet durch zerbrochne Klammern", "tokens": ["Flu\u00b7tet", "durch", "zer\u00b7broch\u00b7ne", "Klam\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiederum ihr Lebensblut.", "tokens": ["Wie\u00b7de\u00b7rum", "ihr", "Le\u00b7bens\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und des Donners ersten Schl\u00e4gen,", "tokens": ["Und", "des", "Don\u00b7ners", "ers\u00b7ten", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der den Fr\u00fchlingschor beginnt,", "tokens": ["Der", "den", "Fr\u00fch\u00b7ling\u00b7schor", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Wettersturm entgegen", "tokens": ["Und", "dem", "Wet\u00b7ter\u00b7sturm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzt der Sonne Lieblingskind.", "tokens": ["Jauchzt", "der", "Son\u00b7ne", "Lieb\u00b7lings\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da, wie Eis im Fr\u00fchlingswinde,", "tokens": ["Da", ",", "wie", "Eis", "im", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem gro\u00dfen Werdehauch", "tokens": ["In", "dem", "gro\u00b7\u00dfen", "Wer\u00b7de\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmilzt des Frostes starre Rinde", "tokens": ["Schmilzt", "des", "Fros\u00b7tes", "star\u00b7re", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tief in unserm Herzen auch.", "tokens": ["Tief", "in", "un\u00b7serm", "Her\u00b7zen", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sprudelnd mit den Erdenfl\u00fcssen,", "tokens": ["Spru\u00b7delnd", "mit", "den", "Er\u00b7den\u00b7fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Gletscherstr\u00f6me Flut,", "tokens": ["Mit", "der", "Glet\u00b7scher\u00b7str\u00f6\u00b7me", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bricht in m\u00e4chtigen Entschl\u00fcssen", "tokens": ["Bricht", "in", "m\u00e4ch\u00b7ti\u00b7gen", "Ent\u00b7schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu hervor der Lebensmut.", "tokens": ["Neu", "her\u00b7vor", "der", "Le\u00b7bens\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und der lang, ein Schlafbet\u00e4ubter,", "tokens": ["Und", "der", "lang", ",", "ein", "Schlaf\u00b7be\u00b7t\u00e4ub\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "$,", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dagelegen, wieder kreist", "tokens": ["Da\u00b7ge\u00b7le\u00b7gen", ",", "wie\u00b7der", "kreist"], "token_info": ["word", "punct", "word", "word"], "pos": ["PAV", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um der Alpen Riesenh\u00e4upter", "tokens": ["Um", "der", "Al\u00b7pen", "Rie\u00b7sen\u00b7h\u00e4up\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Adlern nun der Geist.", "tokens": ["Mit", "den", "Ad\u00b7lern", "nun", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df er hoch und h\u00f6her ringe", "tokens": ["Da\u00df", "er", "hoch", "und", "h\u00f6\u00b7her", "rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, durchgl\u00fcht von deinem Ku\u00df,", "tokens": ["Und", ",", "durch\u00b7gl\u00fcht", "von", "dei\u00b7nem", "Ku\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz sein Lebenswerk vollbringe,", "tokens": ["Ganz", "sein", "Le\u00b7bens\u00b7werk", "voll\u00b7brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei mit ihm, o Genius!", "tokens": ["Sei", "mit", "ihm", ",", "o", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}