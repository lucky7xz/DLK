{"textgrid.poem.24144": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heimlich, auf Diebes-Sohlen,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heimlich, auf Diebes-Sohlen,", "tokens": ["Heim\u00b7lich", ",", "auf", "Die\u00b7bes\u00b7Soh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Bist du, ein Sklave, geschlichen,", "tokens": ["Bist", "du", ",", "ein", "Skla\u00b7ve", ",", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Der Lust Brosamen zu stehlen;", "tokens": ["Der", "Lust", "Bro\u00b7sa\u00b7men", "zu", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Pfui, du hast dich gef\u00fcrchtet.", "tokens": ["Pfui", ",", "du", "hast", "dich", "ge\u00b7f\u00fcrch\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Nun gehst du nackt in der Sonne", "tokens": ["Nun", "gehst", "du", "nackt", "in", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und nimmst dir die Lust als dein Eigen,", "tokens": ["Und", "nimmst", "dir", "die", "Lust", "als", "dein", "Ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den runden, goldenen Apfel vom Paradiesesbaume.", "tokens": ["Den", "run\u00b7den", ",", "gol\u00b7de\u00b7nen", "Ap\u00b7fel", "vom", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7bau\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Wo ist der Engel mit dem Flammenschwerte?", "tokens": ["Wo", "ist", "der", "En\u00b7gel", "mit", "dem", "Flam\u00b7men\u00b7schwer\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "K\u00fcsse die saftige Frucht and\u00e4chtig, vor du sie issest,", "tokens": ["K\u00fcs\u00b7se", "die", "saf\u00b7ti\u00b7ge", "Frucht", "an\u00b7d\u00e4ch\u00b7tig", ",", "vor", "du", "sie", "is\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "$,", "APPR", "PPER", "PPER", "VAFIN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "K\u00fc\u00df deine Hand, die sie nahm, und die den Engel vertrieb.", "tokens": ["K\u00fc\u00df", "dei\u00b7ne", "Hand", ",", "die", "sie", "nahm", ",", "und", "die", "den", "En\u00b7gel", "ver\u00b7trieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}