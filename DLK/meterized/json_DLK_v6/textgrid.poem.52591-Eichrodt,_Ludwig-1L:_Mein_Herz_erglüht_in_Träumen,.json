{"textgrid.poem.52591": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz ergl\u00fcht in Tr\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ergl\u00fcht in Tr\u00e4umen,", "tokens": ["Mein", "Herz", "er\u00b7gl\u00fcht", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es sprudelt launetoll,", "tokens": ["Es", "spru\u00b7delt", "lau\u00b7ne\u00b7toll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Becher \u00fcbersch\u00e4umen", "tokens": ["Wie", "Be\u00b7cher", "\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herrlichen Weines voll.", "tokens": ["Herr\u00b7li\u00b7chen", "Wei\u00b7nes", "voll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Von deinem s\u00fc\u00dfen Bilde", "tokens": ["Von", "dei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist es berauscht, beseelt,", "tokens": ["Ist", "es", "be\u00b7rauscht", ",", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann hab ich je so wilde", "tokens": ["Wann", "hab", "ich", "je", "so", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So selige Schl\u00e4ge gez\u00e4hlt!", "tokens": ["So", "se\u00b7li\u00b7ge", "Schl\u00e4\u00b7ge", "ge\u00b7z\u00e4hlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die Nacht ist angebrochen,", "tokens": ["Die", "Nacht", "ist", "an\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die feierliche Nacht,", "tokens": ["Die", "fei\u00b7er\u00b7li\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von der du mir versprochen,", "tokens": ["Von", "der", "du", "mir", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sie mich gl\u00fccklich macht.", "tokens": ["Da\u00df", "sie", "mich", "gl\u00fcck\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}