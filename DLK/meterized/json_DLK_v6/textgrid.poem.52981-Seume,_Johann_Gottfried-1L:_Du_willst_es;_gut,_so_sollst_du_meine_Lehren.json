{"textgrid.poem.52981": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du willst es; gut, so sollst du meine Lehren", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst es; gut, so sollst du meine Lehren", "tokens": ["Du", "willst", "es", ";", "gut", ",", "so", "sollst", "du", "mei\u00b7ne", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "$.", "ADJD", "$,", "ADV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Abfahrt auf die Reise h\u00f6ren.", "tokens": ["Zur", "Ab\u00b7fahrt", "auf", "die", "Rei\u00b7se", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du gehst jetzt in die gro\u00dfe Welt,", "tokens": ["Du", "gehst", "jetzt", "in", "die", "gro\u00b7\u00dfe", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gleich zu gelten, Lieber, fehlt dir Geld:", "tokens": ["Und", "gleich", "zu", "gel\u00b7ten", ",", "Lie\u00b7ber", ",", "fehlt", "dir", "Geld", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$,", "ADJD", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn Geld nur gilt, wie schon die Sprache lehrt,", "tokens": ["Denn", "Geld", "nur", "gilt", ",", "wie", "schon", "die", "Spra\u00b7che", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Gold allein gibt stracks dem Manne Werth.", "tokens": ["Und", "Gold", "al\u00b7lein", "gibt", "stracks", "dem", "Man\u00b7ne", "Werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und diesen Mangel auszuf\u00fcllen,", "tokens": ["Und", "die\u00b7sen", "Man\u00b7gel", "aus\u00b7zu\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00dft du nach manches Thoren Grillen", "tokens": ["Mu\u00dft", "du", "nach", "man\u00b7ches", "Tho\u00b7ren", "Gril\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Fahne deines Lebens drehn,", "tokens": ["Die", "Fah\u00b7ne", "dei\u00b7nes", "Le\u00b7bens", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Um durch die Klippen gl\u00fccklich hin zu gehn.", "tokens": ["Um", "durch", "die", "Klip\u00b7pen", "gl\u00fcck\u00b7lich", "hin", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcrs erste suche zu studieren,", "tokens": ["F\u00fcrs", "ers\u00b7te", "su\u00b7che", "zu", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit welcher Art von Menschenthieren", "tokens": ["Mit", "wel\u00b7cher", "Art", "von", "Men\u00b7schen\u00b7thie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das Schicksal dich zusammen schl\u00e4gt;", "tokens": ["Das", "Schick\u00b7sal", "dich", "zu\u00b7sam\u00b7men", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Auf welchem Puncte du sie kannst ber\u00fchren,", "tokens": ["Auf", "wel\u00b7chem", "Punc\u00b7te", "du", "sie", "kannst", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und was ihr Geist f\u00fcr Farbe tr\u00e4gt.", "tokens": ["Und", "was", "ihr", "Geist", "f\u00fcr", "Far\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Verl\u00e4ugne dich; la\u00df nie den Menschen blicken", "tokens": ["Ver\u00b7l\u00e4ug\u00b7ne", "dich", ";", "la\u00df", "nie", "den", "Men\u00b7schen", "bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "VVIMP", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Denn Menschheit ist nun vor der Hand", "tokens": ["Denn", "Menschheit", "ist", "nun", "vor", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Fast \u00fcberall noch konterband,", "tokens": ["Fast", "\u00fc\u00b7be\u00b7rall", "noch", "kon\u00b7ter\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und ihr Phantom wird oft nur ausgespannt,", "tokens": ["Und", "ihr", "Phan\u00b7tom", "wird", "oft", "nur", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Den Sinn der Bl\u00f6den zu ber\u00fccken.", "tokens": ["Den", "Sinn", "der", "Bl\u00f6\u00b7den", "zu", "be\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Schnell lerne dich mit Anstand b\u00fccken,", "tokens": ["Schnell", "ler\u00b7ne", "dich", "mit", "An\u00b7stand", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und in der Mode welschem Ton,", "tokens": ["Und", "in", "der", "Mo\u00b7de", "wel\u00b7schem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der frevelnden Vernunft zum Hohn,", "tokens": ["Der", "fre\u00b7veln\u00b7den", "Ver\u00b7nunft", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nonsensikalisch Formeln auszuflicken.", "tokens": ["Non\u00b7sen\u00b7si\u00b7ka\u00b7lisch", "For\u00b7meln", "aus\u00b7zu\u00b7fli\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVIZU", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "Leg' auf das warme Menschenherz,", "tokens": ["Leg'", "auf", "das", "war\u00b7me", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Damit in kindischen Gef\u00fchlen", "tokens": ["Da\u00b7mit", "in", "kin\u00b7di\u00b7schen", "Ge\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die Knabenadern dir nicht Streiche spielen,", "tokens": ["Die", "Kna\u00b7be\u00b7na\u00b7dern", "dir", "nicht", "Strei\u00b7che", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ein dreyfach dickes kaltes Erz.", "tokens": ["Ein", "drey\u00b7fach", "di\u00b7ckes", "kal\u00b7tes", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "La\u00df die Moral den Schulmonarchen,", "tokens": ["La\u00df", "die", "Mo\u00b7ral", "den", "Schul\u00b7mon\u00b7ar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und suche bald im ersten hohen Rausch", "tokens": ["Und", "su\u00b7che", "bald", "im", "ers\u00b7ten", "ho\u00b7hen", "Rausch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Mit \u00fcberlegtem klugen Tausch", "tokens": ["Mit", "\u00fc\u00b7ber\u00b7leg\u00b7tem", "klu\u00b7gen", "Tausch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Der Schule D\u00fcnste wegzuschnarchen.", "tokens": ["Der", "Schu\u00b7le", "D\u00fcns\u00b7te", "weg\u00b7zu\u00b7schnar\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Schlie\u00df dich an reiche goldne Narren", "tokens": ["Schlie\u00df", "dich", "an", "rei\u00b7che", "gold\u00b7ne", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Mit wohlbedachter Narrheit an;", "tokens": ["Mit", "wohl\u00b7be\u00b7dach\u00b7ter", "Nar\u00b7rheit", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Sonst kannst du auf Fortunens Bahn", "tokens": ["Sonst", "kannst", "du", "auf", "For\u00b7tu\u00b7nens", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Umsonst Olympiaden karren.", "tokens": ["Um\u00b7sonst", "O\u00b7lym\u00b7pia\u00b7den", "kar\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Erfrech dich nie Vernunft zu haben,", "tokens": ["Er\u00b7frech", "dich", "nie", "Ver\u00b7nunft", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Die deinem G\u00f6nner widerspricht,", "tokens": ["Die", "dei\u00b7nem", "G\u00f6n\u00b7ner", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und schl\u00fcg' er wie die Fibelknaben", "tokens": ["Und", "schl\u00fcg'", "er", "wie", "die", "Fi\u00b7bel\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Dem Menschensinn ins Angesicht.", "tokens": ["Dem", "Men\u00b7schen\u00b7sinn", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Wag nie, die alte Nebeldecke", "tokens": ["Wag", "nie", ",", "die", "al\u00b7te", "Ne\u00b7bel\u00b7de\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Der bunten und der schwarzen R\u00f6cke,", "tokens": ["Der", "bun\u00b7ten", "und", "der", "schwar\u00b7zen", "R\u00f6\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Aus welcher Bann und lange Fl\u00fcche rauchen,", "tokens": ["Aus", "wel\u00b7cher", "Bann", "und", "lan\u00b7ge", "Fl\u00fc\u00b7che", "rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Mit Ph\u00f6bus Lichtstrahl anzuhauchen.", "tokens": ["Mit", "Ph\u00f6\u00b7bus", "Licht\u00b7strahl", "an\u00b7zu\u00b7hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Sprich keck; nur wage keine Kasten", "tokens": ["Sprich", "keck", ";", "nur", "wa\u00b7ge", "kei\u00b7ne", "Kas\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Mit deiner K\u00fchnheit anzutasten.", "tokens": ["Mit", "dei\u00b7ner", "K\u00fchn\u00b7heit", "an\u00b7zu\u00b7tas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Red' in der Selbstsucht hohem Grimme,", "tokens": ["Red'", "in", "der", "Selbst\u00b7sucht", "ho\u00b7hem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.48": {"text": "So oft man dein Verdienst verkennt,", "tokens": ["So", "oft", "man", "dein", "Ver\u00b7dienst", "ver\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Von deinem Werth mit Stentors Eisenstimme,", "tokens": ["Von", "dei\u00b7nem", "Werth", "mit", "Sten\u00b7tors", "Ei\u00b7sen\u00b7stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Bis dich auch die Belohnungsliste nennt.", "tokens": ["Bis", "dich", "auch", "die", "Be\u00b7loh\u00b7nungs\u00b7lis\u00b7te", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Sey gro\u00df bey Kleinen, und bey Gro\u00dfen klein;", "tokens": ["Sey", "gro\u00df", "bey", "Klei\u00b7nen", ",", "und", "bey", "Gro\u00b7\u00dfen", "klein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "NN", "$,", "KON", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Im Tadel bei\u00dfendklug, im Lobe fein;", "tokens": ["Im", "Ta\u00b7del", "bei\u00b7\u00dfend\u00b7klug", ",", "im", "Lo\u00b7be", "fein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Doch sage stets mit Peter Squenz,", "tokens": ["Doch", "sa\u00b7ge", "stets", "mit", "Pe\u00b7ter", "Squenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Vortrefflich! zu der Excellenz.", "tokens": ["Vor\u00b7treff\u00b7lich", "!", "zu", "der", "Ex\u00b7cel\u00b7lenz", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Bey allen Abderitenstreichen halte", "tokens": ["Bey", "al\u00b7len", "Ab\u00b7de\u00b7ri\u00b7ten\u00b7strei\u00b7chen", "hal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Den kleinsten Muskel in der Falte:", "tokens": ["Den", "kleins\u00b7ten", "Mus\u00b7kel", "in", "der", "Fal\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Versuch' es nie, dem Laster nachzusp\u00fcren,", "tokens": ["Ver\u00b7such'", "es", "nie", ",", "dem", "Las\u00b7ter", "nach\u00b7zu\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.58": {"text": "Und Tugend zu analysiren.", "tokens": ["Und", "Tu\u00b7gend", "zu", "a\u00b7na\u00b7ly\u00b7si\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.59": {"text": "Ergreif die Laune, die den Mann besitzt,", "tokens": ["Er\u00b7greif", "die", "Lau\u00b7ne", ",", "die", "den", "Mann", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.60": {"text": "Mit Kuns