{"textgrid.poem.65734": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du selbst Musik, und h\u00f6rst Musik so tr\u00fcbe?", "tokens": ["Du", "selbst", "Mu\u00b7sik", ",", "und", "h\u00f6rst", "Mu\u00b7sik", "so", "tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "KON", "VVFIN", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "S\u00fc\u00dfes k\u00e4mpft nicht mit S\u00fc\u00dfem, Lust weckt Lust.", "tokens": ["S\u00fc\u00b7\u00dfes", "k\u00e4mpft", "nicht", "mit", "S\u00fc\u00b7\u00dfem", ",", "Lust", "weckt", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "NN", "VVFIN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Liebst du etwas, damit es dich betr\u00fcbe?", "tokens": ["Liebst", "du", "et\u00b7was", ",", "da\u00b7mit", "es", "dich", "be\u00b7tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er\u00f6ffnest freudig deiner Qual die Brust?", "tokens": ["Er\u00b7\u00f6ff\u00b7nest", "freu\u00b7dig", "dei\u00b7ner", "Qual", "die", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn dir das Ohr Einklang der rein gesellten,", "tokens": ["Wenn", "dir", "das", "Ohr", "Ein\u00b7klang", "der", "rein", "ge\u00b7sell\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Einigkeit verm\u00e4hlten T\u00f6ne st\u00f6rt,", "tokens": ["In", "Ei\u00b7nig\u00b7keit", "ver\u00b7m\u00e4hl\u00b7ten", "T\u00f6\u00b7ne", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So scheinen sie nur lieblich dich zu schelten,", "tokens": ["So", "schei\u00b7nen", "sie", "nur", "lieb\u00b7lich", "dich", "zu", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Horch wie ein Klang die Saiten, gleiches Falles,", "tokens": ["Horch", "wie", "ein", "Klang", "die", "Sai\u00b7ten", ",", "glei\u00b7ches", "Fal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie teure Gatten wechselseits durchdringt;", "tokens": ["Wie", "teu\u00b7re", "Gat\u00b7ten", "wech\u00b7sel\u00b7seits", "durch\u00b7dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie Vater, Kind, und frohe Mutter, alles", "tokens": ["Wie", "Va\u00b7ter", ",", "Kind", ",", "und", "fro\u00b7he", "Mut\u00b7ter", ",", "al\u00b7les"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "$,", "NN", "$,", "KON", "ADJA", "NN", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "In eins, die eine muntre Note singt!", "tokens": ["In", "eins", ",", "die", "ei\u00b7ne", "mun\u00b7tre", "No\u00b7te", "singt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein sprachlos Lied, der vielen eine Pflicht,", "tokens": ["Ein", "sprach\u00b7los", "Lied", ",", "der", "vie\u00b7len", "ei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dir singt es: einsam gehest du zunicht.", "tokens": ["Dir", "singt", "es", ":", "ein\u00b7sam", "ge\u00b7hest", "du", "zu\u00b7nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADJD", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}