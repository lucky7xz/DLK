{"dta.poem.8570": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So eine kleine Fin-de-Si\u00e8cle-Krabbe, die Lawn tennis schl\u00e4gt!", "tokens": ["So", "ei\u00b7ne", "klei\u00b7ne", "Fin\u00b7de\u00b7Si\u00e8cle\u00b7Krab\u00b7be", ",", "die", "Lawn", "ten\u00b7nis", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Rote, gewellte Madonnenscheitel,", "tokens": ["Ro\u00b7te", ",", "ge\u00b7well\u00b7te", "Ma\u00b7don\u00b7nen\u00b7schei\u00b7tel", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "eine lichtblaue Blouse aus Merveilleux", "tokens": ["ei\u00b7ne", "licht\u00b7blau\u00b7e", "Blou\u00b7se", "aus", "Mer\u00b7veil\u00b7leux"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "und im flohfarbnen G\u00fcrtel ein Veilchenstr\u00e4uschen,", "tokens": ["und", "im", "floh\u00b7farb\u00b7nen", "G\u00fcr\u00b7tel", "ein", "Veil\u00b7chen\u00b7str\u00e4u\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "das nach amerikanischen Cigaretten duftet.", "tokens": ["das", "nach", "a\u00b7me\u00b7ri\u00b7ka\u00b7ni\u00b7schen", "Ci\u00b7ga\u00b7ret\u00b7ten", "duf\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Um ihren linken Seidenkn\u00f6chel,", "tokens": ["Um", "ih\u00b7ren", "lin\u00b7ken", "Sei\u00b7den\u00b7kn\u00f6\u00b7chel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wenn sie die weissen B\u00e4lle pariert,", "tokens": ["wenn", "sie", "die", "weis\u00b7sen", "B\u00e4l\u00b7le", "pa\u00b7riert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "klirrt ein Goldkettchen.", "tokens": ["klirrt", "ein", "Gold\u00b7kett\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Abends ist Feuerwerk.", "tokens": ["A\u00b7bends", "ist", "Feu\u00b7er\u00b7werk", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Man dr\u00e4ngelt sich mit ihr in eine m\u00f6glichst dustre Ecke,", "tokens": ["Man", "dr\u00e4n\u00b7gelt", "sich", "mit", "ihr", "in", "ei\u00b7ne", "m\u00f6g\u00b7lichst", "dust\u00b7re", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "l\u00e4sst sie sich schmachtend an seinen Busen lehnen", "tokens": ["l\u00e4sst", "sie", "sich", "schmach\u00b7tend", "an", "sei\u00b7nen", "Bu\u00b7sen", "leh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "und sieht zu, wie die Sterne zerplatzen.", "tokens": ["und", "sieht", "zu", ",", "wie", "die", "Ster\u00b7ne", "zer\u00b7plat\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ah!", "tokens": ["Ah", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Ein F\u00fcnfminutenkuss und gar kein Fischbein.", "tokens": ["Ein", "F\u00fcnf\u00b7mi\u00b7nu\u00b7ten\u00b7kuss", "und", "gar", "kein", "Fischbein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}