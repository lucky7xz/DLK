{"textgrid.poem.55579": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Bleibe, bleibe bei mir ...", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleibe, bleibe bei mir,", "tokens": ["Blei\u00b7be", ",", "blei\u00b7be", "bei", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Holder Fremdling, s\u00fc\u00dfe Liebe,", "tokens": ["Hol\u00b7der", "Fremd\u00b7ling", ",", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Holde, s\u00fc\u00dfe Liebe,", "tokens": ["Hol\u00b7de", ",", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und verlasse die Seele nicht!", "tokens": ["Und", "ver\u00b7las\u00b7se", "die", "See\u00b7le", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ach, wie anders, wie sch\u00f6n", "tokens": ["Ach", ",", "wie", "an\u00b7ders", ",", "wie", "sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADV", "$,", "PWAV", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Lebt der Himmel, lebt die Erde,", "tokens": ["Lebt", "der", "Him\u00b7mel", ",", "lebt", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, wie f\u00fchl ich, wie f\u00fchl ich", "tokens": ["Ach", ",", "wie", "f\u00fchl", "ich", ",", "wie", "f\u00fchl", "ich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Dieses Leben zum ersten Mal!", "tokens": ["Die\u00b7ses", "Le\u00b7ben", "zum", "ers\u00b7ten", "Mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}