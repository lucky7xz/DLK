{"textgrid.poem.35868": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr! nicht bin ich werth der Ehre", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! nicht bin ich werth der Ehre", "tokens": ["Herr", "!", "nicht", "bin", "ich", "werth", "der", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PTKNEG", "VAFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Einkehr, nimmer werth!", "tokens": ["Dei\u00b7ner", "Ein\u00b7kehr", ",", "nim\u00b7mer", "werth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst Zach\u00e4us, Magdalene", "tokens": ["Selbst", "Zac\u00b7h\u00e4us", ",", "Mag\u00b7da\u00b7le\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "NE", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Waren minder s\u00fcndbeschwert,", "tokens": ["Wa\u00b7ren", "min\u00b7der", "s\u00fcnd\u00b7be\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn nicht hatten sie der Taufe", "tokens": ["Denn", "nicht", "hat\u00b7ten", "sie", "der", "Tau\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gnadenkleid befleckt, entehrt.", "tokens": ["Gna\u00b7den\u00b7kleid", "be\u00b7fleckt", ",", "ent\u00b7ehrt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und doch willst Du: ich soll kommen,", "tokens": ["Und", "doch", "willst", "Du", ":", "ich", "soll", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll empfahn dies Engelbrod.", "tokens": ["Soll", "em\u00b7pfahn", "dies", "En\u00b7gel\u00b7brod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleib' ich fern, meid' ich das Leben,", "tokens": ["Bleib'", "ich", "fern", ",", "meid'", "ich", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00e4nke wohl in ew'gen Tod. \u2013", "tokens": ["S\u00e4n\u00b7ke", "wohl", "in", "ew'\u00b7gen", "Tod", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm denn, treuer Arzt der Kranken!", "tokens": ["Komm", "denn", ",", "treu\u00b7er", "Arzt", "der", "Kran\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heile selbst der Seele Noth.", "tokens": ["Hei\u00b7le", "selbst", "der", "See\u00b7le", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}