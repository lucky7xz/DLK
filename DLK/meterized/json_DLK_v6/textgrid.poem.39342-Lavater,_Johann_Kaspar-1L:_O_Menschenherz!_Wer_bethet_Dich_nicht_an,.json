{"textgrid.poem.39342": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Menschenherz! Wer bethet Dich nicht an,", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Menschenherz! Wer bethet Dich nicht an,", "tokens": ["O", "Men\u00b7schen\u00b7herz", "!", "Wer", "be\u00b7thet", "Dich", "nicht", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erblickt er Dich im Patriotenauge!", "tokens": ["Er\u00b7blickt", "er", "Dich", "im", "Pat\u00b7ri\u00b7o\u00b7ten\u00b7au\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Herzen keimt des Vaterlandes Liebe,", "tokens": ["Im", "Her\u00b7zen", "keimt", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gro\u00dfe Sinn f\u00fcr's h\u00f6chste Wohl der B\u00fcrger,", "tokens": ["Der", "gro\u00b7\u00dfe", "Sinn", "f\u00fcr's", "h\u00f6chs\u00b7te", "Wohl", "der", "B\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bed\u00fcrfni\u00df, das nichts will als Freyheit Aller", "tokens": ["Be\u00b7d\u00fcrf\u00b7ni\u00df", ",", "das", "nichts", "will", "als", "Frey\u00b7heit", "Al\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PIS", "VMFIN", "KOUS", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00f6glichsten Genu\u00df des Erdelebens", "tokens": ["Und", "m\u00f6g\u00b7lichs\u00b7ten", "Ge\u00b7nu\u00df", "des", "Er\u00b7de\u00b7le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "In der Vereinigung mit Gleichgesinnten!", "tokens": ["In", "der", "Ver\u00b7ei\u00b7ni\u00b7gung", "mit", "Gleich\u00b7ge\u00b7sinn\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der edle Sinn macht Tausende zu Einem,", "tokens": ["Der", "ed\u00b7le", "Sinn", "macht", "Tau\u00b7sen\u00b7de", "zu", "Ei\u00b7nem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Eine wird sein Vater, Bruder, Sohn,", "tokens": ["Der", "Ei\u00b7ne", "wird", "sein", "Va\u00b7ter", ",", "Bru\u00b7der", ",", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird Herzensfreund und Br\u00e4utigam und Braut.", "tokens": ["Wird", "Her\u00b7zens\u00b7freund", "und", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er lebt im Ganzen nur und \u00fcbersiehet", "tokens": ["Er", "lebt", "im", "Gan\u00b7zen", "nur", "und", "\u00fc\u00b7ber\u00b7sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Theil im Ganzen nicht; der Einzelste", "tokens": ["Den", "Theil", "im", "Gan\u00b7zen", "nicht", ";", "der", "Ein\u00b7zels\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PTKNEG", "$.", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ist heilig ihm als Theil des gro\u00dfen Ganzen;", "tokens": ["Ist", "hei\u00b7lig", "ihm", "als", "Theil", "des", "gro\u00b7\u00dfen", "Gan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er findet Namen nicht f\u00fcr seinen Abscheu", "tokens": ["Er", "fin\u00b7det", "Na\u00b7men", "nicht", "f\u00fcr", "sei\u00b7nen", "Ab\u00b7scheu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor Eigennutz und vor Verg\u00f6tt'rung seiner.", "tokens": ["Vor", "Ei\u00b7gen\u00b7nutz", "und", "vor", "Ver\u00b7g\u00f6tt'\u00b7rung", "sei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihm ist der K\u00f6nig nichts als ein Verbrecher,", "tokens": ["Ihm", "ist", "der", "K\u00f6\u00b7nig", "nichts", "als", "ein", "Ver\u00b7bre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er sich bl\u00e4ht auf seines Landes Nacken!", "tokens": ["Wenn", "er", "sich", "bl\u00e4ht", "auf", "sei\u00b7nes", "Lan\u00b7des", "Na\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm ist der F\u00fcrst der Misseth\u00e4ther Erster,", "tokens": ["Ihm", "ist", "der", "F\u00fcrst", "der", "Mis\u00b7set\u00b7h\u00e4t\u00b7her", "Ers\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem heilig nicht wie Gott der Reichsgenossen", "tokens": ["Dem", "hei\u00b7lig", "nicht", "wie", "Gott", "der", "Reichs\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PTKNEG", "KOKOM", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bed\u00fcrfni\u00df ist nach Freyheit und nach Leben!", "tokens": ["Be\u00b7d\u00fcrf\u00b7ni\u00df", "ist", "nach", "Frey\u00b7heit", "und", "nach", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ihm ist nur K\u00f6nig der, ein Bild der Gottheit,", "tokens": ["Ihm", "ist", "nur", "K\u00f6\u00b7nig", "der", ",", "ein", "Bild", "der", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ART", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der sich ein Tropfen nur im Ozean", "tokens": ["Der", "sich", "ein", "Trop\u00b7fen", "nur", "im", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Vaterlandes ist, der jauchzend Viele", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "ist", ",", "der", "jauch\u00b7zend", "Vie\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begl\u00fcckter sieht als sich, den N\u00e4chte schlaflos,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "sieht", "als", "sich", ",", "den", "N\u00e4ch\u00b7te", "schlaf\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOUS", "PRF", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Den Tage fastend finden, Morgen staunend", "tokens": ["Den", "Ta\u00b7ge", "fas\u00b7tend", "fin\u00b7den", ",", "Mor\u00b7gen", "stau\u00b7nend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr V\u00f6lkerwohl, der sich als Nation", "tokens": ["F\u00fcr", "V\u00f6l\u00b7ker\u00b7wohl", ",", "der", "sich", "als", "Na\u00b7tion"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "KOUS", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dem Herrscher stellt der Erdenationen!", "tokens": ["Dem", "Herr\u00b7scher", "stellt", "der", "Er\u00b7de\u00b7na\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}