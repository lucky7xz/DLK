{"dta.poem.8615": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Nach S\u00fcden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Dort nach S\u00fcden zieht der Regen,", "tokens": ["Dort", "nach", "S\u00fc\u00b7den", "zieht", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winde brausen s\u00fcdenw\u00e4rts,", "tokens": ["Win\u00b7de", "brau\u00b7sen", "s\u00fc\u00b7den\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach des Donners fernen Schl\u00e4gen,", "tokens": ["Nach", "des", "Don\u00b7ners", "fer\u00b7nen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort nach S\u00fcden will mein Herz.", "tokens": ["Dort", "nach", "S\u00fc\u00b7den", "will", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort im fernen Ungerlande", "tokens": ["Dort", "im", "fer\u00b7nen", "Un\u00b7ger\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freundlich schmuck ein D\u00f6rfchen steht,", "tokens": ["Freund\u00b7lich", "schmuck", "ein", "D\u00f6rf\u00b7chen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings umrauscht von Waldesrande,", "tokens": ["Rings", "um\u00b7rauscht", "von", "Wal\u00b7des\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mild von Segen rings umweht.", "tokens": ["Mild", "von", "Se\u00b7gen", "rings", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An des D\u00f6rfchens stillem Saume", "tokens": ["An", "des", "D\u00f6rf\u00b7chens", "stil\u00b7lem", "Sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein H\u00fcttlein hingestellt,", "tokens": ["Ist", "ein", "H\u00fctt\u00b7lein", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das in seinem engen Raume", "tokens": ["Das", "in", "sei\u00b7nem", "en\u00b7gen", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahret meine Herzenswelt.", "tokens": ["Wah\u00b7ret", "mei\u00b7ne", "Her\u00b7zens\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "B\u00e4ume halten es umschlungen", "tokens": ["B\u00e4u\u00b7me", "hal\u00b7ten", "es", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Zweigen inniglich,", "tokens": ["Mit", "den", "Zwei\u00b7gen", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4ume, die dem Wald entsprungen,", "tokens": ["B\u00e4u\u00b7me", ",", "die", "dem", "Wald", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehnend nach dem H\u00fcttlein sich.", "tokens": ["Seh\u00b7nend", "nach", "dem", "H\u00fctt\u00b7lein", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus dem Fenster blickt nun schweigend", "tokens": ["Aus", "dem", "Fens\u00b7ter", "blickt", "nun", "schwei\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lilla nach dem Wald hinaus,", "tokens": ["Lil\u00b7la", "nach", "dem", "Wald", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr Gesichtchen traurig neigend", "tokens": ["Ihr", "Ge\u00b7sicht\u00b7chen", "trau\u00b7rig", "nei\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blickt sie nach dem Laubgebraus.", "tokens": ["Blickt", "sie", "nach", "dem", "Laub\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sie sieht's mit stillem Sinnen,", "tokens": ["Und", "sie", "sieht's", "mit", "stil\u00b7lem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie sieht es bang ger\u00fchrt,", "tokens": ["Und", "sie", "sieht", "es", "bang", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Wasser niederrinnen,", "tokens": ["Wie", "die", "Was\u00b7ser", "nie\u00b7der\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Wind das Laub entf\u00fchrt.", "tokens": ["Wie", "der", "Wind", "das", "Laub", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lauter wogt der Bach und tr\u00fcber,", "tokens": ["Lau\u00b7ter", "wogt", "der", "Bach", "und", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter wird der L\u00fcfte Streit,", "tokens": ["Lau\u00b7ter", "wird", "der", "L\u00fcf\u00b7te", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rbar rauscht die Zeit vor\u00fcber", "tokens": ["H\u00f6r\u00b7bar", "rauscht", "die", "Zeit", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des M\u00e4dchens Einsamkeit.", "tokens": ["An", "des", "M\u00e4d\u00b7chens", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}