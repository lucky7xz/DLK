{"textgrid.poem.53532": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Religionsunterricht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berliner Past\u00f6re und Zentrumsherren", "tokens": ["Ber\u00b7li\u00b7ner", "Pas\u00b7t\u00f6\u00b7re", "und", "Zent\u00b7rums\u00b7her\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "durchziehen die Stra\u00dfen und pl\u00e4rren", "tokens": ["durch\u00b7zie\u00b7hen", "die", "Stra\u00b7\u00dfen", "und", "pl\u00e4r\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Chor\u00e4le.", "tokens": ["Cho\u00b7r\u00e4\u00b7le", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Denn die revolution\u00e4ren Affen", "tokens": ["Denn", "die", "re\u00b7vo\u00b7lu\u00b7ti\u00b7o\u00b7n\u00e4\u00b7ren", "Af\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "wollen die Schulreligion abschaffen.", "tokens": ["wol\u00b7len", "die", "Schul\u00b7re\u00b7li\u00b7gi\u00b7on", "ab\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wer garantiert nun der gutgl\u00e4ubigen Jugend", "tokens": ["Wer", "ga\u00b7ran\u00b7tiert", "nun", "der", "gut\u00b7gl\u00e4u\u00b7bi\u00b7gen", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die garantiert echte christliche Tugend?", "tokens": ["die", "ga\u00b7ran\u00b7tiert", "ech\u00b7te", "christ\u00b7li\u00b7che", "Tu\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Denn was da geht in ein christlich Ohr,", "tokens": ["Denn", "was", "da", "geht", "in", "ein", "christ\u00b7lich", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "f\u00fcrs ganze Leben h\u00e4lt das vor.", "tokens": ["f\u00fcrs", "gan\u00b7ze", "Le\u00b7ben", "h\u00e4lt", "das", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer lehrt nun die Kleinen nach diesem Krieg", "tokens": ["Wer", "lehrt", "nun", "die", "Klei\u00b7nen", "nach", "die\u00b7sem", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "die S\u00e4tze der praktischen Metaphysik?", "tokens": ["die", "S\u00e4t\u00b7ze", "der", "prak\u00b7ti\u00b7schen", "Me\u00b7ta\u00b7phy\u00b7sik", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Als da sind: Du sollst nicht t\u00f6ten!", "tokens": ["Als", "da", "sind", ":", "Du", "sollst", "nicht", "t\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Au\u00dfer, wenn die Fahne in N\u00f6ten.", "tokens": ["Au\u00b7\u00dfer", ",", "wenn", "die", "Fah\u00b7ne", "in", "N\u00f6\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Diese weisen Lehren \u2013 wie Paulus uralt . . .", "tokens": ["Die\u00b7se", "wei\u00b7sen", "Leh\u00b7ren", "\u2013", "wie", "Pau\u00b7lus", "ur\u00b7alt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDAT", "ADJA", "NN", "$(", "KOKOM", "NE", "NE", "$.", "$.", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und was macht, nebenbei, das Pastorengehalt?", "tokens": ["Und", "was", "macht", ",", "ne\u00b7ben\u00b7bei", ",", "das", "Pas\u00b7to\u00b7ren\u00b7ge\u00b7halt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "ADV", "$,", "ART", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "Das Pastorengehalt \u2013 Herr Gott in Gnaden!", "tokens": ["Das", "Pas\u00b7to\u00b7ren\u00b7ge\u00b7halt", "\u2013", "Herr", "Gott", "in", "Gna\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "NN", "APPR", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "wolle doch die S\u00fcnder zur H\u00f6lle laden!", "tokens": ["wol\u00b7le", "doch", "die", "S\u00fcn\u00b7der", "zur", "H\u00f6l\u00b7le", "la\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.9": {"line.1": {"text": "Sieh, der B\u00fcrger zieht ein Gesicht.", "tokens": ["Sieh", ",", "der", "B\u00fcr\u00b7ger", "zieht", "ein", "Ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gegen den Priester? Er traut sich nicht.", "tokens": ["Ge\u00b7gen", "den", "Pries\u00b7ter", "?", "Er", "traut", "sich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Er gedenkt seiner Jugend und wird wieder kindlich.", "tokens": ["Er", "ge\u00b7denkt", "sei\u00b7ner", "Ju\u00b7gend", "und", "wird", "wie\u00b7der", "kind\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gegen den Priester? Er ist pl\u00f6tzlich empfindlich.", "tokens": ["Ge\u00b7gen", "den", "Pries\u00b7ter", "?", "Er", "ist", "pl\u00f6tz\u00b7lich", "emp\u00b7find\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}, "stanza.11": {"line.1": {"text": "Kluge Gesichter l\u00e4cheln in Rom:", "tokens": ["Klu\u00b7ge", "Ge\u00b7sich\u00b7ter", "l\u00e4\u00b7cheln", "in", "Rom", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Deutschland war stets ein einziger Dom.", "tokens": ["Deutschland", "war", "stets", "ein", "ein\u00b7zi\u00b7ger", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Herren von der Konkurrenzfakult\u00e4t", "tokens": ["Die", "Her\u00b7ren", "von", "der", "Kon\u00b7kur\u00b7renz\u00b7fa\u00b7kul\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "tun mit, weils um dem Gelde geht.", "tokens": ["tun", "mit", ",", "weils", "um", "dem", "Gel\u00b7de", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Friede, ihr Fakult\u00e4ten, auf Erden!", "tokens": ["Frie\u00b7de", ",", "ihr", "Fa\u00b7kul\u00b7t\u00e4\u00b7ten", ",", "auf", "Er\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es wird mit dem Umsturz so schlimm nicht werden.", "tokens": ["Es", "wird", "mit", "dem", "Um\u00b7sturz", "so", "schlimm", "nicht", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Man kann sich ja euer gar nicht entw\u00f6hnen!", "tokens": ["Man", "kann", "sich", "ja", "eu\u00b7er", "gar", "nicht", "ent\u00b7w\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "PPOSAT", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und pa\u00dft mal auf: meinen Herren S\u00f6hnen", "tokens": ["Und", "pa\u00dft", "mal", "auf", ":", "mei\u00b7nen", "Her\u00b7ren", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$.", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "werden im Schulunterricht wieder ert\u00f6nen", "tokens": ["wer\u00b7den", "im", "Schul\u00b7un\u00b7ter\u00b7richt", "wie\u00b7der", "er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVINF"], "meter": "+--++-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Chor\u00e4le!", "tokens": ["Cho\u00b7r\u00e4\u00b7le", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}