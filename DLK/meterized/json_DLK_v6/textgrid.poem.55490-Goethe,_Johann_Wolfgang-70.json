{"textgrid.poem.55490": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "70", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abwesend ist kein Freund zu achten,", "tokens": ["Ab\u00b7we\u00b7send", "ist", "kein", "Freund", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der immer f\u00fcr uns denkt und strebt", "tokens": ["Der", "im\u00b7mer", "f\u00fcr", "uns", "denkt", "und", "strebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wie es auch die Zeiten brachten,", "tokens": ["Und", ",", "wie", "es", "auch", "die", "Zei\u00b7ten", "brach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr uns in gleichem Sinne lebt.", "tokens": ["F\u00fcr", "uns", "in", "glei\u00b7chem", "Sin\u00b7ne", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei Sonnenschein und Regenschauer", "tokens": ["Bei", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruft ein verkl\u00e4rter, heitrer Blick", "tokens": ["Ruft", "ein", "ver\u00b7kl\u00e4r\u00b7ter", ",", "hei\u00b7trer", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem zweifelhaften Zustand Gl\u00fcck", "tokens": ["Dem", "zwei\u00b7fel\u00b7haf\u00b7ten", "Zu\u00b7stand", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jedem Gl\u00fcck die l\u00e4ngste Dauer.", "tokens": ["Und", "je\u00b7dem", "Gl\u00fcck", "die", "l\u00e4ngs\u00b7te", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}