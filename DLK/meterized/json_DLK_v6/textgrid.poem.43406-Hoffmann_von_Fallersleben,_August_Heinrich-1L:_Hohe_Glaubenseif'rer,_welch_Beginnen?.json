{"textgrid.poem.43406": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hohe Glaubenseif'rer, welch Beginnen?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hohe Glaubenseif'rer, welch Beginnen?", "tokens": ["Ho\u00b7he", "Glau\u00b7bens\u00b7eif'\u00b7rer", ",", "welch", "Be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hat euch euer Herz noch nie gequ\u00e4lt?", "tokens": ["Hat", "euch", "eu\u00b7er", "Herz", "noch", "nie", "ge\u00b7qu\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eure T\u00f6chter werden Ketzerinnen,", "tokens": ["Eu\u00b7re", "T\u00f6ch\u00b7ter", "wer\u00b7den", "Ket\u00b7ze\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn ein Ketzer ihnen sich verm\u00e4hlt.", "tokens": ["Wenn", "ein", "Ket\u00b7zer", "ih\u00b7nen", "sich", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Katholikenf\u00fcrsten, o des Spottes!", "tokens": ["Ka\u00b7tho\u00b7li\u00b7ken\u00b7f\u00fcrs\u00b7ten", ",", "o", "des", "Spot\u00b7tes", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eurer T\u00f6chter Glaube gilt euch gleich,", "tokens": ["Eu\u00b7rer", "T\u00f6ch\u00b7ter", "Glau\u00b7be", "gilt", "euch", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihr versto\u00dft sie aus dem Reiche Gottes", "tokens": ["Ihr", "ver\u00b7sto\u00dft", "sie", "aus", "dem", "Rei\u00b7che", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NE", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um ein irdisch Ketzerk\u00f6nigreich.", "tokens": ["Um", "ein", "ir\u00b7disch", "Ket\u00b7zer\u00b7k\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}