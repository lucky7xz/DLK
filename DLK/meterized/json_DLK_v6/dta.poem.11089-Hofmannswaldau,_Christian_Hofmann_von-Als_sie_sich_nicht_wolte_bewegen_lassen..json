{"dta.poem.11089": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich nicht wolte bewegen lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Brauche, f\u00fcrstin meiner seelen!", "tokens": ["Brau\u00b7che", ",", "f\u00fcrs\u00b7tin", "mei\u00b7ner", "see\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so strenge deine macht.", "tokens": ["Nicht", "so", "stren\u00b7ge", "dei\u00b7ne", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mein hertze nicht so qu\u00e4len,", "tokens": ["La\u00df", "mein", "hert\u00b7ze", "nicht", "so", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das du selbst verliebt gemacht!", "tokens": ["Das", "du", "selbst", "ver\u00b7liebt", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sey nicht stets un\u00fcberwindlich!", "tokens": ["Sey", "nicht", "stets", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lindre meine liebes-pein!", "tokens": ["Lind\u00b7re", "mei\u00b7ne", "lie\u00b7bes\u00b7pein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seynd die g\u00f6tter doch empfindlich,", "tokens": ["Seynd", "die", "g\u00f6t\u00b7ter", "doch", "emp\u00b7find\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Solt\u2019 es nicht ein engel seyn?", "tokens": ["Solt'", "es", "nicht", "ein", "en\u00b7gel", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}