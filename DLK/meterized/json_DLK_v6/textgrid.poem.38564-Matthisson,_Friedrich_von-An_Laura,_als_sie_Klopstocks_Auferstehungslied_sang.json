{"textgrid.poem.38564": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Laura, als sie Klopstocks Auferstehungslied sang", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzen, die gen Himmel sich erheben,", "tokens": ["Her\u00b7zen", ",", "die", "gen", "Him\u00b7mel", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thr\u00e4nen, die dem Auge still entbeben,", "tokens": ["Thr\u00e4\u00b7nen", ",", "die", "dem", "Au\u00b7ge", "still", "ent\u00b7be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seufzer, die den Lippen leis' entfliehn,", "tokens": ["Seuf\u00b7zer", ",", "die", "den", "Lip\u00b7pen", "leis'", "ent\u00b7fliehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wangen, die mit Andachtsglut sich malen,", "tokens": ["Wan\u00b7gen", ",", "die", "mit", "An\u00b7dachts\u00b7glut", "sich", "ma\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Trunckne Blicke, die Entz\u00fcckung stralen,", "tokens": ["Trunck\u00b7ne", "Bli\u00b7cke", ",", "die", "Ent\u00b7z\u00fc\u00b7ckung", "stra\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Danken dir, o Heilverk\u00fcnderin!", "tokens": ["Dan\u00b7ken", "dir", ",", "o", "Heil\u00b7ver\u00b7k\u00fcn\u00b7de\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Laura! Laura! horchend diesen T\u00f6nen,", "tokens": ["Lau\u00b7ra", "!", "Lau\u00b7ra", "!", "hor\u00b7chend", "die\u00b7sen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "ADJD", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00fcssen Engelseelen sich versch\u00f6nen,", "tokens": ["M\u00fcs\u00b7sen", "En\u00b7gel\u00b7see\u00b7len", "sich", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heilige den Himmel offen sehn,", "tokens": ["Hei\u00b7li\u00b7ge", "den", "Him\u00b7mel", "of\u00b7fen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schwermuthsvolle Zweifler sanfter klagen,", "tokens": ["Schwer\u00b7muths\u00b7vol\u00b7le", "Zweif\u00b7ler", "sanf\u00b7ter", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kalte Frevler an die Brust sich schlagen", "tokens": ["Kal\u00b7te", "Frev\u00b7ler", "an", "die", "Brust", "sich", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und wie Seraph Abbadona flehn!", "tokens": ["Und", "wie", "Se\u00b7raph", "Ab\u00b7ba\u00b7do\u00b7na", "flehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "NE", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Mit den T\u00f6nen des Triumphgesanges", "tokens": ["Mit", "den", "T\u00f6\u00b7nen", "des", "Tri\u00b7umph\u00b7ge\u00b7san\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trank ich Vorgef\u00fchl des Ueberganges", "tokens": ["Trank", "ich", "Vor\u00b7ge\u00b7f\u00fchl", "des", "Ue\u00b7ber\u00b7gan\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Von der Grabnacht zum Verkl\u00e4rungsglanz!", "tokens": ["Von", "der", "Grab\u00b7nacht", "zum", "Ver\u00b7kl\u00e4\u00b7rungs\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als vern\u00e4hm' ich Engelmelodien", "tokens": ["Als", "ver\u00b7n\u00e4hm'", "ich", "En\u00b7gel\u00b7me\u00b7lo\u00b7di\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "W\u00e4hnt' ich dir, o Erde, zu entfliehen,", "tokens": ["W\u00e4hnt'", "ich", "dir", ",", "o", "Er\u00b7de", ",", "zu", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sah' schon unter mir der Sterne Tanz!", "tokens": ["Sah'", "schon", "un\u00b7ter", "mir", "der", "Ster\u00b7ne", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Schon umathmete mich Himmelsmilde,", "tokens": ["Schon", "um\u00b7ath\u00b7me\u00b7te", "mich", "Him\u00b7mels\u00b7mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schon begr\u00fc\u00dft' ich jauchzend die Gefilde,", "tokens": ["Schon", "be\u00b7gr\u00fc\u00dft'", "ich", "jauch\u00b7zend", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo des Lebens Strom durch Palmen fleu\u00dft!", "tokens": ["Wo", "des", "Le\u00b7bens", "Strom", "durch", "Pal\u00b7men", "fleu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00e4nzend von der n\u00e4hern Gottheit Strale", "tokens": ["Gl\u00e4n\u00b7zend", "von", "der", "n\u00e4\u00b7hern", "Got\u00b7theit", "Stra\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wandelte durch Paradiesesthale", "tokens": ["Wan\u00b7del\u00b7te", "durch", "Pa\u00b7ra\u00b7die\u00b7sest\u00b7ha\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wonneschauernd mein entschwebter Geist!", "tokens": ["Won\u00b7ne\u00b7schau\u00b7ernd", "mein", "ent\u00b7schweb\u00b7ter", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}