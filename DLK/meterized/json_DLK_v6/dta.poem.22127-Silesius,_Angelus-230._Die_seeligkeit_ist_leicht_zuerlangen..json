{"dta.poem.22127": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "230. Die seeligkeit ist leicht zuerlangen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Es dunkt mich leichter seyn in Him\u0303el sich zuschwingen:", "tokens": ["Es", "dunkt", "mich", "leich\u00b7ter", "seyn", "in", "Him\u0303el", "sich", "zu\u00b7schwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VAINF", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als mit der S\u00fcnden m\u00fch in Abgrund ein zudringen.", "tokens": ["Als", "mit", "der", "S\u00fcn\u00b7den", "m\u00fch", "in", "Ab\u00b7grund", "ein", "zu\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "APPR", "NN", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}