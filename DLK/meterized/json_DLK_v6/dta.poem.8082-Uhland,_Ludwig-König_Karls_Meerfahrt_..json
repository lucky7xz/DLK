{"dta.poem.8082": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Karls Meerfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig Karl fuhr \u00fcber Meer", "tokens": ["Der", "K\u00f6\u00b7nig", "Karl", "fuhr", "\u00fc\u00b7ber", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinen zw\u00f6lf Genossen,", "tokens": ["Mit", "sei\u00b7nen", "zw\u00f6lf", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ZnmZum heil'gen Lande steuert' er,", "tokens": ["Znm", "Zum", "heil'\u00b7gen", "Lan\u00b7de", "steu\u00b7ert'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und ward vom Sturm versto\u00dfen.", "tokens": ["Und", "ward", "vom", "Sturm", "ver\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sprach der k\u00fchne Held Roland:", "tokens": ["Da", "sprach", "der", "k\u00fch\u00b7ne", "Held", "Ro\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich kann wohl fechten und schirmen,", "tokens": ["\u201e", "ich", "kann", "wohl", "fech\u00b7ten", "und", "schir\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch h\u00e4lt mir diese Kunst nicht Stand", "tokens": ["Doch", "h\u00e4lt", "mir", "die\u00b7se", "Kunst", "nicht", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "PTKNEG", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Wellen und vor St\u00fcrmen.\u201c", "tokens": ["Vor", "Wel\u00b7len", "und", "vor", "St\u00fcr\u00b7men", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann sprach Herr Holger aus D\u00e4nemark:", "tokens": ["Dann", "sprach", "Herr", "Hol\u00b7ger", "aus", "D\u00e4\u00b7ne\u00b7mark", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201eich kann die Harfe schlagen;", "tokens": ["\u201e", "ich", "kann", "die", "Har\u00b7fe", "schla\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was hilft mir das, wenn also stark", "tokens": ["Was", "hilft", "mir", "das", ",", "wenn", "al\u00b7so", "stark"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PDS", "$,", "KOUS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wind\u2019 und Wellen jagen?\u201c", "tokens": ["Die", "Wind'", "und", "Wel\u00b7len", "ja\u00b7gen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Oliver war auch nicht froh,", "tokens": ["Herr", "O\u00b7li\u00b7ver", "war", "auch", "nicht", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Er sah auf seine Wehre:", "tokens": ["Er", "sah", "auf", "sei\u00b7ne", "Weh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees ist mir um mich selbst nicht so,", "tokens": ["\u201e", "es", "ist", "mir", "um", "mich", "selbst", "nicht", "so", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "APPR", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie um die Altekl\u00e4re.\u201c", "tokens": ["Wie", "um", "die", "Al\u00b7te\u00b7kl\u00e4\u00b7re", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann sprach der schlimme Ganelon,", "tokens": ["Dann", "sprach", "der", "schlim\u00b7me", "Ga\u00b7ne\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sprach es nur verstohlen:", "tokens": ["Er", "sprach", "es", "nur", "ver\u00b7stoh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ew\u00e4r\u2019 ich mit guter Art davon,", "tokens": ["\u201e", "w\u00e4r'", "ich", "mit", "gu\u00b7ter", "Art", "da\u00b7von", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht\u2019 euch der Teufel holen!\u201c", "tokens": ["M\u00f6cht'", "euch", "der", "Teu\u00b7fel", "ho\u00b7len", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erzbischof Turpin seufzte sehr:", "tokens": ["Erz\u00b7bi\u00b7schof", "Tur\u00b7pin", "seufz\u00b7te", "sehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u201ewir sind die Gottesstreiter;", "tokens": ["\u201e", "wir", "sind", "die", "Got\u00b7tes\u00b7strei\u00b7ter", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm, liebster Heiland, \u00fcber das Meer", "tokens": ["Komm", ",", "liebs\u00b7ter", "Hei\u00b7land", ",", "\u00fc\u00b7ber", "das", "Meer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und f\u00fchr uns gn\u00e4dig weiter!\u201c", "tokens": ["Und", "f\u00fchr", "uns", "gn\u00e4\u00b7dig", "wei\u00b7ter", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Graf Richard Ohnefurcht hub an:", "tokens": ["Graf", "Ric\u00b7hard", "Oh\u00b7ne\u00b7furcht", "hub", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eihr Geister aus der H\u00f6lle!", "tokens": ["\u201e", "ihr", "Geis\u00b7ter", "aus", "der", "H\u00f6l\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab\u2019 euch manchen Dienst gethan,", "tokens": ["Ich", "hab'", "euch", "man\u00b7chen", "Dienst", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt helft mir von der Stelle!\u201c", "tokens": ["Jetzt", "helft", "mir", "von", "der", "Stel\u00b7le", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Herr Naimis diesen Ausspruch that:", "tokens": ["Herr", "Nai\u00b7mis", "die\u00b7sen", "Aus\u00b7spruch", "that", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eschon Vielen rieth ich heuer,", "tokens": ["\u201e", "schon", "Vie\u00b7len", "rieth", "ich", "heu\u00b7er", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch s\u00fc\u00dfes Wasser und guter Rath", "tokens": ["Doch", "s\u00fc\u00b7\u00dfes", "Was\u00b7ser", "und", "gu\u00b7ter", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sind oft zu Schiffe theuer.\u201c", "tokens": ["Sind", "oft", "zu", "Schif\u00b7fe", "theu\u00b7er", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da sprach der graue Herr Riol:", "tokens": ["Da", "sprach", "der", "grau\u00b7e", "Herr", "Ri\u00b7ol", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich bin ein alter Degen,", "tokens": ["\u201e", "ich", "bin", "ein", "al\u00b7ter", "De\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00f6chte meinen Leichnam wohl", "tokens": ["Und", "m\u00f6ch\u00b7te", "mei\u00b7nen", "Leich\u00b7nam", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dereinst in\u2019s Trockne legen.\u201c", "tokens": ["De\u00b7reinst", "in's", "Trock\u00b7ne", "le\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es war Herr Gui, ein Ritter fein,", "tokens": ["Es", "war", "Herr", "Gui", ",", "ein", "Rit\u00b7ter", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NE", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fing wohl an zu singen:", "tokens": ["Der", "fing", "wohl", "an", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich wollt\u2019, ich w\u00e4r\u2019 ein V\u00f6gelein,", "tokens": ["\u201e", "ich", "wollt'", ",", "ich", "w\u00e4r'", "ein", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wollt\u2019 mich zu Liebchen schwingen.\u201c", "tokens": ["Wollt'", "mich", "zu", "Lieb\u00b7chen", "schwin\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da sprach der edle Graf Garein:", "tokens": ["Da", "sprach", "der", "ed\u00b7le", "Graf", "Ga\u00b7rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201egott helf\u2019 uns aus der Schwere!", "tokens": ["\u201e", "gott", "hel\u00b7f'", "uns", "aus", "der", "Schwe\u00b7re", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich trink\u2019 viel lieber den rothen Wein,", "tokens": ["Ich", "trink'", "viel", "lie\u00b7ber", "den", "ro\u00b7then", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Wasser in dem Meere.\u201c", "tokens": ["Als", "Was\u00b7ser", "in", "dem", "Mee\u00b7re", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Herr Lambert sprach, ein J\u00fcngling frisch:", "tokens": ["Herr", "Lam\u00b7bert", "sprach", ",", "ein", "J\u00fcng\u00b7ling", "frisch", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201egott woll\u2019 uns nicht vergessen!", "tokens": ["\u201e", "gott", "woll'", "uns", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aess\u2019 lieber selbst \u2019nen guten Fisch,", "tokens": ["A\u00b7ess'", "lie\u00b7ber", "selbst", "'nen", "gu\u00b7ten", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Statt da\u00df mich Fische fressen.\u201c", "tokens": ["Statt", "da\u00df", "mich", "Fi\u00b7sche", "fres\u00b7sen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOUS", "PPER", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Da sprach Herr Gottfried lobesan:", "tokens": ["Da", "sprach", "Herr", "Gott\u00b7fried", "lo\u00b7be\u00b7san", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich lass\u2019 mir\u2019s halt gefallen,", "tokens": ["\u201e", "ich", "lass'", "mir's", "halt", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man richtet mir nicht anders an,", "tokens": ["Man", "rich\u00b7tet", "mir", "nicht", "an\u00b7ders", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als meinen Br\u00fcdern allen.\u201c", "tokens": ["Als", "mei\u00b7nen", "Br\u00fc\u00b7dern", "al\u00b7len", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der K\u00f6nig Karl am Steuer sa\u00df,", "tokens": ["Der", "K\u00f6\u00b7nig", "Karl", "am", "Steu\u00b7er", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hat kein Wort gesprochen,", "tokens": ["Der", "hat", "kein", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er lenkt das Schiff mit festem Maa\u00df,", "tokens": ["Er", "lenkt", "das", "Schiff", "mit", "fes\u00b7tem", "Maa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis sich der Sturm gebrochen.", "tokens": ["Bis", "sich", "der", "Sturm", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}