nfo": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Vollbefugt und selbstbefreit,", "tokens": ["Voll\u00b7be\u00b7fugt", "und", "selbst\u00b7be\u00b7freit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu entfalten,", "tokens": ["Zu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zu gestalten", "tokens": ["Zu", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Gro\u00dfen Zug der Menschlichkeit.", "tokens": ["Gro\u00b7\u00dfen", "Zug", "der", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}