{"dta.poem.12073": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts sagen kann ein Mund, worin nicht Wahrheit w\u00e4re,", "tokens": ["Nichts", "sa\u00b7gen", "kann", "ein", "Mund", ",", "wo\u00b7rin", "nicht", "Wahr\u00b7heit", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "ART", "NN", "$,", "PWAV", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob wissentlich das Herz auch L\u00fcge nur geb\u00e4re.", "tokens": ["Ob", "wis\u00b7sent\u00b7lich", "das", "Herz", "auch", "L\u00fc\u00b7ge", "nur", "ge\u00b7b\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ADV", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn was er spricht, ist doch ein Bild des was er denkt,", "tokens": ["Denn", "was", "er", "spricht", ",", "ist", "doch", "ein", "Bild", "des", "was", "er", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "ART", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er willk\u00fcrlich auch die Z\u00fcge dran verrenkt;", "tokens": ["Wie", "er", "will\u00b7k\u00fcr\u00b7lich", "auch", "die", "Z\u00fc\u00b7ge", "dran", "ver\u00b7renkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und was er denkt, ist doch die Wahrheit die er sieht,", "tokens": ["Und", "was", "er", "denkt", ",", "ist", "doch", "die", "Wahr\u00b7heit", "die", "er", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er in sich ihr Bild zum Zerrbild auch verzieht.", "tokens": ["Wie", "er", "in", "sich", "ihr", "Bild", "zum", "Zerr\u00b7bild", "auch", "ver\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PRF", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So, was er erst gedacht, und dann was er gesprochen,", "tokens": ["So", ",", "was", "er", "erst", "ge\u00b7dacht", ",", "und", "dann", "was", "er", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "KON", "ADV", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nur der Wahrheit Stral, der zwiefach ist gebrochen.", "tokens": ["Ist", "nur", "der", "Wahr\u00b7heit", "Stral", ",", "der", "zwie\u00b7fach", "ist", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und h\u00e4ttest du in dir den Stral, der r\u00fcckwerts bricht", "tokens": ["Und", "h\u00e4t\u00b7test", "du", "in", "dir", "den", "Stral", ",", "der", "r\u00fcck\u00b7werts", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Doppelbrechungen, du stelltest her das Licht.", "tokens": ["Die", "Dop\u00b7pel\u00b7bre\u00b7chun\u00b7gen", ",", "du", "stell\u00b7test", "her", "das", "Licht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Nur Gott hat diesen Stral in seiner vollen Klarheit;", "tokens": ["Nur", "Gott", "hat", "die\u00b7sen", "Stral", "in", "sei\u00b7ner", "vol\u00b7len", "Klar\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PDAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sieht, du aber ahnst durch ihn, im Lug die Wahrheit.", "tokens": ["Er", "sieht", ",", "du", "a\u00b7ber", "ahnst", "durch", "ihn", ",", "im", "Lug", "die", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "ADV", "APPR", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}