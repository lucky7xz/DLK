{"textgrid.poem.65267": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine tr\u00fcbe Wolke", "tokens": ["Wie", "ei\u00b7ne", "tr\u00fc\u00b7be", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch heitre L\u00fcfte geht,", "tokens": ["Durch", "heit\u00b7re", "L\u00fcf\u00b7te", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wann in der Tanne Wipfel", "tokens": ["Wann", "in", "der", "Tan\u00b7ne", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein mattes L\u00fcftchen weht:", "tokens": ["Ein", "mat\u00b7tes", "L\u00fcft\u00b7chen", "weht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So zieh' ich meine Stra\u00dfe", "tokens": ["So", "zieh'", "ich", "mei\u00b7ne", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dahin mit tr\u00e4gem Fu\u00df,", "tokens": ["Da\u00b7hin", "mit", "tr\u00e4\u00b7gem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch helles, frohes Leben,", "tokens": ["Durch", "hel\u00b7les", ",", "fro\u00b7hes", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einsam und ohne Gru\u00df.", "tokens": ["Ein\u00b7sam", "und", "oh\u00b7ne", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ach, da\u00df die Lust so ruhig!", "tokens": ["Ach", ",", "da\u00df", "die", "Lust", "so", "ru\u00b7hig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach, da\u00df die Welt so licht!", "tokens": ["Ach", ",", "da\u00df", "die", "Welt", "so", "licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als noch die St\u00fcrme tobten,", "tokens": ["Als", "noch", "die", "St\u00fcr\u00b7me", "tob\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War ich so elend nicht.", "tokens": ["War", "ich", "so", "e\u00b7lend", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}