{"textgrid.poem.26125": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sonne kommt herab den Berg,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne kommt herab den Berg,", "tokens": ["Son\u00b7ne", "kommt", "her\u00b7ab", "den", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonne staubt die Augen ab,", "tokens": ["Son\u00b7ne", "staubt", "die", "Au\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streichelt jeden Menschenzwerg.", "tokens": ["Strei\u00b7chelt", "je\u00b7den", "Men\u00b7schenz\u00b7werg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcne B\u00e4ume kann sie zaubern", "tokens": ["Gr\u00fc\u00b7ne", "B\u00e4u\u00b7me", "kann", "sie", "zau\u00b7bern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Tag von Sorgen saubern", "tokens": ["Und", "den", "Tag", "von", "Sor\u00b7gen", "sau\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vergi\u00dft den Kleinsten nie.", "tokens": ["Und", "ver\u00b7gi\u00dft", "den", "Kleins\u00b7ten", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jeder Fink bringt ihr ein St\u00e4ndchen,", "tokens": ["Je\u00b7der", "Fink", "bringt", "ihr", "ein", "St\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Kind reicht ihr sein H\u00e4ndchen,", "tokens": ["Je\u00b7des", "Kind", "reicht", "ihr", "sein", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder liebt wie 'n Sch\u00e4tzchen sie.", "tokens": ["Je\u00b7der", "liebt", "wie", "'n", "Sch\u00e4tz\u00b7chen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}