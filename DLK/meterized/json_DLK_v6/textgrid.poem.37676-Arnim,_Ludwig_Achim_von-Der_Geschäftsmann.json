{"textgrid.poem.37676": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gesch\u00e4ftsmann", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es walten drei Hammer in lustigem Schlag,", "tokens": ["Es", "wal\u00b7ten", "drei", "Ham\u00b7mer", "in", "lus\u00b7ti\u00b7gem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie schmieden in Gluthen mein Leben,", "tokens": ["Sie", "schmie\u00b7den", "in", "Glu\u00b7then", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Vom Weltrauch gedunkelt erbleichet der Tag,", "tokens": ["Vom", "Welt\u00b7rauch", "ge\u00b7dun\u00b7kelt", "er\u00b7blei\u00b7chet", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich schau sie mit ahndendem Leben.", "tokens": ["Ich", "schau", "sie", "mit", "ahn\u00b7den\u00b7dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Nothdurft ergreift mich mit haltender Zang", "tokens": ["Die", "Noth\u00b7durft", "er\u00b7greift", "mich", "mit", "hal\u00b7ten\u00b7der", "Zang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und h\u00e4lt mich hier fest und verbogen,", "tokens": ["Und", "h\u00e4lt", "mich", "hier", "fest", "und", "ver\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der h\u00e4rteste Hammer ert\u00f6net jetzt bang',", "tokens": ["Der", "h\u00e4r\u00b7tes\u00b7te", "Ham\u00b7mer", "er\u00b7t\u00f6\u00b7net", "jetzt", "bang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er hat mich zum Leben erzogen.", "tokens": ["Er", "hat", "mich", "zum", "Le\u00b7ben", "er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Der andere Hammer schl\u00e4gt dumpfig und breit,", "tokens": ["Der", "an\u00b7de\u00b7re", "Ham\u00b7mer", "schl\u00e4gt", "dump\u00b7fig", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er dr\u00fcckt mich mit Lehren der Alten,", "tokens": ["Er", "dr\u00fcckt", "mich", "mit", "Leh\u00b7ren", "der", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich bin hier zum Lernen noch gar nicht bereit,", "tokens": ["Ich", "bin", "hier", "zum", "Ler\u00b7nen", "noch", "gar", "nicht", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ADV", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da mu\u00df ich beim dritten erkalten.", "tokens": ["Da", "mu\u00df", "ich", "beim", "drit\u00b7ten", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der schl\u00e4gt mir durch's Innre ein viereckig Loch,", "tokens": ["Der", "schl\u00e4gt", "mir", "durch's", "Inn\u00b7re", "ein", "vier\u00b7ec\u00b7kig", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mich nagelt an's Pflugholz zum Schneiden,", "tokens": ["Mich", "na\u00b7gelt", "an's", "Pflug\u00b7holz", "zum", "Schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es ziehet mich jedes Paar Ochsen am Joch,", "tokens": ["Es", "zie\u00b7het", "mich", "je\u00b7des", "Paar", "Och\u00b7sen", "am", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr Vorsitz will's Leben verleiden.", "tokens": ["Ihr", "Vor\u00b7sitz", "will's", "Le\u00b7ben", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ich werde nun d\u00fcnner, ich werde nun blank,", "tokens": ["Ich", "wer\u00b7de", "nun", "d\u00fcn\u00b7ner", ",", "ich", "wer\u00b7de", "nun", "blank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Feld' ist kein Bl\u00fcmchen geblieben,", "tokens": ["Im", "Feld'", "ist", "kein", "Bl\u00fcm\u00b7chen", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "F\u00fcr diese Beschwerden, zum herrlichen Dank", "tokens": ["F\u00fcr", "die\u00b7se", "Be\u00b7schwer\u00b7den", ",", "zum", "herr\u00b7li\u00b7chen", "Dank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Werd' ich nun vom Saatfeld vertrieben.", "tokens": ["Werd'", "ich", "nun", "vom", "Saat\u00b7feld", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ausgehn dem Gesch\u00e4ftsmann fr\u00fche die Haar',", "tokens": ["Aus\u00b7gehn", "dem", "Ge\u00b7sch\u00e4fts\u00b7mann", "fr\u00fc\u00b7he", "die", "Haa\u00b7r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Pl\u00fcsch ist vom Sitzen geschoren;", "tokens": ["Der", "Pl\u00fcsch", "ist", "vom", "Sit\u00b7zen", "ge\u00b7scho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein ander Systemchen im anderen Jahr", "tokens": ["Ein", "an\u00b7der", "Sys\u00b7tem\u00b7chen", "im", "an\u00b7de\u00b7ren", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und alle die M\u00fch' ist verloren.", "tokens": ["Und", "al\u00b7le", "die", "M\u00fch'", "ist", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}