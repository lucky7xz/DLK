{"textgrid.poem.54422": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "[widmungsgedicht]", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Blumen vnser zeit/ die Tugendt hoch gezieret/", "tokens": ["Ihr", "Blu\u00b7men", "vn\u00b7ser", "zeit", "/", "die", "Tu\u00b7gendt", "hoch", "ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gottesfurcht bestralt/ die Ehr vnd Zucht regieret/", "tokens": ["Die", "Got\u00b7tes\u00b7furcht", "be\u00b7stralt", "/", "die", "Ehr", "vnd", "Zucht", "re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nembt willig von Mir an/ das zwar geringe Pfand/", "tokens": ["Nembt", "wil\u00b7lig", "von", "Mir", "an", "/", "das", "zwar", "ge\u00b7rin\u00b7ge", "Pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NE", "PTKVZ", "$(", "PDS", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch das ein danckbar Hertz selbst legt in Ewre Hand/", "tokens": ["Doch", "das", "ein", "dan\u00b7ck\u00b7bar", "Hertz", "selbst", "legt", "in", "Ew\u00b7re", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "ADJD", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ihr seids durch die Ich bin gew\u00fcntschter Ruh' gewehret/", "tokens": ["Ihr", "seids", "durch", "die", "Ich", "bin", "ge\u00b7w\u00fcnt\u00b7schter", "Ruh'", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRELS", "PPER", "VAFIN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn mich das grause Gl\u00fcck mit seinem Pfeil versehret/", "tokens": ["Wenn", "mich", "das", "grau\u00b7se", "Gl\u00fcck", "mit", "sei\u00b7nem", "Pfeil", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnnd grimmig auff mich plitzt: Ihr seids/ der trawte Hold", "tokens": ["Vnnd", "grim\u00b7mig", "auff", "mich", "plitzt", ":", "Ihr", "seids", "/", "der", "traw\u00b7te", "Hold"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir H\u00fclff vnd Rettung both/ da ich versincken wolt:", "tokens": ["Mir", "H\u00fclff", "vnd", "Ret\u00b7tung", "both", "/", "da", "ich", "ver\u00b7sin\u00b7cken", "wolt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr seids/ durch welcher Gunst ich jtzt so ruhig bleibe:", "tokens": ["Ihr", "seids", "/", "durch", "wel\u00b7cher", "Gunst", "ich", "jtzt", "so", "ru\u00b7hig", "blei\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "APPR", "PWAT", "NN", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd ohne Noth die Noth der schweren Zeit vertreibe;", "tokens": ["Vnd", "oh\u00b7ne", "Noth", "die", "Noth", "der", "schwe\u00b7ren", "Zeit", "ver\u00b7trei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr seids der hoher Ruhm soll jmmer mit mir gehn/", "tokens": ["Ihr", "seids", "der", "ho\u00b7her", "Ruhm", "soll", "jm\u00b7mer", "mit", "mir", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So lang der Seelen Fewr mir wird im Hertzen stehn.", "tokens": ["So", "lang", "der", "See\u00b7len", "Fewr", "mir", "wird", "im", "Hert\u00b7zen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wil in kurtzem mich noch gar viel h\u00f6her schwingen;", "tokens": ["Ich", "wil", "in", "kurt\u00b7zem", "mich", "noch", "gar", "viel", "h\u00f6\u00b7her", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd Ewrer Tugend Lob/ mir freyem Munde singen.", "tokens": ["Vnd", "Ew\u00b7rer", "Tu\u00b7gend", "Lob", "/", "mir", "frey\u00b7em", "Mun\u00b7de", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$(", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Schawt vnter des di\u00df an/ Wenn Weyrauch nicht ist dar;", "tokens": ["Schawt", "vn\u00b7ter", "des", "di\u00df", "an", "/", "Wenn", "Wey\u00b7rauch", "nicht", "ist", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PDS", "PTKVZ", "$(", "KOUS", "NN", "PTKNEG", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So strewt man Gerst mit Saltz/ der ", "tokens": ["So", "strewt", "man", "Gerst", "mit", "Saltz", "/", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}