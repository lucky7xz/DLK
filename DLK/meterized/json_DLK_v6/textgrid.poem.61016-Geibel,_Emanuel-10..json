{"textgrid.poem.61016": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin die Rose auf der Au,", "tokens": ["Ich", "bin", "die", "Ro\u00b7se", "auf", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die still in D\u00fcften leuchtet;", "tokens": ["Die", "still", "in", "D\u00fcf\u00b7ten", "leuch\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch du, o Liebe, bist der Tau,", "tokens": ["Doch", "du", ",", "o", "Lie\u00b7be", ",", "bist", "der", "Tau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der n\u00e4hrend sie befeuchtet.", "tokens": ["Der", "n\u00e4h\u00b7rend", "sie", "be\u00b7feuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin der dunkle Edelstein,", "tokens": ["Ich", "bin", "der", "dunk\u00b7le", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus tiefem Schacht gew\u00fchlet:", "tokens": ["Aus", "tie\u00b7fem", "Schacht", "ge\u00b7w\u00fch\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du aber bist der Sonnenschein,", "tokens": ["Du", "a\u00b7ber", "bist", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darin er Farben spielet.", "tokens": ["Da\u00b7rin", "er", "Far\u00b7ben", "spie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin der Becher von Kristall,", "tokens": ["Ich", "bin", "der", "Be\u00b7cher", "von", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus dem der K\u00f6nig trinket;", "tokens": ["Aus", "dem", "der", "K\u00f6\u00b7nig", "trin\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist des Weines s\u00fc\u00dfer Schwall,", "tokens": ["Du", "bist", "des", "Wei\u00b7nes", "s\u00fc\u00b7\u00dfer", "Schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der purpurn ihn durchblinket.", "tokens": ["Der", "pur\u00b7purn", "ihn", "durch\u00b7blin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bin die tr\u00fcbe Wolkenwand,", "tokens": ["Ich", "bin", "die", "tr\u00fc\u00b7be", "Wol\u00b7ken\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Himmel aufgezogen;", "tokens": ["Am", "Him\u00b7mel", "auf\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch du bist klar auf mich gespannt", "tokens": ["Doch", "du", "bist", "klar", "auf", "mich", "ge\u00b7spannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als bunter Regenbogen.", "tokens": ["Als", "bun\u00b7ter", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich bin der Memnon stumm und tot,", "tokens": ["Ich", "bin", "der", "Mem\u00b7non", "stumm", "und", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von W\u00fcstennacht bedecket;", "tokens": ["Von", "W\u00fcs\u00b7ten\u00b7nacht", "be\u00b7de\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast den Klang als Morgenrot", "tokens": ["Du", "hast", "den", "Klang", "als", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner Brust erwecket.", "tokens": ["In", "mei\u00b7ner", "Brust", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich bin der Mensch, der vielbewegt", "tokens": ["Ich", "bin", "der", "Mensch", ",", "der", "viel\u00b7be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchirrt das Tal der M\u00e4ngel;", "tokens": ["Durch\u00b7irrt", "das", "Tal", "der", "M\u00e4n\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du aber bist's, die stark mich tr\u00e4gt,", "tokens": ["Du", "a\u00b7ber", "bist's", ",", "die", "stark", "mich", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein lichter Gottesengel.", "tokens": ["Ein", "lich\u00b7ter", "Got\u00b7te\u00b7sen\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}