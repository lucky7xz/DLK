{"textgrid.poem.56555": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbkraut! Kraut!\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkraut! Kraut!\u00ab", "tokens": ["\u00bb", "kraut", "!", "Kraut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "De Heiders slapt geh\u00f6ri ut,", "tokens": ["De", "Hei\u00b7ders", "slapt", "ge\u00b7h\u00f6\u00b7ri", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier s\u00fcnd noch Luken v\u0153r.", "tokens": ["Hier", "s\u00fcnd", "noch", "Lu\u00b7ken", "v\u0153r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns Annre ward dat nich so gut,", "tokens": ["Uns", "A\u00b7nnre", "ward", "dat", "nich", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VAFIN", "ART", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wi m\u0153t der fix hend\u0153r!", "tokens": ["Wi", "m\u0153t", "der", "fix", "hen\u00b7d\u0153r", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ik kam nu al vunn Butendik", "tokens": ["Ik", "kam", "nu", "al", "vunn", "Bu\u00b7ten\u00b7dik"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ut't natte kole Haf;", "tokens": ["Ut't", "nat\u00b7te", "ko\u00b7le", "Haf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "De reckt sik mal un wunnert sik \u2013", "tokens": ["De", "reckt", "sik", "mal", "un", "wun\u00b7nert", "sik", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Un seilt noch wedder af.", "tokens": ["Un", "seilt", "noch", "wed\u00b7der", "af", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "KON", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbkraut! Kraut!\u00ab", "tokens": ["\u00bb", "kraut", "!", "Kraut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "De Dare pumpt \u0119r Ammer vull:", "tokens": ["De", "Da\u00b7re", "pumpt", "\u0119r", "Am\u00b7mer", "vull", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbkraut! l\u0119bendi un krall!", "tokens": ["\u00bb", "kraut", "!", "l\u0119\u00b7ben\u00b7di", "un", "krall", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "FM", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Min Deern, wenn'k j\u00fcmmer t\u00f6ben schull,", "tokens": ["Min", "Deern", ",", "wenn'k", "j\u00fcm\u00b7mer", "t\u00f6\u00b7ben", "schull", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warn se v\u0153r Abnd ni all.\u00ab", "tokens": ["Warn", "se", "v\u0153r", "Abnd", "ni", "all", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Dar kumt Een mit en Achndeelssett:", "tokens": ["Dar", "kumt", "E\u00b7en", "mit", "en", "A\u00b7chndeels\u00b7sett", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "\u00bbmin Kind, de Kraut s\u00fcnd rar! \u2013", "tokens": ["\u00bb", "min", "Kind", ",", "de", "Kraut", "s\u00fcnd", "rar", "!", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Scheerkrauten? n\u0119! de weern ni fett,", "tokens": ["Scheer\u00b7krau\u00b7ten", "?", "n\u0119", "!", "de", "weern", "ni", "fett", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$.", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "De wasst bet tokum Jahr.\u00ab", "tokens": ["De", "wasst", "bet", "to\u00b7kum", "Jahr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbkraut! Kraut!\u00ab", "tokens": ["\u00bb", "kraut", "!", "Kraut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wa weer de J\u00fcmfer al in Staat,", "tokens": ["Wa", "weer", "de", "J\u00fcm\u00b7fer", "al", "in", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Schoh un Str\u00fcmp un all!", "tokens": ["In", "Schoh", "un", "Str\u00fcmp", "un", "all", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "FM", "FM", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "De Hals so witt, de Haar na'n Draht,", "tokens": ["De", "Hals", "so", "witt", ",", "de", "Haar", "na'n", "Draht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVFIN", "$,", "NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "As schull se glik to Ball!", "tokens": ["As", "schull", "se", "glik", "to", "Ball", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Un Unserener stiggt barfot", "tokens": ["Un", "Un\u00b7se\u00b7re\u00b7ner", "stiggt", "bar\u00b7fot"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vun B\u00fcsum na de Heid,", "tokens": ["Vun", "B\u00fc\u00b7sum", "na", "de", "Heid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Un hett se denn \u0119r Solt op Brot,", "tokens": ["Un", "hett", "se", "denn", "\u0119r", "Solt", "op", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "FM", "KON", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So ist aln grote Freid.", "tokens": ["So", "ist", "aln", "gro\u00b7te", "Freid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbkraut! Kraut!\u00ab", "tokens": ["\u00bb", "kraut", "!", "Kraut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wa lett dat Mark doch wunnersch\u00f6n", "tokens": ["Wa", "lett", "dat", "Mark", "doch", "wun\u00b7ner\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit all de Lust un Pracht!", "tokens": ["Mit", "all", "de", "Lust", "un", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Un rund herum de B\u00f6m so gr\u00f6n,", "tokens": ["Un", "rund", "he\u00b7rum", "de", "B\u00f6m", "so", "gr\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "NE", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dat Allens l\u0119vt un lacht!", "tokens": ["Dat", "Al\u00b7lens", "l\u0119vt", "un", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Un Hus bi Hus en Bank so witt", "tokens": ["Un", "Hus", "bi", "Hus", "en", "Bank", "so", "witt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "ADV", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Un Finstern spegelglatt,", "tokens": ["Un", "Fins\u00b7tern", "spe\u00b7gel\u00b7glatt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Un de dar binn' un buten sitt,", "tokens": ["Un", "de", "dar", "binn'", "un", "bu\u00b7ten", "sitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "De l\u0119vt un freit sik satt.", "tokens": ["De", "l\u0119vt", "un", "freit", "sik", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbkraut! Kraut!\u00ab", "tokens": ["\u00bb", "kraut", "!", "Kraut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "O wahn ik hier un harr min Brot", "tokens": ["O", "wahn", "ik", "hier", "un", "harr", "min", "Brot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "ADV", "FM", "FM", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Un keek hier S\u00fcnnabnds ut!", "tokens": ["Un", "keek", "hier", "S\u00fcn\u00b7nabnds", "ut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mi d\u00fcnkt, mi weer dat Hart so grot,", "tokens": ["Mi", "d\u00fcnkt", ",", "mi", "weer", "dat", "Hart", "so", "grot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "NE", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "As wull't mi baben rut.", "tokens": ["As", "wull't", "mi", "ba\u00b7ben", "rut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "De Boden all in S\u00fcnnenschin,", "tokens": ["De", "Bo\u00b7den", "all", "in", "S\u00fcn\u00b7nen\u00b7schin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "De Wagens un de P\u0119r:", "tokens": ["De", "Wa\u00b7gens", "un", "de", "P\u0119r", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mein Gott! wa kunn ik gl\u00fccklich sin,", "tokens": ["Mein", "Gott", "!", "wa", "kunn", "ik", "gl\u00fcck\u00b7lich", "sin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "XY", "NE", "NE", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn'k blot en Heider weer!", "tokens": ["Wenn'k", "blot", "en", "Hei\u00b7der", "weer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbkraut! Kraut!\u00ab", "tokens": ["\u00bb", "kraut", "!", "Kraut", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wa weer de Mann doch dodenblass,", "tokens": ["Wa", "weer", "de", "Mann", "doch", "do\u00b7den\u00b7blass", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "De dar ut't Finster kik!", "tokens": ["De", "dar", "ut't", "Fins\u00b7ter", "kik", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bewahre! ik entz\u00fcck mi fast", "tokens": ["Be\u00b7wah\u00b7re", "!", "ik", "ent\u00b7z\u00fcck", "mi", "fast"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Un meen, ik seeg en Lik! \u2013", "tokens": ["Un", "meen", ",", "ik", "seeg", "en", "Lik", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "N\u0119, lewer sund un guden Moth,", "tokens": ["N\u0119", ",", "le\u00b7wer", "sund", "un", "gu\u00b7den", "Moth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "As krank un inne Heid!", "tokens": ["As", "krank", "un", "in\u00b7ne", "Heid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bewahr mi man de lewe Gott,", "tokens": ["Be\u00b7wahr", "mi", "man", "de", "le\u00b7we", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dat mi't nie slechter geit!", "tokens": ["Dat", "mi't", "nie", "slech\u00b7ter", "geit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}