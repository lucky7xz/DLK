{"textgrid.poem.66719": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von stillen Bergesh\u00f6hen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von stillen Bergesh\u00f6hen,", "tokens": ["Von", "stil\u00b7len", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seeufers friedlichem Haus,", "tokens": ["Se\u00b7eu\u00b7fers", "fried\u00b7li\u00b7chem", "Haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nahm ich mit Schwingenwehen", "tokens": ["Nahm", "ich", "mit", "Schwin\u00b7gen\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meinen Flug zu der Weltstadt Gebraus.", "tokens": ["Mei\u00b7nen", "Flug", "zu", "der", "Welt\u00b7stadt", "Ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Habe mit Felsen und Quellen", "tokens": ["Ha\u00b7be", "mit", "Fel\u00b7sen", "und", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Jahrelang Zwiesprach getauscht,", "tokens": ["Jah\u00b7re\u00b7lang", "Zwie\u00b7sprach", "ge\u00b7tauscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00f6hrensausen und Wasserf\u00e4llen", "tokens": ["F\u00f6h\u00b7ren\u00b7sau\u00b7sen", "und", "Was\u00b7ser\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie Bruder und Kind gelauscht.", "tokens": ["Wie", "Bru\u00b7der", "und", "Kind", "ge\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Meine einsamen Pfade ging ich,", "tokens": ["Mei\u00b7ne", "ein\u00b7sa\u00b7men", "Pfa\u00b7de", "ging", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Im Herzen umspannend die Welt,", "tokens": ["Im", "Her\u00b7zen", "um\u00b7span\u00b7nend", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Krone der Kraft empfing ich", "tokens": ["Die", "Kro\u00b7ne", "der", "Kraft", "emp\u00b7fing", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vom schimmernden Sternenzelt ...", "tokens": ["Vom", "schim\u00b7mern\u00b7den", "Ster\u00b7nen\u00b7zelt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun geh ich im dichten Gew\u00fchle", "tokens": ["Nun", "geh", "ich", "im", "dich\u00b7ten", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Durch die menschenwimmelnde Stadt,", "tokens": ["Durch", "die", "men\u00b7schen\u00b7wim\u00b7meln\u00b7de", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch im Get\u00fcmmel f\u00fchle", "tokens": ["Doch", "im", "Ge\u00b7t\u00fcm\u00b7mel", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimmer den Mut ich matt.", "tokens": ["Nim\u00b7mer", "den", "Mut", "ich", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Was einst den J\u00fcngling wie Klammer", "tokens": ["Was", "einst", "den", "J\u00fcng\u00b7ling", "wie", "Klam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "KOKOM", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zusammengepre\u00dft und erdr\u00fcckt,", "tokens": ["Zu\u00b7sam\u00b7men\u00b7ge\u00b7pre\u00dft", "und", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Greife ich heut als Hammer,", "tokens": ["Grei\u00b7fe", "ich", "heut", "als", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wird nun zum Wurfspeer gez\u00fcckt.", "tokens": ["Wird", "nun", "zum", "Wurf\u00b7speer", "ge\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ich werfe das Ungeheuer, \u2013", "tokens": ["Ich", "wer\u00b7fe", "das", "Un\u00b7ge\u00b7heu\u00b7er", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein sp\u00e4hender Blick erkennt's \u2013", "tokens": ["Mein", "sp\u00e4\u00b7hen\u00b7der", "Blick", "er\u00b7kennt's", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und singe durch Rauch und Feuer", "tokens": ["Und", "sin\u00b7ge", "durch", "Rauch", "und", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In seinen Rachen den Lenz.", "tokens": ["In", "sei\u00b7nen", "Ra\u00b7chen", "den", "Lenz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}