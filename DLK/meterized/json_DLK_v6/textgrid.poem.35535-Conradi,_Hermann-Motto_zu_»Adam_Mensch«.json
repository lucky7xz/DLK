{"textgrid.poem.35535": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Motto zu \u00bbAdam Mensch\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df fahren, was dich traurig macht,", "tokens": ["La\u00df", "fah\u00b7ren", ",", "was", "dich", "trau\u00b7rig", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was die Enge dir geboren \u2013:", "tokens": ["Und", "was", "die", "En\u00b7ge", "dir", "ge\u00b7bo\u00b7ren", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPER", "VVPP", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dieser gro\u00dfen Freudennacht", "tokens": ["In", "die\u00b7ser", "gro\u00b7\u00dfen", "Freu\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt dir dein Genius unverloren.", "tokens": ["Bleibt", "dir", "dein", "Ge\u00b7nius", "un\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Wir leben, mein geliebtes Weib \u2013", "tokens": ["Wir", "le\u00b7ben", ",", "mein", "ge\u00b7lieb\u00b7tes", "Weib", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unser Leben atmet F\u00fclle \u2013:", "tokens": ["Und", "un\u00b7ser", "Le\u00b7ben", "at\u00b7met", "F\u00fcl\u00b7le", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dinge unverstandne H\u00fclle", "tokens": ["Der", "Din\u00b7ge", "un\u00b7ver\u00b7stand\u00b7ne", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fiel ab vom nackten Gottesleib.", "tokens": ["Fiel", "ab", "vom", "nack\u00b7ten", "Got\u00b7tes\u00b7leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}