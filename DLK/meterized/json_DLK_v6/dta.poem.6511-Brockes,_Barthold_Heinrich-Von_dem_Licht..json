{"dta.poem.6511": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Gleichni\u00df macht es klar. Man stelle sich nur f\u00fcr,", "tokens": ["Ein", "Gleich\u00b7ni\u00df", "macht", "es", "klar", ".", "Man", "stel\u00b7le", "sich", "nur", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "PIS", "VVFIN", "PRF", "ADV", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob hinge zwischen dir und mir", "tokens": ["Ob", "hin\u00b7ge", "zwi\u00b7schen", "dir", "und", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein langer Spie\u00df,", "tokens": ["Ein", "lan\u00b7ger", "Spie\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und da\u00df ich dann an eine Spitze stie\u00df,", "tokens": ["Und", "da\u00df", "ich", "dann", "an", "ei\u00b7ne", "Spit\u00b7ze", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So w\u00fcrde gleich die andere dich dr\u00fccken,", "tokens": ["So", "w\u00fcr\u00b7de", "gleich", "die", "an\u00b7de\u00b7re", "dich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ohn da\u00df man erst den Spie\u00df gebrauchte fortzur\u00fccken.", "tokens": ["Ohn", "da\u00df", "man", "erst", "den", "Spie\u00df", "ge\u00b7brauch\u00b7te", "fort\u00b7zu\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Lichtes W\u00fcrckung nun ist mehrentheils dieselbe,", "tokens": ["Des", "Lich\u00b7tes", "W\u00fcr\u00b7ckung", "nun", "ist", "meh\u00b7ren\u00b7theils", "die\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "ADV", "PDAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Strahl, der zu uns kommt, vom himmlischen Gew\u00f6lbe,", "tokens": ["Ein", "Strahl", ",", "der", "zu", "uns", "kommt", ",", "vom", "himm\u00b7li\u00b7schen", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Besteht aus C\u00f6rperchen, die an einander liegen,", "tokens": ["Be\u00b7steht", "aus", "C\u00f6r\u00b7per\u00b7chen", ",", "die", "an", "ein\u00b7an\u00b7der", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "So bald man eines r\u00fchrt, so dr\u00fcckt zu gleicher Zeit", "tokens": ["So", "bald", "man", "ei\u00b7nes", "r\u00fchrt", ",", "so", "dr\u00fcckt", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der den, der an ihn liegt, mit grosser Schnelligkeit,", "tokens": ["Der", "den", ",", "der", "an", "ihn", "liegt", ",", "mit", "gros\u00b7ser", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ohn da\u00df sie sich darum aus ihren Stellen f\u00fcgen,", "tokens": ["Ohn", "da\u00df", "sie", "sich", "da\u00b7rum", "aus", "ih\u00b7ren", "Stel\u00b7len", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PRF", "PAV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Von einem Punct, woraus ein Strahl auf uns gezielt,", "tokens": ["Von", "ei\u00b7nem", "Punct", ",", "wo\u00b7raus", "ein", "Strahl", "auf", "uns", "ge\u00b7zielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es sey der Abstand auch noch eins so lang, so weit,", "tokens": ["Es", "sey", "der", "Ab\u00b7stand", "auch", "noch", "eins", "so", "lang", ",", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "PIS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bis zu der andern Eck\u2019, ist gleich der Sto\u00df gef\u00fchlt.", "tokens": ["Bis", "zu", "der", "an\u00b7dern", "Eck'", ",", "ist", "gleich", "der", "Sto\u00df", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}