{"dta.poem.18380": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wie sein vogel starb/ mit dem er vor  \n diesem gerne speisen wolte/ wie selbi-  \n ger einer jungfer aus dem  \n munde a\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das hab ich wohl gedacht/ dein allzu gut geschicke/", "tokens": ["Das", "hab", "ich", "wohl", "ge\u00b7dacht", "/", "dein", "all\u00b7zu", "gut", "ge\u00b7schi\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$(", "PPOSAT", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein zei\u00dfgen/ dr\u00e4ute dir den j\u00e4hen untergang.", "tokens": ["Mein", "zei\u00df\u00b7gen", "/", "dr\u00e4u\u00b7te", "dir", "den", "j\u00e4\u00b7hen", "un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer sich nicht m\u00e4\u00dfiget bey gar zu grossen gl\u00fccke/", "tokens": ["Wer", "sich", "nicht", "m\u00e4\u00b7\u00dfi\u00b7get", "bey", "gar", "zu", "gros\u00b7sen", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "VVFIN", "APPR", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gewi\u00dflich er genie\u00dft des wechsels gar nicht lang.", "tokens": ["Ge\u00b7wi\u00df\u00b7lich", "er", "ge\u00b7nie\u00dft", "des", "wech\u00b7sels", "gar", "nicht", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da hast du nun die frucht der ungewehnten speisen/", "tokens": ["Da", "hast", "du", "nun", "die", "frucht", "der", "un\u00b7ge\u00b7wehn\u00b7ten", "spei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von denen ich gesagt/ sie dienen v\u00f6geln nicht.", "tokens": ["Von", "de\u00b7nen", "ich", "ge\u00b7sagt", "/", "sie", "die\u00b7nen", "v\u00f6\u00b7geln", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$(", "PPER", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du kanst mir andere auf dein exempel weisen", "tokens": ["Du", "kanst", "mir", "an\u00b7de\u00b7re", "auf", "dein", "ex\u00b7em\u00b7pel", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wann sie an diesen tisch zu gehn der haber sticht.", "tokens": ["Wann", "sie", "an", "die\u00b7sen", "tisch", "zu", "gehn", "der", "ha\u00b7ber", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PDAT", "ADJD", "PTKZU", "VVINF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was n\u00fctzt dir nun der neid der mich zur\u00fcck getrieben/", "tokens": ["Was", "n\u00fctzt", "dir", "nun", "der", "neid", "der", "mich", "zu\u00b7r\u00fcck", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als ich dein gast zu seyn so sehnlich von dir bat.", "tokens": ["Als", "ich", "dein", "gast", "zu", "seyn", "so", "sehn\u00b7lich", "von", "dir", "bat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKZU", "VAINF", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der speisen ist ja noch viel vorrath \u00fcberblieben/", "tokens": ["Der", "spei\u00b7sen", "ist", "ja", "noch", "viel", "vor\u00b7rath", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da dich nunmehr der todt schon in den klauen hat.", "tokens": ["Da", "dich", "nun\u00b7mehr", "der", "todt", "schon", "in", "den", "klau\u00b7en", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJD", "ADV", "APPR", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "H\u00e4tt ich dirs vorgemacht/ und du mir zugesehen/", "tokens": ["H\u00e4tt", "ich", "dirs", "vor\u00b7ge\u00b7macht", "/", "und", "du", "mir", "zu\u00b7ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "VVPP", "$(", "KON", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie man von lippen trinckt/ und aus dem munde spei\u00dft", "tokens": ["Wie", "man", "von", "lip\u00b7pen", "trinckt", "/", "und", "aus", "dem", "mun\u00b7de", "spei\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "NN", "VVFIN", "$(", "KON", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So w\u00e4re es vielleicht noch nicht umb dich geschehen.", "tokens": ["So", "w\u00e4\u00b7re", "es", "viel\u00b7leicht", "noch", "nicht", "umb", "dich", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und deine seele zu dem grabe fortgerei\u00dft.", "tokens": ["Und", "dei\u00b7ne", "see\u00b7le", "zu", "dem", "gra\u00b7be", "fort\u00b7ge\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich h\u00e4tte dich gelehrt/ wie die Rubinen h\u00f6le", "tokens": ["Ich", "h\u00e4t\u00b7te", "dich", "ge\u00b7lehrt", "/", "wie", "die", "Ru\u00b7bi\u00b7nen", "h\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zwar mu\u00df ein sammel platz der lebensgeister seyn;", "tokens": ["Zwar", "mu\u00df", "ein", "sam\u00b7mel", "platz", "der", "le\u00b7bens\u00b7geis\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "ART", "ADJA", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Doch so/ da\u00df wiederum die satt gek\u00fcste seele", "tokens": ["Doch", "so", "/", "da\u00df", "wie\u00b7de\u00b7rum", "die", "satt", "ge\u00b7k\u00fcs\u00b7te", "see\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "KOUS", "ADV", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sich von den lippen dringt in alle glieder ein.", "tokens": ["Sich", "von", "den", "lip\u00b7pen", "dringt", "in", "al\u00b7le", "glie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da hastu armes thier der wiederkehr vergessen/", "tokens": ["Da", "has\u00b7tu", "ar\u00b7mes", "thier", "der", "wie\u00b7der\u00b7kehr", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "ART", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wie dein leben an des schnabels spitze hieng/", "tokens": ["Und", "wie", "dein", "le\u00b7ben", "an", "des", "schna\u00b7bels", "spit\u00b7ze", "hieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "VVFIN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da hastu zugeschnapt noch mehr und mehr zu essen/", "tokens": ["Da", "has\u00b7tu", "zu\u00b7ge\u00b7schnapt", "noch", "mehr", "und", "mehr", "zu", "es\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bi\u00df durch das offne maul die seele von dir gieng.", "tokens": ["Bi\u00df", "durch", "das", "off\u00b7ne", "maul", "die", "see\u00b7le", "von", "dir", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jtzt solt ich billiger mich freuen/ als betr\u00fcben/", "tokens": ["Jtzt", "solt", "ich", "bil\u00b7li\u00b7ger", "mich", "freu\u00b7en", "/", "als", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$(", "KOKOM", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "(ein neydisches gem\u00fcth ist keiner thr\u00e4nen wehrt.)", "tokens": ["(", "ein", "ney\u00b7di\u00b7sches", "ge\u00b7m\u00fcth", "ist", "kei\u00b7ner", "thr\u00e4\u00b7nen", "wehrt", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch mu\u00df ich dich mein thier des todtes wegen lieben/", "tokens": ["Doch", "mu\u00df", "ich", "dich", "mein", "thier", "des", "tod\u00b7tes", "we\u00b7gen", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PPOSAT", "NN", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "De\u00df ich mit mehrer lust/ als du/ gar offt begehrt.", "tokens": ["De\u00df", "ich", "mit", "meh\u00b7rer", "lust", "/", "als", "du", "/", "gar", "offt", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "$(", "KOUS", "PPER", "$(", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch mit dem unterscheid/ da\u00df ichs so grob nicht machte/", "tokens": ["Doch", "mit", "dem", "un\u00b7ter\u00b7scheid", "/", "da\u00df", "ichs", "so", "grob", "nicht", "mach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "KOUS", "PIS", "ADV", "ADJD", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und gar die seele aus den lippen von mir sties.", "tokens": ["Und", "gar", "die", "see\u00b7le", "aus", "den", "lip\u00b7pen", "von", "mir", "sties", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nein/ sondern wann das gl\u00fcck am freundlichsten mir lachte/", "tokens": ["Nein", "/", "son\u00b7dern", "wann", "das", "gl\u00fcck", "am", "freund\u00b7lichs\u00b7ten", "mir", "lach\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KON", "PWAV", "ART", "NN", "APPRART", "ADJA", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.32": {"text": "Und es am besten schmeckt/ die speise fahreu lie\u00df.", "tokens": ["Und", "es", "am", "bes\u00b7ten", "schmeckt", "/", "die", "spei\u00b7se", "fah\u00b7reu", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKA", "ADJD", "VVFIN", "$(", "ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Des zuckers gar zu viel verschleimet un\u00df den magen/", "tokens": ["Des", "zu\u00b7ckers", "gar", "zu", "viel", "ver\u00b7schlei\u00b7met", "un\u00df", "den", "ma\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PTKA", "PIS", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Alkermes wil gesund/ doch nicht gemi\u00dfbraucht seyn.", "tokens": ["Al\u00b7ker\u00b7mes", "wil", "ge\u00b7sund", "/", "doch", "nicht", "ge\u00b7mi\u00df\u00b7braucht", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADJD", "$(", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Geh\u00e4uffter Mithridat kan uns zu grabe tragen/", "tokens": ["Ge\u00b7h\u00e4uff\u00b7ter", "Mith\u00b7ri\u00b7dat", "kan", "uns", "zu", "gra\u00b7be", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "PTKZU", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und Ambra stets gebraucht/ nimmt haupt und sinnen ein.", "tokens": ["Und", "A\u00b7mbra", "stets", "ge\u00b7braucht", "/", "nimmt", "haupt", "und", "sin\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "VVPP", "$(", "VVFIN", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ey! H\u00e4ttstu dies bedacht! doch magstu seyn gestorben", "tokens": ["Ey", "!", "H\u00e4tts\u00b7tu", "dies", "be\u00b7dacht", "!", "doch", "mags\u00b7tu", "seyn", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PDS", "VVPP", "$.", "ADV", "VMFIN", "PPOSAT", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Von thorheit oder auch von grossem \u00fcberflu\u00df;", "tokens": ["Von", "thor\u00b7heit", "o\u00b7der", "auch", "von", "gros\u00b7sem", "\u00fc\u00b7berf\u00b7lu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So hastu doch das lob von aller welt erworben/", "tokens": ["So", "has\u00b7tu", "doch", "das", "lob", "von", "al\u00b7ler", "welt", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df man dich iederzeit gl\u00fcckselig preisen mu\u00df.", "tokens": ["Da\u00df", "man", "dich", "ie\u00b7der\u00b7zeit", "gl\u00fcck\u00b7se\u00b7lig", "prei\u00b7sen", "mu\u00df", "."], "token_info