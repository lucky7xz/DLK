{"textgrid.poem.61995": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Vogelin-Prinze\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein V\u00f6gelein,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kanaria von Geschlecht,", "tokens": ["Ka\u00b7na\u00b7ria", "von", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es war so sch\u00f6n, so gelb, so fein,", "tokens": ["Es", "war", "so", "sch\u00f6n", ",", "so", "gelb", ",", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's V\u00f6geln eben recht.", "tokens": ["Wie's", "V\u00f6\u00b7geln", "e\u00b7ben", "recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch ach, das arme V\u00f6gelein", "tokens": ["Doch", "ach", ",", "das", "ar\u00b7me", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im goldnen K\u00e4fig sa\u00df,", "tokens": ["Im", "gold\u00b7nen", "K\u00e4\u00b7fig", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit den kleinen Aeugelein", "tokens": ["Und", "mit", "den", "klei\u00b7nen", "A\u00b7e\u00b7u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den gro\u00dfen Himmel ma\u00df.", "tokens": ["Den", "gro\u00b7\u00dfen", "Him\u00b7mel", "ma\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein frecher Sperling flog vorbei", "tokens": ["Ein", "fre\u00b7cher", "Sper\u00b7ling", "flog", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sang ihr zum Exze\u00df:", "tokens": ["Und", "sang", "ihr", "zum", "Ex\u00b7ze\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "\u00bbich lieb' Dich bis zur Raserei,", "tokens": ["\u00bb", "ich", "lieb'", "Dich", "bis", "zur", "Ra\u00b7se\u00b7rei", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbo, Vogelin-Prinze\u00df!\u00ab", "tokens": ["\u00bb", "o", ",", "Vo\u00b7ge\u00b7lin\u00b7Prin\u00b7ze\u00df", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "FM", "$,", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbo, Vogelin, Dein K\u00f6pfchen klein", "tokens": ["\u00bb", "o", ",", "Vo\u00b7ge\u00b7lin", ",", "Dein", "K\u00f6pf\u00b7chen", "klein"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "$,", "NE", "$,", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4llt mir gar zu gut!\u00ab", "tokens": ["Ge\u00b7f\u00e4llt", "mir", "gar", "zu", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kocht des stolzen V\u00f6gelein", "tokens": ["Da", "kocht", "des", "stol\u00b7zen", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kanarisch hei\u00dfes Blut!", "tokens": ["Ka\u00b7na\u00b7risch", "hei\u00b7\u00dfes", "Blut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbich mag Dich nicht, ich brauch' Dich nicht,", "tokens": ["\u00bb", "ich", "mag", "Dich", "nicht", ",", "ich", "brauch'", "Dich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir ist nach Dir nicht bang,", "tokens": ["Mir", "ist", "nach", "Dir", "nicht", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl sehn' ich mich nach Himmelslicht,", "tokens": ["Wohl", "sehn'", "ich", "mich", "nach", "Him\u00b7mels\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nach des K\u00fcnstlers Sang!\u00ab", "tokens": ["Und", "nach", "des", "K\u00fcnst\u00b7lers", "Sang", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch nach des frechen Sperlings Lied", "tokens": ["Doch", "nach", "des", "fre\u00b7chen", "Sper\u00b7lings", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War mir noch niemals bang,", "tokens": ["War", "mir", "noch", "nie\u00b7mals", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdenn \u2013 singt sie himmlisch \u2013 nie ergl\u00fcht", "tokens": ["\u00bb", "denn", "\u2013", "singt", "sie", "himm\u00b7lisch", "\u2013", "nie", "er\u00b7gl\u00fcht"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "$(", "VVFIN", "PPER", "ADJD", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz bei niedrem Klang!\u00ab \u2013", "tokens": ["Mein", "Herz", "bei", "nied\u00b7rem", "Klang", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Selbst noch eine Menschenbl\u00fcte,", "tokens": ["Selbst", "noch", "ei\u00b7ne", "Men\u00b7schen\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trug ich Kummer im Gem\u00fcte,", "tokens": ["Trug", "ich", "Kum\u00b7mer", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00df genug f\u00fcr eine Welt.", "tokens": ["Gro\u00df", "ge\u00b7nug", "f\u00fcr", "ei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jeder Wahn, der sie bet\u00f6rte,", "tokens": ["Je\u00b7der", "Wahn", ",", "der", "sie", "be\u00b7t\u00f6r\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den sie \u2013 grausam \u2013 hoch verehrte,", "tokens": ["Den", "sie", "\u2013", "grau\u00b7sam", "\u2013", "hoch", "ver\u00b7ehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "ADJD", "$(", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese kleine Menschenwelt. \u2013 \u2013", "tokens": ["Die\u00b7se", "klei\u00b7ne", "Men\u00b7schen\u00b7welt", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PDAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sieh, er ward in mir zur Wunde,", "tokens": ["Sieh", ",", "er", "ward", "in", "mir", "zur", "Wun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmerzend, blutend jede Stunde,", "tokens": ["Schmer\u00b7zend", ",", "blu\u00b7tend", "je\u00b7de", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer tiefer dringend ein. \u2013", "tokens": ["Im\u00b7mer", "tie\u00b7fer", "drin\u00b7gend", "ein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Fremdes Leid und eigne Schmerzen", "tokens": ["Frem\u00b7des", "Leid", "und", "eig\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trug ich in dem weichen Herzen,", "tokens": ["Trug", "ich", "in", "dem", "wei\u00b7chen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandte aufw\u00e4rts meinen Blick:", "tokens": ["Wand\u00b7te", "auf\u00b7w\u00e4rts", "mei\u00b7nen", "Blick", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Helfen wollt' ich, lindern, retten,", "tokens": ["Hel\u00b7fen", "wollt'", "ich", ",", "lin\u00b7dern", ",", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADJA", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fcck an dieses Weltall ketten,", "tokens": ["Gl\u00fcck", "an", "die\u00b7ses", "Wel\u00b7tall", "ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosig bilden sein Geschick.", "tokens": ["Ro\u00b7sig", "bil\u00b7den", "sein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Solche Freuden, diesen Segen,", "tokens": ["Sol\u00b7che", "Freu\u00b7den", ",", "die\u00b7sen", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Betend sich die Lippen regen,", "tokens": ["Be\u00b7tend", "sich", "die", "Lip\u00b7pen", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gott mich's ahnen lie\u00df! \u2013", "tokens": ["Gott", "mich's", "ah\u00b7nen", "lie\u00df", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PIS", "VVINF", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}