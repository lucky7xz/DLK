{"dta.poem.20547": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "6.  \n  Stein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Wer reist dort so sp&#228;t noch bei Regen und Wind?", "tokens": ["Wer", "reist", "dort", "so", "sp", "&#228;", "t", "noch", "bei", "Re\u00b7gen", "und", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADJD", "$(", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Mann ist\u2019s, sinnig und edel gesinnt.", "tokens": ["Ein", "Mann", "ist's", ",", "sin\u00b7nig", "und", "e\u00b7del", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er f\u00e4hrt in der Nacht ohne mondlichen Schein,", "tokens": ["Er", "f\u00e4hrt", "in", "der", "Nacht", "oh\u00b7ne", "mond\u00b7li\u00b7chen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Freiherr des Reiches, der Herr ", "tokens": ["Der", "Frei\u00b7herr", "des", "Rei\u00b7ches", ",", "der", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "In polnischer Waldmark ist\u2019s eisig und leer,", "tokens": ["In", "pol\u00b7ni\u00b7scher", "Wald\u00b7mark", "ist's", "ei\u00b7sig", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Grell heulen die W\u00f6lfe, dumpf brummet der B\u00e4r!", "tokens": ["Grell", "heu\u00b7len", "die", "W\u00f6l\u00b7fe", ",", "dumpf", "brum\u00b7met", "der", "B\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zur Kutsche hinaus streckt podagrisches Bein", "tokens": ["Zur", "Kut\u00b7sche", "hin\u00b7aus", "streckt", "po\u00b7da\u00b7gri\u00b7sches", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APZR", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Freiherr des Reiches, der Herr ", "tokens": ["Der", "Frei\u00b7herr", "des", "Rei\u00b7ches", ",", "der", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Nicht achtet er Beulen, noch gichtisches Ziehn,", "tokens": ["Nicht", "ach\u00b7tet", "er", "Beu\u00b7len", ",", "noch", "gich\u00b7ti\u00b7sches", "Ziehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "VVINF", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sein Antlitz, er f\u00fchlt es in Freude ergl\u00fch\u2019n;", "tokens": ["Sein", "Ant\u00b7litz", ",", "er", "f\u00fchlt", "es", "in", "Freu\u00b7de", "er\u00b7gl\u00fch'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Von Petersburg reist bis Stadt Breslau hinein", "tokens": ["Von", "Pe\u00b7ters\u00b7burg", "reist", "bis", "Stadt", "Bres\u00b7lau", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "APPR", "NN", "NE", "APZR"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Freiherr des Reiches, der Herr ", "tokens": ["Der", "Frei\u00b7herr", "des", "Rei\u00b7ches", ",", "der", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}