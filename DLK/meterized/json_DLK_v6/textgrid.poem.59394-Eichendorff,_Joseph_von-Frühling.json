{"textgrid.poem.59394": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn die Lerche hell anstimmt", "tokens": ["Und", "wenn", "die", "Ler\u00b7che", "hell", "an\u00b7stimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Fr\u00fchling rings bricht an:", "tokens": ["Und", "Fr\u00fch\u00b7ling", "rings", "bricht", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schauert tief und Fl\u00fcgel nimmt,", "tokens": ["Da", "schau\u00b7ert", "tief", "und", "Fl\u00fc\u00b7gel", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer irgend fliegen kann.", "tokens": ["Wer", "ir\u00b7gend", "flie\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Erde gr\u00fc\u00dft er hochbegl\u00fcckt,", "tokens": ["Die", "Er\u00b7de", "gr\u00fc\u00dft", "er", "hoch\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die, eine junge Braut,", "tokens": ["Die", ",", "ei\u00b7ne", "jun\u00b7ge", "Braut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Blumen wild und bunt geschm\u00fcckt,", "tokens": ["Mit", "Blu\u00b7men", "wild", "und", "bunt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tief in das Herz ihm schaut.", "tokens": ["Tief", "in", "das", "Herz", "ihm", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Himmel dann, das blaue Meer", "tokens": ["Den", "Him\u00b7mel", "dann", ",", "das", "blau\u00b7e", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sehnsucht, gr\u00fc\u00dft er treu,", "tokens": ["Der", "Sehn\u00b7sucht", ",", "gr\u00fc\u00dft", "er", "treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stammen Lied und S\u00e4nger her", "tokens": ["Da", "stam\u00b7men", "Lied", "und", "S\u00e4n\u00b7ger", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sp\u00fcren's immer neu.", "tokens": ["Und", "sp\u00fc\u00b7ren's", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die dunkeln Gr\u00fcnde s\u00e4useln kaum,", "tokens": ["Die", "dun\u00b7keln", "Gr\u00fcn\u00b7de", "s\u00e4u\u00b7seln", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schaun so fremd herauf.", "tokens": ["Sie", "schaun", "so", "fremd", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tiefschauernd f\u00fchlt er, 's war ein Traum \u2013", "tokens": ["Tief\u00b7schau\u00b7ernd", "f\u00fchlt", "er", ",", "'s", "war", "ein", "Traum", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wacht im Himmel auf.", "tokens": ["Und", "wacht", "im", "Him\u00b7mel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}