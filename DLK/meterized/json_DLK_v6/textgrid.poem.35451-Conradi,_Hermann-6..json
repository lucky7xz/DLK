{"textgrid.poem.35451": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es spiegelt sich das Abendrot", "tokens": ["Es", "spie\u00b7gelt", "sich", "das", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Goldgelben in den Regenpf\u00fctzen ...", "tokens": ["Gold\u00b7gel\u00b7ben", "in", "den", "Re\u00b7gen\u00b7pf\u00fct\u00b7zen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und schmiegt sich an die Scheiben dicht,", "tokens": ["Und", "schmiegt", "sich", "an", "die", "Schei\u00b7ben", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie wie rote Feuer blitzen ...", "tokens": ["Da\u00df", "sie", "wie", "ro\u00b7te", "Feu\u00b7er", "blit\u00b7zen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geregnet hat's den ganzen Tag,", "tokens": ["Ge\u00b7reg\u00b7net", "hat's", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun hellt sich's noch, bevor es nachtet ...", "tokens": ["Nun", "hellt", "sich's", "noch", ",", "be\u00b7vor", "es", "nach\u00b7tet", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.4": {"text": "Hast du dein ganzes Leben lang", "tokens": ["Hast", "du", "dein", "gan\u00b7zes", "Le\u00b7ben", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Leben bodenlos verachtet:", "tokens": ["Das", "Le\u00b7ben", "bo\u00b7den\u00b7los", "ver\u00b7ach\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zur Stunde, wenn's zum Sterben geht,", "tokens": ["Zur", "Stun\u00b7de", ",", "wenn's", "zum", "Ster\u00b7ben", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sich die Nacht noch einmal \u2013 kl\u00e4ren,", "tokens": ["Wird", "sich", "die", "Nacht", "noch", "ein\u00b7mal", "\u2013", "kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADV", "ADV", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wert, da\u00df du sie krampfhaft h\u00e4ltst,", "tokens": ["Und", "wert", ",", "da\u00df", "du", "sie", "krampf\u00b7haft", "h\u00e4ltst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wirst du sie finden, die \u2013 ", "tokens": ["Wirst", "du", "sie", "fin\u00b7den", ",", "die", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$,", "PRELS", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}