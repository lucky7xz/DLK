{"textgrid.poem.30544": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schick mir au\u00df dem Pferde-Brunnen, F\u00fcrst Apollo, eine Flasche,", "tokens": ["Schick", "mir", "au\u00df", "dem", "Pfer\u00b7de\u00b7Brun\u00b7nen", ",", "F\u00fcrst", "A\u00b7pol\u00b7lo", ",", "ei\u00b7ne", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df ich mir zu guten Reimen meine Sinnen t\u00fcchtig wasche;", "tokens": ["Da\u00df", "ich", "mir", "zu", "gu\u00b7ten", "Rei\u00b7men", "mei\u00b7ne", "Sin\u00b7nen", "t\u00fcch\u00b7tig", "wa\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dann ich kan nicht reisig kummen auff dem blancken Tichter-Pferde;", "tokens": ["Dann", "ich", "kan", "nicht", "rei\u00b7sig", "kum\u00b7men", "auff", "dem", "blan\u00b7cken", "Tich\u00b7ter\u00b7P\u00b7fer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gicht die hat mich au\u00dfgestiefelt, da\u00df ich ietzo Sporn-lo\u00df werde.", "tokens": ["Gicht", "die", "hat", "mich", "au\u00df\u00b7ge\u00b7stie\u00b7felt", ",", "da\u00df", "ich", "iet\u00b7zo", "Sporn\u00b7lo\u00df", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}