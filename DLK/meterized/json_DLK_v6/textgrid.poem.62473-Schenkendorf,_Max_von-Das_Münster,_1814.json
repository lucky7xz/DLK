{"textgrid.poem.62473": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00fcnster, 1814", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Stra\u00dfburg steht ein hoher Thurm,", "tokens": ["In", "Stra\u00df\u00b7burg", "steht", "ein", "ho\u00b7her", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der steht viel hundert Jahr',", "tokens": ["Der", "steht", "viel", "hun\u00b7dert", "Jahr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es weht um ihn so mancher Sturm,", "tokens": ["Es", "weht", "um", "ihn", "so", "man\u00b7cher", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er bleibet fest und klar.", "tokens": ["Er", "blei\u00b7bet", "fest", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So war auch wol die fromme Welt,", "tokens": ["So", "war", "auch", "wol", "die", "from\u00b7me", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die solches Werk gedacht,", "tokens": ["Die", "sol\u00b7ches", "Werk", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu dem sie von dem Sternenzelt", "tokens": ["Zu", "dem", "sie", "von", "dem", "Ster\u00b7nen\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Abri\u00df hergebracht.", "tokens": ["Den", "Ab\u00b7ri\u00df", "her\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie sich, ein ew'ges Heldenmal,", "tokens": ["Wie", "sich", ",", "ein", "ew'\u00b7ges", "Hel\u00b7den\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gotteshaus erhebt,", "tokens": ["Das", "Got\u00b7tes\u00b7haus", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus dem ein heller, schlanker Strahl,", "tokens": ["Aus", "dem", "ein", "hel\u00b7ler", ",", "schlan\u00b7ker", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Thurm gen Himmel strebt;", "tokens": ["Der", "Thurm", "gen", "Him\u00b7mel", "strebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So war auch einst das deutsche Reich,", "tokens": ["So", "war", "auch", "einst", "das", "deut\u00b7sche", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So war der deutsche Mann,", "tokens": ["So", "war", "der", "deut\u00b7sche", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf starkem Grund, im Herzen reich,", "tokens": ["Auf", "star\u00b7kem", "Grund", ",", "im", "Her\u00b7zen", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Haupt zu Gott hinan.", "tokens": ["Das", "Haupt", "zu", "Gott", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie den festen Bau umgibt", "tokens": ["Und", "wie", "den", "fes\u00b7ten", "Bau", "um\u00b7gibt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6ne Heil'genwelt,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Heil'\u00b7gen\u00b7welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So hatte Jeder, was er liebt',", "tokens": ["So", "hat\u00b7te", "Je\u00b7der", ",", "was", "er", "liebt'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihren Schutz gestellt.", "tokens": ["In", "ih\u00b7ren", "Schutz", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir wollen vor dem Altar noch", "tokens": ["Wir", "wol\u00b7len", "vor", "dem", "Al\u00b7tar", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein fromm Gel\u00fcbde thun,", "tokens": ["Ein", "fromm", "Ge\u00b7l\u00fcb\u00b7de", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df nimmermehr soll fremdes Joch", "tokens": ["Da\u00df", "nim\u00b7mer\u00b7mehr", "soll", "frem\u00b7des", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deutschem Nacken ruhn.", "tokens": ["Auf", "deut\u00b7schem", "Na\u00b7cken", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir sprechen dort ein hohes Wort,", "tokens": ["Wir", "spre\u00b7chen", "dort", "ein", "ho\u00b7hes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein br\u00fcnstiges Gebet,", "tokens": ["Ein", "br\u00fcns\u00b7ti\u00b7ges", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Da\u00df Gott, der Deutschen starker Hort,", "tokens": ["Da\u00df", "Gott", ",", "der", "Deut\u00b7schen", "star\u00b7ker", "Hort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbleibe stet und stet;", "tokens": ["Ver\u00b7blei\u00b7be", "stet", "und", "stet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da\u00df, wie der Thurm, der deutsche Sinn", "tokens": ["Da\u00df", ",", "wie", "der", "Thurm", ",", "der", "deut\u00b7sche", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entwachse seiner Zeit,", "tokens": ["Ent\u00b7wach\u00b7se", "sei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nach dem Himmel strebe hin,", "tokens": ["Und", "nach", "dem", "Him\u00b7mel", "stre\u00b7be", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihn die Welt bedr\u00e4ut.", "tokens": ["Wenn", "ihn", "die", "Welt", "be\u00b7dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und ob wir wieder heimw\u00e4rts gehn,", "tokens": ["Und", "ob", "wir", "wie\u00b7der", "heim\u00b7w\u00e4rts", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wenden unsern Blick,", "tokens": ["Wir", "wen\u00b7den", "un\u00b7sern", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schauen nach des Vasgaus H\u00f6h'n,", "tokens": ["Und", "schau\u00b7en", "nach", "des", "Vas\u00b7gaus", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie nach dem Thurm zur\u00fcck.", "tokens": ["Wie", "nach", "dem", "Thurm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Die Bundesfahn' in Feindes Hand?", "tokens": ["Die", "Bun\u00b7des\u00b7fahn'", "in", "Fein\u00b7des", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Thurm in welscher Macht?", "tokens": ["Der", "Thurm", "in", "wel\u00b7scher", "Macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O nein, sie sind vorausgesandt", "tokens": ["O", "nein", ",", "sie", "sind", "vor\u00b7aus\u00b7ge\u00b7sandt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKANT", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als k\u00fchne Vorderwacht.", "tokens": ["Als", "k\u00fch\u00b7ne", "Vor\u00b7der\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wir retten euch, wir haben's Eil,", "tokens": ["Wir", "ret\u00b7ten", "euch", ",", "wir", "ha\u00b7ben's", "Eil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verga\u00df euch doch kein Herz,", "tokens": ["Ver\u00b7ga\u00df", "euch", "doch", "kein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Wolkens\u00e4ul', o Feuers\u00e4ul',", "tokens": ["O", "Wol\u00b7ken\u00b7s\u00e4ul'", ",", "o", "Feu\u00b7er\u00b7s\u00e4ul'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schaut immer heimatw\u00e4rts.", "tokens": ["Schaut", "im\u00b7mer", "hei\u00b7mat\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}