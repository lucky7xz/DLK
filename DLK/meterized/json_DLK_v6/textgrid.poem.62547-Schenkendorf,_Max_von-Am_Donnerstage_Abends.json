{"textgrid.poem.62547": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Donnerstage Abends", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weich, ihr d\u00fcstern Sorgen, weicht!", "tokens": ["Weich", ",", "ihr", "d\u00fcs\u00b7tern", "Sor\u00b7gen", ",", "weicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihr schwarzen Nachtgebilde!", "tokens": ["Und", "ihr", "schwar\u00b7zen", "Nacht\u00b7ge\u00b7bil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich entschlumm're sanft und leicht", "tokens": ["Ich", "ent\u00b7schlum\u00b7m'\u00b7re", "sanft", "und", "leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unter meines Gottes Schilde.", "tokens": ["Un\u00b7ter", "mei\u00b7nes", "Got\u00b7tes", "Schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend Cherubim bewachen", "tokens": ["Tau\u00b7send", "Che\u00b7ru\u00b7bim", "be\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jedes Haar auf meinem Haupt.", "tokens": ["Je\u00b7des", "Haar", "auf", "mei\u00b7nem", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er wird's herrlich mit mir machen,", "tokens": ["Er", "wird's", "herr\u00b7lich", "mit", "mir", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Wie er sprach und ich geglaubt.", "tokens": ["Wie", "er", "sprach", "und", "ich", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}