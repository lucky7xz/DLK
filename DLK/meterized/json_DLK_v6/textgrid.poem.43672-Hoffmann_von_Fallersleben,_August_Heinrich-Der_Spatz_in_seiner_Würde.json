{"textgrid.poem.43672": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Spatz in seiner W\u00fcrde", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Spatz von Gottes Gnaden,", "tokens": ["Ich", "bin", "ein", "Spatz", "von", "Got\u00b7tes", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab' ein Recht wie ihr zu sein.", "tokens": ["Ich", "hab'", "ein", "Recht", "wie", "ihr", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOKOM", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr meint, ich k\u00f6nnte nur euch schaden,", "tokens": ["Ihr", "meint", ",", "ich", "k\u00f6nn\u00b7te", "nur", "euch", "scha\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das aber f\u00e4llt mir gar nicht ein.", "tokens": ["Das", "a\u00b7ber", "f\u00e4llt", "mir", "gar", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welt ist gro\u00df und ich bin klein,", "tokens": ["Die", "Welt", "ist", "gro\u00df", "und", "ich", "bin", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch freut es mich, ein Spatz zu sein,", "tokens": ["Doch", "freut", "es", "mich", ",", "ein", "Spatz", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Spi-Spa-Spatz zu sein,", "tokens": ["Spi\u00b7Spa\u00b7Spatz", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ein Spatz zu sein!", "tokens": ["Ein", "Spatz", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr wi\u00dft recht gut, wie ich euch n\u00fctze", "tokens": ["Ihr", "wi\u00dft", "recht", "gut", ",", "wie", "ich", "euch", "n\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und euren B\u00e4umen bin zum Heil;", "tokens": ["Und", "eu\u00b7ren", "B\u00e4u\u00b7men", "bin", "zum", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Knospen ich und Bl\u00fcthen sch\u00fctze,", "tokens": ["Wenn", "Knos\u00b7pen", "ich", "und", "Bl\u00fc\u00b7then", "sch\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geb\u00fchrt von Fr\u00fcchten mir mein Theil.", "tokens": ["Ge\u00b7b\u00fchrt", "von", "Fr\u00fcch\u00b7ten", "mir", "mein", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Macht ihr's denn anders, lieben Kinder?", "tokens": ["Macht", "ih\u00b7r's", "denn", "an\u00b7ders", ",", "lie\u00b7ben", "Kin\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ich seh' euch naschen nah' und fern,", "tokens": ["Ich", "seh'", "euch", "na\u00b7schen", "nah'", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Kirschen lieb' ich auch nicht minder", "tokens": ["Und", "Kir\u00b7schen", "lieb'", "ich", "auch", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und junge Erbsen ess' ich gern.", "tokens": ["Und", "jun\u00b7ge", "Erb\u00b7sen", "ess'", "ich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "G\u00f6nnt mir die Trauben mal zu kosten,", "tokens": ["G\u00f6nnt", "mir", "die", "Trau\u00b7ben", "mal", "zu", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft euer Klappern, L\u00e4rmen, Schrei'n!", "tokens": ["La\u00dft", "eu\u00b7er", "Klap\u00b7pern", ",", "L\u00e4r\u00b7men", ",", "Schrei'n", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr findet noch genug zum Mosten,", "tokens": ["Ihr", "fin\u00b7det", "noch", "ge\u00b7nug", "zum", "Mos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und trinket Wein Jahr aus Jahr ein.", "tokens": ["Und", "trin\u00b7ket", "Wein", "Jahr", "aus", "Jahr", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr braucht nicht erst mich einzuladen,", "tokens": ["Ihr", "braucht", "nicht", "erst", "mich", "ein\u00b7zu\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich finde mich von selbst schon ein:", "tokens": ["Ich", "fin\u00b7de", "mich", "von", "selbst", "schon", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein Spatz von Gottes Gnaden,", "tokens": ["Ich", "bin", "ein", "Spatz", "von", "Got\u00b7tes", "Gna\u00b7den", ","], "token_in