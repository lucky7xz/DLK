{"textgrid.poem.47886": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stille! was h\u00f6r ich? Stein-Waldbeseelende Lieder;", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille! was h\u00f6r ich? Stein-Waldbeseelende Lieder;", "tokens": ["Stil\u00b7le", "!", "was", "h\u00f6r", "ich", "?", "Stein\u00b7Wald\u00b7be\u00b7seelen\u00b7de", "Lie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "$.", "NN", "NN", "$."], "meter": "+--+-++-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "K\u00f6mt auch wol Orfeus von dem Elyser Feld wieder?", "tokens": ["K\u00f6mt", "auch", "wol", "Or\u00b7feus", "von", "dem", "E\u00b7ly\u00b7ser", "Feld", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NE", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Wie da\u00df er nicht auf Hemus singt?", "tokens": ["Wie", "da\u00df", "er", "nicht", "auf", "He\u00b7mus", "singt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PTKNEG", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie da\u00df er Teutsche Reimen zwingt/", "tokens": ["Wie", "da\u00df", "er", "Teut\u00b7sche", "Rei\u00b7men", "zwingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sprach und Lands vergessen/", "tokens": ["Der", "Sprach", "und", "Lands", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ihn geseugt vordessen?", "tokens": ["Die", "ihn", "ge\u00b7seugt", "vor\u00b7des\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weil da\u00df in ihm der Bunzlerschwan etwa gereist/", "tokens": ["Weil", "da\u00df", "in", "ihm", "der", "Bunz\u00b7ler\u00b7schwan", "et\u00b7wa", "ge\u00b7reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PPER", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den G\u00f6tterkost/ vermenschet hat lang schon gespeist?", "tokens": ["Den", "G\u00f6t\u00b7ter\u00b7kost", "/", "ver\u00b7men\u00b7schet", "hat", "lang", "schon", "ge\u00b7speist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVPP", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Tolle Menaden schikken den Thrazer zu Schatten/", "tokens": ["Tol\u00b7le", "Me\u00b7na\u00b7den", "schik\u00b7ken", "den", "Thra\u00b7zer", "zu", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Opitzens Geist wird nimmer nicht Teutsches begatten.", "tokens": ["O\u00b7pit\u00b7zens", "Geist", "wird", "nim\u00b7mer", "nicht", "Teut\u00b7sches", "be\u00b7gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wer ist dann der so zierlich singt?", "tokens": ["Wer", "ist", "dann", "der", "so", "zier\u00b7lich", "singt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wer ist/ der solche S\u00e4tze bringt?", "tokens": ["Wer", "ist", "/", "der", "sol\u00b7che", "S\u00e4t\u00b7ze", "bringt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wer bannet von dem Hertzen", "tokens": ["Wer", "ban\u00b7net", "von", "dem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Die Mutes-fresser Schmertzen.", "tokens": ["Die", "Mu\u00b7tes\u00b7fres\u00b7ser", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ein Meisner Schwan vom Meisnischen H\u00fcgel sich trug/", "tokens": ["Ein", "Meis\u00b7ner", "Schwan", "vom", "Meis\u00b7ni\u00b7schen", "H\u00fc\u00b7gel", "sich", "trug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "die Pegnitz hemmt sein Wolkenanseglenden Flug/", "tokens": ["die", "Peg\u00b7nitz", "hemmt", "sein", "Wol\u00b7ken\u00b7an\u00b7seg\u00b7len\u00b7den", "Flug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Rhodopeus Lufft- und Felsenruff m\u00fcrmelt ihm strenger;", "tokens": ["Rho\u00b7do\u00b7peus", "Lufft", "und", "Fel\u00b7sen\u00b7ruff", "m\u00fcr\u00b7melt", "ihm", "stren\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "VMFIN", "PPER", "ADJD", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Proteus \u00e4hmt und \u00e4hnlet der Pindische S\u00e4nger:", "tokens": ["Pro\u00b7teus", "\u00e4hmt", "und", "\u00e4hn\u00b7let", "der", "Pin\u00b7di\u00b7sche", "S\u00e4n\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.19": {"text": "Der bald in Sch\u00e4ferkleidern singt/", "tokens": ["Der", "bald", "in", "Sch\u00e4\u00b7fer\u00b7klei\u00b7dern", "singt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der bald auf sieben R\u00f6hren klingt/", "tokens": ["Der", "bald", "auf", "sie\u00b7ben", "R\u00f6h\u00b7ren", "klingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bald Celtische Witdoden", "tokens": ["Bald", "Cel\u00b7ti\u00b7sche", "Wit\u00b7do\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Selbst Witdod/ rufft von Toden.", "tokens": ["Selbst", "Wit\u00b7dod", "/", "rufft", "von", "To\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Vnd bald/ ein neuer Gottesbot mahlet sch\u00f6n aus", "tokens": ["Vnd", "bald", "/", "ein", "neu\u00b7er", "Got\u00b7tes\u00b7bot", "mah\u00b7let", "sch\u00f6n", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Der G\u00f6tter Wiegen/ Wolkenrei\u00df/ Stygierstrau\u00df.", "tokens": ["Der", "G\u00f6t\u00b7ter", "Wie\u00b7gen", "/", "Wol\u00b7ken\u00b7rei\u00df", "/", "Sty\u00b7gier\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "NN", "$(", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "L\u00e4sst er nicht jetzt ein Teutscher Eiferer blitzen", "tokens": ["L\u00e4sst", "er", "nicht", "jetzt", "ein", "Teut\u00b7scher", "Ei\u00b7fe\u00b7rer", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Teutischen Donner/ auf die Tyrannen lo\u00dfspr\u00fctzen?", "tokens": ["Teu\u00b7ti\u00b7schen", "Don\u00b7ner", "/", "auf", "die", "Ty\u00b7ran\u00b7nen", "lo\u00df\u00b7spr\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "Fort Persi/ Orfeus/ immer singt/", "tokens": ["Fort", "Per\u00b7si", "/", "Or\u00b7feus", "/", "im\u00b7mer", "singt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "NE", "$(", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Fort Pan und Witdod/ immer klingt/", "tokens": ["Fort", "Pan", "und", "Wit\u00b7dod", "/", "im\u00b7mer", "klingt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$(", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die Elbe wird euch preisen/", "tokens": ["Die", "El\u00b7be", "wird", "euch", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Die Pegnitz wilkomm heissen.", "tokens": ["Die", "Peg\u00b7nitz", "wil\u00b7komm", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Hier diese Brut/ die keine der Schwestern ge\u00e4tzt/", "tokens": ["Hier", "die\u00b7se", "Brut", "/", "die", "kei\u00b7ne", "der", "Schwes\u00b7tern", "ge\u00b7\u00e4tzt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$(", "ART", "PIAT", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Zu euerm Ruhm \u2013\u2013", "tokens": ["Zu", "eu\u00b7erm", "Ruhm", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "der Strebende Floridan setzt.", "tokens": ["der", "Stre\u00b7ben\u00b7de", "Flo\u00b7ri\u00b7dan", "setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "met