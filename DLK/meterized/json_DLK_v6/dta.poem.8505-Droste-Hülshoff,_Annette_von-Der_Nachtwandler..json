{"dta.poem.8505": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtwandler.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst du das Haus an dem Geh&#228;ge nicht?", "tokens": ["Siehst", "du", "das", "Haus", "an", "dem", "Geh", "&#228;", "ge", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NE", "$(", "NE", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die D\u00e4mmrung sinkt, la\u00dft uns vor\u00fcbereilen,", "tokens": ["Die", "D\u00e4mm\u00b7rung", "sinkt", ",", "la\u00dft", "uns", "vor\u00b7\u00fc\u00b7be\u00b7rei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald hebt der Vollmond sein gespenstig Licht,", "tokens": ["Bald", "hebt", "der", "Voll\u00b7mond", "sein", "ge\u00b7spens\u00b7tig", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann ist nicht gut in jener N\u00e4he weilen;", "tokens": ["Dann", "ist", "nicht", "gut", "in", "je\u00b7ner", "N\u00e4\u00b7he", "wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier schwebt kein Spuck den Buchengang hinauf,", "tokens": ["Hier", "schwebt", "kein", "Spuck", "den", "Bu\u00b7chen\u00b7gang", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein R\u00e4uber pa\u00dft im finstern Schuppen auf,", "tokens": ["Kein", "R\u00e4u\u00b7ber", "pa\u00dft", "im", "fins\u00b7tern", "Schup\u00b7pen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein B\u00fcrgerhaus, ein b\u00fcrgerlich Beginnen,", "tokens": ["Ein", "B\u00fcr\u00b7ger\u00b7haus", ",", "ein", "b\u00fcr\u00b7ger\u00b7lich", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es wohnt ein Kr\u00e4mer, wohnen Diener drinnen.", "tokens": ["Es", "wohnt", "ein", "Kr\u00e4\u00b7mer", ",", "woh\u00b7nen", "Die\u00b7ner", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Alt ist der Herr, wie alt, man wei\u00df es kaum,", "tokens": ["Alt", "ist", "der", "Herr", ",", "wie", "alt", ",", "man", "wei\u00df", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PWAV", "ADJD", "$,", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er liebt es nicht, im Kirchenbuch zu lesen;", "tokens": ["Er", "liebt", "es", "nicht", ",", "im", "Kir\u00b7chen\u00b7buch", "zu", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm lebt\u2019 ein Weib vor vieler Jahre Raum,", "tokens": ["Ihm", "lebt'", "ein", "Weib", "vor", "vie\u00b7ler", "Jah\u00b7re", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er hatt\u2019 ein Kind, das ist nun lang gewesen;", "tokens": ["Er", "hatt'", "ein", "Kind", ",", "das", "ist", "nun", "lang", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man sagt, er habe ihr den Arzt versagt,", "tokens": ["Man", "sagt", ",", "er", "ha\u00b7be", "ihr", "den", "Arzt", "ver\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit schlechter Kost zu Tod das Kind geplagt;", "tokens": ["Mit", "schlech\u00b7ter", "Kost", "zu", "Tod", "das", "Kind", "ge\u00b7plagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was sagt man nicht, um Leute zu verdammen,", "tokens": ["Was", "sagt", "man", "nicht", ",", "um", "Leu\u00b7te", "zu", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "PTKNEG", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo sich das Gold in Haufen rollt zusammen.", "tokens": ["Wo", "sich", "das", "Gold", "in", "Hau\u00b7fen", "rollt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einst war er arm, hat k\u00fcmmerlich gezehrt,", "tokens": ["Einst", "war", "er", "arm", ",", "hat", "k\u00fcm\u00b7mer\u00b7lich", "ge\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohl k\u00fcmmerlicher noch als Andre eben;", "tokens": ["Wohl", "k\u00fcm\u00b7mer\u00b7li\u00b7cher", "noch", "als", "And\u00b7re", "e\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KOUS", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da, hei\u00dft es, hab\u2019 um eines Thalers Werth", "tokens": ["Da", ",", "hei\u00dft", "es", ",", "hab'", "um", "ei\u00b7nes", "Tha\u00b7lers", "Werth"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er einen Dieb dem Galgen \u00fcbergeben.", "tokens": ["Er", "ei\u00b7nen", "Dieb", "dem", "Gal\u00b7gen", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jung sei der Dieb gewesen, hungerbleich,", "tokens": ["Jung", "sei", "der", "Dieb", "ge\u00b7we\u00b7sen", ",", "hun\u00b7ger\u00b7bleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VAPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und seine Mutter krank, man glaubt es gleich;", "tokens": ["Und", "sei\u00b7ne", "Mut\u00b7ter", "krank", ",", "man", "glaubt", "es", "gleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$,", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dies folgt dem Reichen; sieh die H\u00fctten dr\u00fcben!", "tokens": ["Dies", "folgt", "dem", "Rei\u00b7chen", ";", "sieh", "die", "H\u00fct\u00b7ten", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "VVIMP", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dort wohnt die Noth, sein ist ihr Gut geblieben.", "tokens": ["Dort", "wohnt", "die", "Noth", ",", "sein", "ist", "ihr", "Gut", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VAINF", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Man kann ihn flei\u00dfig in der Kirche sehn,", "tokens": ["Man", "kann", "ihn", "flei\u00b7\u00dfig", "in", "der", "Kir\u00b7che", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Sitten d\u00fcrfte Keiner r\u00fcgen;", "tokens": ["Und", "sei\u00b7ne", "Sit\u00b7ten", "d\u00fcrf\u00b7te", "Kei\u00b7ner", "r\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch seit des K\u00f6rpers Kr\u00e4fte ihm vergehn,", "tokens": ["Doch", "seit", "des", "K\u00f6r\u00b7pers", "Kr\u00e4f\u00b7te", "ihm", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df einem schweren Siechthum er erliegen;", "tokens": ["Mu\u00df", "ei\u00b7nem", "schwe\u00b7ren", "Siech\u00b7thum", "er", "er\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So oft der Vollmond senkt den blassen Schein,", "tokens": ["So", "oft", "der", "Voll\u00b7mond", "senkt", "den", "blas\u00b7sen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "H\u00fcllt er sich schaudernd in das Lailach ein,", "tokens": ["H\u00fcllt", "er", "sich", "schau\u00b7dernd", "in", "das", "Lai\u00b7lach", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und k\u00f6mmt vom Bett, das Kerzenst\u00fcmpflein tragend,", "tokens": ["Und", "k\u00f6mmt", "vom", "Bett", ",", "das", "Ker\u00b7zen\u00b7st\u00fcm\u00b7pflein", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Diener folgt ihm ganz von fern und zagend.", "tokens": ["Ein", "Die\u00b7ner", "folgt", "ihm", "ganz", "von", "fern", "und", "za\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Durch jene H\u00fcttenfenster sieht man dann", "tokens": ["Durch", "je\u00b7ne", "H\u00fct\u00b7ten\u00b7fens\u00b7ter", "sieht", "man", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am langen Tisch ihn emsig wieder z\u00e4hlen,", "tokens": ["Am", "lan\u00b7gen", "Tisch", "ihn", "em\u00b7sig", "wie\u00b7der", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Golde schaben, und mit raschem Spann", "tokens": ["Am", "Gol\u00b7de", "scha\u00b7ben", ",", "und", "mit", "ra\u00b7schem", "Spann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihn pl\u00f6tzlich greifen, wie nach Diebeskehlen;", "tokens": ["Ihn", "pl\u00f6tz\u00b7lich", "grei\u00b7fen", ",", "wie", "nach", "Die\u00b7bes\u00b7keh\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,", "PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schon ist auch wohl ein Schrei hinausgeschallt,", "tokens": ["Schon", "ist", "auch", "wohl", "ein", "Schrei", "hin\u00b7aus\u00b7ge\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als thue einer Seele man Gewalt,", "tokens": ["Als", "thue", "ei\u00b7ner", "See\u00b7le", "man", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "PIS", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bis ihm die Arme sinken wie verwittert,", "tokens": ["Bis", "ihm", "die", "Ar\u00b7me", "sin\u00b7ken", "wie", "ver\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und weiter er mit seinem St\u00fcmpfchen zittert.", "tokens": ["Und", "wei\u00b7ter", "er", "mit", "sei\u00b7nem", "St\u00fcmpf\u00b7chen", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sein n\u00e4chster Gang ist in die Kammer, wo", "tokens": ["Sein", "n\u00e4chs\u00b7ter", "Gang", "ist", "in", "die", "Kam\u00b7mer", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei einem gr\u00f6\u00dfern Lager steht ein kleines;", "tokens": ["Bei", "ei\u00b7nem", "gr\u00f6\u00b7\u00dfern", "La\u00b7ger", "steht", "ein", "klei\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort kramet er am Bettchen so und so,", "tokens": ["Dort", "kra\u00b7met", "er", "am", "Bett\u00b7chen", "so", "und", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als \u00f6ffn\u2019 er eine Flasche edlen Weines,", "tokens": ["Als", "\u00f6ffn'", "er", "ei\u00b7ne", "Fla\u00b7sche", "ed\u00b7len", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und gie\u00dft dann, gie\u00dft, als sei es nie genug,", "tokens": ["Und", "gie\u00dft", "dann", ",", "gie\u00dft", ",", "als", "sei", "es", "nie", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und stopft und legt wie Bissen an das Tuch,", "tokens": ["Und", "stopft", "und", "legt", "wie", "Bis\u00b7sen", "an", "das", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann sto\u00dfend scheint er an den Puls zu greifen,", "tokens": ["Dann", "sto\u00b7\u00dfend", "scheint", "er", "an", "den", "Puls", "zu", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geb\u00fcckt, als lauschend schwachen Odems Pfeifen.", "tokens": ["Ge\u00b7b\u00fcckt", ",", "als", "lau\u00b7schend", "schwa\u00b7chen", "O\u00b7dems", "Pfei\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schleicht dann zu jenes Lagers grobem Flaus,", "tokens": ["Schleicht", "dann", "zu", "je\u00b7nes", "La\u00b7gers", "gro\u00b7bem", "Flaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Scheint tr\u00f6pfelnd \u00fcber Arzenei\u2019n zu b\u00fccken;", "tokens": ["Scheint", "tr\u00f6p\u00b7felnd", "\u00fc\u00b7ber", "Ar\u00b7ze\u00b7nei'n", "zu", "b\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er breitet schweigend eine Decke aus,", "tokens": ["Er", "brei\u00b7tet", "schwei\u00b7gend", "ei\u00b7ne", "De\u00b7cke", "aus", ","], "toke