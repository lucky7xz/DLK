", "nicht", "zu\u00b7r\u00fcck", "!", "Ver\u00b7weg\u00b7ner", ",", "fluch'", "ihm", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$.", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "La\u00df ihn allein mit Gott und dem Gericht!", "tokens": ["La\u00df", "ihn", "al\u00b7lein", "mit", "Gott", "und", "dem", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Meinst du, ein Fluch vergr\u00f6\u00dfre seine Leiden?", "tokens": ["Meinst", "du", ",", "ein", "Fluch", "ver\u00b7gr\u00f6\u00df\u00b7re", "sei\u00b7ne", "Lei\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Dieb am Galgen m\u00f6chte er beneiden!", "tokens": ["Den", "Dieb", "am", "Gal\u00b7gen", "m\u00f6ch\u00b7te", "er", "be\u00b7nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}