{"dta.poem.7467": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm nicht als Himmel an die Wolkenschichte,", "tokens": ["Nimm", "nicht", "als", "Him\u00b7mel", "an", "die", "Wol\u00b7ken\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erprobe selbst Dein jugendlich Gefieder,", "tokens": ["Er\u00b7pro\u00b7be", "selbst", "Dein", "ju\u00b7gend\u00b7lich", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirf mutig in die schwanken Schalen nieder", "tokens": ["Wirf", "mu\u00b7tig", "in", "die", "schwan\u00b7ken", "Scha\u00b7len", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Zweifels Deine eigenen Gewichte!", "tokens": ["Des", "Zwei\u00b7fels", "Dei\u00b7ne", "ei\u00b7ge\u00b7nen", "Ge\u00b7wich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erw\u00e4rm' den Geist am ", "tokens": ["Er\u00b7w\u00e4rm'", "den", "Geist", "am"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und forsche heut und forsche morgen wieder,", "tokens": ["Und", "for\u00b7sche", "heut", "und", "for\u00b7sche", "mor\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Senk' nie zufrieden Deine Augenlider,", "tokens": ["Senk'", "nie", "zu\u00b7frie\u00b7den", "Dei\u00b7ne", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ruf' Deinen Glauben t\u00e4glich zu Gerichte!", "tokens": ["Ruf'", "Dei\u00b7nen", "Glau\u00b7ben", "t\u00e4g\u00b7lich", "zu", "Ge\u00b7rich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch was Du immer wagest, o besch\u00f6nig's", "tokens": ["Doch", "was", "Du", "im\u00b7mer", "wa\u00b7gest", ",", "o", "be\u00b7sch\u00f6\u00b7nig's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "FM", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie vor den Menschen durch ein zaghaft Schweigen,", "tokens": ["Nie", "vor", "den", "Men\u00b7schen", "durch", "ein", "zag\u00b7haft", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Bekenn' es mit dem Freimut eines K\u00f6nigs!", "tokens": ["Be\u00b7kenn'", "es", "mit", "dem", "Frei\u00b7mut", "ei\u00b7nes", "K\u00f6\u00b7nigs", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob sie Dir flammend auch den Holzsto\u00df zeigen;", "tokens": ["Ob", "sie", "Dir", "flam\u00b7mend", "auch", "den", "Holz\u00b7sto\u00df", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Flammen tauft der Ewige den Ph\u00f6nix,", "tokens": ["Mit", "Flam\u00b7men", "tauft", "der", "E\u00b7wi\u00b7ge", "den", "Ph\u00f6\u00b7nix", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der stolz soll \u00fcber ihre Wasser steigen.", "tokens": ["Der", "stolz", "soll", "\u00fc\u00b7ber", "ih\u00b7re", "Was\u00b7ser", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}