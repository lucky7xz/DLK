{"textgrid.poem.59562": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den tiefen Ernst des Lebens zu verk\u00fcnden", "tokens": ["Den", "tie\u00b7fen", "Ernst", "des", "Le\u00b7bens", "zu", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Winkt, weist und spielt die Allmacht aus Geschichten.", "tokens": ["Winkt", ",", "weist", "und", "spielt", "die", "All\u00b7macht", "aus", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Vorwelt einzig darf die Nachwelt richten,", "tokens": ["Die", "Vor\u00b7welt", "ein\u00b7zig", "darf", "die", "Nach\u00b7welt", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gegenwart tappt taumelnd fort mit Blinden.", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "tappt", "tau\u00b7melnd", "fort", "mit", "Blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nie mag den Weg zum Sternenlande finden,", "tokens": ["Nie", "mag", "den", "Weg", "zum", "Ster\u00b7nen\u00b7lan\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wer nicht, wann Wolken sich f\u00fcr Donner dichten,", "tokens": ["Wer", "nicht", ",", "wann", "Wol\u00b7ken", "sich", "f\u00fcr", "Don\u00b7ner", "dich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "$,", "PWAV", "NN", "PRF", "APPR", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Blitzen wagt dahin den Flug zu richten,", "tokens": ["Auf", "Blit\u00b7zen", "wagt", "da\u00b7hin", "den", "Flug", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Tod und Leben ineinander schwinden.", "tokens": ["Wo", "Tod", "und", "Le\u00b7ben", "in\u00b7ein\u00b7an\u00b7der", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum strebe, Mut, zum alten G\u00f6tterh\u00fcgel,", "tokens": ["Drum", "stre\u00b7be", ",", "Mut", ",", "zum", "al\u00b7ten", "G\u00f6t\u00b7ter\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem strahlenden der Sonnen, welche gingen,", "tokens": ["Dem", "strah\u00b7len\u00b7den", "der", "Son\u00b7nen", ",", "wel\u00b7che", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem d\u00e4mmernden der Sonnen, welche kommen.", "tokens": ["Dem", "d\u00e4m\u00b7mern\u00b7den", "der", "Son\u00b7nen", ",", "wel\u00b7che", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort steht mein Bild im ungetr\u00fcbten Spiegel,", "tokens": ["Dort", "steht", "mein", "Bild", "im", "un\u00b7ge\u00b7tr\u00fcb\u00b7ten", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort tragen mich der Muse \u00c4therschwingen", "tokens": ["Dort", "tra\u00b7gen", "mich", "der", "Mu\u00b7se", "\u00c4\u00b7ther\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Empor ins Land der Tapfern und der Frommen.", "tokens": ["Em\u00b7por", "ins", "Land", "der", "Tap\u00b7fern", "und", "der", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}