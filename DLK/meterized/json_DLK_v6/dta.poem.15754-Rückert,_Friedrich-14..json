{"dta.poem.15754": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gefl\u00fcchtet ist die Kunst zur irdischen Geschichte,", "tokens": ["Ge\u00b7fl\u00fcch\u00b7tet", "ist", "die", "Kunst", "zur", "ir\u00b7di\u00b7schen", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihr das Ideal des Himmels ward zunichte.", "tokens": ["Weil", "ihr", "das", "I\u00b7deal", "des", "Him\u00b7mels", "ward", "zu\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Vorm Chaos der Geschicht' empf\u00e4nde sie ein Grauen,", "tokens": ["Vorm", "Chaos", "der", "Ge\u00b7schicht'", "em\u00b7pf\u00e4n\u00b7de", "sie", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn jenes Ideal sie k\u00f6nnte neu erbauen.", "tokens": ["Wenn", "je\u00b7nes", "I\u00b7deal", "sie", "k\u00f6nn\u00b7te", "neu", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}