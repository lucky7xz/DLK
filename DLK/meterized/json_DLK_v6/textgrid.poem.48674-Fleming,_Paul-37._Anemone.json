{"textgrid.poem.48674": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "37. Anemone", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auserw\u00e4hlte nach der einen,", "tokens": ["Au\u00b7ser\u00b7w\u00e4hl\u00b7te", "nach", "der", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die mir gut war auf den Schein,", "tokens": ["die", "mir", "gut", "war", "auf", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wilst du mich getreue meinen,", "tokens": ["wilst", "du", "mich", "ge\u00b7treu\u00b7e", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "so will ich auch deine sein.", "tokens": ["so", "will", "ich", "auch", "dei\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahre Liebe steht vergn\u00fcget,", "tokens": ["Wah\u00b7re", "Lie\u00b7be", "steht", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wenn sie ihres gleichen krieget.", "tokens": ["wenn", "sie", "ih\u00b7res", "glei\u00b7chen", "krie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Neue Gunst ist nicht ohn' Sorgen,", "tokens": ["Neu\u00b7e", "Gunst", "ist", "nicht", "ohn'", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "doch dein redlichs Herze macht,", "tokens": ["doch", "dein", "red\u00b7lichs", "Her\u00b7ze", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df mir nichts nicht ist verborgen,", "tokens": ["da\u00df", "mir", "nichts", "nicht", "ist", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was die Andern macht bedacht.", "tokens": ["was", "die", "An\u00b7dern", "macht", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andre m\u00f6gen anders denken,", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "an\u00b7ders", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "la\u00df uns uns einander schenken.", "tokens": ["la\u00df", "uns", "uns", "ein\u00b7an\u00b7der", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Neige deiner Liebe Feuer", "tokens": ["Nei\u00b7ge", "dei\u00b7ner", "Lie\u00b7be", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "auf mich, der ich deine bin.", "tokens": ["auf", "mich", ",", "der", "ich", "dei\u00b7ne", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "PPOSAT", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halt mich wert, wie ich dich teuer,", "tokens": ["Halt", "mich", "wert", ",", "wie", "ich", "dich", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "di\u00df ist steter Liebe Sinn.", "tokens": ["di\u00df", "ist", "ste\u00b7ter", "Lie\u00b7be", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sich regt in meinem Blute,", "tokens": ["Was", "sich", "regt", "in", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wei\u00df von keinem Wankelmute.", "tokens": ["wei\u00df", "von", "kei\u00b7nem", "Wan\u00b7kel\u00b7mu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "sei hiermit dir zugesagt.", "tokens": ["sei", "hier\u00b7mit", "dir", "zu\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tu stets, was ich mich stets freue,", "tokens": ["Tu", "stets", ",", "was", "ich", "mich", "stets", "freu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df mein Herze nicht mehr klagt.", "tokens": ["da\u00df", "mein", "Her\u00b7ze", "nicht", "mehr", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was an jener ist verloren,", "tokens": ["Was", "an", "je\u00b7ner", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "das ist mir an dir geboren.", "tokens": ["das", "ist", "mir", "an", "dir", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun, mein Herze, sei geschieden", "tokens": ["Nun", ",", "mein", "Her\u00b7ze", ",", "sei", "ge\u00b7schie\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "VVFIN", "$,", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und gieb jener gute Nacht.", "tokens": ["und", "gieb", "je\u00b7ner", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine stellet dich zufrieden,", "tokens": ["Ei\u00b7ne", "stel\u00b7let", "dich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die dich einig fr\u00f6lich macht", "tokens": ["die", "dich", "ei\u00b7nig", "fr\u00f6\u00b7lich", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die ists, die dich ewig meint.", "tokens": ["die", "ists", ",", "die", "dich", "e\u00b7wig", "meint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}