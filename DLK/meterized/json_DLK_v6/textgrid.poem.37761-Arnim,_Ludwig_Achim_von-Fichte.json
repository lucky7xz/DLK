{"textgrid.poem.37761": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Fichte", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch dich hat uns die Pest der Zeit entrissen,", "tokens": ["Auch", "dich", "hat", "uns", "die", "Pest", "der", "Zeit", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich muthigsten Bestreiter schlechter Zeit;", "tokens": ["Dich", "mut\u00b7higs\u00b7ten", "Be\u00b7strei\u00b7ter", "schlech\u00b7ter", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Du hattest dich als Opfer ihr geweiht,", "tokens": ["Du", "hat\u00b7test", "dich", "als", "Op\u00b7fer", "ihr", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als du ihr strafend riefest in's Gewissen.", "tokens": ["Als", "du", "ihr", "stra\u00b7fend", "rie\u00b7fest", "in's", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es war die Welt von Zweifeln lang zerrissen,", "tokens": ["Es", "war", "die", "Welt", "von", "Zwei\u00b7feln", "lang", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sahst den Abgrund, wie er tief und weit,", "tokens": ["Du", "sahst", "den", "Ab\u00b7grund", ",", "wie", "er", "tief", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wie der R\u00f6mer warst du k\u00fchn bereit", "tokens": ["Doch", "wie", "der", "R\u00f6\u00b7mer", "warst", "du", "k\u00fchn", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihn zu verschlie\u00dfen nach dem besten Wissen;", "tokens": ["Ihn", "zu", "ver\u00b7schlie\u00b7\u00dfen", "nach", "dem", "bes\u00b7ten", "Wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du warfest ", "tokens": ["Du", "war\u00b7fest"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Du sprachst zu Deutschen, als die Andern schwiegen,", "tokens": ["Du", "sprachst", "zu", "Deut\u00b7schen", ",", "als", "die", "An\u00b7dern", "schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du riefst uns aus der Schmach zu neuen Siegen.", "tokens": ["Du", "riefst", "uns", "aus", "der", "Schmach", "zu", "neu\u00b7en", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbbek\u00e4mpft die Zeit in ", "tokens": ["\u00bb", "be\u00b7k\u00e4mpft", "die", "Zeit", "in"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So riefest du. \u2013 Den Bogen spannt im Stillen", "tokens": ["So", "rie\u00b7fest", "du", ".", "\u2013", "Den", "Bo\u00b7gen", "spannt", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die t\u00fcck'sche Zeit, \u2013 auch du mu\u00dft ihr erliegen.", "tokens": ["Die", "t\u00fc\u00b7ck'\u00b7sche", "Zeit", ",", "\u2013", "auch", "du", "mu\u00dft", "ihr", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "ADV", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}