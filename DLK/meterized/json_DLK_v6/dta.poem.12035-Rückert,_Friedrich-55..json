{"dta.poem.12035": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast ein Ma\u00df in dir von Kr\u00e4ften, die du spenden", "tokens": ["Du", "hast", "ein", "Ma\u00df", "in", "dir", "von", "Kr\u00e4f\u00b7ten", ",", "die", "du", "spen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Welt kannst, ohne sie ins Innre zu verwenden.", "tokens": ["Der", "Welt", "kannst", ",", "oh\u00b7ne", "sie", "ins", "Inn\u00b7re", "zu", "ver\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "KOUI", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum sei ein Kreis um dich, ein gr\u00f6\u00dfrer oder kleiner;", "tokens": ["Drum", "sei", "ein", "Kreis", "um", "dich", ",", "ein", "gr\u00f6\u00df\u00b7rer", "o\u00b7der", "klei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "ART", "ADJA", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht viele m\u00fc\u00dfen's seyn, nur einige, nur einer.", "tokens": ["Nicht", "vie\u00b7le", "m\u00fc\u00b7\u00dfen's", "seyn", ",", "nur", "ei\u00b7ni\u00b7ge", ",", "nur", "ei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VMFIN", "VAINF", "$,", "ADV", "PIS", "$,", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Je wenigern du gibst, je mehr nun gibst du ihnen,", "tokens": ["Je", "we\u00b7ni\u00b7gern", "du", "gibst", ",", "je", "mehr", "nun", "gibst", "du", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dann es mehreren zu geben weiter dienen.", "tokens": ["Die", "dann", "es", "meh\u00b7re\u00b7ren", "zu", "ge\u00b7ben", "wei\u00b7ter", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVINF", "PTKZU", "VVINF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}