{"textgrid.poem.25876": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[ich liege still im dunkeln Krankenbett]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liege still im dunkeln Krankenbett,", "tokens": ["Ich", "lie\u00b7ge", "still", "im", "dun\u00b7keln", "Kran\u00b7ken\u00b7bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leichenscheine wanken durch das Zimmer,", "tokens": ["Lei\u00b7chen\u00b7schei\u00b7ne", "wan\u00b7ken", "durch", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Drau\u00dfen sanken gelbe Abendwolken.", "tokens": ["Drau\u00b7\u00dfen", "san\u00b7ken", "gel\u00b7be", "Ab\u00b7end\u00b7wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hinter meinen Augen", "tokens": ["Hin\u00b7ter", "mei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Steigen goldne L\u00e4nder, menschenleer,", "tokens": ["Stei\u00b7gen", "gold\u00b7ne", "L\u00e4n\u00b7der", ",", "men\u00b7schen\u00b7leer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich schreite hehr, ein K\u00f6nig,", "tokens": ["Und", "ich", "schrei\u00b7te", "hehr", ",", 