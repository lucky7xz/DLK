{"dta.poem.6244": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein F\u00fchlen ists, wovon der Sinnen Werckzeug nur", "tokens": ["Ein", "F\u00fch\u00b7len", "ists", ",", "wo\u00b7von", "der", "Sin\u00b7nen", "Wer\u00b7ck\u00b7zeug", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eigentliche Art begreiffen kan,", "tokens": ["Die", "ei\u00b7gent\u00b7li\u00b7che", "Art", "be\u00b7greif\u00b7fen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der denn, nachdem er mehr und minder wird ger\u00fchret,", "tokens": ["Der", "denn", ",", "nach\u00b7dem", "er", "mehr", "und", "min\u00b7der", "wird", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PPER", "ADV", "KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch mehr und minder Druck versp\u00fcret.", "tokens": ["Auch", "mehr", "und", "min\u00b7der", "Druck", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}