{"dta.poem.15829": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wof\u00fcr belohnt ihr mich? Was hab' ich \u00f6ffentlich,", "tokens": ["Wo\u00b7f\u00fcr", "be\u00b7lohnt", "ihr", "mich", "?", "Was", "hab'", "ich", "\u00f6f\u00b7fent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "PWS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Besondres was gethan f\u00fcr dich, und dich, und dich?", "tokens": ["Be\u00b7sond\u00b7res", "was", "ge\u00b7than", "f\u00fcr", "dich", ",", "und", "dich", ",", "und", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "PWS", "VVPP", "APPR", "PPER", "$,", "KON", "PPER", "$,", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Welt belohnt sonst nur die Dienste der Partei,", "tokens": ["Die", "Welt", "be\u00b7lohnt", "sonst", "nur", "die", "Diens\u00b7te", "der", "Par\u00b7tei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Dienste des Bedarfs, des Nutzens mancherlei.", "tokens": ["Die", "Diens\u00b7te", "des", "Be\u00b7darfs", ",", "des", "Nut\u00b7zens", "man\u00b7cher\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch solches Dienstes frei und ledig ist das Sch\u00f6ne;", "tokens": ["Doch", "sol\u00b7ches", "Diens\u00b7tes", "frei", "und", "le\u00b7dig", "ist", "das", "Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darum verlang' es nicht, da\u00df ird'scher Lohn es kr\u00f6ne.", "tokens": ["Da\u00b7rum", "ver\u00b7lang'", "es", "nicht", ",", "da\u00df", "ird'\u00b7scher", "Lohn", "es", "kr\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der stille Beifall soll, die Theilnahm', ihm gen\u00fcgen;", "tokens": ["Der", "stil\u00b7le", "Bei\u00b7fall", "soll", ",", "die", "Theil\u00b7nahm'", ",", "ihm", "ge\u00b7n\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$,", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr aber wollt dazu ein gl\u00e4nzend Zeichen f\u00fcgen.", "tokens": ["Ihr", "a\u00b7ber", "wollt", "da\u00b7zu", "ein", "gl\u00e4n\u00b7zend", "Zei\u00b7chen", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PAV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das was kein K\u00f6nig thut, habt ihr zu thun den Muth,", "tokens": ["Das", "was", "kein", "K\u00f6\u00b7nig", "thut", ",", "habt", "ihr", "zu", "thun", "den", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PIAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Ausdruck innren Werths zu stempeln \u00e4u\u00dfres Gut.", "tokens": ["Mit", "Aus\u00b7druck", "inn\u00b7ren", "Werths", "zu", "stem\u00b7peln", "\u00e4u\u00df\u00b7res", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das ist ein h\u00f6hrer Sinn, ein reicherer Gewinn,", "tokens": ["Das", "ist", "ein", "h\u00f6h\u00b7rer", "Sinn", ",", "ein", "rei\u00b7che\u00b7rer", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stolz empfind' ich mich, wie sch\u00f6n belohnt ich bin.", "tokens": ["Und", "stolz", "emp\u00b7find'", "ich", "mich", ",", "wie", "sch\u00f6n", "be\u00b7lohnt", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Des Dichters Selbstgef\u00fchl soll das zu Thaten treiben,", "tokens": ["Des", "Dich\u00b7ters", "Selbst\u00b7ge\u00b7f\u00fchl", "soll", "das", "zu", "Tha\u00b7ten", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PDS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um w\u00fcrdig, wie ihr ihn befunden habt, zu bleiben.", "tokens": ["Um", "w\u00fcr\u00b7dig", ",", "wie", "ihr", "ihn", "be\u00b7fun\u00b7den", "habt", ",", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}