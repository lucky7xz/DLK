{"dta.poem.18063": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Xx.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Fuchs mit leerem Magen,", "tokens": ["Es", "sa\u00df", "ein", "Fuchs", "mit", "lee\u00b7rem", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und im Gem\u00fcthe gantz zerschlagen,", "tokens": ["Und", "im", "Ge\u00b7m\u00fc\u00b7the", "gantz", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Schatten eines hohen Baums.", "tokens": ["Jm", "Schat\u00b7ten", "ei\u00b7nes", "ho\u00b7hen", "Baums", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf diesen setzte sich ein Rab in gleicher Stunde.", "tokens": ["Auf", "die\u00b7sen", "setz\u00b7te", "sich", "ein", "Rab", "in", "glei\u00b7cher", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der hatt ein St\u00fccke K\u00e4s\u2019 im Munde.", "tokens": ["Der", "hatt", "ein", "St\u00fc\u00b7cke", "K\u00e4s'", "im", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fuchs sah die\u00df Begegni\u00df gern:", "tokens": ["Der", "Fuchs", "sah", "die\u00df", "Be\u00b7geg\u00b7ni\u00df", "gern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PDS", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er redt ihn an: Ich gr\u00fcsse meinen Herrn;", "tokens": ["Er", "redt", "ihn", "an", ":", "Ich", "gr\u00fcs\u00b7se", "mei\u00b7nen", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich bin und bleibe stets dein Knecht.", "tokens": ["Ich", "bin", "und", "blei\u00b7be", "stets", "dein", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das d\u00fcnckt mich billig, gut, und recht.", "tokens": ["Das", "d\u00fcnckt", "mich", "bil\u00b7lig", ",", "gut", ",", "und", "recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du bist so edel, sch\u00f6n, und liederreich,", "tokens": ["Du", "bist", "so", "e\u00b7del", ",", "sch\u00f6n", ",", "und", "lie\u00b7der\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Kein andrer Vogel ist dir gleich;", "tokens": ["Kein", "an\u00b7drer", "Vo\u00b7gel", "ist", "dir", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Man suche gleich in allen K\u00f6nigreichen.", "tokens": ["Man", "su\u00b7che", "gleich", "in", "al\u00b7len", "K\u00f6\u00b7nig\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich schmeichle nicht, mein Herr, dir weichen", "tokens": ["Ich", "schmeich\u00b7le", "nicht", ",", "mein", "Herr", ",", "dir", "wei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Falck an Adel und an Macht;", "tokens": ["Der", "Falck", "an", "A\u00b7del", "und", "an", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Pfau an Schein und lichter Pracht;", "tokens": ["Der", "Pfau", "an", "Schein", "und", "lich\u00b7ter", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "An lieblichem Gesang die s\u00fcsse Nachtigall.", "tokens": ["An", "lieb\u00b7li\u00b7chem", "Ge\u00b7sang", "die", "s\u00fcs\u00b7se", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du \u00fcbertriffst sie weit mit deinem scharffen Schall.", "tokens": ["Du", "\u00fc\u00b7bert\u00b7riffst", "sie", "weit", "mit", "dei\u00b7nem", "scharf\u00b7fen", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Man h\u00f6rt ihn weit und breit im holen Wald erklingen.", "tokens": ["Man", "h\u00f6rt", "ihn", "weit", "und", "breit", "im", "ho\u00b7len", "Wald", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn du die Stimm erhebst ein s\u00fcsses Lied zu singen,", "tokens": ["Wenn", "du", "die", "Stimm", "er\u00b7hebst", "ein", "s\u00fcs\u00b7ses", "Lied", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So dringt die Wollust sich durch alle meine Glieder,", "tokens": ["So", "dringt", "die", "Wol\u00b7lust", "sich", "durch", "al\u00b7le", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich h\u00f6r entz\u00fccket deine Lieder.", "tokens": ["Ich", "h\u00f6r", "ent\u00b7z\u00fc\u00b7cket", "dei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der Rabe sprach: Du hast ein zartes Ohr;", "tokens": ["Der", "Ra\u00b7be", "sprach", ":", "Du", "hast", "ein", "zar\u00b7tes", "Ohr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und Urtheil und Geschmack darinnen.", "tokens": ["Und", "Ur\u00b7theil", "und", "Ge\u00b7schmack", "da\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ich will dir diese Lust auch jezo nicht mi\u00dfg\u00f6nnen.", "tokens": ["Ich", "will", "dir", "die\u00b7se", "Lust", "auch", "je\u00b7zo", "nicht", "mi\u00df\u00b7g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PDAT", "NN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Damit schlo\u00df er das Aug, und hub den Kopf empor.", "tokens": ["Da\u00b7mit", "schlo\u00df", "er", "das", "Aug", ",", "und", "hub", "den", "Kopf", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Er stimmte starck den widrigen Gesang,", "tokens": ["Er", "stimm\u00b7te", "starck", "den", "wid\u00b7ri\u00b7gen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der krachend durch den Berg und tiefen Thal erklang.", "tokens": ["Der", "kra\u00b7chend", "durch", "den", "Berg", "und", "tie\u00b7fen", "Thal", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "In w\u00e4hrendem Gesang entfiel der K\u00e4s\u2019 ihm bald;", "tokens": ["In", "w\u00e4h\u00b7ren\u00b7dem", "Ge\u00b7sang", "ent\u00b7fiel", "der", "K\u00e4s'", "ihm", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den nahm der Fuchs zum Danck, und trug ihn in den Wald.", "tokens": ["Den", "nahm", "der", "Fuchs", "zum", "Danck", ",", "und", "trug", "ihn", "in", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NE", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}