{"dta.poem.21895": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Wegen der Zuschrifft an den  \n Momus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "In Indien pflegt man den Teufel zu ehren/", "tokens": ["In", "In\u00b7di\u00b7en", "pflegt", "man", "den", "Teu\u00b7fel", "zu", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PIS", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "weil er am meisten Schaden thut.", "tokens": ["weil", "er", "am", "meis\u00b7ten", "Scha\u00b7den", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der gute Gott/ dieweil er gut", "tokens": ["der", "gu\u00b7te", "Gott", "/", "die\u00b7weil", "er", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "w\u00fcrd\u2019 ohn di\u00df die Menschen mit nichten be-", "tokens": ["w\u00fcrd'", "ohn", "di\u00df", "die", "Men\u00b7schen", "mit", "nich\u00b7ten", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDS", "ART", "NN", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "schweeren.", "tokens": ["schwee\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Der Momus/ der he\u00dfliche gre\u00dfliche Dieb/", "tokens": ["Der", "Mo\u00b7mus", "/", "der", "he\u00df\u00b7li\u00b7che", "gre\u00df\u00b7li\u00b7che", "Dieb", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "der findet leichtlich ein Gebrechen.", "tokens": ["der", "fin\u00b7det", "leicht\u00b7lich", "ein", "Ge\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drum wolt\u2019 ich ihn zufrieden sprechen.", "tokens": ["Drum", "wolt'", "ich", "ihn", "zu\u00b7frie\u00b7den", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auffrichtigen Lesern ist dieses schon lieb.", "tokens": ["Auf\u00b7frich\u00b7ti\u00b7gen", "Le\u00b7sern", "ist", "die\u00b7ses", "schon", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PDAT", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}