], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indem er durch Mercurens Streich,", "tokens": ["In\u00b7dem", "er", "durch", "Mer\u00b7cu\u00b7rens", "Streich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich die vertraute Kuh lie\u00df stehlen;", "tokens": ["Sich", "die", "ver\u00b7trau\u00b7te", "Kuh", "lie\u00df", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Elende W\u00e4chter! ach wie bald", "tokens": ["E\u00b7len\u00b7de", "W\u00e4ch\u00b7ter", "!", "ach", "wie", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "XY", "KOKOM", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wird euch nicht die Geliebte blenden;", "tokens": ["Wird", "euch", "nicht", "die", "Ge\u00b7lieb\u00b7te", "blen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Rei\u00dft ihr die Wollust mit Gewalt", "tokens": ["Rei\u00dft", "ihr", "die", "Wol\u00b7lust", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Tugend Vorschrift aus den H\u00e4nden.", "tokens": ["Der", "Tu\u00b7gend", "Vor\u00b7schrift", "aus", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "st