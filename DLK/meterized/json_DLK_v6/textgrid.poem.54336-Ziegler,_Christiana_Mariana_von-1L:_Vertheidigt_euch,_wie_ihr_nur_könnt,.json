 "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Drache weicht nicht von dem Vlie\u00df,", "tokens": ["Der", "Dra\u00b7che", "weicht", "nicht", "von", "dem", "Vlie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier mu\u00df ein dreister Jason fliehen;", "tokens": ["Hier", "mu\u00df", "ein", "dreis\u00b7ter", "Ja\u00b7son", "flie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Zukunft wird er ganz gewi\u00df", "tokens": ["In", "Zu\u00b7kunft", "wird", "er", "ganz", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar eine Decke vor sie ziehen.", "tokens": ["Gar", "ei\u00b7ne", "De\u00b7cke", "vor", "sie", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Betr\u00fcbte Phyllis! die allhier", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "Phyl\u00b7lis", "!", "die", "all\u00b7hier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$.", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Schicksal gar zu hart will betten!", "tokens": ["Das", "Schick\u00b7sal", "gar", "zu", "hart", "will", "bet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Welt erblickt f\u00fcrwahr in dir", "tokens": ["Die", "Welt", "er\u00b7blickt", "f\u00fcr\u00b7wahr", "in", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Andromeden in Band und Ketten.", "tokens": ["And\u00b7ro\u00b7me\u00b7den", "in", "Band", "und", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Setzt nur bey selbst geh\u00e4ufter Quaal", "tokens": ["Setzt", "nur", "bey", "selbst", "ge\u00b7h\u00e4uf\u00b7ter", "Qua\u00b7al"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "In euren Kopf euch Falkenaugen,", "tokens": ["In", "eu\u00b7ren", "Kopf", "euch", "Fal\u00b7ken\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Borgt von den Luchsen Blick und Strahl,", "tokens": ["Borgt", "von", "den", "Luch\u00b7sen", "Blick", "und", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Aufsicht wird hier wenig taugen.", "tokens": ["Die", "Auf\u00b7sicht", "wird", "hier", "we\u00b7nig", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART