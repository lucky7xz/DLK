{"textgrid.poem.54336": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vertheidigt euch, wie ihr nur k\u00f6nnt,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vertheidigt euch, wie ihr nur k\u00f6nnt,", "tokens": ["Ver\u00b7thei\u00b7digt", "euch", ",", "wie", "ihr", "nur", "k\u00f6nnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Eifersucht gew\u00f6hnte Geister.", "tokens": ["Zur", "Ei\u00b7fer\u00b7sucht", "ge\u00b7w\u00f6hn\u00b7te", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eur Aug und Sinn ist doch verblendt,", "tokens": ["Eur", "Aug", "und", "Sinn", "ist", "doch", "ver\u00b7blendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr seyd nicht von euch selbsten Meister.", "tokens": ["Ihr", "seyd", "nicht", "von", "euch", "selbs\u00b7ten", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie? la\u00dft ihr euch ein rauschend Blat", "tokens": ["Wie", "?", "la\u00dft", "ihr", "euch", "ein", "rau\u00b7schend", "Blat"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVIMP", "PPER", "PRF", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Furcht und in Bewegung jagen?", "tokens": ["In", "Furcht", "und", "in", "Be\u00b7we\u00b7gung", "ja\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn sichs kaum gereget hat,", "tokens": ["Und", "wenn", "sichs", "kaum", "ge\u00b7re\u00b7get", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wollet ihr schon gar verzagen?", "tokens": ["So", "wol\u00b7let", "ihr", "schon", "gar", "ver\u00b7za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00e4mt euch, die Welt lacht eurer nur,", "tokens": ["Sch\u00e4mt", "euch", ",", "die", "Welt", "lacht", "eu\u00b7rer", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spricht: Es sey ihr nichts verha\u00dfter,", "tokens": ["Und", "spricht", ":", "Es", "sey", "ihr", "nichts", "ver\u00b7ha\u00df\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als eine solche Creatur,", "tokens": ["Als", "ei\u00b7ne", "sol\u00b7che", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein Knecht von diesem schn\u00f6den Laster.", "tokens": ["Ein", "Knecht", "von", "die\u00b7sem", "schn\u00f6\u00b7den", "Las\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Warum? der Eifer, der auch kann", "tokens": ["Wa\u00b7rum", "?", "der", "Ei\u00b7fer", ",", "der", "auch", "kann"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "ART", "NN", "$,", "PRELS", "ADV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den sch\u00f6nsten Ganymed verstellen,", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Ga\u00b7ny\u00b7med", "ver\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Macht, schaut man euch dabey nur an,", "tokens": ["Macht", ",", "schaut", "man", "euch", "da\u00b7bey", "nur", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIS", "PRF", "PAV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Euch zu den Furien der H\u00f6llen.", "tokens": ["Euch", "zu", "den", "Fu\u00b7ri\u00b7en", "der", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verh\u00fcllet dieses Laster nicht", "tokens": ["Ver\u00b7h\u00fcl\u00b7let", "die\u00b7ses", "Las\u00b7ter", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Vorhang zarter Liebe;", "tokens": ["Mit", "ei\u00b7nem", "Vor\u00b7hang", "zar\u00b7ter", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn stellet man es an das Licht,", "tokens": ["Denn", "stel\u00b7let", "man", "es", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sinds des Argwohns schwarze Triebe.", "tokens": ["So", "sinds", "des", "Arg\u00b7wohns", "schwar\u00b7ze", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr meynt, wenn ihr in Spiegel blickt,", "tokens": ["Ihr", "meynt", ",", "wenn", "ihr", "in", "Spie\u00b7gel", "blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00fcchs euch was aus eurer Stirne;", "tokens": ["Als", "w\u00fcchs", "euch", "was", "aus", "eu\u00b7rer", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Phantasie die euch ber\u00fcckt,", "tokens": ["Die", "Phan\u00b7ta\u00b7sie", "die", "euch", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erregt dies Blendwerk im Gehirne.", "tokens": ["Er\u00b7regt", "dies", "Blend\u00b7werk", "im", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Wahn betr\u00fcgt euch allzusehr,", "tokens": ["Der", "Wahn", "be\u00b7tr\u00fcgt", "euch", "all\u00b7zu\u00b7sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dessen seichten Grund ihr bauet,", "tokens": ["Auf", "des\u00b7sen", "seich\u00b7ten", "Grund", "ihr", "bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Argwohn siehet immer mehr,", "tokens": ["Der", "Arg\u00b7wohn", "sie\u00b7het", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Janus mit vier Augen schauet.", "tokens": ["Als", "Ja\u00b7nus", "mit", "vier", "Au\u00b7gen", "schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Und dennoch fehlt ihr insgemein,", "tokens": ["Und", "den\u00b7noch", "fehlt", "ihr", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So scharf ihr hier und dahin blitzet,", "tokens": ["So", "scharf", "ihr", "hier", "und", "da\u00b7hin", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weil euch nur blo\u00df der falsche Schein", "tokens": ["Weil", "euch", "nur", "blo\u00df", "der", "fal\u00b7sche", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von des Act\u00e4ons Kopf erhitzet.", "tokens": ["Von", "des", "Ac\u00b7t\u00e4ons", "Kopf", "er\u00b7hit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Qu\u00e4lt euch doch nicht durch solche Sucht,", "tokens": ["Qu\u00e4lt", "euch", "doch", "nicht", "durch", "sol\u00b7che", "Sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schonet eures armen Lebens.", "tokens": ["Und", "scho\u00b7net", "eu\u00b7res", "ar\u00b7men", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jagt alle Sorgen in die Flucht,", "tokens": ["Jagt", "al\u00b7le", "Sor\u00b7gen", "in", "die", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie sind wahrlich ganz vergebens.", "tokens": ["Denn", "sie", "sind", "wahr\u00b7lich", "ganz", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr fre\u00dft euch selber vor Verdru\u00df;", "tokens": ["Ihr", "fre\u00dft", "euch", "sel\u00b7ber", "vor", "Ver\u00b7dru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Herz, wie dort mit scharfen Zungen", "tokens": ["Das", "Herz", ",", "wie", "dort", "mit", "schar\u00b7fen", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Leber jenes Tityus", "tokens": ["Die", "Le\u00b7ber", "je\u00b7nes", "Ti\u00b7tyus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Von Plutons Geyern ward verschlungen.", "tokens": ["Von", "Plu\u00b7tons", "Ge\u00b7yern", "ward", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Seht mir nur diesen Kranken an,", "tokens": ["Seht", "mir", "nur", "die\u00b7sen", "Kran\u00b7ken", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der einem blossen Schatten gleichet;", "tokens": ["Der", "ei\u00b7nem", "blos\u00b7sen", "Schat\u00b7ten", "glei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und kaum noch Athem holen kann;", "tokens": ["Und", "kaum", "noch", "A\u00b7them", "ho\u00b7len", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt, wie er kr\u00e4chzt, wie stark er keuchet.", "tokens": ["H\u00f6rt", ",", "wie", "er", "kr\u00e4chzt", ",", "wie", "stark", "er", "keuc\u00b7het", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was ists, das ihn so m\u00fcrbe macht,", "tokens": ["Was", "ists", ",", "das", "ihn", "so", "m\u00fcr\u00b7be", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und die Gestalt ganz umgekehret?", "tokens": ["Und", "die", "Ge\u00b7stalt", "ganz", "um\u00b7ge\u00b7keh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Eyfersucht hat Tag und Nacht", "tokens": ["Die", "Ey\u00b7fer\u00b7sucht", "hat", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm Mark und Kraft und Blut verzehret.", "tokens": ["Ihm", "Mark", "und", "Kraft", "und", "Blut", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und warum n\u00e4hrt er seine Wuth,", "tokens": ["Und", "wa\u00b7rum", "n\u00e4hrt", "er", "sei\u00b7ne", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fcrzet sich sein junges Leben?", "tokens": ["Und", "k\u00fcr\u00b7zet", "sich", "sein", "jun\u00b7ges", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn seiner Phyllis redlich Blut", "tokens": ["Denn", "sei\u00b7ner", "Phyl\u00b7lis", "red\u00b7lich", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NE", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann wohl hierzu nicht Anla\u00df geben.", "tokens": ["Kann", "wohl", "hier\u00b7zu", "nicht", "An\u00b7la\u00df", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PAV", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was bl\u00e4st ihm denn den Eifer ein?", "tokens": ["Was", "bl\u00e4st", "ihm", "denn", "den", "Ei\u00b7fer", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur dies; sie weis in allen F\u00e4llen", "tokens": ["Nur", "dies", ";", "sie", "weis", "in", "al\u00b7len", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "$.", "PPER", "PTKVZ", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und gegen jeden insgemein", "tokens": ["Und", "ge\u00b7gen", "je\u00b7den", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich h\u00f6flich und beliebt zu stellen.", "tokens": ["Sich", "h\u00f6f\u00b7lich", "und", "be\u00b7liebt", "zu", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hier stehet ein verliebter Tropf", "tokens": ["Hier", "ste\u00b7het", "ein", "ver\u00b7lieb\u00b7ter", "Tropf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In tausendfachen Furcht und Sorgen,", "tokens": ["In", "tau\u00b7send\u00b7fa\u00b7chen", "Furcht", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er setzt sich Grillen in den Kopf,", "tokens": ["Er", "setzt", "sich", "Gril\u00b7len", "in", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sinnt vom Abend bi\u00df zum Morgen.", "tokens": ["Und", "sinnt", "vom", "A\u00b7bend", "bi\u00df", "zum", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR