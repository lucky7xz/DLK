{"textgrid.poem.62715": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "[ein kleines Licht scheint in der Ferne bey dunkler Nacht ein Stern zu seyn]", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines Licht scheint in der Ferne bey dunkler Nacht ein Stern zu seyn;", "tokens": ["Ein", "klei\u00b7nes", "Licht", "scheint", "in", "der", "Fer\u00b7ne", "bey", "dunk\u00b7ler", "Nacht", "ein", "Stern", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch wenn mans in der N\u00e4h betrachtet, so trift die Meinung gar nicht ein.", "tokens": ["Doch", "wenn", "mans", "in", "der", "N\u00e4h", "be\u00b7trach\u00b7tet", ",", "so", "trift", "die", "Mei\u00b7nung", "gar", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "So f\u00e4llt auch manches Mannes Kunst den fremden trefflich ins Gesichte;", "tokens": ["So", "f\u00e4llt", "auch", "man\u00b7ches", "Man\u00b7nes", "Kunst", "den", "frem\u00b7den", "treff\u00b7lich", "ins", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "NN", "ART", "ADJA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Allein die Seinen in der Stadt erkennens vor ein blos Gedichte.", "tokens": ["Al\u00b7lein", "die", "Sei\u00b7nen", "in", "der", "Stadt", "er\u00b7ken\u00b7nens", "vor", "ein", "blos", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPOSS", "APPR", "ART", "NN", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}