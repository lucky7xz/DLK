{"textgrid.poem.48066": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Himmel ist der Fr\u00fchlingsregen", "tokens": ["Vom", "Him\u00b7mel", "ist", "der", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "herabgerauscht die ganze Nacht, \u2013", "tokens": ["her\u00b7ab\u00b7ge\u00b7rauscht", "die", "gan\u00b7ze", "Nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich h\u00f6r im Traum die Tropfen fallen", "tokens": ["ich", "h\u00f6r", "im", "Traum", "die", "Trop\u00b7fen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und habe l\u00e4chelnd dein gedacht.", "tokens": ["und", "ha\u00b7be", "l\u00e4\u00b7chelnd", "dein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nun im lichten Fr\u00fchrotkleide", "tokens": ["Und", "nun", "im", "lich\u00b7ten", "Fr\u00fch\u00b7rot\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Tag vor meiner T\u00fcre steht,", "tokens": ["der", "Tag", "vor", "mei\u00b7ner", "T\u00fc\u00b7re", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nun schlie\u00df ich unter Tr\u00e4nenstr\u00f6men", "tokens": ["nun", "schlie\u00df", "ich", "un\u00b7ter", "Tr\u00e4\u00b7nen\u00b7str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dich in mein heiligstes Gebet.", "tokens": ["dich", "in", "mein", "hei\u00b7ligs\u00b7tes", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}