{"textgrid.poem.41549": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am ersten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ich getauft in deinem Zeichen,", "tokens": ["Bin", "ich", "ge\u00b7tauft", "in", "dei\u00b7nem", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du heilige Dreifaltigkeit,", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Drei\u00b7fal\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun bleibt es mir und kann nicht weichen", "tokens": ["Nun", "bleibt", "es", "mir", "und", "kann", "nicht", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser nicht und jener Zeit.", "tokens": ["In", "die\u00b7ser", "nicht", "und", "je\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PTKNEG", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich f\u00fchle durch Verstandes Frost,", "tokens": ["Ich", "f\u00fch\u00b7le", "durch", "Ver\u00b7stan\u00b7des", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Menschenwortes Nebelrennen", "tokens": ["Durch", "Men\u00b7schen\u00b7wor\u00b7tes", "Ne\u00b7bel\u00b7ren\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es wie ein klares Funkeln brennen", "tokens": ["Es", "wie", "ein", "kla\u00b7res", "Fun\u00b7keln", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zehren an verj\u00e4hrtem Rost.", "tokens": ["Und", "zeh\u00b7ren", "an", "ver\u00b7j\u00e4hr\u00b7tem", "Rost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In deinem Tempel will sich's regen,", "tokens": ["In", "dei\u00b7nem", "Tem\u00b7pel", "will", "sich's", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich als deine Magd erschien,", "tokens": ["Wo", "ich", "als", "dei\u00b7ne", "Magd", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unter deines Priesters Segen", "tokens": ["Und", "un\u00b7ter", "dei\u00b7nes", "Pries\u00b7ters", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchl' ich es leise Nahrung ziehn.", "tokens": ["F\u00fchl'", "ich", "es", "lei\u00b7se", "Nah\u00b7rung", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn eine teure Mutterhand", "tokens": ["Wenn", "ei\u00b7ne", "teu\u00b7re", "Mut\u00b7ter\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Kreuz mir zeichnet auf die Stirne,", "tokens": ["Das", "Kreuz", "mir", "zeich\u00b7net", "auf", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann zuckt's lebendig im Gehirne", "tokens": ["Dann", "zuckt's", "le\u00b7ben\u00b7dig", "im", "Ge\u00b7hir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meine Sinne stehn in Brand.", "tokens": ["Und", "mei\u00b7ne", "Sin\u00b7ne", "stehn", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja selbst zu Nacht, wenn alle schlafen", "tokens": ["Ja", "selbst", "zu", "Nacht", ",", "wenn", "al\u00b7le", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "APPR", "NN", "$,", "KOUS", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber mich die Angst sich legt,", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "die", "Angst", "sich", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In der Gedanken \u00f6den Hafen", "tokens": ["In", "der", "Ge\u00b7dan\u00b7ken", "\u00f6\u00b7den", "Ha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zweifel seine Flagge tr\u00e4gt:", "tokens": ["Der", "Zwei\u00b7fel", "sei\u00b7ne", "Flag\u00b7ge", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie eine Phosphorpflanze noch", "tokens": ["Wie", "ei\u00b7ne", "Phos\u00b7phor\u00b7pflan\u00b7ze", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchl' ich es warm und leuchtend schwellen,", "tokens": ["F\u00fchl'", "ich", "es", "warm", "und", "leuch\u00b7tend", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber die verst\u00f6rten Wellen", "tokens": ["Und", "\u00fc\u00b7ber", "die", "ver\u00b7st\u00f6r\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Legt sich ein leiser Schimmer doch.", "tokens": ["Legt", "sich", "ein", "lei\u00b7ser", "Schim\u00b7mer", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mu\u00df mir zum Gericht gereichen", "tokens": ["Und", "mu\u00df", "mir", "zum", "Ge\u00b7richt", "ge\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lebenspflanze mir gesellt,", "tokens": ["Die", "Le\u00b7bens\u00b7pflan\u00b7ze", "mir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ich vers\u00e4umte sondergleichen,", "tokens": ["Die", "ich", "ver\u00b7s\u00e4um\u00b7te", "son\u00b7derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00fcrrem Holze gleichgestellt:", "tokens": ["Und", "d\u00fcr\u00b7rem", "Hol\u00b7ze", "gleich\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist sie in der S\u00fcnden Bann,", "tokens": ["So", "ist", "sie", "in", "der", "S\u00fcn\u00b7den", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Geistes schwindelnden Getrieben,", "tokens": ["Des", "Geis\u00b7tes", "schwin\u00b7deln\u00b7den", "Ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein heimlich Kleinod doch geblieben", "tokens": ["Mein", "heim\u00b7lich", "Klei\u00b7nod", "doch", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und angstvoll hangt mein Herz daran.", "tokens": ["Und", "angst\u00b7voll", "hangt", "mein", "Herz", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ob ich vor deiner Gei\u00dfel zage,", "tokens": ["Ob", "ich", "vor", "dei\u00b7ner", "Gei\u00b7\u00dfel", "za\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts k\u00f6mmt doch dem Bewu\u00dftsein gleich,", "tokens": ["Nichts", "k\u00f6mmt", "doch", "dem", "Be\u00b7wu\u00df\u00b7tsein", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dennoch ich dein Zeichen trage", "tokens": ["Da\u00df", "den\u00b7noch", "ich", "dein", "Zei\u00b7chen", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und blute unter deinem Streich.", "tokens": ["Und", "blu\u00b7te", "un\u00b7ter", "dei\u00b7nem", "Streich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fluch allem, was von dir mich st\u00f6\u00dft!", "tokens": ["Fluch", "al\u00b7lem", ",", "was", "von", "dir", "mich", "st\u00f6\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein will ich sein, von dir nur stammen;", "tokens": ["Dein", "will", "ich", "sein", ",", "von", "dir", "nur", "stam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "VAINF", "$,", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Viel lieber sollst du mich verdammen,", "tokens": ["Viel", "lie\u00b7ber", "sollst", "du", "mich", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als da\u00df ein andrer mich erl\u00f6st.", "tokens": ["Als", "da\u00df", "ein", "an\u00b7drer", "mich", "er\u00b7l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}