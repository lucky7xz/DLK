{"textgrid.poem.35454": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der Entsagung stumpfes Br\u00fcten", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Entsagung stumpfes Br\u00fcten", "tokens": ["In", "der", "Ent\u00b7sa\u00b7gung", "stump\u00b7fes", "Br\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich die Seele eingewiegt \u2013", "tokens": ["Hab'", "ich", "die", "See\u00b7le", "ein\u00b7ge\u00b7wiegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdorrt sind meiner Sehnsucht Bl\u00fcten,", "tokens": ["Ver\u00b7dorrt", "sind", "mei\u00b7ner", "Sehn\u00b7sucht", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meiner Kraft Quell ist versiegt ...", "tokens": ["Und", "mei\u00b7ner", "Kraft", "Quell", "ist", "ver\u00b7siegt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Falb und gestaltlos wie der Heide", "tokens": ["Falb", "und", "ge\u00b7stalt\u00b7los", "wie", "der", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verd\u00e4mmerte Monotonie", "tokens": ["Ver\u00b7d\u00e4m\u00b7mer\u00b7te", "Mo\u00b7no\u00b7to\u00b7nie"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt mir mein Leben! Und ich scheide,", "tokens": ["Liegt", "mir", "mein", "Le\u00b7ben", "!", "Und", "ich", "schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als h\u00e4tt' ich's nie gekostet \u2013 nie!", "tokens": ["Als", "h\u00e4tt'", "ich's", "nie", "ge\u00b7kos\u00b7tet", "\u2013", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "ADV", "VVPP", "$(", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als h\u00e4tt' ich nie an seinen Feuern", "tokens": ["Als", "h\u00e4tt'", "ich", "nie", "an", "sei\u00b7nen", "Feu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesessen und gesogen Glut;", "tokens": ["Ge\u00b7ses\u00b7sen", "und", "ge\u00b7so\u00b7gen", "Glut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als h\u00e4tt' mit seinen Ungeheuern", "tokens": ["Als", "h\u00e4tt'", "mit", "sei\u00b7nen", "Un\u00b7ge\u00b7heu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gerungen nie mein Heldenmut!", "tokens": ["Ge\u00b7run\u00b7gen", "nie", "mein", "Hel\u00b7den\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als h\u00e4tt' ich nie auf seinen H\u00f6hen", "tokens": ["Als", "h\u00e4tt'", "ich", "nie", "auf", "sei\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gestanden: Blick und Seele weit!", "tokens": ["Ge\u00b7stan\u00b7den", ":", "Blick", "und", "See\u00b7le", "weit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als h\u00e4tt' ich nie in seinen T\u00e4lern", "tokens": ["Als", "h\u00e4tt'", "ich", "nie", "in", "sei\u00b7nen", "T\u00e4\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfahren, wie das Elend schreit!", "tokens": ["Er\u00b7fah\u00b7ren", ",", "wie", "das", "E\u00b7lend", "schreit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Herz ist still \u2013 mein Auge trocken \u2013", "tokens": ["Mein", "Herz", "ist", "still", "\u2013", "mein", "Au\u00b7ge", "tro\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mehr bewegt mich Menschenbrauch \u2013", "tokens": ["Nicht", "mehr", "be\u00b7wegt", "mich", "Men\u00b7schen\u00b7brauch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Summen fernvertr\u00e4umter Glocken", "tokens": ["Wie", "Sum\u00b7men", "fern\u00b7ver\u00b7tr\u00e4um\u00b7ter", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sp\u00fcr' ich des ", "tokens": ["Sp\u00fcr'", "ich", "des"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}