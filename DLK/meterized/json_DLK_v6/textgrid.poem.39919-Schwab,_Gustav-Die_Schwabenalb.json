{"textgrid.poem.39919": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwabenalb", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lieg' auf weichem Bette,", "tokens": ["Ich", "lieg'", "auf", "wei\u00b7chem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf moos'gem Eichengrund,", "tokens": ["Auf", "moos'\u00b7gem", "Ei\u00b7chen\u00b7grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und vor mir Kett' auf Kette", "tokens": ["Und", "vor", "mir", "Kett'", "auf", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du festes Alpenrund!", "tokens": ["Du", "fes\u00b7tes", "Al\u00b7pen\u00b7rund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sing', ich darf es wagen,", "tokens": ["Ich", "sing'", ",", "ich", "darf", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es mu\u00df ein Lied entstehn,", "tokens": ["Es", "mu\u00df", "ein", "Lied", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich brauche nur zu sagen,", "tokens": ["Ich", "brau\u00b7che", "nur", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ich ringsum gesehn:", "tokens": ["Was", "ich", "ring\u00b7sum", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ganz ferne dort zur Linken,", "tokens": ["Ganz", "fer\u00b7ne", "dort", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ros'gem Abendschein,", "tokens": ["In", "ros'\u00b7gem", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seh' ich ihn duftig winken,", "tokens": ["Seh'", "ich", "ihn", "duf\u00b7tig", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den hohen ", "tokens": ["Den", "ho\u00b7hen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Gesang! vor\u00fcberschwelle", "tokens": ["Ge\u00b7sang", "!", "vor\u00b7\u00fc\u00b7bersc\u00b7hwel\u00b7le"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An seiner Felsenkluft;", "tokens": ["An", "sei\u00b7ner", "Fel\u00b7sen\u00b7kluft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit leuchtender Kapelle", "tokens": ["Mit", "leuch\u00b7ten\u00b7der", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der fromme ", "tokens": ["Der", "from\u00b7me"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Ich spend' ihm ein Gebete;", "tokens": ["Ich", "spend'", "ihm", "ein", "Ge\u00b7be\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bereitet und erbaut,", "tokens": ["Be\u00b7rei\u00b7tet", "und", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So schau' ich nach der St\u00e4tte,", "tokens": ["So", "schau'", "ich", "nach", "der", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Von Kl\u00e4ngen und von Bildern", "tokens": ["Von", "Kl\u00e4n\u00b7gen", "und", "von", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird mir da m\u00e4chtig bang,", "tokens": ["Wird", "mir", "da", "m\u00e4ch\u00b7tig", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man s\u00e4nge, sie zu schildern,", "tokens": ["Man", "s\u00e4n\u00b7ge", ",", "sie", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl ein Jahrhundert lang.", "tokens": ["Wohl", "ein", "Jahr\u00b7hun\u00b7dert", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer forscht nach Staufens Preise,", "tokens": ["Wer", "forscht", "nach", "Stau\u00b7fens", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mag zu den Tr\u00fcmmern gehn,", "tokens": ["Mag", "zu", "den", "Tr\u00fcm\u00b7mern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dort wird mit Geisterweise", "tokens": ["Dort", "wird", "mit", "Geis\u00b7ter\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn ew'ges Lied umwehn.", "tokens": ["Ihn", "ew'\u00b7ges", "Lied", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Vor\u00fcber nun an Bergen,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "nun", "an", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch manche Namen gro\u00df,", "tokens": ["Durch", "man\u00b7che", "Na\u00b7men", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die, ein Gefolg von S\u00e4rgen,", "tokens": ["Die", ",", "ein", "Ge\u00b7folg", "von", "S\u00e4r\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umlagern dieses Schlo\u00df.", "tokens": ["Um\u00b7la\u00b7gern", "die\u00b7ses", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Durch H\u00f6h'n und Th\u00e4ler fl\u00fcchtig,", "tokens": ["Durch", "H\u00f6h'n", "und", "Th\u00e4\u00b7ler", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis zu dem scharfen Eck:", "tokens": ["Bis", "zu", "dem", "schar\u00b7fen", "Eck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort aber steht gewichtig", "tokens": ["Dort", "a\u00b7ber", "steht", "ge\u00b7wich\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die herzogliche ", "tokens": ["Die", "her\u00b7zog\u00b7li\u00b7che"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.10": {"line.1": {"text": "Mit Felsen und mit H\u00f6hlen", "tokens": ["Mit", "Fel\u00b7sen", "und", "mit", "H\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Treibt Abendlicht sein Spiel,", "tokens": ["Treibt", "A\u00b7bend\u00b7licht", "sein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu schau'n und zu erz\u00e4hlen", "tokens": ["Zu", "schau'n", "und", "zu", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Giebt's hier des Ernsten viel.", "tokens": ["Giebt's", "hier", "des", "Erns\u00b7ten", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Man hat dich lassen schleifen,", "tokens": ["Man", "hat", "dich", "las\u00b7sen", "schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergessner Waffensaal!", "tokens": ["Ver\u00b7gess\u00b7ner", "Waf\u00b7fen\u00b7saal", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie neu erbaut, o ", "tokens": ["Wie", "neu", "er\u00b7baut", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "VVPP", "$,", "FM"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gl\u00e4nzst du im Sonnenstral.", "tokens": ["Gl\u00e4nz\u00b7st", "du", "im", "Son\u00b7nen\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und s\u00fc\u00df t\u00f6nt's, wie die Cither,", "tokens": ["Und", "s\u00fc\u00df", "t\u00f6nt's", ",", "wie", "die", "Cit\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus deiner Hallen Grund! \u2013", "tokens": ["Aus", "dei\u00b7ner", "Hal\u00b7len", "Grund", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort sang dein edler Ritter", "tokens": ["Dort", "sang", "dein", "ed\u00b7ler", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Liebchens rotem Mund", "tokens": ["Von", "Lieb\u00b7chens", "ro\u00b7tem", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Aus der Gebirge Kerkern", "tokens": ["Aus", "der", "Ge\u00b7bir\u00b7ge", "Ker\u00b7kern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schaut ", "tokens": ["Schaut"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mit morschen Thurmeserkern,", "tokens": ["Mit", "mor\u00b7schen", "Thur\u00b7me\u00b7ser\u00b7kern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seines Dichters Grab", "tokens": ["Mit", "sei\u00b7nes", "Dich\u00b7ters", "Grab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wie schmiegt der B\u00e4ume Wipfel,", "tokens": ["Wie", "schmiegt", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Rebe sich und Halm", "tokens": ["Wie", "Re\u00b7be", "sich", "und", "Halm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um deinen schlanken Gipfel,", "tokens": ["Um", "dei\u00b7nen", "schlan\u00b7ken", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du herrliches ", "tokens": ["Du", "herr\u00b7li\u00b7ches"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Dort, wo die Eichen sprossen,", "tokens": ["Dort", ",", "wo", "die", "Ei\u00b7chen", "spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Heidenm\u00e4ler stehn,", "tokens": ["Wo", "Hei\u00b7den\u00b7m\u00e4\u00b7ler", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Noch sprechen jene H\u00f6h'n.", "tokens": ["Noch", "spre\u00b7chen", "je\u00b7ne", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Doch Blick und Lied in vollern,", "tokens": ["Doch", "Blick", "und", "Lied", "in", "vol\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In schnellern Bahnen zieht!", "tokens": ["In", "schnel\u00b7lern", "Bah\u00b7nen", "zieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ja ", "tokens": ["Das", "ist", "ja"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Was noch so sonnig gl\u00fcht!", "tokens": ["Was", "noch", "so", "son\u00b7nig", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Der Staufen ist gesunken", "tokens": ["Der", "Stau\u00b7fen", "ist", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In abendliche Nacht,", "tokens": ["In", "a\u00b7bend\u00b7li\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Du aber stehst noch, trunken", "tokens": ["Du", "a\u00b7ber", "stehst", "noch", ",", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von k\u00f6niglicher Pracht!", "tokens": ["Von", "k\u00f6\u00b7nig\u00b7