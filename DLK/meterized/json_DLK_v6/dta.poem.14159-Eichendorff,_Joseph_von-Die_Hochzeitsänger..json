{"dta.poem.14159": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeits\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Lachend reicht man uns die Neigen,", "tokens": ["La\u00b7chend", "reicht", "man", "uns", "die", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Ihr Wohlsein trinken wir;", "tokens": ["Auf", "Ihr", "Wohl\u00b7sein", "trin\u00b7ken", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt' sie sich am Fenster zeigen,", "tokens": ["Wollt'", "sie", "sich", "am", "Fens\u00b7ter", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "'s w\u00e4re doch recht fein von ihr.", "tokens": ["'s", "w\u00e4\u00b7re", "doch", "recht", "fein", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wir fideln und wir singen", "tokens": ["Und", "wir", "fi\u00b7deln", "und", "wir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche sch\u00f6ne Melodei,", "tokens": ["Man\u00b7che", "sch\u00f6\u00b7ne", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die besten Saiten springen,", "tokens": ["Da\u00df", "die", "bes\u00b7ten", "Sai\u00b7ten", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "'s war als spr\u00e4ng' mir's Herz entzwei.", "tokens": ["'", "s", "war", "als", "spr\u00e4ng", "'", "mir's", "Herz", "ent\u00b7zwei", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "KOKOM", "VVFIN", "$(", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}