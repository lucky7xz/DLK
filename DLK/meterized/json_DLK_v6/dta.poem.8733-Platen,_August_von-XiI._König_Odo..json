{"dta.poem.8733": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n  K\u00f6nig Odo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Tausend Lichter funkeln helle,", "tokens": ["Tau\u00b7send", "Lich\u00b7ter", "fun\u00b7keln", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Zug der Beter locken", "tokens": ["Die", "den", "Zug", "der", "Be\u00b7ter", "lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der hohen Kirchenschwelle.", "tokens": ["Nach", "der", "ho\u00b7hen", "Kir\u00b7chen\u00b7schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6nig Odo kommt gefahren,", "tokens": ["K\u00f6\u00b7nig", "O\u00b7do", "kommt", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt vom alten Thurm Gel\u00e4ute,", "tokens": ["H\u00f6rt", "vom", "al\u00b7ten", "Thurm", "Ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er fragt die frommen Schaaren:", "tokens": ["Und", "er", "fragt", "die", "from\u00b7men", "Schaa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber welch ein Fest ist heute?", "tokens": ["A\u00b7ber", "welch", "ein", "Fest", "ist", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie erwiedern d'rauf und sagen:", "tokens": ["Sie", "er\u00b7wie\u00b7dern", "d'\u00b7rauf", "und", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eine Jungfrau nimmt den Schleyer,", "tokens": ["Ei\u00b7ne", "Jung\u00b7frau", "nimmt", "den", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nig Odo springt vom Wagen,", "tokens": ["K\u00f6\u00b7nig", "O\u00b7do", "springt", "vom", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt hinein und schaut die Feyer.", "tokens": ["Tritt", "hin\u00b7ein", "und", "schaut", "die", "Fe\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Um den heil'gen Brauch zu wehren,", "tokens": ["Um", "den", "heil'\u00b7gen", "Brauch", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ruft er aus am Hochaltare:", "tokens": ["Ruft", "er", "aus", "am", "Hoc\u00b7hal\u00b7ta\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Scheere soll versehren", "tokens": ["Kei\u00b7ne", "Schee\u00b7re", "soll", "ver\u00b7seh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese langen, blonden Haare!", "tokens": ["Die\u00b7se", "lan\u00b7gen", ",", "blon\u00b7den", "Haa\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ueber diese feuchten Blicke", "tokens": ["Ue\u00b7ber", "die\u00b7se", "feuch\u00b7ten", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6ge nie ein Schleyer fallen,", "tokens": ["M\u00f6\u00b7ge", "nie", "ein", "Schle\u00b7yer", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein h\u00e4rnes Kleid ersticke", "tokens": ["Und", "kein", "h\u00e4r\u00b7nes", "Kleid", "er\u00b7sti\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Brust gelindes Wallen.", "tokens": ["Die\u00b7ser", "Brust", "ge\u00b7lin\u00b7des", "Wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Rei\u00dfend vom Altar die Reine,", "tokens": ["Rei\u00b7\u00dfend", "vom", "Al\u00b7tar", "die", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trat er nun hervor und tobte:", "tokens": ["Trat", "er", "nun", "her\u00b7vor", "und", "tob\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Christus werde nie der Deine,", "tokens": ["Chris\u00b7tus", "wer\u00b7de", "nie", "der", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nig Odo's Anverlobte!", "tokens": ["K\u00f6\u00b7nig", "O\u00b7do's", "An\u00b7ver\u00b7lob\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Frevelvoll und voll von Wonne,", "tokens": ["Fre\u00b7vel\u00b7voll", "und", "voll", "von", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig im erbotnen Tausche,", "tokens": ["Se\u00b7lig", "im", "er\u00b7bot\u00b7nen", "Tau\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Neigt sich die beth\u00f6rte Nonne", "tokens": ["Neigt", "sich", "die", "be\u00b7th\u00f6r\u00b7te", "Non\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinem sch\u00f6nen Liebesrausche.", "tokens": ["Sei\u00b7nem", "sch\u00f6\u00b7nen", "Lie\u00b7bes\u00b7rau\u00b7sche", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Als die Nacht begann zu schauern", "tokens": ["Als", "die", "Nacht", "be\u00b7gann", "zu", "schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Stunde der Gespenster,", "tokens": ["Um", "die", "Stun\u00b7de", "der", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zitterten des Schlosses Mauern,", "tokens": ["Zit\u00b7ter\u00b7ten", "des", "Schlos\u00b7ses", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und es flogen auf die Fenster.", "tokens": ["Und", "es", "flo\u00b7gen", "auf", "die", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Bebend sahn empor die Gatten,", "tokens": ["Be\u00b7bend", "sahn", "em\u00b7por", "die", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und an's goldne Lager Beyder", "tokens": ["Und", "an's", "gold\u00b7ne", "La\u00b7ger", "Bey\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trat ein wei\u00dfer Zug von Schatten,", "tokens": ["Trat", "ein", "wei\u00b7\u00dfer", "Zug", "von", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Angethan in Nonnenkleider.", "tokens": ["An\u00b7ge\u00b7than", "in", "Non\u00b7nen\u00b7klei\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Alle hielten rothe Kerzen,", "tokens": ["Al\u00b7le", "hiel\u00b7ten", "ro\u00b7the", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche blau und d\u00fcster flammten,", "tokens": ["Wel\u00b7che", "blau", "und", "d\u00fcs\u00b7ter", "flamm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die junge Braut vom Herzen", "tokens": ["Und", "die", "jun\u00b7ge", "Braut", "vom", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rissen sie dem Gottverdammten.", "tokens": ["Ris\u00b7sen", "sie", "dem", "Gott\u00b7ver\u00b7damm\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "H\u00fclfe ruft er, greift verwegen", "tokens": ["H\u00fcl\u00b7fe", "ruft", "er", ",", "greift", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zur geschliffnen Wehr im Grimme;", "tokens": ["Zur", "ge\u00b7schliff\u00b7nen", "Wehr", "im", "Grim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ihm versagt der Degen,", "tokens": ["A\u00b7ber", "ihm", "ver\u00b7sagt", "der", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ihm versagt die Stimme.", "tokens": ["A\u00b7ber", "ihm", "ver\u00b7sagt", "die", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und das M\u00e4dchen ziehn am Haare", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "ziehn", "am", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene fort, das arme, bleiche,", "tokens": ["Je\u00b7ne", "fort", ",", "das", "ar\u00b7me", ",", "blei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PTKVZ", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Legen dann auf eine Bahre", "tokens": ["Le\u00b7gen", "dann", "auf", "ei\u00b7ne", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die lebend'ge, sch\u00f6ne Leiche.", "tokens": ["Die", "le\u00b7ben\u00b7d'\u00b7ge", ",", "sch\u00f6\u00b7ne", "Lei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und der K\u00f6nig folgte bange,", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "folg\u00b7te", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Sinne halb nur m\u00e4chtig:", "tokens": ["Sei\u00b7ner", "Sin\u00b7ne", "halb", "nur", "m\u00e4ch\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Kirche S\u00e4ulengange", "tokens": ["In", "der", "Kir\u00b7che", "S\u00e4u\u00b7len\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielt der lange Zug bed\u00e4chtig.", "tokens": ["Hielt", "der", "lan\u00b7ge", "Zug", "be\u00b7d\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "An des Altars hoher Schwelle", "tokens": ["An", "des", "Al\u00b7tars", "ho\u00b7her", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Thut ein Grab sich auf mit Grauen,", "tokens": ["Thut", "ein", "Grab", "sich", "auf", "mit", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgeh\u00f6hlt, gespenstig schnelle,", "tokens": ["Aus\u00b7ge\u00b7h\u00f6hlt", ",", "ge\u00b7spens\u00b7tig", "schnel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den wei\u00dfvermummten Frauen.", "tokens": ["Von", "den", "wei\u00df\u00b7ver\u00b7mumm\u00b7ten", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Mit Gewalt sein Weib zu holen,", "tokens": ["Mit", "Ge\u00b7walt", "sein", "Weib", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rafft sich auf im Wahn der Gatte;", "tokens": ["Rafft", "sich", "auf", "im", "Wahn", "der", "Gat\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber unter seinen Sohlen", "tokens": ["A\u00b7ber", "un\u00b7ter", "sei\u00b7nen", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dreht sich jede Marmorplatte.", "tokens": ["Dreht", "sich", "je\u00b7de", "Mar\u00b7mor\u00b7plat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und er sieht die sch\u00f6nen Glieder", "tokens": ["Und", "er", "sieht", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eingesargt in einem Schreine,", "tokens": ["Ein\u00b7ge\u00b7sargt", "in", "ei\u00b7nem", "Schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will hinzu, doch immer wieder", "tokens": ["Will", "hin\u00b7zu", ",", "doch", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PTKVZ", "$,", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwanken unter ihm die Steine.", "tokens": ["Schwan\u00b7ken", "un\u00b7ter", "ihm", "die", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Und der Schaufeln Ton verstummet,", "tokens": ["Und", "der", "Schau\u00b7feln", "Ton", "ver\u00b7stum\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stille wird's im Gotteshause,", "tokens": ["Stil\u00b7le", "wird's", "im", "Got\u00b7tes\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Glocke, wenn sie brummet,", "tokens": ["Nur", "die", "Glo\u00b7cke", ",", "wenn", "sie", "brum\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterbricht die tiefe Pause.", "tokens": ["Un\u00b7ter\u00b7bricht", "die", "tie\u00b7fe", "Pau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Und das Dunkel weicht, die Sonne", "tokens": ["Und", "das", "Dun\u00b7kel", "weicht", ",", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt am Horizont sich steiler,", "tokens": ["Hebt", "am", "Ho\u00b7ri\u00b7zont", "sich", "stei\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Man entdeckt das Grab der Nonne,", "tokens": ["Man", "ent\u00b7deckt", "das", "Grab", "der", "Non\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den K\u00f6nig todt am Pfeiler.", "tokens": ["Und", "den", "K\u00f6\u00b7nig", "todt", "am", "Pfei\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}