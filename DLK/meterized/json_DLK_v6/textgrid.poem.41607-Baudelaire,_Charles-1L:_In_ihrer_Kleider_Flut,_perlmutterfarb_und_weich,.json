{"textgrid.poem.41607": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: In ihrer Kleider Flut, perlmutterfarb und weich,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ihrer Kleider Flut, perlmutterfarb und weich,", "tokens": ["In", "ih\u00b7rer", "Klei\u00b7der", "Flut", ",", "perl\u00b7mut\u00b7ter\u00b7farb", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scheint es, da\u00df selbst das Gehn zum Tanze sie gestaltet,", "tokens": ["Scheint", "es", ",", "da\u00df", "selbst", "das", "Gehn", "zum", "Tan\u00b7ze", "sie", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den langen Schlangen der geweihten Gaukler gleich", "tokens": ["Den", "lan\u00b7gen", "Schlan\u00b7gen", "der", "ge\u00b7weih\u00b7ten", "Gauk\u00b7ler", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich ringelnd um den Stab, der ihrer K\u00fcnste waltet.", "tokens": ["Sich", "rin\u00b7gelnd", "um", "den", "Stab", ",", "der", "ih\u00b7rer", "K\u00fcns\u00b7te", "wal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem \u00f6den Sand gleich und des W\u00fcstenhimmels Glut,", "tokens": ["Dem", "\u00f6\u00b7den", "Sand", "gleich", "und", "des", "W\u00fcs\u00b7ten\u00b7him\u00b7mels", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr jedes Mitgef\u00fchl des Menschenleids erkaltet;", "tokens": ["F\u00fcr", "je\u00b7des", "Mit\u00b7ge\u00b7f\u00fchl", "des", "Men\u00b7schen\u00b7leids", "er\u00b7kal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schau, wie gleich dem Gewog der schaumgekr\u00f6nten Flut", "tokens": ["Schau", ",", "wie", "gleich", "dem", "Ge\u00b7wog", "der", "schaum\u00b7ge\u00b7kr\u00f6n\u00b7ten", "Flut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "In tr\u00e4ger Ruhe sie gleichg\u00fcltig sich entfaltet!", "tokens": ["In", "tr\u00e4\u00b7ger", "Ru\u00b7he", "sie", "gleich\u00b7g\u00fcl\u00b7tig", "sich", "ent\u00b7fal\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Augen Schimmer ist von kaltem Mineral.", "tokens": ["Der", "Au\u00b7gen", "Schim\u00b7mer", "ist", "von", "kal\u00b7tem", "Mi\u00b7ne\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem seltsamen Gesch\u00f6pfe will uns scheinen,", "tokens": ["In", "die\u00b7sem", "selt\u00b7sa\u00b7men", "Ge\u00b7sch\u00f6p\u00b7fe", "will", "uns", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df reiner Engel und antike Sphinx sich einen.", "tokens": ["Da\u00df", "rei\u00b7ner", "En\u00b7gel", "und", "an\u00b7ti\u00b7ke", "Sphinx", "sich", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJA", "NN", "PRF", "ART", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Von ihr, die nichts als Gold, Licht, Diamant und Stahl,", "tokens": ["Von", "ihr", ",", "die", "nichts", "als", "Gold", ",", "Licht", ",", "Di\u00b7a\u00b7mant", "und", "Stahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PIS", "KOKOM", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gl\u00e4nzt, unn\u00fctz wie ein Stern im fernen \u00c4therblauen,", "tokens": ["Gl\u00e4nzt", ",", "un\u00b7n\u00fctz", "wie", "ein", "Stern", "im", "fer\u00b7nen", "\u00c4\u00b7ther\u00b7blau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die kalte Majest\u00e4t der unfruchtbaren Frauen.", "tokens": ["Die", "kal\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", "der", "un\u00b7frucht\u00b7ba\u00b7ren", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}