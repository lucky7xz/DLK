{"textgrid.poem.42472": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lustig fort", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lustig fort", "tokens": ["Lus\u00b7tig", "fort"], "token_info": ["word", "word"], "pos": ["NE", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Von Ort zu Ort", "tokens": ["Von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Habt ihr uns geblasen.", "tokens": ["Habt", "ihr", "uns", "ge\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Trommelschlag,", "tokens": ["Trom\u00b7mel\u00b7schlag", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "H\u00f6rnerklang,", "tokens": ["H\u00f6r\u00b7ner\u00b7klang", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Klingt auf allen Stra\u00dfen.", "tokens": ["Klingt", "auf", "al\u00b7len", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Gut Quartier,", "tokens": ["Gut", "Quar\u00b7tier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Junge Maid,", "tokens": ["Jun\u00b7ge", "Maid", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sind uns oft gekommen.", "tokens": ["Sind", "uns", "oft", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Abends dann", "tokens": ["A\u00b7bends", "dann"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Zapfenstreich,", "tokens": ["Zap\u00b7fen\u00b7streich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Hat den Dienst genommen.", "tokens": ["Hat", "den", "Dienst", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Mondes Licht,", "tokens": ["Mon\u00b7des", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Blauer Duft,", "tokens": ["Blau\u00b7er", "Duft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und versteckte Lauben.", "tokens": ["Und", "ver\u00b7steck\u00b7te", "Lau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nachtigall,", "tokens": ["Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nu\u00dfbaumstrauch;", "tokens": ["Nu\u00df\u00b7baum\u00b7strauch", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Liebe l\u00e4\u00dft sich rauben.", "tokens": ["Lie\u00b7be", "l\u00e4\u00dft", "sich", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Doch ganz fr\u00fch", "tokens": ["Doch", "ganz", "fr\u00fch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Sind wir schon", "tokens": ["Sind", "wir", "schon"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Fern auf andern Wegen.", "tokens": ["Fern", "auf", "an\u00b7dern", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sommerglut,", "tokens": ["Som\u00b7mer\u00b7glut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Bl\u00fctenbaum,", "tokens": ["Bl\u00fc\u00b7ten\u00b7baum", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Oft auch Staub und Regen.", "tokens": ["Oft", "auch", "Staub", "und", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Weit, ach weit,", "tokens": ["Weit", ",", "ach", "weit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Weltenweit", "tokens": ["Wel\u00b7ten\u00b7weit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "H\u00f6r ich es noch klingen:", "tokens": ["H\u00f6r", "ich", "es", "noch", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kalbfell dr\u00f6hnt,", "tokens": ["Kalb\u00b7fell", "dr\u00f6hnt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Fl\u00f6te gellt,", "tokens": ["Fl\u00f6\u00b7te", "gellt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und Soldaten singen.", "tokens": ["Und", "Sol\u00b7da\u00b7ten", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}