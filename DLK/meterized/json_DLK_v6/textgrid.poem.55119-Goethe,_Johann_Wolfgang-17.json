{"textgrid.poem.55119": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "17", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Worte sind es, kurz, bequem zu sagen,", "tokens": ["Zwei", "Wor\u00b7te", "sind", "es", ",", "kurz", ",", "be\u00b7quem", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wir so oft mit holder Freude nennen,", "tokens": ["Die", "wir", "so", "oft", "mit", "hol\u00b7der", "Freu\u00b7de", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch keineswegs die Dinge deutlich kennen,", "tokens": ["Doch", "kei\u00b7nes\u00b7wegs", "die", "Din\u00b7ge", "deut\u00b7lich", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wovon sie eigentlich den Stempel tragen.", "tokens": ["Wo\u00b7von", "sie", "ei\u00b7gent\u00b7lich", "den", "Stem\u00b7pel", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es tut gar wohl in jung und alten Tagen,", "tokens": ["Es", "tut", "gar", "wohl", "in", "jung", "und", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eins an dem andern kecklich zu verbrennen;", "tokens": ["Eins", "an", "dem", "an\u00b7dern", "keck\u00b7lich", "zu", "ver\u00b7bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kann man sie vereint zusammen nennen,", "tokens": ["Und", "kann", "man", "sie", "ver\u00b7eint", "zu\u00b7sam\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So dr\u00fcckt man aus ein seliges Behagen.", "tokens": ["So", "dr\u00fcckt", "man", "aus", "ein", "se\u00b7li\u00b7ges", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun aber such ich ihnen zu gefallen", "tokens": ["Nun", "a\u00b7ber", "such", "ich", "ih\u00b7nen", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bitte, mit sich selbst mich zu begl\u00fccken;", "tokens": ["Und", "bit\u00b7te", ",", "mit", "sich", "selbst", "mich", "zu", "be\u00b7gl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "APPR", "PRF", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hoffe still, doch hoff ich's zu erlangen:", "tokens": ["Ich", "hof\u00b7fe", "still", ",", "doch", "hoff", "ich's", "zu", "er\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als Namen der Geliebten sie zu lallen,", "tokens": ["Als", "Na\u00b7men", "der", "Ge\u00b7lieb\u00b7ten", "sie", "zu", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}}}}