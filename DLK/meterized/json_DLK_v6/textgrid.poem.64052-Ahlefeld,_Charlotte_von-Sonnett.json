{"textgrid.poem.64052": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnett", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als mir, von goldner Freiheit noch umfangen,", "tokens": ["Als", "mir", ",", "von", "gold\u00b7ner", "Frei\u00b7heit", "noch", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Daseyns F\u00fclle bl\u00fchend sich erschloss,", "tokens": ["Des", "Da\u00b7seyns", "F\u00fcl\u00b7le", "bl\u00fc\u00b7hend", "sich", "er\u00b7schloss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da war's ein dunkles, heiliges Verlangen,", "tokens": ["Da", "wa\u00b7r's", "ein", "dunk\u00b7les", ",", "hei\u00b7li\u00b7ges", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das \u00fcber mich der Sehnsucht Flammen goss.", "tokens": ["Das", "\u00fc\u00b7ber", "mich", "der", "Sehn\u00b7sucht", "Flam\u00b7men", "goss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "ART", "NN", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da blickt ich froh und k\u00fchn in die Gefilde", "tokens": ["Da", "blickt", "ich", "froh", "und", "k\u00fchn", "in", "die", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Zukunft hin, von Morgenroth begl\u00e4nzt;", "tokens": ["Der", "Zu\u00b7kunft", "hin", ",", "von", "Mor\u00b7gen\u00b7roth", "be\u00b7gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben schien in ungetr\u00fcbter Milde", "tokens": ["Das", "Le\u00b7ben", "schien", "in", "un\u00b7ge\u00b7tr\u00fcb\u00b7ter", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von der Natur mir tausendfach umkr\u00e4nzt.", "tokens": ["Von", "der", "Na\u00b7tur", "mir", "tau\u00b7send\u00b7fach", "um\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch \u2013 von allen Bl\u00fcthen, die es schm\u00fccken,", "tokens": ["Und", "doch", "\u2013", "von", "al\u00b7len", "Bl\u00fc\u00b7then", ",", "die", "es", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von allen Freuden, die das Herz begl\u00fccken,", "tokens": ["Von", "al\u00b7len", "Freu\u00b7den", ",", "die", "das", "Herz", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verdient nur eine, dass man sie beweine.", "tokens": ["Ver\u00b7di\u00b7ent", "nur", "ei\u00b7ne", ",", "dass", "man", "sie", "be\u00b7wei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Es ist das s\u00fcsse, trunkene Entz\u00fccken,", "tokens": ["Es", "ist", "das", "s\u00fcs\u00b7se", ",", "trun\u00b7ke\u00b7ne", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das nur durch Schweigen wagt sich auszudr\u00fccken", "tokens": ["Das", "nur", "durch", "Schwei\u00b7gen", "wagt", "sich", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "PRF", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In stummer Liebe seeligem Vereine.", "tokens": ["In", "stum\u00b7mer", "Lie\u00b7be", "see\u00b7li\u00b7gem", "Ver\u00b7ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}