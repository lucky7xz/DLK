{"textgrid.poem.37851": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Friedenslied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe, liebe Friedenstaube,", "tokens": ["S\u00fc\u00b7\u00dfe", ",", "lie\u00b7be", "Frie\u00b7dens\u00b7tau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du schnell den Oehlzweig bringst,", "tokens": ["Die", "du", "schnell", "den", "O\u00b7ehl\u00b7zweig", "bringst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wenn du vor des Geyers Raube,", "tokens": ["Wenn", "du", "vor", "des", "Ge\u00b7yers", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frey den kleinen Fittig schwingst!", "tokens": ["Frey", "den", "klei\u00b7nen", "Fit\u00b7tig", "schwingst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist es wahr, da\u00df du den Kl\u00fcften", "tokens": ["Ist", "es", "wahr", ",", "da\u00df", "du", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Elends dich entziehst,", "tokens": ["Dei\u00b7nes", "E\u00b7lends", "dich", "ent\u00b7ziehst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von Hoffnung aus den L\u00fcften,", "tokens": ["Und", "von", "Hoff\u00b7nung", "aus", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Froh auf unsre Fluren siehst?", "tokens": ["Froh", "auf", "uns\u00b7re", "Flu\u00b7ren", "siehst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm, verzeuch nicht, la\u00df dich nieder,", "tokens": ["Komm", ",", "ver\u00b7zeuch", "nicht", ",", "la\u00df", "dich", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PTKNEG", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Herzen \u00f6ffnen sich,", "tokens": ["Uns\u00b7re", "Her\u00b7zen", "\u00f6ff\u00b7nen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb uns Fried und Eintracht wieder,", "tokens": ["Gieb", "uns", "Fried", "und", "Ein\u00b7tracht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du findest sie f\u00fcr dich.", "tokens": ["Und", "du", "fin\u00b7dest", "sie", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df das holde Zweiglein fallen!", "tokens": ["La\u00df", "das", "hol\u00b7de", "Zwei\u00b7glein", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn, sobald es Wurzeln schl\u00e4gt,", "tokens": ["Denn", ",", "so\u00b7bald", "es", "Wur\u00b7zeln", "schl\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sehn wir Heil und Wohlgefallen", "tokens": ["Sehn", "wir", "Heil", "und", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den Fr\u00fcchten, die es tr\u00e4gt.", "tokens": ["In", "den", "Fr\u00fcch\u00b7ten", ",", "die", "es", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo es bl\u00fcht, t\u00f6nt durch die W\u00e4lder", "tokens": ["Wo", "es", "bl\u00fcht", ",", "t\u00f6nt", "durch", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kein entheilgend Beil zum Fall,", "tokens": ["Kein", "en\u00b7theil\u00b7gend", "Beil", "zum", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die saatenreiche Felder", "tokens": ["Und", "die", "saa\u00b7ten\u00b7rei\u00b7che", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Th\u00fcrmt kein Spat zu Schanz und Wall.", "tokens": ["Th\u00fcrmt", "kein", "Spat", "zu", "Schanz", "und", "Wall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NE", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fc\u00dfe Fr\u00fchlingsbl\u00fcmchen sprie\u00dfen,", "tokens": ["S\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7bl\u00fcm\u00b7chen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unzertreten, vor uns auf,", "tokens": ["Un\u00b7zer\u00b7tre\u00b7ten", ",", "vor", "uns", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die B\u00e4che, die hier flie\u00dfen,", "tokens": ["Und", "die", "B\u00e4\u00b7che", ",", "die", "hier", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00e4rbt kein Blut in ihrem Lauf.", "tokens": ["F\u00e4rbt", "kein", "Blut", "in", "ih\u00b7rem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schmachtend seufzt nach seinem Schatten,", "tokens": ["Schmach\u00b7tend", "seufzt", "nach", "sei\u00b7nem", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das von Gram versenkte Gl\u00fcck,", "tokens": ["Das", "von", "Gram", "ver\u00b7senk\u00b7te", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zarten M\u00fcttern, treuen Gatten,", "tokens": ["Zar\u00b7ten", "M\u00fct\u00b7tern", ",", "treu\u00b7en", "Gat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt er ihren Wunsch zur\u00fcck;", "tokens": ["Bringt", "er", "ih\u00b7ren", "Wunsch", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "V\u00e4ter, vaterlosen Kleinen,", "tokens": ["V\u00e4\u00b7ter", ",", "va\u00b7ter\u00b7lo\u00b7sen", "Klei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den J\u00fcngling seiner Braut;", "tokens": ["Und", "den", "J\u00fcng\u00b7ling", "sei\u00b7ner", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle, wo sie ja noch weinen,", "tokens": ["Al\u00b7le", ",", "wo", "sie", "ja", "noch", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weinen vor Entz\u00fccken laut.", "tokens": ["Wei\u00b7nen", "vor", "Ent\u00b7z\u00fc\u00b7cken", "laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun, du holde Friedenstaube!", "tokens": ["Nun", ",", "du", "hol\u00b7de", "Frie\u00b7dens\u00b7tau\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du uns den Oehlzweig bringst,", "tokens": ["Die", "du", "uns", "den", "O\u00b7ehl\u00b7zweig", "bringst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.3": {"text": "Wenn du vor des Geyers Raube,", "tokens": ["Wenn", "du", "vor", "des", "Ge\u00b7yers", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frey den kleinen Fittig schwingst!", "tokens": ["Frey", "den", "klei\u00b7nen", "Fit\u00b7tig", "schwingst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm, verzeuch nicht! La\u00df dich nieder!", "tokens": ["Komm", ",", "ver\u00b7zeuch", "nicht", "!", "La\u00df", "dich", "nie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PTKNEG", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Herzen \u00f6ffnen sich,", "tokens": ["Uns\u00b7re", "Her\u00b7zen", "\u00f6ff\u00b7nen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gib der Welt den Frieden wieder,", "tokens": ["Gib", "der", "Welt", "den", "Frie\u00b7den", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und nimm ihn dann auch f\u00fcr dich.", "tokens": ["Und", "nimm", "ihn", "dann", "auch", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}