{"textgrid.poem.46494": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Vater- und Muttergeist geformt und beschworen,", "genre": "verse", "period": "N.A.", "pub_year": 1917, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Vater- und Muttergeist geformt und beschworen,", "tokens": ["Vom", "Va\u00b7ter", "und", "Mut\u00b7ter\u00b7geist", "ge\u00b7formt", "und", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "NN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus Nachtmeer und Schweigen, aus Wolkenfall-Schicht,", "tokens": ["Aus", "Nacht\u00b7meer", "und", "Schwei\u00b7gen", ",", "aus", "Wol\u00b7ken\u00b7fall\u00b7Schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Aus dem Scho\u00dfe des ewigen Kreisens geboren:", "tokens": ["Aus", "dem", "Scho\u00b7\u00dfe", "des", "e\u00b7wi\u00b7gen", "Krei\u00b7sens", "ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Fiel ich aufschluchzend stirnlings ins Licht.", "tokens": ["Fiel", "ich", "auf\u00b7schluch\u00b7zend", "stirn\u00b7lings", "ins", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Nun bin ich erdereich und bin auch arm:", "tokens": ["Nun", "bin", "ich", "er\u00b7de\u00b7reich", "und", "bin", "auch", "arm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich halte Kiesel pr\u00fcfend in der Hand,", "tokens": ["Ich", "hal\u00b7te", "Kie\u00b7sel", "pr\u00fc\u00b7fend", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Schuh tr\u00e4gt Staub von Werk und Land,", "tokens": ["Mein", "Schuh", "tr\u00e4gt", "Staub", "von", "Werk", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Weib ist mir Gef\u00e4hrtin, stark und warm.", "tokens": ["Ein", "Weib", "ist", "mir", "Ge\u00b7f\u00e4hr\u00b7tin", ",", "stark", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Volk umsp\u00fclt mich breit im Schreiten \u2013", "tokens": ["Und", "Volk", "um\u00b7sp\u00fclt", "mich", "breit", "im", "Schrei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Brot und Milch ist mir nicht mehr als Gl\u00fcck und Harm", "tokens": ["Doch", "Brot", "und", "Milch", "ist", "mir", "nicht", "mehr", "als", "Gl\u00fcck", "und", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Schlacht und Schlaf nicht mehr als Stundengleiten.", "tokens": ["Und", "Schlacht", "und", "Schlaf", "nicht", "mehr", "als", "Stun\u00b7deng\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es schwillt der Tag und summt mein Blut,", "tokens": ["Es", "schwillt", "der", "Tag", "und", "summt", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was w\u00e4re mir nicht atemnah und h\u00e4uslich gut \u2013", "tokens": ["Was", "w\u00e4\u00b7re", "mir", "nicht", "a\u00b7tem\u00b7nah", "und", "h\u00e4us\u00b7lich", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum nun immer meine Sehnsucht treibt", "tokens": ["Wa\u00b7rum", "nun", "im\u00b7mer", "mei\u00b7ne", "Sehn\u00b7sucht", "treibt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nach den Fl\u00fcgeln alle Sinne st\u00fcrzen?", "tokens": ["Und", "nach", "den", "Fl\u00fc\u00b7geln", "al\u00b7le", "Sin\u00b7ne", "st\u00fcr\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu und zu und niemals schlafend bleibt \u2013", "tokens": ["Zu", "und", "zu", "und", "nie\u00b7mals", "schla\u00b7fend", "bleibt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "KON", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie nutzlos Tag und Leben sich verk\u00fcrzen \u2013", "tokens": ["Wie", "nutz\u00b7los", "Tag", "und", "Le\u00b7ben", "sich", "ver\u00b7k\u00fcr\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "KON", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Brich auf, flieg auf mit hundert Segelwinden!", "tokens": ["Brich", "auf", ",", "flieg", "auf", "mit", "hun\u00b7dert", "Se\u00b7gel\u00b7win\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "VVFIN", "APPR", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Einmal mu\u00dft Du die Heimat wiederfinden,", "tokens": ["Ein\u00b7mal", "mu\u00dft", "Du", "die", "Hei\u00b7mat", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Daraus man Dich ins Leben schuf!", "tokens": ["Da\u00b7raus", "man", "Dich", "ins", "Le\u00b7ben", "schuf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es w\u00f6lbt vom Anfang sich der Vater-Ruf", "tokens": ["Es", "w\u00f6lbt", "vom", "An\u00b7fang", "sich", "der", "Va\u00b7ter\u00b7Ruf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und gro\u00df von Mutterewigkeit umf\u00e4chelt", "tokens": ["Und", "gro\u00df", "von", "Mut\u00b7te\u00b7re\u00b7wig\u00b7keit", "um\u00b7f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Singt Deine sph\u00e4renferne Seele jugendlich", "tokens": ["Singt", "Dei\u00b7ne", "sph\u00e4\u00b7ren\u00b7fer\u00b7ne", "See\u00b7le", "ju\u00b7gend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und singt erl\u00f6st und gl\u00e4nzt und l\u00e4chelt", "tokens": ["Und", "singt", "er\u00b7l\u00f6st", "und", "gl\u00e4nzt", "und", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und wartet nur auf Dich.", "tokens": ["Und", "war\u00b7tet", "nur", "auf", "Dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}