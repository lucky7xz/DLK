{"textgrid.poem.50984": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied an die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcll ein mich in die gr\u00fcnen Decken,", "tokens": ["H\u00fcll", "ein", "mich", "in", "die", "gr\u00fc\u00b7nen", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinem S\u00e4useln sing mich ein,", "tokens": ["Mit", "dei\u00b7nem", "S\u00e4u\u00b7seln", "sing", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei guter Zeit magst du mich wecken", "tokens": ["Bei", "gu\u00b7ter", "Zeit", "magst", "du", "mich", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deines Tages jungem Schein!", "tokens": ["Mit", "dei\u00b7nes", "Ta\u00b7ges", "jun\u00b7gem", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab mich m\u00fcd in dir ergangen,", "tokens": ["Ich", "hab", "mich", "m\u00fcd", "in", "dir", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Aug ist matt von deiner Pracht;", "tokens": ["Mein", "Aug", "ist", "matt", "von", "dei\u00b7ner", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun ist mein einziges Verlangen,", "tokens": ["Nun", "ist", "mein", "ein\u00b7zi\u00b7ges", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Im Traum zu ruhn, in deiner Nacht.", "tokens": ["Im", "Traum", "zu", "ruhn", ",", "in", "dei\u00b7ner", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Kinderauges freudig Leuchten", "tokens": ["Des", "Kin\u00b7der\u00b7au\u00b7ges", "freu\u00b7dig", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon fingest du mit Blumen ein,", "tokens": ["Schon", "fin\u00b7gest", "du", "mit", "Blu\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wollte junger Gram es feuchten,", "tokens": ["Und", "woll\u00b7te", "jun\u00b7ger", "Gram", "es", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du scheuchtest ihn mit buntem Schein.", "tokens": ["Du", "scheuch\u00b7test", "ihn", "mit", "bun\u00b7tem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob wildes Hassen, ma\u00dflos Lieben", "tokens": ["Ob", "wil\u00b7des", "Has\u00b7sen", ",", "ma\u00df\u00b7los", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich zeither auch gefangen nahm:", "tokens": ["Mich", "zeit\u00b7her", "auch", "ge\u00b7fan\u00b7gen", "nahm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch immer bin ich Kind geblieben,", "tokens": ["Doch", "im\u00b7mer", "bin", "ich", "Kind", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ich zu dir ins Freie kam!", "tokens": ["Wenn", "ich", "zu", "dir", "ins", "Frei\u00b7e", "kam", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geliebte, die mit ew'ger Treue", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "die", "mit", "ew'\u00b7ger", "Treu\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ew'ger Jugend mich erquickt,", "tokens": ["Und", "ew'\u00b7ger", "Ju\u00b7gend", "mich", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du einz'ge Lust, die ohne Reue", "tokens": ["Du", "einz'\u00b7ge", "Lust", ",", "die", "oh\u00b7ne", "Reu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ohne Nachweh mich entz\u00fcckt \u2013", "tokens": ["Und", "oh\u00b7ne", "Nach\u00b7weh", "mich", "ent\u00b7z\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sollt ich dir jemals untreu werden,", "tokens": ["Sollt", "ich", "dir", "je\u00b7mals", "un\u00b7treu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich kalt vergessen, ohne Dank,", "tokens": ["Dich", "kalt", "ver\u00b7ges\u00b7sen", ",", "oh\u00b7ne", "Dank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann ist mein Fall genaht auf Erden,", "tokens": ["Dann", "ist", "mein", "Fall", "ge\u00b7naht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz verdorben oder krank!", "tokens": ["Mein", "Herz", "ver\u00b7dor\u00b7ben", "o\u00b7der", "krank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O steh mir immerdar im R\u00fccken,", "tokens": ["O", "steh", "mir", "im\u00b7mer\u00b7dar", "im", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lieg ich im Feld mit meiner Zeit!", "tokens": ["Lieg", "ich", "im", "Feld", "mit", "mei\u00b7ner", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen warmen Mutterblicken", "tokens": ["Mit", "dei\u00b7nen", "war\u00b7men", "Mut\u00b7ter\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruh auf mir, auch im sch\u00e4rfsten Streit!", "tokens": ["Ruh", "auf", "mir", ",", "auch", "im", "sch\u00e4rfs\u00b7ten", "Streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sollte mich das Ende finden,", "tokens": ["Und", "soll\u00b7te", "mich", "das", "En\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schnell decke mich mit Rasen zu;", "tokens": ["Schnell", "de\u00b7cke", "mich", "mit", "Ra\u00b7sen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O selig Sterben und Verschwinden", "tokens": ["O", "se\u00b7lig", "Ster\u00b7ben", "und", "Ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "In deiner stillen Herbergsruh!", "tokens": ["In", "dei\u00b7ner", "stil\u00b7len", "Her\u00b7bergs\u00b7ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}