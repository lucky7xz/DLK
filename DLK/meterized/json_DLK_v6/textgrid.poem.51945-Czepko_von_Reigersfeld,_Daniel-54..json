{"textgrid.poem.51945": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abgrund ist das Nicht. Das Nicht ist dann die Sucht,", "tokens": ["Der", "Ab\u00b7grund", "ist", "das", "Nicht", ".", "Das", "Nicht", "ist", "dann", "die", "Sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "PTKNEG", "$.", "PDS", "PTKNEG", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Draus kam die Welt und ich: Gott hat es blo\u00df vermocht.", "tokens": ["Draus", "kam", "die", "Welt", "und", "ich", ":", "Gott", "hat", "es", "blo\u00df", "ver\u00b7mocht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "KON", "PPER", "$.", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}