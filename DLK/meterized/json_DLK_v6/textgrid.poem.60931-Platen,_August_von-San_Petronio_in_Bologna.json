{"textgrid.poem.60931": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "San Petronio in Bologna", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist gotische Kunst; doch ohne belastende Schn\u00f6rkel:", "tokens": ["Dies", "ist", "go\u00b7ti\u00b7sche", "Kunst", ";", "doch", "oh\u00b7ne", "be\u00b7las\u00b7ten\u00b7de", "Schn\u00f6r\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$.", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Geistiger Schwung hat hier Massen und Schwere besiegt.", "tokens": ["Geis\u00b7ti\u00b7ger", "Schwung", "hat", "hier", "Mas\u00b7sen", "und", "Schwe\u00b7re", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}