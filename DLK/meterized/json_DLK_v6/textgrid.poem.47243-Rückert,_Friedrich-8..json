{"textgrid.poem.47243": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah den Himmel seltsam geteilt", "tokens": ["Ich", "sah", "den", "Him\u00b7mel", "selt\u00b7sam", "ge\u00b7teilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In Tr\u00fcbes und in Klares;", "tokens": ["In", "Tr\u00fc\u00b7bes", "und", "in", "Kla\u00b7res", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Alles Dunkle zusammengeeilt", "tokens": ["Al\u00b7les", "Dunk\u00b7le", "zu\u00b7sam\u00b7men\u00b7ge\u00b7eilt"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVPP"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und alles Helle war es.", "tokens": ["Und", "al\u00b7les", "Hel\u00b7le", "war", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ganze nordische H\u00e4lfte war", "tokens": ["Die", "gan\u00b7ze", "nor\u00b7di\u00b7sche", "H\u00e4lf\u00b7te", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit grauem Gew\u00f6lk umzogen,", "tokens": ["Mit", "grau\u00b7em", "Ge\u00b7w\u00f6lk", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die ganze s\u00fcdliche H\u00e4lfte klar", "tokens": ["Die", "gan\u00b7ze", "s\u00fcd\u00b7li\u00b7che", "H\u00e4lf\u00b7te", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Lichtblau angeflogen.", "tokens": ["Von", "Licht\u00b7blau", "an\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonne stand an dem Scheiderand", "tokens": ["Die", "Son\u00b7ne", "stand", "an", "dem", "Schei\u00b7de\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Blauen und des Grauen,", "tokens": ["Des", "Blau\u00b7en", "und", "des", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unsichtbar halb in der Wolkenwand", "tokens": ["Un\u00b7sicht\u00b7bar", "halb", "in", "der", "Wol\u00b7ken\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und halb im Hellen zu schauen.", "tokens": ["Und", "halb", "im", "Hel\u00b7len", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wird sie treten ins Blaue ganz,", "tokens": ["Wird", "sie", "tre\u00b7ten", "ins", "Blau\u00b7e", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPRART", "ADJA", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ins Graue ganz verschwinden?", "tokens": ["Ins", "Grau\u00b7e", "ganz", "ver\u00b7schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Oder wird sie mit ihrem Glanz", "tokens": ["O\u00b7der", "wird", "sie", "mit", "ih\u00b7rem", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Selbst das Grau \u00fcberwinden?", "tokens": ["Selbst", "das", "Grau", "\u00fc\u00b7berw\u00b7in\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}