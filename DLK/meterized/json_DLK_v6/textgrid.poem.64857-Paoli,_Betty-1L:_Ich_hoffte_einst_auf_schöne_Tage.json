{"textgrid.poem.64857": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hoffte einst auf sch\u00f6ne Tage", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hoffte einst auf sch\u00f6ne Tage", "tokens": ["Ich", "hoff\u00b7te", "einst", "auf", "sch\u00f6\u00b7ne", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lauschte mit erschloss'ner Brust", "tokens": ["Und", "lauschte", "mit", "er\u00b7schloss'\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der m\u00e4hrchenhaften Wundersage", "tokens": ["Der", "m\u00e4hr\u00b7chen\u00b7haf\u00b7ten", "Wun\u00b7der\u00b7sa\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ewig heitrer Liebeslust.", "tokens": ["Von", "e\u00b7wig", "hei\u00b7trer", "Lie\u00b7bes\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In jugendfrohem Uebermuthe", "tokens": ["In", "ju\u00b7gend\u00b7fro\u00b7hem", "Ue\u00b7ber\u00b7mu\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Glaubt' ich von jedem Gl\u00fcck und Gute,", "tokens": ["Glaubt'", "ich", "von", "je\u00b7dem", "Gl\u00fcck", "und", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df es mir zugewiesen sei \u2013", "tokens": ["Da\u00df", "es", "mir", "zu\u00b7ge\u00b7wie\u00b7sen", "sei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist vorbei!", "tokens": ["Es", "ist", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und als der fromme Wahn entschwunden,", "tokens": ["Und", "als", "der", "from\u00b7me", "Wahn", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da fleht' ich, stolz auf meine Qual:", "tokens": ["Da", "fleht'", "ich", ",", "stolz", "auf", "mei\u00b7ne", "Qual", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleibt ewig offen, meine Wunden,", "tokens": ["Bleibt", "e\u00b7wig", "of\u00b7fen", ",", "mei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als unverg\u00e4nglich Liebesmahl.", "tokens": ["Als", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Lie\u00b7bes\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mu\u00dften Freud und Gl\u00fcck verwehen,", "tokens": ["Und", "mu\u00df\u00b7ten", "Freud", "und", "Gl\u00fcck", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So soll mein heil'ger Schmerz bestehen,", "tokens": ["So", "soll", "mein", "heil'\u00b7ger", "Schmerz", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df Eines doch unsterblich sei \u2013 \u2013", "tokens": ["Da\u00df", "Ei\u00b7nes", "doch", "uns\u00b7terb\u00b7lich", "sei", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VAFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist vorbei!", "tokens": ["Es", "ist", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}