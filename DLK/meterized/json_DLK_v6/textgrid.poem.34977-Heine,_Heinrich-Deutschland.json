{"textgrid.poem.34977": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Deutschland", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland ist noch ein kleines Kind,", "tokens": ["Deutschland", "ist", "noch", "ein", "klei\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Sonne ist seine Amme;", "tokens": ["Doch", "die", "Son\u00b7ne", "ist", "sei\u00b7ne", "Am\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sie s\u00e4ugt es nicht mit stiller Milch,", "tokens": ["Sie", "s\u00e4ugt", "es", "nicht", "mit", "stil\u00b7ler", "Milch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie s\u00e4ugt es mit wilder Flamme.", "tokens": ["Sie", "s\u00e4ugt", "es", "mit", "wil\u00b7der", "Flam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bei solcher Nahrung w\u00e4chst man schnell", "tokens": ["Bei", "sol\u00b7cher", "Nah\u00b7rung", "w\u00e4chst", "man", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kocht das Blut in den Adern.", "tokens": ["Und", "kocht", "das", "Blut", "in", "den", "A\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Nachbarskinder, h\u00fctet euch,", "tokens": ["Ihr", "Nach\u00b7bars\u00b7kin\u00b7der", ",", "h\u00fc\u00b7tet", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem jungen Burschen zu hadern!", "tokens": ["Mit", "dem", "jun\u00b7gen", "Bur\u00b7schen", "zu", "ha\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist ein t\u00e4ppisches Rieselein,", "tokens": ["Es", "ist", "ein", "t\u00e4p\u00b7pi\u00b7sches", "Rie\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rei\u00dft aus dem Boden die Eiche,", "tokens": ["Rei\u00dft", "aus", "dem", "Bo\u00b7den", "die", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und schl\u00e4gt euch damit den R\u00fccken wund", "tokens": ["Und", "schl\u00e4gt", "euch", "da\u00b7mit", "den", "R\u00fc\u00b7cken", "wund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ART", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die K\u00f6pfe windelweiche.", "tokens": ["Und", "die", "K\u00f6p\u00b7fe", "win\u00b7del\u00b7wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem Siegfried gleicht er, dem edlen Fant,", "tokens": ["Dem", "Sieg\u00b7fried", "gleicht", "er", ",", "dem", "ed\u00b7len", "Fant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von dem wir singen und sagen;", "tokens": ["Von", "dem", "wir", "sin\u00b7gen", "und", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der hat, nachdem er geschmiedet sein Schwert,", "tokens": ["Der", "hat", ",", "nach\u00b7dem", "er", "ge\u00b7schmie\u00b7det", "sein", "Schwert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "PPER", "VVPP", "VAINF", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Ambo\u00df entzweigeschlagen!", "tokens": ["Den", "Am\u00b7bo\u00df", "ent\u00b7zwei\u00b7ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, du wirst einst wie Siegfried sein", "tokens": ["Ja", ",", "du", "wirst", "einst", "wie", "Sieg\u00b7fried", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "KOKOM", "NE", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und t\u00f6ten den h\u00e4\u00dflichen Drachen,", "tokens": ["Und", "t\u00f6\u00b7ten", "den", "h\u00e4\u00df\u00b7li\u00b7chen", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Heisa! wie freudig vom Himmel herab", "tokens": ["Hei\u00b7sa", "!", "wie", "freu\u00b7dig", "vom", "Him\u00b7mel", "her\u00b7ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADJD", "APPRART", "NN", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wird deine Frau Amme lachen!", "tokens": ["Wird", "dei\u00b7ne", "Frau", "Am\u00b7me", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Du wirst ihn t\u00f6ten, und seinen Hort,", "tokens": ["Du", "wirst", "ihn", "t\u00f6\u00b7ten", ",", "und", "sei\u00b7nen", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Reichskleinodien, besitzen.", "tokens": ["Die", "Reichs\u00b7klein\u00b7o\u00b7di\u00b7en", ",", "be\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heisa! wie wird auf deinem Haupt", "tokens": ["Hei\u00b7sa", "!", "wie", "wird", "auf", "dei\u00b7nem", "Haupt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die goldne Krone blitzen!", "tokens": ["Die", "gold\u00b7ne", "Kro\u00b7ne", "blit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}