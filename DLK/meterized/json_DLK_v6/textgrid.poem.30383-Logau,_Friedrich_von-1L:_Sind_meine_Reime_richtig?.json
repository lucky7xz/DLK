{"textgrid.poem.30383": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sind meine Reime richtig?", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind meine Reime richtig?", "tokens": ["Sind", "mei\u00b7ne", "Rei\u00b7me", "rich\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind meine Worte wichtig?", "tokens": ["Sind", "mei\u00b7ne", "Wor\u00b7te", "wich\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur da\u00df nicht beydes nichtig!", "tokens": ["Nur", "da\u00df", "nicht", "bey\u00b7des", "nich\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PTKNEG", "PIS", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst sind sie gar nicht t\u00fcchtig.", "tokens": ["Sonst", "sind", "sie", "gar", "nicht", "t\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}