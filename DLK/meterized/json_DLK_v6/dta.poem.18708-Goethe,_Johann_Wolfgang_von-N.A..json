{"dta.poem.18708": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Keinen Reimer wird man finden", "tokens": ["Kei\u00b7nen", "Rei\u00b7mer", "wird", "man", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich nicht den besten hielte,", "tokens": ["Der", "sich", "nicht", "den", "bes\u00b7ten", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Fiedler der nicht lieber", "tokens": ["Kei\u00b7nen", "Fied\u00b7ler", "der", "nicht", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eigne Melodieen spielte.", "tokens": ["Eig\u00b7ne", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "spiel\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Und ich konnte sie nicht tadeln;", "tokens": ["Und", "ich", "konn\u00b7te", "sie", "nicht", "ta\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir andern Ehre geben", "tokens": ["Wenn", "wir", "an\u00b7dern", "Eh\u00b7re", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen wir uns selbst entadeln.", "tokens": ["M\u00fcs\u00b7sen", "wir", "uns", "selbst", "en\u00b7ta\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt man denn wenn andre leben?", "tokens": ["Lebt", "man", "denn", "wenn", "and\u00b7re", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "KOUS", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so fand ich\u2019s denn auch juste", "tokens": ["Und", "so", "fand", "ich's", "denn", "auch", "jus\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In gewissen Antichambern,", "tokens": ["In", "ge\u00b7wis\u00b7sen", "An\u00b7ti\u00b7ch\u00b7am\u00b7bern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wo man nicht zu sondern wusste", "tokens": ["Wo", "man", "nicht", "zu", "son\u00b7dern", "wuss\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PTKNEG", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4usedreck von Koriandern.", "tokens": ["M\u00e4u\u00b7se\u00b7dreck", "von", "Ko\u00b7ri\u00b7an\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}