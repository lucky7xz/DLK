{"textgrid.poem.34400": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es lebt ein Gott, der Sch\u00f6pfer des Weltenrunds,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebt ein Gott, der Sch\u00f6pfer des Weltenrunds,", "tokens": ["Es", "lebt", "ein", "Gott", ",", "der", "Sch\u00f6p\u00b7fer", "des", "Wel\u00b7ten\u00b7runds", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "so sagen sie. \u2013 Doch geben sie Kunde auch,", "tokens": ["so", "sa\u00b7gen", "sie", ".", "\u2013", "Doch", "ge\u00b7ben", "sie", "Kun\u00b7de", "auch", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "KON", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ob von dem Funkeln, das den einen", "tokens": ["ob", "von", "dem", "Fun\u00b7keln", ",", "das", "den", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "ART", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tropfen im Meere des Alls umflimmert,", "tokens": ["Trop\u00b7fen", "im", "Mee\u00b7re", "des", "Alls", "um\u00b7flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "ob er vom Ringen menschlicher Nichtigkeit", "tokens": ["ob", "er", "vom", "Rin\u00b7gen", "menschli\u00b7cher", "Nich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "jemals vernahm? \u2013 All-m\u00e4chtig und -liebevoll", "tokens": ["je\u00b7mals", "ver\u00b7nahm", "?", "\u2013", "All\u00b7m\u00e4ch\u00b7tig", "und"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "$(", "ADJD", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "ist er! Vor seinen Vateraugen", "tokens": ["ist", "er", "!", "Vor", "sei\u00b7nen", "Va\u00b7te\u00b7rau\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "birgt im unendlichen Raum sich Niemand!", "tokens": ["birgt", "im", "un\u00b7end\u00b7li\u00b7chen", "Raum", "sich", "Nie\u00b7mand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PRF", "PIS", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Kein Schmerz ist ihm, kein Jubel der Freude fremd:", "tokens": ["Kein", "Schmerz", "ist", "ihm", ",", "kein", "Ju\u00b7bel", "der", "Freu\u00b7de", "fremd", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PIAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "als Gott der Liebe preisen wir ihn auf Knien!", "tokens": ["als", "Gott", "der", "Lie\u00b7be", "prei\u00b7sen", "wir", "ihn", "auf", "Kni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u2013 So ", "tokens": ["\u2013", "So"], "token_info": ["punct", "word"], "pos": ["$(", "ADV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "nimmer ermessne Jammerw\u00fcste?", "tokens": ["nim\u00b7mer", "er\u00b7mess\u00b7ne", "Jam\u00b7mer\u00b7w\u00fcs\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "des Tiefgemeinen? ", "tokens": ["des", "Tief\u00b7ge\u00b7mei\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "sich w\u00e4lzen Millionen Herzen,", "tokens": ["sich", "w\u00e4l\u00b7zen", "Mil\u00b7lion\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "blutig, gemartert ein langes Leben?", "tokens": ["blu\u00b7tig", ",", "ge\u00b7mar\u00b7tert", "ein", "lan\u00b7ges", "Le\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "die Welt ins wahnlos friedliche Nichts zur\u00fcck?", "tokens": ["die", "Welt", "ins", "wahn\u00b7los", "fried\u00b7li\u00b7che", "Nichts", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Gott \u2013 grausamer w\u00e4r er wahrlich,", "tokens": ["Der", "Gott", "\u2013", "grau\u00b7sa\u00b7mer", "w\u00e4r", "er", "wahr\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "als der verworfenste Menschenbube.", "tokens": ["als", "der", "ver\u00b7wor\u00b7fens\u00b7te", "Men\u00b7schen\u00b7bu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}