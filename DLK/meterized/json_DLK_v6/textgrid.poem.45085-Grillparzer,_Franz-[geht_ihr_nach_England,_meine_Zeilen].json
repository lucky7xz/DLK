{"textgrid.poem.45085": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[geht ihr nach England, meine Zeilen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht ihr nach England, meine Zeilen,", "tokens": ["Geht", "ihr", "nach", "En\u00b7gland", ",", "mei\u00b7ne", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O n\u00e4hmt ihr mich, den Schreiber mit!", "tokens": ["O", "n\u00e4hmt", "ihr", "mich", ",", "den", "Schrei\u00b7ber", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort wo sie schreiten statt zu eilen,", "tokens": ["Dort", "wo", "sie", "schrei\u00b7ten", "statt", "zu", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VVFIN", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch eine Spur l\u00e4\u00dft jeder Tritt.", "tokens": ["Doch", "ei\u00b7ne", "Spur", "l\u00e4\u00dft", "je\u00b7der", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}