{"textgrid.poem.47870": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "In der Person Mariae Magdalenae", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil du von oben blinkest", "tokens": ["Weil", "du", "von", "o\u00b7ben", "blin\u00b7kest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du blasses Schwesterlicht/", "tokens": ["Du", "blas\u00b7ses", "Schwes\u00b7ter\u00b7licht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und mit den H\u00f6rnern winkest", "tokens": ["und", "mit", "den", "H\u00f6r\u00b7nern", "win\u00b7kest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auff mein bleich Angesicht/", "tokens": ["Auff", "mein", "bleich", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach wiltu mir nicht sagen/", "tokens": ["Ach", "wil\u00b7tu", "mir", "nicht", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer mich so sehr betr\u00fcbt/", "tokens": ["Wer", "mich", "so", "sehr", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und den hat weggetragen/", "tokens": ["und", "den", "hat", "weg\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den meine Seele liebt?", "tokens": ["Den", "mei\u00b7ne", "See\u00b7le", "liebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Sternen weil jhr blikket", "tokens": ["Ihr", "Ster\u00b7nen", "weil", "jhr", "blik\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Annoch mit halbem Licht/", "tokens": ["An\u00b7noch", "mit", "hal\u00b7bem", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "und euch zur Reise schikket/", "tokens": ["und", "euch", "zur", "Rei\u00b7se", "schik\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Fall der Tag einbricht/", "tokens": ["Im", "Fall", "der", "Tag", "ein\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach wolt jhr mir nicht sagen/", "tokens": ["Ach", "wolt", "jhr", "mir", "nicht", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was Trost und Labsal gibt/", "tokens": ["Was", "Trost", "und", "Lab\u00b7sal", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie hat man weggetragen", "tokens": ["Wie", "hat", "man", "weg\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den meine Seele liebt?", "tokens": ["Den", "mei\u00b7ne", "See\u00b7le", "liebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Auen/ B\u00fcsch und W\u00e4lder/", "tokens": ["Ihr", "Au\u00b7en", "/", "B\u00fcsch", "und", "W\u00e4l\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der schwangern Erdenlust/", "tokens": ["Der", "schwan\u00b7gern", "Er\u00b7den\u00b7lust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Wiesen/ Saaten/ Felder/", "tokens": ["Ihr", "Wie\u00b7sen", "/", "Saa\u00b7ten", "/", "Fel\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist irgend euch bewust/", "tokens": ["Ist", "ir\u00b7gend", "euch", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn hat man weggetragen", "tokens": ["Wenn", "hat", "man", "weg\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den meine Seele liebt?", "tokens": ["Den", "mei\u00b7ne", "See\u00b7le", "liebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach mindert doch mein Klagen/", "tokens": ["Ach", "min\u00b7dert", "doch", "mein", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das mich kr\u00e4nkt und betr\u00fcbt!", "tokens": ["Das", "mich", "kr\u00e4nkt", "und", "be\u00b7tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Jhr gr\u00fcnen W\u00fcsteneien/", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "W\u00fcs\u00b7te\u00b7nei\u00b7en", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du buntbeblumte Flur/", "tokens": ["Du", "bunt\u00b7be\u00b7blum\u00b7te", "Flur", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo neue Bl\u00fcten schneien/", "tokens": ["Wo", "neu\u00b7e", "Bl\u00fc\u00b7ten", "schnei\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach zeiget mir die Spur!", "tokens": ["Ach", "zei\u00b7get", "mir", "die", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach sagt mir ihr Violen!", "tokens": ["Ach", "sagt", "mir", "ihr", "Vi\u00b7o\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo der sich hingewand/", "tokens": ["Wo", "der", "sich", "hin\u00b7ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PRF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So heinte mir gestolen/", "tokens": ["So", "hein\u00b7te", "mir", "ge\u00b7sto\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In dem mein Hertz entbrand.", "tokens": ["In", "dem", "mein", "Hertz", "ent\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u2013 \u2013 \u2013 Das Wasser rinnet mir", "tokens": ["\u2013", "\u2013", "\u2013", "Das", "Was\u00b7ser", "rin\u00b7net", "mir"], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "$(", "$(", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Au\u00df beyder Augen Qvell ohn Vnterla\u00df herf\u00fcr/", "tokens": ["Au\u00df", "bey\u00b7der", "Au\u00b7gen", "Qvell", "ohn", "Vn\u00b7ter\u00b7la\u00df", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Haut ist Scherbend\u00fcrr/ das Mark schwind in den Knochen/", "tokens": ["Die", "Haut", "ist", "Scher\u00b7ben\u00b7d\u00fcrr", "/", "das", "Mark", "schwind", "in", "den", "Kno\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "$(", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Hertze lebet nur und \u00e4ngstet mich mit Bochen.", "tokens": ["Das", "Hert\u00b7ze", "le\u00b7bet", "nur", "und", "\u00e4ngs\u00b7tet", "mich", "mit", "Bo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "ADV", "KON", "VVFIN", "PRF", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}