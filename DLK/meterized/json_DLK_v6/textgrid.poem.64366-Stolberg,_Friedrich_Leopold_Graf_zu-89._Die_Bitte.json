{"textgrid.poem.64366": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "89. Die Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebt' ich sie mehr, als dich? Ich liebte mehr sie,", "tokens": ["Liebt'", "ich", "sie", "mehr", ",", "als", "dich", "?", "Ich", "lieb\u00b7te", "mehr", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "PPER", "$.", "PPER", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Darum nahmst du sie mir! Den Wonnebecher", "tokens": ["Da\u00b7rum", "nahmst", "du", "sie", "mir", "!", "Den", "Won\u00b7ne\u00b7be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trank ich, dankte, lobte den Geber; liebte", "tokens": ["Trank", "ich", ",", "dank\u00b7te", ",", "lob\u00b7te", "den", "Ge\u00b7ber", ";", "lieb\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Hei\u00dfer die Gabe!", "tokens": ["Hei\u00b7\u00dfer", "die", "Ga\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dennoch erbarmtest mein dich, als sich schlossen,", "tokens": ["Den\u00b7noch", "er\u00b7barm\u00b7test", "mein", "dich", ",", "als", "sich", "schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "PPER", "$,", "KOUS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Meine Himmel auf Erden! ihre Augen!", "tokens": ["Mei\u00b7ne", "Him\u00b7mel", "auf", "Er\u00b7den", "!", "ih\u00b7re", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Als die kalte Lippe des Jammervollen", "tokens": ["Als", "die", "kal\u00b7te", "Lip\u00b7pe", "des", "Jam\u00b7mer\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "K\u00fcsse nicht f\u00fchlte.", "tokens": ["K\u00fcs\u00b7se", "nicht", "f\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wilder Verzweiflung wehrtest du! Du schmeidigst", "tokens": ["Wil\u00b7der", "Ver\u00b7zwei\u00b7flung", "wehr\u00b7test", "du", "!", "Du", "schmei\u00b7digst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Meinen starrenden Gram! Du g\u00fcrtest, kr\u00e4ftigst", "tokens": ["Mei\u00b7nen", "star\u00b7ren\u00b7den", "Gram", "!", "Du", "g\u00fcr\u00b7test", ",", "kr\u00e4f\u00b7tigst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mich zum schweren Wandel des \u00f6den Lebens,", "tokens": ["Mich", "zum", "schwe\u00b7ren", "Wan\u00b7del", "des", "\u00f6\u00b7den", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Zeigest das Ziel mir!", "tokens": ["Zei\u00b7gest", "das", "Ziel", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach, an dem Ziele harret mein im Kranze", "tokens": ["Ach", ",", "an", "dem", "Zie\u00b7le", "har\u00b7ret", "mein", "im", "Kran\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ewigduftender Wonne meine Agnes!", "tokens": ["E\u00b7wig\u00b7duf\u00b7ten\u00b7der", "Won\u00b7ne", "mei\u00b7ne", "Ag\u00b7nes", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NE", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und die Liebevollste der Liebevollen", "tokens": ["Und", "die", "Lie\u00b7be\u00b7volls\u00b7te", "der", "Lie\u00b7be\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Liebt mich im Himmel!", "tokens": ["Liebt", "mich", "im", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Schwebet herab vielleicht zu mir, und l\u00e4chelt,", "tokens": ["Schwe\u00b7bet", "her\u00b7ab", "viel\u00b7leicht", "zu", "mir", ",", "und", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "KON", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wenn dem Einsamen hold ihr Traumbild l\u00e4chelt,", "tokens": ["Wenn", "dem", "Ein\u00b7sa\u00b7men", "hold", "ihr", "Traum\u00b7bild", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bildet s\u00fc\u00dfe T\u00e4uschungen, lockt aus welkem", "tokens": ["Bil\u00b7det", "s\u00fc\u00b7\u00dfe", "T\u00e4u\u00b7schun\u00b7gen", ",", "lockt", "aus", "wel\u00b7kem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "$,", "VVFIN", "APPR", "PWAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Auge mir Thr\u00e4nen!", "tokens": ["Au\u00b7ge", "mir", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Aber ich seh' sie nicht! \u2013 ich soll aufs Ziel schaun!", "tokens": ["A\u00b7ber", "ich", "seh'", "sie", "nicht", "!", "\u2013", "ich", "soll", "aufs", "Ziel", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Darum seh' ich sie nicht! sie war mein Abgott!", "tokens": ["Da\u00b7rum", "seh'", "ich", "sie", "nicht", "!", "sie", "war", "mein", "Ab\u00b7gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wollt's nicht! will's im Himmel nicht sein! und h\u00fcllet", "tokens": ["Wollt's", "nicht", "!", "will's", "im", "Him\u00b7mel", "nicht", "sein", "!", "und", "h\u00fcl\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKNEG", "$.", "VMFIN", "APPRART", "NN", "PTKNEG", "VAINF", "$.", "KON", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Liebend in Nacht sich.", "tokens": ["Lie\u00b7bend", "in", "Nacht", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PRF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbliebst du mich mehr als Ihn?\u00ab so fragte warnend,", "tokens": ["\u00bb", "liebst", "du", "mich", "mehr", "als", "Ihn", "?", "\u00ab", "so", "frag\u00b7te", "war\u00b7nend", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "ADV", "KOUS", "PPER", "$.", "$(", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sie lebte, die Holde! Denn sie liebte", "tokens": ["Als", "sie", "leb\u00b7te", ",", "die", "Hol\u00b7de", "!", "Denn", "sie", "lieb\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mehr als mich, Allliebender, dich! Der Weiber", "tokens": ["Mehr", "als", "mich", ",", "A\u00b7lllie\u00b7ben\u00b7der", ",", "dich", "!", "Der", "Wei\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "KOKOM", "PPER", "$,", "NN", "$,", "PPER", "$.", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Z\u00e4rtlichste, mehr dich!", "tokens": ["Z\u00e4rt\u00b7lichs\u00b7te", ",", "mehr", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Siehe, sie f\u00fchlte nicht des Engels Hippe!", "tokens": ["Sie\u00b7he", ",", "sie", "f\u00fchl\u00b7te", "nicht", "des", "En\u00b7gels", "Hip\u00b7pe", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schnellgereifet am Strahle deiner Liebe", "tokens": ["Schnell\u00b7ge\u00b7rei\u00b7fet", "am", "Strah\u00b7le", "dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fiel in deine l\u00f6sende Hand sie, schien mir,", "tokens": ["Fiel", "in", "dei\u00b7ne", "l\u00f6\u00b7sen\u00b7de", "Hand", "sie", ",", "schien", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Tot schon, zu schlummern!", "tokens": ["Tot", "schon", ",", "zu", "schlum\u00b7mern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Lehre mich lieben, wie sie liebte! La\u00df mich,", "tokens": ["Leh\u00b7re", "mich", "lie\u00b7ben", ",", "wie", "sie", "lieb\u00b7te", "!", "La\u00df", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$.", "VVIMP", "PPER", "$,"], "meter": "+--+-+-+---", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie nachreifende Frucht, im Sand des Tr\u00fcbsals", "tokens": ["Wie", "nach\u00b7rei\u00b7fen\u00b7de", "Frucht", ",", "im", "Sand", "des", "Tr\u00fcb\u00b7sals"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "M\u00fcrbe werden, zeitigen f\u00fcr die sch\u00f6ne", "tokens": ["M\u00fcr\u00b7be", "wer\u00b7den", ",", "zei\u00b7ti\u00b7gen", "f\u00fcr", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAINF", "$,", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Stunde des Festes!", "tokens": ["Stun\u00b7de", "des", "Fes\u00b7tes", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Sei mir gesegnet, Stunde, j\u00fcngstgeborne", "tokens": ["Sei", "mir", "ge\u00b7seg\u00b7net", ",", "Stun\u00b7de", ",", "j\u00fcngst\u00b7ge\u00b7bor\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwester freudiger und durchweinter Stunden!", "tokens": ["Schwes\u00b7ter", "freu\u00b7di\u00b7ger", "und", "durch\u00b7wein\u00b7ter", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6ner einst, als jene, da sprachlos Agnes", "tokens": ["Sch\u00f6\u00b7ner", "einst", ",", "als", "je\u00b7ne", ",", "da", "sprach\u00b7los", "Ag\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "KOUS", "PDS", "$,", "KOUS", "NE", "NE"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Herzte den Erstling.", "tokens": ["Herz\u00b7te", "den", "Erst\u00b7ling", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Dann ist sie wieder mein! und wonnetrunken", "tokens": ["Dann", "ist", "sie", "wie\u00b7der", "mein", "!", "und", "won\u00b7ne\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "$.", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich wieder mein Weib, und rufe: Du bist", "tokens": ["Seh'", "ich", "wie\u00b7der", "mein", "Weib", ",", "und", "ru\u00b7fe", ":", "Du", "bist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Geist von meinem Geiste! Bist Herz von meinem", "tokens": ["Geist", "von", "mei\u00b7nem", "Geis\u00b7te", "!", "Bist", "Herz", "von", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$.", "VAFIN", "NN", "APPR", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Herzen, o Agnes!", "tokens": ["Her\u00b7zen", ",", "o", "Ag\u00b7nes", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}