{"textgrid.poem.63308": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ich baumle mit de Beene", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Mutter liegt im Bette,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "liegt", "im", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sie kriegt das dritte Kind;", "tokens": ["Denn", "sie", "kriegt", "das", "drit\u00b7te", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Schwester geht zur Mette,", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", "geht", "zur", "Met\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil wir so katholisch sind.", "tokens": ["Weil", "wir", "so", "ka\u00b7tho\u00b7lisch", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Manchmal troppt mir eine Tr\u00e4ne", "tokens": ["Manch\u00b7mal", "troppt", "mir", "ei\u00b7ne", "Tr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und im Herzen puppert's schwer;", "tokens": ["Und", "im", "Her\u00b7zen", "pup\u00b7pert's", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich baumle mit de Beene,", "tokens": ["Und", "ich", "baum\u00b7le", "mit", "de", "Bee\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit de Beene vor mich her.", "tokens": ["Mit", "de", "Bee\u00b7ne", "vor", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Neulich kommt ein Herr gegangen", "tokens": ["Neu\u00b7lich", "kommt", "ein", "Herr", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit 'nem violetten Shawl,", "tokens": ["Mit", "'nem", "vi\u00b7o\u00b7let\u00b7ten", "Shawl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und er hat sich eingehangen,", "tokens": ["Und", "er", "hat", "sich", "ein\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es ging nach Jeschkenthal!", "tokens": ["Und", "es", "ging", "nach", "Jeschken\u00b7thal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sonntag war's. Er grinste: \u00bbKleene,", "tokens": ["Sonn\u00b7tag", "wa\u00b7r'", "s.", "Er", "grins\u00b7te", ":", "\u00bb", "Klee\u00b7ne", ","], "token_info": ["word", "word", "abbreviation", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "VVIMP", "PPER", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wa, ", "tokens": ["Wa", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und ich baumle mit de Beene,", "tokens": ["Und", "ich", "baum\u00b7le", "mit", "de", "Bee\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit de Beene vor mich her.", "tokens": ["Mit", "de", "Bee\u00b7ne", "vor", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vater sitzt zum 'zigsten Male,", "tokens": ["Va\u00b7ter", "sitzt", "zum", "'zigs\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wegen \u00bbHm\u00ab in Pl\u00f6tzensee,", "tokens": ["We\u00b7gen", "\u00bb", "Hm", "\u00ab", "in", "Pl\u00f6t\u00b7zen\u00b7see", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$(", "XY", "$(", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sein Schatz, der schimpft sich Male,", "tokens": ["Und", "sein", "Schatz", ",", "der", "schimpft", "sich", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PRF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Mutter tut's so weh!", "tokens": ["Und", "der", "Mut\u00b7ter", "tut's", "so", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja so gut wie der hat's Keener,", "tokens": ["Ja", "so", "gut", "wie", "der", "hat's", "Kee\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJD", "KOKOM", "ART", "NE", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Fressen kriegt er und noch mehr,", "tokens": ["Fres\u00b7sen", "kriegt", "er", "und", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er baumelt mit de Beene,", "tokens": ["Und", "er", "bau\u00b7melt", "mit", "de", "Bee\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit de Beene vor sich her.", "tokens": ["Mit", "de", "Bee\u00b7ne", "vor", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Manchmal in den Vollmondn\u00e4chten", "tokens": ["Manch\u00b7mal", "in", "den", "Voll\u00b7mond\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Is mir gar so wunderlich:", "tokens": ["Is", "mir", "gar", "so", "wun\u00b7der\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sie meinen Emil br\u00e4chten,", "tokens": ["Ob", "sie", "mei\u00b7nen", "E\u00b7mil", "br\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er auf dem Striche strich!", "tokens": ["Weil", "er", "auf", "dem", "Stri\u00b7che", "strich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fch um dreie kr\u00e4hten H\u00e4hne,", "tokens": ["Fr\u00fch", "um", "drei\u00b7e", "kr\u00e4h\u00b7ten", "H\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Galgen ragt, und er...,", "tokens": ["Und", "ein", "Gal\u00b7gen", "ragt", ",", "und", "er", "...", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "$(", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Und er baumelt mit de Beene,", "tokens": ["Und", "er", "bau\u00b7melt", "mit", "de", "Bee\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit de Beene vor sich her.", "tokens": ["Mit", "de", "Bee\u00b7ne", "vor", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}