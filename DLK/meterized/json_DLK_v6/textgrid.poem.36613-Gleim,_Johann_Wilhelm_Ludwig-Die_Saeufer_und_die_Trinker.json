{"textgrid.poem.36613": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Saeufer und die Trinker", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bacchus edlen Saft verschwenden,", "tokens": ["Die", "Bac\u00b7chus", "ed\u00b7len", "Saft", "ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bestraft er durch die Gicht,", "tokens": ["Be\u00b7straft", "er", "durch", "die", "Gicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit lahmen F\u00fcssen, krummen Haenden,", "tokens": ["Mit", "lah\u00b7men", "F\u00fcs\u00b7sen", ",", "krum\u00b7men", "Haen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kupfrigem Gesicht.", "tokens": ["Und", "kupf\u00b7ri\u00b7gem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo Scythen und Praelaten saufen,", "tokens": ["Wo", "Scyt\u00b7hen", "und", "Prae\u00b7la\u00b7ten", "sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Da ist der Gott der Freuden nicht dabey;", "tokens": ["Da", "ist", "der", "Gott", "der", "Freu\u00b7den", "nicht", "da\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es herrscht in ihren wilden Haufen", "tokens": ["Es", "herrscht", "in", "ih\u00b7ren", "wil\u00b7den", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Dummheit, und die Zaenkerey.", "tokens": ["Die", "Dumm\u00b7heit", ",", "und", "die", "Za\u00b7en\u00b7ke\u00b7rey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "O Bacchus, deine freye Freuden", "tokens": ["O", "Bac\u00b7chus", ",", "dei\u00b7ne", "frey\u00b7e", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kennt weiser Trinker Zunft!", "tokens": ["Kennt", "wei\u00b7ser", "Trin\u00b7ker", "Zunft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die nehmen dein Geschenk bescheiden,", "tokens": ["Die", "neh\u00b7men", "dein", "Ge\u00b7schenk", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rasen mit Vernunft.", "tokens": ["Und", "ra\u00b7sen", "mit", "Ver\u00b7nunft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die singen, in vergn\u00fcgten Ch\u00f6ren,", "tokens": ["Die", "sin\u00b7gen", ",", "in", "ver\u00b7gn\u00fcg\u00b7ten", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Lobgesang der Weisheit und der Ruh,", "tokens": ["Den", "Lob\u00b7ge\u00b7sang", "der", "Weis\u00b7heit", "und", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn sie volle Glaeser leeren,", "tokens": ["Und", "wenn", "sie", "vol\u00b7le", "Glae\u00b7ser", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So sehn die keuschen Musen zu.", "tokens": ["So", "sehn", "die", "keu\u00b7schen", "Mu\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}