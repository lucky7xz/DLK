{"textgrid.poem.39182": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Windstille", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht die schlanke Pinie im gr\u00fcnen Raume stumm,", "tokens": ["Es", "steht", "die", "schlan\u00b7ke", "Pi\u00b7nie", "im", "gr\u00fc\u00b7nen", "Rau\u00b7me", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es h\u00e4ngt die Windharmonika am gr\u00fcnen Baume stumm,", "tokens": ["Es", "h\u00e4ngt", "die", "Wind\u00b7har\u00b7mo\u00b7ni\u00b7ka", "am", "gr\u00fc\u00b7nen", "Bau\u00b7me", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Die Wolke zieht in Majest\u00e4t vor\u00fcber ohne Laut,", "tokens": ["Die", "Wol\u00b7ke", "zieht", "in", "Ma\u00b7jes\u00b7t\u00e4t", "vor\u00b7\u00fc\u00b7ber", "oh\u00b7ne", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der Adler tut den Fl\u00fcgelschlag am Wolkensaume stumm;", "tokens": ["Der", "Ad\u00b7ler", "tut", "den", "Fl\u00fc\u00b7gel\u00b7schlag", "am", "Wol\u00b7ken\u00b7sau\u00b7me", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Es schwebt vom Turm in blaue Luft die Wetterfahne still,", "tokens": ["Es", "schwebt", "vom", "Turm", "in", "blau\u00b7e", "Luft", "die", "Wet\u00b7ter\u00b7fah\u00b7ne", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Es schl\u00e4ft am Strand der Wellensto\u00df im wei\u00dfen Schaume stumm;", "tokens": ["Es", "schl\u00e4ft", "am", "Strand", "der", "Wel\u00b7len\u00b7sto\u00df", "im", "wei\u00b7\u00dfen", "Schau\u00b7me", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Und bis des Sturmes schwarzer Hengst die Wolkenz\u00fcgel rei\u00dft,", "tokens": ["Und", "bis", "des", "Stur\u00b7mes", "schwar\u00b7zer", "Hengst", "die", "Wol\u00b7ken\u00b7z\u00fc\u00b7gel", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "So ruht des All's Titanenleib im G\u00f6ttertraume stumm;", "tokens": ["So", "ruht", "des", "All's", "Ti\u00b7ta\u00b7nen\u00b7leib", "im", "G\u00f6t\u00b7ter\u00b7trau\u00b7me", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Bis aus des Herzens Felsengrund die Liebe st\u00fcrmend schnellt,", "tokens": ["Bis", "aus", "des", "Her\u00b7zens", "Fel\u00b7sen\u00b7grund", "die", "Lie\u00b7be", "st\u00fcr\u00b7mend", "schnellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Schl\u00e4ft das Ghasel, das Blumenkind, auf rosigem Flaume stumm.", "tokens": ["Schl\u00e4ft", "das", "Gha\u00b7sel", ",", "das", "Blu\u00b7men\u00b7kind", ",", "auf", "ro\u00b7si\u00b7gem", "Flau\u00b7me", "stumm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+--+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}}}}