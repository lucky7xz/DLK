{"textgrid.poem.49863": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Krankenbette", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie ein Vogel am Fenster vorbei mit sonnebegl\u00e4nztem", "tokens": ["Gleich\u00b7wie", "ein", "Vo\u00b7gel", "am", "Fens\u00b7ter", "vor\u00b7bei", "mit", "son\u00b7ne\u00b7be\u00b7gl\u00e4nz\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "APPRART", "NN", "ADV", "APPR", "ADJA"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Fl\u00fcgel den blitzenden Schein wirft in ein schattig Gemach,", "tokens": ["Fl\u00fc\u00b7gel", "den", "blit\u00b7zen\u00b7den", "Schein", "wirft", "in", "ein", "schat\u00b7tig", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Also, mitten im Gram um verlorene Jahre des Siechbetts,", "tokens": ["Al\u00b7so", ",", "mit\u00b7ten", "im", "Gram", "um", "ver\u00b7lo\u00b7re\u00b7ne", "Jah\u00b7re", "des", "Siech\u00b7betts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "\u00dcberraschet und weckt leuchtende Hoffnung mich oft.", "tokens": ["\u00dc\u00b7berr\u00b7a\u00b7schet", "und", "weckt", "leuch\u00b7ten\u00b7de", "Hoff\u00b7nung", "mich", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJA", "NN",