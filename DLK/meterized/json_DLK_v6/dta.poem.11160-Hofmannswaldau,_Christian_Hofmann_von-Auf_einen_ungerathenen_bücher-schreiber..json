{"dta.poem.11160": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen ungerathenen b\u00fccher-schreiber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjlst du dich auch, ein buch zu schreiben, unterwinden?", "tokens": ["Wjlst", "du", "dich", "auch", ",", "ein", "buch", "zu", "schrei\u00b7ben", ",", "un\u00b7ter\u00b7win\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Apollo wundert sich mit der gelehrten welt,", "tokens": ["A\u00b7pol\u00b7lo", "wun\u00b7dert", "sich", "mit", "der", "ge\u00b7lehr\u00b7ten", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Er giebt auch schon befehl, damit es ewig h\u00e4lt,", "tokens": ["Er", "giebt", "auch", "schon", "be\u00b7fehl", ",", "da\u00b7mit", "es", "e\u00b7wig", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein ungemeines buch in esels-haut zu binden.", "tokens": ["Dein", "un\u00b7ge\u00b7mei\u00b7nes", "buch", "in", "e\u00b7sels\u00b7haut", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}