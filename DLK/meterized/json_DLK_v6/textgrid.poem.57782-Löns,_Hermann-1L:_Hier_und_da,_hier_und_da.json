{"textgrid.poem.57782": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier und da, hier und da", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier und da, hier und da", "tokens": ["Hier", "und", "da", ",", "hier", "und", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "$,", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geht ein Licht und das ist blau,", "tokens": ["Geht", "ein", "Licht", "und", "das", "ist", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PDS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern und nah, fern und nah", "tokens": ["Fern", "und", "nah", ",", "fern", "und", "nah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Abends in dem Tau;", "tokens": ["A\u00b7bends", "in", "dem", "Tau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Margarete, Margrete,", "tokens": ["Mar\u00b7ga\u00b7re\u00b7te", ",", "Mar\u00b7gre\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Du hast die Liebe verschm\u00e4ht,", "tokens": ["Du", "hast", "die", "Lie\u00b7be", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Deine arme arme Seele", "tokens": ["Dei\u00b7ne", "ar\u00b7me", "ar\u00b7me", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach Liebe suchen geht.", "tokens": ["Nach", "Lie\u00b7be", "su\u00b7chen", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hin und her, hin und her,", "tokens": ["Hin", "und", "her", ",", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$,", "ADV", "KON", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wo die wei\u00dfen Rosen stehn,", "tokens": ["Wo", "die", "wei\u00b7\u00dfen", "Ro\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kreuz und quer, kreuz und quer", "tokens": ["Kreuz", "und", "quer", ",", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "NN", "KON", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mu\u00df die Flamme gehn;", "tokens": ["Mu\u00df", "die", "Flam\u00b7me", "gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Margarete, Margrete,", "tokens": ["Mar\u00b7ga\u00b7re\u00b7te", ",", "Mar\u00b7gre\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Der Wind und der weht,", "tokens": ["Der", "Wind", "und", "der", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Deine arme arme Seele", "tokens": ["Dei\u00b7ne", "ar\u00b7me", "ar\u00b7me", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach Liebe suchen geht.", "tokens": ["Nach", "Lie\u00b7be", "su\u00b7chen", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ohne Rast, ohne Ruh", "tokens": ["Oh\u00b7ne", "Rast", ",", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Geht die Flamme auf und ab,", "tokens": ["Geht", "die", "Flam\u00b7me", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immerzu, immerzu", "tokens": ["Im\u00b7mer\u00b7zu", ",", "im\u00b7mer\u00b7zu"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "ADV"], "meter": "+-++-O", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u00dcber ihrem Grab;", "tokens": ["\u00dc\u00b7ber", "ih\u00b7rem", "Grab", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Margarete, Margrete,", "tokens": ["Mar\u00b7ga\u00b7re\u00b7te", ",", "Mar\u00b7gre\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Nun ist es viel zu sp\u00e4t,", "tokens": ["Nun", "ist", "es", "viel", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Deine arme arme Seele", "tokens": ["Dei\u00b7ne", "ar\u00b7me", "ar\u00b7me", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach Liebe suchen geht.", "tokens": ["Nach", "Lie\u00b7be", "su\u00b7chen", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Still und stumm, still und stumm", "tokens": ["Still", "und", "stumm", ",", "still", "und", "stumm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Geht die Flamme kalt und blau", "tokens": ["Geht", "die", "Flam\u00b7me", "kalt", "und", "blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um und um, um und um", "tokens": ["Um", "und", "um", ",", "um", "und", "um"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "KON", "PTKVZ", "$,", "KOUI", "KON", "KOUI"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Morgens in dem Tau;", "tokens": ["Mor\u00b7gens", "in", "dem", "Tau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Margarete, Margrete,", "tokens": ["Mar\u00b7ga\u00b7re\u00b7te", ",", "Mar\u00b7gre\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Der Hahn und der kr\u00e4ht,", "tokens": ["Der", "Hahn", "und", "der", "kr\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Deine arme arme Seele", "tokens": ["Dei\u00b7ne", "ar\u00b7me", "ar\u00b7me", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach Liebe suchen geht.", "tokens": ["Nach", "Lie\u00b7be", "su\u00b7chen", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}