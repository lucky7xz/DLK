{"dta.poem.13856": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Friedensbote .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Kriege kommen wir heim;", "tokens": ["Aus", "dem", "Krie\u00b7ge", "kom\u00b7men", "wir", "heim", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "In st\u00fcrmischer Nacht und Regen,", "tokens": ["In", "st\u00fcr\u00b7mi\u00b7scher", "Nacht", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn ich auf der Lauer gelegen,", "tokens": ["Wenn", "ich", "auf", "der", "Lau\u00b7er", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie dachte ich dorten Dein!", "tokens": ["Wie", "dach\u00b7te", "ich", "dor\u00b7ten", "Dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gott stand in der Noth uns bei,", "tokens": ["Gott", "stand", "in", "der", "Noth", "uns", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun droben, bei Mondenschein,", "tokens": ["Nun", "dro\u00b7ben", ",", "bei", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Schlaf ruhig, das Land ist ja frei!", "tokens": ["Schlaf", "ru\u00b7hig", ",", "das", "Land", "ist", "ja", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}