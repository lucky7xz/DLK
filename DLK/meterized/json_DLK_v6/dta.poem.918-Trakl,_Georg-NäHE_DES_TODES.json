{"dta.poem.918": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4HE DES TODES", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "O der Abend, der in die finsteren D\u00f6rfer der Kindheit geht.", "tokens": ["O", "der", "A\u00b7bend", ",", "der", "in", "die", "fins\u00b7te\u00b7ren", "D\u00f6r\u00b7fer", "der", "Kind\u00b7heit", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der Weiher unter den Weiden", "tokens": ["Der", "Wei\u00b7her", "un\u00b7ter", "den", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcllt sich mit den verpesteten Seufzern der Schwermut.", "tokens": ["F\u00fcllt", "sich", "mit", "den", "ver\u00b7pes\u00b7te\u00b7ten", "Seuf\u00b7zern", "der", "Schwer\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "O der Wald, der leise die braunen Augen senkt,", "tokens": ["O", "der", "Wald", ",", "der", "lei\u00b7se", "die", "brau\u00b7nen", "Au\u00b7gen", "senkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da aus des Einsamen kn\u00f6chernen H\u00e4nden", "tokens": ["Da", "aus", "des", "Ein\u00b7sa\u00b7men", "kn\u00f6\u00b7cher\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Der Purpur seiner verz\u00fcckten Tage hinsinkt.", "tokens": ["Der", "Pur\u00b7pur", "sei\u00b7ner", "ver\u00b7z\u00fcck\u00b7ten", "Ta\u00b7ge", "hin\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O die N\u00e4he des Todes. La\u00df uns beten.", "tokens": ["O", "die", "N\u00e4\u00b7he", "des", "To\u00b7des", ".", "La\u00df", "uns", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In dieser Nacht l\u00f6sen auf lauen Kissen", "tokens": ["In", "die\u00b7ser", "Nacht", "l\u00f6\u00b7sen", "auf", "lau\u00b7en", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vergilbt von Weihrauch sich der Liebenden schm\u00e4chtige Glieder.", "tokens": ["Ver\u00b7gilbt", "von", "Weih\u00b7rauch", "sich", "der", "Lie\u00b7ben\u00b7den", "schm\u00e4ch\u00b7ti\u00b7ge", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}}}}