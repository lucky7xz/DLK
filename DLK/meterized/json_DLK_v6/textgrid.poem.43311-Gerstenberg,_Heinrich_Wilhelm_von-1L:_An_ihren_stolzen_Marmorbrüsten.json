{"textgrid.poem.43311": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An ihren stolzen Marmorbr\u00fcsten", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An ihren stolzen Marmorbr\u00fcsten", "tokens": ["An", "ih\u00b7ren", "stol\u00b7zen", "Mar\u00b7mor\u00b7br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprang jeder Pfeil des Amors ab;", "tokens": ["Sprang", "je\u00b7der", "Pfeil", "des", "A\u00b7mors", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es mochte da der Gott sich noch so sehr entr\u00fcsten,", "tokens": ["Es", "moch\u00b7te", "da", "der", "Gott", "sich", "noch", "so", "sehr", "ent\u00b7r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zehnmal kriegrischer sich r\u00fcsten,", "tokens": ["Und", "zehn\u00b7mal", "krie\u00b7gri\u00b7scher", "sich", "r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Pfeil sprang ab.", "tokens": ["Der", "Pfeil", "sprang", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie oft bat ich sie zu verziehen,", "tokens": ["Wie", "oft", "bat", "ich", "sie", "zu", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie, gleich Rehen, vor mir floh!", "tokens": ["Wenn", "sie", ",", "gleich", "Re\u00b7hen", ",", "vor", "mir", "floh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll stets mein z\u00e4rtlich Herz vergebens um dich gl\u00fchen?", "tokens": ["Soll", "stets", "mein", "z\u00e4rt\u00b7lich", "Herz", "ver\u00b7ge\u00b7bens", "um", "dich", "gl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJD", "NN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! wird mich Doris ewig fliehen?", "tokens": ["Ach", "!", "wird", "mich", "Do\u00b7ris", "e\u00b7wig", "flie\u00b7hen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "NE", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und Doris floh.", "tokens": ["Und", "Do\u00b7ris", "floh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die sie mit ihrem Nektar netzt,", "tokens": ["Die", "sie", "mit", "ih\u00b7rem", "Nek\u00b7tar", "netzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Ganymed ihn f\u00fcnfmal durchgel\u00e4utert,", "tokens": ["Wenn", "Ga\u00b7ny\u00b7med", "ihn", "f\u00fcnf\u00b7mal", "durch\u00b7ge\u00b7l\u00e4u\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie, deren Reiz so oft das Herz des Mars geletzt,", "tokens": ["Sie", ",", "de\u00b7ren", "Reiz", "so", "oft", "das", "Herz", "des", "Mars", "ge\u00b7letzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und siegreich seine Stirn zum L\u00e4cheln aufgeheitert,", "tokens": ["Und", "sieg\u00b7reich", "sei\u00b7ne", "Stirn", "zum", "L\u00e4\u00b7cheln", "auf\u00b7ge\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn sie die Welt in Schrecken setzt.", "tokens": ["Wenn", "sie", "die", "Welt", "in", "Schre\u00b7cken", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da f\u00fchlte sie der Liebe Gl\u00fcck,", "tokens": ["Da", "f\u00fchl\u00b7te", "sie", "der", "Lie\u00b7be", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gab mir schnell den Ku\u00df zur\u00fcck;", "tokens": ["Und", "gab", "mir", "schnell", "den", "Ku\u00df", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da f\u00fchlt ich ihre Lippen gl\u00fchen,", "tokens": ["Da", "f\u00fchlt", "ich", "ih\u00b7re", "Lip\u00b7pen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und diese Wangen, die mir bl\u00fchen.", "tokens": ["Und", "die\u00b7se", "Wan\u00b7gen", ",", "die", "mir", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da lernte sie zum erstenmal verziehen \u2013", "tokens": ["Da", "lern\u00b7te", "sie", "zum", "ers\u00b7ten\u00b7mal", "ver\u00b7zie\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nun wird sie nimmer vor mir fliehen.", "tokens": ["Nun", "wird", "sie", "nim\u00b7mer", "vor", "mir", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}