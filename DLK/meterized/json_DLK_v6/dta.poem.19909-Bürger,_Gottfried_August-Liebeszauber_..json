{"dta.poem.19909": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Liebeszauber .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4del, schau mir ins Gesicht!               ", "tokens": ["M\u00e4\u00b7del", ",", "schau", "mir", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schelmenauge, blinzle nicht!", "tokens": ["Schel\u00b7men\u00b7au\u00b7ge", ",", "blinz\u00b7le", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4del, merke was ich sage!", "tokens": ["M\u00e4\u00b7del", ",", "mer\u00b7ke", "was", "ich", "sa\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gib mir Rede, wenn ich frage!", "tokens": ["Gib", "mir", "Re\u00b7de", ",", "wenn", "ich", "fra\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Holla hoch mir ins Gesicht!", "tokens": ["Hol\u00b7la", "hoch", "mir", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schelmenauge, blinzle nicht!", "tokens": ["Schel\u00b7men\u00b7au\u00b7ge", ",", "blinz\u00b7le", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist nicht h\u00e4slich, das ist wahr;", "tokens": ["Bist", "nicht", "h\u00e4s\u00b7lich", ",", "das", "ist", "wahr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "A\u00fcglein hast du, blau und klar;", "tokens": ["A\u00b7\u00fc\u00b7glein", "hast", "du", ",", "blau", "und", "klar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wang\u2019 und Mund sind s\u00fcsse Feigen;", "tokens": ["Wang'", "und", "Mund", "sind", "s\u00fcs\u00b7se", "Fei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! vom Busen las mich schweigen!", "tokens": ["Ach", "!", "vom", "Bu\u00b7sen", "las", "mich", "schwei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reizend, Liebchen, das ist wahr,", "tokens": ["Rei\u00b7zend", ",", "Lieb\u00b7chen", ",", "das", "ist", "wahr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reizend bist du offenbar.", "tokens": ["Rei\u00b7zend", "bist", "du", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber reizend her und hin!", "tokens": ["A\u00b7ber", "rei\u00b7zend", "her", "und", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist ja doch nicht Kaiserin;", "tokens": ["Bist", "ja", "doch", "nicht", "Kai\u00b7se\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht die Kaiserin der Sch\u00f6nen,", "tokens": ["Nicht", "die", "Kai\u00b7se\u00b7rin", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrdig ganz allein zum Kr\u00f6nen.", "tokens": ["W\u00fcr\u00b7dig", "ganz", "al\u00b7lein", "zum", "Kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reizend her und reizend hin!", "tokens": ["Rei\u00b7zend", "her", "und", "rei\u00b7zend", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fehlt noch viel zur Kaiserin.", "tokens": ["Fehlt", "noch", "viel", "zur", "Kai\u00b7se\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hundert Sch\u00f6nen sicherlich,", "tokens": ["Hun\u00b7dert", "Sch\u00f6\u00b7nen", "si\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hundert, hundert! f\u00e4nden sich,", "tokens": ["Hun\u00b7dert", ",", "hun\u00b7dert", "!", "f\u00e4n\u00b7den", "sich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "$,", "CARD", "$.", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor Eifer w\u00fcrden lodern,", "tokens": ["Die", "vor", "Ei\u00b7fer", "w\u00fcr\u00b7den", "lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich auf Sch\u00f6nheit \u2019rauszufodern.", "tokens": ["Dich", "auf", "Sch\u00f6n\u00b7heit", "'r\u00b7aus\u00b7zu\u00b7fo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Hundert Sch\u00f6nen f\u00e4nden sich;", "tokens": ["Hun\u00b7dert", "Sch\u00f6\u00b7nen", "f\u00e4n\u00b7den", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hundert siegten \u00fcber dich.", "tokens": ["Hun\u00b7dert", "sieg\u00b7ten", "\u00fc\u00b7ber", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dennoch hegst du Kaiserrecht", "tokens": ["Den\u00b7noch", "hegst", "du", "Kai\u00b7ser\u00b7recht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber deinen treuen Knecht:", "tokens": ["Ue\u00b7ber", "dei\u00b7nen", "treu\u00b7en", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaiserrecht in seinem Herzen,", "tokens": ["Kai\u00b7ser\u00b7recht", "in", "sei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald zu Wonne, bald zu Schmerzen.", "tokens": ["Bald", "zu", "Won\u00b7ne", ",", "bald", "zu", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tod und Leben, Kaiserrecht,", "tokens": ["Tod", "und", "Le\u00b7ben", ",", "Kai\u00b7ser\u00b7recht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimt von dir der treue Knecht!", "tokens": ["Nimt", "von", "dir", "der", "treu\u00b7e", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hundert ist wol grosse Zal;", "tokens": ["Hun\u00b7dert", "ist", "wol", "gros\u00b7se", "Zal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber, Liebchen, las es mal", "tokens": ["A\u00b7ber", ",", "Lieb\u00b7chen", ",", "las", "es", "mal"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hunderttausend Sch\u00f6nen wagen,", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send", "Sch\u00f6\u00b7nen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich von Thron und Reich zu jagen!", "tokens": ["Dich", "von", "Thron", "und", "Reich", "zu", "ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hunderttausend! Welche Zal!", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send", "!", "Wel\u00b7che", "Zal", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie verl\u00f6ren alzumal.", "tokens": ["Sie", "ver\u00b7l\u00f6\u00b7ren", "al\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schelmenauge, Schelmenmund,", "tokens": ["Schel\u00b7men\u00b7au\u00b7ge", ",", "Schel\u00b7men\u00b7mund", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh mich an und thu mir\u2019s kund!", "tokens": ["Sieh", "mich", "an", "und", "thu", "mir's", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "He, warum bist du die Meine?", "tokens": ["He", ",", "wa\u00b7rum", "bist", "du", "die", "Mei\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du allein und anders Keine?", "tokens": ["Du", "al\u00b7lein", "und", "an\u00b7ders", "Kei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh mich an und thu mir\u2019s kund,", "tokens": ["Sieh", "mich", "an", "und", "thu", "mir's", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schelmenauge, Schelmenmund!", "tokens": ["Schel\u00b7men\u00b7au\u00b7ge", ",", "Schel\u00b7men\u00b7mund", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sinnig forsch\u2019 ich auf und ab:", "tokens": ["Sin\u00b7nig", "for\u00b7sch'", "ich", "auf", "und", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Was so ganz dir hin mich gab? \u2014", "tokens": ["Was", "so", "ganz", "dir", "hin", "mich", "gab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "ADV", "PPER", "ADV", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha! durch nichts mich so zu zwingen,", "tokens": ["Ha", "!", "durch", "nichts", "mich", "so", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PIS", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht nicht zu mit rechten Dingen.", "tokens": ["Geht", "nicht", "zu", "mit", "rech\u00b7ten", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zauberm\u00e4del, auf und ab,", "tokens": ["Zau\u00b7ber\u00b7m\u00e4\u00b7del", ",", "auf", "und", "ab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprich, wo ist dein Zauberstab?", "tokens": ["Sprich", ",", "wo", "ist", "dein", "Zau\u00b7ber\u00b7stab", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}