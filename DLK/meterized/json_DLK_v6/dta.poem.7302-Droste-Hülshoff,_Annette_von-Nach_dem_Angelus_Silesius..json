 "sprie\u00dft", "von", "selbst", "die", "Saat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJD", "$,", "ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Ruhe wohnt die Kraft, du mu\u00dft nur ruhig seyn,", "tokens": ["In", "Ru\u00b7he", "wohnt", "die", "Kraft", ",", "du", "mu\u00dft", "nur", "ru\u00b7hig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch offne Th\u00fcr und Thor die Gnade lassen ein;", "tokens": ["Durch", "off\u00b7ne", "Th\u00fcr", "und", "Thor", "die", "Gna\u00b7de", "las\u00b7sen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dann wird aus lockerm, Grund dir Myrth' und Balsam steigen,", "tokens": ["Dann", "wird", "aus", "lo\u00b7ckerm", ",", "Grund", "dir", "Myr\u00b7th'", "und", "Bal\u00b7sam", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NE", "$,", "NN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Er k\u00f6mmt, er k\u00f6mmt, dein Lieb, giebt sich der Braut zu eigen,", "tokens": ["Er", "k\u00f6mmt", ",", "er", "k\u00f6mmt", ",", "dein", "Lieb", ",", "giebt", "sich", "der", "Braut", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "pun