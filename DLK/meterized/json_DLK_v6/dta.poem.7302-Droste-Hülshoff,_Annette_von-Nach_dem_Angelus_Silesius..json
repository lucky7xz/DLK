{"dta.poem.7302": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Angelus Silesius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Seele du, vor Allem wunderbar,", "tokens": ["Des", "Men\u00b7schen", "See\u00b7le", "du", ",", "vor", "Al\u00b7lem", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "$,", "APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Alles und auch Nichts, Gott, Priester und Altar,", "tokens": ["Du", "Al\u00b7les", "und", "auch", "Nichts", ",", "Gott", ",", "Pries\u00b7ter", "und", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "KON", "ADV", "PIS", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein P\u00fcnktchen durch dich selbst, doch \u00fcber alles Maa\u00df", "tokens": ["Kein", "P\u00fcnkt\u00b7chen", "durch", "dich", "selbst", ",", "doch", "\u00fc\u00b7ber", "al\u00b7les", "Maa\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPER", "ADV", "$,", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Reich in geschenktem Gut, und als die Engel ba\u00df;", "tokens": ["Reich", "in", "ge\u00b7schenk\u00b7tem", "Gut", ",", "und", "als", "die", "En\u00b7gel", "ba\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "KON", "KOUS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn h\u00f6her steht dein Ziel, ", "tokens": ["Denn", "h\u00f6\u00b7her", "steht", "dein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So, Seele, bist du's schon; denn was zu Gl\u00fcck und Ruhm", "tokens": ["So", ",", "See\u00b7le", ",", "bist", "du's", "schon", ";", "denn", "was", "zu", "Gl\u00fcck", "und", "Ruhm"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PIS", "ADV", "$.", "KON", "PWS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In dir verborgen liegt, es ist dein Eigenthum,", "tokens": ["In", "dir", "ver\u00b7bor\u00b7gen", "liegt", ",", "es", "ist", "dein", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob unentwickelt auch, wie's Keimlein in der Erden", "tokens": ["Ob", "un\u00b7ent\u00b7wi\u00b7ckelt", "auch", ",", "wie's", "Keim\u00b7lein", "in", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "$,", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht minder als der Baum, und wie als Million", "tokens": ["Nicht", "min\u00b7der", "als", "der", "Baum", ",", "und", "wie", "als", "Mil\u00b7li\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "KOUS", "ART", "NN", "$,", "KON", "KOKOM", "KOUS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nichts Andres ist die Eins, bist du ", "tokens": ["Nichts", "And\u00b7res", "ist", "die", "Eins", ",", "bist", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "PIS", "VAFIN", "ART", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So wie dem Tropfen Blut, der aus der Wunde quillt", "tokens": ["So", "wie", "dem", "Trop\u00b7fen", "Blut", ",", "der", "aus", "der", "Wun\u00b7de", "quillt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ganz \u00e4hnlich ist das Roth, das noch die Adern f\u00fcllt;", "tokens": ["Ganz", "\u00e4hn\u00b7lich", "ist", "das", "Roth", ",", "das", "noch", "die", "A\u00b7dern", "f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nicht Kletten tr\u00e4gt die Ros', der Dornstrauch keine Reben,", "tokens": ["Nicht", "Klet\u00b7ten", "tr\u00e4gt", "die", "Ros'", ",", "der", "Dorn\u00b7strauch", "kei\u00b7ne", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drum, Seele, st\u00fcrbest du, Gott m\u00fc\u00dft den Geist aufgeben.", "tokens": ["Drum", ",", "See\u00b7le", ",", "st\u00fcr\u00b7best", "du", ",", "Gott", "m\u00fc\u00dft", "den", "Geist", "auf\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja, Alles ist in dir was nur das Weltall beut,", "tokens": ["Ja", ",", "Al\u00b7les", "ist", "in", "dir", "was", "nur", "das", "Wel\u00b7tall", "beut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VAFIN", "APPR", "PPER", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmel und die H\u00f6ll', Gericht und Ewigkeit,", "tokens": ["Der", "Him\u00b7mel", "und", "die", "H\u00f6ll'", ",", "Ge\u00b7richt", "und", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott ist dein Richter nicht, du mu\u00dft dir selbst verzeihn,", "tokens": ["Gott", "ist", "dein", "Rich\u00b7ter", "nicht", ",", "du", "mu\u00dft", "dir", "selbst", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst an des H\u00f6chsten Thron stehst du in ew'ger Pein;", "tokens": ["Sonst", "an", "des", "H\u00f6chs\u00b7ten", "Thron", "stehst", "du", "in", "ew'\u00b7ger", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er, der dem Suchenden noch nie verl\u00f6scht die Spur,", "tokens": ["Er", ",", "der", "dem", "Su\u00b7chen\u00b7den", "noch", "nie", "ver\u00b7l\u00f6scht", "die", "Spur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er hat selbst Satan nicht verdammt nach Zeit und Ort;", "tokens": ["Er", "hat", "selbst", "Sa\u00b7tan", "nicht", "ver\u00b7dammt", "nach", "Zeit", "und", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKNEG", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "De\u00df unergr\u00fcndlich Grab ist seine Ichheit nur:", "tokens": ["De\u00df", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "Grab", "ist", "sei\u00b7ne", "Ich\u00b7heit", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "W\u00e4r er des Himmels Herr, er brennte ewig fort,", "tokens": ["W\u00e4r", "er", "des", "Him\u00b7mels", "Herr", ",", "er", "brenn\u00b7te", "e\u00b7wig", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie Gott im H\u00f6llenpfuhl w\u00e4r selig f\u00fcr und f\u00fcr,", "tokens": ["Wie", "Gott", "im", "H\u00f6l\u00b7len\u00b7pfuhl", "w\u00e4r", "se\u00b7lig", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VAFIN", "ADJD", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und, Seele, bist du treu, so steht dies auch bei dir.", "tokens": ["Und", ",", "See\u00b7le", ",", "bist", "du", "treu", ",", "so", "steht", "dies", "auch", "bei", "dir", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PDS", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Also ist deine Macht auch heute schon dein eigen,", "tokens": ["Al\u00b7so", "ist", "dei\u00b7ne", "Macht", "auch", "heu\u00b7te", "schon", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Du kannst, so oft du willst, die Himmelsleiter steigen;", "tokens": ["Du", "kannst", ",", "so", "oft", "du", "willst", ",", "die", "Him\u00b7mels\u00b7lei\u00b7ter", "stei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ort, Raum, sind Worte nur von Tr\u00e4gheit ausgedacht,", "tokens": ["Ort", ",", "Raum", ",", "sind", "Wor\u00b7te", "nur", "von", "Tr\u00e4g\u00b7heit", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die nicht Bed\u00fcrfni\u00df in dein W\u00f6rterbuch gebracht.", "tokens": ["Die", "nicht", "Be\u00b7d\u00fcrf\u00b7ni\u00df", "in", "dein", "W\u00f6r\u00b7ter\u00b7buch", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Aug' ist Blitz und Nu, dein Flug bedarf nicht Zeit,", "tokens": ["Dein", "Aug'", "ist", "Blitz", "und", "Nu", ",", "dein", "Flug", "be\u00b7darf", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und im Moment ergreifst du Gott und Ewigkeit;", "tokens": ["Und", "im", "Mo\u00b7ment", "er\u00b7greifst", "du", "Gott", "und", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein der Sinne Schrift, die mu\u00dft du dunkel nennen,", "tokens": ["Al\u00b7lein", "der", "Sin\u00b7ne", "Schrift", ",", "die", "mu\u00dft", "du", "dun\u00b7kel", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,", "PRELS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da dir das Werkzeug fehlt die Lettern zu erkennen;", "tokens": ["Da", "dir", "das", "Werk\u00b7zeug", "fehlt", "die", "Let\u00b7tern", "zu", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur Geist'ges fa\u00dft der Geist, ihm ist der Leib zu schwer,", "tokens": ["Nur", "Geist'\u00b7ges", "fa\u00dft", "der", "Geist", ",", "ihm", "ist", "der", "Leib", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du schmeckst, du f\u00fchlst, du riechst, und wei\u00dft um gar nichts", "tokens": ["Du", "schmeckst", ",", "du", "f\u00fchlst", ",", "du", "riechst", ",", "und", "wei\u00dft", "um", "gar", "nichts"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADV", "PIS"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "mehr;", "tokens": ["mehr", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Hat nicht vom Tr\u00f6pfchen Thau die Eigenschaft zu messen", "tokens": ["Hat", "nicht", "vom", "Tr\u00f6pf\u00b7chen", "Thau", "die", "Ei\u00b7gen\u00b7schaft", "zu", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPRART", "NN", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jahrtausende der Mensch vergebens sich vermessen?", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "der", "Mensch", "ver\u00b7ge\u00b7bens", "sich", "ver\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drum, plagt dich Irdisches, du hast es selbst bestellt,", "tokens": ["Drum", ",", "plagt", "dich", "Ir\u00b7di\u00b7sches", ",", "du", "hast", "es", "selbst", "be\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Viel n\u00e4her als dein Kleid ist dir die Geisterwelt!", "tokens": ["Viel", "n\u00e4\u00b7her", "als", "dein", "Kleid", "ist", "dir", "die", "Geis\u00b7ter\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Fa\u00dft's nicht zuweilen dich, als m\u00fc\u00dftest in der That", "tokens": ["Fa\u00dft's", "nicht", "zu\u00b7wei\u00b7len", "dich", ",", "als", "m\u00fc\u00df\u00b7test", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "PPER", "$,", "KOUS", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du \u00fcber dich hinaus, das Ganze zu durchdringen,", "tokens": ["Du", "\u00fc\u00b7ber", "dich", "hin\u00b7aus", ",", "das", "Gan\u00b7ze", "zu", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "APZR", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie jener Philosoph um einen Punkt nur bat,", "tokens": ["Wie", "je\u00b7ner", "Phi\u00b7lo\u00b7soph", "um", "ei\u00b7nen", "Punkt", "nur", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um dann der Erde Ball aus seiner Bahn zu schwingen?", "tokens": ["Um", "dann", "der", "Er\u00b7de", "Ball", "aus", "sei\u00b7ner", "Bahn", "zu", "schwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fchlst du in Demuth so, in Liebesflammen rein,", "tokens": ["F\u00fchlst", "du", "in", "De\u00b7muth", "so", ",", "in", "Lie\u00b7bes\u00b7flam\u00b7men", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Dann ist's der Sch\u00f6pfung Mark, la\u00df dir nicht leide seyn!", "tokens": ["Dann", "ist's", "der", "Sch\u00f6p\u00b7fung", "Mark", ",", "la\u00df", "dir", "nicht", "lei\u00b7de", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann f\u00fchlst du dich von Gott als Wesenheit begr\u00fcndet,", "tokens": ["Dann", "f\u00fchlst", "du", "dich", "von", "Gott", "als", "We\u00b7sen\u00b7heit", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie Quelle an dem Strand, wo Ocean sich r\u00fcndet.", "tokens": ["Wie", "Quel\u00b7le", "an", "dem", "Strand", ",", "wo", "O\u00b7cean", "sich", "r\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "PWAV", "NE", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "So sey denn freudig, Geist, da Nichts mag gr\u00f6\u00dfer seyn,", "tokens": ["So", "sey", "denn", "freu\u00b7dig", ",", "Geist", ",", "da", "Nichts", "mag", "gr\u00f6\u00b7\u00dfer", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "NN", "$,", "KOUS", "PIS", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wirf dich in den Staub, da Nichts wie du so klein!", "tokens": ["So", "wirf", "dich", "in", "den", "Staub", ",", "da", "Nichts", "wie", "du", "so", "klein", "!"], "