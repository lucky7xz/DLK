{"textgrid.poem.64859": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du willst mich, sagst du, ewig lieben?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst mich, sagst du, ewig lieben?", "tokens": ["Du", "willst", "mich", ",", "sagst", "du", ",", "e\u00b7wig", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du die\u00df Wort auch recht bedacht?", "tokens": ["Hast", "du", "die\u00df", "Wort", "auch", "recht", "be\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll sich dein heitrer Himmel tr\u00fcben", "tokens": ["Soll", "sich", "dein", "hei\u00b7trer", "Him\u00b7mel", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit den Gewittern meiner Nacht?", "tokens": ["Mit", "den", "Ge\u00b7wit\u00b7tern", "mei\u00b7ner", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Willst du, da\u00df deine Freude sterbe,", "tokens": ["Willst", "du", ",", "da\u00df", "dei\u00b7ne", "Freu\u00b7de", "ster\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Gl\u00fcck und Hoffnung weich' von dir,", "tokens": ["Da\u00df", "Gl\u00fcck", "und", "Hoff\u00b7nung", "weich'", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sich dein Leben schnell entf\u00e4rbe, \u2013", "tokens": ["Da\u00df", "sich", "dein", "Le\u00b7ben", "schnell", "ent\u00b7f\u00e4r\u00b7be", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja dann, dann fordre Lieb' von mir!", "tokens": ["Ja", "dann", ",", "dann", "ford\u00b7re", "Lieb'", "von", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ADV", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In klaren Sonnenscheines Blicken", "tokens": ["In", "kla\u00b7ren", "Son\u00b7nen\u00b7schei\u00b7nes", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt licht vor dir des Lebens Bahn;", "tokens": ["Liegt", "licht", "vor", "dir", "des", "Le\u00b7bens", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was willst du von dem Quelle trinken,", "tokens": ["Was", "willst", "du", "von", "dem", "Quel\u00b7le", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nur den Tod dir geben kann?", "tokens": ["Der", "nur", "den", "Tod", "dir", "ge\u00b7ben", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der heitern Kr\u00e4nze, die dich schm\u00fccken,", "tokens": ["Der", "hei\u00b7tern", "Kr\u00e4n\u00b7ze", ",", "die", "dich", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vergessend, willst in s\u00fc\u00dfem Traum", "tokens": ["Ver\u00b7ges\u00b7send", ",", "willst", "in", "s\u00fc\u00b7\u00dfem", "Traum"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du in dem Schatten dich erquicken", "tokens": ["Du", "in", "dem", "Schat\u00b7ten", "dich", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von meiner Liebe Upasbaum?", "tokens": ["Von", "mei\u00b7ner", "Lie\u00b7be", "U\u00b7pas\u00b7baum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O wisse, da\u00df, was je im Herzen", "tokens": ["O", "wis\u00b7se", ",", "da\u00df", ",", "was", "je", "im", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trug als h\u00f6chstes Lebensziel,", "tokens": ["Ich", "trug", "als", "h\u00f6chs\u00b7tes", "Le\u00b7bens\u00b7ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar bald dem hei\u00dfen Brand der Schmerzen", "tokens": ["Gar", "bald", "dem", "hei\u00b7\u00dfen", "Brand", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als rettungsloses Opfer fiel;", "tokens": ["Als", "ret\u00b7tungs\u00b7lo\u00b7ses", "Op\u00b7fer", "fiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, die ich zu den Meinen z\u00e4hle,", "tokens": ["Da\u00df", ",", "die", "ich", "zu", "den", "Mei\u00b7nen", "z\u00e4h\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Gram als Seine auch begr\u00fc\u00dft;", "tokens": ["Vom", "Gram", "als", "Sei\u00b7ne", "auch", "be\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kurz, da\u00df die Liebe meiner Seele", "tokens": ["Kurz", ",", "da\u00df", "die", "Lie\u00b7be", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Ku\u00df der Eisenjungfrau ist! \u2013", "tokens": ["Der", "Ku\u00df", "der", "Ei\u00b7sen\u00b7jung\u00b7frau", "ist", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6r' auf, nach einem Ziel zu streben,", "tokens": ["H\u00f6r'", "auf", ",", "nach", "ei\u00b7nem", "Ziel", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gl\u00fcck verspricht und Ungl\u00fcck beut!", "tokens": ["Das", "Gl\u00fcck", "ver\u00b7spricht", "und", "Un\u00b7gl\u00fcck", "beut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df mich auf Schmerzesh\u00f6hen leben", "tokens": ["La\u00df", "mich", "auf", "Schmer\u00b7zes\u00b7h\u00f6\u00b7hen", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In k\u00f6niglicher Einsamkeit.", "tokens": ["In", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verwahre deines Gl\u00fcckes Bl\u00fcthen,", "tokens": ["Ver\u00b7wah\u00b7re", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gern netzt' ich sie mit meinem Blut!", "tokens": ["Gern", "netzt'", "ich", "sie", "mit", "mei\u00b7nem", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gern \u2013 \u2013 nein! dich m\u00f6ge Gott beh\u00fcten", "tokens": ["Gern", "\u2013", "\u2013", "nein", "!", "dich", "m\u00f6\u00b7ge", "Gott", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "$(", "PTKANT", "$.", "PPER", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor meiner Dichterliebe Glut.", "tokens": ["Vor", "mei\u00b7ner", "Dich\u00b7ter\u00b7lie\u00b7be", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}