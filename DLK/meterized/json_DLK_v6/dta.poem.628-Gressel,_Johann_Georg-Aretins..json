{"dta.poem.628": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Aretins.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war der Venus lieb und zeigete der Welt", "tokens": ["Ich", "war", "der", "Ve\u00b7nus", "lieb", "und", "zei\u00b7ge\u00b7te", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sie sich ehmahls hat zu dem ", "tokens": ["Wie", "sie", "sich", "eh\u00b7mahls", "hat", "zu", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VAFIN", "APPR", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Was sie von Lieblichkeit annoch verborgen hielte/", "tokens": ["Was", "sie", "von", "Lieb\u00b7lich\u00b7keit", "an\u00b7noch", "ver\u00b7bor\u00b7gen", "hiel\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das wurde offenbahr/ wenn meine Harffr spielte.", "tokens": ["Das", "wur\u00b7de", "of\u00b7fen\u00b7bahr", "/", "wenn", "mei\u00b7ne", "Harf\u00b7fr", "spiel\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}