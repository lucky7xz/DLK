{"dta.poem.21813": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Besser ruhig lieben/ als m\u00fchsam  \n Kriegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Eh ich wolte deiner missen/", "tokens": ["Eh", "ich", "wol\u00b7te", "dei\u00b7ner", "mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Karilis/ mein sch\u00f6nstes Licht/", "tokens": ["Ka\u00b7ri\u00b7lis", "/", "mein", "sch\u00f6ns\u00b7tes", "Licht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "eh mir/ deinen Mund zu k\u00fcssen/", "tokens": ["eh", "mir", "/", "dei\u00b7nen", "Mund", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ferner solte werden nicht:", "tokens": ["fer\u00b7ner", "sol\u00b7te", "wer\u00b7den", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VAFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wolt\u2019 ich eh/ da\u00df alles Kriegen", "tokens": ["wolt'", "ich", "eh", "/", "da\u00df", "al\u00b7les", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "KOUS", "$(", "KOUS", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "m\u00fcst\u2019 in Pluto Pf\u00fclen liegen.", "tokens": ["m\u00fcst'", "in", "Plu\u00b7to", "Pf\u00fc\u00b7len", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Solt\u2019 ich gleich mit Sieges-Zweigen", "tokens": ["Solt'", "ich", "gleich", "mit", "Sie\u00b7ges\u00b7Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "fahren zum Kapitolin/", "tokens": ["fah\u00b7ren", "zum", "Ka\u00b7pi\u00b7to\u00b7lin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "auff den g\u00f6ldnen Wagen steigen/", "tokens": ["auff", "den", "g\u00f6ld\u00b7nen", "Wa\u00b7gen", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "durch Kapenens Pforte ziehn/", "tokens": ["durch", "Ka\u00b7pe\u00b7nens", "Pfor\u00b7te", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "mit der R\u00f6mer Schaar umgeben", "tokens": ["mit", "der", "R\u00f6\u00b7mer", "Schaar", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die mich/ Sieger/ hiesse leben.", "tokens": ["die", "mich", "/", "Sie\u00b7ger", "/", "hies\u00b7se", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "NN", "$(", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Solt\u2019 ich tausend St\u00e4te haben/", "tokens": ["Solt'", "ich", "tau\u00b7send", "St\u00e4\u00b7te", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "CARD", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fcrchte mich der Szyten Land/", "tokens": ["f\u00fcrch\u00b7te", "mich", "der", "Szy\u00b7ten", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und der Nil\u2019 in meiner Hand/", "tokens": ["und", "der", "Nil'", "in", "mei\u00b7ner", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solt\u2019 ich Indien beherschen/", "tokens": ["Solt'", "ich", "In\u00b7di\u00b7en", "be\u00b7her\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "ehrte mich das Reich der Persen:", "tokens": ["ehr\u00b7te", "mich", "das", "Reich", "der", "Per\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wolt\u2019 ich doch ohn dich Karille/", "tokens": ["Wolt'", "ich", "doch", "ohn", "dich", "Ka\u00b7ril\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "alles schlagen in den Wind:", "tokens": ["al\u00b7les", "schla\u00b7gen", "in", "den", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Besser bey dir in der Stille:", "tokens": ["Bes\u00b7ser", "bey", "dir", "in", "der", "Stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "als wo Kron und Zepter sind/", "tokens": ["als", "wo", "Kron", "und", "Zep\u00b7ter", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "NE", "KON", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die man mit Unruhigkeiten", "tokens": ["die", "man", "mit", "Un\u00b7ru\u00b7hig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "mu\u00df erhalten und bestreiten.", "tokens": ["mu\u00df", "er\u00b7hal\u00b7ten", "und", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gerne wil ich bey dir pfl\u00fcgen/", "tokens": ["Ger\u00b7ne", "wil", "ich", "bey", "dir", "pfl\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gern\u2019 auff harten Gersten-stroh/", "tokens": ["gern'", "auff", "har\u00b7ten", "Gers\u00b7ten\u00b7stroh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "liebstes Kind/ Karille/ liegen/", "tokens": ["liebs\u00b7tes", "Kind", "/", "Ka\u00b7ril\u00b7le", "/", "lie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NE", "$(", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gerne dreschen/ h\u00f6lzen: wo", "tokens": ["ger\u00b7ne", "dre\u00b7schen", "/", "h\u00f6l\u00b7zen", ":", "wo"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVINF", "$(", "VVINF", "$.", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ich bey dir nur m\u00f6ge leben", "tokens": ["ich", "bey", "dir", "nur", "m\u00f6\u00b7ge", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und zur Zeit ein K\u00fc\u00dfgen geben.", "tokens": ["und", "zur", "Zeit", "ein", "K\u00fc\u00df\u00b7gen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fahret hin/ ihr eiteln Krieger/", "tokens": ["Fah\u00b7ret", "hin", "/", "ihr", "ei\u00b7teln", "Krie\u00b7ger", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hochmuht/ Beuten/ fahret hin/", "tokens": ["Hoch\u00b7muht", "/", "Beu\u00b7ten", "/", "fah\u00b7ret", "hin", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "VVINF", "$(", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "hin/ ihr Bluht-besprengten Sieger!", "tokens": ["hin", "/", "ihr", "Bluht\u00b7be\u00b7spreng\u00b7ten", "Sie\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb\u2019 und Ruh ist mein Gewinn.", "tokens": ["Lieb'", "und", "Ruh", "ist", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fort hin wil ich bey den Schaffen/", "tokens": ["Fort", "hin", "wil", "ich", "bey", "den", "Schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "forthin bey Karillchen schlaffen.", "tokens": ["for\u00b7thin", "bey", "Ka\u00b7rill\u00b7chen", "schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}