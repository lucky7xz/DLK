{"textgrid.poem.46329": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wolauff, wolauff, nun lobet Gott]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolauff, wolauff, nun lobet Gott", "tokens": ["Wo\u00b7lauff", ",", "wo\u00b7lauff", ",", "nun", "lo\u00b7bet", "Gott"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "$,", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Himmel-Volck dort oben;", "tokens": ["Ihr", "Him\u00b7mel\u00b7Volck", "dort", "o\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Engel Gottes Sabaoth,", "tokens": ["Ihr", "En\u00b7gel", "Got\u00b7tes", "Sa\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Euch so gar erhoben;", "tokens": ["Der", "Euch", "so", "gar", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "St\u00e4ts schawet ihr sein angesicht,", "tokens": ["St\u00e4ts", "scha\u00b7wet", "ihr", "sein", "an\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O lust in lustes brunnen!", "tokens": ["O", "lust", "in", "lus\u00b7tes", "brun\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "All frewd vnd wunn, vnd glantz, vnd liecht", "tokens": ["All", "frewd", "vnd", "wunn", ",", "vnd", "glantz", ",", "vnd", "liecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "KON", "NE", "$,", "KON", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Komt euch von ihm geronnen.", "tokens": ["Komt", "euch", "von", "ihm", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+----+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ach das nur alles weit, vnd breit", "tokens": ["Ach", "das", "nur", "al\u00b7les", "weit", ",", "vnd", "breit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PDS", "ADV", "PIS", "ADJD", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem lob erf\u00fcllet,", "tokens": ["Mit", "sei\u00b7nem", "lob", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll krafft, vnd macht, vnd herrlichkeit", "tokens": ["Voll", "krafft", ",", "vnd", "macht", ",", "vnd", "herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$,", "KON", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von schall, vnd hall erbr\u00fcllet!", "tokens": ["Von", "schall", ",", "vnd", "hall", "er\u00b7br\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach preiset ihn mit h\u00f6chstem schall,", "tokens": ["Ach", "prei\u00b7set", "ihn", "mit", "h\u00f6chs\u00b7tem", "schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit starck vnd starcken singen,", "tokens": ["Mit", "starck", "vnd", "star\u00b7cken", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Solt schon die Welt von hellem knall", "tokens": ["Solt", "schon", "die", "Welt", "von", "hel\u00b7lem", "knall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In tausent st\u00fcck zerspringen.", "tokens": ["In", "tau\u00b7sent", "st\u00fcck", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}