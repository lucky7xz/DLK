{"dta.poem.5743": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir werden stets von ihr umgeben und ber\u00fchret;", "tokens": ["Wir", "wer\u00b7den", "stets", "von", "ihr", "um\u00b7ge\u00b7ben", "und", "be\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja unser C\u00f6rper selbst besteht aus ihr.", "tokens": ["Ja", "un\u00b7ser", "C\u00f6r\u00b7per", "selbst", "be\u00b7steht", "aus", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wird dieselbe f\u00fcr und f\u00fcr", "tokens": ["Es", "wird", "die\u00b7sel\u00b7be", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von uns auf manche Art versp\u00fcret:", "tokens": ["Von", "uns", "auf", "man\u00b7che", "Art", "ver\u00b7sp\u00fc\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein, durch diese Eigenschafften,", "tokens": ["Al\u00b7lein", ",", "durch", "die\u00b7se", "Ei\u00b7gen\u00b7schaff\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die unaufh\u00f6rlich an ihr hafften,", "tokens": ["Die", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "an", "ihr", "haff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wird unser Dencken offt get\u00e4uschet und verf\u00fchret.", "tokens": ["Wird", "un\u00b7ser", "Den\u00b7cken", "offt", "ge\u00b7t\u00e4u\u00b7schet", "und", "ver\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}