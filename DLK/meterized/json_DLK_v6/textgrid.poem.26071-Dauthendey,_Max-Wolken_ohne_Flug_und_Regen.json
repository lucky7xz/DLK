{"textgrid.poem.26071": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wolken ohne Flug und Regen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolken l\u00e4hmend drau\u00dfen stehen,", "tokens": ["Wol\u00b7ken", "l\u00e4h\u00b7mend", "drau\u00b7\u00dfen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken durch die W\u00e4nde gehen;", "tokens": ["Wol\u00b7ken", "durch", "die", "W\u00e4n\u00b7de", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut' vor Wolken, weltengro\u00dfen,", "tokens": ["Heut'", "vor", "Wol\u00b7ken", ",", "wel\u00b7ten\u00b7gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PWAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Himmel nicht zu sehen.", "tokens": ["Ist", "der", "Him\u00b7mel", "nicht", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wolken dr\u00fccken stumm verdrossen.", "tokens": ["Wol\u00b7ken", "dr\u00fc\u00b7cken", "stumm", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wolken sich zusammenschlossen,", "tokens": ["Wol\u00b7ken", "sich", "zu\u00b7sam\u00b7men\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wolken, die nicht zu bewegen,", "tokens": ["Wol\u00b7ken", ",", "die", "nicht", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wolken ohne Flug und Regen,", "tokens": ["Wol\u00b7ken", "oh\u00b7ne", "Flug", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wolken, die ans Herz grob sto\u00dfen.", "tokens": ["Wol\u00b7ken", ",", "die", "ans", "Herz", "grob", "sto\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}