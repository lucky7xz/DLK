kens": ["Ha\u00b7be", ",", "wenn", "in", "rei\u00b7cher", "Stil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn in einem Blick und Laut", "tokens": ["Wenn", "in", "ei\u00b7nem", "Blick", "und", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Ruhe, seine F\u00fclle", "tokens": ["Sei\u00b7ne", "Ru\u00b7he", ",", "sei\u00b7ne", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ihr Genius vertraut,", "tokens": ["Mir", "ihr", "Ge\u00b7nius", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn der Gott, der mich begeistert,", "tokens": ["Wenn", "der", "Gott", ",", "der", "mich", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir an ihrer Stirne tagt,", "tokens": ["Mir", "an", "ih\u00b7rer", "Stir\u00b7ne", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Von Bewundrung \u00fcbermeistert,", "tokens": ["Von", "Be\u00b7wund\u00b7rung", "\u00fc\u00b7ber\u00b7meis\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Z\u00fcrnend ihr mein Nichts geklagt;", "tokens": ["Z\u00fcr\u00b7nend", "ihr", "mein", "Nichts", "ge\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Dann umf\u00e4ngt ihr himmlisch Wesen", "tokens": ["Dann", "um\u00b7f\u00e4ngt", "ihr", "himm\u00b7lisch", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00df im Kinderspiele mich,", "tokens": ["S\u00fc\u00df", "im", "Kin\u00b7der\u00b7spie\u00b7le", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in ihrem Zauber l\u00f6sen", "tokens": ["Und", "in", "ih\u00b7rem", "Zau\u00b7ber", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudig meine Bande sich;", "tokens": ["Freu\u00b7dig", "mei\u00b7ne", "Ban\u00b7de", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hin ist dann mein d\u00fcrftig Streben,", "tokens": ["Hin", "ist", "dann", "mein", "d\u00fcrf\u00b7tig", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Hin des Kampfes letzte Spur,", "tokens": ["Hin", "des", "Kamp\u00b7fes", "letz\u00b7te", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ins volle G\u00f6tterleben", "tokens": ["Und", "ins", "vol\u00b7le", "G\u00f6t\u00b7ter\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tritt die sterbliche Natur.", "tokens": ["Tritt", "die", "sterb\u00b7li\u00b7che", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ha! wo keine Macht auf Erden,", "tokens": ["Ha", "!", "wo", "kei\u00b7ne", "Macht", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keines Gottes Wink uns trennt,", "tokens": ["Kei\u00b7nes", "Got\u00b7tes", "Wink", "uns", "trennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo wir Eins und Alles werden,", "tokens": ["Wo", "wir", "Eins", "und", "Al\u00b7les", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "PIS", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist nur mein Element;", "tokens": ["Das", "ist", "nur", "mein", "E\u00b7le\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wo wir Not und Zeit vergessen,", "tokens": ["Wo", "wir", "Not", "und", "Zeit", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den k\u00e4rglichen Gewinn", "tokens": ["Und", "den", "k\u00e4rg\u00b7li\u00b7chen", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer mit der Spanne messen,", "tokens": ["Nim\u00b7mer", "mit", "der", "Span\u00b7ne", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da, da sag ich, da\u00df ich bin.", "tokens": ["Da", ",", "da", "sag", "ich", ",", "da\u00df", "ich", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wie der Stern der Tyndariden,", "tokens": ["Wie", "der", "Stern", "der", "Tyn\u00b7da\u00b7ri\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in leichter Majest\u00e4t", "tokens": ["Der", "in", "leich\u00b7ter", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Bahn, wie wir, zufrieden", "tokens": ["Sei\u00b7ne", "Bahn", ",", "wie", "wir", ",", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort in dunkler H\u00f6he geht,", "tokens": ["Dort", "in", "dunk\u00b7ler", "H\u00f6\u00b7he", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun in heitre Meereswogen,", "tokens": ["Nun", "in", "heit\u00b7re", "Mee\u00b7res\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wo die sch\u00f6ne Ruhe winkt,", "tokens": ["Wo", "die", "sch\u00f6\u00b7ne", "Ru\u00b7he", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von des Himmels steilem Bogen", "tokens": ["Von", "des", "Him\u00b7mels", "stei\u00b7lem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Klar und gro\u00df hinuntersinkt:", "tokens": ["Klar", "und", "gro\u00df", "hin\u00b7un\u00b7ter\u00b7sinkt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "O Begeisterung! so finden", "tokens": ["O", "Be\u00b7geis\u00b7te\u00b7rung", "!", "so", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir in dir ein selig Grab,", "tokens": ["Wir", "in", "dir", "ein", "se\u00b7lig", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tief in deine Woge schwinden,", "tokens": ["Tief", "in", "dei\u00b7ne", "Wo\u00b7ge", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stillfrohlockend wir hinab,", "tokens": ["Still\u00b7froh\u00b7lo\u00b7ckend", "wir", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis der Hore Ruf wir h\u00f6ren,", "tokens": ["Bis", "der", "Ho\u00b7re", "Ruf", "wir", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit neuem Stolz erwacht,", "tokens": ["Und", "mit", "neu\u00b7em", "Stolz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die Sterne, wiederkehren", "tokens": ["Wie", "die", "Ster\u00b7ne", ",", "wie\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Lebens kurze Nacht.", "tokens": ["In", "des", "Le\u00b7bens", "kur\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}