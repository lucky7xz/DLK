{"textgrid.poem.67099": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Diotima", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange tot und tiefverschlossen,", "tokens": ["Lan\u00b7ge", "tot", "und", "tief\u00b7ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft mein Herz die sch\u00f6ne Welt;", "tokens": ["Gr\u00fc\u00dft", "mein", "Herz", "die", "sch\u00f6\u00b7ne", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Zweige bl\u00fchn und sprossen,", "tokens": ["Sei\u00b7ne", "Zwei\u00b7ge", "bl\u00fchn", "und", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu von Lebenskraft geschwellt;", "tokens": ["Neu", "von", "Le\u00b7bens\u00b7kraft", "ge\u00b7schwellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O! ich kehre noch ins Leben,", "tokens": ["O", "!", "ich", "keh\u00b7re", "noch", "ins", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie heraus in Luft und Licht", "tokens": ["Wie", "he\u00b7raus", "in", "Luft", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meiner Blumen selig Streben", "tokens": ["Mei\u00b7ner", "Blu\u00b7men", "se\u00b7lig", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der d\u00fcrren H\u00fclse bricht.", "tokens": ["Aus", "der", "d\u00fcr\u00b7ren", "H\u00fcl\u00b7se", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie so anders ists geworden!", "tokens": ["Wie", "so", "an\u00b7ders", "ists", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles, was ich ha\u00dft und mied,", "tokens": ["Al\u00b7les", ",", "was", "ich", "ha\u00dft", "und", "mied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stimmt in freundlichen Akkorden", "tokens": ["Stimmt", "in", "freund\u00b7li\u00b7chen", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun in meines Lebens Lied,", "tokens": ["Nun", "in", "mei\u00b7nes", "Le\u00b7bens", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit jedem Stundenschlage", "tokens": ["Und", "mit", "je\u00b7dem", "Stun\u00b7den\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werd ich wunderbar gemahnt", "tokens": ["Werd", "ich", "wun\u00b7der\u00b7bar", "ge\u00b7mahnt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An der Kindheit goldne Tage,", "tokens": ["An", "der", "Kind\u00b7heit", "gold\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seit ich dieses Eine fand.", "tokens": ["Seit", "ich", "die\u00b7ses", "Ei\u00b7ne", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diotima! selig Wesen!", "tokens": ["Dio\u00b7ti\u00b7ma", "!", "se\u00b7lig", "We\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Herrliche, durch die mein Geist,", "tokens": ["Herr\u00b7li\u00b7che", ",", "durch", "die", "mein", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "PPOSAT", "NN", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Von des Lebens Angst genesen,", "tokens": ["Von", "des", "Le\u00b7bens", "Angst", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6tterjugend sich verhei\u00dft!", "tokens": ["G\u00f6t\u00b7ter\u00b7ju\u00b7gend", "sich", "ver\u00b7hei\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Himmel wird bestehen,", "tokens": ["Un\u00b7ser", "Him\u00b7mel", "wird", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unergr\u00fcndlich sich verwandt,", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "sich", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat sich, eh wir uns gesehen,", "tokens": ["Hat", "sich", ",", "eh", "wir", "uns", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unser Innerstes gekannt.", "tokens": ["Un\u00b7ser", "In\u00b7ners\u00b7tes", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ich noch in Kindertr\u00e4umen,", "tokens": ["Da", "ich", "noch", "in", "Kin\u00b7der\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friedlich, wie der blaue Tag,", "tokens": ["Fried\u00b7lich", ",", "wie", "der", "blau\u00b7e", "Tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter meines Gartens B\u00e4umen", "tokens": ["Un\u00b7ter", "mei\u00b7nes", "Gar\u00b7tens", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der warmen Erde lag,", "tokens": ["Auf", "der", "war\u00b7men", "Er\u00b7de", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in leiser Lust und Sch\u00f6ne", "tokens": ["Und", "in", "lei\u00b7ser", "Lust", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Herzens Mai begann,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "Mai", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "S\u00e4uselte, wie Zephirst\u00f6ne,", "tokens": ["S\u00e4u\u00b7sel\u00b7te", ",", "wie", "Ze\u00b7phir\u00b7st\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Diotimas Geist mich an.", "tokens": ["Dio\u00b7ti\u00b7mas", "Geist", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ach! und da, wie eine Sage,", "tokens": ["Ach", "!", "und", "da", ",", "wie", "ei\u00b7ne", "Sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir des Lebens Sch\u00f6ne schwand,", "tokens": ["Mir", "des", "Le\u00b7bens", "Sch\u00f6\u00b7ne", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ich vor des Himmels Tage", "tokens": ["Da", "ich", "vor", "des", "Him\u00b7mels", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Darbend, wie ein Blinder, stand,", "tokens": ["Dar\u00b7bend", ",", "wie", "ein", "Blin\u00b7der", ",", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da die Last der Zeit mich beugte,", "tokens": ["Da", "die", "Last", "der", "Zeit", "mich", "beug\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und mein Leben, kalt und bleich,", "tokens": ["Und", "mein", "Le\u00b7ben", ",", "kalt", "und", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sehnend schon hinab sich neigte", "tokens": ["Seh\u00b7nend", "schon", "hin\u00b7ab", "sich", "neig\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Schatten stummes Reich;", "tokens": ["In", "der", "Schat\u00b7ten", "stum\u00b7mes", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Da, da kam vom Ideale,", "tokens": ["Da", ",", "da", "kam", "vom", "I\u00b7dea\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie vom Himmel, Mut und Macht,", "tokens": ["Wie", "vom", "Him\u00b7mel", ",", "Mut", "und", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du erscheinst mit deinem Strahle,", "tokens": ["Du", "er\u00b7scheinst", "mit", "dei\u00b7nem", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6tterbild! in meiner Nacht;", "tokens": ["G\u00f6t\u00b7ter\u00b7bild", "!", "in", "mei\u00b7ner", "Nacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich zu finden, warf ich wieder,", "tokens": ["Dich", "zu", "fin\u00b7den", ",", "warf", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warf ich den entschlafnen Kahn", "tokens": ["Warf", "ich", "den", "ent\u00b7schlaf\u00b7nen", "Kahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem toten Porte nieder", "tokens": ["Von", "dem", "to\u00b7ten", "Por\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den blauen Ozean. \u2013", "tokens": ["In", "den", "blau\u00b7en", "O\u00b7ze\u00b7an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun! ich habe dich gefunden,", "tokens": ["Nun", "!", "ich", "ha\u00b7be", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner, als ich ahndend sah", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "ich", "ahn\u00b7dend", "sah"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Liebe Feierstunden,", "tokens": ["In", "der", "Lie\u00b7be", "Fei\u00b7er\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohe! Gute! bist du da;", "tokens": ["Ho\u00b7he", "!", "Gu\u00b7te", "!", "bist", "du", "da", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O der armen Phantasien!", "tokens": ["O", "der", "ar\u00b7men", "Phan\u00b7ta\u00b7si\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Dieses Eine bildest nur", "tokens": ["Die\u00b7ses", "Ei\u00b7ne", "bil\u00b7dest", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du, in ewgen Harmonien", "tokens": ["Du", ",", "in", "ew\u00b7gen", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frohvollendete Natur!", "tokens": ["Froh\u00b7vol\u00b7len\u00b7de\u00b7te", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Wie die Seligen dort oben,", "tokens": ["Wie", "die", "Se\u00b7li\u00b7gen", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo hinauf die Freude flieht,", "tokens": ["Wo", "hin\u00b7auf", "die", "Freu\u00b7de", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo, des Daseins \u00fcberhoben,", "tokens": ["Wo", ",", "des", "Da\u00b7seins", "\u00fc\u00b7berh\u00b7o\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandellose Sch\u00f6ne bl\u00fcht,", "tokens": ["Wan\u00b7del\u00b7lo\u00b7se", "Sch\u00f6\u00b7ne", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie melodisch bei des alten", "tokens": ["Wie", "me\u00b7lo\u00b7disch", "bei", "des", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Chaos Zwist Urania,", "tokens": ["Chaos", "Zwist", "U\u00b7ra\u00b7nia", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Steht sie, g\u00f6ttlich rein erhalten,", "tokens": ["Steht", "sie", ",", "g\u00f6tt\u00b7lich", "rein", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Im Ruin der Zeiten da.", "tokens": ["Im", "Ru\u00b7in", "der", "Zei\u00b7ten", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Unter tausend Huldigungen", "tokens": ["Un\u00b7ter", "tau\u00b7send", "Hul\u00b7di\u00b7gun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mein Geist, besch\u00e4mt, besiegt,", "tokens": ["Hat", "mein", "Geist", ",", "be\u00b7sch\u00e4mt", ",", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie zu fassen schon gerungen,", "tokens": ["Sie", "zu", "fas\u00b7sen", "schon", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sein K\u00fchnstes \u00fcberfliegt.", "tokens": ["Die", "sein", "K\u00fchns\u00b7tes", "\u00fc\u00b7berf\u00b7liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonnenglut und Fr\u00fchlingsmilde,", "tokens": ["Son\u00b7nen\u00b7glut", "und", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Streit und Frieden wechselt hier", "tokens": ["Streit", "und", "Frie\u00b7den", "wech\u00b7selt", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor dem sch\u00f6nen Engelsbilde", "tokens": ["Vor", "dem", "sch\u00f6\u00b7nen", "En\u00b7gels\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Busens Tiefe mir.", "tokens": ["In", "des", "Bu\u00b7sens", "Tie\u00b7fe", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Viel der heilgen Herzenstr\u00e4nen", "tokens": ["Viel", "der", "heil\u00b7gen", "Her\u00b7zen\u00b7str\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich schon vor ihr geweint,", "tokens": ["Hab", "ich", "schon", "vor", "ihr", "ge\u00b7weint", ","], "token_info": [