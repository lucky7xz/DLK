{"textgrid.poem.43899": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "Dasz man im Lieben nicht auf Reichthum, sondern auf die Vergn\u00fcgung sehen m\u00fcsze", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe nur, was mich vergn\u00fcgt,", "tokens": ["Ich", "lie\u00b7be", "nur", ",", "was", "mich", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht was mit Golde kirrt;", "tokens": ["Nicht", "was", "mit", "Gol\u00b7de", "kirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein freyes Herz wird nicht besiegt,", "tokens": ["Mein", "frey\u00b7es", "Herz", "wird", "nicht", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn gleich der Beuthel schwirrt.", "tokens": ["Wenn", "gleich", "der", "Beut\u00b7hel", "schwirrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein goldner Strick f\u00e4ngt meinen Fu\u00df, kein heller Klang mein Ohr;", "tokens": ["Kein", "gold\u00b7ner", "Strick", "f\u00e4ngt", "mei\u00b7nen", "Fu\u00df", ",", "kein", "hel\u00b7ler", "Klang", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PIAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Die Redligkeit", "tokens": ["Die", "Red\u00b7lig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Geht allezeit", "tokens": ["Geht", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bey mir dem Nuzen vor.", "tokens": ["Bey", "mir", "dem", "Nu\u00b7zen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was hilft es, wenn das Silber blizt", "tokens": ["Was", "hilft", "es", ",", "wenn", "das", "Sil\u00b7ber", "blizt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch der Br\u00e4utgam schielt?", "tokens": ["Und", "doch", "der", "Br\u00e4ut\u00b7gam", "schielt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Mann, der stets beym Kasten sizt", "tokens": ["Ein", "Mann", ",", "der", "stets", "beym", "Kas\u00b7ten", "sizt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in dem Sacke w\u00fchlt,", "tokens": ["Und", "in", "dem", "Sa\u00b7cke", "w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Theilt mit dem Mammon seine Gunst, die blos der Frau geh\u00f6rt;", "tokens": ["Theilt", "mit", "dem", "Mam\u00b7mon", "sei\u00b7ne", "Gunst", ",", "die", "blos", "der", "Frau", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.6": {"text": "Sein Zeitvertreib", "tokens": ["Sein", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Macht, da\u00df das Weib", "tokens": ["Macht", ",", "da\u00df", "das", "Weib"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Oft fremde G\u00f6tter ehrt.", "tokens": ["Oft", "frem\u00b7de", "G\u00f6t\u00b7ter", "ehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Reichthum \u00fcberwiegt das Weh,", "tokens": ["Kein", "Reicht\u00b7hum", "\u00fc\u00b7berw\u00b7iegt", "das", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Thaler hilft der Braut,", "tokens": ["Kein", "Tha\u00b7ler", "hilft", "der", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ihr die Zwietracht in der Eh", "tokens": ["Wenn", "ihr", "die", "Zwiet\u00b7racht", "in", "der", "Eh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zulezt ein Zuchthaus baut.", "tokens": ["Zu\u00b7lezt", "ein", "Zucht\u00b7haus", "baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Ungewitter ist nicht weit, wo gelbe Raben schreyn;", "tokens": ["Das", "Un\u00b7ge\u00b7wit\u00b7ter", "ist", "nicht", "weit", ",", "wo", "gel\u00b7be", "Ra\u00b7ben", "schreyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "PWAV", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Wer wollte nun", "tokens": ["Wer", "woll\u00b7te", "nun"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So th\u00f6richt thun", "tokens": ["So", "th\u00f6\u00b7richt", "thun"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und ihm zum Schaden freyn?", "tokens": ["Und", "ihm", "zum", "Scha\u00b7den", "freyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Beth\u00f6rter Mund, ach spare doch", "tokens": ["Be\u00b7th\u00f6r\u00b7ter", "Mund", ",", "ach", "spa\u00b7re", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "ITJ", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Worte frechen Stolz!", "tokens": ["Der", "Wor\u00b7te", "fre\u00b7chen", "Stolz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Umgang ist mir stets ein Joch,", "tokens": ["Dein", "Um\u00b7gang", "ist", "mir", "stets", "ein", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du selbst ein Marterholz.", "tokens": ["Du", "selbst", "ein", "Mar\u00b7ter\u00b7holz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dies W\u00f6rtchen bringt mir deinen Ha\u00df, der ficht mich wenig an;", "tokens": ["Dies", "W\u00f6rt\u00b7chen", "bringt", "mir", "dei\u00b7nen", "Ha\u00df", ",", "der", "ficht", "mich", "we\u00b7nig", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Wie bald st\u00f6\u00dft mir", "tokens": ["Wie", "bald", "st\u00f6\u00dft", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was Be\u00dfers f\u00fcr,", "tokens": ["Was", "Be\u00b7\u00dfers", "f\u00fcr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Das mich vergn\u00fcgen kan!", "tokens": ["Das", "mich", "ver\u00b7gn\u00fc\u00b7gen", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du aber, den des Himmels Schlu\u00df", "tokens": ["Du", "a\u00b7ber", ",", "den", "des", "Him\u00b7mels", "Schlu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dereinst vor mich bestimmt,", "tokens": ["De\u00b7reinst", "vor", "mich", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Magst glauben, da\u00df mein reiner Ku\u00df", "tokens": ["Magst", "glau\u00b7ben", ",", "da\u00df", "mein", "rei\u00b7ner", "Ku\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von keiner Geldsucht glimmt.", "tokens": ["Von", "kei\u00b7ner", "Geld\u00b7sucht", "glimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm also meinen ganzen Schaz, die reine Hand voll Blut!", "tokens": ["Nimm", "al\u00b7so", "mei\u00b7nen", "gan\u00b7zen", "Schaz", ",", "die", "rei\u00b7ne", "Hand", "voll", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Ein treues Herz", "tokens": ["Ein", "treu\u00b7es", "Herz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ist sonder Scherz", "tokens": ["Ist", "son\u00b7der", "Scherz"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Das beste Heiratsgut.", "tokens": ["Das", "bes\u00b7te", "Hei\u00b7rats\u00b7gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}