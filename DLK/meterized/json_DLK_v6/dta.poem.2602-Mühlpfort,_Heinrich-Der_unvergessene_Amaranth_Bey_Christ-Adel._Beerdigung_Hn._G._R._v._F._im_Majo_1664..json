{"dta.poem.2602": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der unvergessene Amaranth  \n  Bey Christ-Adel. Beerdigung Hn. G. R. v. F.  \n im Majo 1664.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als sich der s\u00fcsse Lentz in holder Anmuth zeigte/", "tokens": ["Als", "sich", "der", "s\u00fcs\u00b7se", "Lentz", "in", "hol\u00b7der", "An\u00b7muth", "zeig\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die belebte Lufft wie Diamanten schien\u2019", "tokens": ["Und", "die", "be\u00b7leb\u00b7te", "Lufft", "wie", "Di\u00b7a\u00b7man\u00b7ten", "schien'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "KOKOM", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als sich der Himmel selbst zu seiner Erden neigte/", "tokens": ["Als", "sich", "der", "Him\u00b7mel", "selbst", "zu", "sei\u00b7ner", "Er\u00b7den", "neig\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wolt\u2019 ihr wiederumb den Sommer-Rock anziehne", "tokens": ["Und", "wolt'", "ihr", "wie\u00b7de\u00b7rumb", "den", "Som\u00b7mer\u00b7Rock", "an\u00b7zieh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat Flora sich bem\u00fcht ihr Garten-Werck zu bauen/", "tokens": ["Hat", "Flo\u00b7ra", "sich", "be\u00b7m\u00fcht", "ihr", "Gar\u00b7ten\u00b7\u00b7Werck", "zu", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie rief den Westen-Wind als ihren Liebsten an/", "tokens": ["Sie", "rief", "den", "Wes\u00b7ten\u00b7Wind", "als", "ih\u00b7ren", "Liebs\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er solte Safft und Krafft auf ieder Lust-St\u00fcck thauen/", "tokens": ["Er", "sol\u00b7te", "Safft", "und", "Krafft", "auf", "ie\u00b7der", "Lust\u00b7St\u00fcck", "thau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und durch den linden Hauch beseelen ihren Plan.", "tokens": ["Und", "durch", "den", "lin\u00b7den", "Hauch", "be\u00b7see\u00b7len", "ih\u00b7ren", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Sonne stimmte bey mit reichen Gnaden-Blicken/", "tokens": ["Die", "Son\u00b7ne", "stimm\u00b7te", "bey", "mit", "rei\u00b7chen", "Gna\u00b7den\u00b7Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Monde sagt ihr nichts als Fruchtbarkeiten zu/", "tokens": ["Der", "Mon\u00b7de", "sagt", "ihr", "nichts", "als", "Frucht\u00b7bar\u00b7kei\u00b7ten", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Lufft die wolte sich nach ihrem Willen schicken/", "tokens": ["Die", "Lufft", "die", "wol\u00b7te", "sich", "nach", "ih\u00b7rem", "Wil\u00b7len", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit Regen milde seyn/ den Winden schaffen Ruh.", "tokens": ["Mit", "Re\u00b7gen", "mil\u00b7de", "seyn", "/", "den", "Win\u00b7den", "schaf\u00b7fen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAINF", "$(", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drauff hat sie ingesambt die Gratien gebeten/", "tokens": ["Drauff", "hat", "sie", "in\u00b7ge\u00b7sambt", "die", "Gra\u00b7ti\u00b7en", "ge\u00b7be\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie m\u00f6chten bey der Lust doch nur Geh\u00fclffin seyn/", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "bey", "der", "Lust", "doch", "nur", "Ge\u00b7h\u00fclf\u00b7fin", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ADV", "ADV", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es w\u00fcrden auch hierzu die Feld-Napeen treten/", "tokens": ["Es", "w\u00fcr\u00b7den", "auch", "hier\u00b7zu", "die", "Feld\u00b7Na\u00b7peen", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-++-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Und Samen bringen mit und Pflantzen sencken ein.", "tokens": ["Und", "Sa\u00b7men", "brin\u00b7gen", "mit", "und", "Pflant\u00b7zen", "sen\u00b7cken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "APPR", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drauff sind nach ihrem Wunsch die Nymfen alle kommen/", "tokens": ["Drauff", "sind", "nach", "ih\u00b7rem", "Wunsch", "die", "Nym\u00b7fen", "al\u00b7le", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und haben mit Begier den Garten angelegt/", "tokens": ["Und", "ha\u00b7ben", "mit", "Be\u00b7gier", "den", "Gar\u00b7ten", "an\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Flora hat zu erst den Rosen-Stock genommen/", "tokens": ["Die", "Flo\u00b7ra", "hat", "zu", "erst", "den", "Ro\u00b7sen\u00b7Stock", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil sie ihr buntes Kleid damit zu zieren pflegt.", "tokens": ["Weil", "sie", "ihr", "bun\u00b7tes", "Kleid", "da\u00b7mit", "zu", "zie\u00b7ren", "pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den Gratien beliebt der Stengel weisser Lilgen/", "tokens": ["Den", "Gra\u00b7ti\u00b7en", "be\u00b7liebt", "der", "Sten\u00b7gel", "weis\u00b7ser", "Lil\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn ihre Reinligkeit sucht eben solchen Glantz.", "tokens": ["Denn", "ih\u00b7re", "Rein\u00b7lig\u00b7keit", "sucht", "e\u00b7ben", "sol\u00b7chen", "Glantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Beynebenst Ro\u00dfmarin/ den nicht der Frost kan tilgen/", "tokens": ["Bey\u00b7ne\u00b7benst", "Ro\u00df\u00b7ma\u00b7rin", "/", "den", "nicht", "der", "Frost", "kan", "til\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "ART", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Der unbefleckte Strauch mehrt ihren Ehren-Krantz.", "tokens": ["Der", "un\u00b7be\u00b7fleck\u00b7te", "Strauch", "mehrt", "ih\u00b7ren", "Eh\u00b7ren\u00b7Krantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das andere Nymfen-Volck k\u00fcst Blumen von dem Orden", "tokens": ["Das", "an\u00b7de\u00b7re", "Nym\u00b7fen\u00b7Volck", "k\u00fcst", "Blu\u00b7men", "von", "dem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Der Buhler/ wo Narciss die Oberstelle hat/", "tokens": ["Der", "Buh\u00b7ler", "/", "wo", "Nar\u00b7ciss", "die", "O\u00b7bers\u00b7tel\u00b7le", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "NN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Weil seine Sch\u00f6nheit ist sein Meuchel-M\u00f6rder worden/", "tokens": ["Weil", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "ist", "sein", "Meu\u00b7chel\u00b7M\u00f6r\u00b7der", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das weiche Hochzeit-Bett\u2019 ein eitle Grabesstat.", "tokens": ["Das", "wei\u00b7che", "Hoch\u00b7zeit\u00b7Bett'", "ein", "eit\u00b7le", "Gra\u00b7bess\u00b7tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die hat den Hyacinth der Sonnen-Schatz/ erkohren", "tokens": ["Die", "hat", "den", "Hya\u00b7cin\u00b7th", "der", "Son\u00b7nen\u00b7Schatz", "/", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$(", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und jen\u2019 ein\u2019 Anemon\u2019 in ihrer Purpurtracht", "tokens": ["Und", "jen'", "ein'", "A\u00b7ne\u00b7mon'", "in", "ih\u00b7rer", "Pur\u00b7pur\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein andre Clytien so mit dem Tag gebohren", "tokens": ["Ein", "and\u00b7re", "Cly\u00b7ti\u00b7en", "so", "mit", "dem", "Tag", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und mit dem Tage sich auch wieder schlaffen macht.", "tokens": ["Und", "mit", "dem", "Ta\u00b7ge", "sich", "auch", "wie\u00b7der", "schlaf\u00b7fen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hier stie\u00df des Saffrans Gold/ dort der Je\u00dfminen Seele/", "tokens": ["Hier", "stie\u00df", "des", "Saf\u00b7frans", "Gold", "/", "dort", "der", "Je\u00df\u00b7mi\u00b7nen", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$(", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den st\u00e4rcksten Balsam aus/ der den Geruch ergetzt/", "tokens": ["Den", "st\u00e4rcks\u00b7ten", "Bal\u00b7sam", "aus", "/", "der", "den", "Ge\u00b7ruch", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$(", "ART", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Myrrhen schwitzten noch ihr br\u00fcnstig Liebes-Oele/", "tokens": ["Die", "Myr\u00b7rhen", "schwitz\u00b7ten", "noch", "ihr", "br\u00fcns\u00b7tig", "Lie\u00b7bes\u00b7O\u00b7ele", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Nebst welchen nach der Rey Citronen eingesetzt.", "tokens": ["Nebst", "wel\u00b7chen", "nach", "der", "Rey", "Cit\u00b7ro\u00b7nen", "ein\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Garten war nunmehr dem Himmel zu vergleichen/", "tokens": ["Der", "Gar\u00b7ten", "war", "nun\u00b7mehr", "dem", "Him\u00b7mel", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Blumen konten da die sch\u00f6nsten Sterne seyn/", "tokens": ["Die", "Blu\u00b7men", "kon\u00b7ten", "da", "die", "sch\u00f6ns\u00b7ten", "Ster\u00b7ne", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als Febus und sein Chor von dem Parnassus weichen/", "tokens": ["Als", "Fe\u00b7bus", "und", "sein", "Chor", "von", "dem", "Par\u00b7nas\u00b7sus", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PPOSAT", "NN", "APPR", "ART", "NE", "VVINF", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Und stellen sich behend\u2019 in Florens Lust-Haus ein.", "tokens": ["Und", "stel\u00b7len", "sich", "be\u00b7hend'", "in", "Flo\u00b7rens", "Lust\u00b7Haus", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "NE", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es sey uns auch verg\u00f6nnt/ sprach eine Pierinne/", "tokens": ["Es", "sey", "uns", "auch", "ver\u00b7g\u00f6nnt", "/", "sprach", "ei\u00b7ne", "Pie\u00b7rin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Weil der Apollo wil zusetzen unsre Blum/", "tokens": ["Weil", "der", "A\u00b7pol\u00b7lo", "wil", "zu\u00b7set\u00b7zen", "uns\u00b7re", "Blum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "VMFIN", "VVINF", "PPOSAT", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Jhr Nam\u2019 heist ", "tokens": ["Ihr", "Nam