{"textgrid.poem.35356": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schmerzen begreifen, ehren, nachempfinden", "tokens": ["Schmer\u00b7zen", "be\u00b7grei\u00b7fen", ",", "eh\u00b7ren", ",", "na\u00b7ch\u00b7emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVINF", "$,", "VVINF", "$,", "ADJA"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und dann das Mitgef\u00fchl, das ihn bemeistert,", "tokens": ["Und", "dann", "das", "Mit\u00b7ge\u00b7f\u00fchl", ",", "das", "ihn", "be\u00b7meis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie die Bewunderung, die ihn begeistert,", "tokens": ["Wie", "die", "Be\u00b7wun\u00b7de\u00b7rung", ",", "die", "ihn", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In edle, kunstgerechte Formen binden,", "tokens": ["In", "ed\u00b7le", ",", "kunst\u00b7ge\u00b7rech\u00b7te", "For\u00b7men", "bin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das ist des Auserkorenen Beruf,", "tokens": ["Das", "ist", "des", "Au\u00b7ser\u00b7ko\u00b7re\u00b7nen", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.6": {"text": "Das seine Sendung \u2013; wie Posaunenst\u00f6\u00dfe", "tokens": ["Das", "sei\u00b7ne", "Sen\u00b7dung", "\u2013", ";", "wie", "Po\u00b7sau\u00b7nen\u00b7st\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "$(", "$.", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ert\u00f6nt sein Klagelied, wenn ihn die Gr\u00f6\u00dfe", "tokens": ["Er\u00b7t\u00f6nt", "sein", "Kla\u00b7ge\u00b7lied", ",", "wenn", "ihn", "die", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Menschenelends zum Poeten schuf.", "tokens": ["Des", "Men\u00b7sche\u00b7ne\u00b7lends", "zum", "Po\u00b7et\u00b7en", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wohl l\u00e4\u00dft sich auch das andre nicht vermeiden:", "tokens": ["Wohl", "l\u00e4\u00dft", "sich", "auch", "das", "and\u00b7re", "nicht", "ver\u00b7mei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df Seufzer, die der Phantasie entquellen,", "tokens": ["Da\u00df", "Seuf\u00b7zer", ",", "die", "der", "Phan\u00b7ta\u00b7sie", "ent\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Behutsam sich in sch\u00f6ne Worte kleiden,", "tokens": ["Be\u00b7hut\u00b7sam", "sich", "in", "sch\u00f6\u00b7ne", "Wor\u00b7te", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bevor sie fremdem Leid sich beigesellen.", "tokens": ["Be\u00b7vor", "sie", "frem\u00b7dem", "Leid", "sich", "bei\u00b7ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch la\u00dft sie gelten! Der Gedanke heiligt,", "tokens": ["Doch", "la\u00dft", "sie", "gel\u00b7ten", "!", "Der", "Ge\u00b7dan\u00b7ke", "hei\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Was die Beredsamkeit mit Blumen schm\u00fcckt;", "tokens": ["Was", "die", "Be\u00b7red\u00b7sam\u00b7keit", "mit", "Blu\u00b7men", "schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und wenn dem K\u00fcnstler das Gepr\u00e4ge gl\u00fcckt,", "tokens": ["Und", "wenn", "dem", "K\u00fcnst\u00b7ler", "das", "Ge\u00b7pr\u00e4\u00b7ge", "gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "War auch sein Herz im Schaffensdrang beteiligt.", "tokens": ["War", "auch", "sein", "Herz", "im", "Schaf\u00b7fens\u00b7drang", "be\u00b7tei\u00b7ligt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und fallen von den Augen ihm die Schuppen,", "tokens": ["Und", "fal\u00b7len", "von", "den", "Au\u00b7gen", "ihm", "die", "Schup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Erkennt er, da\u00df die Menschheit sehr verge\u00dflich,", "tokens": ["Er\u00b7kennt", "er", ",", "da\u00df", "die", "Menschheit", "sehr", "ver\u00b7ge\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "So wei\u00df er auch, da\u00df Schmerzen, unerme\u00dflich,", "tokens": ["So", "wei\u00df", "er", "auch", ",", "da\u00df", "Schmer\u00b7zen", ",", "un\u00b7er\u00b7me\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "In mancher Brust ungreifbar sich verpuppen.", "tokens": ["In", "man\u00b7cher", "Brust", "un\u00b7greif\u00b7bar", "sich", "ver\u00b7pup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dann wird die eigne Klage leiser, milder;", "tokens": ["Dann", "wird", "die", "eig\u00b7ne", "Kla\u00b7ge", "lei\u00b7ser", ",", "mil\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Vor Qualen, die nur Grabesruhe heilt,", "tokens": ["Vor", "Qua\u00b7len", ",", "die", "nur", "Gra\u00b7bes\u00b7ru\u00b7he", "heilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ist selbst der sch\u00f6nste Vers umsonst gefeilt,", "tokens": ["Ist", "selbst", "der", "sch\u00f6ns\u00b7te", "Vers", "um\u00b7sonst", "ge\u00b7feilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Erbleichen alle Worte, alle Bilder.", "tokens": ["Er\u00b7blei\u00b7chen", "al\u00b7le", "Wor\u00b7te", ",", "al\u00b7le", "Bil\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Im Sonnenlichte und in Todesgr\u00fcften", "tokens": ["Im", "Son\u00b7nen\u00b7lich\u00b7te", "und", "in", "To\u00b7des\u00b7gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Weilt der Poet \u2013 so will es sein Geschick;", "tokens": ["Weilt", "der", "Po\u00b7et", "\u2013", "so", "will", "es", "sein", "Ge\u00b7schick", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Doch nicht in alle Schmerzen dringt sein Blick,", "tokens": ["Doch", "nicht", "in", "al\u00b7le", "Schmer\u00b7zen", "dringt", "sein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Er kann, er darf nicht alle Schleier l\u00fcften.", "tokens": ["Er", "kann", ",", "er", "darf", "nicht", "al\u00b7le", "Schlei\u00b7er", "l\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}