{"textgrid.poem.27136": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Karl von Karlsberg", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was der ber\u00fchmte Verfasser des menschlichen Elends verdiene?", "tokens": ["Was", "der", "be\u00b7r\u00fchm\u00b7te", "Ver\u00b7fas\u00b7ser", "des", "menschli\u00b7chen", "E\u00b7lends", "ver\u00b7die\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sich in der Charit\u00e9 gratis verk\u00f6stigt zu sehn.", "tokens": ["Sich", "in", "der", "Cha\u00b7rit\u00e9", "gra\u00b7tis", "ver\u00b7k\u00f6s\u00b7tigt", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NE", "NE", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}