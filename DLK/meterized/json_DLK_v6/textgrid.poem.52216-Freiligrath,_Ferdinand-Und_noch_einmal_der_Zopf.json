{"textgrid.poem.52216": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Und noch einmal der Zopf", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und noch einmal der Zopf! \u2013 Jenseits sogar der Meere", "tokens": ["Und", "noch", "ein\u00b7mal", "der", "Zopf", "!", "\u2013", "Jen\u00b7seits", "so\u00b7gar", "der", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$.", "$(", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat er gew\u00fctet einst im Indo-Britenheere,", "tokens": ["Hat", "er", "ge\u00b7w\u00fc\u00b7tet", "einst", "im", "In\u00b7do\u00b7Bri\u00b7ten\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat baumelnd er gef\u00fchrt sein haarig Regiment.", "tokens": ["Hat", "bau\u00b7melnd", "er", "ge\u00b7f\u00fchrt", "sein", "haa\u00b7rig", "Re\u00b7gi\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "VVPP", "VAINF", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was dort ein Rotrock war, trug auch den krummen, straffen;", "tokens": ["Was", "dort", "ein", "Rot\u00b7rock", "war", ",", "trug", "auch", "den", "krum\u00b7men", ",", "straf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$,", "VVFIN", "ADV", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Geschmeichelt sahen es am Gangesstrand die Affen \u2013", "tokens": ["Ge\u00b7schmei\u00b7chelt", "sa\u00b7hen", "es", "am", "Gan\u00b7ges\u00b7strand", "die", "Af\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie nahmen's f\u00fcr ein Kopliment.", "tokens": ["Sie", "nah\u00b7men's", "f\u00fcr", "ein", "Ko\u00b7pli\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O, welch ein Staat das war an Sonn- und Feiertagen!", "tokens": ["O", ",", "welch", "ein", "Staat", "das", "war", "an", "Sonn", "und", "Fei\u00b7er\u00b7ta\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "PDS", "VAFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ward er feierlich und endlos erst getragen!", "tokens": ["Da", "ward", "er", "fei\u00b7er\u00b7lich", "und", "end\u00b7los", "erst", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da schm\u00fcckt' ", "tokens": ["Da", "schm\u00fcckt'"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Im Sattel sa\u00dfen sie, gradleibig wie die Puppen;", "tokens": ["Im", "Sat\u00b7tel", "sa\u00b7\u00dfen", "sie", ",", "grad\u00b7lei\u00b7big", "wie", "die", "Pup\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er unterdessen lag ausruhend auf den Croupen", "tokens": ["Er", "un\u00b7ter\u00b7des\u00b7sen", "lag", "aus\u00b7ru\u00b7hend", "auf", "den", "Crou\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In sinniger Betrachtsamkeit.", "tokens": ["In", "sin\u00b7ni\u00b7ger", "Be\u00b7tracht\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und war zu Ende nur die schimmernde Parade,", "tokens": ["Und", "war", "zu", "En\u00b7de", "nur", "die", "schim\u00b7mern\u00b7de", "Pa\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann sprengten Offizier und F\u00e4hnrich ans Gestade,", "tokens": ["Dann", "spreng\u00b7ten", "Of\u00b7fi\u00b7zier", "und", "F\u00e4hn\u00b7rich", "ans", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann gab's ein Rennen noch um eine Flasche Port!", "tokens": ["Dann", "gab's", "ein", "Ren\u00b7nen", "noch", "um", "ei\u00b7ne", "Fla\u00b7sche", "Port", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann band sich m\u00e4nniglich die angeh\u00e4ngte B\u00fcrde", "tokens": ["Dann", "band", "sich", "m\u00e4n\u00b7nig\u00b7lich", "die", "an\u00b7ge\u00b7h\u00e4ng\u00b7te", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Zopfes ehrbar ab, hielt ihn mit Schick und W\u00fcrde", "tokens": ["Des", "Zop\u00b7fes", "ehr\u00b7bar", "ab", ",", "hielt", "ihn", "mit", "Schick", "und", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fest in der Hand und schnalzte: \u00bbFort!\u00ab", "tokens": ["Fest", "in", "der", "Hand", "und", "schnalz\u00b7te", ":", "\u00bb", "Fort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN", "$.", "$(", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Der Zopf behielt den Sieg, wie sich das Ro\u00df auch sperrte!", "tokens": ["Der", "Zopf", "be\u00b7hielt", "den", "Sieg", ",", "wie", "sich", "das", "Ro\u00df", "auch", "sperr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein indo-britisch Spiel: \u2013 Weh, da\u00df man es verdeutscht!", "tokens": ["Ein", "in\u00b7do\u00b7bri\u00b7tisch", "Spiel", ":", "\u2013", "Weh", ",", "da\u00df", "man", "es", "ver\u00b7deutscht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$.", "$(", "NN", "$,", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df man auch unter uns vom r\u00fcckw\u00e4rts schaunden Kopfe", "tokens": ["Da\u00df", "man", "auch", "un\u00b7ter", "uns", "vom", "r\u00fcck\u00b7w\u00e4rts", "schaun\u00b7den", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PPER", "APPRART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den starren Unhold langt \u2013 bei uns auch mit dem Zopfe", "tokens": ["Den", "star\u00b7ren", "Un\u00b7hold", "langt", "\u2013", "bei", "uns", "auch", "mit", "dem", "Zop\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "APPR", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein edel Ro\u00df, das Volk, zerpeitscht!", "tokens": ["Ein", "e\u00b7del", "Ro\u00df", ",", "das", "Volk", ",", "zer\u00b7peitscht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}