{"textgrid.poem.45018": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr ein Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ich dich nie gesehn; die du mich blo\u00df geh\u00f6rt,", "tokens": ["Der", "ich", "dich", "nie", "ge\u00b7sehn", ";", "die", "du", "mich", "blo\u00df", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVPP", "$.", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll darum uns das Band der N\u00e4he fehlen?", "tokens": ["Soll", "da\u00b7rum", "uns", "das", "Band", "der", "N\u00e4\u00b7he", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Auge bindet \u00c4u\u00dferliches blo\u00df,", "tokens": ["Das", "Au\u00b7ge", "bin\u00b7det", "\u00c4u\u00b7\u00dfer\u00b7li\u00b7ches", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Ohr verkn\u00fcpft die Herzen und die Seelen.", "tokens": ["Das", "Ohr", "ver\u00b7kn\u00fcpft", "die", "Her\u00b7zen", "und", "die", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}