{"textgrid.poem.59096": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "315. Von Herzen klein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist f\u00fcrwahr der Mindst' und Letzte,", "tokens": ["Du", "bist", "f\u00fcr\u00b7wahr", "der", "Mind\u00b7st'", "und", "Letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denk: Ach, da\u00df jeder mich so sch\u00e4tzte!", "tokens": ["Denk", ":", "Ach", ",", "da\u00df", "je\u00b7der", "mich", "so", "sch\u00e4tz\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ITJ", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lieb Gottes Gutheit, die dich tr\u00e4gt", "tokens": ["Lieb", "Got\u00b7tes", "Gut\u00b7heit", ",", "die", "dich", "tr\u00e4gt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruft und wie sein Kindlein pflegt!", "tokens": ["Und", "ruft", "und", "wie", "sein", "Kin\u00b7dlein", "pflegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}