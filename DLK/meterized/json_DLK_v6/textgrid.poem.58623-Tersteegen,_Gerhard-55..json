{"textgrid.poem.58623": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O hohe Majest\u00e4t, die du erhaben wohnest", "tokens": ["O", "ho\u00b7he", "Ma\u00b7jes\u00b7t\u00e4t", ",", "die", "du", "er\u00b7ha\u00b7ben", "woh\u00b7nest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In stiller Ewigkeit, im dunkeln Heiligtum,", "tokens": ["In", "stil\u00b7ler", "E\u00b7wig\u00b7keit", ",", "im", "dun\u00b7keln", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bitte, da\u00df du mein, o Heiligkeit, verschonest", "tokens": ["Ich", "bit\u00b7te", ",", "da\u00df", "du", "mein", ",", "o", "Hei\u00b7lig\u00b7keit", ",", "ver\u00b7scho\u00b7nest"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach deiner Freundlicheit, indem ich zu dir komm'.", "tokens": ["Nach", "dei\u00b7ner", "Freund\u00b7li\u00b7cheit", ",", "in\u00b7dem", "ich", "zu", "dir", "komm'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach, mache meinen Geist dem\u00fctig, sanft und kleine", "tokens": ["Ach", ",", "ma\u00b7che", "mei\u00b7nen", "Geist", "de\u00b7m\u00fc\u00b7tig", ",", "sanft", "und", "klei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wohn dann auch in mir, weil du hast Lust daran!", "tokens": ["Und", "wohn", "dann", "auch", "in", "mir", ",", "weil", "du", "hast", "Lust", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "VAFIN", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier lieg' ich als wie tot, dein Angesicht alleine", "tokens": ["Hier", "lieg'", "ich", "als", "wie", "tot", ",", "dein", "An\u00b7ge\u00b7sicht", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "KOKOM", "ADJD", "$,", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und deines Geistes Kraft mich lebend machen kann.", "tokens": ["Und", "dei\u00b7nes", "Geis\u00b7tes", "Kraft", "mich", "le\u00b7bend", "ma\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}