{"textgrid.poem.37640": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Kastor an Pollux \u00fcber seine Zukunft", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei unsterblich, weil zum Mutterscho\u00dfe", "tokens": ["Sei", "uns\u00b7terb\u00b7lich", ",", "weil", "zum", "Mut\u00b7ter\u00b7scho\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Frei ein singend heller Schwan", "tokens": ["Frei", "ein", "sin\u00b7gend", "hel\u00b7ler", "Schwan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrte deines Lichtes Bahn,", "tokens": ["F\u00fchr\u00b7te", "dei\u00b7nes", "Lich\u00b7tes", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterblich lag ich doch in selbem Scho\u00dfe.", "tokens": ["Sterb\u00b7lich", "lag", "ich", "doch", "in", "sel\u00b7bem", "Scho\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "F\u00fchlte auch Prometheus Himmelfunken", "tokens": ["F\u00fchl\u00b7te", "auch", "Pro\u00b7me\u00b7theus", "Him\u00b7mel\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NE", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fr\u00fchlings \u00fcberm Herzen ziehn,", "tokens": ["Fr\u00fch\u00b7lings", "\u00fc\u00b7berm", "Her\u00b7zen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mir Stirn und Adern gl\u00fchn,", "tokens": ["Da\u00df", "mir", "Stirn", "und", "A\u00b7dern", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch mein Sinn im frohen Muthe trunken.", "tokens": ["Hoch", "mein", "Sinn", "im", "fro\u00b7hen", "Mu\u00b7the", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df vereint uns durch die Zukunft dringen,", "tokens": ["La\u00df", "ver\u00b7eint", "uns", "durch", "die", "Zu\u00b7kunft", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf und sei mein Weltgeno\u00df,", "tokens": ["Auf", "und", "sei", "mein", "Welt\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtig ich auf wildem Ro\u00df,", "tokens": ["M\u00e4ch\u00b7tig", "ich", "auf", "wil\u00b7dem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kr\u00e4ft'ger du in Armes schwellend Ringen.", "tokens": ["Kr\u00e4ft'\u00b7ger", "du", "in", "Ar\u00b7mes", "schwel\u00b7lend", "Rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00df uns wagen, wo die Tapfern zagen,", "tokens": ["La\u00df", "uns", "wa\u00b7gen", ",", "wo", "die", "Tap\u00b7fern", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie zum Spiele, ohne Lohn,", "tokens": ["Wie", "zum", "Spie\u00b7le", ",", "oh\u00b7ne", "Lohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer locket uns ein Thron,", "tokens": ["Nim\u00b7mer", "lo\u00b7cket", "uns", "ein", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Reich nur wollen wir durchjagen.", "tokens": ["Je\u00b7des", "Reich", "nur", "wol\u00b7len", "wir", "durch\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Jeder trauet, die sich selbst vertrauten,", "tokens": ["Je\u00b7der", "trau\u00b7et", ",", "die", "sich", "selbst", "ver\u00b7trau\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und in tapfrer Freundschaft gleich,", "tokens": ["Und", "in", "tapf\u00b7rer", "Freund\u00b7schaft", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Machen wir die Freunde reich,", "tokens": ["Ma\u00b7chen", "wir", "die", "Freun\u00b7de", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Streiten, siegen f\u00fcr die Argonauten.", "tokens": ["Strei\u00b7ten", ",", "sie\u00b7gen", "f\u00fcr", "die", "Ar\u00b7go\u00b7nau\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Doch es ruft zu unserm alten Hause", "tokens": ["Doch", "es", "ruft", "zu", "un\u00b7serm", "al\u00b7ten", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Uns die Todtenstill zur\u00fcck,", "tokens": ["Uns", "die", "Tod\u00b7ten\u00b7still", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir sehn mit scheuem Blick", "tokens": ["Und", "wir", "sehn", "mit", "scheu\u00b7em", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam gr\u00fcn den Hof vom Grafe.", "tokens": ["Ein\u00b7sam", "gr\u00fcn", "den", "Hof", "vom", "Gra\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00fcssen die gefangne Schwester l\u00f6sen,", "tokens": ["M\u00fcs\u00b7sen", "die", "ge\u00b7fang\u00b7ne", "Schwes\u00b7ter", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die in fernen Ketten liegt,", "tokens": ["Die", "in", "fer\u00b7nen", "Ket\u00b7ten", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, von unserm Glanz besiegt,", "tokens": ["Doch", ",", "von", "un\u00b7serm", "Glanz", "be\u00b7siegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinkt von ihr die dunkle Macht der B\u00f6sen.", "tokens": ["Sinkt", "von", "ihr", "die", "dunk\u00b7le", "Macht", "der", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ziehen nun vereint zum Kampf um Liebe", "tokens": ["Zie\u00b7hen", "nun", "ver\u00b7eint", "zum", "Kampf", "um", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVPP", "APPRART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hier vereinzelt trifft mich Tod,", "tokens": ["Hier", "ver\u00b7ein\u00b7zelt", "trifft", "mich", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch mein Leben sinkt in Tod,", "tokens": ["Doch", "mein", "Le\u00b7ben", "sinkt", "in", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wasser gleich im Danaiden Siebe.", "tokens": ["Was\u00b7ser", "gleich", "im", "Da\u00b7na\u00b7i\u00b7den", "Sie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Denn es rinnet schon zum neuen Tage", "tokens": ["Denn", "es", "rin\u00b7net", "schon", "zum", "neu\u00b7en", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem dunklen Felsenschlund,", "tokens": ["Aus", "dem", "dunk\u00b7len", "Fel\u00b7sen\u00b7schlund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als mir schenkt dein milder Mund", "tokens": ["Als", "mir", "schenkt", "dein", "mil\u00b7der", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Ewigkeiten H\u00e4lfte-Tage.", "tokens": ["Dei\u00b7ner", "E\u00b7wig\u00b7kei\u00b7ten", "H\u00e4lf\u00b7te\u00b7Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Nimm zur\u00fcck die milden G\u00f6ttergaben,", "tokens": ["Nimm", "zu\u00b7r\u00fcck", "die", "mil\u00b7den", "G\u00f6t\u00b7ter\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Opfre nicht dein halbes Gl\u00fcck;", "tokens": ["Opf\u00b7re", "nicht", "dein", "hal\u00b7bes", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer heitert meinen Blick,", "tokens": ["Nim\u00b7mer", "hei\u00b7tert", "mei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dich nicht vereint kann laben.", "tokens": ["Was", "dich", "nicht", "ver\u00b7eint", "kann", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVPP", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Was kann mir des Tages Sonne geben,", "tokens": ["Was", "kann", "mir", "des", "Ta\u00b7ges", "Son\u00b7ne", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wenn nicht mehr dein hell Gestirn", "tokens": ["Wenn", "nicht", "mehr", "dein", "hell", "Ge\u00b7stirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Leuchtet meiner dunklen Stirn?", "tokens": ["Leuch\u00b7tet", "mei\u00b7ner", "dunk\u00b7len", "Stirn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Morgenstern m\u00f6cht' ich verschweben! \u2013", "tokens": ["Mit", "dem", "Mor\u00b7gens\u00b7tern", "m\u00f6cht'", "ich", "ver\u00b7schwe\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}