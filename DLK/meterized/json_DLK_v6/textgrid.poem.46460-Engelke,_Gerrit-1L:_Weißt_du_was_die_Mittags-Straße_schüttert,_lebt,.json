{"textgrid.poem.46460": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft du was die Mittags-Stra\u00dfe sch\u00fcttert, lebt,", "genre": "verse", "period": "N.A.", "pub_year": 1904, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du was die Mittags-Stra\u00dfe sch\u00fcttert, lebt,", "tokens": ["Wei\u00dft", "du", "was", "die", "Mit\u00b7tags\u00b7Stra\u00b7\u00dfe", "sch\u00fct\u00b7tert", ",", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn chaotisch tausend Lebenstakte schlagen", "tokens": ["Wenn", "chao\u00b7tisch", "tau\u00b7send", "Le\u00b7bens\u00b7tak\u00b7te", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus den Menschen, H\u00e4usern, Pferden, Wagen?", "tokens": ["Aus", "den", "Men\u00b7schen", ",", "H\u00e4u\u00b7sern", ",", "Pfer\u00b7den", ",", "Wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gottesrhythmus!", "tokens": ["Got\u00b7tesr\u00b7hyth\u00b7mus", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wei\u00dft du was des Nachts das m\u00fcde Haus durchbebt,", "tokens": ["Wei\u00dft", "du", "was", "des", "Nachts", "das", "m\u00fc\u00b7de", "Haus", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn der Mondlichthimmel auf die Stadt gesunken?", "tokens": ["Wenn", "der", "Mond\u00b7licht\u00b7him\u00b7mel", "auf", "die", "Stadt", "ge\u00b7sun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Was die Stra\u00dfen sausen unter Sternenfunken?", "tokens": ["Was", "die", "Stra\u00b7\u00dfen", "sau\u00b7sen", "un\u00b7ter", "Ster\u00b7nen\u00b7fun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Gottesrhythmus!", "tokens": ["Got\u00b7tesr\u00b7hyth\u00b7mus", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Unaufh\u00f6rlich drangvoll, fluten, beben", "tokens": ["Un\u00b7auf\u00b7h\u00f6r\u00b7lich", "drang\u00b7voll", ",", "flu\u00b7ten", ",", "be\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "ADJD", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rhythmusstr\u00f6me durch die Stunden um dich her,", "tokens": ["Rhyth\u00b7mus\u00b7str\u00f6\u00b7me", "durch", "die", "Stun\u00b7den", "um", "dich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Schwellen, wellen \u00fcber dich zu Einem Meer:", "tokens": ["Schwel\u00b7len", ",", "wel\u00b7len", "\u00fc\u00b7ber", "dich", "zu", "Ei\u00b7nem", "Meer", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Gottesrhythmus!", "tokens": ["Got\u00b7tesr\u00b7hyth\u00b7mus", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und du selbst, du Mensch in diesem Herzschlag-Leben,", "tokens": ["Und", "du", "selbst", ",", "du", "Mensch", "in", "die\u00b7sem", "Herz\u00b7schlag\u00b7Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "PPER", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Von Tr\u00e4nen \u00fcbersp\u00fclt, vom Stra\u00dfenbraus gepackt,", "tokens": ["Von", "Tr\u00e4\u00b7nen", "\u00fc\u00b7bers\u00b7p\u00fclt", ",", "vom", "Stra\u00b7\u00dfen\u00b7braus", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bist der h\u00f6chste Rhythmus, vollster Blutstrom-Takt:", "tokens": ["Bist", "der", "h\u00f6chs\u00b7te", "Rhyth\u00b7mus", ",", "volls\u00b7ter", "Blutstrom\u00b7Takt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Denn in Dir ist Gott!", "tokens": ["Denn", "in", "Dir", "ist", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}