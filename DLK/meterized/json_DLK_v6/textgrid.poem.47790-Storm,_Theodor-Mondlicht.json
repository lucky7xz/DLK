{"textgrid.poem.47790": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Mondlicht", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie liegt im Mondenlichte", "tokens": ["Wie", "liegt", "im", "Mon\u00b7den\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begraben nun die Welt;", "tokens": ["Be\u00b7gra\u00b7ben", "nun", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie selig ist der Friede,", "tokens": ["Wie", "se\u00b7lig", "ist", "der", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sie umfangen h\u00e4lt!", "tokens": ["Der", "sie", "um\u00b7fan\u00b7gen", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Winde m\u00fcssen schweigen,", "tokens": ["Die", "Win\u00b7de", "m\u00fcs\u00b7sen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sanft ist dieser Schein;", "tokens": ["So", "sanft", "ist", "die\u00b7ser", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie s\u00e4useln nur und weben", "tokens": ["Sie", "s\u00e4u\u00b7seln", "nur", "und", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schlafen endlich ein.", "tokens": ["Und", "schla\u00b7fen", "end\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und was in Tagesgluten", "tokens": ["Und", "was", "in", "Ta\u00b7ges\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Bl\u00fcte nicht erwacht,", "tokens": ["Zur", "Bl\u00fc\u00b7te", "nicht", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es \u00f6ffnet seine Kelche", "tokens": ["Es", "\u00f6ff\u00b7net", "sei\u00b7ne", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und duftet in die Nacht.", "tokens": ["Und", "duf\u00b7tet", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie bin ich solchen Friedens", "tokens": ["Wie", "bin", "ich", "sol\u00b7chen", "Frie\u00b7dens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seit lange nicht gewohnt!", "tokens": ["Seit", "lan\u00b7ge", "nicht", "ge\u00b7wohnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei du in meinem Leben", "tokens": ["Sei", "du", "in", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der liebevolle Mond!", "tokens": ["Der", "lie\u00b7be\u00b7vol\u00b7le", "Mond", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}