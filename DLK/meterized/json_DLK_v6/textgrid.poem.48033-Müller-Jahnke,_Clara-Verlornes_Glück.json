{"textgrid.poem.48033": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Verlornes Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal, eh' am Himmelsrande", "tokens": ["Noch", "ein\u00b7mal", ",", "eh'", "am", "Him\u00b7mels\u00b7ran\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der letzte Sonnenblick vergl\u00fcht,", "tokens": ["der", "letz\u00b7te", "Son\u00b7nen\u00b7blick", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "zieht mich ein Sehnen an die St\u00e4tte,", "tokens": ["zieht", "mich", "ein", "Seh\u00b7nen", "an", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wo meines Lebens Gl\u00fcck gebl\u00fcht.", "tokens": ["wo", "mei\u00b7nes", "Le\u00b7bens", "Gl\u00fcck", "ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch hochgew\u00f6lbte G\u00e4nge fluten", "tokens": ["Durch", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7te", "G\u00e4n\u00b7ge", "flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "der D\u00e4mmrung Schatten kalt und bleich \u2013", "tokens": ["der", "D\u00e4mm\u00b7rung", "Schat\u00b7ten", "kalt", "und", "bleich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "leis mahnend pocht wie Geisterfinger", "tokens": ["leis", "mah\u00b7nend", "pocht", "wie", "Geis\u00b7ter\u00b7fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "ans Fenster ein Spireenzweig.", "tokens": ["ans", "Fens\u00b7ter", "ein", "Spi\u00b7reen\u00b7zweig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und rings im Haus ein tiefes Schweigen,", "tokens": ["Und", "rings", "im", "Haus", "ein", "tie\u00b7fes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie ausgestorben jeder Raum . . .", "tokens": ["wie", "aus\u00b7ge\u00b7stor\u00b7ben", "je\u00b7der", "Raum", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVPP", "PIAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meiner Seite l\u00e4chelnd wandelt", "tokens": ["An", "mei\u00b7ner", "Sei\u00b7te", "l\u00e4\u00b7chelnd", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein halbvergessner Jugendtraum;", "tokens": ["ein", "halb\u00b7ver\u00b7gess\u00b7ner", "Ju\u00b7gend\u00b7traum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "von weltverlornen K\u00fcsten zaubert", "tokens": ["von", "welt\u00b7ver\u00b7lor\u00b7nen", "K\u00fcs\u00b7ten", "zau\u00b7bert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "entflohene Wonnen er zur\u00fcck", "tokens": ["ent\u00b7flo\u00b7he\u00b7ne", "Won\u00b7nen", "er", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und k\u00fc\u00dft mir in die m\u00fcde Seele", "tokens": ["und", "k\u00fc\u00dft", "mir", "in", "die", "m\u00fc\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "ein letztes Bild vom Erdengl\u00fcck.", "tokens": ["ein", "letz\u00b7tes", "Bild", "vom", "Er\u00b7den\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein letztes Lied in diesen R\u00e4umen!", "tokens": ["Ein", "letz\u00b7tes", "Lied", "in", "die\u00b7sen", "R\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herbstwind rast am Gartentor \u2013", "tokens": ["Der", "Herbst\u00b7wind", "rast", "am", "Gar\u00b7ten\u00b7tor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "hier aber wogen Rosend\u00fcfte", "tokens": ["hier", "a\u00b7ber", "wo\u00b7gen", "Ro\u00b7sen\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und singt ein Nachtigallenchor.", "tokens": ["und", "singt", "ein", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von all den s\u00fc\u00dfen Liebesworten,", "tokens": ["Von", "all", "den", "s\u00fc\u00b7\u00dfen", "Lie\u00b7bes\u00b7wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die schmeichelnd deine Lippe sprach, \u2013", "tokens": ["die", "schmei\u00b7chelnd", "dei\u00b7ne", "Lip\u00b7pe", "sprach", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "von meinen Seufzern, deinen K\u00fcssen", "tokens": ["von", "mei\u00b7nen", "Seuf\u00b7zern", ",", "dei\u00b7nen", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "wird hier ein fl\u00fcsternd Echo wach.", "tokens": ["wird", "hier", "ein", "fl\u00fcs\u00b7ternd", "E\u00b7cho", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der alte Zauber lockt mich wieder,", "tokens": ["Der", "al\u00b7te", "Zau\u00b7ber", "lockt", "mich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Leib und Seele mir gebannt:", "tokens": ["der", "Leib", "und", "See\u00b7le", "mir", "ge\u00b7bannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dein Odem \u00fcber meiner Stirne,", "tokens": ["dein", "O\u00b7dem", "\u00fc\u00b7ber", "mei\u00b7ner", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "auf meinem Herzen deine Hand!", "tokens": ["auf", "mei\u00b7nem", "Her\u00b7zen", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Spiegel wirft im D\u00e4mmerschimmer", "tokens": ["Der", "Spie\u00b7gel", "wirft", "im", "D\u00e4m\u00b7mer\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "mir dein geliebtes Bild zur\u00fcck \u2013 \u2013", "tokens": ["mir", "dein", "ge\u00b7lieb\u00b7tes", "Bild", "zu\u00b7r\u00fcck", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "zum letzten Male trink ich wieder", "tokens": ["zum", "letz\u00b7ten", "Ma\u00b7le", "trink", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "aus deinem Born, verlornes Gl\u00fcck!", "tokens": ["aus", "dei\u00b7nem", "Born", ",", "ver\u00b7lor\u00b7nes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und lauter t\u00f6nt des Windes Brausen,", "tokens": ["Und", "lau\u00b7ter", "t\u00f6nt", "des", "Win\u00b7des", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Sonne letzter Strahl erblich;", "tokens": ["der", "Son\u00b7ne", "letz\u00b7ter", "Strahl", "er\u00b7blich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich aber berg in meine H\u00e4nde", "tokens": ["ich", "a\u00b7ber", "berg", "in", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das Haupt und weine bitterlich.", "tokens": ["das", "Haupt", "und", "wei\u00b7ne", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun liegt die Nacht auf allen Wegen . . . . . .", "tokens": ["Nun", "liegt", "die", "Nacht", "auf", "al\u00b7len", "We\u00b7gen", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und langsam wend ich meinen Schritt", "tokens": ["und", "lang\u00b7sam", "wend", "ich", "mei\u00b7nen", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und nehm aus den geliebten R\u00e4umen", "tokens": ["und", "nehm", "aus", "den", "ge\u00b7lieb\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "mir der Erinnrung Sterne mit.", "tokens": ["mir", "der", "E\u00b7rinn\u00b7rung", "Ster\u00b7ne", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}