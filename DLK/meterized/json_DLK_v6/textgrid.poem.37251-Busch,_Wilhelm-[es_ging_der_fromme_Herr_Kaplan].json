{"textgrid.poem.37251": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es ging der fromme Herr Kaplan]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging der fromme Herr Kaplan,", "tokens": ["Es", "ging", "der", "from\u00b7me", "Herr", "Ka\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem er bereits viel Gutes getan,", "tokens": ["Nach\u00b7dem", "er", "be\u00b7reits", "viel", "Gu\u00b7tes", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In stiller Betrachtung der sch\u00f6nen Natur", "tokens": ["In", "stil\u00b7ler", "Be\u00b7trach\u00b7tung", "der", "sch\u00f6\u00b7nen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Einst zur Erholung durch die Flur.", "tokens": ["Einst", "zur", "Er\u00b7ho\u00b7lung", "durch", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Und als er kam an den Waldessaum,", "tokens": ["Und", "als", "er", "kam", "an", "den", "Wal\u00b7des\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da rief der Kuckuck lustig vom Baum:", "tokens": ["Da", "rief", "der", "Ku\u00b7ckuck", "lus\u00b7tig", "vom", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "W\u00fcnsch guten Abend, Herr Kollege!", "tokens": ["W\u00fcnsch", "gu\u00b7ten", "A\u00b7bend", ",", "Herr", "Kol\u00b7le\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Storch dagegen, nicht weit vom Wege,", "tokens": ["Der", "Storch", "da\u00b7ge\u00b7gen", ",", "nicht", "weit", "vom", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Steigt in der Wiese auf und ab", "tokens": ["Steigt", "in", "der", "Wie\u00b7se", "auf", "und", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spricht verdrie\u00dflich: Plapperapapp!", "tokens": ["Und", "spricht", "ver\u00b7drie\u00df\u00b7lich", ":", "Plap\u00b7pe\u00b7ra\u00b7papp", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "G\u00e4b's lauter Pfaffen lobesam,", "tokens": ["G\u00e4b's", "lau\u00b7ter", "Pfaf\u00b7fen", "lo\u00b7be\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00e4re l\u00e4ngst schon fl\u00fcgellahm!", "tokens": ["Ich", "w\u00e4\u00b7re", "l\u00e4ngst", "schon", "fl\u00fc\u00b7gel\u00b7lahm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man sieht, da\u00df selbst der fr\u00f6mmste Mann", "tokens": ["Man", "sieht", ",", "da\u00df", "selbst", "der", "fr\u00f6mms\u00b7te", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht allen Leuten gefallen kann.", "tokens": ["Nicht", "al\u00b7len", "Leu\u00b7ten", "ge\u00b7fal\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}