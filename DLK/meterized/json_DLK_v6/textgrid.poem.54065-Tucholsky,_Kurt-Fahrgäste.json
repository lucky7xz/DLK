{"textgrid.poem.54065": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Fahrg\u00e4ste", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchmorgens, wenn das graue Licht", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", ",", "wenn", "das", "grau\u00b7e", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "durch Jalousien sickert;", "tokens": ["durch", "Ja\u00b7lou\u00b7si\u00b7en", "si\u00b7ckert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn j\u00e4h dein Schlaf in Kr\u00fcmel bricht,", "tokens": ["wenn", "j\u00e4h", "dein", "Schlaf", "in", "Kr\u00fc\u00b7mel", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Wecker tickt und tickert:", "tokens": ["der", "We\u00b7cker", "tickt", "und", "ti\u00b7ckert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "dann fahren und stuckern und fahren sie so", "tokens": ["dann", "fah\u00b7ren", "und", "stu\u00b7ckern", "und", "fah\u00b7ren", "sie", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVINF", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "in die Federnfabrik und ins Auskunftsb\u00fcro . . .", "tokens": ["in", "die", "Fe\u00b7dern\u00b7fab\u00b7rik", "und", "ins", "Aus\u00b7kunfts\u00b7b\u00fc\u00b7ro", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPRART", "NN", "$.", "$.", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Die Leute von der Spree,", "tokens": ["Die", "Leu\u00b7te", "von", "der", "Spree", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "die st\u00fcrzen ins Gefecht sich", "tokens": ["die", "st\u00fcr\u00b7zen", "ins", "Ge\u00b7fecht", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "mit der F \u2013 mit der I \u2013 mit der W \u2013", "tokens": ["mit", "der", "F", "\u2013", "mit", "der", "I", "\u2013", "mit", "der", "W", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "XY", "$(", "APPR", "ART", "NE", "$("], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "mit der Q \u2013 mit der 69.", "tokens": ["mit", "der", "Q", "\u2013", "mit", "der", "69."], "token_info": ["word", "word", "word", "punct", "word", "word", "ordinal"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "ADJA"], "meter": "+----", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Sie sitzen wie die V\u00f6gel da", "tokens": ["Sie", "sit\u00b7zen", "wie", "die", "V\u00f6\u00b7gel", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auf einer langen Stange.", "tokens": ["auf", "ei\u00b7ner", "lan\u00b7gen", "Stan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sind sich alle gar so nah", "tokens": ["Sie", "sind", "sich", "al\u00b7le", "gar", "so", "nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "PIS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Kampf, im Druck, im Zwange.", "tokens": ["im", "Kampf", ",", "im", "Druck", ",", "im", "Zwan\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch jeder lebt auf dem eigenen Stern;", "tokens": ["Doch", "je\u00b7der", "lebt", "auf", "dem", "ei\u00b7ge\u00b7nen", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "sie sehn sich nicht an und sie haben sich nicht gern . . .", "tokens": ["sie", "sehn", "sich", "nicht", "an", "und", "sie", "ha\u00b7ben", "sich", "nicht", "gern", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "KON", "PPER", "VAFIN", "PRF", "PTKNEG", "ADV", "$.", "$.", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der liebt die Rotarmee,", "tokens": ["Der", "liebt", "die", "Rot\u00b7ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "der orientiert nach rechts sich \u2013", "tokens": ["der", "o\u00b7rien\u00b7tiert", "nach", "rechts", "sich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "ADV", "PRF", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "mit der F \u2013 mit der I \u2013 mit der W \u2013", "tokens": ["mit", "der", "F", "\u2013", "mit", "der", "I", "\u2013", "mit", "der", "W", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "XY", "$(", "APPR", "ART", "NE", "$("], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "mit der Q \u2013 mit der 69.", "tokens": ["mit", "der", "Q", "\u2013", "mit", "der", "69."], "token_info": ["word", "word", "word", "punct", "word", "word", "ordinal"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "ADJA"], "meter": "+----", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Die Scheiben klirrn. Der Mittag naht.", "tokens": ["Die", "Schei\u00b7ben", "klirrn", ".", "Der", "Mit\u00b7tag", "naht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hunderttausend Leute,", "tokens": ["Die", "hun\u00b7dert\u00b7tau\u00b7send", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "sie fahren dienstlich und privat,", "tokens": ["sie", "fah\u00b7ren", "dienst\u00b7lich", "und", "pri\u00b7vat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die Kerls und ihre Br\u00e4ute . . .", "tokens": ["die", "Kerls", "und", "ih\u00b7re", "Br\u00e4u\u00b7te", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur manchmal blitzt auf in dem laufenden Band", "tokens": ["Nur", "manch\u00b7mal", "blitzt", "auf", "in", "dem", "lau\u00b7fen\u00b7den", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "ein Gedanke an Sonntag und Havelstrand . . .", "tokens": ["ein", "Ge\u00b7dan\u00b7ke", "an", "Sonn\u00b7tag", "und", "Ha\u00b7vel\u00b7strand", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NE", "$.", "$.", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Ein Blick . . . Ein stummes: He!", "tokens": ["Ein", "Blick", ".", ".", ".", "Ein", "stum\u00b7mes", ":", "He", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "$.", "$.", "ART", "ADJA", "$.", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dann meldet das Geschlecht sich", "tokens": ["Dann", "mel\u00b7det", "das", "Ge\u00b7schlecht", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "mit der F \u2013 mit der I \u2013 mit der W \u2013", "tokens": ["mit", "der", "F", "\u2013", "mit", "der", "I", "\u2013", "mit", "der", "W", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "XY", "$(", "APPR", "ART", "NE", "$("], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "mit der Q \u2013 mit der 69.", "tokens": ["mit", "der", "Q", "\u2013", "mit", "der", "69."], "token_info": ["word", "word", "word", "punct", "word", "word", "ordinal"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "ADJA"], "meter": "+----", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Und abends, staubig im Gesicht,", "tokens": ["Und", "a\u00b7bends", ",", "stau\u00b7big", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so fahren sie heim und schwanken.", "tokens": ["so", "fah\u00b7ren", "sie", "heim", "und", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer Arbeit hat, der jammere nicht,", "tokens": ["Wer", "Ar\u00b7beit", "hat", ",", "der", "jam\u00b7me\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "er darf dem Herrgott danken.", "tokens": ["er", "darf", "dem", "Herr\u00b7gott", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja, denkt denn da keiner \u2013 wies schade ist! \u2013", "tokens": ["Ja", ",", "denkt", "denn", "da", "kei\u00b7ner", "\u2013", "wies", "scha\u00b7de", "ist", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ADV", "PIS", "$(", "VVFIN", "ADJD", "VAFIN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "da\u00df Arbeit doch keine Gnade ist?", "tokens": ["da\u00df", "Ar\u00b7beit", "doch", "kei\u00b7ne", "Gna\u00b7de", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Arbeitende Armee!", "tokens": ["Ar\u00b7bei\u00b7ten\u00b7de", "Ar\u00b7mee", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.8": {"text": "Wann nimmt sie wohl ihr Recht sich . . .", "tokens": ["Wann", "nimmt", "sie", "wohl", "ihr", "Recht", "sich", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PRF", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mit der F \u2013 mit der I \u2013 mit der W \u2013", "tokens": ["Mit", "der", "F", "\u2013", "mit", "der", "I", "\u2013", "mit", "der", "W", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "XY", "$(", "APPR", "ART", "NE", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "mit der Q \u2013 mit der 69!", "tokens": ["mit", "der", "Q", "\u2013", "mit", "der", "69", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "number", "punct"], "pos": ["APPR", "ART", "NE", "$(", "APPR", "ART", "CARD", "$."], "meter": "+----", "measure": "dactylic.init"}}}}}