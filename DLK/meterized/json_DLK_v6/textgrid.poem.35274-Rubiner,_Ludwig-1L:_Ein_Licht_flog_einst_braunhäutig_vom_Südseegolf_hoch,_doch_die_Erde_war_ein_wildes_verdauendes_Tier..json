{"textgrid.poem.35274": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Licht flog einst braunh\u00e4utig vom S\u00fcdseegolf hoch, doch die Erde war ein wildes verdauendes Tier.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Licht flog einst braunh\u00e4utig vom S\u00fcdseegolf hoch, doch die Erde war ein wildes verdauendes Tier.", "tokens": ["Ein", "Licht", "flog", "einst", "braun\u00b7h\u00e4u\u00b7tig", "vom", "S\u00fcd\u00b7see\u00b7golf", "hoch", ",", "doch", "die", "Er\u00b7de", "war", "ein", "wil\u00b7des", "ver\u00b7dau\u00b7en\u00b7des", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "ADJD", "$,", "ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--++--+--+--+---+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Jahrtausende mischten Atem und Blut f\u00fcr uns, wir sind Sternbr\u00fcder auf himmlischen Erden.", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "mischten", "A\u00b7tem", "und", "Blut", "f\u00fcr", "uns", ",", "wir", "sind", "Stern\u00b7br\u00fc\u00b7der", "auf", "himm\u00b7li\u00b7schen", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "$,", "PPER", "VAFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+---+--+-++-++--+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "O wir m\u00fcssen den Mund auftun und laut reden f\u00fcr alle heute bis zum Morgen.", "tokens": ["O", "wir", "m\u00fcs\u00b7sen", "den", "Mund", "auf\u00b7tun", "und", "laut", "re\u00b7den", "f\u00fcr", "al\u00b7le", "heu\u00b7te", "bis", "zum", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "ART", "NN", "VVINF", "KON", "ADJD", "VVINF", "APPR", "PIS", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Der letzte Reporter ist unser lieber Bruder,", "tokens": ["Der", "letz\u00b7te", "Re\u00b7por\u00b7ter", "ist", "un\u00b7ser", "lie\u00b7ber", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der Reklamechef der gro\u00dfen Kaufh\u00e4user ist unser Bruder!", "tokens": ["Der", "Re\u00b7kla\u00b7me\u00b7chef", "der", "gro\u00b7\u00dfen", "Kauf\u00b7h\u00e4u\u00b7ser", "ist", "un\u00b7ser", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Jeder, der nicht schweigt, ist unser Bruder!", "tokens": ["Je\u00b7der", ",", "der", "nicht", "schweigt", ",", "ist", "un\u00b7ser", "Bru\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PTKNEG", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "O zersprengt die Stahlkasematten Eurer Einsamkeit!", "tokens": ["O", "zer\u00b7sprengt", "die", "Stahl\u00b7ka\u00b7se\u00b7mat\u00b7ten", "Eu\u00b7rer", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "O springt aus den violetten Grotten, wo Eure Schatten im Dunkel aus Eurem Blut lebend schl\u00fcrfen!", "tokens": ["O", "springt", "aus", "den", "vi\u00b7o\u00b7let\u00b7ten", "Grot\u00b7ten", ",", "wo", "Eu\u00b7re", "Schat\u00b7ten", "im", "Dun\u00b7kel", "aus", "Eu\u00b7rem", "Blut", "le\u00b7bend", "schl\u00fcr\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+--+-+--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Schwarzpressender Traum, Vergangenheit, o Schlaf im eisernen Keuchen!", "tokens": ["Schwarz\u00b7pres\u00b7sen\u00b7der", "Traum", ",", "Ver\u00b7gan\u00b7gen\u00b7heit", ",", "o", "Schlaf", "im", "ei\u00b7ser\u00b7nen", "Keu\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "FM", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Aber dann wachen Sie auf, und Ihr Wort spr\u00fcht ums Rund in Kometen und Feuerbrand.", "tokens": ["A\u00b7ber", "dann", "wa\u00b7chen", "Sie", "auf", ",", "und", "Ihr", "Wort", "spr\u00fcht", "ums", "Rund", "in", "Ko\u00b7me\u00b7ten", "und", "Feu\u00b7er\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--++-+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "Sie sind das Auge. Und der schimmernde Raum. Und Sie bauen das neue irdische Land.", "tokens": ["Sie", "sind", "das", "Au\u00b7ge", ".", "Und", "der", "schim\u00b7mern\u00b7de", "Raum", ".", "Und", "Sie", "bau\u00b7en", "das", "neu\u00b7e", "ir\u00b7di\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "KON", "ART", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+--+-+--+", "measure": "iambic.octa.plus.relaxed"}}}}}