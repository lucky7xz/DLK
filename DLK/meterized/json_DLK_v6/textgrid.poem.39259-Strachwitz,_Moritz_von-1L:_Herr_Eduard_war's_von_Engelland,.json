{"textgrid.poem.39259": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Eduard war's von Engelland,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Eduard war's von Engelland,", "tokens": ["Herr", "E\u00b7duard", "wa\u00b7r's", "von", "En\u00b7gel\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ein Ritter, k\u00fchn und gut,", "tokens": ["Ein", "Rit\u00b7ter", ",", "k\u00fchn", "und", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hat ein S\u00f6hnlein, keck und schlank,", "tokens": ["Der", "hat", "ein", "S\u00f6hn\u00b7lein", ",", "keck", "und", "schlank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein rasches Heldenblut.", "tokens": ["Ein", "ra\u00b7sches", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sprach der Herr zum Sohne sein:", "tokens": ["Es", "sprach", "der", "Herr", "zum", "Soh\u00b7ne", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdu Junker frank, hab' acht!", "tokens": ["\u00bb", "du", "Jun\u00b7ker", "frank", ",", "hab'", "acht", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "NE", "VVFIN", "$,", "VAFIN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will dir geben Land und Burg", "tokens": ["Ich", "will", "dir", "ge\u00b7ben", "Land", "und", "Burg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und reisige Heeresmacht.\u00ab", "tokens": ["Und", "rei\u00b7si\u00b7ge", "Hee\u00b7res\u00b7macht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbich will dir geben ein M\u00e4gdlein lieb,", "tokens": ["\u00bb", "ich", "will", "dir", "ge\u00b7ben", "ein", "M\u00e4gd\u00b7lein", "lieb", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Recht rosig rot und mild,", "tokens": ["Recht", "ro\u00b7sig", "rot", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will dir geben ein gutes Schwert", "tokens": ["Ich", "will", "dir", "ge\u00b7ben", "ein", "gu\u00b7tes", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ein schnaubendes Rotro\u00df wild.\u00ab", "tokens": ["Und", "ein", "schnau\u00b7ben\u00b7des", "Rot\u00b7ro\u00df", "wild", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbich will dir geben ein Sammetkleid,", "tokens": ["\u00bb", "ich", "will", "dir", "ge\u00b7ben", "ein", "Sam\u00b7met\u00b7kleid", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von Purpur und Golde blank,", "tokens": ["Von", "Pur\u00b7pur", "und", "Gol\u00b7de", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich will dir geben den Helmbusch wei\u00df", "tokens": ["Ich", "will", "dir", "ge\u00b7ben", "den", "Helm\u00b7busch", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die funkelnde Lanze schlank.\u00ab", "tokens": ["Und", "die", "fun\u00b7keln\u00b7de", "Lan\u00b7ze", "schlank", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "\u00bbla\u00dft Land und Leut' und Burg und Schlo\u00df,", "tokens": ["\u00bb", "la\u00dft", "Land", "und", "Leut'", "und", "Burg", "und", "Schlo\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebt Panzer mir und Schwert,", "tokens": ["Gebt", "Pan\u00b7zer", "mir", "und", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da will ich sitzen auf gutem Ro\u00df,", "tokens": ["Da", "will", "ich", "sit\u00b7zen", "auf", "gu\u00b7tem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein reisiger Ritter wert.\u00ab", "tokens": ["Ein", "rei\u00b7si\u00b7ger", "Rit\u00b7ter", "wert", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbzum Br\u00e4utlein gebt mir Altengland gr\u00fcn,", "tokens": ["\u00bb", "zum", "Br\u00e4ut\u00b7lein", "gebt", "mir", "Al\u00b7ten\u00b7gland", "gr\u00fcn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VVFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das liebliche M\u00e4dchen hold,", "tokens": ["Das", "lieb\u00b7li\u00b7che", "M\u00e4d\u00b7chen", "hold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und gebt zum Gewand mir das Panzerhemd", "tokens": ["Und", "gebt", "zum", "Ge\u00b7wand", "mir", "das", "Pan\u00b7zer\u00b7hemd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Statt Seide und rotem Gold.\u00ab", "tokens": ["Statt", "Sei\u00b7de", "und", "ro\u00b7tem", "Gold", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbund nehmt mich mit euch \u00fcbers blaue Meer", "tokens": ["\u00bb", "und", "nehmt", "mich", "mit", "euch", "\u00fc\u00b7bers", "blau\u00b7e", "Meer"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VVFIN", "PRF", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch tosenden Wogendrang,", "tokens": ["Durch", "to\u00b7sen\u00b7den", "Wo\u00b7gen\u00b7drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da will ich zerbrechen des Franzmanns Helm", "tokens": ["Da", "will", "ich", "zer\u00b7bre\u00b7chen", "des", "Franz\u00b7manns", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit grimmigem Schwertesschwang.\u00ab", "tokens": ["Mit", "grim\u00b7mi\u00b7gem", "Schwer\u00b7tes\u00b7schwang", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbda will ich zerrei\u00dfen des Franzmanns Panier,", "tokens": ["\u00bb", "da", "will", "ich", "zer\u00b7rei\u00b7\u00dfen", "des", "Franz\u00b7manns", "Pa\u00b7nier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Will's treten in Schmach und Tod,", "tokens": ["Will's", "tre\u00b7ten", "in", "Schmach", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da will ich stampfen die Lilie wei\u00df", "tokens": ["Da", "will", "ich", "stamp\u00b7fen", "die", "Li\u00b7lie", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tief in den blutigen Kot.\u00ab", "tokens": ["Tief", "in", "den", "blu\u00b7ti\u00b7gen", "Kot", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbso ziehe zum Meere, du Ritter frank,", "tokens": ["\u00bb", "so", "zie\u00b7he", "zum", "Mee\u00b7re", ",", "du", "Rit\u00b7ter", "frank", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPRART", "NN", "$,", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nach Frankreich mit Schwert und Schild.", "tokens": ["Nach", "Fran\u00b7kreich", "mit", "Schwert", "und", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nehme St. Georg dich in feste Hut,", "tokens": ["Und", "neh\u00b7me", "St.", "Ge\u00b7org", "dich", "in", "fes\u00b7te", "Hut", ","], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Du wackerer Degen wild.\u00ab", "tokens": ["Du", "wa\u00b7cke\u00b7rer", "De\u00b7gen", "wild", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}