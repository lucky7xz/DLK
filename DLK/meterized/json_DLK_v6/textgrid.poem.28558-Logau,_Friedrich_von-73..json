{"textgrid.poem.28558": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Malprobus ist ein Schelm und nam ihm eine Hure;", "tokens": ["Mal\u00b7pro\u00b7bus", "ist", "ein", "Schelm", "und", "nam", "ihm", "ei\u00b7ne", "Hu\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ey, recht! sagt alles Volk, da\u00df di\u00df ihm widerfuhre.", "tokens": ["Ey", ",", "recht", "!", "sagt", "al\u00b7les", "Volk", ",", "da\u00df", "di\u00df", "ihm", "wi\u00b7der\u00b7fuh\u00b7re", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$.", "VVFIN", "PIAT", "NN", "$,", "KOUS", "PDS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Topff bekam also, wie schicklich, eine St\u00fcrtze;", "tokens": ["Der", "Topff", "be\u00b7kam", "al\u00b7so", ",", "wie", "schick\u00b7lich", ",", "ei\u00b7ne", "St\u00fcrt\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geborgtes ward gezahlt; drum hat er keine K\u00fcrtze.", "tokens": ["Ge\u00b7borg\u00b7tes", "ward", "ge\u00b7zahlt", ";", "drum", "hat", "er", "kei\u00b7ne", "K\u00fcrt\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$.", "PAV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}