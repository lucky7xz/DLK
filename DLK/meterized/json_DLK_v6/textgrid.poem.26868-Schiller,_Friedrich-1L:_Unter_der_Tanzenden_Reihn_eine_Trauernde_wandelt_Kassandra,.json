{"textgrid.poem.26868": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unter der Tanzenden Reihn eine Trauernde wandelt Kassandra,", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter der Tanzenden Reihn eine Trauernde wandelt Kassandra,", "tokens": ["Un\u00b7ter", "der", "Tan\u00b7zen\u00b7den", "Reihn", "ei\u00b7ne", "Trau\u00b7ern\u00b7de", "wan\u00b7delt", "Kas\u00b7sand\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Mit dem Lorbeer Apolls kr\u00e4nzt sie die g\u00f6ttliche Stirn.", "tokens": ["Mit", "dem", "Lor\u00b7beer", "A\u00b7polls", "kr\u00e4nzt", "sie", "die", "g\u00f6tt\u00b7li\u00b7che", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch die Trauer ist sch\u00f6n, wenn sie g\u00f6ttlich ist, und mit der Freude", "tokens": ["Auch", "die", "Trau\u00b7er", "ist", "sch\u00f6n", ",", "wenn", "sie", "g\u00f6tt\u00b7lich", "ist", ",", "und", "mit", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "M\u00f6ge, lieblich gesellt, wandeln der heilige Ernst.", "tokens": ["M\u00f6\u00b7ge", ",", "lieb\u00b7lich", "ge\u00b7sellt", ",", "wan\u00b7deln", "d