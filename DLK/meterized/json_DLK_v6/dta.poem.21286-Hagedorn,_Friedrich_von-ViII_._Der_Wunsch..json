{"dta.poem.21286": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "ViII .  \n  Der Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du holder Gott der s\u00fcssten Lust auf Erden,", "tokens": ["Du", "hol\u00b7der", "Gott", "der", "s\u00fcss\u00b7ten", "Lust", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6nsten G\u00f6ttinn sch\u00f6ner Sohn!", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "G\u00f6t\u00b7tinn", "sch\u00f6\u00b7ner", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, lehre mich die Kunst, geliebt zu werden;", "tokens": ["Komm", ",", "leh\u00b7re", "mich", "die", "Kunst", ",", "ge\u00b7liebt", "zu", "wer\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "ART", "NN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die leichte Kunst zu lieben wei\u00df ich schon.", "tokens": ["Die", "leich\u00b7te", "Kunst", "zu", "lie\u00b7ben", "wei\u00df", "ich", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Komm ebenfalls und bilde Phyllis Lachen,", "tokens": ["Komm", "e\u00b7ben\u00b7falls", "und", "bil\u00b7de", "Phyl\u00b7lis", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Cythere! gib ihr Unterricht;", "tokens": ["Cy\u00b7the\u00b7re", "!", "gib", "ihr", "Un\u00b7ter\u00b7richt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Phyllis wei\u00df die Kunst verliebt zu machen;", "tokens": ["Denn", "Phyl\u00b7lis", "wei\u00df", "die", "Kunst", "ver\u00b7liebt", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die leichte Kunst zu lieben wei\u00df sie nicht.", "tokens": ["Die", "leich\u00b7te", "Kunst", "zu", "lie\u00b7ben", "wei\u00df", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}