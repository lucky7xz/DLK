{"textgrid.poem.66197": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An mein Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil dir, du liebes Vaterland,", "tokens": ["Heil", "dir", ",", "du", "lie\u00b7bes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Land voll Kraft und Muth!", "tokens": ["Du", "Land", "voll", "Kraft", "und", "Muth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heil dir! dein Kaiserkronengold", "tokens": ["Heil", "dir", "!", "dein", "Kai\u00b7ser\u00b7kro\u00b7nen\u00b7gold"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gabst du dem K\u00f6nig ", "tokens": ["Gabst", "du", "dem", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie gro\u00df ist er, wie gut!", "tokens": ["Wie", "gro\u00df", "ist", "er", ",", "wie", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "All\u00fcberall verk\u00fcndet es", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "ver\u00b7k\u00fcn\u00b7det", "es"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Metallner Donnerton.", "tokens": ["Me\u00b7tall\u00b7ner", "Don\u00b7ner\u00b7ton", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn du ein deutscher B\u00fcrger bist,", "tokens": ["Wenn", "du", "ein", "deut\u00b7scher", "B\u00fcr\u00b7ger", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So jauchze laut! Dein Kaiser ist", "tokens": ["So", "jauch\u00b7ze", "laut", "!", "Dein", "Kai\u00b7ser", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Theresens weiser Sohn.", "tokens": ["The\u00b7re\u00b7sens", "wei\u00b7ser", "Sohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Die g\u00f6ttliche Religion", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Hat ihn so gut gemacht,", "tokens": ["Hat", "ihn", "so", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die g\u00f6ttliche Religion,", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Sie, die des h\u00f6chsten Herrschers Sohn", "tokens": ["Sie", ",", "die", "des", "h\u00f6chs\u00b7ten", "Herr\u00b7schers", "Sohn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf unsre Welt gebracht.", "tokens": ["Auf", "uns\u00b7re", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum tr\u00e4gt die Wage er so st\u00e4t,", "tokens": ["Drum", "tr\u00e4gt", "die", "Wa\u00b7ge", "er", "so", "st\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihm ein Engel gab,", "tokens": ["Die", "ihm", "ein", "En\u00b7gel", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er w\u00e4gt, gleich ist ihm Herr und Knecht,", "tokens": ["Er", "w\u00e4gt", ",", "gleich", "ist", "ihm", "Herr", "und", "Knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des F\u00fcrsten Recht, des Bettlers Recht", "tokens": ["Des", "F\u00fcrs\u00b7ten", "Recht", ",", "des", "Bett\u00b7lers", "Recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf gleichen Schalen ab.", "tokens": ["Auf", "glei\u00b7chen", "Scha\u00b7len", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dem Weisen, Guten, Frommen ist", "tokens": ["Dem", "Wei\u00b7sen", ",", "Gu\u00b7ten", ",", "From\u00b7men", "ist"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Auge Sonnenschein;", "tokens": ["Sein", "Au\u00b7ge", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lohn gl\u00e4nzt f\u00fcr sie in seiner Hand.", "tokens": ["Lohn", "gl\u00e4nzt", "f\u00fcr", "sie", "in", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wird sein Auge Wetterbrand", "tokens": ["Doch", "wird", "sein", "Au\u00b7ge", "Wet\u00b7ter\u00b7brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Lasterhaften sein.", "tokens": ["Dem", "Las\u00b7ter\u00b7haf\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr Musen, stellt euch um ihn her,", "tokens": ["Ihr", "Mu\u00b7sen", ",", "stellt", "euch", "um", "ihn", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist den Musen hold,", "tokens": ["Er", "ist", "den", "Mu\u00b7sen", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der guten F\u00fcrstin S\u00e4ngerin", "tokens": ["Der", "gu\u00b7ten", "F\u00fcrs\u00b7tin", "S\u00e4n\u00b7ge\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt in die goldene Telyn", "tokens": ["T\u00f6nt", "in", "die", "gol\u00b7de\u00b7ne", "Te\u00b7lyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Namen ", "tokens": ["Den", "Na\u00b7men"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Der Aufruhr, dessen Schwefelhauch", "tokens": ["Der", "Auf\u00b7ruhr", ",", "des\u00b7sen", "Schwe\u00b7fel\u00b7hauch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herzen wild beflammt,", "tokens": ["Die", "Her\u00b7zen", "wild", "be\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flieht aus Thuiskons Eichenhain", "tokens": ["Flieht", "aus", "Thu\u00b7is\u00b7kons", "Ei\u00b7chen\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Im H\u00f6llenschlund verdammt.", "tokens": ["Im", "H\u00f6l\u00b7len\u00b7schlund", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es herrscht des Himmels sch\u00f6nster Sohn,", "tokens": ["Es", "herrscht", "des", "Him\u00b7mels", "sch\u00f6ns\u00b7ter", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Friede, weit und breit;", "tokens": ["Der", "Frie\u00b7de", ",", "weit", "und", "breit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihm geht Duldung Hand in Hand", "tokens": ["Mit", "ihm", "geht", "Dul\u00b7dung", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch unser liebes Vaterland,", "tokens": ["Durch", "un\u00b7ser", "lie\u00b7bes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Treu und Redlichkeit.", "tokens": ["Und", "Treu", "und", "Red\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr deutschen Br\u00fcder, mich durchdringt", "tokens": ["Ihr", "deut\u00b7schen", "Br\u00fc\u00b7der", ",", "mich", "durch\u00b7dringt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels Vorgef\u00fchl;", "tokens": ["Des", "Him\u00b7mels", "Vor\u00b7ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es t\u00f6nt zusammen ", "tokens": ["Es", "t\u00f6nt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So himmlisch, wie ein hohes Lied,", "tokens": ["So", "himm\u00b7lisch", ",", "wie", "ein", "ho\u00b7hes", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Engelharfenspiel.", "tokens": ["Auf", "En\u00b7gel\u00b7har\u00b7fen\u00b7spiel", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}