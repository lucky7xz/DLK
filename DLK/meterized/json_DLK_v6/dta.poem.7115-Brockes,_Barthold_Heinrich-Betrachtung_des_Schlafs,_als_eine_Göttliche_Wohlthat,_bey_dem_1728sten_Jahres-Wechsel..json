{"dta.poem.7115": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist mehr, als man glaubt, Danck-und Bewunderns werth,", "tokens": ["Es", "ist", "mehr", ",", "als", "man", "glaubt", ",", "Dan\u00b7ck\u00b7\u00b7und", "Be\u00b7wun\u00b7derns", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "NN", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was uns von GOTT im Schlaf vor Gnade wiederf\u00e4hrt.", "tokens": ["Was", "uns", "von", "GoTT", "im", "Schlaf", "vor", "Gna\u00b7de", "wie\u00b7der\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn wir aufmercksam \u00fcberlegen,", "tokens": ["Wenn", "wir", "auf\u00b7merck\u00b7sam", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was GOTT, durch die Natur, zu unsrer Ruh\u2019,", "tokens": ["Was", "GoTT", ",", "durch", "die", "Na\u00b7tur", ",", "zu", "uns\u00b7rer", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$,", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr Wunder-Dinge w\u00fcrck\u2019, und was sie desfalls thu\u2019;", "tokens": ["F\u00fcr", "Wun\u00b7der\u00b7Din\u00b7ge", "w\u00fcrck'", ",", "und", "was", "sie", "des\u00b7falls", "thu'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$,", "KON", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan man blo\u00df allein", "tokens": ["So", "kan", "man", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus den so wichtigen Besch\u00e4fftigungen sehen,", "tokens": ["Aus", "den", "so", "wich\u00b7ti\u00b7gen", "Be\u00b7sch\u00e4ff\u00b7ti\u00b7gun\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie wichtig und wie gro\u00df der Endzweck m\u00fcsse seyn,", "tokens": ["Wie", "wich\u00b7tig", "und", "wie", "gro\u00df", "der", "End\u00b7zweck", "m\u00fcs\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weshalben sie mit so viel Kunst geschehen.", "tokens": ["We\u00b7shal\u00b7ben", "sie", "mit", "so", "viel", "Kunst", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}