{"textgrid.poem.48932": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieg ich/ Arme/ noch gebunden/", "tokens": ["Wie", "lieg", "ich", "/", "Ar\u00b7me", "/", "noch", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "NN", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie druckt mich meiner Fessel Last.", "tokens": ["Wie", "druckt", "mich", "mei\u00b7ner", "Fes\u00b7sel", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich meint'/ ich h\u00e4tte Freyheit funden/", "tokens": ["Ich", "meint'", "/", "ich", "h\u00e4t\u00b7te", "Frey\u00b7heit", "fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als mich die Lieb der Welt gefast.", "tokens": ["Als", "mich", "die", "Lieb", "der", "Welt", "ge\u00b7fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wollt ich ungebunden gehn/", "tokens": ["Da", "wollt", "ich", "un\u00b7ge\u00b7bun\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und meinen freyen Willen haben:", "tokens": ["Und", "mei\u00b7nen", "frey\u00b7en", "Wil\u00b7len", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drauff must ich zu gebotte stehn", "tokens": ["Drauff", "must", "ich", "zu", "ge\u00b7bot\u00b7te", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Feinden/ die mich nicht lo\u00df gaben.", "tokens": ["Den", "Fein\u00b7den", "/", "die", "mich", "nicht", "lo\u00df", "ga\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nun sah ich/ da\u00df ich noch nicht bin", "tokens": ["Nun", "sah", "ich", "/", "da\u00df", "ich", "noch", "nicht", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "PTKNEG", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Au\u00df ihrer Macht und Stricken gangen:", "tokens": ["Au\u00df", "ih\u00b7rer", "Macht", "und", "Stri\u00b7cken", "gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ach Herr/ nimm diese Fessel hin:", "tokens": ["Ach", "Herr", "/", "nimm", "die\u00b7se", "Fes\u00b7sel", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "VVIMP", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mach lo\u00df/ was noch von mir gefangen.", "tokens": ["Mach", "lo\u00df", "/", "was", "noch", "von", "mir", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "PWS", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Welt-Lieb ist noch sehr subtil", "tokens": ["Die", "Welt\u00b7Lieb", "ist", "noch", "sehr", "sub\u00b7til"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In mir nach der Natur verborgen/", "tokens": ["In", "mir", "nach", "der", "Na\u00b7tur", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und was ich noch vom Fleische f\u00fchl/", "tokens": ["Und", "was", "ich", "noch", "vom", "Flei\u00b7sche", "f\u00fchl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Das macht dem Geist viel tausend Sorgen.", "tokens": ["Das", "macht", "dem", "Geist", "viel", "tau\u00b7send", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "O schaue mein Gef\u00e4ngni\u00df an/", "tokens": ["O", "schau\u00b7e", "mein", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ich lasse nimmer ab zu schreyen.", "tokens": ["Ich", "las\u00b7se", "nim\u00b7mer", "ab", "zu", "schre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Doch deine Langmuth ists/ die kan", "tokens": ["Doch", "dei\u00b7ne", "Lang\u00b7muth", "ists", "/", "die", "kan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$(", "PDS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mich nach Verzug geschwind erfreuen.", "tokens": ["Mich", "nach", "Ver\u00b7zug", "ge\u00b7schwind", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Mach nur eins nach dem andern los/", "tokens": ["Mach", "nur", "eins", "nach", "dem", "an\u00b7dern", "los", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So komm ich frey in deinen Schoos.", "tokens": ["So", "komm", "ich", "frey", "in", "dei\u00b7nen", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}