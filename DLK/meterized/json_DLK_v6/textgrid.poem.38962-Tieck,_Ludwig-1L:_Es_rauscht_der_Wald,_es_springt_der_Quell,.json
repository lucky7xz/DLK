{"textgrid.poem.38962": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es rauscht der Wald, es springt der Quell,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht der Wald, es springt der Quell,", "tokens": ["Es", "rauscht", "der", "Wald", ",", "es", "springt", "der", "Quell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne scheint hernieder,", "tokens": ["Die", "Son\u00b7ne", "scheint", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wandert froh der Junggesell,", "tokens": ["Da", "wan\u00b7dert", "froh", "der", "Jung\u00b7ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Singt Baum und Felsen seine Lieder", "tokens": ["Singt", "Baum", "und", "Fel\u00b7sen", "sei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem muntern freien Blut", "tokens": ["Dem", "mun\u00b7tern", "frei\u00b7en", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die ganze Welt so hold und freundlich thut.", "tokens": ["Die", "gan\u00b7ze", "Welt", "so", "hold", "und", "freund\u00b7lich", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da unten ist der St\u00e4dte Zahl,", "tokens": ["Da", "un\u00b7ten", "ist", "der", "St\u00e4d\u00b7te", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wohnen Noth und Leiden,", "tokens": ["Da", "woh\u00b7nen", "Noth", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Armuth klagt im stillen Thal,", "tokens": ["Die", "Ar\u00b7muth", "klagt", "im", "stil\u00b7len", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich wollen Ehleut scheiden,", "tokens": ["Sich", "wol\u00b7len", "Eh\u00b7leut", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da wandert fort, eilt weg so schnell", "tokens": ["Da", "wan\u00b7dert", "fort", ",", "eilt", "weg", "so", "schnell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der muntre lustge Junggesell.", "tokens": ["Der", "mun\u00b7tre", "lust\u00b7ge", "Jung\u00b7ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und will die Lieb' ihn listig fangen,", "tokens": ["Und", "will", "die", "Lieb'", "ihn", "lis\u00b7tig", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lockt ihn die Sehnsucht und Genu\u00df,", "tokens": ["Lockt", "ihn", "die", "Sehn\u00b7sucht", "und", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Er k\u00fc\u00dft die Lippen und die Wangen,", "tokens": ["Er", "k\u00fc\u00dft", "die", "Lip\u00b7pen", "und", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vermeid't des Ehestands Verdru\u00df,", "tokens": ["Ver\u00b7meid't", "des", "E\u00b7hes\u00b7tands", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Spannt man die Heiraths-Netze aus,", "tokens": ["Spannt", "man", "die", "Hei\u00b7raths\u00b7Net\u00b7ze", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich dreht der Knabe sich zur Th\u00fcr hinaus.", "tokens": ["Gleich", "dreht", "der", "Kna\u00b7be", "sich", "zur", "Th\u00fcr", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}