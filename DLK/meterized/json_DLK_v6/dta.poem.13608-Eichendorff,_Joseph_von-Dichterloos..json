{"dta.poem.13608": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Dichterloos.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr Alle mu\u00df vor Freuden", "tokens": ["F\u00fcr", "Al\u00b7le", "mu\u00df", "vor", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein treues Herze gl\u00fch'n,", "tokens": ["Mein", "treu\u00b7es", "Her\u00b7ze", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Alle mu\u00df ich leiden,", "tokens": ["F\u00fcr", "Al\u00b7le", "mu\u00df", "ich", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Alle mu\u00df ich bl\u00fch'n,", "tokens": ["F\u00fcr", "Al\u00b7le", "mu\u00df", "ich", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die Bl\u00fcthen Fr\u00fcchte haben,", "tokens": ["Und", "wenn", "die", "Bl\u00fc\u00b7then", "Fr\u00fcch\u00b7te", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da haben sie mich l\u00e4ngst begraben.", "tokens": ["Da", "ha\u00b7ben", "sie", "mich", "l\u00e4ngst", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}