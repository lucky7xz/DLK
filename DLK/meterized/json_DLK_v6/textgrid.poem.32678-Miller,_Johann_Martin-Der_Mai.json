{"textgrid.poem.32678": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Mai", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6gel schlagen", "tokens": ["V\u00f6\u00b7gel", "schla\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Im Gestr\u00e4uch;", "tokens": ["Im", "Ge\u00b7str\u00e4uch", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Fische jagen", "tokens": ["Fi\u00b7sche", "ja\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sich im Teich.", "tokens": ["Sich", "im", "Teich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Schafe bl\u00f6ken", "tokens": ["Scha\u00b7fe", "bl\u00f6\u00b7ken"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Durch den Klee;", "tokens": ["Durch", "den", "Klee", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mutig lecken", "tokens": ["Mu\u00b7tig", "le\u00b7cken"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hirsch und Reh.", "tokens": ["Hirsch", "und", "Reh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Fl\u00f6ten klingen", "tokens": ["Fl\u00f6\u00b7ten", "klin\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Durch den Hain;", "tokens": ["Durch", "den", "Hain", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hirten schlingen", "tokens": ["Hir\u00b7ten", "schlin\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sich im Reihn.", "tokens": ["Sich", "im", "Reihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Was da lebet,", "tokens": ["Was", "da", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Liebt und lacht,", "tokens": ["Liebt", "und", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und erhebet", "tokens": ["Und", "er\u00b7he\u00b7bet"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Amors Macht.", "tokens": ["A\u00b7mors", "Macht", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Aber tr\u00fcbe", "tokens": ["A\u00b7ber", "tr\u00fc\u00b7be"], "token_info": ["word", "word"], "pos": ["KON", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Flie\u00dft der Mai,", "tokens": ["Flie\u00dft", "der", "Mai", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sonder Liebe,", "tokens": ["Son\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mir vorbei.", "tokens": ["Mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Bang und \u00f6de", "tokens": ["Bang", "und", "\u00f6\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Traurt die Flur;", "tokens": ["Traurt", "die", "Flur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Denn die Spr\u00f6de", "tokens": ["Denn", "die", "Spr\u00f6\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Denk' ich nur.", "tokens": ["Denk'", "ich", "nur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Sch\u00fcchtern fliehet", "tokens": ["Sch\u00fcch\u00b7tern", "flie\u00b7het"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sie zur\u00fcck!", "tokens": ["Sie", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nimmer gl\u00fchet", "tokens": ["Nim\u00b7mer", "gl\u00fc\u00b7het"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Lieb' ihr Blick!", "tokens": ["Lieb'", "ihr", "Blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Und ich weine", "tokens": ["Und", "ich", "wei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meine Qual,", "tokens": ["Mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wie die kleine", "tokens": ["Wie", "die", "klei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Nachtigall.", "tokens": ["Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Tief in Schatten,", "tokens": ["Tief", "in", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sp\u00e4t und fr\u00fch,", "tokens": ["Sp\u00e4t", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Um den Gatten,", "tokens": ["Um", "den", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Jammert sie.", "tokens": ["Jam\u00b7mert", "sie", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Ach! gefangen,", "tokens": ["Ach", "!", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PTKVZ", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Folget er", "tokens": ["Fol\u00b7get", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Nicht dem 