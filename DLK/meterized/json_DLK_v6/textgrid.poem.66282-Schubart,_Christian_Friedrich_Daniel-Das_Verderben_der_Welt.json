{"textgrid.poem.66282": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Das Verderben der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf die verdorbne Welt", "tokens": ["Wenn", "auf", "die", "ver\u00b7dorb\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Still ein Blick der Seele f\u00e4llt:", "tokens": ["Still", "ein", "Blick", "der", "See\u00b7le", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, so schauert mir die Haut,", "tokens": ["O", ",", "so", "schau\u00b7ert", "mir", "die", "Haut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem Tode ruf' ich laut.", "tokens": ["Und", "dem", "To\u00b7de", "ruf'", "ich", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Laster brausen hoch einher,", "tokens": ["Las\u00b7ter", "brau\u00b7sen", "hoch", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Wogen auf dem Meer,", "tokens": ["Wie", "die", "Wo\u00b7gen", "auf", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unaufhaltsam, wie die Wuth", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", ",", "wie", "die", "Wuth"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener allgemeinen Fluth.", "tokens": ["Je\u00b7ner", "all\u00b7ge\u00b7mei\u00b7nen", "Fluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf den schwarzen Wogen thront,", "tokens": ["Auf", "den", "schwar\u00b7zen", "Wo\u00b7gen", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Satan, den die Rache schont;", "tokens": ["Sa\u00b7tan", ",", "den", "die", "Ra\u00b7che", "schont", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn sie spart auf jenen Tag", "tokens": ["Denn", "sie", "spart", "auf", "je\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren gro\u00dfen Donnerschlag.", "tokens": ["Ih\u00b7ren", "gro\u00b7\u00dfen", "Don\u00b7ner\u00b7schlag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Menschen treiben k\u00fchnen Spott,", "tokens": ["Men\u00b7schen", "trei\u00b7ben", "k\u00fch\u00b7nen", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwellen auf und l\u00e4stern Gott;", "tokens": ["Schwel\u00b7len", "auf", "und", "l\u00e4s\u00b7tern", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der kaum geformte Thon", "tokens": ["Und", "der", "kaum", "ge\u00b7form\u00b7te", "Thon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Trotzet seinem T\u00f6pfer schon.", "tokens": ["Trot\u00b7zet", "sei\u00b7nem", "T\u00f6p\u00b7fer", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wahn und k\u00fchner Zweifel macht", "tokens": ["Wahn", "und", "k\u00fch\u00b7ner", "Zwei\u00b7fel", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Tage Mitternacht;", "tokens": ["Aus", "dem", "Ta\u00b7ge", "Mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00f6tter der Religion", "tokens": ["Sp\u00f6t\u00b7ter", "der", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sprechen Gott und Tugend Hohn.", "tokens": ["Spre\u00b7chen", "Gott", "und", "Tu\u00b7gend", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von dem Throne bis zum Pflug", "tokens": ["Von", "dem", "Thro\u00b7ne", "bis", "zum", "Pflug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrscht Verstellung und Betrug;", "tokens": ["Herrscht", "Ver\u00b7stel\u00b7lung", "und", "Be\u00b7trug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Verbrecher k\u00f6nnen nun", "tokens": ["Und", "Ver\u00b7bre\u00b7cher", "k\u00f6n\u00b7nen", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Ahndung B\u00f6ses thun.", "tokens": ["Oh\u00b7ne", "Ahn\u00b7dung", "B\u00f6\u00b7ses", "thun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aufgefressnes Wittwengut,", "tokens": ["Auf\u00b7ge\u00b7fress\u00b7nes", "Witt\u00b7wen\u00b7gut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zerquetschter Waisen Blut;", "tokens": ["Und", "zer\u00b7quetschter", "Wai\u00b7sen", "Blut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frevler, unter deren Fu\u00df", "tokens": ["Frev\u00b7ler", ",", "un\u00b7ter", "de\u00b7ren", "Fu\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die Tugend b\u00fccken mu\u00df;", "tokens": ["Sich", "die", "Tu\u00b7gend", "b\u00fc\u00b7cken", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Stolz und Heuchelei und Neid", "tokens": ["Stolz", "und", "Heu\u00b7che\u00b7lei", "und", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter einem frommen Kleid;", "tokens": ["Un\u00b7ter", "ei\u00b7nem", "from\u00b7men", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geiz, der in dem Winkel sitzt", "tokens": ["Geiz", ",", "der", "in", "dem", "Win\u00b7kel", "sitzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Drachenschatz besch\u00fctzt;", "tokens": ["Und", "den", "Dra\u00b7chen\u00b7schatz", "be\u00b7sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wilde Lust, die lockend blickt", "tokens": ["Wil\u00b7de", "Lust", ",", "die", "lo\u00b7ckend", "blickt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Geist zur Erde dr\u00fcckt;", "tokens": ["Und", "den", "Geist", "zur", "Er\u00b7de", "dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Christen, die berauscht und blind", "tokens": ["Chris\u00b7ten", ",", "die", "be\u00b7rauscht", "und", "blind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre eigne Henker sind;", "tokens": ["Ih\u00b7re", "eig\u00b7ne", "Hen\u00b7ker", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Kinder, gegen Lehrer taub;", "tokens": ["Kin\u00b7der", ",", "ge\u00b7gen", "Leh\u00b7rer", "taub", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00fcnglinge, der L\u00fcste Raub;", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", ",", "der", "L\u00fcs\u00b7te", "Raub", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "M\u00e4nnerherzen, ohne Muth;", "tokens": ["M\u00e4n\u00b7ner\u00b7her\u00b7zen", ",", "oh\u00b7ne", "Muth", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fcnden, die das Alter thut;", "tokens": ["S\u00fcn\u00b7den", ",", "die", "das", "Al\u00b7ter", "thut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Einen Freund, wie Joab ist,", "tokens": ["Ei\u00b7nen", "Freund", ",", "wie", "Joab", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NE", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der mit gift'gen Lippen k\u00fc\u00dft,", "tokens": ["Der", "mit", "gift'\u00b7gen", "Lip\u00b7pen", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit heitrem Angesicht", "tokens": ["Und", "mit", "heit\u00b7rem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Freunde m\u00f6rderisch ersticht:", "tokens": ["Freun\u00b7de", "m\u00f6r\u00b7de\u00b7risch", "er\u00b7sticht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Erde, bist du das \u2013 und Er", "tokens": ["Er\u00b7de", ",", "bist", "du", "das", "\u2013", "und", "Er"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER", "ART", "$(", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Donnert nicht, der Donnerer?", "tokens": ["Don\u00b7nert", "nicht", ",", "der", "Don\u00b7ne\u00b7rer", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mitternacht, o! decke du", "tokens": ["Mit\u00b7ter\u00b7nacht", ",", "o", "!", "de\u00b7cke", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Drachenh\u00f6hle zu.", "tokens": ["Die\u00b7se", "Dra\u00b7chen\u00b7h\u00f6h\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Komm, des Schlafes Bruder, Tod!", "tokens": ["Komm", ",", "des", "Schla\u00b7fes", "Bru\u00b7der", ",", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich, wie das Morgenroth,", "tokens": ["Lieb\u00b7lich", ",", "wie", "das", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchlend, wie der Westwind weht,", "tokens": ["K\u00fch\u00b7lend", ",", "wie", "der", "West\u00b7wind", "weht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd, wie ein Blumenbeet.", "tokens": ["L\u00e4\u00b7chelnd", ",", "wie", "ein", "Blu\u00b7men\u00b7beet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "F\u00fchre mich aus dieser Welt,", "tokens": ["F\u00fch\u00b7re", "mich", "aus", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehe Feuer auf sie f\u00e4llt;", "tokens": ["E\u00b7he", "Feu\u00b7er", "auf", "sie", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reich' mir deine milde Hand,", "tokens": ["Reich'", "mir", "dei\u00b7ne", "mil\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringe mich ins Vaterland.", "tokens": ["Brin\u00b7ge", "mich", "ins", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Todte in den Gr\u00e4bern hier,", "tokens": ["Tod\u00b7te", "in", "den", "Gr\u00e4\u00b7bern", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O! wie gl\u00fccklich seid ihr mir,", "tokens": ["O", "!", "wie", "gl\u00fcck\u00b7lich", "seid", "ihr", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr vor der Laster Wuth", "tokens": ["Die", "ihr", "vor", "der", "Las\u00b7ter", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft besch\u00fctzt im Grabe ruht.", "tokens": ["Sanft", "be\u00b7sch\u00fctzt", "im", "Gra\u00b7be", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}