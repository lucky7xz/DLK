{"textgrid.poem.50851": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Taugenichts", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ersten Veilchen waren schon", "tokens": ["Die", "ers\u00b7ten", "Veil\u00b7chen", "wa\u00b7ren", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwacht im stillen Tal,", "tokens": ["Er\u00b7wacht", "im", "stil\u00b7len", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Bettelpack schlug auf den Thron", "tokens": ["Das", "Bet\u00b7tel\u00b7pack", "schlug", "auf", "den", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Feld zum ersten Mal.", "tokens": ["Im", "Feld", "zum", "ers\u00b7ten", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Alte auf dem R\u00fccken lag,", "tokens": ["Der", "Al\u00b7te", "auf", "dem", "R\u00fc\u00b7cken", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Mutter wusch am See;", "tokens": ["Die", "Mut\u00b7ter", "wusch", "am", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bestaubt und unrein schmolz im Hag", "tokens": ["Be\u00b7staubt", "und", "un\u00b7rein", "schmolz", "im", "Hag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das letzte H\u00e4uflein Schnee.", "tokens": ["Das", "letz\u00b7te", "H\u00e4uf\u00b7lein", "Schnee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Vollmond warf den Silberschein", "tokens": ["Der", "Voll\u00b7mond", "warf", "den", "Sil\u00b7ber\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Bettler in die Hand,", "tokens": ["Dem", "Bett\u00b7ler", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bestreut' der Frau mit Edelstein", "tokens": ["Be\u00b7streut'", "der", "Frau", "mit", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lumpen, die sie wand;", "tokens": ["Die", "Lum\u00b7pen", ",", "die", "sie", "wand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein linder West blies in die Glut", "tokens": ["Ein", "lin\u00b7der", "West", "blies", "in", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von einem Dorngeflecht,", "tokens": ["Von", "ei\u00b7nem", "Dorn\u00b7ge\u00b7flecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drauf kocht' in Bettelmannes Hut", "tokens": ["Drauf", "kocht'", "in", "Bet\u00b7tel\u00b7man\u00b7nes", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein s\u00fcndengrauer Hecht.", "tokens": ["Ein", "s\u00fcn\u00b7den\u00b7grau\u00b7er", "Hecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da kam der kleine Betteljung,", "tokens": ["Da", "kam", "der", "klei\u00b7ne", "Bet\u00b7tel\u00b7jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Hunger schwach und matt,", "tokens": ["Vor", "Hun\u00b7ger", "schwach", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch gl\u00fchend in Begeisterung", "tokens": ["Doch", "gl\u00fc\u00b7hend", "in", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Streifen durch die Stadt,", "tokens": ["Vom", "Strei\u00b7fen", "durch", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hielt eine Hyazinth empor", "tokens": ["Hielt", "ei\u00b7ne", "Hy\u00b7a\u00b7zinth", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dunkelblauer Luft;", "tokens": ["In", "dun\u00b7kel\u00b7blau\u00b7er", "Luft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Blume war von seltnem Flor", "tokens": ["Die", "Blu\u00b7me", "war", "von", "selt\u00b7nem", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und selig s\u00fc\u00df ihr Duft.", "tokens": ["Und", "se\u00b7lig", "s\u00fc\u00df", "ihr", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Vater rief: \u00bbWohl hast du mir", "tokens": ["Der", "Va\u00b7ter", "rief", ":", "\u00bb", "Wohl", "hast", "du", "mir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Pfennige gebracht?\u00ab", "tokens": ["Viel", "Pfen\u00b7ni\u00b7ge", "ge\u00b7bracht", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Knabe rief: \u00bbO sehet hier", "tokens": ["Der", "Kna\u00b7be", "rief", ":", "\u00bb", "O", "se\u00b7het", "hier"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blume Zauberpracht!", "tokens": ["Der", "Blu\u00b7me", "Zau\u00b7ber\u00b7pracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich lag am goldnen Gittertor", "tokens": ["Ich", "lag", "am", "gold\u00b7nen", "Git\u00b7ter\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Morgen bis zur Nacht,", "tokens": ["Vom", "Mor\u00b7gen", "bis", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Blume aus dem Wunderflor", "tokens": ["Die", "Blu\u00b7me", "aus", "dem", "Wun\u00b7der\u00b7flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu stehlen nur bedacht!", "tokens": ["Zu", "steh\u00b7len", "nur", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Seht nur, wie vornehm und wie fein,", "tokens": ["Seht", "nur", ",", "wie", "vor\u00b7nehm", "und", "wie", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie zierlich sie gebaut!", "tokens": ["Wie", "zier\u00b7lich", "sie", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe starr nach ihrem Schein", "tokens": ["Ich", "ha\u00b7be", "starr", "nach", "ih\u00b7rem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ganzen Tag geschaut.", "tokens": ["Den", "gan\u00b7zen", "Tag", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O schlaget nicht mich armen Wicht,", "tokens": ["O", "schla\u00b7get", "nicht", "mich", "ar\u00b7men", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00dft euren Stecken ruhn!", "tokens": ["La\u00dft", "eu\u00b7ren", "Ste\u00b7cken", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will ja nichts, mich hungert nicht,", "tokens": ["Ich", "will", "ja", "nichts", ",", "mich", "hun\u00b7gert", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will's nicht wieder tun!", "tokens": ["Ich", "will's", "nicht", "wie\u00b7der", "tun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O sehet nur, ich werde toll,", "tokens": ["O", "se\u00b7het", "nur", ",", "ich", "wer\u00b7de", "toll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gl\u00f6cklein alle an!", "tokens": ["Die", "Gl\u00f6c\u00b7klein", "al\u00b7le", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Duft, so fremd und wundervoll,", "tokens": ["Ihr", "Duft", ",", "so", "fremd", "und", "wun\u00b7der\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat mir es angetan!", "tokens": ["Hat", "mir", "es", "an\u00b7ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch alle Blumen nun im Feld", "tokens": ["Auch", "al\u00b7le", "Blu\u00b7men", "nun", "im", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lieb ich von heute an;", "tokens": ["Lieb", "ich", "von", "heu\u00b7te", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die Hexe, welche neue Welt", "tokens": ["Die", "He\u00b7xe", ",", "wel\u00b7che", "neu\u00b7e", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat sie mir aufgetan!\u00ab \u2013", "tokens": ["Hat", "sie", "mir", "auf\u00b7ge\u00b7tan", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbo wehe mir geschlagnem Tropf!\u00ab", "tokens": ["\u00bb", "o", "we\u00b7he", "mir", "ge\u00b7schlag\u00b7nem", "Tropf", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Brach nun der Alte aus;", "tokens": ["Brach", "nun", "der", "Al\u00b7te", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Kind kommt mit verr\u00fccktem Kopf", "tokens": ["\u00bb", "mein", "Kind", "kommt", "mit", "ver\u00b7r\u00fcck\u00b7tem", "Kopf"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anstatt mit Brot nach Haus!", "tokens": ["An\u00b7statt", "mit", "Brot", "nach", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du Taugenichts, du Tagedieb", "tokens": ["Du", "Tau\u00b7ge\u00b7nichts", ",", "du", "Ta\u00b7ge\u00b7dieb"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und deiner Eltern Schmach!\u00ab", "tokens": ["Und", "dei\u00b7ner", "El\u00b7tern", "Schmach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, 