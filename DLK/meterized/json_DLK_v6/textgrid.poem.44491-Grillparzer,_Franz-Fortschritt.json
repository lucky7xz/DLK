{"textgrid.poem.44491": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Fortschritt", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit, sie eilt so schnell voraus,", "tokens": ["Die", "Zeit", ",", "sie", "eilt", "so", "schnell", "vo\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich, ich blieb zur\u00fcck.", "tokens": ["Und", "ich", ",", "ich", "blieb", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sch\u00e4me mich! Was kommt heraus?", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", "!", "Was", "kommt", "he\u00b7raus", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es bleibt ein Mi\u00dfgeschick.", "tokens": ["Es", "bleibt", "ein", "Mi\u00df\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort st\u00fcrmt sie hin unb\u00e4ndig jach,", "tokens": ["Dort", "st\u00fcrmt", "sie", "hin", "un\u00b7b\u00e4n\u00b7dig", "jach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum reicht so fern mein Blick;", "tokens": ["Kaum", "reicht", "so", "fern", "mein", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Bahngenossen st\u00fcrmen nach,", "tokens": ["Die", "Bahn\u00b7ge\u00b7nos\u00b7sen", "st\u00fcr\u00b7men", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich, ich blieb zur\u00fcck.", "tokens": ["Und", "ich", ",", "ich", "blieb", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vielleicht kehrt wieder sie des Wegs;", "tokens": ["Viel\u00b7leicht", "kehrt", "wie\u00b7der", "sie", "des", "Wegs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft sitzen mich am Stein!", "tokens": ["La\u00dft", "sit\u00b7zen", "mich", "am", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht \u2013 hat sie sich m\u00fcd gerannt \u2013", "tokens": ["Viel\u00b7leicht", "\u2013", "hat", "sie", "sich", "m\u00fcd", "ge\u00b7rannt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hol ich sie doch noch ein.", "tokens": ["Hol", "ich", "sie", "doch", "noch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Gang der Welt ist nicht so rasch,", "tokens": ["Der", "Gang", "der", "Welt", "ist", "nicht", "so", "rasch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Torheit meint und spricht;", "tokens": ["Als", "Tor\u00b7heit", "meint", "und", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man wei\u00df wohl: Fl\u00fcgel hat die Zeit,", "tokens": ["Man", "wei\u00df", "wohl", ":", "Fl\u00fc\u00b7gel", "hat", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeiten aber nicht!", "tokens": ["Die", "Zei\u00b7ten", "a\u00b7ber", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}