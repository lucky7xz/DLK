{"textgrid.poem.62365": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnstlerleben", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhabner, seliger Beruf,", "tokens": ["Er\u00b7hab\u00b7ner", ",", "se\u00b7li\u00b7ger", "Be\u00b7ruf", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dem der Geist, der Alles schuf,", "tokens": ["Zu", "dem", "der", "Geist", ",", "der", "Al\u00b7les", "schuf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich vor der Zeit und Ewigkeit", "tokens": ["Mich", "vor", "der", "Zeit", "und", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als seinen Priester eingeweiht!", "tokens": ["Als", "sei\u00b7nen", "Pries\u00b7ter", "ein\u00b7ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Tempel, wo der K\u00fcnstler lebt,", "tokens": ["Ein", "Tem\u00b7pel", ",", "wo", "der", "K\u00fcnst\u00b7ler", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo rings um ihn die Gottheit webt,", "tokens": ["Wo", "rings", "um", "ihn", "die", "Got\u00b7theit", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die er, wohin sein Fu\u00df auch dringt,", "tokens": ["Die", "er", ",", "wo\u00b7hin", "sein", "Fu\u00df", "auch", "dringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer F\u00fclle mit sich bringt!", "tokens": ["In", "ih\u00b7rer", "F\u00fcl\u00b7le", "mit", "sich", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie nahm ihn fr\u00fch auf ihren Schoo\u00df,", "tokens": ["Sie", "nahm", "ihn", "fr\u00fch", "auf", "ih\u00b7ren", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie herzte ihn, sie zog ihn gro\u00df,", "tokens": ["Sie", "herz\u00b7te", "ihn", ",", "sie", "zog", "ihn", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wo er geht und wo er steht,", "tokens": ["Und", ",", "wo", "er", "geht", "und", "wo", "er", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Lebensathem ihn umweht.", "tokens": ["Ihr", "Le\u00b7ben\u00b7sa\u00b7them", "ihn", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie l\u00e4chelt ihm die gr\u00fcne Flur,", "tokens": ["Wie", "l\u00e4\u00b7chelt", "ihm", "die", "gr\u00fc\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er liest im Sanskrit der Natur;", "tokens": ["Er", "liest", "im", "Sans\u00b7krit", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wohin er f\u00e4llt, sein Sch\u00f6pferblick,", "tokens": ["Wo\u00b7hin", "er", "f\u00e4llt", ",", "sein", "Sch\u00f6p\u00b7fer\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entstr\u00f6mt ihm Leben, Freud' und Gl\u00fcck.", "tokens": ["Ent\u00b7str\u00f6mt", "ihm", "Le\u00b7ben", ",", "Freud'", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn Abends er zur Zelle flieht,", "tokens": ["Wenn", "A\u00b7bends", "er", "zur", "Zel\u00b7le", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihm hinein die G\u00f6ttin zieht,", "tokens": ["Mit", "ihm", "hin\u00b7ein", "die", "G\u00f6t\u00b7tin", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommt der sanfte Mondenschein,", "tokens": ["Es", "kommt", "der", "sanf\u00b7te", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Heiligthum den Ort zu weihn.", "tokens": ["Zum", "Hei\u00b7lig\u00b7thum", "den", "Ort", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der J\u00fcngling sinkt aufs Lager hin,", "tokens": ["Der", "J\u00fcng\u00b7ling", "sinkt", "aufs", "La\u00b7ger", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hoch und h\u00f6her strebt sein Sinn,", "tokens": ["Und", "hoch", "und", "h\u00f6\u00b7her", "strebt", "sein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm \u00f6ffnet sich das Himmelsthor,", "tokens": ["Ihm", "\u00f6ff\u00b7net", "sich", "das", "Him\u00b7mel\u00b7sthor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Traume steigt sein Geist empor.", "tokens": ["Im", "Trau\u00b7me", "steigt", "sein", "Geist", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer naht sich ihm im milden Glanz,", "tokens": ["Wer", "naht", "sich", "ihm", "im", "mil\u00b7den", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt Lorbeer ihm und Myrthenkranz?", "tokens": ["Bringt", "Lor\u00b7beer", "ihm", "und", "Myr\u00b7then\u00b7kranz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Ideal, das ihn umschwebt,", "tokens": ["Das", "I\u00b7deal", ",", "das", "ihn", "um\u00b7schwebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hat es ein Gott f\u00fcr ihn belebt?", "tokens": ["Hat", "es", "ein", "Gott", "f\u00fcr", "ihn", "be\u00b7lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbwillkommen auf der Erde hier!", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", "auf", "der", "Er\u00b7de", "hier", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist willkomm' und gesegnet mir!", "tokens": ["Bist", "will\u00b7komm'", "und", "ge\u00b7seg\u00b7net", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm Altar gleich und Tempel ein,", "tokens": ["Nimm", "Al\u00b7tar", "gleich", "und", "Tem\u00b7pel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "KON", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00fcll' ihn mit deinem Heil'genschein.\u00ab", "tokens": ["F\u00fcll'", "ihn", "mit", "dei\u00b7nem", "Heil'\u00b7gen\u00b7schein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O Liebe, Liebe, D\u00e4mmerung", "tokens": ["O", "Lie\u00b7be", ",", "Lie\u00b7be", ",", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Von sch\u00f6nerer Verherrlichung,", "tokens": ["Von", "sch\u00f6\u00b7ne\u00b7rer", "Ver\u00b7herr\u00b7li\u00b7chung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des goldnen Tages Morgenroth,", "tokens": ["Des", "gold\u00b7nen", "Ta\u00b7ges", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Friedensherold ist der Tod.", "tokens": ["Dein", "Frie\u00b7dens\u00b7he\u00b7rold", "ist", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Von dir erquickt, von dir gelabt,", "tokens": ["Von", "dir", "er\u00b7quickt", ",", "von", "dir", "ge\u00b7labt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem h\u00f6hern Sinn begabt,", "tokens": ["Mit", "ei\u00b7nem", "h\u00f6\u00b7hern", "Sinn", "be\u00b7gabt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deinem Leben angehaucht,", "tokens": ["Von", "dei\u00b7nem", "Le\u00b7ben", "an\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Wonnemeer der Geist enttaucht.", "tokens": ["Dem", "Won\u00b7ne\u00b7meer", "der", "Geist", "ent\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O nimm mich traulich in den Arm,", "tokens": ["O", "nimm", "mich", "trau\u00b7lich", "in", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier ist es still, hier ist es warm.", "tokens": ["Hier", "ist", "es", "still", ",", "hier", "ist", "es", "warm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da drau\u00dfen ist's so kalt und rauh.", "tokens": ["Da", "drau\u00b7\u00dfen", "ist's", "so", "kalt", "und", "rauh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier Mondenschein, dort Nebelgrau!", "tokens": ["Hier", "Mon\u00b7den\u00b7schein", ",", "dort", "Ne\u00b7bel\u00b7grau", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Umf\u00e4chle stets mich Himmelsluft,", "tokens": ["Um\u00b7f\u00e4ch\u00b7le", "stets", "mich", "Him\u00b7mel\u00b7sluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verweh' nicht Paradiesesduft \u2013", "tokens": ["Ver\u00b7weh'", "nicht", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7duft", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Leib und Seele ganz und gar", "tokens": ["Mit", "Leib", "und", "See\u00b7le", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weih' ich mich deinem Hochaltar.", "tokens": ["Weih'", "ich", "mich", "dei\u00b7nem", "Hoc\u00b7hal\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}