{"textgrid.poem.40279": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwere Ringe ... und die Meinung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwere Ringe ... und die Meinung", "tokens": ["Schwe\u00b7re", "Rin\u00b7ge", "...", "und", "die", "Mei\u00b7nung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "leere Schlinge ... werbe, werbe!", "tokens": ["lee\u00b7re", "Schlin\u00b7ge", "...", "wer\u00b7be", ",", "wer\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dumpfe Kammer ... und das Erbe", "tokens": ["Dump\u00b7fe", "Kam\u00b7mer", "...", "und", "das", "Er\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u2013 bringe Jammer \u2013 wird Erscheinung.", "tokens": ["\u2013", "brin\u00b7ge", "Jam\u00b7mer", "\u2013", "wird", "Er\u00b7schei\u00b7nung", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$(", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn dich aus dem ersten Schlaf", "tokens": ["Wenn", "dich", "aus", "dem", "ers\u00b7ten", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "um Mitternacht", "tokens": ["um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "dein rasend klopfendes Herz", "tokens": ["dein", "ra\u00b7send", "klop\u00b7fen\u00b7des", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "aus deinen Tr\u00e4umen jagt,", "tokens": ["aus", "dei\u00b7nen", "Tr\u00e4u\u00b7men", "jagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "furchtsam horcht dein Atem,", "tokens": ["furcht\u00b7sam", "horcht", "dein", "A\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "und sich durch dein \u00f6des Zimmer", "tokens": ["und", "sich", "durch", "dein", "\u00f6\u00b7des", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "wei\u00dfe Schatten vor dir fl\u00fcchten;", "tokens": ["wei\u00b7\u00dfe", "Schat\u00b7ten", "vor", "dir", "fl\u00fcch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "kennst du dieses Grauen?", "tokens": ["kennst", "du", "die\u00b7ses", "Grau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wenn sich aus der hohlen Nacht", "tokens": ["Wenn", "sich", "aus", "der", "hoh\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "fern mit klagenden Augen", "tokens": ["fern", "mit", "kla\u00b7gen\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "ein geliebtes Gesicht", "tokens": ["ein", "ge\u00b7lieb\u00b7tes", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "aus den blassen Kreisen ringt", "tokens": ["aus", "den", "blas\u00b7sen", "Krei\u00b7sen", "ringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "und sprechen will;", "tokens": ["und", "spre\u00b7chen", "will", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "kennst du dieses Grauen?", "tokens": ["kennst", "du", "die\u00b7ses", "Grau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Mit langen H\u00e4nden", "tokens": ["Mit", "lan\u00b7gen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "will es nach dir greifen", "tokens": ["will", "es", "nach", "dir", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "und dich erw\u00fcrgen", "tokens": ["und", "dich", "er\u00b7w\u00fcr\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "f\u00fcr eine Schuld ...", "tokens": ["f\u00fcr", "ei\u00b7ne", "Schuld", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}