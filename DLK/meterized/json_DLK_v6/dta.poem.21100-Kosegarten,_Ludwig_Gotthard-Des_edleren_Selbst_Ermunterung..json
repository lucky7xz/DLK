{"dta.poem.21100": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Des  \n edleren Selbst Ermunterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ermanne dich, mein Geist! Entraffe dich der               ", "tokens": ["Er\u00b7man\u00b7ne", "dich", ",", "mein", "Geist", "!", "E\u00b7ntraf\u00b7fe", "dich", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nacht,", "tokens": ["Nacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die eisern dich umsitzt. Der Schwermuth Raben-", "tokens": ["Die", "ei\u00b7sern", "dich", "um\u00b7sitzt", ".", "Der", "Schwer\u00b7muth", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "tracht", "tokens": ["tracht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Besch\u00e4mt das Rosenroth von Gottes sch\u00f6ner Welt.", "tokens": ["Be\u00b7sch\u00e4mt", "das", "Ro\u00b7sen\u00b7roth", "von", "Got\u00b7tes", "sch\u00f6\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sey Weiser, Mann und Held!", "tokens": ["Sey", "Wei\u00b7ser", ",", "Mann", "und", "Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bist gegen Tausend du gleich klein und namenlos;", "tokens": ["Bist", "ge\u00b7gen", "Tau\u00b7send", "du", "gleich", "klein", "und", "na\u00b7men\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "CARD", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du vor Tausenden doch herrlich auch und gross,", "tokens": ["Bist", "du", "vor", "Tau\u00b7sen\u00b7den", "doch", "herr\u00b7lich", "auch", "und", "gross", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bist vor Zehntausenden geformt aus edlerm Thon,", "tokens": ["Bist", "vor", "Zehn\u00b7tau\u00b7sen\u00b7den", "ge\u00b7formt", "aus", "ed\u00b7lerm", "Thon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uraniens Lieblingssohn!", "tokens": ["U\u00b7ra\u00b7ni\u00b7ens", "Lieb\u00b7lings\u00b7sohn", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spannt deinen Bogen nicht noch ungeschw\u00e4chte", "tokens": ["Spannt", "dei\u00b7nen", "Bo\u00b7gen", "nicht", "noch", "un\u00b7ge\u00b7schw\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kraft?", "tokens": ["Kraft", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Schwellt deine Adern nicht die Woge Leidenschaft?", "tokens": ["Schwellt", "dei\u00b7ne", "A\u00b7dern", "nicht", "die", "Wo\u00b7ge", "Lei\u00b7den\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Giebt dem Begeisterten der Sch\u00f6nheit Genius", "tokens": ["Giebt", "dem", "Be\u00b7geis\u00b7ter\u00b7ten", "der", "Sch\u00f6n\u00b7heit", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NE"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Nicht manchen Liebeskuss?", "tokens": ["Nicht", "man\u00b7chen", "Lie\u00b7bes\u00b7kuss", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist nicht die Wahrheit dir, bist du nicht ihr", "tokens": ["Ist", "nicht", "die", "Wahr\u00b7heit", "dir", ",", "bist", "du", "nicht", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "PPOSAT"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.2": {"text": "vertraut?", "tokens": ["ver\u00b7traut", "?"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dr\u00fcckt dich nicht an ihr Herz Natur, wie eine", "tokens": ["Dr\u00fcckt", "dich", "nicht", "an", "ihr", "Herz", "Na\u00b7tur", ",", "wie", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$,", "PWAV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Braut?", "tokens": ["Braut", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Schlingt nicht um deinen Hals die Tugend liebe-", "tokens": ["Schlingt", "nicht", "um", "dei\u00b7nen", "Hals", "die", "Tu\u00b7gend", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "warm", "tokens": ["warm"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Den himmelhellen Arm?", "tokens": ["Den", "him\u00b7mel\u00b7hel\u00b7len", "Arm", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Str\u00f6mt nicht, wie Schlossen wild, wie Gottes", "tokens": ["Str\u00f6mt", "nicht", ",", "wie", "Schlos\u00b7sen", "wild", ",", "wie", "Got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "NN", "ADJD", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Donner stark,", "tokens": ["Don\u00b7ner", "stark", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dein Hochgesang daher, und sch\u00fcttert Nerv' und", "tokens": ["Dein", "Hoch\u00b7ge\u00b7sang", "da\u00b7her", ",", "und", "sch\u00fct\u00b7tert", "Ner\u00b7v'", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV", "$,", "KON", "VVFIN", "NE", "KON"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mark?", "tokens": ["Mark", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Schmelzt nicht dein sanftres Lied des Edlern f\u00fchlend", "tokens": ["Schmelzt", "nicht", "dein", "sanf\u00b7tres", "Lied", "des", "Ed\u00b7lern", "f\u00fch\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Herz", "tokens": ["Herz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "In wollustvollen Schmerz?", "tokens": ["In", "wol\u00b7lust\u00b7vol\u00b7len", "Schmerz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gelang im Dunkeln dir nicht manche bessre", "tokens": ["Ge\u00b7lang", "im", "Dun\u00b7keln", "dir", "nicht", "man\u00b7che", "bess\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "PPER", "PTKNEG", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "That,", "tokens": ["That", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die keine Zeugen hier, die Zeugen droben hat?", "tokens": ["Die", "kei\u00b7ne", "Zeu\u00b7gen", "hier", ",", "die", "Zeu\u00b7gen", "dro\u00b7ben", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$,", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hast du die Thr\u00e4ne nicht der Inbrunst, ernst", "tokens": ["Hast", "du", "die", "Thr\u00e4\u00b7ne", "nicht", "der", "In\u00b7brunst", ",", "ernst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und sch\u00f6n,", "tokens": ["und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Dir dankbar fliessen sehn?", "tokens": ["Dir", "dank\u00b7bar", "flies\u00b7sen", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sind dir nicht nah und fern die Guten hold", "tokens": ["Sind", "dir", "nicht", "nah", "und", "fern", "die", "Gu\u00b7ten", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und freund?", "tokens": ["und", "freund", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Schm\u00e4ht dich der P\u00f6bel nicht? Sind Schurken dir", "tokens": ["Schm\u00e4ht", "dich", "der", "P\u00f6\u00b7bel", "nicht", "?", "Sind", "Schur\u00b7ken", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$.", "VAFIN", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nicht feind?", "tokens": ["nicht", "feind", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und dr\u00e4ngt nicht manches Herz, das nirgends hal-", "tokens": ["Und", "dr\u00e4ngt", "nicht", "man\u00b7ches", "Herz", ",", "das", "nir\u00b7gends", "ha\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "PIAT", "NN", "$,", "PRELS", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "ten kann,", "tokens": ["ten", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Sich liebend an dich an?", "tokens": ["Sich", "lie\u00b7bend", "an", "dich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ermanne dich, mein Geist! Entraffe dich der", "tokens": ["Er\u00b7man\u00b7ne", "dich", ",", "mein", "Geist", "!", "E\u00b7ntraf\u00b7fe", "dich", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nacht,", "tokens": ["Nacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die eisern dich umh\u00fcllt. Der Schwermuth Trauer-", "tokens": ["Die", "ei\u00b7sern", "dich", "um\u00b7h\u00fcllt", ".", "Der", "Schwer\u00b7muth", "Trau\u00b7e\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "tracht", "tokens": ["tracht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Lass jenem, dem der Born der Hoffnung gar ver-", "tokens": ["Lass", "je\u00b7nem", ",", "dem", "der", "Born", "der", "Hoff\u00b7nung",