{"textgrid.poem.65550": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "23. Flachheit und Dunkelheit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Alles, was dunkel ist, tief auch w\u00e4r',", "tokens": ["Wenn", "Al\u00b7les", ",", "was", "dun\u00b7kel", "ist", ",", "tief", "auch", "w\u00e4r'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADJD", "ADV", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So stie\u00dfe sich Keiner im Dunkeln mehr.", "tokens": ["So", "stie\u00b7\u00dfe", "sich", "Kei\u00b7ner", "im", "Dun\u00b7keln", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIS", "APPRART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und guckst du bei Nacht zum Fenster hinaus,", "tokens": ["Und", "guckst", "du", "bei", "Nacht", "zum", "Fens\u00b7ter", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Schornstein sieht schwarz wie der Himmel aus.", "tokens": ["Der", "Schorn\u00b7stein", "sieht", "schwarz", "wie", "der", "Him\u00b7mel", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}