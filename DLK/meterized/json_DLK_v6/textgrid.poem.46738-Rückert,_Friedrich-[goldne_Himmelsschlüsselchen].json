{"textgrid.poem.46738": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[goldne Himmelsschl\u00fcsselchen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldne Himmelsschl\u00fcsselchen", "tokens": ["Gold\u00b7ne", "Him\u00b7mels\u00b7schl\u00fcs\u00b7sel\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Erschlie\u00dft den Blumenhimmel jetzt!", "tokens": ["Er\u00b7schlie\u00dft", "den", "Blu\u00b7men\u00b7him\u00b7mel", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit thaugef\u00fcllten Sch\u00fcsselchen", "tokens": ["Mit", "thau\u00b7ge\u00b7f\u00fcll\u00b7ten", "Sch\u00fcs\u00b7sel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Werde der Fr\u00fchlingstisch besetzt!", "tokens": ["Wer\u00b7de", "der", "Fr\u00fch\u00b7lings\u00b7tisch", "be\u00b7setzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Mit thaugef\u00fcllten Sch\u00fcsselchen", "tokens": ["Mit", "thau\u00b7ge\u00b7f\u00fcll\u00b7ten", "Sch\u00fcs\u00b7sel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ist besetzet der Fr\u00fchlingstisch;", "tokens": ["Ist", "be\u00b7set\u00b7zet", "der", "Fr\u00fch\u00b7lings\u00b7tisch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Bienen tauchen die R\u00fcsselchen", "tokens": ["Bie\u00b7nen", "tau\u00b7chen", "die", "R\u00fcs\u00b7sel\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "In das saftige Duftgemisch.", "tokens": ["In", "das", "saf\u00b7ti\u00b7ge", "Duft\u00b7ge\u00b7misch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wie die Bien' ihr R\u00fcsselchen,", "tokens": ["Wie", "die", "Bi\u00b7en'", "ihr", "R\u00fcs\u00b7sel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--++-++-", "measure": "anapaest.init"}, "line.2": {"text": "Tauch' ich mein Herz in Liebesduft;", "tokens": ["Tauch'", "ich", "mein", "Herz", "in", "Lie\u00b7bes\u00b7duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Goldne Himmelsschl\u00fcsselchen,", "tokens": ["Gold\u00b7ne", "Him\u00b7mels\u00b7schl\u00fcs\u00b7sel\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schlie\u00dfet mir auf der Liebe Gruft!", "tokens": ["Schlie\u00b7\u00dfet", "mir", "auf", "der", "Lie\u00b7be", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}