{"textgrid.poem.25038": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Mund, vollatmend hei\u00df an meinem Munde \u2013", "tokens": ["Dein", "Mund", ",", "vol\u00b7lat\u00b7mend", "hei\u00df", "an", "mei\u00b7nem", "Mun\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Herz mit hohem Schlag an meins gepre\u00dft,", "tokens": ["Dein", "Herz", "mit", "ho\u00b7hem", "Schlag", "an", "meins", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie weihst du jede fl\u00fcchtige Sekunde", "tokens": ["Wie", "weihst", "du", "je\u00b7de", "fl\u00fcch\u00b7ti\u00b7ge", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Tages mir zum Liebesfest!", "tokens": ["Des", "Ta\u00b7ges", "mir", "zum", "Lie\u00b7bes\u00b7fest", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und dann die heil'gen, wonnem\u00fcden N\u00e4chte,", "tokens": ["Und", "dann", "die", "heil'\u00b7gen", ",", "won\u00b7ne\u00b7m\u00fc\u00b7den", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Schwelgen Arm in Arm und Brust an Brust!", "tokens": ["Das", "Schwel\u00b7gen", "Arm", "in", "Arm", "und", "Brust", "an", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mi\u00dfg\u00f6nnen nicht dem sterblichen Geschlechte", "tokens": ["Mi\u00df\u00b7g\u00f6n\u00b7nen", "nicht", "dem", "sterb\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die G\u00f6tter solche Himmelslust?", "tokens": ["Die", "G\u00f6t\u00b7ter", "sol\u00b7che", "Him\u00b7mels\u00b7lust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, denk' ich alles, was du mir gegeben", "tokens": ["Ja", ",", "denk'", "ich", "al\u00b7les", ",", "was", "du", "mir", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PIS", "$,", "PWS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und noch mir giebst, so f\u00fcrcht' ich ihren Neid;", "tokens": ["Und", "noch", "mir", "giebst", ",", "so", "f\u00fcrcht'", "ich", "ih\u00b7ren", "Neid", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leicht zuckt ihr Blitzstrahl nieder auf ein Leben,", "tokens": ["Leicht", "zuckt", "ihr", "Blitz\u00b7strahl", "nie\u00b7der", "auf", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das allzu voll von Seligkeit.", "tokens": ["Das", "all\u00b7zu", "voll", "von", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKA", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}