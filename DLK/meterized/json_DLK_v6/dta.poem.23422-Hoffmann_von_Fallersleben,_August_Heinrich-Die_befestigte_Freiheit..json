{"dta.poem.23422": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die befestigte Freiheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie euch die Freiheit doch bel\u00e4stigt!", "tokens": ["Wie", "euch", "die", "Frei\u00b7heit", "doch", "be\u00b7l\u00e4s\u00b7tigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geduld! bald geht's damit vorbei,", "tokens": ["Ge\u00b7duld", "!", "bald", "geht's", "da\u00b7mit", "vor\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ist Paris nur erst befestigt,", "tokens": ["Denn", "ist", "Pa\u00b7ris", "nur", "erst", "be\u00b7fes\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Befestigt ist die Tyrannei.", "tokens": ["Be\u00b7fes\u00b7tigt", "ist", "die", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nig wird es anders deuten,", "tokens": ["Der", "K\u00f6\u00b7nig", "wird", "es", "an\u00b7ders", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er spricht: mein Volk bleibt ewig frei!", "tokens": ["Er", "spricht", ":", "mein", "Volk", "bleibt", "e\u00b7wig", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber sag's euch freien Leuten:", "tokens": ["Ich", "a\u00b7ber", "sag's", "euch", "frei\u00b7en", "Leu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Befestigt ist die Tyrannei.", "tokens": ["Be\u00b7fes\u00b7tigt", "ist", "die", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig lehrt euch von den Schanzen", "tokens": ["Der", "K\u00f6\u00b7nig", "lehrt", "euch", "von", "den", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n nach Kanonenmelodei", "tokens": ["Sch\u00f6n", "nach", "Ka\u00b7no\u00b7nen\u00b7me\u00b7lo\u00b7dei"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zu eurer Marseillaise tanzen \u2014", "tokens": ["Zu", "eu\u00b7rer", "Mar\u00b7seil\u00b7laise", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Befestigt ist die Tyrannei.", "tokens": ["Be\u00b7fes\u00b7tigt", "ist", "die", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}