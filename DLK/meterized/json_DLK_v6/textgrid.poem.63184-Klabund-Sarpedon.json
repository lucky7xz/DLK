{"textgrid.poem.63184": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Sarpedon", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeus liebte seinen Sohn", "tokens": ["Zeus", "lieb\u00b7te", "sei\u00b7nen", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Sohn der Laodamia: Sarpedon:", "tokens": ["Den", "Sohn", "der", "Lao\u00b7da\u00b7mia", ":", "Sar\u00b7pe\u00b7don", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "$.", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie ein Geliebter den Geliebten.", "tokens": ["Wie", "ein", "Ge\u00b7lieb\u00b7ter", "den", "Ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heimlich zuweilen", "tokens": ["Heim\u00b7lich", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In der Gestalt einer Schlange", "tokens": ["In", "der", "Ge\u00b7stalt", "ei\u00b7ner", "Schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Lag er bei ihm.", "tokens": ["Lag", "er", "bei", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+---", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Eines Tags begegneten einander", "tokens": ["Ei\u00b7nes", "Tags", "be\u00b7ge\u00b7gne\u00b7ten", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sarpedon und Hyakinthos,", "tokens": ["Sar\u00b7pe\u00b7don", "und", "Hy\u00b7a\u00b7kin\u00b7thos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Hirten.", "tokens": ["Sch\u00f6\u00b7ne", "Hir\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Zwischen sie trat Aphrodite", "tokens": ["Zwi\u00b7schen", "sie", "trat", "A\u00b7phro\u00b7di\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "L\u00fcstern beider.", "tokens": ["L\u00fcs\u00b7tern", "bei\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "In den H\u00e4nden ihre Br\u00fcste tragend wie zwei Teller", "tokens": ["In", "den", "H\u00e4n\u00b7den", "ih\u00b7re", "Br\u00fcs\u00b7te", "tra\u00b7gend", "wie", "zwei", "Tel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "ADJD", "KOKOM", "CARD", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Voll von Fr\u00fcchten.", "tokens": ["Voll", "von", "Fr\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Da stiessen die J\u00fcnglinge gegeneinander", "tokens": ["Da", "sties\u00b7sen", "die", "J\u00fcng\u00b7lin\u00b7ge", "ge\u00b7gen\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie Geier", "tokens": ["Wie", "Gei\u00b7er"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mit ihren Lanzen und strohenen Schilden.", "tokens": ["Mit", "ih\u00b7ren", "Lan\u00b7zen", "und", "stro\u00b7he\u00b7nen", "Schil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Auf seinem gl\u00e4sernen Stuhl", "tokens": ["Auf", "sei\u00b7nem", "gl\u00e4\u00b7ser\u00b7nen", "Stuhl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schloss Zeus die Augen,", "tokens": ["Schloss", "Zeus", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und eine Tr\u00e4ne tropfte aus den Wimpern.", "tokens": ["Und", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "tropf\u00b7te", "aus", "den", "Wim\u00b7pern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn keine Macht er hatte \u00fcber Ananke,", "tokens": ["Denn", "kei\u00b7ne", "Macht", "er", "hat\u00b7te", "\u00fc\u00b7ber", "A\u00b7nan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Das Schicksal", "tokens": ["Das", "Schick\u00b7sal"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und den Tod.", "tokens": ["Und", "den", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Die Tr\u00e4ne tropfte Sarpedon ins Auge", "tokens": ["Die", "Tr\u00e4\u00b7ne", "tropf\u00b7te", "Sar\u00b7pe\u00b7don", "ins", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und machte ihn blind,", "tokens": ["Und", "mach\u00b7te", "ihn", "blind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dass er der Deckung vergass.", "tokens": ["Dass", "er", "der", "De\u00b7ckung", "ver\u00b7gass", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Da traf ihn der w\u00fctige Feind", "tokens": ["Da", "traf", "ihn", "der", "w\u00fc\u00b7ti\u00b7ge", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ins Zwerchfell,", "tokens": ["Ins", "Zwerch\u00b7fell", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Dass er st\u00fcrzte", "tokens": ["Dass", "er", "st\u00fcrz\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wie eine Fichte am Bergbach.", "tokens": ["Wie", "ei\u00b7ne", "Fich\u00b7te", "am", "Berg\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Rot floss der Bach.", "tokens": ["Rot", "floss", "der", "Bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Tief auf seufzte Zeus,", "tokens": ["Tief", "auf", "seufz\u00b7te", "Zeus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "VVFIN", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dass die Erde bebte", "tokens": ["Dass", "die", "Er\u00b7de", "beb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Sonnenscheibe wie ein Zinnteller", "tokens": ["Und", "die", "Son\u00b7nen\u00b7schei\u00b7be", "wie", "ein", "Zinn\u00b7tel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Klirrte.", "tokens": ["Klirr\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Hyakinthos aber umarmte \u00fcber der Leiche", "tokens": ["Hy\u00b7a\u00b7kin\u00b7thos", "a\u00b7ber", "um\u00b7arm\u00b7te", "\u00fc\u00b7ber", "der", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die girrende G\u00f6ttin.", "tokens": ["Die", "gir\u00b7ren\u00b7de", "G\u00f6t\u00b7tin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Am Abend flog Apollon hernieder", "tokens": ["Am", "A\u00b7bend", "flog", "A\u00b7pol\u00b7lon", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NE", "PTKVZ"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schlug den Leichnam in seinen flatternden Mantel.", "tokens": ["Und", "schlug", "den", "Leich\u00b7nam", "in", "sei\u00b7nen", "flat\u00b7tern\u00b7den", "Man\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er trug ihn an die Gestade des Meeres", "tokens": ["Er", "trug", "ihn", "an", "die", "Ge\u00b7sta\u00b7de", "des", "Mee\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wusch ihn rein von Blut und Staub", "tokens": ["Und", "wusch", "ihn", "rein", "von", "Blut", "und", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und salbte ihn mit Ambrosia.", "tokens": ["Und", "salb\u00b7te", "ihn", "mit", "A\u00b7mbro\u00b7sia", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Da nahten fl\u00fcgelrauschend zwei Tauben", "tokens": ["Da", "nah\u00b7ten", "fl\u00fc\u00b7gel\u00b7rau\u00b7schend", "zwei", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "CARD", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwarz und weiss.", "tokens": ["Schwarz", "und", "weiss", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die schwarze Taube setzte sich auf die Schulter de Toten,", "tokens": ["Die", "schwar\u00b7ze", "Tau\u00b7be", "setz\u00b7te", "sich", "auf", "die", "Schul\u00b7ter", "de", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die weisse auf den Helm des schimmernden Gottes,", "tokens": ["Die", "weis\u00b7se", "auf", "den", "Helm", "des", "schim\u00b7mern\u00b7den", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der auf Wolken zum Olympos stieg.", "tokens": ["Der", "auf", "Wol\u00b7ken", "zum", "O\u00b7lym\u00b7pos", "stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}