{"textgrid.poem.37228": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[der Hausknecht in dem \u00bbWeidenbusch\u00ab]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hausknecht in dem \u00bbWeidenbusch\u00ab", "tokens": ["Der", "Haus\u00b7knecht", "in", "dem", "\u00bb", "Wei\u00b7den\u00b7busch", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Frankfurt an dem Main,", "tokens": ["Zu", "Frank\u00b7furt", "an", "dem", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der war Poet, doch immer kurz,", "tokens": ["Der", "war", "Po\u00b7et", ",", "doch", "im\u00b7mer", "kurz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn wenig fiel ihm ein.", "tokens": ["Denn", "we\u00b7nig", "fiel", "ihm", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, sprach er, Freund, wir leben jetzt", "tokens": ["Ja", ",", "sprach", "er", ",", "Freund", ",", "wir", "le\u00b7ben", "jetzt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der Depeschenzeit,", "tokens": ["In", "der", "De\u00b7pe\u00b7schen\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Schiller, k\u00e4m er heut zur\u00fcck,", "tokens": ["Und", "Schil\u00b7ler", ",", "k\u00e4m", "er", "heut", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4r auch nicht mehr so breit.", "tokens": ["W\u00e4r", "auch", "nicht", "mehr", "so", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}