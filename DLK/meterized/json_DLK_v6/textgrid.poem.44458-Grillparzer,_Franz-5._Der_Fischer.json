{"textgrid.poem.44458": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "5. Der Fischer", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sitz ich mit l\u00e4ssigen H\u00e4nden,", "tokens": ["Hier", "sitz", "ich", "mit", "l\u00e4s\u00b7si\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In still behaglicher Ruh,", "tokens": ["In", "still", "be\u00b7hag\u00b7li\u00b7cher", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und schaue den spielenden Fischlein", "tokens": ["Und", "schau\u00b7e", "den", "spie\u00b7len\u00b7den", "Fisc\u00b7hlein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+++", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im glitzernden Wasser zu.", "tokens": ["Im", "glit\u00b7zern\u00b7den", "Was\u00b7ser", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie jagen und gehen und kommen;", "tokens": ["Sie", "ja\u00b7gen", "und", "ge\u00b7hen", "und", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Doch werf ich die Angel aus,", "tokens": ["Doch", "werf", "ich", "die", "An\u00b7gel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Flugs sind sie von dannen geschwommen,", "tokens": ["Flugs", "sind", "sie", "von", "dan\u00b7nen", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und leer kehr ich abends nach Haus.", "tokens": ["Und", "leer", "kehr", "ich", "a\u00b7bends", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Versucht ichs und tr\u00fcbte das Wasser,", "tokens": ["Ver\u00b7sucht", "ichs", "und", "tr\u00fcb\u00b7te", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vielleicht gel\u00e4ng es eh;", "tokens": ["Viel\u00b7leicht", "ge\u00b7l\u00e4ng", "es", "eh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch m\u00fc\u00dft ich dann auch verzichten,", "tokens": ["Doch", "m\u00fc\u00dft", "ich", "dann", "auch", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie spielen zu sehen im See.", "tokens": ["Sie", "spie\u00b7len", "zu", "se\u00b7hen", "im", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}