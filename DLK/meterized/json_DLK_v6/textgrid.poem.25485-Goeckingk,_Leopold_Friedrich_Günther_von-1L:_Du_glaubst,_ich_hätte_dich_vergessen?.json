{"textgrid.poem.25485": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du glaubst, ich h\u00e4tte dich vergessen?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du glaubst, ich h\u00e4tte dich vergessen?", "tokens": ["Du", "glaubst", ",", "ich", "h\u00e4t\u00b7te", "dich", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wahrer Freund vergi\u00dft uns nicht!", "tokens": ["Ein", "wah\u00b7rer", "Freund", "ver\u00b7gi\u00dft", "uns", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gestehen mu\u00df ich es indessen", "tokens": ["Ge\u00b7ste\u00b7hen", "mu\u00df", "ich", "es", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit hell err\u00f6thendem Gesicht':", "tokens": ["Mit", "hell", "er\u00b7r\u00f6\u00b7then\u00b7dem", "Ge\u00b7sicht'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu lange hab' ich schon geschwiegen,", "tokens": ["Zu", "lan\u00b7ge", "hab'", "ich", "schon", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu schlecht gehalten dir mein Wort!", "tokens": ["Zu", "schlecht", "ge\u00b7hal\u00b7ten", "dir", "mein", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach! drei von deinen Briefen liegen", "tokens": ["Ach", "!", "drei", "von", "dei\u00b7nen", "Brie\u00b7fen", "lie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "CARD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Laut mahnend, leise z\u00fcrnend, dort!", "tokens": ["Laut", "mah\u00b7nend", ",", "lei\u00b7se", "z\u00fcr\u00b7nend", ",", "dort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJD", "VVPP", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weg sollte l\u00e4ngst die Antwort fliegen,", "tokens": ["Weg", "soll\u00b7te", "l\u00e4ngst", "die", "Ant\u00b7wort", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ist aber leider! noch nicht fort.", "tokens": ["Ist", "a\u00b7ber", "lei\u00b7der", "!", "noch", "nicht", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$.", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch hab ich Ursach' zu err\u00f6then?", "tokens": ["Doch", "hab", "ich", "Ur\u00b7sach'", "zu", "er\u00b7r\u00f6\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "War denn das Schweigen meine Schuld?", "tokens": ["War", "denn", "das", "Schwei\u00b7gen", "mei\u00b7ne", "Schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie? Hatten mich so tief die Fl\u00f6ten", "tokens": ["Wie", "?", "Hat\u00b7ten", "mich", "so", "tief", "die", "Fl\u00f6\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Liebesg\u00f6tter eingelullt,", "tokens": ["Der", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "ein\u00b7ge\u00b7lullt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df ich im Sybaritenschlummer", "tokens": ["Da\u00df", "ich", "im", "Sy\u00b7ba\u00b7ri\u00b7ten\u00b7schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Von meiner alten Freundin Kummer", "tokens": ["Von", "mei\u00b7ner", "al\u00b7ten", "Freun\u00b7din", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auch selbst in Tr\u00e4umen nichts empfand?", "tokens": ["Auch", "selbst", "in", "Tr\u00e4u\u00b7men", "nichts", "emp\u00b7fand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Lag ich, von ihrer zarten Hand", "tokens": ["Lag", "ich", ",", "von", "ih\u00b7rer", "zar\u00b7ten", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Gestreichelt, in Fortunens Schoo\u00df,", "tokens": ["Ge\u00b7strei\u00b7chelt", ",", "in", "For\u00b7tu\u00b7nens", "Schoo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und dachte: Hast das gro\u00dfe Loos,", "tokens": ["Und", "dach\u00b7te", ":", "Hast", "das", "gro\u00b7\u00dfe", "Loos", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Was k\u00fcmmern dich der Freunde Nieten?", "tokens": ["Was", "k\u00fcm\u00b7mern", "dich", "der", "Freun\u00b7de", "Nie\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und konnt' ich, (wie die Henn' im Br\u00fcten", "tokens": ["Und", "konnt'", "ich", ",", "(", "wie", "die", "Henn'", "im", "Br\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "$(", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nicht einen Augenblick ihr Nest,", "tokens": ["Nicht", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "ihr", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Trotz allen Lockungen, verl\u00e4\u00dft,)", "tokens": ["Trotz", "al\u00b7len", "Lo\u00b7ckun\u00b7gen", ",", "ver\u00b7l\u00e4\u00dft", ",", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "$,", "$("], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Mein Pult so lange nicht verlassen,", "tokens": ["Mein", "Pult", "so", "lan\u00b7ge", "nicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Um dich, die wartend vor mir stand,", "tokens": ["Um", "dich", ",", "die", "war\u00b7tend", "vor", "mir", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Bei der von Sorgen welken Hand,", "tokens": ["Bei", "der", "von", "Sor\u00b7gen", "wel\u00b7ken", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die so viel Sch\u00f6nes schrieb, zu fassen?", "tokens": ["Die", "so", "viel", "Sch\u00f6\u00b7nes", "schrieb", ",", "zu", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Wenn ich der Karte Matador", "tokens": ["Wenn", "ich", "der", "Kar\u00b7te", "Ma\u00b7ta\u00b7dor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Weit mehr als meine Leyer sch\u00e4tzte,", "tokens": ["Weit", "mehr", "als", "mei\u00b7ne", "Le\u00b7yer", "sch\u00e4tz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und einen Hasen lieber hetzte,", "tokens": ["Und", "ei\u00b7nen", "Ha\u00b7sen", "lie\u00b7ber", "hetz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Als durch ein Lied der Freundin Ohr,", "tokens": ["Als", "durch", "ein", "Lied", "der", "Freun\u00b7din", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wie vor zwei Jahren noch, erg\u00f6tzte;", "tokens": ["Wie", "vor", "zwei", "Jah\u00b7ren", "noch", ",", "er\u00b7g\u00f6tz\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "CARD", "NN", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Wenn nicht, wie sonst, ich inniglich,", "tokens": ["Wenn", "nicht", ",", "wie", "sonst", ",", "ich", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "PWAV", "ADV", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Noch deine Sorgen jetzt empf\u00e4nde:", "tokens": ["Noch", "dei\u00b7ne", "Sor\u00b7gen", "jetzt", "em\u00b7pf\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "O dann vergi\u00df, verachte mich!", "tokens": ["O", "dann", "ver\u00b7gi\u00df", ",", "ver\u00b7ach\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Denn mit der Freundschaft w\u00e4r's am Ende.", "tokens": ["Denn", "mit", "der", "Freund\u00b7schaft", "w\u00e4r's", "am", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Mein waren freilich tausend Stunden,", "tokens": ["Mein", "wa\u00b7ren", "frei\u00b7lich", "tau\u00b7send", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Seitdem von meinem letzten Laut'", "tokens": ["Seit\u00b7dem", "von", "mei\u00b7nem", "letz\u00b7ten", "Laut'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Der Nachhall deinem Ohr' entschwunden;", "tokens": ["Der", "Nach\u00b7hall", "dei\u00b7nem", "Ohr'", "ent\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Doch war ich, wann der Abend graut',", "tokens": ["Doch", "war", "ich", ",", "wann", "der", "A\u00b7bend", "graut'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "An meinem Geiste so geschunden,", "tokens": ["An", "mei\u00b7nem", "Geis\u00b7te", "so", "ge\u00b7schun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Wie Marsias an seiner Haut.", "tokens": ["Wie", "Mar\u00b7si\u00b7as", "an", "sei\u00b7ner", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Streich' diese Zeit von meinem Leben,", "tokens": ["Streich'", "die\u00b7se", "Zeit", "von", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Wie billig, auf der Rechnung aus,", "tokens": ["Wie", "bil\u00b7lig", ",", "auf", "der", "Rech\u00b7nung", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Dann kommt vom ganzen Jahr' nur eben", "tokens": ["Dann", "kommt", "vom", "gan\u00b7zen", "Jahr'", "nur", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Ein voller freier Tag heraus.", "tokens": ["Ein", "vol\u00b7ler", "frei\u00b7er", "Tag", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Nicht wahr: Nun wirst du mir vergeben?", "tokens": ["Nicht", "wahr", ":", "Nun", "wirst", "du", "mir", "ver\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Doch sag' ich nicht: Bedaure mich!", "tokens": ["Doch", "sag'", "ich", "nicht", ":", "Be\u00b7dau\u00b7re", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Nein, nein! denn diese Spanne Leben", "tokens": ["Nein", ",", "nein", "!", "denn", "die\u00b7se", "Span\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "KON", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Zu nehmen wie sie ist, wird Pflicht.", "tokens": ["Zu", "neh\u00b7men", "wie", "sie", "ist", ",", "wird", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "PPER", "VAFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Die\u00df, die\u00df nur kann den Muth erheben,", "tokens": ["Die\u00df", ",", "die\u00df", "nur", "kann", "den", "Muth", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PDS", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Bedauren aber hebt ihn nicht.", "tokens": ["Be\u00b7dau\u00b7ren", "a\u00b7ber", "hebt", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Sprich lieber: \u00bbDr\u00fcckt dich da der Schuh?", "tokens": ["Sprich", "lie\u00b7ber", ":", "\u00bb", "Dr\u00fcckt", "dich", "da", "der", "Schuh", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "O la\u00df den Blick umher nur wandern!", "tokens": ["O", "la\u00df", "den", "Blick", "um\u00b7her", "nur", "wan\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Schlie\u00dft Unabh\u00e4ngigkeit nicht andern,", "tokens": ["Schlie\u00dft", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit", "nicht", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Die alle besser sind, als du,", "tokens": ["Die", "al\u00b7le", "bes\u00b7ser", "sind", ",", "als", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Das Pf\u00f6rtchen vor der Nase zu?", "tokens": ["Das", "Pf\u00f6rt\u00b7chen", "vor", "der", "Na\u00b7se", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Darf manches Lied, das ich gesungen,", "tokens": ["Darf", "man\u00b7ches", "Lied", ",", "das", "ich", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Vergleich mit Sappho's Liedern scheun?", "tokens": ["Ver\u00b7gleich", "mit", "Sap\u00b7pho's", "Lie\u00b7dern", "scheun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Und dennoch lie\u00dfe sich's verzeihn,", "tokens": ["Und", "den\u00b7noch", "lie\u00b7\u00dfe", "sich's", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "W\u00e4r' ich, die Leyer fest umschlungen,", "tokens": ["W\u00e4r'", "ich", ",", "die", "Le\u00b7yer", "fest", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.63": {"text": "Vom Felsen in das Meer gesprungen.", "tokens": ["Vom", "Fel\u00b7sen", "in", "das", "Meer", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "Ich blieb, und bin doch nur ein Weib!", "tokens": ["Ich", "blieb", ",", "und", "bin", "doch", "nur", "ein", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Trag du denn auch dein Joch durchs Leben!", "tokens": ["Trag", "du", "denn", "auch", "dein", "Joch", "durchs", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Kannst du mir schreiben? Wohl! so schreib!", "tokens": ["Kannst", "du", "mir", "schrei\u00b7ben", "?", "Wohl", "!", "so", "schreib", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "ADV", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Wo nicht? Ei nun! In jenem Leben", "tokens": ["Wo", "nicht", "?", "Ei", "nun", "!", "In", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$.", "NN", "ADV", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "Wird's keine Sorgen f\u00fcr den Leib", "tokens": ["Wird's", "kei\u00b7ne", "Sor\u00b7gen", "f\u00fcr", "den", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.69": {"text": "Und keine Kanzeleien geben.\u00ab", "tokens": ["Und", "kei\u00b7ne", "Kan\u00b7ze\u00b7lei\u00b7en", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.70": {"text": "So, liebe Freundin, h\u00f6r' ich's gern!", "tokens": ["So", ",", "lie\u00b7be", "Freun\u00b7din", ",", "h\u00f6r'", "ich's", "gern", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "NN", "$,", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.71": {"text": "Mir wird ein solches Wort zum Stern',", "tokens": ["Mir", "wird", "ein", "sol\u00b7ches", "Wort", "zum", "Stern'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.72": {"text": "Auf dunkelm Pfad' nicht zu verzagen;", "tokens": ["Auf", "dun\u00b7kelm", "Pfad'", "nicht", "zu", "ver\u00b7za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.73": {"text": "Doch w\u00fcrdest du mich blo\u00df beklagen:", "tokens": ["Doch", "w\u00fcr\u00b7dest", "du", "mich", "blo\u00df", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.74": {"text": "Ungl\u00fccklicher m\u00f6cht' ich vielleicht", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "m\u00f6cht'", "ich", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.75": {"text": "Als ich schon wirklich bin, mich halten;", "tokens": ["Als", "ich", "schon", "wirk\u00b7lich", "bin", ",", "mich", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.76": {"text": "Denn ach! die Eigenliebe schleicht", "tokens": ["Denn", "ach", "!", "die", "Ei\u00b7gen\u00b7lie\u00b7be", "schleicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.77": {"text": "Sich in des Herzens engste Falten,", "tokens": ["Sich", "in", "des", "Her\u00b7zens", "engs\u00b7te", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.78": {"text": "Und ist die letzte, die entweicht.", "tokens": ["Und", "ist", "die", "letz\u00b7te", ",", "die", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.79": {"text": "Der Freund, der uns nicht helfen kann,", "tokens": ["Der", "Freund", ",", "der", "uns", "nicht", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.80": {"text": "Mag auch sein Mitleid an sich halten,", "tokens": ["Mag", "auch", "sein", "Mit\u00b7leid", "an", "sich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.81": {"text": "Und ruf' uns mit anscheinend kalten", "tokens": ["Und", "ruf'", "uns", "mit", "an\u00b7schei\u00b7nend", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.82": {"text": "Humor, statt dessen zu: Sey Mann!", "tokens": ["Hu\u00b7mor", ",", "statt", "des\u00b7sen", "zu", ":", "Sey", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "PDS", "PTKVZ", "$.", "VAFIN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.83": {"text": "Der Feige nur wird drum ihn hassen,", "tokens": ["Der", "Fei\u00b7ge", "nur", "wird", "drum", "ihn", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.84": {"text": "Ihn, der als Freund erprobet war,", "tokens": ["Ihn", ",", "der", "als", "Freund", "er\u00b7pro\u00b7bet", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "KOUS", "NN", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.85": {"text": "Und f\u00fcr uns \u2013 k\u00f6nnt' er es \u2013 gelassen", "tokens": ["Und", "f\u00fcr", "uns", "\u2013", "k\u00f6nnt'", "er", "es", "\u2013", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPER", "$(", "VMFIN", "PPER", "PPER", "$(", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.86": {"text": "Gern trotzen w\u00fcrde der Gefahr.", "tokens": ["Gern", "trot\u00b7zen", "w\u00fcr\u00b7de", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.87": {"text": "Soll das, was Weisheit von uns heischt,", "tokens": ["Soll", "das", ",", "was", "Weis\u00b7heit", "von", "uns", "heischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "$,", "PWS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.88": {"text": "Ein Freund von uns zu heischen, zagen?", "tokens": ["Ein", "Freund", "von", "uns", "zu", "hei\u00b7schen", ",", "za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.89": {"text": "Ein solches Mitleid, Freundin, t\u00e4uscht", "tokens": ["Ein", "sol\u00b7ches", "Mit\u00b7leid", ",", "Freun\u00b7din", ",", "t\u00e4uscht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "PIAT", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.90": {"text": "Das Herz, wie Schneemilchschaum den Magen.", "tokens": ["Das", "Herz", ",", "wie", "Schnee\u00b7milch\u00b7schaum", "den", "Ma\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.91": {"text": "Und ach! der letztere Betrug", "tokens": ["Und", "ach", "!", "der", "letz\u00b7te\u00b7re", "Be\u00b7trug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.92": {"text": "Wird allemal der schlimmste werden.", "tokens": ["Wird", "al\u00b7le\u00b7mal", "der", "schlimms\u00b7te", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.93": {"text": "Nur Muth! Nur Muth! So ist auf Erden", "tokens": ["Nur", "Muth", "!", "Nur", "Muth", "!", "So", "ist", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "ADV", "NN", "$.", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.94": {"text": "Der Mensch sich selbst noch Trost genug.", "tokens": ["Der", "Mensch", "sich", "selbst", "noch", "Trost", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.95": {"text": "Darum bewahr' dich Gott, (dein Joch", "tokens": ["Da\u00b7rum", "be\u00b7wahr'", "dich", "Gott", ",", "(", "dein", "Joch"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.96": {"text": "Ist schwer genug!) vor Leutchen doch,", "tokens": ["Ist", "schwer", "ge\u00b7nug", "!", ")", "vor", "Leut\u00b7chen", "doch", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$.", "$(", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.97": {"text": "Die dich mit kaltem Troste kosen,", "tokens": ["Die", "dich", "mit", "kal\u00b7tem", "Tros\u00b7te", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.98": {"text": "Und dir so leicht mit Myrth und Rosen,", "tokens": ["Und", "dir", "so", "leicht", "mit", "Myrth", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.99": {"text": "Zum wenigsten mit Thimian,", "tokens": ["Zum", "we\u00b7nigs\u00b7ten", "mit", "Thi\u00b7mi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.100": {"text": "Wenn andern sie, als sich, ihn g\u00f6nnten,", "tokens": ["Wenn", "an\u00b7dern", "sie", ",", "als", "sich", ",", "ihn", "g\u00f6nn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "KOUS", "PRF", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.101": {"text": "Den kurzen Rest der Lebensbahn'", "tokens": ["Den", "kur\u00b7zen", "Rest", "der", "Le\u00b7bens\u00b7bahn'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.102": {"text": "Bis an das Grab bestreuen k\u00f6nnten.", "tokens": ["Bis", "an", "das", "Grab", "be\u00b7streu\u00b7en", "k\u00f6nn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.103": {"text": "O! liebst du dich, und mich dazu,", "tokens": ["O", "!", "liebst", "du", "dich", ",", "und", "mich", "da\u00b7zu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PRF", "$,", "KON", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.104": {"text": "So wirst du nichts von ihnen hoffen.", "tokens": ["So", "wirst", "du", "nichts", "von", "ih\u00b7nen", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.105": {"text": "So steht mein Arm, mein Haus dir offen.", "tokens": ["So", "steht", "mein", "Arm", ",", "mein", "Haus", "dir", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.106": {"text": "Leb' wohl! der Morgen kommt! Betroffen", "tokens": ["Leb'", "wohl", "!", "der", "Mor\u00b7gen", "kommt", "!", "Be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "ADV", "$.", "ART", "NN", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.107": {"text": "Schlie\u00dft er mein m\u00fcdes Auge zu.", "tokens": ["Schlie\u00dft", "er", "mein", "m\u00fc\u00b7des", "Au\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}