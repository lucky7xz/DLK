{"textgrid.poem.26243": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Schlachtfeldkreuze", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht dort die Reihen Kreuze stehen,", "tokens": ["Seht", "dort", "die", "Rei\u00b7hen", "Kreu\u00b7ze", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich einem Pilgerzug zu sehen", "tokens": ["Gleich", "ei\u00b7nem", "Pil\u00b7ger\u00b7zug", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Himmelsrand ohn' Ende.", "tokens": ["Am", "Him\u00b7mels\u00b7rand", "ohn'", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seht um die Kreuze Primeln bl\u00fchen,", "tokens": ["Seht", "um", "die", "Kreu\u00b7ze", "Pri\u00b7meln", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gr\u00e4ber frohen Mutes gl\u00fchen", "tokens": ["Die", "Gr\u00e4\u00b7ber", "fro\u00b7hen", "Mu\u00b7tes", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4cheln im Gel\u00e4nde.", "tokens": ["Und", "l\u00e4\u00b7cheln", "im", "Ge\u00b7l\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Toten aus den Feldern winken:", "tokens": ["Die", "To\u00b7ten", "aus", "den", "Fel\u00b7dern", "win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbla\u00dft nicht den Mut zum Kampfe sinken!", "tokens": ["\u00bb", "la\u00dft", "nicht", "den", "Mut", "zum", "Kamp\u00b7fe", "sin\u00b7ken", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKNEG", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir reichen euch die H\u00e4nde.\u00ab", "tokens": ["Wir", "rei\u00b7chen", "euch", "die", "H\u00e4n\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}