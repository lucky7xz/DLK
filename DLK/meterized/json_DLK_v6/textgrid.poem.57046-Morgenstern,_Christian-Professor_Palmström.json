{"textgrid.poem.57046": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Professor Palmstr\u00f6m", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Irgendwo im Lande gibt es meist", "tokens": ["Ir\u00b7gend\u00b7wo", "im", "Lan\u00b7de", "gibt", "es", "meist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "einen Staat, von dem, was sich an Geist", "tokens": ["ei\u00b7nen", "Staat", ",", "von", "dem", ",", "was", "sich", "an", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "irgendwo befindet und erweist,", "tokens": ["ir\u00b7gend\u00b7wo", "be\u00b7fin\u00b7det", "und", "er\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "doch noch nirgendwo Professor hei\u00dft,", "tokens": ["doch", "noch", "nir\u00b7gend\u00b7wo", "Pro\u00b7fes\u00b7sor", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "eben zum Professor wird gemacht,", "tokens": ["e\u00b7ben", "zum", "Pro\u00b7fes\u00b7sor", "wird", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wie von wem, der unaufh\u00f6rlich wacht,", "tokens": ["wie", "von", "wem", ",", "der", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ob er auch jeder Seele wird gedacht,", "tokens": ["ob", "er", "auch", "je\u00b7der", "See\u00b7le", "wird", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die der Menschheit Gl\u00fcck und Heil gebracht.", "tokens": ["die", "der", "Menschheit", "Gl\u00fcck", "und", "Heil", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Solch ein Staat und solch ein F\u00fcrst, o denkt,", "tokens": ["Solch", "ein", "Staat", "und", "solch", "ein", "F\u00fcrst", ",", "o", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "KON", "PIAT", "ART", "NN", "$,", "FM", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "hat auch Palmstr\u00f6ms Los zum Licht gelenkt,", "tokens": ["hat", "auch", "Palm\u00b7str\u00f6ms", "Los", "zum", "Licht", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "hat ihm den Professorrang geschenkt", "tokens": ["hat", "ihm", "den", "Pro\u00b7fes\u00b7sor\u00b7rang", "ge\u00b7schenkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und das Kreuz f\u00fcr Kunst ihm umgehenkt.", "tokens": ["und", "das", "Kreuz", "f\u00fcr", "Kunst", "ihm", "um\u00b7ge\u00b7henkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Palmstr\u00f6m gibt das Kreuz f\u00fcr Kunst zur\u00fcck;", "tokens": ["Palm\u00b7str\u00f6m", "gibt", "das", "Kreuz", "f\u00fcr", "Kunst", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "denn er tr\u00e4gt kein solches Kleidungsst\u00fcck.", "tokens": ["denn", "er", "tr\u00e4gt", "kein", "sol\u00b7ches", "Klei\u00b7dungs\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Den Professor nicht; denn man versteht:", "tokens": ["Den", "Pro\u00b7fes\u00b7sor", "nicht", ";", "denn", "man", "ver\u00b7steht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$.", "KON", "PIS", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Als Professor gilt erst ein Prophet.", "tokens": ["Als", "Pro\u00b7fes\u00b7sor", "gilt", "erst", "ein", "Pro\u00b7phet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}