{"textgrid.poem.60598": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Gott, ein Erretter", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Finsterni\u00df und schnelle Wetter", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", "und", "schnel\u00b7le", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brechen \u00fcber mich herein;", "tokens": ["Bre\u00b7chen", "\u00fc\u00b7ber", "mich", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich sehe keinen Retter,", "tokens": ["Und", "ich", "se\u00b7he", "kei\u00b7nen", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner Hoffnung blassen Schein.", "tokens": ["Kei\u00b7ner", "Hoff\u00b7nung", "blas\u00b7sen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine schweren Donner rauschen,", "tokens": ["Dei\u00b7ne", "schwe\u00b7ren", "Don\u00b7ner", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott! vom weiten wider mich:", "tokens": ["Gott", "!", "vom", "wei\u00b7ten", "wi\u00b7der", "mich", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "ADJA", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber meine Feinde lauschen;", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Fein\u00b7de", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein Verfolger freuet sich.", "tokens": ["Mein", "Ver\u00b7fol\u00b7ger", "freu\u00b7et", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sehet! sprechen, die mich hassen,", "tokens": ["Se\u00b7het", "!", "spre\u00b7chen", ",", "die", "mich", "has\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Netz hat ihn gef\u00e4llt!", "tokens": ["Un\u00b7ser", "Netz", "hat", "ihn", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, er liegt und liegt verlassen,", "tokens": ["Ja", ",", "er", "liegt", "und", "liegt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "KON", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem wir lange nachgestellt!", "tokens": ["Dem", "wir", "lan\u00b7ge", "nach\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine T\u00fccke, schwarze Rotte,", "tokens": ["Dei\u00b7ne", "T\u00fc\u00b7cke", ",", "schwar\u00b7ze", "Rot\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind mir wenig f\u00fcrchterlich!", "tokens": ["Sind", "mir", "we\u00b7nig", "f\u00fcrch\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich erzittre nur vor Gotte:", "tokens": ["Ich", "er\u00b7zitt\u00b7re", "nur", "vor", "Got\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gott ist aber wider mich!", "tokens": ["Gott", "ist", "a\u00b7ber", "wi\u00b7der", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O entsetzlicher Gedanke,", "tokens": ["O", "ent\u00b7setz\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich von Gott verfolget sehn!", "tokens": ["Sich", "von", "Gott", "ver\u00b7fol\u00b7get", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wag ichs, in verwegnem Zanke,", "tokens": ["Wag", "ichs", ",", "in", "ver\u00b7weg\u00b7nem", "Zan\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Allm\u00e4chtigen zu schm\u00e4hn?", "tokens": ["Den", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "zu", "schm\u00e4hn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine schrecklichsten Gerichte", "tokens": ["Sei\u00b7ne", "schreck\u00b7lichs\u00b7ten", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Sind gerecht: was wend ich ein?", "tokens": ["Sind", "ge\u00b7recht", ":", "was", "wend", "ich", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PWS", "KOUS", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O vor seinem Angesichte", "tokens": ["O", "vor", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind die Engel selbst nicht rein!", "tokens": ["Sind", "die", "En\u00b7gel", "selbst", "nicht", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Will ich mich der Straf entziehen:", "tokens": ["Will", "ich", "mich", "der", "Straf", "ent\u00b7zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie umsonst ist meine Flucht!", "tokens": ["Wie", "um\u00b7sonst", "ist", "mei\u00b7ne", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Mag ein Sterblicher entfliehen,", "tokens": ["Mag", "ein", "Sterb\u00b7li\u00b7cher", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den des H\u00f6chsten Auge sucht?", "tokens": ["Den", "des", "H\u00f6chs\u00b7ten", "Au\u00b7ge", "sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heere, Lager, Scepter, Krone", "tokens": ["Hee\u00b7re", ",", "La\u00b7ger", ",", "Scep\u00b7ter", ",", "Kro\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00fctzen den Verbrecher nicht:", "tokens": ["Sch\u00fct\u00b7zen", "den", "Ver\u00b7bre\u00b7cher", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch beym schimmerreichen Throne", "tokens": ["Auch", "beym", "schim\u00b7mer\u00b7rei\u00b7chen", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Findet Gott den B\u00f6sewicht.", "tokens": ["Fin\u00b7det", "Gott", "den", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Herr! Mit kindlichem Vertrauen", "tokens": ["Herr", "!", "Mit", "kind\u00b7li\u00b7chem", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hang ich dennoch fest an dir,", "tokens": ["Hang", "ich", "den\u00b7noch", "fest", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie sollte mir noch grauen?", "tokens": ["O", "wie", "soll\u00b7te", "mir", "noch", "grau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater, du verzeihest mir!", "tokens": ["Va\u00b7ter", ",", "du", "ver\u00b7zei\u00b7hest", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich verfluche meine S\u00fcnden,", "tokens": ["Ich", "ver\u00b7flu\u00b7che", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mir deinen Schutz entwandt!", "tokens": ["Die", "mir", "dei\u00b7nen", "Schutz", "ent\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df dich finden, la\u00df dich finden,", "tokens": ["La\u00df", "dich", "fin\u00b7den", ",", "la\u00df", "dich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie dich stets die Reue fand!", "tokens": ["Wie", "dich", "stets", "die", "Reu\u00b7e", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn der Blitz in deinen H\u00e4nden", "tokens": ["Wenn", "der", "Blitz", "in", "dei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von entbranntem Zorne schnaubt:", "tokens": ["Von", "ent\u00b7brann\u00b7tem", "Zor\u00b7ne", "schnaubt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft er sich durch Reue wenden,", "tokens": ["L\u00e4\u00dft", "er", "sich", "durch", "Reu\u00b7e", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschont ein schuldig Haupt.", "tokens": ["Und", "ver\u00b7schont", "ein", "schul\u00b7dig", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du bist nicht ein Mensch, der z\u00fcrne,", "tokens": ["Du", "bist", "nicht", "ein", "Mensch", ",", "der", "z\u00fcr\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herr, auf wen? Auf mattes Laub?", "tokens": ["Herr", ",", "auf", "wen", "?", "Auf", "mat\u00b7tes", "Laub", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PWS", "$.", "APPR", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Du, der Sch\u00f6pfer der Gestirne,", "tokens": ["Du", ",", "der", "Sch\u00f6p\u00b7fer", "der", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du bist Gott, und ich bin Staub!", "tokens": ["Du", "bist", "Gott", ",", "und", "ich", "bin", "Staub", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "KON", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach! da\u00df ich dich zu befl\u00fcgeln,", "tokens": ["Ach", "!", "da\u00df", "ich", "dich", "zu", "be\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tag der H\u00fclfe, nicht vermag!", "tokens": ["Tag", "der", "H\u00fcl\u00b7fe", ",", "nicht", "ver\u00b7mag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4nze bald auf unsern H\u00fcgeln!", "tokens": ["Gl\u00e4n\u00b7ze", "bald", "auf", "un\u00b7sern", "H\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brich doch an, erseufzter Tag!", "tokens": ["Brich", "doch", "an", ",", "er\u00b7seufz\u00b7ter", "Tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Knirscht vor Unmuth, meine Feinde!", "tokens": ["Knirscht", "vor", "Un\u00b7muth", ",", "mei\u00b7ne", "Fein\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure Bosheit f\u00e4llt mich nicht:", "tokens": ["Eu\u00b7re", "Bos\u00b7heit", "f\u00e4llt", "mich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ich habe Gott zum Freunde!", "tokens": ["Denn", "ich", "ha\u00b7be", "Gott", "zum", "Freun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gott ist meine Zuversicht!", "tokens": ["Gott", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}