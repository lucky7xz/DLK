{"dta.poem.4313": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schnee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern sie nun, in ihrem Fall, solch eine kalte Luft ber\u00fch-", "tokens": ["Wo\u00b7fern", "sie", "nun", ",", "in", "ih\u00b7rem", "Fall", ",", "solch", "ei\u00b7ne", "kal\u00b7te", "Luft", "be\u00b7r\u00fch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "PIAT", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "ren,", "tokens": ["ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Worinn sich eine K\u00e4lte findet, so kalt, da\u00df sie darinn gefrie-", "tokens": ["Wo\u00b7rinn", "sich", "ei\u00b7ne", "K\u00e4l\u00b7te", "fin\u00b7det", ",", "so", "kalt", ",", "da\u00df", "sie", "da\u00b7rinn", "ge\u00b7frie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "PAV", "TRUNC"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "ren;", "tokens": ["ren", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "So wird aus vieler Bl\u00e4sgen Klumpen bald kleiner und", "tokens": ["So", "wird", "aus", "vie\u00b7ler", "Bl\u00e4s\u00b7gen", "Klum\u00b7pen", "bald", "klei\u00b7ner", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "NN", "ADV", "ADJD", "KON"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "bald grosser Schnee.", "tokens": ["bald", "gros\u00b7ser", "Schnee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie dieser Schnee nun fl\u00fcchtig Salz und Oel und Fettig-", "tokens": ["Wie", "die\u00b7ser", "Schnee", "nun", "fl\u00fcch\u00b7tig", "Salz", "und", "O\u00b7el", "und", "Fet\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "ADV", "ADJD", "NN", "KON", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "keit enth\u00e4lt,", "tokens": ["keit", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Nebst einem kleinen Feuer-Theilchen, der in dem Mittel-", "tokens": ["Nebst", "ei\u00b7nem", "klei\u00b7nen", "Feu\u00b7er\u00b7T\u00b7heil\u00b7chen", ",", "der", "in", "dem", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "punct verschr\u00e4nkt;", "tokens": ["punct", "ver\u00b7schr\u00e4nkt", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "So mu\u00df aus diesen Eigenschaften, wenn solcher Schnee", "tokens": ["So", "mu\u00df", "aus", "die\u00b7sen", "Ei\u00b7gen\u00b7schaf\u00b7ten", ",", "wenn", "sol\u00b7cher", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PDAT", "NN", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "sich abwerts senkt,", "tokens": ["sich", "ab\u00b7werts", "senkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Und auf den luckern Boden f\u00e4llt, das Theil des Ackers", "tokens": ["Und", "auf", "den", "lu\u00b7ckern", "Bo\u00b7den", "f\u00e4llt", ",", "das", "Theil", "des", "A\u00b7ckers"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und der Erden,", "tokens": ["und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Das er befeuchtet und zugleich bed\u00fcnget, durch ihn frucht-", "tokens": ["Das", "er", "be\u00b7feuch\u00b7tet", "und", "zu\u00b7gleich", "be\u00b7d\u00fcn\u00b7get", ",", "durch", "ihn", "frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "KON", "ADV", "VVFIN", "$,", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "bar werden.", "tokens": ["bar", "wer\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}