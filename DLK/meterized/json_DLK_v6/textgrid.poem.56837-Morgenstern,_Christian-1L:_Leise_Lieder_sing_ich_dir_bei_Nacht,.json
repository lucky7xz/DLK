{"textgrid.poem.56837": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leise Lieder sing ich dir bei Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise Lieder sing ich dir bei Nacht,", "tokens": ["Lei\u00b7se", "Lie\u00b7der", "sing", "ich", "dir", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Lieder, die kein sterblich Ohr vernimmt,", "tokens": ["Lie\u00b7der", ",", "die", "kein", "sterb\u00b7lich", "Ohr", "ver\u00b7nimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "noch ein Stern, der etwa sp\u00e4hend wacht,", "tokens": ["noch", "ein", "Stern", ",", "der", "et\u00b7wa", "sp\u00e4\u00b7hend", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "noch der Mond, der still im \u00c4ther schwimmt;", "tokens": ["noch", "der", "Mond", ",", "der", "still", "im", "\u00c4\u00b7ther", "schwimmt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "denen niemand als das eigne Herz,", "tokens": ["de\u00b7nen", "nie\u00b7mand", "als", "das", "eig\u00b7ne", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PIS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "das sie tr\u00e4umt, in tiefer Wehmut lauscht,", "tokens": ["das", "sie", "tr\u00e4umt", ",", "in", "tie\u00b7fer", "Weh\u00b7mut", "lauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und an denen niemand als der Schmerz,", "tokens": ["und", "an", "de\u00b7nen", "nie\u00b7mand", "als", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "der sie zeugt, sich kummervoll berauscht.", "tokens": ["der", "sie", "zeugt", ",", "sich", "kum\u00b7mer\u00b7voll", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Leise Lieder sing ich dir bei Nacht,", "tokens": ["Lei\u00b7se", "Lie\u00b7der", "sing", "ich", "dir", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "dir, in deren Aug mein Sinn versank,", "tokens": ["dir", ",", "in", "de\u00b7ren", "Aug", "mein", "Sinn", "ver\u00b7sank", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRELAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und aus dessen tiefem, dunklen Schacht", "tokens": ["und", "aus", "des\u00b7sen", "tie\u00b7fem", ",", "dunk\u00b7len", "Schacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "meine Seele ewige Sehnsucht trank.", "tokens": ["mei\u00b7ne", "See\u00b7le", "e\u00b7wi\u00b7ge", "Sehn\u00b7sucht", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}