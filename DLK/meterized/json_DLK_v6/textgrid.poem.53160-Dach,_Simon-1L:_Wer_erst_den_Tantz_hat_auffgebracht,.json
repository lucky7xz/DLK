{"textgrid.poem.53160": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer erst den Tantz hat auffgebracht,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer erst den Tantz hat auffgebracht,", "tokens": ["Wer", "erst", "den", "Tantz", "hat", "auff\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat die Verliebten wol bedacht", "tokens": ["Hat", "die", "Ver\u00b7lieb\u00b7ten", "wol", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jhren schweren Flammen:", "tokens": ["In", "jhren", "schwe\u00b7ren", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wann nichts sonst jhren Sinn begn\u00fcgt,", "tokens": ["Wann", "nichts", "sonst", "jhren", "Sinn", "be\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kein Ort sie an einander f\u00fcgt,", "tokens": ["Kein", "Ort", "sie", "an", "ein\u00b7an\u00b7der", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bringt sie der Tantz zusammen.", "tokens": ["Bringt", "sie", "der", "Tantz", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Hertz liegt in der Liebe kranck,", "tokens": ["Ihr", "Hertz", "liegt", "in", "der", "Lie\u00b7be", "kranck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird kein Mahl, kein s\u00fcsser Tranck", "tokens": ["Es", "wird", "kein", "Mahl", ",", "kein", "s\u00fcs\u00b7ser", "Tranck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey jhnen was verfangen:", "tokens": ["Bey", "jh\u00b7nen", "was", "ver\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sieht sie voller Hoffnung stehn,", "tokens": ["Man", "sieht", "sie", "vol\u00b7ler", "Hoff\u00b7nung", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn nun die Tantz-Lust an-solgehn,", "tokens": ["Wenn", "nun", "die", "Tant\u00b7zLust", "an\u00b7sol\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die stillet jhr Verlangen.", "tokens": ["Die", "stil\u00b7let", "jhr", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sind auch die Tische gleich beyseit,", "tokens": ["Sind", "auch", "die", "Ti\u00b7sche", "gleich", "bey\u00b7seit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht jhnen doch die lange Zeit", "tokens": ["Macht", "jh\u00b7nen", "doch", "die", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch immer tausent Schmertzen:", "tokens": ["Noch", "im\u00b7mer", "tau\u00b7sent", "Schmert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie kommen allem Tantz zuvor,", "tokens": ["Sie", "kom\u00b7men", "al\u00b7lem", "Tantz", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sind jhnen beydes Sahl vnd Chor", "tokens": ["Sind", "jh\u00b7nen", "bey\u00b7des", "Sahl", "vnd", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PIS", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd tantzen frisch im Hertzen.", "tokens": ["Vnd", "tant\u00b7zen", "frisch", "im", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die\u00df ist der Liebe strenge Zucht,", "tokens": ["Die\u00df", "ist", "der", "Lie\u00b7be", "stren\u00b7ge", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer Rhu in jhren Diensten sucht,", "tokens": ["Wer", "Rhu", "in", "jhren", "Diens\u00b7ten", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sucht Wasser in dem Fewer,", "tokens": ["Sucht", "Was\u00b7ser", "in", "dem", "Fe\u00b7wer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Volck mus, wie die Sclaven, fort,", "tokens": ["Ihr", "Volck", "mus", ",", "wie", "die", "Scla\u00b7ven", ",", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "PWAV", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ist f\u00fcrwar, mit einem Wort,", "tokens": ["Sie", "ist", "f\u00fcr\u00b7war", ",", "mit", "ei\u00b7nem", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein rechtes Vngehewer.", "tokens": ["Ein", "rech\u00b7tes", "Vn\u00b7ge\u00b7he\u00b7wer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Wol denen, die in Heyraht stehn", "tokens": ["Wol", "de\u00b7nen", ",", "die", "in", "Hey\u00b7raht", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "$,", "PRELS", "APPR", "NN", "VVINF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vnd jhrer Bande m\u00fcssig gehn,", "tokens": ["Vnd", "jhrer", "Ban\u00b7de", "m\u00fcs\u00b7sig", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie weit sind sie von Leiden!", "tokens": ["Wie", "weit", "sind", "sie", "von", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seht vnsern wehrten Br\u00e4utgam an,", "tokens": ["Seht", "vn\u00b7sern", "wehr\u00b7ten", "Br\u00e4ut\u00b7gam", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der jhren Hochmuth trotzen kan,", "tokens": ["Der", "jhren", "Hoch\u00b7muth", "trot\u00b7zen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie tantzet er in Frewden!", "tokens": ["Wie", "tant\u00b7zet", "er", "in", "Frew\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er f\u00fchrt an seiner rechten Hand", "tokens": ["Er", "f\u00fchrt", "an", "sei\u00b7ner", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein au\u00dferwehltes Seelen-Pfand,", "tokens": ["Sein", "au\u00b7\u00dfer\u00b7wehl\u00b7tes", "See\u00b7len\u00b7Pfand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Lohn f\u00fcr seine Tugendt,", "tokens": ["Den", "Lohn", "f\u00fcr", "sei\u00b7ne", "Tu\u00b7gendt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Sie, die Sch\u00f6nste, die er weis,", "tokens": ["Vnd", "Sie", ",", "die", "Sch\u00f6ns\u00b7te", ",", "die", "er", "weis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tr\u00e4gt aller Zucht vnd Vnschuld Preis,", "tokens": ["Tr\u00e4gt", "al\u00b7ler", "Zucht", "vnd", "Vn\u00b7schuld", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd ist ein Glantz der Jugend.", "tokens": ["Vnd", "ist", "ein", "Glantz", "der", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Folgt jhnen in dem Tantze zwar,", "tokens": ["Folgt", "jh\u00b7nen", "in", "dem", "Tant\u00b7ze", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr aber in der Gaben Schar,", "tokens": ["Mehr", "a\u00b7ber", "in", "der", "Ga\u00b7ben", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird es keinem fehlen,", "tokens": ["So", "wird", "es", "kei\u00b7nem", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott weis umb euch allein bescheid,", "tokens": ["Gott", "weis", "umb", "euch", "al\u00b7lein", "be\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird einem jeden mit der Zeit", "tokens": ["Wird", "ei\u00b7nem", "je\u00b7den", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das seine wol erwehlen.", "tokens": ["Das", "sei\u00b7ne", "wol", "er\u00b7weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So tantzet nun gerad vnd krumm,", "tokens": ["So", "tant\u00b7zet", "nun", "ge\u00b7rad", "vnd", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt jhr die Liebste, wechselt umb,", "tokens": ["Wollt", "jhr", "die", "Liebs\u00b7te", ",", "wech\u00b7selt", "umb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Z\u00fcrnt, wenn jhr still solt stehen:", "tokens": ["Z\u00fcrnt", ",", "wenn", "jhr", "still", "solt", "ste\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gebraucht in Ehren euch der Welt,", "tokens": ["Ge\u00b7braucht", "in", "Eh\u00b7ren", "euch", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn euch das Alter \u00fcberf\u00e4llt,", "tokens": ["Wenn", "euch", "das", "Al\u00b7ter", "\u00fc\u00b7berf\u00b7\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es wird euch wol vergehen.", "tokens": ["Es", "wird", "euch", "wol", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}