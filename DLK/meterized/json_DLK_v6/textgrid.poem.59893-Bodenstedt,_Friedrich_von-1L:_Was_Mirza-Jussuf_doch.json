{"textgrid.poem.59893": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was Mirza-Jussuf doch", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Mirza-Jussuf doch", "tokens": ["Was", "Mir\u00b7za\u00b7Jus\u00b7suf", "doch"], "token_info": ["word", "word", "word"], "pos": ["PWS", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein kritischer Gesell ist!", "tokens": ["Ein", "kri\u00b7ti\u00b7scher", "Ge\u00b7sell", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Tag gef\u00e4llt ihm nicht,", "tokens": ["Der", "Tag", "ge\u00b7f\u00e4llt", "ihm", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weil ihm der Tag zu hell ist!", "tokens": ["Weil", "ihm", "der", "Tag", "zu", "hell", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er liebt die Rose nicht,", "tokens": ["Er", "liebt", "die", "Ro\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weil Stachel sie und Dorn hat,", "tokens": ["Weil", "Sta\u00b7chel", "sie", "und", "Dorn", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und liebt den Menschen nicht,", "tokens": ["Und", "liebt", "den", "Men\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weil er die Nase vorn hat!", "tokens": ["Weil", "er", "die", "Na\u00b7se", "vorn", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "+-++---", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Er tadelt alles rings,", "tokens": ["Er", "ta\u00b7delt", "al\u00b7les", "rings", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was nicht nach seinem Kopf ist \u2013", "tokens": ["Was", "nicht", "nach", "sei\u00b7nem", "Kopf", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Merkt alles in der Welt,", "tokens": ["Merkt", "al\u00b7les", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur nicht, da\u00df er ein Tropf ist!", "tokens": ["Nur", "nicht", ",", "da\u00df", "er", "ein", "Tropf", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So liegt er immer mit", "tokens": ["So", "liegt", "er", "im\u00b7mer", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Natur und Kunst im Kampf,", "tokens": ["Na\u00b7tur", "und", "Kunst", "im", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So treibt es Tag und Nacht ihn", "tokens": ["So", "treibt", "es", "Tag", "und", "Nacht", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch blauen Dunst und Dampf!", "tokens": ["Durch", "blau\u00b7en", "Dunst", "und", "Dampf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mirza-Schaffy belacht ihn", "tokens": ["Mir\u00b7za\u00b7Schaffy", "be\u00b7lacht", "ihn"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit schelmischem Gesicht", "tokens": ["Mit", "schel\u00b7mi\u00b7schem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und macht aus seiner Bitterkeit", "tokens": ["Und", "macht", "aus", "sei\u00b7ner", "Bit\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das s\u00fc\u00dfeste Gedicht!", "tokens": ["Das", "s\u00fc\u00b7\u00dfes\u00b7te", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}