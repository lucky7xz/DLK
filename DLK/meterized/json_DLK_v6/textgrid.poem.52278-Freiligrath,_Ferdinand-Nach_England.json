{"textgrid.poem.52278": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Nach England", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich her von Frankreich fuhr,", "tokens": ["Als", "ich", "her", "von", "Fran\u00b7kreich", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach das Meer: \u00bbTreib sie zu Paaren!", "tokens": ["Sprach", "das", "Meer", ":", "\u00bb", "Treib", "sie", "zu", "Paa\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleiche dem Erobrer nur,", "tokens": ["Glei\u00b7che", "dem", "E\u00b7rob\u00b7rer", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ich trug vor tausend Jahren!", "tokens": ["Den", "ich", "trug", "vor", "tau\u00b7send", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In derselben Furch' einher", "tokens": ["In", "der\u00b7sel\u00b7ben", "Furch'", "ein\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwimmst du, die ", "tokens": ["Schwimmst", "du", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "K\u00fchnen Sprunges drum, wie er,", "tokens": ["K\u00fch\u00b7nen", "Sprun\u00b7ges", "drum", ",", "wie", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "$,", "PWAV", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Wirf dich wider diese Briten!", "tokens": ["Wirf", "dich", "wi\u00b7der", "die\u00b7se", "Bri\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Spring ans Land und fall ans Land!", "tokens": ["Spring", "ans", "Land", "und", "fall", "ans", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur auch decke mit der Hand es!", "tokens": ["Nur", "auch", "de\u00b7cke", "mit", "der", "Hand", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rufe: Mein dies Engelland!", "tokens": ["Ru\u00b7fe", ":", "Mein", "dies", "En\u00b7gel\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein! Denn meine Hand umspannt es!", "tokens": ["Mein", "!", "Denn", "mei\u00b7ne", "Hand", "um\u00b7spannt", "es", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann empor und in den Streit!", "tokens": ["Dann", "em\u00b7por", "und", "in", "den", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vorgeeilt auf r\u00fcst'gen F\u00fc\u00dfen!", "tokens": ["Vor\u00b7ge\u00b7eilt", "auf", "r\u00fcst'\u00b7gen", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es wird zu rechter Zeit", "tokens": ["Und", "es", "wird", "zu", "rech\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hastings dich als Sieger gr\u00fc\u00dfen!", "tokens": ["Has\u00b7tings", "dich", "als", "Sie\u00b7ger", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hastingsfeld ist allerw\u00e4rts,", "tokens": ["Has\u00b7tings\u00b7feld", "ist", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hastingsschlacht ist allerwegen,", "tokens": ["Has\u00b7tings\u00b7schlacht", "ist", "al\u00b7ler\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein mutig M\u00e4nnerherz", "tokens": ["Wo", "ein", "mu\u00b7tig", "M\u00e4n\u00b7ner\u00b7herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "K\u00fchn sich stell des Lebens Schl\u00e4gen!", "tokens": ["K\u00fchn", "sich", "stell", "des", "Le\u00b7bens", "Schl\u00e4\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer da keinen Thron begehrt,", "tokens": ["Wer", "da", "kei\u00b7nen", "Thron", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat um ander Gut zu rechten:", "tokens": ["Hat", "um", "an\u00b7der", "Gut", "zu", "rech\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auch die mu\u00dft du erfechten!", "tokens": ["Und", "auch", "die", "mu\u00dft", "du", "er\u00b7fech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wider dich, weil froh du sangst,", "tokens": ["Wi\u00b7der", "dich", ",", "weil", "froh", "du", "sangst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Gebell von tausend Hunden!", "tokens": ["Das", "Ge\u00b7bell", "von", "tau\u00b7send", "Hun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wider dich die bl\u00f6de Angst", "tokens": ["Wi\u00b7der", "dich", "die", "bl\u00f6\u00b7de", "Angst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Dichter-Vagabunden!", "tokens": ["Vor", "dem", "Dich\u00b7ter\u00b7Va\u00b7ga\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wider dich und deinen Trutz", "tokens": ["Wi\u00b7der", "dich", "und", "dei\u00b7nen", "Trutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Waffen des Gemeinen:", "tokens": ["Al\u00b7le", "Waf\u00b7fen", "des", "Ge\u00b7mei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00e4lte, D\u00fcnkel, Eigennutz \u2013", "tokens": ["K\u00e4l\u00b7te", ",", "D\u00fcn\u00b7kel", ",", "Ei\u00b7gen\u00b7nutz", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles wider dich, den einen!", "tokens": ["Al\u00b7les", "wi\u00b7der", "dich", ",", "den", "ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "$,", "PRELS", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch du bist dir selbst ein Heer!", "tokens": ["Doch", "du", "bist", "dir", "selbst", "ein", "Heer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir voraus mit hellem Taillefer,", "tokens": ["Dir", "vo\u00b7raus", "mit", "hel\u00b7lem", "Tail\u00b7le\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mut und Freude dir zu bringen!", "tokens": ["Mut", "und", "Freu\u00b7de", "dir", "zu", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann der Wille, dann der Flei\u00df,", "tokens": ["Dann", "der", "Wil\u00b7le", ",", "dann", "der", "Flei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann, die alles kann, die Liebe \u2013", "tokens": ["Dann", ",", "die", "al\u00b7les", "kann", ",", "die", "Lie\u00b7be", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PIS", "VMFIN", "$,", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Schlacht so grimm und hei\u00df,", "tokens": ["Kei\u00b7ne", "Schlacht", "so", "grimm", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "W\u00e4rst du einzeln, ernster Mann,", "tokens": ["W\u00e4rst", "du", "ein\u00b7zeln", ",", "erns\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt' ich dir: Bleib auf der Welle!", "tokens": ["Sagt'", "ich", "dir", ":", "Bleib", "auf", "der", "Wel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Meide Liliput fortan,", "tokens": ["Mei\u00b7de", "Li\u00b7li\u00b7put", "for\u00b7tan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei des Elements Geselle!", "tokens": ["Sei", "des", "E\u00b7le\u00b7ments", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eintagsunruh', Eintagsstreit,", "tokens": ["Ein\u00b7tags\u00b7un\u00b7ruh'", ",", "Ein\u00b7tags\u00b7streit", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Woll' auf meinen Grund sie tauchen!", "tokens": ["Woll'", "auf", "mei\u00b7nen", "Grund", "sie", "tau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Odem der Unendlichkeit,", "tokens": ["O\u00b7dem", "der", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df mich in die Brust dir hauchen!", "tokens": ["La\u00df", "mich", "in", "die", "Brust", "dir", "hau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber nicht bei Mast und Tau,", "tokens": ["A\u00b7ber", "nicht", "bei", "Mast", "und", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht auf Planken, sturmdurchn\u00e4\u00dften \u2013", "tokens": ["Nicht", "auf", "Plan\u00b7ken", ",", "sturm\u00b7durc\u00b7hn\u00e4\u00df\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zarte Kinder, m\u00fcde Frau", "tokens": ["Zar\u00b7te", "Kin\u00b7der", ",", "m\u00fc\u00b7de", "Frau"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen wandeln auf dem Festen!", "tokens": ["Wol\u00b7len", "wan\u00b7deln", "auf", "dem", "Fes\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum, wo die Ernte wallt,", "tokens": ["Da\u00b7rum", ",", "wo", "die", "Ern\u00b7te", "wallt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Willst du s\u00e4'n und willst du pflanzen;", "tokens": ["Willst", "du", "s\u00e4'n", "und", "willst", "du", "pflan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo der L\u00e4rm der St\u00e4dte schallt,", "tokens": ["Wo", "der", "L\u00e4rm", "der", "St\u00e4d\u00b7te", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit im Gliede willst du schanzen:", "tokens": ["Mit", "im", "Glie\u00b7de", "willst", "du", "schan\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auch ein Mann, der Steine bricht:", "tokens": ["Auch", "ein", "Mann", ",", "der", "Stei\u00b7ne", "bricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ein Mann in Eisenh\u00fctten! \u2013", "tokens": ["Auch", "ein", "Mann", "in", "Ei\u00b7sen\u00b7h\u00fct\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasse nur den Alltag nicht", "tokens": ["Las\u00b7se", "nur", "den", "All\u00b7tag", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Dichtung dir versch\u00fctten!", "tokens": ["Dei\u00b7ne", "Dich\u00b7tung", "dir", "ver\u00b7sch\u00fct\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei, der zwiefach reisig steht", "tokens": ["Sei", ",", "der", "zwie\u00b7fach", "rei\u00b7sig", "steht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PRELS", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der frisch erk\u00e4mpften Grenze:", "tokens": ["Auf", "der", "frisch", "er\u00b7k\u00e4mpf\u00b7ten", "Gren\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tagel\u00f6hner und Poet,", "tokens": ["Ta\u00b7ge\u00b7l\u00f6h\u00b7ner", "und", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine beider W\u00fcrden Kr\u00e4nze!", "tokens": ["Ei\u00b7ne", "bei\u00b7der", "W\u00fcr\u00b7den", "Kr\u00e4n\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sieh, da liegt die K\u00fcste schon!\u00ab \u2013", "tokens": ["Sieh", ",", "da", "liegt", "die", "K\u00fcs\u00b7te", "schon", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, da lag sie! Nah zum Greifen,", "tokens": ["Ja", ",", "da", "lag", "sie", "!", "Nah", "zum", "Grei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "$.", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotzig hob sich Albion", "tokens": ["Trot\u00b7zig", "hob", "sich", "Al\u00b7bi\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Flut, ein wei\u00dfer Streifen.", "tokens": ["Aus", "der", "Flut", ",", "ein", "wei\u00b7\u00dfer", "Strei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles still und morgengrau!", "tokens": ["Al\u00b7les", "still", "und", "mor\u00b7gen\u00b7grau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Felsenripp' um Felsenrippe", "tokens": ["Fel\u00b7sen\u00b7ripp'", "um", "Fel\u00b7sen\u00b7rip\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Flog vorbei zu fl\u00fccht'ger Schau:", "tokens": ["Flog", "vor\u00b7bei", "zu", "fl\u00fccht'\u00b7ger", "Schau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Dover-Schlo\u00df und Shakespeares Klippe!", "tokens": ["Do\u00b7ver\u00b7Schlo\u00df", "und", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7res", "Klip\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Hier und da ein Fischerboot!", "tokens": ["Hier", "und", "da", "ein", "Fi\u00b7scher\u00b7boot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"]