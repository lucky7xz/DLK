{"dta.poem.1139": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "25. Die Sch\u00f6ne Zahnl\u00fcckigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Mund ist mehr gef\u00fcllt mit L\u00fccken als mit Z\u00e4hnen: ", "tokens": ["Dein", "Mund", "ist", "mehr", "ge\u00b7f\u00fcllt", "mit", "L\u00fc\u00b7cken", "als", "mit", "Z\u00e4h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gatter fehlet dir von weissen Helffenbein/", "tokens": ["Das", "Gat\u00b7ter", "feh\u00b7let", "dir", "von", "weis\u00b7sen", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So das Corallne Thor der Lippen schr\u00e4ncket ein.", "tokens": ["So", "das", "Co\u00b7rall\u00b7ne", "Thor", "der", "Lip\u00b7pen", "schr\u00e4n\u00b7cket", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch aber hast du nicht den Mangel zu bethr\u00e4nen.", "tokens": ["Doch", "a\u00b7ber", "hast", "du", "nicht", "den", "Man\u00b7gel", "zu", "be\u00b7th\u00b7r\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Hier darff kein s\u00fcsses Wort nach seiner Freyheit g\u00e4hnen/", "tokens": ["Hier", "darff", "kein", "s\u00fcs\u00b7ses", "Wort", "nach", "sei\u00b7ner", "Frey\u00b7heit", "g\u00e4h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Athems Bisam-Lufft darff nicht gefangen seyn.", "tokens": ["Des", "A\u00b7thems", "Bi\u00b7sam\u00b7Lufft", "darff", "nicht", "ge\u00b7fan\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In einer Purpur-See ohn Klippe/ Banck und Stein/", "tokens": ["In", "ei\u00b7ner", "Pur\u00b7pur\u00b7See", "ohn", "Klip\u00b7pe", "/", "Banck", "und", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan ihm der Zunge Schiff stets neue Wege b\u00e4hnen.", "tokens": ["Kan", "ihm", "der", "Zun\u00b7ge", "Schiff", "stets", "neu\u00b7e", "We\u00b7ge", "b\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Perlen-Mutter ist darum nicht zu verschm\u00e4hen/", "tokens": ["Die", "Per\u00b7len\u00b7Mut\u00b7ter", "ist", "da\u00b7rum", "nicht", "zu", "ver\u00b7schm\u00e4\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df man ihr zartes Kind aus ihrer Scho\u00df entri\u00df.", "tokens": ["Da\u00df", "man", "ihr", "zar\u00b7tes", "Kind", "aus", "ih\u00b7rer", "Scho\u00df", "ent\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer kan darvor/ was durch Gewalt und Zeit geschehen?", "tokens": ["Wer", "kan", "dar\u00b7vor", "/", "was", "durch", "Ge\u00b7walt", "und", "Zeit", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PAV", "$(", "PWS", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du unbewehrter Mund/ ich liebe dich gewi\u00df.", "tokens": ["Du", "un\u00b7be\u00b7wehr\u00b7ter", "Mund", "/", "ich", "lie\u00b7be", "dich", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Entz\u00e4hnte Schlangen wird man minder zornig sehen.", "tokens": ["Ent\u00b7z\u00e4hn\u00b7te", "Schlan\u00b7gen", "wird", "man", "min\u00b7der", "zor\u00b7nig", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Stehl ich dir einen Ku\u00df/ so f\u00fcrcht ich keinen Bi\u00df.", "tokens": ["Stehl", "ich", "dir", "ei\u00b7nen", "Ku\u00df", "/", "so", "f\u00fcrcht", "ich", "kei\u00b7nen", "Bi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}