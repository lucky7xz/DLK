{"textgrid.poem.40956": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck im Ungl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trifft ein Leid ein Herz voll Kummer,", "tokens": ["Trifft", "ein", "Leid", "ein", "Herz", "voll", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird das minder aufgeschreckt,", "tokens": ["Wird", "das", "min\u00b7der", "auf\u00b7ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wenn Leid aus seinem Schlummer", "tokens": ["Als", "wenn", "Leid", "aus", "sei\u00b7nem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Herz, das in Lust ist, weckt.", "tokens": ["Ein", "Herz", ",", "das", "in", "Lust", "ist", ",", "weckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da im Leben mich verlassen", "tokens": ["Da", "im", "Le\u00b7ben", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmerz kaum einen Augenblick,", "tokens": ["Schmerz", "kaum", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann ich mich, kommt neuer, fassen, \u2013", "tokens": ["Kann", "ich", "mich", ",", "kommt", "neu\u00b7er", ",", "fas\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$,", "VVFIN", "ADJA", "$,", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So ist Ungl\u00fcck oft ein Gl\u00fcck.", "tokens": ["So", "ist", "Un\u00b7gl\u00fcck", "oft", "ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}