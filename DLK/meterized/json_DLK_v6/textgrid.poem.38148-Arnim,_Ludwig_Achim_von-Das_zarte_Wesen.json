{"textgrid.poem.38148": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das zarte Wesen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Backnang wohnt ein Schneiderlein,", "tokens": ["Zu", "Back\u00b7nang", "wohnt", "ein", "Schnei\u00b7derl\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat ein einzigs Geiselein,", "tokens": ["Es", "hat", "ein", "ein\u00b7zigs", "Gei\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bracht ihm Gras, er bracht im Kraut,", "tokens": ["Er", "bracht", "ihm", "Gras", ",", "er", "bracht", "im", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das best', das er im Garten baut.", "tokens": ["Das", "best'", ",", "das", "er", "im", "Gar\u00b7ten", "baut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ward das zarte Wesen krank,", "tokens": ["Da", "ward", "das", "zar\u00b7te", "We\u00b7sen", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schneider war in grossem Leid,", "tokens": ["Der", "Schnei\u00b7der", "war", "in", "gros\u00b7sem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als sie den Tod mu\u00dft leiden:", "tokens": ["Als", "sie", "den", "Tod", "mu\u00dft", "lei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bbmein edle Gei\u00df, die H\u00e4ddel hei\u00dft,", "tokens": ["\u00bb", "mein", "ed\u00b7le", "Gei\u00df", ",", "die", "H\u00e4d\u00b7del", "hei\u00dft", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hat manches Kraut gefressen.", "tokens": ["Hat", "man\u00b7ches", "Kraut", "ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Jezt mu\u00df ich gar vor Herzeleid", "tokens": ["Jezt", "mu\u00df", "ich", "gar", "vor", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mein s\u00fc\u00dfe Gei\u00df vergessen!\u00ab", "tokens": ["Mein", "s\u00fc\u00b7\u00dfe", "Gei\u00df", "ver\u00b7ges\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Stadtknecht gieng am Zaune nah,", "tokens": ["Der", "Stadt\u00b7knecht", "gieng", "am", "Zau\u00b7ne", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sobald, als er die Gei\u00df ersah:", "tokens": ["So\u00b7bald", ",", "als", "er", "die", "Gei\u00df", "er\u00b7sah", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbpotz Kreutz! was seh ich liegen!", "tokens": ["\u00bb", "potz", "Kreutz", "!", "was", "seh", "ich", "lie\u00b7gen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$.", "PWS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das w\u00e4r' jezt eine gute Sach,", "tokens": ["Das", "w\u00e4r'", "jezt", "ei\u00b7ne", "gu\u00b7te", "Sach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn es nur blieb verschwiegen.\u00ab", "tokens": ["Wenn", "es", "nur", "blieb", "ver\u00b7schwie\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Stadtknecht zeigts dem Metzger an:", "tokens": ["Der", "Stadt\u00b7knecht", "zeigts", "dem", "Metz\u00b7ger", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbei guten Abend Metzger du,", "tokens": ["\u00bb", "ei", "gu\u00b7ten", "A\u00b7bend", "Metz\u00b7ger", "du", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "ADJA", "NN", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beim Bettelhaus, da liegt ein Rehbock,", "tokens": ["Beim", "Bet\u00b7tel\u00b7haus", ",", "da", "liegt", "ein", "Reh\u00b7bock", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Haut ist abgezogen.", "tokens": ["Die", "Haut", "ist", "ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Das w\u00e4r ein gute Sach f\u00fcr uns,", "tokens": ["Das", "w\u00e4r", "ein", "gu\u00b7te", "Sach", "f\u00fcr", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn es nur bleibt verschwiegen.\u00ab", "tokens": ["Wenn", "es", "nur", "bleibt", "ver\u00b7schwie\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Metzger in die Metzel kam,", "tokens": ["Der", "Metz\u00b7ger", "in", "die", "Met\u00b7zel", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein G\u00fcrtel und Messer mit sich nahm,", "tokens": ["Sein", "G\u00fcr\u00b7tel", "und", "Mes\u00b7ser", "mit", "sich", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein weissen Schurz darneben.", "tokens": ["Ein", "weis\u00b7sen", "Schurz", "dar\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Pfarrerin mit dem Gelenk heim gieng,", "tokens": ["Die", "Pfar\u00b7re\u00b7rin", "mit", "dem", "Ge\u00b7lenk", "heim", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die V\u00f6gtin macht ein Braten,", "tokens": ["Die", "V\u00f6g\u00b7tin", "macht", "ein", "Bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es habens kauft mehr als zehn Frau'n,", "tokens": ["Es", "ha\u00b7bens", "kauft", "mehr", "als", "zehn", "Frau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "PIS", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist reissend abgegangen.", "tokens": ["Ist", "reis\u00b7send", "ab\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Backnanger Herrn sind zusammen gesessen,", "tokens": ["Die", "Back\u00b7nan\u00b7ger", "Herrn", "sind", "zu\u00b7sam\u00b7men", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Das zarte Wesen als einen Rehbock gegessen,", "tokens": ["Das", "zar\u00b7te", "We\u00b7sen", "als", "ei\u00b7nen", "Reh\u00b7bock", "ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ein Guckuck f\u00fcr eine Taube,", "tokens": ["Ein", "Gu\u00b7ckuck", "f\u00fcr", "ei\u00b7ne", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und blaue Schleen f\u00fcr Trauben.", "tokens": ["Und", "blau\u00b7e", "Schleen", "f\u00fcr", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Backnanger Liedlein lautet nit wohl,", "tokens": ["Das", "Back\u00b7nan\u00b7ger", "Lied\u00b7lein", "lau\u00b7tet", "nit", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Man schl\u00e4gt einem gleich den Buckel voll,", "tokens": ["Man", "schl\u00e4gt", "ei\u00b7nem", "gleich", "den", "Bu\u00b7ckel", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie konnten das zarte Wesen nit verdauen.", "tokens": ["Sie", "konn\u00b7ten", "das", "zar\u00b7te", "We\u00b7sen", "nit", "ver\u00b7dau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}