{"dta.poem.20288": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wohlmeynende gedancken \u00fcber den  \n geburts-tag der Ble\u00dfine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ble\u00dfine la\u00df mich doch in diesem brieffe schertzen/", "tokens": ["Ble\u00b7\u00dfi\u00b7ne", "la\u00df", "mich", "doch", "in", "die\u00b7sem", "brief\u00b7fe", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es scheint/ da\u00df heute mir der himmel selber lacht;", "tokens": ["Es", "scheint", "/", "da\u00df", "heu\u00b7te", "mir", "der", "him\u00b7mel", "sel\u00b7ber", "lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ADV", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es qvillt/ ich wei\u00df nicht was/ aus meinem engen hertzen/", "tokens": ["Es", "qvillt", "/", "ich", "wei\u00df", "nicht", "was", "/", "aus", "mei\u00b7nem", "en\u00b7gen", "hert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das alle schmertzen mir zu s\u00fcssen zucker macht.", "tokens": ["Das", "al\u00b7le", "schmert\u00b7zen", "mir", "zu", "s\u00fcs\u00b7sen", "zu\u00b7cker", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Venus will mir selbst die dicke dinte r\u00fchren/", "tokens": ["Die", "Ve\u00b7nus", "will", "mir", "selbst", "die", "di\u00b7cke", "din\u00b7te", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Cupido tr\u00e4get mir die weissen bl\u00e4tter zu;", "tokens": ["Cu\u00b7pi\u00b7do", "tr\u00e4\u00b7get", "mir", "die", "weis\u00b7sen", "bl\u00e4t\u00b7ter", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jedoch was dieses mahl soll meine feder f\u00fchren/", "tokens": ["Je\u00b7doch", "was", "die\u00b7ses", "mahl", "soll", "mei\u00b7ne", "fe\u00b7der", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PDAT", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das kan nichts anders seyn/ als nur Ble\u00dfine du.", "tokens": ["Das", "kan", "nichts", "an\u00b7ders", "seyn", "/", "als", "nur", "Ble\u00b7\u00dfi\u00b7ne", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "VAINF", "$(", "KOKOM", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du weist am besten mir die geister zu erwecken/", "tokens": ["Du", "weist", "am", "bes\u00b7ten", "mir", "die", "geis\u00b7ter", "zu", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und legst in sand und ei\u00df bebl\u00fcmte g\u00e4rten an/", "tokens": ["Und", "legst", "in", "sand", "und", "ei\u00df", "be\u00b7bl\u00fcm\u00b7te", "g\u00e4r\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "KON", "ADJD", "VVFIN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du l\u00e4st mich nicht im schlamm der bleichen sorgen stecken/", "tokens": ["Du", "l\u00e4st", "mich", "nicht", "im", "schlamm", "der", "blei\u00b7chen", "sor\u00b7gen", "ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und machest/ da\u00df ich noch/ was lust ist/ schmecken kan.", "tokens": ["Und", "ma\u00b7chest", "/", "da\u00df", "ich", "noch", "/", "was", "lust", "ist", "/", "schme\u00b7cken", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADV", "$(", "PWS", "VVFIN", "VAFIN", "$(", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du kanst aus n\u00e4chten tag/ aus winter fr\u00fchling machen/", "tokens": ["Du", "kanst", "aus", "n\u00e4ch\u00b7ten", "tag", "/", "aus", "win\u00b7ter", "fr\u00fch\u00b7ling", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$(", "APPR", "NE", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus deinen augen qvillt der zeug zum hurtig seyn.", "tokens": ["Aus", "dei\u00b7nen", "au\u00b7gen", "qvillt", "der", "zeug", "zum", "hur\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du lehrst die traurigkeit und schwermuth selber lachen/", "tokens": ["Du", "lehrst", "die", "trau\u00b7rig\u00b7keit", "und", "schwer\u00b7muth", "sel\u00b7ber", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und lockst aus tr\u00fcber nacht den hellen sonnenschein.", "tokens": ["Und", "lockst", "aus", "tr\u00fc\u00b7ber", "nacht", "den", "hel\u00b7len", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die jahre deiner gunst sind ohne marter-wochen/", "tokens": ["Die", "jah\u00b7re", "dei\u00b7ner", "gunst", "sind", "oh\u00b7ne", "mar\u00b7ter\u00b7wo\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der schwartze sonntag wird durch dich zum oster-fest.", "tokens": ["Der", "schwart\u00b7ze", "sonn\u00b7tag", "wird", "durch", "dich", "zum", "os\u00b7ter\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es l\u00e4st dein paradei\u00df mich liebes-\u00e4pffel suchen/", "tokens": ["Es", "l\u00e4st", "dein", "pa\u00b7ra\u00b7dei\u00df", "mich", "lie\u00b7bes\u00b7\u00e4pf\u00b7fel", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Darbey die schlange sich nicht leichtlich sp\u00fcren l\u00e4st.", "tokens": ["Dar\u00b7bey", "die", "schlan\u00b7ge", "sich", "nicht", "leicht\u00b7lich", "sp\u00fc\u00b7ren", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "VVFIN", "PRF", "PTKNEG", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ble\u00dfine/ weist du auch/ warum ich dieses schreibe/", "tokens": ["Ble\u00b7\u00dfi\u00b7ne", "/", "weist", "du", "auch", "/", "wa\u00b7rum", "ich", "die\u00b7ses", "schrei\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "ADV", "$(", "PWAV", "PPER", "PDS", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Warum dir meine faust itzt hundert reime schickt?", "tokens": ["Wa\u00b7rum", "dir", "mei\u00b7ne", "faust", "itzt", "hun\u00b7dert", "rei\u00b7me", "schickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "CARD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du kennst den sch\u00f6nen mertz/ als aus der mutter leibe", "tokens": ["Du", "kennst", "den", "sch\u00f6\u00b7nen", "mertz", "/", "als", "aus", "der", "mut\u00b7ter", "lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "KOKOM", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vor siebzehn jahren du die welt hast angeblickt.", "tokens": ["Vor", "sieb\u00b7zehn", "jah\u00b7ren", "du", "die", "welt", "hast", "an\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da hat die Venus dich bald auff den arm geleget/", "tokens": ["Da", "hat", "die", "Ve\u00b7nus", "dich", "bald", "auff", "den", "arm", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und dich mit ihrer milch als mutter auch getr\u00e4nckt;", "tokens": ["Und", "dich", "mit", "ih\u00b7rer", "milch", "als", "mut\u00b7ter", "auch", "ge\u00b7tr\u00e4nckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "KOUS", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie hat die lieblichkeit dir reichlich eingepr\u00e4get/", "tokens": ["Sie", "hat", "die", "lieb\u00b7lich\u00b7keit", "dir", "reich\u00b7lich", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und selbst ihr ebenbild auff deine brust gehenckt.", "tokens": ["Und", "selbst", "ihr", "e\u00b7ben\u00b7bild", "auff", "dei\u00b7ne", "brust", "ge\u00b7henckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie hat mit rosen-blut die lippen dir bespr\u00fctzet/", "tokens": ["Sie", "hat", "mit", "ro\u00b7sen\u00b7blut", "die", "lip\u00b7pen", "dir", "be\u00b7spr\u00fct\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und ihre zunge hat die deinige genetzt;", "tokens": ["Und", "ih\u00b7re", "zun\u00b7ge", "hat", "die", "dei\u00b7ni\u00b7ge", "ge\u00b7netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "PPOSS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie hat dir alsobald das junge blut erhitzet/", "tokens": ["Sie", "hat", "dir", "al\u00b7so\u00b7bald", "das", "jun\u00b7ge", "blut", "er\u00b7hit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und warmen wunder-schnee in deine hand gesetzt.", "tokens": ["Und", "war\u00b7men", "wun\u00b7der\u00b7schnee", "in", "dei\u00b7ne", "hand", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nach diesem hat sie dich den Gratien befohlen/", "tokens": ["Nach", "die\u00b7sem", "hat", "sie", "dich", "den", "Gra\u00b7ti\u00b7en", "be\u00b7foh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "PRF", "ART", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die eine k\u00fc\u00dfte dich/ du weist es wohl auff was;", "tokens": ["Die", "ei\u00b7ne", "k\u00fc\u00df\u00b7te", "dich", "/", "du", "weist", "es", "wohl", "auff", "was", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Cupido muste dir zeug zu den windeln holen/", "tokens": ["Cu\u00b7pi\u00b7do", "mus\u00b7te", "dir", "zeug", "zu", "den", "win\u00b7deln", "ho\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Der niemahls allzuweit von deiner wiege sa\u00df/", "tokens": ["Der", "nie\u00b7mahls", "all\u00b7zu\u00b7weit", "von", "dei\u00b7ner", "wie\u00b7ge", "sa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er sang dir: kindgen schlaff; dein mund ist wie rubinen/", "tokens": ["Er", "sang", "dir", ":", "kind\u00b7gen", "schlaff", ";", "dein", "mund", "ist", "wie", "ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "KOKOM", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dein b\u00e4uchlein schwanen-wei\u00df/ dein hals wie helffenbein/", "tokens": ["Dein", "b\u00e4uch\u00b7lein", "schwa\u00b7nen\u00b7wei\u00df", "/", "dein", "hals", "wie", "helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$(", "PPOSAT", "NN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es wird die freyheit dir vor eine sclavin dienen/", "tokens": ["Es", "wird", "die", "frey\u00b7heit", "dir", "vor", "ei\u00b7ne", "scla\u00b7vin", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wann um dein br\u00fcnnlein wird ein sch\u00f6nes p\u00fcschgen seyn.", "tokens": ["Wann", "um", "dein", "br\u00fcnn\u00b7lein", "wird", "ein", "sch\u00f6\u00b7nes", "p\u00fcschgen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.41": {"text": "Schlaff sanfft! Es m\u00fcsse dich kein harter schall erwecken/", "tokens": ["Schlaff", "sanfft", "!", "Es", "m\u00fcs\u00b7se", "dich", "kein", "har\u00b7ter", "schall", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die mutter decket dich mit ihrem flore zu.", "tokens": ["Die", "mut\u00b7ter", "de\u00b7cket", "dich", "mit", "ih\u00b7rem", "flo\u00b7re", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "In deine lippen will sie zucker-stengel stecken/", "tokens": ["In", "dei\u00b7ne", "lip\u00b7pen", "will", "sie", "zu\u00b7cke\u00b7rs\u00b7ten\u00b7gel", "ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Die mehr als zucker sind/ und lieblich seyn/ wie du.", "tokens": ["Die", "mehr", "als", "zu\u00b7cker", "sind", "/", "und", "lieb\u00b7lich", "seyn", "/", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "ADJD", "VAFIN", "$(", "KON", "ADJD", "VAINF", "$(", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Er lehrte bald darauff die glatten f\u00fcsse schreiten/", "tokens": ["Er", "lehr\u00b7te", "bald", "dar\u00b7auff", "die", "glat\u