 "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Ich habe schon vorl\u00e4ngst mein hertze dir geschencket/", "tokens": ["Ich", "ha\u00b7be", "schon", "vor\u00b7l\u00e4ngst", "mein", "hert\u00b7ze", "dir", "ge\u00b7schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Dein mund zerreist es zwar/ zermalmt es aber nicht.", "tokens": ["Dein", "mund", "zer\u00b7reist", "es", "zwar", "/", "zer\u00b7malmt", "es", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$(", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Ach! freundin/ glaub es mir/ worauff dein geist gedencket/", "tokens": ["Ach", "!", "freun\u00b7din", "/", "glaub", "es", "mir", "/", "wo\u00b7rauff", "dein", "geist", "ge\u00b7den\u00b7cket", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "$(", "VVFIN", "PPER", "PPER", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Das hab ich allbereit als diener ausgericht.", "tokens": ["Das", "hab", "ich", "all\u00b7be\u00b7reit", "als", "die\u00b7ner", "aus\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, 