{"dta.poem.19512": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Hengert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Vater Lucas sprach beim Fr\u00fchst\u00fcck:", "tokens": ["Va\u00b7ter", "Lu\u00b7cas", "sprach", "beim", "Fr\u00fch\u00b7st\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201eheute, Herr, ist hier ein Hengert!\u201c", "tokens": ["\u201e", "heu\u00b7te", ",", "Herr", ",", "ist", "hier", "ein", "Hen\u00b7gert", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "VAFIN", "ADV", "ART", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich fragte: \u201eWas ist Hengert?\u201c", "tokens": ["Und", "ich", "frag\u00b7te", ":", "\u201e", "Was", "ist", "Hen\u00b7gert", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PWS", "VAFIN", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich belehrte Vater Lucas:", "tokens": ["Mich", "be\u00b7lehr\u00b7te", "Va\u00b7ter", "Lu\u00b7cas", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u201ehengert, Herr, bedeutet Reigen,", "tokens": ["\u201e", "hen\u00b7gert", ",", "Herr", ",", "be\u00b7deu\u00b7tet", "Rei\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ball und Sprung und Fu\u00dfgezappel", "tokens": ["Ball", "und", "Sprung", "und", "Fu\u00df\u00b7ge\u00b7zap\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Sprache der Grisonen", "tokens": ["In", "der", "Spra\u00b7che", "der", "Gri\u00b7so\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Ihr m\u00f6chtet b\u00f6se schlummern,", "tokens": ["Und", "Ihr", "m\u00f6ch\u00b7tet", "b\u00f6\u00b7se", "schlum\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sucht Ihr heut nicht still're Ruhstatt!\u201c", "tokens": ["Sucht", "Ihr", "heut", "nicht", "still'\u00b7re", "Ruh\u00b7statt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201evater Lucas, keine Sorge!", "tokens": ["\u201e", "va\u00b7ter", "Lu\u00b7cas", ",", "kei\u00b7ne", "Sor\u00b7ge", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich erst mich m\u00fcd' gewandert,", "tokens": ["Hab'", "ich", "erst", "mich", "m\u00fcd'", "ge\u00b7wan\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "VMFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlief' ich auch in einem Meersturm!\u201c", "tokens": ["Schlie\u00b7f'", "ich", "auch", "in", "ei\u00b7nem", "Meer\u00b7sturm", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Freudig nahm ich meinen Bergstock,", "tokens": ["Freu\u00b7dig", "nahm", "ich", "mei\u00b7nen", "Berg\u00b7stock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stieg hinan die saft'gen Weiden,", "tokens": ["Stieg", "hi\u00b7nan", "die", "saft'\u00b7gen", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wo sich tummeln braune Fohlen,", "tokens": ["Wo", "sich", "tum\u00b7meln", "brau\u00b7ne", "Foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch bewegliches Ger\u00f6lle", "tokens": ["Durch", "be\u00b7weg\u00b7li\u00b7ches", "Ge\u00b7r\u00f6l\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klomm ich auf zum sel'gen Gipfel,", "tokens": ["Klomm", "ich", "auf", "zum", "sel'\u00b7gen", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den mit leichtem Ku\u00df ber\u00fchren", "tokens": ["Den", "mit", "leich\u00b7tem", "Ku\u00df", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heimatlose Wanderwolken.", "tokens": ["Hei\u00b7mat\u00b7lo\u00b7se", "Wan\u00b7der\u00b7wol\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "M\u00fcde kehrt' ich heim ins Berghaus", "tokens": ["M\u00fc\u00b7de", "kehrt'", "ich", "heim", "ins", "Berg\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Zeit der ersten Lichter.", "tokens": ["Um", "die", "Zeit", "der", "ers\u00b7ten", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor der Pforte stand ein H\u00e4uflein,", "tokens": ["Vor", "der", "Pfor\u00b7te", "stand", "ein", "H\u00e4uf\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Mitte Musikanten,", "tokens": ["In", "der", "Mit\u00b7te", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rechts die Bursche, links die M\u00e4dchen,", "tokens": ["Rechts", "die", "Bur\u00b7sche", ",", "links", "die", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch kein Scherzwort flog her\u00fcber", "tokens": ["Doch", "kein", "Scherz\u00b7wort", "flog", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und hin\u00fcber flog kein Trutzwort.", "tokens": ["Und", "hin\u00b7\u00fc\u00b7ber", "flog", "kein", "Trutz\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4ssig mit gekreuzten Armen", "tokens": ["L\u00e4s\u00b7sig", "mit", "ge\u00b7kreuz\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Standen sie geschieden, feindlich", "tokens": ["Stan\u00b7den", "sie", "ge\u00b7schie\u00b7den", ",", "feind\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "VVPP", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sich mit dunkeln Blicken messend.", "tokens": ["Sich", "mit", "dun\u00b7keln", "Bli\u00b7cken", "mes\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ich stieg in meine Kammer,", "tokens": ["Und", "ich", "stieg", "in", "mei\u00b7ne", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Legte mich getrost zur Ruhe.", "tokens": ["Leg\u00b7te", "mich", "ge\u00b7trost", "zur", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald erklang Musik piano,", "tokens": ["Bald", "er\u00b7klang", "Mu\u00b7sik", "pi\u00b7a\u00b7no", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Allgemach begann der Hengert,", "tokens": ["All\u00b7ge\u00b7mach", "be\u00b7gann", "der", "Hen\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sachte schritt er, schl\u00e4frig schleift' er,", "tokens": ["Sach\u00b7te", "schritt", "er", ",", "schl\u00e4f\u00b7rig", "schleift'", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Geschlurfe von Pantoffeln.", "tokens": ["Wie", "Ge\u00b7schlur\u00b7fe", "von", "Pan\u00b7tof\u00b7feln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Heimlich spottet' ich der tr\u00e4gen", "tokens": ["Heim\u00b7lich", "spot\u00b7tet'", "ich", "der", "tr\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fc\u00dfe, der bequemen Herzen", "tokens": ["F\u00fc\u00b7\u00dfe", ",", "der", "be\u00b7que\u00b7men", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Im Gebirge der Grisonen", "tokens": ["Im", "Ge\u00b7bir\u00b7ge", "der", "Gri\u00b7so\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und versank in s\u00fc\u00dfen Schlummer ....", "tokens": ["Und", "ver\u00b7sank", "in", "s\u00fc\u00b7\u00dfen", "Schlum\u00b7mer", "...."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Horch! Ein Ton, ein feurig greller,", "tokens": ["Horch", "!", "Ein", "Ton", ",", "ein", "feu\u00b7rig", "grel\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "$,", "ART", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4gt empor wie eine Flamme!", "tokens": ["Schl\u00e4gt", "em\u00b7por", "wie", "ei\u00b7ne", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jach erhitzen sich die Bleche", "tokens": ["Jach", "er\u00b7hit\u00b7zen", "sich", "die", "Ble\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Geige streicht ein D\u00e4mon!", "tokens": ["Und", "die", "Gei\u00b7ge", "streicht", "ein", "D\u00e4\u00b7mon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir zur Rechten, mir zur Linken,", "tokens": ["Mir", "zur", "Rech\u00b7ten", ",", "mir", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir zu H\u00e4upten, mir zu F\u00fc\u00dfen,", "tokens": ["Mir", "zu", "H\u00e4up\u00b7ten", ",", "mir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ungez\u00fcgelt, ungeb\u00e4ndigt,", "tokens": ["Un\u00b7ge\u00b7z\u00fc\u00b7gelt", ",", "un\u00b7ge\u00b7b\u00e4n\u00b7digt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Erdersch\u00fctternd stampft der Reigen,", "tokens": ["Er\u00b7der\u00b7sch\u00fct\u00b7ternd", "stampft", "der", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Immer lauter, wilder, toller", "tokens": ["Im\u00b7mer", "lau\u00b7ter", ",", "wil\u00b7der", ",", "tol\u00b7ler"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tobt und rast und dr\u00f6hnt und tritt er,", "tokens": ["Tobt", "und", "rast", "und", "dr\u00f6hnt", "und", "tritt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df erbeben alle Balken,", "tokens": ["Da\u00df", "er\u00b7be\u00b7ben", "al\u00b7le", "Bal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Tosend sausten durch die L\u00fcfte", "tokens": ["To\u00b7send", "saus\u00b7ten", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Berghaus, Hengert, Folterkammer,", "tokens": ["Berg\u00b7haus", ",", "Hen\u00b7gert", ",", "Fol\u00b7ter\u00b7kam\u00b7mer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie voreinst die hochgelobte", "tokens": ["Wie", "vor\u00b7e\u00b7inst", "die", "hoch\u00b7ge\u00b7lob\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Casa santa durch die L\u00fcfte", "tokens": ["Ca\u00b7sa", "san\u00b7ta", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Fuhr von Istrien nach Loretto,", "tokens": ["Fuhr", "von", "I\u00b7stri\u00b7en", "nach", "Lo\u00b7ret\u00b7to", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "APPR", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Doch von Engeln sie getragen,", "tokens": ["Doch", "von", "En\u00b7geln", "sie", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ich von h\u00f6