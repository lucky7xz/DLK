{"textgrid.poem.41143": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Hades ist dies schwarze Blatt ein Bild,", "tokens": ["Vom", "Ha\u00b7des", "ist", "dies", "schwar\u00b7ze", "Blatt", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PDS", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier ist kein Sternenhimmel, kein Gefild,", "tokens": ["Hier", "ist", "kein", "Ster\u00b7nen\u00b7him\u00b7mel", ",", "kein", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Menschenlaut ist hier, kein Vogelsang,", "tokens": ["Kein", "Men\u00b7schen\u00b7laut", "ist", "hier", ",", "kein", "Vo\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier rauscht kein Bach ein gr\u00fcnes Tal entlang,", "tokens": ["Hier", "rauscht", "kein", "Bach", "ein", "gr\u00fc\u00b7nes", "Tal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier schweigt des Marktes l\u00e4rmender Verkehr,", "tokens": ["Hier", "schweigt", "des", "Mark\u00b7tes", "l\u00e4r\u00b7men\u00b7der", "Ver\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hier, wo nur Schatten schweben stumm umher.", "tokens": ["Hier", ",", "wo", "nur", "Schat\u00b7ten", "schwe\u00b7ben", "stumm", "um\u00b7her", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der eine wei\u00df vom andern hier kein Wort,", "tokens": ["Der", "ei\u00b7ne", "wei\u00df", "vom", "an\u00b7dern", "hier", "kein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "ADJA", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er meint, er sei allein an diesem Ort,", "tokens": ["Er", "meint", ",", "er", "sei", "al\u00b7lein", "an", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Am Orte, wo sie Schlimmes einst vollbracht,", "tokens": ["Am", "Or\u00b7te", ",", "wo", "sie", "Schlim\u00b7mes", "einst", "voll\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Hier schweben sie als Schatten durch die Nacht.", "tokens": ["Hier", "schwe\u00b7ben", "sie", "als", "Schat\u00b7ten", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ihr Schatten hier in schwarzer Einsamkeit", "tokens": ["Ihr", "Schat\u00b7ten", "hier", "in", "schwar\u00b7zer", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Macht euch zur Einkehr in euch selbst bereit!", "tokens": ["Macht", "euch", "zur", "Ein\u00b7kehr", "in", "euch", "selbst", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hier streift die Erdenschwere von euch ab,", "tokens": ["Hier", "streift", "die", "Er\u00b7den\u00b7schwe\u00b7re", "von", "euch", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die euch das vor'ge irre Leben gab,", "tokens": ["Die", "euch", "das", "vor'\u00b7ge", "ir\u00b7re", "Le\u00b7ben", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die also schwer die Seele euch umfing,", "tokens": ["Die", "al\u00b7so", "schwer", "die", "See\u00b7le", "euch", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df sie statt aufw\u00e4rts \u2013 weh, nach unten ging!", "tokens": ["Da\u00df", "sie", "statt", "auf\u00b7w\u00e4rts", "\u2013", "weh", ",", "nach", "un\u00b7ten", "ging", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "$(", "PTKVZ", "$,", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}