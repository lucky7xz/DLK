{"textgrid.poem.45549": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig von Preu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie reich begabt, wie f\u00e4hig war der Mann,", "tokens": ["Wie", "reich", "be\u00b7gabt", ",", "wie", "f\u00e4\u00b7hig", "war", "der", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt erkennts und auch zum Teil bewunderts,", "tokens": ["Die", "Welt", "er\u00b7kennts", "und", "auch", "zum", "Teil", "be\u00b7wun\u00b7derts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Fehler klebte leider nur ihm an:", "tokens": ["Ein", "Feh\u00b7ler", "kleb\u00b7te", "lei\u00b7der", "nur", "ihm", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er war ein Deutscher des neunzehnten Jahrhunderts.", "tokens": ["Er", "war", "ein", "Deut\u00b7scher", "des", "neun\u00b7zehn\u00b7ten", "Jahr\u00b7hun\u00b7derts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}