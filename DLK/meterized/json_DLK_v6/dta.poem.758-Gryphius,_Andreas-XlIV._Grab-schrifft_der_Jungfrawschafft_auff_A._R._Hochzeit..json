{"dta.poem.758": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.  Grab-schrifft/ der Jungfrawschafft/  \n auff  A. R.  Hochzeit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Das an sich selbst nichts ist/ vnd ander herlich macht: ", "tokens": ["Das", "an", "sich", "selbst", "nichts", "ist", "/", "vnd", "an\u00b7der", "her\u00b7lich", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRF", "ADV", "PIS", "VAFIN", "$(", "KON", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das man vor k\u00f6stlich sch\u00e4tzt/ vnd niemand doch be-", "tokens": ["Das", "man", "vor", "k\u00f6st\u00b7lich", "sch\u00e4tzt", "/", "vnd", "nie\u00b7mand", "doch", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "APPR", "ADJD", "VVFIN", "$(", "KON", "PIS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schawet;", "tokens": ["scha\u00b7wet", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Das baldt verlohren wirdt/ in dem es wirdt vertrawet", "tokens": ["Das", "baldt", "ver\u00b7loh\u00b7ren", "wirdt", "/", "in", "dem", "es", "wirdt", "ver\u00b7tra\u00b7wet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVPP", "VAFIN", "$(", "APPR", "PRELS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das/ wenn es einmal hin nicht mehr wirdt wiederbracht:", "tokens": ["Das", "/", "wenn", "es", "ein\u00b7mal", "hin", "nicht", "mehr", "wirdt", "wie\u00b7der\u00b7bracht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das/ wer es krigt nicht hat/ vnd doch f\u00fcr trefflich acht.", "tokens": ["Das", "/", "wer", "es", "krigt", "nicht", "hat", "/", "vnd", "doch", "f\u00fcr", "treff\u00b7lich", "acht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "VVFIN", "PTKNEG", "VAFIN", "$(", "KON", "ADV", "APPR", "ADJD", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das h\u00e4user wenn es f\u00e4lt vnd gantze L\u00e4nder bawet. ", "tokens": ["Das", "h\u00e4u\u00b7ser", "wenn", "es", "f\u00e4lt", "vnd", "gant\u00b7ze", "L\u00e4n\u00b7der", "ba\u00b7wet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "PPER", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Ob gleich f\u00fcr dessen fall den zarten ", "tokens": ["Ob", "gleich", "f\u00fcr", "des\u00b7sen", "fall", "den", "zar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PRELAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ohne schwerdt/ im kampf erlegt vergangne nacht.", "tokens": ["Ist", "oh\u00b7ne", "schwerdt", "/", "im", "kampf", "er\u00b7legt", "ver\u00b7gang\u00b7ne", "nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJD", "$(", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das keinen leib nicht hat/ vnd doch durch grimme wunden", "tokens": ["Das", "kei\u00b7nen", "leib", "nicht", "hat", "/", "vnd", "doch", "durch", "grim\u00b7me", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VAFIN", "$(", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm schweis vnd blut verschmacht: wirdt nun es ist gefunden", "tokens": ["Jm", "schweis", "vnd", "blut", "ver\u00b7schmacht", ":", "wirdt", "nun", "es", "ist", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "KON", "NN", "VVPP", "$.", "VAFIN", "ADV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit threnen/ voll von lust ins sanffte grab versetzt/", "tokens": ["Mit", "thre\u00b7nen", "/", "voll", "von", "lust", "ins", "sanff\u00b7te", "grab", "ver\u00b7setzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADJD", "APPR", "NN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aus dem es wieder bl\u00fcht. ", "tokens": ["Aus", "dem", "es", "wie\u00b7der", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die wolten es wol auch nach gleicher art vergraben ", "tokens": ["Die", "wol\u00b7ten", "es", "wol", "auch", "nach", "glei\u00b7cher", "art", "ver\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob wol sein vntergang (sagt ob es wahr) verletzt.", "tokens": ["Ob", "wol", "sein", "vn\u00b7ter\u00b7gang", "(", "sagt", "ob", "es", "wahr", ")", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$(", "VVFIN", "KOUS", "PPER", "ADJD", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}