{"dta.poem.3040": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In allen Zonen liegt die Menschheit auf den Knien", "tokens": ["In", "al\u00b7len", "Zo\u00b7nen", "liegt", "die", "Menschheit", "auf", "den", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor einem G\u00f6ttlichen, das sie empor soll ziehn.", "tokens": ["Vor", "ei\u00b7nem", "G\u00f6tt\u00b7li\u00b7chen", ",", "das", "sie", "em\u00b7por", "soll", "ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verachte keinen Brauch und keine Flehgeberde,", "tokens": ["Ver\u00b7ach\u00b7te", "kei\u00b7nen", "Brauch", "und", "kei\u00b7ne", "Fleh\u00b7ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit ein armes Herz emporringt von der Erde.", "tokens": ["Wo\u00b7mit", "ein", "ar\u00b7mes", "Herz", "em\u00b7por\u00b7ringt", "von", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Ein Kind mit 