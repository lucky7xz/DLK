{"textgrid.poem.58490": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Such dies und das, such's hier, such's dar,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Such dies und das, such's hier, such's dar,", "tokens": ["Such", "dies", "und", "das", ",", "such's", "hier", ",", "such's", "dar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PDS", "KON", "PDS", "$,", "NE", "ADV", "$,", "NE", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du bleibst im Jammer immerdar,", "tokens": ["Du", "bleibst", "im", "Jam\u00b7mer", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur eines kann dich stillen!", "tokens": ["Nur", "ei\u00b7nes", "kann", "dich", "stil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dieses Eins ist gar nicht fern,", "tokens": ["Und", "die\u00b7ses", "Eins", "ist", "gar", "nicht", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In dir ist's nah, Gott selbst will gern", "tokens": ["In", "dir", "ist's", "nah", ",", "Gott", "selbst", "will", "gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "$,", "NN", "ADV", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich sammeln und erf\u00fcllen.", "tokens": ["Dich", "sam\u00b7meln", "und", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}