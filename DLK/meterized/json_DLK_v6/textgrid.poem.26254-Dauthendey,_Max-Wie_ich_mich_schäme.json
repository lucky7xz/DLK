{"textgrid.poem.26254": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wie ich mich sch\u00e4me", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich mich sch\u00e4me, wenn ich mich labe,", "tokens": ["Wie", "ich", "mich", "sch\u00e4\u00b7me", ",", "wenn", "ich", "mich", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da\u00df ich t\u00e4glich mein Essen habe.", "tokens": ["Da\u00df", "ich", "t\u00e4g\u00b7lich", "mein", "Es\u00b7sen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wie ich mich sch\u00e4me des Bettes, der Kissen,", "tokens": ["Wie", "ich", "mich", "sch\u00e4\u00b7me", "des", "Bet\u00b7tes", ",", "der", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und meine Br\u00fcder im Schneefeld zu wissen,", "tokens": ["Und", "mei\u00b7ne", "Br\u00fc\u00b7der", "im", "Schnee\u00b7feld", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die da im Laufgraben brechen ihr Brot,", "tokens": ["Die", "da", "im", "Lauf\u00b7gra\u00b7ben", "bre\u00b7chen", "ihr", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zur Seite den k\u00e4ltesten Freund \u2013 den Tod.", "tokens": ["Zur", "Sei\u00b7te", "den", "k\u00e4l\u00b7tes\u00b7ten", "Freund", "\u2013", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$(", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wie ich mich sch\u00e4me der tatlosen H\u00e4nde", "tokens": ["Wie", "ich", "mich", "sch\u00e4\u00b7me", "der", "tat\u00b7lo\u00b7sen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ich nur falten kann t\u00e4glich ohn' Ende,", "tokens": ["Die", "ich", "nur", "fal\u00b7ten", "kann", "t\u00e4g\u00b7lich", "ohn'", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Himmel droben um Segen zu flehen,", "tokens": ["Den", "Him\u00b7mel", "dro\u00b7ben", "um", "Se\u00b7gen", "zu", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Segen f\u00fcr sie, die im Feuer heut stehen,", "tokens": ["Se\u00b7gen", "f\u00fcr", "sie", ",", "die", "im", "Feu\u00b7er", "heut", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PRELS", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sch\u00e4me mich meiner Atemlust,", "tokens": ["Sch\u00e4\u00b7me", "mich", "mei\u00b7ner", "A\u00b7tem\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sch\u00e4m' mich im Schlafe noch unbewu\u00dft.", "tokens": ["Sch\u00e4m'", "mich", "im", "Schla\u00b7fe", "noch", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Sch\u00e4me mich blind vor den Sonnentagen,", "tokens": ["Sch\u00e4\u00b7me", "mich", "blind", "vor", "den", "Son\u00b7nen\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die da gl\u00e4nzen, nicht Kummer tragen,", "tokens": ["Die", "da", "gl\u00e4n\u00b7zen", ",", "nicht", "Kum\u00b7mer", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "PTKNEG", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sehn' mich nach Nebel, nach grauem Regen,", "tokens": ["Sehn'", "mich", "nach", "Ne\u00b7bel", ",", "nach", "grau\u00b7em", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Darin die Tage sich trauernd bewegen.", "tokens": ["Da\u00b7rin", "die", "Ta\u00b7ge", "sich", "trau\u00b7ernd", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sch\u00e4me mich st\u00fcndlich und trage Gram,", "tokens": ["Sch\u00e4\u00b7me", "mich", "st\u00fcnd\u00b7lich", "und", "tra\u00b7ge", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Bald erstick ich an dieser Scham.", "tokens": ["Bald", "er\u00b7stick", "ich", "an", "die\u00b7ser", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}