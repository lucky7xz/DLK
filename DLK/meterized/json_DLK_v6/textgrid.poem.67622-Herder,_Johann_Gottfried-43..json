{"textgrid.poem.67622": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Esel dient' beim G\u00e4rtner.", "tokens": ["Ein", "E\u00b7sel", "dient'", "beim", "G\u00e4rt\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da mu\u00dft' er", "tokens": ["Da", "mu\u00dft'", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mit Kraut zum Markt, das ward ihm schwer;", "tokens": ["Mit", "Kraut", "zum", "Markt", ",", "das", "ward", "ihm", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er zog zum T\u00f6pfer.", "tokens": ["Er", "zog", "zum", "T\u00f6p\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Esel dient' beim T\u00f6pfer.", "tokens": ["Der", "E\u00b7sel", "dient'", "beim", "T\u00f6p\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier schleppt' er", "tokens": ["Hier", "schleppt'", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Gar Winter durch und Sommer", "tokens": ["Gar", "Win\u00b7ter", "durch", "und", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch schwerer;", "tokens": ["Noch", "schwe\u00b7rer", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Und weh! zerbrach er", "tokens": ["Und", "weh", "!", "zer\u00b7brach", "er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PTKVZ", "$.", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Ein T\u00f6pfchen \u2013 armer Esel,", "tokens": ["Ein", "T\u00f6pf\u00b7chen", "\u2013", "ar\u00b7mer", "E\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Kriegst Pr\u00fcgel.", "tokens": ["Kriegst", "Pr\u00fc\u00b7gel", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Der Esel zog zum Gerber.", "tokens": ["Der", "E\u00b7sel", "zog", "zum", "Ger\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "\u00bbweh mir! hier schlepp' ich leider", "tokens": ["\u00bb", "weh", "mir", "!", "hier", "schlepp'", "ich", "lei\u00b7der"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Schon Leder meiner Br\u00fcder!", "tokens": ["Schon", "Le\u00b7der", "mei\u00b7ner", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Komm' nimmer los nun, nimmer.", "tokens": ["Komm'", "nim\u00b7mer", "los", "nun", ",", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ich seh', er schielt schon immer", "tokens": ["Ich", "seh'", ",", "er", "schielt", "schon", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Nach meinem Leder.\u00ab", "tokens": ["Nach", "mei\u00b7nem", "Le\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nur mit Beding, da\u00df Er mir nie", "tokens": ["Nur", "mit", "Be\u00b7ding", ",", "da\u00df", "Er", "mir", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meinem Lande zieh';", "tokens": ["Aus", "mei\u00b7nem", "Lan\u00b7de", "zieh'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Fell steckt mit im Lohn.", "tokens": ["Sein", "Fell", "steckt", "mit", "im", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}