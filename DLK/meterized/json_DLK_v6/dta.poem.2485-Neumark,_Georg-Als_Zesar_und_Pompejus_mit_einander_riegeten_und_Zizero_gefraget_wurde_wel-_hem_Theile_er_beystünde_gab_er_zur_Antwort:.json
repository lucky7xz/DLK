{"dta.poem.2485": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als Zesar und Pompejus mit einander  \n riegeten/ und Zizero gefraget wurde/ wel-  \n hem Theile er beyst\u00fcnde/ gab er zur Antwort:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Es bleiben Sonn und Mond an ihren Himmelsstellen/", "tokens": ["Es", "blei\u00b7ben", "Sonn", "und", "Mond", "an", "ih\u00b7ren", "Him\u00b7melss\u00b7tel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gantz unbeweglich stehn/ wie sehr die Hund\u2019 auch belle\u0303;", "tokens": ["Gantz", "un\u00b7be\u00b7weg\u00b7lich", "stehn", "/", "wie", "sehr", "die", "Hund'", "auch", "bell\u1ebd", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$(", "KOKOM", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der blauvergiffte Neid der tob\u2019 auch wie er wil/", "tokens": ["Der", "blau\u00b7ver\u00b7giff\u00b7te", "Neid", "der", "tob'", "auch", "wie", "er", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "KOKOM", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein \u00e4dler Geist der lacht und schweigt zur Thorheit still.", "tokens": ["Ein", "\u00e4d\u00b7ler", "Geist", "der", "lacht", "und", "schweigt", "zur", "Thor\u00b7hei