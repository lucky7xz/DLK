{"textgrid.poem.39983": {"metadata": {"author": {"name": "Laurentius von Schn\u00fcffis", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es hat die G\u00f6ldne Sonn-", "genre": "verse", "period": "N.A.", "pub_year": 1667, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat die G\u00f6ldne Sonn-", "tokens": ["Es", "hat", "die", "G\u00f6ld\u00b7ne", "Sonn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu Gnaden sich geneiget/", "tokens": ["Zu", "Gna\u00b7den", "sich", "ge\u00b7nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihr bestrahltes Haupt-", "tokens": ["Und", "ihr", "be\u00b7strahl\u00b7tes", "Haup\u00b7t"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versencket in das Meer/", "tokens": ["Ver\u00b7sen\u00b7cket", "in", "das", "Meer", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der m\u00fcde Tag nunmehr-", "tokens": ["Der", "m\u00fc\u00b7de", "Tag", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den braunen Abend zeiget/", "tokens": ["Den", "brau\u00b7nen", "A\u00b7bend", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Himmel wird entf\u00e4rbt/-", "tokens": ["Der", "Him\u00b7mel", "wird", "ent\u00b7f\u00e4rbt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Welt verduncklet sehr/", "tokens": ["Die", "Welt", "ver\u00b7dunck\u00b7let", "sehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dahero will ich mich-", "tokens": ["Da\u00b7he\u00b7ro", "will", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nach Hause nun verf\u00fcgen/", "tokens": ["Nach", "Hau\u00b7se", "nun", "ver\u00b7f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bi\u00df ", "tokens": ["Bi\u00df"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Mit frischem Tag erscheint/", "tokens": ["Mit", "fri\u00b7schem", "Tag", "er\u00b7scheint", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Geliebte Leser/ la\u00dft-", "tokens": ["Ge\u00b7lieb\u00b7te", "Le\u00b7ser", "/", "la\u00dft"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "NN", "$(", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Mit diesem euch begn\u00fcgen/", "tokens": ["Mit", "die\u00b7sem", "euch", "be\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Was ich nicht wohl gereimt-", "tokens": ["Was", "ich", "nicht", "wohl", "ge\u00b7reim\u00b7t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Hab' ich doch wohl gemeint.", "tokens": ["Hab'", "ich", "doch", "wohl", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Allein will freundlich ich-", "tokens": ["Al\u00b7lein", "will", "freund\u00b7lich", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Noch was geb\u00e4tten haben/", "tokens": ["Noch", "was", "ge\u00b7b\u00e4t\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "VAINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wann dieses Wercklein nicht-", "tokens": ["Wann", "die\u00b7ses", "Wer\u00b7cklein", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Nach jedes Willen krau\u00df/", "tokens": ["Nach", "je\u00b7des", "Wil\u00b7len", "krau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Da\u00df man nicht etwan w\u00f6ll'-", "tokens": ["Da\u00df", "man", "nicht", "et\u00b7wan", "w\u00f6ll'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "TRUNC"], "meter": "+-++--", "measure": "unknown.measure.tri"}, "line.22": {"text": "Nach hinckenden Buchstaben/", "tokens": ["Nach", "hin\u00b7cken\u00b7den", "Buch\u00b7sta\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Sondern mehr nach dem Sinn-", "tokens": ["Son\u00b7dern", "mehr", "nach", "dem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "TRUNC"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Es treulich legen au\u00df/", "tokens": ["Es", "treu\u00b7lich", "le\u00b7gen", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Und nicht nach b\u00f6ser Art-", "tokens": ["Und", "nicht", "nach", "b\u00f6\u00b7ser", "Ar\u00b7t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Der Neideren verrencken", "tokens": ["Der", "Nei\u00b7de\u00b7ren", "ver\u00b7ren\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Auff einen/ der sehr weit-", "tokens": ["Auff", "ei\u00b7nen", "/", "der", "sehr", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$(", "ART", "ADV", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Entfernt von meinem Sinn:", "tokens": ["Ent\u00b7fernt", "von", "mei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Sie w\u00f6llen \u00f6fftermal-", "tokens": ["Sie", "w\u00f6l\u00b7len", "\u00f6ff\u00b7ter\u00b7ma\u00b7l"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Clorinden Thun bedencken/", "tokens": ["Clo\u00b7rin\u00b7den", "Thun", "be\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "So wird dann hoffentlich-", "tokens": ["So", "wird", "dann", "hof\u00b7fent\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Seyn h\u00e4uffig mein Gewinn.", "tokens": ["Seyn", "h\u00e4uf\u00b7fig", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}