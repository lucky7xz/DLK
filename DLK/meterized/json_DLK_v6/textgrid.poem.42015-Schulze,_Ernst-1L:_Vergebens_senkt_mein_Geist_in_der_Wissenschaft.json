{"textgrid.poem.42015": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergebens senkt mein Geist in der Wissenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergebens senkt mein Geist in der Wissenschaft", "tokens": ["Ver\u00b7ge\u00b7bens", "senkt", "mein", "Geist", "in", "der", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verborgnes Reich sich, forschet der alten Zeit", "tokens": ["Ver\u00b7borg\u00b7nes", "Reich", "sich", ",", "for\u00b7schet", "der", "al\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PRF", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verh\u00fcllten Quellen nach und tr\u00e4umt im", "tokens": ["Ver\u00b7h\u00fcll\u00b7ten", "Quel\u00b7len", "nach", "und", "tr\u00e4umt", "im"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "KON", "VVFIN", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Th\u00f6richten Stolz von dem Kranz des Nachruhms:", "tokens": ["Th\u00f6\u00b7rich\u00b7ten", "Stolz", "von", "dem", "Kranz", "des", "Nach\u00b7ruhms", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein s\u00fc\u00dfes Bildni\u00df spottet den heil'gen Ernst", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Bild\u00b7ni\u00df", "spot\u00b7tet", "den", "heil'\u00b7gen", "Ernst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von meiner Stirn fort: ach, wenn die Huldg\u00f6ttin", "tokens": ["Von", "mei\u00b7ner", "Stirn", "fort", ":", "ach", ",", "wenn", "die", "Huld\u00b7g\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "XY", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich l\u00e4chelnd naht, so kr\u00e4nzt die Weisheit", "tokens": ["Sich", "l\u00e4\u00b7chelnd", "naht", ",", "so", "kr\u00e4nzt", "die", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00f6hlich mit Myrten die Silberlocken.", "tokens": ["Fr\u00f6h\u00b7lich", "mit", "Myr\u00b7ten", "die", "Sil\u00b7berl\u00b7o\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Nicht l\u00e4nger duld' ich dann das verha\u00dfte Joch:", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "duld'", "ich", "dann", "das", "ver\u00b7ha\u00df\u00b7te", "Joch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit freiern Schwingen \u00fcber das frost'ge Land", "tokens": ["Mit", "frei\u00b7ern", "Schwin\u00b7gen", "\u00fc\u00b7ber", "das", "frost'\u00b7ge", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Wissens schwebt mein Geist, und staunend", "tokens": ["Des", "Wis\u00b7sens", "schwebt", "mein", "Geist", ",", "und", "stau\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchl' ich ein Herz mir im Busen schlagen.", "tokens": ["F\u00fchl'", "ich", "ein", "Herz", "mir", "im", "Bu\u00b7sen", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Der Ehre Blendwerk, welches in fr\u00fch'rer Zeit", "tokens": ["Der", "Eh\u00b7re", "Blend\u00b7werk", ",", "wel\u00b7ches", "in", "fr\u00fch'\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mich einst umspielte, jeglichen eitlen Wahn", "tokens": ["Mich", "einst", "um\u00b7spiel\u00b7te", ",", "jeg\u00b7li\u00b7chen", "eit\u00b7len", "Wahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Lob der Mitwelt, von des Enkels", "tokens": ["Vom", "Lob", "der", "Mit\u00b7welt", ",", "von", "des", "En\u00b7kels"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Staunendem Schweigen verscheucht mir siegreich", "tokens": ["Stau\u00b7nen\u00b7dem", "Schwei\u00b7gen", "ver\u00b7scheucht", "mir", "sieg\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Dein zartes L\u00e4cheln, welches der Bl\u00fcthe gleich,", "tokens": ["Dein", "zar\u00b7tes", "L\u00e4\u00b7cheln", ",", "wel\u00b7ches", "der", "Bl\u00fc\u00b7the", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die halb verh\u00fcllt vom Saume der Knospe noch", "tokens": ["Die", "halb", "ver\u00b7h\u00fcllt", "vom", "Sau\u00b7me", "der", "Knos\u00b7pe", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sich sch\u00fcchtern zeigt, der geist'gen Freude", "tokens": ["Sich", "sch\u00fcch\u00b7tern", "zeigt", ",", "der", "geist'\u00b7gen", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leiseres Wehen in dir verk\u00fcndet.", "tokens": ["Lei\u00b7se\u00b7res", "We\u00b7hen", "in", "dir", "ver\u00b7k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ich folg', ich folge, reizendes Bild, wohin", "tokens": ["Ich", "fol\u00b7g'", ",", "ich", "fol\u00b7ge", ",", "rei\u00b7zen\u00b7des", "Bild", ",", "wo\u00b7hin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PWAV"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Wink mich leitet; \u00fcber das weiche Gr\u00fcn", "tokens": ["Dein", "Wink", "mich", "lei\u00b7tet", ";", "\u00fc\u00b7ber", "das", "wei\u00b7che", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Wies' und durch den Duft der Haine", "tokens": ["Der", "Wies'", "und", "durch", "den", "Duft", "der", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Folg' ich mit ewiger, s\u00fc\u00dfer Sehnsucht!", "tokens": ["Fol\u00b7g'", "ich", "mit", "e\u00b7wi\u00b7ger", ",", "s\u00fc\u00b7\u00dfer", "Sehn\u00b7sucht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.7": {"line.1": {"text": "Mit leisem Fl\u00fcstern s\u00e4uselt in stiller Luft", "tokens": ["Mit", "lei\u00b7sem", "Fl\u00fcs\u00b7tern", "s\u00e4u\u00b7selt", "in", "stil\u00b7ler", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Tanz der D\u00e4mmrung; freundlich erwachen rings", "tokens": ["Der", "Tanz", "der", "D\u00e4mm\u00b7rung", ";", "freund\u00b7lich", "er\u00b7wa\u00b7chen", "rings"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Kelch der Bl\u00fcthen, wo beim Rieseln", "tokens": ["Im", "Kelch", "der", "Bl\u00fc\u00b7then", ",", "wo", "beim", "Rie\u00b7seln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcpfender Wellen sie sanft geschlummert,", "tokens": ["H\u00fcp\u00b7fen\u00b7der", "Wel\u00b7len", "sie", "sanft", "ge\u00b7schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Die Traumgebilde; sieh, zu dem Rosensaum", "tokens": ["Die", "Traum\u00b7ge\u00b7bil\u00b7de", ";", "sieh", ",", "zu", "dem", "Ro\u00b7sen\u00b7saum"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des fliehnden Tages schwingen sie leicht sich auf,", "tokens": ["Des", "fliehn\u00b7den", "Ta\u00b7ges", "schwin\u00b7gen", "sie", "leicht", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und in der Sonne letztes L\u00e4cheln", "tokens": ["Und", "in", "der", "Son\u00b7ne", "letz\u00b7tes", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tauchen sie leise die linden Fl\u00fcgel.", "tokens": ["Tau\u00b7chen", "sie", "lei\u00b7se", "die", "lin\u00b7den", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Bethaute Kr\u00e4nze dann um die hei\u00dfe Stirn", "tokens": ["Be\u00b7thau\u00b7te", "Kr\u00e4n\u00b7ze", "dann", "um", "die", "hei\u00b7\u00dfe", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der wachen Sorge winden die Freundlichen,", "tokens": ["Der", "wa\u00b7chen", "Sor\u00b7ge", "win\u00b7den", "die", "Freund\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Sehnsucht um den Mund der Liebe", "tokens": ["Und", "Sehn\u00b7sucht", "um", "den", "Mund", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hauchen sie, Lust auf des Kindes Wange.", "tokens": ["Hau\u00b7chen", "sie", ",", "Lust", "auf", "des", "Kin\u00b7des", "Wan\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "O, schweb' empor am Rande der dunklen Welt,", "tokens": ["O", ",", "schweb'", "em\u00b7por", "am", "Ran\u00b7de", "der", "dunk\u00b7len", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und gleich dem D\u00e4mmern ferner Erinnerung,", "tokens": ["Und", "gleich", "dem", "D\u00e4m\u00b7mern", "fer\u00b7ner", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sinnend uns aus Lust und Wehmuth", "tokens": ["Die", "sin\u00b7nend", "uns", "aus", "Lust", "und", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gaukelnde Zaubergebild' entfaltet,", "tokens": ["Gau\u00b7keln\u00b7de", "Zau\u00b7ber\u00b7ge\u00b7bild'", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Gie\u00df ihn herab vom schweigenden Pfad, o Mond,", "tokens": ["Gie\u00df", "ihn", "her\u00b7ab", "vom", "schwei\u00b7gen\u00b7den", "Pfad", ",", "o", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den geist'gen Schimmer, da\u00df die verkl\u00e4rte Flur", "tokens": ["Den", "geist'\u00b7gen", "Schim\u00b7mer", ",", "da\u00df", "die", "ver\u00b7kl\u00e4r\u00b7te", "Flur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zum sel'gen Traum der sch\u00f6nern Zukunft", "tokens": ["Zum", "sel'\u00b7gen", "Traum", "der", "sch\u00f6\u00b7nern", "Zu\u00b7kunft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd' und die irdische Form verh\u00fclle.", "tokens": ["Werd'", "und", "die", "ir\u00b7di\u00b7sche", "Form", "ver\u00b7h\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Hier la\u00df uns ruhn, am Silber des fl\u00fccht'gen Quells;", "tokens": ["Hier", "la\u00df", "uns", "ruhn", ",", "am", "Sil\u00b7ber", "des", "fl\u00fccht'\u00b7gen", "Quells", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gleich deiner Seele giebt er, vom luft'gen Strahl", "tokens": ["Gleich", "dei\u00b7ner", "See\u00b7le", "giebt", "er", ",", "vom", "luft'\u00b7gen", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des keuschen Lichts umwallt, mein Bild mir", "tokens": ["Des", "keu\u00b7schen", "Lichts", "um\u00b7wallt", ",", "mein", "Bild", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-", "measure": "