{"textgrid.poem.42322": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Als das Obige f\u00fcr Vers\u00fcndigung erkl\u00e4rt wurde", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schelte nicht die edle Gabe,", "tokens": ["Ich", "schel\u00b7te", "nicht", "die", "ed\u00b7le", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich von Gott empfangen habe.", "tokens": ["Die", "ich", "von", "Gott", "emp\u00b7fan\u00b7gen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gabe hat mir Heil gew\u00e4hrt,", "tokens": ["Die", "Ga\u00b7be", "hat", "mir", "Heil", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein ihr Ruhm oft Fluch beschert.", "tokens": ["Al\u00b7lein", "ihr", "Ruhm", "oft", "Fluch", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}