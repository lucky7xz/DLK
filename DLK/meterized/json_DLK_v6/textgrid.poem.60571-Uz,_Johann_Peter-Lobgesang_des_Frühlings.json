{"textgrid.poem.60571": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Lobgesang des Fr\u00fchlings", "genre": "verse", "period": "N.A.", "pub_year": 1741, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lang hat meine Muse schon,", "tokens": ["Wie", "lang", "hat", "mei\u00b7ne", "Mu\u00b7se", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Witz und edle Einfalt ehret,", "tokens": ["Die", "Witz", "und", "ed\u00b7le", "Ein\u00b7falt", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am blumenvollen Helikon,", "tokens": ["Am", "blu\u00b7men\u00b7vol\u00b7len", "He\u00b7li\u00b7kon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Musen Griechenlands begierig zugeh\u00f6ret!", "tokens": ["Den", "Mu\u00b7sen", "Grie\u00b7chen\u00b7lands", "be\u00b7gie\u00b7rig", "zu\u00b7ge\u00b7h\u00f6\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun aber will sie selbst einmal", "tokens": ["Nun", "a\u00b7ber", "will", "sie", "selbst", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hochgestimmte Cyther schlagen;", "tokens": ["Die", "hoch\u00b7ges\u00b7timm\u00b7te", "Cy\u00b7ther", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Mavors blutbefleckter Stal", "tokens": ["Doch", "Ma\u00b7vors", "blut\u00b7be\u00b7fleck\u00b7ter", "Stal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbeut ihr, sich ins Feld, voll Furchtsamkeit, zu wagen.", "tokens": ["Ver\u00b7beut", "ihr", ",", "sich", "ins", "Feld", ",", "voll", "Furcht\u00b7sam\u00b7keit", ",", "zu", "wa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRF", "APPRART", "NN", "$,", "ADJD", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie schlich sich zwar, mit seltnem Muth,", "tokens": ["Sie", "schlich", "sich", "zwar", ",", "mit", "selt\u00b7nem", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "J\u00fcngsthin ins dicke Kriegsgedr\u00e4nge,", "tokens": ["J\u00fcng\u00b7sthin", "ins", "di\u00b7cke", "Kriegs\u00b7ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und sann auf Leichen und auf Blut", "tokens": ["Und", "sann", "auf", "Lei\u00b7chen", "und", "auf", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in erhitztem Kampf, auf kriegrische Ges\u00e4nge.", "tokens": ["Und", "in", "er\u00b7hitz\u00b7tem", "Kampf", ",", "auf", "krie\u00b7gri\u00b7sche", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie drang mit Zittern an den Ort,", "tokens": ["Sie", "drang", "mit", "Zit\u00b7tern", "an", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo, trotz der Glut, die donnernd krachte,", "tokens": ["Wo", ",", "trotz", "der", "Glut", ",", "die", "don\u00b7nernd", "krach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Muth und durch sein m\u00e4chtig Wort", "tokens": ["Durch", "Muth", "und", "durch", "sein", "m\u00e4ch\u00b7tig", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Brandenburgs Monarch das Kriegsgl\u00fcck dienstbar machte.", "tokens": ["Sich", "Bran\u00b7den\u00b7burgs", "Mon\u00b7arch", "das", "Kriegs\u00b7gl\u00fcck", "dienst\u00b7bar", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "NE", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch Ph\u00f6bus ri\u00df sie aus dem Brand,", "tokens": ["Doch", "Ph\u00f6\u00b7bus", "ri\u00df", "sie", "aus", "dem", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bracht, durch ihre Furcht ger\u00fchret,", "tokens": ["Und", "bracht", ",", "durch", "ih\u00b7re", "Furcht", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie in das sonnenreiche Land,", "tokens": ["Sie", "in", "das", "son\u00b7nen\u00b7rei\u00b7che", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allwo der Wahrheit Faust den sanften Zepter f\u00fchret.", "tokens": ["All\u00b7wo", "der", "Wahr\u00b7heit", "Faust", "den", "sanf\u00b7ten", "Zep\u00b7ter", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hier, sprach er, wo kein M\u00f6rsel w\u00fchlt,", "tokens": ["Hier", ",", "sprach", "er", ",", "wo", "kein", "M\u00f6r\u00b7sel", "w\u00fchlt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesen ungest\u00f6rten Gr\u00fcnden,", "tokens": ["In", "die\u00b7sen", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Versuche, wie dein Finger spielt;", "tokens": ["Ver\u00b7su\u00b7che", ",", "wie", "dein", "Fin\u00b7ger", "spielt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kannst auch hier den Stoff zu hohen Liedern finden.", "tokens": ["Du", "kannst", "auch", "hier", "den", "Stoff", "zu", "ho\u00b7hen", "Lie\u00b7dern", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dort, in der G\u00f6ttinn Heiligthum,", "tokens": ["Dort", ",", "in", "der", "G\u00f6t\u00b7tinn", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Licht und reiner Schimmer lachen,", "tokens": ["Wo", "Licht", "und", "rei\u00b7ner", "Schim\u00b7mer", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da thront ihr Liebling und ihr Ruhm,", "tokens": ["Da", "thront", "ihr", "Lieb\u00b7ling", "und", "ihr", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wolf, der f\u00fcr Eifer gl\u00fcht, die Wahrheit gro\u00df zu machen.", "tokens": ["Wolf", ",", "der", "f\u00fcr", "Ei\u00b7fer", "gl\u00fcht", ",", "die", "Wahr\u00b7heit", "gro\u00df", "zu", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Sie reicht, auf unsch\u00e4tzbarem Gold,", "tokens": ["Sie", "reicht", ",", "auf", "un\u00b7sch\u00e4tz\u00b7ba\u00b7rem", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm necktarreiche G\u00f6tterspeise,", "tokens": ["Ihm", "neck\u00b7tar\u00b7rei\u00b7che", "G\u00f6t\u00b7ter\u00b7spei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die jener fette Weinstock zollt,", "tokens": ["Die", "je\u00b7ner", "fet\u00b7te", "Wein\u00b7stock", "zollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der um den Tempel kriecht, gepflanzt von ihrem Flei\u00dfe.", "tokens": ["Der", "um", "den", "Tem\u00b7pel", "kriecht", ",", "ge\u00b7pflanzt", "von", "ih\u00b7rem", "Flei\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wolf reicht es nun dem Grafen dar,", "tokens": ["Wolf", "reicht", "es", "nun", "dem", "Gra\u00b7fen", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Philurenens Fluren schm\u00fccket;", "tokens": ["Der", "Phi\u00b7lu\u00b7re\u00b7nens", "Flu\u00b7ren", "schm\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Den schon die frohe Musenschaar,", "tokens": ["Den", "schon", "die", "fro\u00b7he", "Mu\u00b7sen\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die seine Rechte sch\u00fctzt, bis an die Sterne r\u00fccket.", "tokens": ["Die", "sei\u00b7ne", "Rech\u00b7te", "sch\u00fctzt", ",", "bis", "an", "die", "Ster\u00b7ne", "r\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der hat, von hoher Glut entbrannt,", "tokens": ["Der", "hat", ",", "von", "ho\u00b7her", "Glut", "ent\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den lorbernwerthen Bund errichtet,", "tokens": ["Den", "lor\u00b7bern\u00b7wert\u00b7hen", "Bund", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der durch ein neugekn\u00fcpftes Band,", "tokens": ["Der", "durch", "ein", "neu\u00b7ge\u00b7kn\u00fcpf\u00b7tes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wahrheit beyzustehn, jedweden Freund verpflichtet.", "tokens": ["Der", "Wahr\u00b7heit", "bey\u00b7zu\u00b7stehn", ",", "jed\u00b7we\u00b7den", "Freund", "ver\u00b7pflich\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Was k\u00f6mmt da f\u00fcr ein k\u00fchner Held?", "tokens": ["Was", "k\u00f6mmt", "da", "f\u00fcr", "ein", "k\u00fch\u00b7ner", "Held", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es fliegt der Sieg an seiner Seite;", "tokens": ["Es", "fliegt", "der", "Sieg", "an", "sei\u00b7ner", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Klugheit ist ihm beygesellt;", "tokens": ["Die", "Klug\u00b7heit", "ist", "ihm", "bey\u00b7ge\u00b7sellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sieht der Kriegsgott aus, erhitzt vom scharfen Streite.", "tokens": ["So", "sieht", "der", "Kriegs\u00b7gott", "aus", ",", "er\u00b7hitzt", "vom", "schar\u00b7fen", "Strei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Wer kann es sonst, als Friedrich, seyn?", "tokens": ["Wer", "kann", "es", "sonst", ",", "als", "Fried\u00b7rich", ",", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$,", "KOUS", "NE", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist es, ja, des Feindes Schrecken.", "tokens": ["Der", "ist", "es", ",", "ja", ",", "des", "Fein\u00b7des", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PTKANT", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00e4ngt in jenem Palmenh\u00e4yn", "tokens": ["Er", "h\u00e4ngt", "in", "je\u00b7nem", "Pal\u00b7men\u00b7h\u00e4yn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die g\u00fcldnen Waffen auf, die Staub und Blut bedecken.", "tokens": ["Die", "g\u00fcld\u00b7nen", "Waf\u00b7fen", "auf", ",", "die", "Staub", "und", "Blut", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Ihn k\u00fc\u00dft der G\u00f6ttinn holder Mund;", "tokens": ["Ihn", "k\u00fc\u00dft", "der", "G\u00f6t\u00b7tinn", "hol\u00b7der", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punc