{"textgrid.poem.41916": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcr der Liebe frische Bl\u00fcthen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr der Liebe frische Bl\u00fcthen", "tokens": ["F\u00fcr", "der", "Lie\u00b7be", "fri\u00b7sche", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebst du welke Rosen mir;", "tokens": ["Giebst", "du", "wel\u00b7ke", "Ro\u00b7sen", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ich will sie freundlich h\u00fcten,", "tokens": ["Doch", "ich", "will", "sie", "freund\u00b7lich", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie das lange Leid von dir.", "tokens": ["Wie", "das", "lan\u00b7ge", "Leid", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn in tr\u00fcben Wintertagen", "tokens": ["Wenn", "in", "tr\u00fc\u00b7ben", "Win\u00b7ter\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcnftig nun die St\u00fcrme wehn,", "tokens": ["K\u00fcnf\u00b7tig", "nun", "die", "St\u00fcr\u00b7me", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll ihr bleiches Bild mir sagen,", "tokens": ["Soll", "ihr", "blei\u00b7ches", "Bild", "mir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mir selber einst geschehn:", "tokens": ["Was", "mir", "sel\u00b7ber", "einst", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In des Fr\u00fchlings Bl\u00fcthenhallen", "tokens": ["In", "des", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7then\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sahn auch wir den Sonnenschein,", "tokens": ["Sahn", "auch", "wir", "den", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Lied der Nachtigallen", "tokens": ["Und", "das", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sang auch uns in Schlummer ein.", "tokens": ["Sang", "auch", "uns", "in", "Schlum\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die das Gl\u00fcck nur einst besessen,", "tokens": ["Die", "das", "Gl\u00fcck", "nur", "einst", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verl\u00e4\u00dft sein Schatten nie;", "tokens": ["Die", "ver\u00b7l\u00e4\u00dft", "sein", "Schat\u00b7ten", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bittrer ist es, zu vergessen,", "tokens": ["Bit\u00b7trer", "ist", "es", ",", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als zu klagen sp\u00e4t und fr\u00fch.", "tokens": ["Als", "zu", "kla\u00b7gen", "sp\u00e4t", "und", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Blieb von allem unsern Prangen", "tokens": ["Blieb", "von", "al\u00b7lem", "un\u00b7sern", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der Dorn uns nur zur\u00fcck;", "tokens": ["Auch", "der", "Dorn", "uns", "nur", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurend freun wir uns und hangen", "tokens": ["Trau\u00b7rend", "freun", "wir", "uns", "und", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem hingewelkten Gl\u00fcck.", "tokens": ["An", "dem", "hin\u00b7ge\u00b7welk\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}