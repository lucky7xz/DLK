{"textgrid.poem.62457": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod der K\u00f6nigin", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rose, sch\u00f6ne K\u00f6nigsrose,", "tokens": ["Ro\u00b7se", ",", "sch\u00f6\u00b7ne", "K\u00f6\u00b7nigs\u00b7ro\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat auch dich der Sturm getroffen?", "tokens": ["Hat", "auch", "dich", "der", "Sturm", "ge\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gilt kein Beten mehr, kein Hoffen", "tokens": ["Gilt", "kein", "Be\u00b7ten", "mehr", ",", "kein", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei dem schreckenvollen Loose?", "tokens": ["Bei", "dem", "schre\u00b7cken\u00b7vol\u00b7len", "Loo\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seid ihr, hochgeweihte Glieder,", "tokens": ["Seid", "ihr", ",", "hoch\u00b7ge\u00b7weih\u00b7te", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon dem d\u00fcstern Reich verfallen?", "tokens": ["Schon", "dem", "d\u00fcs\u00b7tern", "Reich", "ver\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haupt, um das die Locken wallen,", "tokens": ["Haupt", ",", "um", "das", "die", "Lo\u00b7cken", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinkest du zum Schlummer nieder?", "tokens": ["Sin\u00b7kest", "du", "zum", "Schlum\u00b7mer", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sink' in Schlummer, aufgefunden", "tokens": ["Sink'", "in", "Schlum\u00b7mer", ",", "auf\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVIMP", "APPR", "NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist das Ziel, nach dem du schrittest,", "tokens": ["Ist", "das", "Ziel", ",", "nach", "dem", "du", "schrit\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der Kranz, um den du littest,", "tokens": ["Ist", "der", "Kranz", ",", "um", "den", "du", "lit\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUI", "ART", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhe labt am Quell der Wunden.", "tokens": ["Ru\u00b7he", "labt", "am", "Quell", "der", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf, Gesang, vom Klagethale!", "tokens": ["Auf", ",", "Ge\u00b7sang", ",", "vom", "Kla\u00b7ge\u00b7tha\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweb' empor zu lichten Hallen,", "tokens": ["Schweb'", "em\u00b7por", "zu", "lich\u00b7ten", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Siegeshymnen schallen,", "tokens": ["Wo", "die", "Sie\u00b7ges\u00b7hym\u00b7nen", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singe Tr\u00f6stung dem Gemahle!", "tokens": ["Sin\u00b7ge", "Tr\u00f6s\u00b7tung", "dem", "Ge\u00b7mah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sink' an deiner V\u00f6lker Herzen,", "tokens": ["Sink'", "an", "dei\u00b7ner", "V\u00f6l\u00b7ker", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du im tiefsten Leid Verlorner,", "tokens": ["Du", "im", "tiefs\u00b7ten", "Leid", "Ver\u00b7lor\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du zum Martyrthum Erkorner,", "tokens": ["Du", "zum", "Mar\u00b7tyr\u00b7thum", "Er\u00b7kor\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auszubluten deine Schmerzen.", "tokens": ["Aus\u00b7zu\u00b7blu\u00b7ten", "dei\u00b7ne", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr und K\u00f6nig, schau' nach oben,", "tokens": ["Herr", "und", "K\u00f6\u00b7nig", ",", "schau'", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo Sie leuchtet gleich den Sternen,", "tokens": ["Wo", "Sie", "leuch\u00b7tet", "gleich", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo in Himmels weiten Fernen", "tokens": ["Wo", "in", "Him\u00b7mels", "wei\u00b7ten", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Heilige Sie loben.", "tokens": ["Al\u00b7le", "Hei\u00b7li\u00b7ge", "Sie", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}