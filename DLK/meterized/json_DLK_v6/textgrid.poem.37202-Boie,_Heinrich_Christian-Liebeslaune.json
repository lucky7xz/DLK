{"textgrid.poem.37202": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Liebeslaune", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie liebte mich die wunderholde Braune,", "tokens": ["Sie", "lieb\u00b7te", "mich", "die", "wun\u00b7der\u00b7hol\u00b7de", "Brau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mich umgab was Gl\u00fcck nur hei\u00dfen mag.", "tokens": ["Und", "mich", "um\u00b7gab", "was", "Gl\u00fcck", "nur", "hei\u00b7\u00dfen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "VVFIN", "PIS", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie liebte mich, das war nun ihre Laune,", "tokens": ["Sie", "lieb\u00b7te", "mich", ",", "das", "war", "nun", "ih\u00b7re", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch Laune nur f\u00fcr einen Fr\u00fchlingstag.", "tokens": ["Doch", "Lau\u00b7ne", "nur", "f\u00fcr", "ei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit andern bald schlo\u00df eben diese Braune", "tokens": ["Mit", "an\u00b7dern", "bald", "schlo\u00df", "e\u00b7ben", "die\u00b7se", "Brau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und andern drauf den wankenden Vertrag,", "tokens": ["Und", "an\u00b7dern", "drauf", "den", "wan\u00b7ken\u00b7den", "Ver\u00b7trag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Himmelswonn' empfand in ihrer Laune", "tokens": ["Und", "Him\u00b7mels\u00b7wonn'", "emp\u00b7fand", "in", "ih\u00b7rer", "Lau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beseltem Spiel auch jeder \u2013 einen Tag.", "tokens": ["Be\u00b7sel\u00b7tem", "Spiel", "auch", "je\u00b7der", "\u2013", "ei\u00b7nen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PIAT", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wechselnd schafft die wunderholde Braune", "tokens": ["So", "wech\u00b7selnd", "schafft", "die", "wun\u00b7der\u00b7hol\u00b7de", "Brau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Elysium und H\u00f6lle Tag um Tag.", "tokens": ["E\u00b7ly\u00b7si\u00b7um", "und", "H\u00f6l\u00b7le", "Tag", "um", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man z\u00fcrnt ihr laut. Mir folgt der s\u00fc\u00dfen Laune", "tokens": ["Man", "z\u00fcrnt", "ihr", "laut", ".", "Mir", "folgt", "der", "s\u00fc\u00b7\u00dfen", "Lau\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erinnerung mit stiller Wonne nach.", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "mit", "stil\u00b7ler", "Won\u00b7ne", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Entflattert uns die s\u00fc\u00dfe holde Braune,", "tokens": ["Ent\u00b7flat\u00b7tert", "uns", "die", "s\u00fc\u00b7\u00dfe", "hol\u00b7de", "Brau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umsonst wird Hader dann und Klage sein.", "tokens": ["Um\u00b7sonst", "wird", "Ha\u00b7der", "dann", "und", "Kla\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begehren wir die Wiederkehr der Laune,", "tokens": ["Be\u00b7geh\u00b7ren", "wir", "die", "Wie\u00b7der\u00b7kehr", "der", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie? koset sie, hauchst du sie doch nicht ein!", "tokens": ["Wie", "?", "ko\u00b7set", "sie", ",", "hauchst", "du", "sie", "doch", "nicht", "ein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Im Haine j\u00fcngst fand ich die holde Braune,", "tokens": ["Im", "Hai\u00b7ne", "j\u00fcngst", "fand", "ich", "die", "hol\u00b7de", "Brau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo ich mit Rosen sie umkr\u00e4nzend sprach:", "tokens": ["Wo", "ich", "mit", "Ro\u00b7sen", "sie", "um\u00b7kr\u00e4n\u00b7zend", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwann kehrt einmal zur\u00fcck die s\u00fc\u00dfe Laune,", "tokens": ["\u00bb", "wann", "kehrt", "ein\u00b7mal", "zu\u00b7r\u00fcck", "die", "s\u00fc\u00b7\u00dfe", "Lau\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df neu ich liebend neu ich leben mag?\u00ab", "tokens": ["Da\u00df", "neu", "ich", "lie\u00b7bend", "neu", "ich", "le\u00b7ben", "mag", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADJD", "ADJD", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da blickt auf mich die wunderholde Braune,", "tokens": ["Da", "blickt", "auf", "mich", "die", "wun\u00b7der\u00b7hol\u00b7de", "Brau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In sanfter R\u00f6th entschl\u00fcpft ihr leis ein Ach!", "tokens": ["In", "sanf\u00b7ter", "R\u00f6th", "ent\u00b7schl\u00fcpft", "ihr", "leis", "ein", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es kehrt zur\u00fcck die s\u00fc\u00dfe, s\u00fc\u00dfe Laune,", "tokens": ["Es", "kehrt", "zu\u00b7r\u00fcck", "die", "s\u00fc\u00b7\u00dfe", ",", "s\u00fc\u00b7\u00dfe", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie dauert nun schon manchen sch\u00f6nen Tag.", "tokens": ["Sie", "dau\u00b7ert", "nun", "schon", "man\u00b7chen", "sch\u00f6\u00b7nen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}