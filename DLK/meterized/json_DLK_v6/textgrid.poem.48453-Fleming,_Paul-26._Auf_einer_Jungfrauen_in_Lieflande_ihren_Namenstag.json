{"textgrid.poem.48453": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "26. Auf einer Jungfrauen in Lieflande ihren Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was, Sch\u00f6ne, sollen wir euch heute Sch\u00f6nes schenken,", "tokens": ["Was", ",", "Sch\u00f6\u00b7ne", ",", "sol\u00b7len", "wir", "euch", "heu\u00b7te", "Sch\u00f6\u00b7nes", "schen\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VMFIN", "PPER", "PPER", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "darbei ihr eures Tags und unser m\u00f6gt gedenken,", "tokens": ["dar\u00b7bei", "ihr", "eu\u00b7res", "Tags", "und", "un\u00b7ser", "m\u00f6gt", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des sch\u00f6nen Tags, der euch mit Freuden hat begabt,", "tokens": ["des", "sch\u00f6\u00b7nen", "Tags", ",", "der", "euch", "mit", "Freu\u00b7den", "hat", "be\u00b7gabt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und unser, die ihr euch mit Gunst verpflichtet habt?", "tokens": ["und", "un\u00b7ser", ",", "die", "ihr", "euch", "mit", "Gunst", "ver\u00b7pflich\u00b7tet", "habt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "$,", "PRELS", "PPER", "PRF", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was Tajus Sch\u00f6nes f\u00fchrt in seinem gelben Strande,", "tokens": ["Was", "Ta\u00b7jus", "Sch\u00f6\u00b7nes", "f\u00fchrt", "in", "sei\u00b7nem", "gel\u00b7ben", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und was Bassora liest aus Ormus reichem Sande,", "tokens": ["und", "was", "Bas\u00b7so\u00b7ra", "liest", "aus", "Or\u00b7mus", "rei\u00b7chem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "was man wirkt zu Schiras, die k\u00fcnstlich ist und reich,", "tokens": ["was", "man", "wirkt", "zu", "Schi\u00b7ras", ",", "die", "k\u00fcnst\u00b7lich", "ist", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "APPR", "NE", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "das ist zwar sch\u00f6n' in sich, doch sch\u00f6ne nicht f\u00fcr euch,", "tokens": ["das", "ist", "zwar", "sch\u00f6n'", "in", "sich", ",", "doch", "sch\u00f6\u00b7ne", "nicht", "f\u00fcr", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "APPR", "PRF", "$,", "ADV", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "ihr Schauplatz aller Zier! Die goldgemengten Haare", "tokens": ["ihr", "Schau\u00b7platz", "al\u00b7ler", "Zier", "!", "Die", "gold\u00b7ge\u00b7meng\u00b7ten", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sind g\u00fcldner noch als Gold, die zarte Perlenware", "tokens": ["sind", "g\u00fcld\u00b7ner", "noch", "als", "Gold", ",", "die", "zar\u00b7te", "Per\u00b7len\u00b7wa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "KOUS", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ist grob f\u00fcr eurer Zier; kein g\u00fcldnes St\u00fcck ist rein',", "tokens": ["ist", "grob", "f\u00fcr", "eu\u00b7rer", "Zier", ";", "kein", "g\u00fcld\u00b7nes", "St\u00fcck", "ist", "rein'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$.", "PIAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ihr gebt mit kluger Hand den Farben h\u00f6hern Schein,", "tokens": ["ihr", "gebt", "mit", "klu\u00b7ger", "Hand", "den", "Far\u00b7ben", "h\u00f6\u00b7hern", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "wenn ihr die Nadel f\u00fchrt. Die brennenden Rubinen,", "tokens": ["wenn", "ihr", "die", "Na\u00b7del", "f\u00fchrt", ".", "Die", "bren\u00b7nen\u00b7den", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "die d\u00fcrfen euren Mund zu sehn sich nicht erk\u00fchnen,", "tokens": ["die", "d\u00fcr\u00b7fen", "eu\u00b7ren", "Mund", "zu", "sehn", "sich", "nicht", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "sind bla\u00df und tot vor ihm. Kein heller Demant nicht,", "tokens": ["sind", "bla\u00df", "und", "tot", "vor", "ihm", ".", "Kein", "hel\u00b7ler", "De\u00b7mant", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "$.", "PIAT", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "der wagt sich einen Blick in eurer Augen Licht.", "tokens": ["der", "wagt", "sich", "ei\u00b7nen", "Blick", "in", "eu\u00b7rer", "Au\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So d\u00fcrfen wir auch nicht ein s\u00fc\u00dfes St\u00fcndlein bringen", "tokens": ["So", "d\u00fcr\u00b7fen", "wir", "auch", "nicht", "ein", "s\u00fc\u00b7\u00dfes", "St\u00fcnd\u00b7lein", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "und in manch Instrument die Knaben lassen singen,", "tokens": ["und", "in", "manch", "Inst\u00b7ru\u00b7ment", "die", "Kna\u00b7ben", "las\u00b7sen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "weil itzund Gottes Sohn f\u00fcr unsre S\u00fcnde b\u00fc\u00dft,", "tokens": ["weil", "it\u00b7zund", "Got\u00b7tes", "Sohn", "f\u00fcr", "uns\u00b7re", "S\u00fcn\u00b7de", "b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "und ohn' di\u00df euer Haus voll nassen Weinens ist,", "tokens": ["und", "ohn'", "di\u00df", "eu\u00b7er", "Haus", "voll", "nas\u00b7sen", "Wei\u00b7nens", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "so da\u00df nichts Sch\u00f6nes euch von uns itzt her kan kommen,", "tokens": ["so", "da\u00df", "nichts", "Sch\u00f6\u00b7nes", "euch", "von", "uns", "itzt", "her", "kan", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADJA", "PPER", "APPR", "PPER", "ADV", "APZR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "weil aller Sch\u00f6nheit Gut uns g\u00e4nzlich ist benommen.", "tokens": ["weil", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Gut", "uns", "g\u00e4nz\u00b7lich", "ist", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Di\u00df ungeformte Band ist einig \u00fcbrig noch,", "tokens": ["Di\u00df", "un\u00b7ge\u00b7form\u00b7te", "Band", "ist", "ei\u00b7nig", "\u00fcb\u00b7rig", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "ver\u00e4chtlich, unwert, arm. Nehmt, Sch\u00f6ne, nehmt es doch", "tokens": ["ver\u00b7\u00e4cht\u00b7lich", ",", "un\u00b7wert", ",", "arm", ".", "Nehmt", ",", "Sch\u00f6\u00b7ne", ",", "nehmt", "es", "doch"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "und la\u00dfts ein Zeichen sein, da\u00df man euch hat gebunden!", "tokens": ["und", "la\u00dfts", "ein", "Zei\u00b7chen", "sein", ",", "da\u00df", "man", "euch", "hat", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VAINF", "$,", "KOUS", "PIS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Seht tausentmal den Tag, doch mit erfreuten Stunden!", "tokens": ["Seht", "tau\u00b7sent\u00b7mal", "den", "Tag", ",", "doch", "mit", "er\u00b7freu\u00b7ten", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er stelle sich fortan mit lauter Lachen ein", "tokens": ["Er", "stel\u00b7le", "sich", "for\u00b7tan", "mit", "lau\u00b7ter", "La\u00b7chen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "und lass' euch \u00fcbers Jahr ein fr\u00f6lichs Br\u00e4utlein sein!", "tokens": ["und", "lass'", "euch", "\u00fc\u00b7bers", "Jahr", "ein", "fr\u00f6\u00b7lichs", "Br\u00e4ut\u00b7lein", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kein sch\u00f6nrer Wundsch ist da. Doch wollet ihr gedenken:", "tokens": ["Kein", "sch\u00f6n\u00b7rer", "Wund\u00b7sch", "ist", "da", ".", "Doch", "wol\u00b7let", "ihr", "ge\u00b7den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADV", "$.", "KON", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "ihr habt Schuld und nicht wir, da\u00df wir nichts k\u00f6nnen schenken,", "tokens": ["ihr", "habt", "Schuld", "und", "nicht", "wir", ",", "da\u00df", "wir", "nichts", "k\u00f6n\u00b7nen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "PTKNEG", "PPER", "$,", "KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "was sch\u00f6ne hei\u00dft und ist! Ihr habts schon in der Tat,", "tokens": ["was", "sch\u00f6\u00b7ne", "hei\u00dft", "und", "ist", "!", "Ihr", "habts", "schon", "in", "der", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVFIN", "KON", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "als der die Sch\u00f6nheit sich selbselbst verehret hat.", "tokens": ["als", "der", "die", "Sch\u00f6n\u00b7heit", "sich", "selb\u00b7selbst", "ver\u00b7eh\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "PRF", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}