{"dta.poem.12464": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geschworen hatt' ich in der Stille:", "tokens": ["Ge\u00b7schwo\u00b7ren", "hatt'", "ich", "in", "der", "Stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, keine Verse in Paris!", "tokens": ["Nein", ",", "kei\u00b7ne", "Ver\u00b7se", "in", "Pa\u00b7ris", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "APPR", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und Mirabeau's Trib\u00fcne ", "tokens": ["Und", "Mi\u00b7ra\u00b7be\u00b7au's", "Tri\u00b7b\u00fc\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ein Volk im Julisonnenbrand?", "tokens": ["Ein", "Volk", "im", "Ju\u00b7li\u00b7son\u00b7nen\u00b7brand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der Christ der Freiheit auferstand?", "tokens": ["Der", "Christ", "der", "Frei\u00b7heit", "auf\u00b7er\u00b7stand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die T\u00e4uschung ward mir schnell benommen,", "tokens": ["Die", "T\u00e4u\u00b7schung", "ward", "mir", "schnell", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie f\u00e4llt vom Auge St\u00fcck f\u00fcr St\u00fcck;", "tokens": ["Sie", "f\u00e4llt", "vom", "Au\u00b7ge", "St\u00fcck", "f\u00fcr", "St\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin so durstig hergekommen,", "tokens": ["Ich", "bin", "so", "durs\u00b7tig", "her\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kehre ohne Trunk zur\u00fcck.", "tokens": ["Und", "keh\u00b7re", "oh\u00b7ne", "Trunk", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gern auf die Knie' w\u00e4r' ich gesunken \u2014", "tokens": ["Gern", "auf", "die", "Knie'", "w\u00e4r'", "ich", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind eure Buden ein Altar?", "tokens": ["Sind", "eu\u00b7re", "Bu\u00b7den", "ein", "Al\u00b7tar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht eine Flamme, nicht ein Funken,", "tokens": ["Nicht", "ei\u00b7ne", "Flam\u00b7me", ",", "nicht", "ein", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo des Jahrhunderts Krater war!", "tokens": ["Wo", "des", "Jahr\u00b7hun\u00b7derts", "Kra\u00b7ter", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Gesch\u00e4ndet selbst die kalte Lave,", "tokens": ["Ge\u00b7sch\u00e4n\u00b7det", "selbst", "die", "kal\u00b7te", "La\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In die so heilig Blut gethaut,", "tokens": ["In", "die", "so", "hei\u00b7lig", "Blut", "ge\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihr nun, wie Neapels Sklave,", "tokens": ["Da\u00df", "ihr", "nun", ",", "wie", "Nea\u00b7pels", "Skla\u00b7ve", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "NE", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Drauf eurer Wollust Reben baut!", "tokens": ["Drauf", "eu\u00b7rer", "Wol\u00b7lust", "Re\u00b7ben", "baut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O nehmt sie fort, die Trikolore,", "tokens": ["O", "nehmt", "sie", "fort", ",", "die", "Tri\u00b7ko\u00b7lo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die eurer V\u00e4ter Thaten sah,", "tokens": ["Die", "eu\u00b7rer", "V\u00e4\u00b7ter", "Tha\u00b7ten", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreibet warnend an die Thore:", "tokens": ["Und", "schrei\u00b7bet", "war\u00b7nend", "an", "die", "Tho\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ehier ist der Freiheit Capua!\u201c", "tokens": ["\u201e", "hier", "ist", "der", "Frei\u00b7heit", "Ca\u00b7pua", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "NE", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}