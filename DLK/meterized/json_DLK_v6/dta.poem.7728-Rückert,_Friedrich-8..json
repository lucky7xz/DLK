{"dta.poem.7728": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Volk ist gl\u00fccklich, des Mannsalter ist durchdrungen", "tokens": ["Das", "Volk", "ist", "gl\u00fcck\u00b7lich", ",", "des", "Mann\u00b7sal\u00b7ter", "ist", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von unveraltenden Jugenderinnerungen;", "tokens": ["Von", "un\u00b7ver\u00b7al\u00b7ten\u00b7den", "Ju\u00b7gen\u00b7de\u00b7rin\u00b7ne\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Das, immer werdend, nie Gewordenes verliert,", "tokens": ["Das", ",", "im\u00b7mer", "wer\u00b7dend", ",", "nie", "Ge\u00b7wor\u00b7de\u00b7nes", "ver\u00b7liert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "ADJD", "$,", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich aus eignem Grund stets h\u00f6her umgebiert.", "tokens": ["Und", "sich", "aus", "eig\u00b7nem", "Grund", "stets", "h\u00f6\u00b7her", "um\u00b7ge\u00b7biert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sowie der Einzelne sich auch nur kann verj\u00fcngen,", "tokens": ["So\u00b7wie", "der", "Ein\u00b7zel\u00b7ne", "sich", "auch", "nur", "kann", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PRF", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn sein Bewu\u00dftseyn ruht auf seinen Selbsturspr\u00fcngen;", "tokens": ["Wenn", "sein", "Be\u00b7wu\u00df\u00b7tseyn", "ruht", "auf", "sei\u00b7nen", "Selbs\u00b7tur\u00b7spr\u00fcn\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn er die Ordnung f\u00fchlt, in der durch jede Wendung", "tokens": ["Wenn", "er", "die", "Ord\u00b7nung", "f\u00fchlt", ",", "in", "der", "durch", "je\u00b7de", "Wen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Stufen sich sein Gang gesteigert zur Vollendung;", "tokens": ["Der", "Stu\u00b7fen", "sich", "sein", "Gang", "ge\u00b7stei\u00b7gert", "zur", "Vol\u00b7len\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "F\u00fchlt, da\u00df zur Ordnung selbst geh\u00f6rten St\u00f6rungen,", "tokens": ["F\u00fchlt", ",", "da\u00df", "zur", "Ord\u00b7nung", "selbst", "ge\u00b7h\u00f6r\u00b7ten", "St\u00f6\u00b7run\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPRART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und die Besonnenheit wuchs aus Beth\u00f6rungen.", "tokens": ["Und", "die", "Be\u00b7son\u00b7nen\u00b7heit", "wuchs", "aus", "Be\u00b7th\u00f6\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--++-++-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie sich viel Knoten- durch ein Rohr zur Reife dr\u00e4ngt,", "tokens": ["Wie", "sich", "viel", "Kno\u00b7ten", "durch", "ein", "Rohr", "zur", "Rei\u00b7fe", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "TRUNC", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Strom sein Bette durch besch\u00e4umte Felsen sprengt.", "tokens": ["Ein", "Strom", "sein", "Bet\u00b7te", "durch", "be\u00b7sch\u00e4um\u00b7te", "Fel\u00b7sen", "sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zum Himmelspiegel ist zuletzt der Strom geworden,", "tokens": ["Zum", "Him\u00b7mel\u00b7spie\u00b7gel", "ist", "zu\u00b7letzt", "der", "Strom", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcrz'gen Markes voll das Rohr an seinen Borden.", "tokens": ["Und", "w\u00fcrz'\u00b7gen", "Mar\u00b7kes", "voll", "das", "Rohr", "an", "sei\u00b7nen", "Bor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}