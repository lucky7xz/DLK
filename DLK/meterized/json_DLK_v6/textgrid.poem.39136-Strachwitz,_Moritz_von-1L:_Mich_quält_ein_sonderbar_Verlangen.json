{"textgrid.poem.39136": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich qu\u00e4lt ein sonderbar Verlangen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich qu\u00e4lt ein sonderbar Verlangen", "tokens": ["Mich", "qu\u00e4lt", "ein", "son\u00b7der\u00b7bar", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Sorg' und M\u00fch', Gefahr und Streit,", "tokens": ["Nach", "Sor\u00b7g'", "und", "M\u00fch'", ",", "Ge\u00b7fahr", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Es ist mir stets zu gut gegangen", "tokens": ["Es", "ist", "mir", "stets", "zu", "gut", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser seid'nen Friedenszeit.", "tokens": ["In", "die\u00b7ser", "sei\u00b7d'\u00b7nen", "Frie\u00b7dens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es hat kein Schmerz mich \u00fcberflutet,", "tokens": ["Es", "hat", "kein", "Schmerz", "mich", "\u00fc\u00b7berf\u00b7lu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat kein Sturm mein Haupt umtobt,", "tokens": ["Es", "hat", "kein", "Sturm", "mein", "Haupt", "um\u00b7tobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat mein Herz noch nie geblutet,", "tokens": ["Es", "hat", "mein", "Herz", "noch", "nie", "ge\u00b7blu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es hat kein Streit mein Schwert erprobt.", "tokens": ["Es", "hat", "kein", "Streit", "mein", "Schwert", "er\u00b7probt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch ward kein Joch mir zum Zerbrechen,", "tokens": ["Noch", "ward", "kein", "Joch", "mir", "zum", "Zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Knoten, um ihn zu durchhaun,", "tokens": ["Kein", "Kno\u00b7ten", ",", "um", "ihn", "zu", "durch\u00b7haun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch keine Schmach, um sie zu r\u00e4chen,", "tokens": ["Noch", "kei\u00b7ne", "Schmach", ",", "um", "sie", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Tod, um ihm ins Aug' zu schaun.", "tokens": ["Kein", "Tod", ",", "um", "ihm", "ins", "Aug'", "zu", "schaun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir ward kein Banner, es zu schirmen,", "tokens": ["Mir", "ward", "kein", "Ban\u00b7ner", ",", "es", "zu", "schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Kranz \u2013 dieweil ich nichts getan,", "tokens": ["Kein", "Kranz", "\u2013", "die\u00b7weil", "ich", "nichts", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KOUS", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ward kein Gipfel zum Erst\u00fcrmen", "tokens": ["Mir", "ward", "kein", "Gip\u00b7fel", "zum", "Er\u00b7st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zum Durchrennen keine Bahn.", "tokens": ["Und", "zum", "Durch\u00b7ren\u00b7nen", "kei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "In der Charybde Strudelwallen", "tokens": ["In", "der", "Cha\u00b7ryb\u00b7de", "Stru\u00b7del\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da taucht' ich freudig lange schon;", "tokens": ["Da", "taucht'", "ich", "freu\u00b7dig", "lan\u00b7ge", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Keiner l\u00e4\u00dft den Becher fallen,", "tokens": ["Doch", "Kei\u00b7ner", "l\u00e4\u00dft", "den", "Be\u00b7cher", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und keine K\u00f6n'gin ist der Lohn.", "tokens": ["Und", "kei\u00b7ne", "K\u00f6n'\u00b7gin", "ist", "der", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich wollt', ich h\u00f6rte Schwerter singen,", "tokens": ["Ich", "wollt'", ",", "ich", "h\u00f6r\u00b7te", "Schwer\u00b7ter", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rte, wie ein Schlachtro\u00df schnauft:", "tokens": ["Und", "h\u00f6r\u00b7te", ",", "wie", "ein", "Schlach\u00b7tro\u00df", "schnauft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wollt' ich in den Sattel springen,", "tokens": ["Wie", "wollt'", "ich", "in", "den", "Sat\u00b7tel", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wo die Zeit mit Blute tauft!", "tokens": ["Da", "wo", "die", "Zeit", "mit", "Blu\u00b7te", "tauft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In Kugelwetter, Speeresdornen,", "tokens": ["In", "Ku\u00b7gel\u00b7wet\u00b7ter", ",", "Spee\u00b7res\u00b7dor\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Tod der Schnitter h\u00e4lt die Mahd,", "tokens": ["Wo", "Tod", "der", "Schnit\u00b7ter", "h\u00e4lt", "die", "Mahd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wollt' ich meinen Renner spornen", "tokens": ["Da", "wollt'", "ich", "mei\u00b7nen", "Ren\u00b7ner", "spor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie in ein k\u00fchlend Rosenbad.", "tokens": ["Wie", "in", "ein", "k\u00fch\u00b7lend", "Ro\u00b7sen\u00b7bad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gebt mir ein Schwert und la\u00dft mich's ziehen", "tokens": ["Gebt", "mir", "ein", "Schwert", "und", "la\u00dft", "mich's", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00f6nnt mir einen einz'gen Schlag,", "tokens": ["Und", "g\u00f6nnt", "mir", "ei\u00b7nen", "einz'\u00b7gen", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seht ihr mich vom Felde fliehen,", "tokens": ["Und", "seht", "ihr", "mich", "vom", "Fel\u00b7de", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sei's mein letzter Lebenstag.", "tokens": ["So", "sei's", "mein", "letz\u00b7ter", "Le\u00b7bens\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Gebt mir den Feind, da\u00df ich ihn schlage,", "tokens": ["Gebt", "mir", "den", "Feind", ",", "da\u00df", "ich", "ihn", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gebt mir den Tod mit seiner Pein,", "tokens": ["Gebt", "mir", "den", "Tod", "mit", "sei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebt Sieg mir, oder Niederlage,", "tokens": ["Gebt", "Sieg", "mir", ",", "o\u00b7der", "Nie\u00b7der\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur la\u00dft es bald gestritten sein!", "tokens": ["Nur", "la\u00dft", "es", "bald", "ge\u00b7strit\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}