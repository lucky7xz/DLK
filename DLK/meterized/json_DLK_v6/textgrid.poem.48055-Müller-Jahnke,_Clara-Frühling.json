{"textgrid.poem.48055": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu meinen F\u00fc\u00dfen im welken Laub", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "im", "wel\u00b7ken", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und mir zu H\u00e4upten singt der Wind", "tokens": ["und", "mir", "zu", "H\u00e4up\u00b7ten", "singt", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in den knospenden Buchenkronen.", "tokens": ["in", "den", "knos\u00b7pen\u00b7den", "Bu\u00b7chen\u00b7kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "bl\u00fchen die Anemonen,", "tokens": ["bl\u00fc\u00b7hen", "die", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ist das ein strahlender Sonnenschein \u2013", "tokens": ["Ist", "das", "ein", "strah\u00b7len\u00b7der", "Son\u00b7nen\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ist das ein wonniges Wetter!", "tokens": ["ist", "das", "ein", "won\u00b7ni\u00b7ges", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Es rauschen unter meinem Fu\u00df", "tokens": ["Es", "rau\u00b7schen", "un\u00b7ter", "mei\u00b7nem", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die abgestorbenen Bl\u00e4tter . . .", "tokens": ["die", "ab\u00b7ge\u00b7stor\u00b7be\u00b7nen", "Bl\u00e4t\u00b7ter", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das ist der lachende Fr\u00fchlingswind,", "tokens": ["Das", "ist", "der", "la\u00b7chen\u00b7de", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der kommt aus dem sonnigen S\u00fcden", "tokens": ["der", "kommt", "aus", "dem", "son\u00b7ni\u00b7gen", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "und gr\u00fc\u00dft von der blauen Adria", "tokens": ["und", "gr\u00fc\u00dft", "von", "der", "blau\u00b7en", "Ad\u00b7ria"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die Wellen, die winterm\u00fcden.", "tokens": ["die", "Wel\u00b7len", ",", "die", "win\u00b7ter\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Das ist der lachende Fr\u00fchlingswind,", "tokens": ["Das", "ist", "der", "la\u00b7chen\u00b7de", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der wandert weiter am Strande", "tokens": ["der", "wan\u00b7dert", "wei\u00b7ter", "am", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und k\u00fc\u00dft noch heute ein einsam Grab", "tokens": ["und", "k\u00fc\u00dft", "noch", "heu\u00b7te", "ein", "ein\u00b7sam", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "im nordischen Nebellande.", "tokens": ["im", "nor\u00b7di\u00b7schen", "Ne\u00b7bel\u00b7lan\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch in den d\u00fcstern Tannenwald", "tokens": ["Auch", "in", "den", "d\u00fcs\u00b7tern", "Tan\u00b7nen\u00b7wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zieht singend K\u00f6nig Fr\u00fchling ein:", "tokens": ["zieht", "sin\u00b7gend", "K\u00f6\u00b7nig", "Fr\u00fch\u00b7ling", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die jungen Knospen lockt er bald,", "tokens": ["die", "jun\u00b7gen", "Knos\u00b7pen", "lockt", "er", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die gl\u00fchn wie Blut im Sonnenschein.", "tokens": ["die", "gl\u00fchn", "wie", "Blut", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Durch die wogende Brust des Waldes geht", "tokens": ["Durch", "die", "wo\u00b7gen\u00b7de", "Brust", "des", "Wal\u00b7des", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ein Atemholen tief und stark \u2013", "tokens": ["ein", "A\u00b7tem\u00b7ho\u00b7len", "tief", "und", "stark", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein Baum nur trauernd seitw\u00e4rts steht,", "tokens": ["ein", "Baum", "nur", "trau\u00b7ernd", "seit\u00b7w\u00e4rts", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den traf der Frost bis tief ins Mark . . .", "tokens": ["den", "traf", "der", "Frost", "bis", "tief", "ins", "Mark", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "APPR", "ADJD", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}