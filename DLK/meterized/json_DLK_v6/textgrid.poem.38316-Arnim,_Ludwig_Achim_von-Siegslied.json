{"textgrid.poem.38316": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Siegslied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fangt an zu singen,", "tokens": ["Fangt", "an", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Trommel r\u00fchren,", "tokens": ["Die", "Trom\u00b7mel", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zertrennt ist Pharaos gro\u00df Heer;", "tokens": ["Zer\u00b7trennt", "ist", "Pha\u00b7raos", "gro\u00df", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "La\u00dft Saiten klingen,", "tokens": ["La\u00dft", "Sai\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und jubiliren,", "tokens": ["Und", "ju\u00b7bi\u00b7li\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.6": {"text": "Verschont hat uns das rothe Meer.", "tokens": ["Ver\u00b7schont", "hat", "uns", "das", "ro\u00b7the", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat nachgelassen", "tokens": ["Hat", "nach\u00b7ge\u00b7las\u00b7sen"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "So stark zu fliessen,", "tokens": ["So", "stark", "zu", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Gestanden wie die Mauren fest;", "tokens": ["Ge\u00b7stan\u00b7den", "wie", "die", "Mau\u00b7ren", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch gute Stra\u00dfen,", "tokens": ["Durch", "gu\u00b7te", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Mit trocknen F\u00fcssen", "tokens": ["Mit", "trock\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Gehn wir hindurch, wir sind getr\u00f6st.", "tokens": ["Gehn", "wir", "hin\u00b7durch", ",", "wir", "sind", "ge\u00b7tr\u00f6st", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Will Moses f\u00fchren", "tokens": ["Will", "Mo\u00b7ses", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das Heer der M\u00e4nner,", "tokens": ["Das", "Heer", "der", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Kommt ihr zu mir ihr Jungfr\u00e4ulein;", "tokens": ["Kommt", "ihr", "zu", "mir", "ihr", "Jung\u00b7fr\u00e4u\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Heer zu zieren,", "tokens": ["Mein", "Heer", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Trotz euch Bekenner,", "tokens": ["Trotz", "euch", "Be\u00b7ken\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bin Aron ich die Schwester dein.", "tokens": ["Bin", "A\u00b7ron", "ich", "die", "Schwes\u00b7ter", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil wir entronnen", "tokens": ["Weil", "wir", "ent\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF"], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Den Wasserwellen,", "tokens": ["Den", "Was\u00b7ser\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Sollt ihr der h\u00f6chsten Majest\u00e4t", "tokens": ["Sollt", "ihr", "der", "h\u00f6chs\u00b7ten", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So viel verg\u00f6nnen,", "tokens": ["So", "viel", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ein Fest anstellen,", "tokens": ["Ein", "Fest", "an\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und singen, da\u00df der Osten weht.", "tokens": ["Und", "sin\u00b7gen", ",", "da\u00df", "der", "Os\u00b7ten", "weht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Thau wird fallen,", "tokens": ["Der", "Thau", "wird", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und euch begiessen,", "tokens": ["Und", "euch", "be\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Herab vom hohen Himmelsbau;", "tokens": ["Her\u00b7ab", "vom", "ho\u00b7hen", "Him\u00b7mels\u00b7bau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sollt vor allen", "tokens": ["Ihr", "sollt", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Das Herz erschliessen,", "tokens": ["Das", "Herz", "er\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dem Honig s\u00fcssen Himmelthau.", "tokens": ["Dem", "Ho\u00b7nig", "s\u00fcs\u00b7sen", "Him\u00b7melt\u00b7hau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann wird benetzet,", "tokens": ["Dann", "wird", "be\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Was vor geblieben,", "tokens": ["Was", "vor", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "APPR", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und ohne dies wohl Frucht gebracht;", "tokens": ["Und", "oh\u00b7ne", "dies", "wohl", "Frucht", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zugleich erg\u00f6tzet,", "tokens": ["Zu\u00b7gleich", "er\u00b7g\u00f6t\u00b7zet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Mehr angetrieben,", "tokens": ["Mehr", "an\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Was ausgedorret und verschmacht.", "tokens": ["Was", "aus\u00b7ge\u00b7dor\u00b7ret", "und", "ver\u00b7schmacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}