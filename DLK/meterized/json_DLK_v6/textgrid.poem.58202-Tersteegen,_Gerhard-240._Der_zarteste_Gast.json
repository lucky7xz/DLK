{"textgrid.poem.58202": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "240. Der zarteste Gast", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott wohnet gar zu gern in schlichten Menschenkindern,", "tokens": ["Gott", "woh\u00b7net", "gar", "zu", "gern", "in", "schlich\u00b7ten", "Men\u00b7schen\u00b7kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKA", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kann das mindste Ding den zarten Gast schon hindern,", "tokens": ["Doch", "kann", "das", "minds\u00b7te", "Ding", "den", "zar\u00b7ten", "Gast", "schon", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein kleiner Eigenwill', ein heimlich L\u00fcstchen st\u00f6rt;", "tokens": ["Ein", "klei\u00b7ner", "Ei\u00b7gen\u00b7will'", ",", "ein", "heim\u00b7lich", "L\u00fcst\u00b7chen", "st\u00f6rt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie rein mu\u00df der nicht sein, der diesen Gast begehrt!", "tokens": ["Wie", "rein", "mu\u00df", "der", "nicht", "sein", ",", "der", "die\u00b7sen", "Gast", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ART", "PTKNEG", "VAINF", "$,", "PRELS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}