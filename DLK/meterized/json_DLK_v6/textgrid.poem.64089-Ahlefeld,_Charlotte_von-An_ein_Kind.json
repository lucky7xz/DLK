{"textgrid.poem.64089": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "An ein Kind", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ungetr\u00fcbt und klar und helle", "tokens": ["Un\u00b7ge\u00b7tr\u00fcbt", "und", "klar", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rinnet noch des Lebens reine Quelle,", "tokens": ["Rin\u00b7net", "noch", "des", "Le\u00b7bens", "rei\u00b7ne", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zwischen Blumenufern Dir dahin.", "tokens": ["Zwi\u00b7schen", "Blu\u00b7me\u00b7nu\u00b7fern", "Dir", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Morgenroth gl\u00e4nzt auf den stillen Fluthen,", "tokens": ["Mor\u00b7gen\u00b7roth", "gl\u00e4nzt", "auf", "den", "stil\u00b7len", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und im Wiederschein der Purpurgluthen", "tokens": ["Und", "im", "Wie\u00b7der\u00b7schein", "der", "Pur\u00b7pur\u00b7glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Spiegelt sich Dein unbefangner Sinn.", "tokens": ["Spie\u00b7gelt", "sich", "Dein", "un\u00b7be\u00b7fang\u00b7ner", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber bald umw\u00f6lkt ein tr\u00fcber Schleier", "tokens": ["A\u00b7ber", "bald", "um\u00b7w\u00f6lkt", "ein", "tr\u00fc\u00b7ber", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dir des Daseyns jugendliche Feier,", "tokens": ["Dir", "des", "Da\u00b7seyns", "ju\u00b7gend\u00b7li\u00b7che", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und Aurora's milder Glanz verbleicht.", "tokens": ["Und", "Au\u00b7rora's", "mil\u00b7der", "Glanz", "ver\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wolken ziehn sich \u00fcber Dir zusammen,", "tokens": ["Wol\u00b7ken", "ziehn", "sich", "\u00fc\u00b7ber", "Dir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Denn der Leidenschaften wilde Flammen", "tokens": ["Denn", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", "wil\u00b7de", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Haben bald die zarte Brust erreicht.", "tokens": ["Ha\u00b7ben", "bald", "die", "zar\u00b7te", "Brust", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und verlohren ist der goldne Frieden,", "tokens": ["Und", "ver\u00b7loh\u00b7ren", "ist", "der", "gold\u00b7ne", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der der Kindheit nur allein beschieden,", "tokens": ["Der", "der", "Kind\u00b7heit", "nur", "al\u00b7lein", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn Dir ", "tokens": ["Wenn", "Dir"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Hat er einmahl Dir das Herz durchdrungen,", "tokens": ["Hat", "er", "ein\u00b7mahl", "Dir", "das", "Herz", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Machen selbst des Erdballs Huldigungen", "tokens": ["Ma\u00b7chen", "selbst", "des", "Erd\u00b7balls", "Hul\u00b7di\u00b7gun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nicht des Busens tiefe Wunde beil.", "tokens": ["Nicht", "des", "Bu\u00b7sens", "tie\u00b7fe", "Wun\u00b7de", "beil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Bl\u00fche Deinem Schicksal denn entgegen,", "tokens": ["Bl\u00fc\u00b7he", "Dei\u00b7nem", "Schick\u00b7sal", "denn", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und wenn St\u00fcrme schauernd Dich bewegen,", "tokens": ["Und", "wenn", "St\u00fcr\u00b7me", "schau\u00b7ernd", "Dich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wanke dennoch nimmermehr Dein Sinn.", "tokens": ["Wan\u00b7ke", "den\u00b7noch", "nim\u00b7mer\u00b7mehr", "Dein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kraft und Muth siegt \u00fcber bittre Leiden,", "tokens": ["Kraft", "und", "Muth", "siegt", "\u00fc\u00b7ber", "bitt\u00b7re", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und wenn alle Rosen von Dir scheiden,", "tokens": ["Und", "wenn", "al\u00b7le", "Ro\u00b7sen", "von", "Dir", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Welke nie der Hoffnung Gr\u00fcn Dir hin.", "tokens": ["Wel\u00b7ke", "nie", "der", "Hoff\u00b7nung", "Gr\u00fcn", "Dir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}