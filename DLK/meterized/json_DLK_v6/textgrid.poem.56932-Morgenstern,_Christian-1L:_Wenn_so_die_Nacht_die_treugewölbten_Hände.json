{"textgrid.poem.56932": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn so die Nacht die treugew\u00f6lbten H\u00e4nde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn so die Nacht die treugew\u00f6lbten H\u00e4nde", "tokens": ["Wenn", "so", "die", "Nacht", "die", "treu\u00b7ge\u00b7w\u00f6lb\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von ihrer Erde stillem Antlitz hebt,", "tokens": ["von", "ih\u00b7rer", "Er\u00b7de", "stil\u00b7lem", "Ant\u00b7litz", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und in die k\u00fchlen, duftenden Gel\u00e4nde", "tokens": ["und", "in", "die", "k\u00fch\u00b7len", ",", "duf\u00b7ten\u00b7den", "Ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der erste Hauch des jungen Morgens bebt \u2013", "tokens": ["der", "ers\u00b7te", "Hauch", "des", "jun\u00b7gen", "Mor\u00b7gens", "bebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "da la\u00df uns Arm in Arm nach Osten gehen", "tokens": ["da", "la\u00df", "uns", "Arm", "in", "Arm", "nach", "Os\u00b7ten", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "NN", "APPR", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bis vor das Tor der gro\u00dfen, stummen Stadt,", "tokens": ["bis", "vor", "das", "Tor", "der", "gro\u00b7\u00dfen", ",", "stum\u00b7men", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Schl\u00e4f' an Schl\u00e4f' die junge Sonne sehen,", "tokens": ["und", "Schl\u00e4f'", "an", "Schl\u00e4f'", "die", "jun\u00b7ge", "Son\u00b7ne", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die uns so s\u00fc\u00dfem Sein erschaffen hat.", "tokens": ["die", "uns", "so", "s\u00fc\u00b7\u00dfem", "Sein", "er\u00b7schaf\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}