{"dta.poem.11878": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O seht die Taube, wenn ihr ihre Jungen schlachtet,", "tokens": ["O", "seht", "die", "Tau\u00b7be", ",", "wenn", "ihr", "ih\u00b7re", "Jun\u00b7gen", "schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Schlag verl\u00e4\u00dft sie nicht, wo ihr das Nest ihr machtet.", "tokens": ["Den", "Schlag", "ver\u00b7l\u00e4\u00dft", "sie", "nicht", ",", "wo", "ihr", "das", "Nest", "ihr", "mach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie wehrt sich nicht, noch klagt, wenn man ihr Liebstes raubt,", "tokens": ["Sie", "wehrt", "sich", "nicht", ",", "noch", "klagt", ",", "wenn", "man", "ihr", "Liebs\u00b7tes", "raubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "ADV", "VVFIN", "$,", "KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zufrieden, wenn man nur das Daseyn ihr erlaubt.", "tokens": ["Zu\u00b7frie\u00b7den", ",", "wenn", "man", "nur", "das", "Da\u00b7seyn", "ihr", "er\u00b7laubt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nicht, ob ein Bild der vollen Sklaverei,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ob", "ein", "Bild", "der", "vol\u00b7len", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob der Ergebung sie vollkommnes Muster sei.", "tokens": ["Ob", "der", "Er\u00b7ge\u00b7bung", "sie", "voll\u00b7komm\u00b7nes", "Mus\u00b7ter", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}