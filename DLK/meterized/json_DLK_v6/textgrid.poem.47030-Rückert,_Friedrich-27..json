{"textgrid.poem.47030": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O die du lebest mir mit deinem Grolle,", "tokens": ["O", "die", "du", "le\u00b7best", "mir", "mit", "dei\u00b7nem", "Grol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRELS", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ich mit meiner Liebe dir zur Plage;", "tokens": ["Wie", "ich", "mit", "mei\u00b7ner", "Lie\u00b7be", "dir", "zur", "Pla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun geh' ich schon um dich so lange Tage,", "tokens": ["Nun", "geh'", "ich", "schon", "um", "dich", "so", "lan\u00b7ge", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und glaubst du noch nicht, da\u00df ich wohl dir wolle?", "tokens": ["Und", "glaubst", "du", "noch", "nicht", ",", "da\u00df", "ich", "wohl", "dir", "wol\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbwer wei\u00df.\u00ab Wer wei\u00df? Ei, du sollst's wissen, Tolle!", "tokens": ["\u00bb", "wer", "wei\u00df", ".", "\u00ab", "Wer", "wei\u00df", "?", "Ei", ",", "du", "sollst's", "wis\u00b7sen", ",", "Tol\u00b7le", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$.", "$(", "PWS", "VVFIN", "$.", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun sage das nur, ob dir's denn behage,", "tokens": ["Nun", "sa\u00b7ge", "das", "nur", ",", "ob", "dir's", "denn", "be\u00b7ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du mich um dich gehen siehst? \u00bbDas sage", "tokens": ["Da\u00df", "du", "mich", "um", "dich", "ge\u00b7hen", "siehst", "?", "\u00bb", "Das", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PRF", "VVINF", "VVFIN", "$.", "$(", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich nicht.\u00ab So sag' denn, da\u00df ich's lassen solle.", "tokens": ["Ich", "nicht", ".", "\u00ab", "So", "sag'", "denn", ",", "da\u00df", "ich's", "las\u00b7sen", "sol\u00b7le", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "$(", "ADV", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbdas sag' ich auch nicht.\u00ab Nun, beim Flor des Sarges!", "tokens": ["\u00bb", "das", "sag'", "ich", "auch", "nicht", ".", "\u00ab", "Nun", ",", "beim", "Flor", "des", "Sar\u00b7ges", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "$(", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du t\u00f6test mich; so sage doch nur, was du", "tokens": ["Du", "t\u00f6\u00b7test", "mich", ";", "so", "sa\u00b7ge", "doch", "nur", ",", "was", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ADV", "ADV", "$,", "PWS", "PPER"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn \u00fcberhaupt mir sagest? \u00bbGar nichts sag' ich.\u00ab", "tokens": ["Denn", "\u00fc\u00b7ber\u00b7haupt", "mir", "sa\u00b7gest", "?", "\u00bb", "Gar", "nichts", "sag'", "ich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$.", "$(", "ADV", "PIS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Bei Gott! so wollt' ich, da\u00df du doch recht Arges", "tokens": ["Bei", "Gott", "!", "so", "wollt'", "ich", ",", "da\u00df", "du", "doch", "recht", "Ar\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir sagtest, statt so nichts zu sagen. Da\u00df du", "tokens": ["Mir", "sag\u00b7test", ",", "statt", "so", "nichts", "zu", "sa\u00b7gen", ".", "Da\u00df", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ADV", "PIS", "PTKZU", "VVINF", "$.", "KOUS", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So gar nichts sagend mir so viel sagst, klag' ich.", "tokens": ["So", "gar", "nichts", "sa\u00b7gend", "mir", "so", "viel", "sagst", ",", "klag'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "PPER", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}