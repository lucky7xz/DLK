{"textgrid.poem.52740": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Missionshymne", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, der du die das Menschenalter", "tokens": ["Du", ",", "der", "du", "die", "das", "Men\u00b7schen\u00b7al\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcllende Zeit erschufst,", "tokens": ["Er\u00b7f\u00fcl\u00b7len\u00b7de", "Zeit", "er\u00b7schufst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du edler Raum- und Zeitverwalter,", "tokens": ["Du", "ed\u00b7ler", "Raum", "und", "Zeit\u00b7ver\u00b7wal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du selber Ewigkeitsausgestrahlter,", "tokens": ["Du", "sel\u00b7ber", "E\u00b7wig\u00b7keits\u00b7aus\u00b7ge\u00b7strahl\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dir singen wir den Davidspsalter,", "tokens": ["Dir", "sin\u00b7gen", "wir", "den", "Da\u00b7vids\u00b7psal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du dein Volk berufst.", "tokens": ["Wenn", "du", "dein", "Volk", "be\u00b7rufst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hallelujah!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Kaffern, Mohren, Hottentotten,", "tokens": ["Kaf\u00b7fern", ",", "Moh\u00b7ren", ",", "Hot\u00b7ten\u00b7tot\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aller Heiden S\u00fcnderrotten,", "tokens": ["Al\u00b7ler", "Hei\u00b7den", "S\u00fcn\u00b7der\u00b7rot\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In Zions Zelt f\u00fchrst du sie ja!", "tokens": ["In", "Zi\u00b7ons", "Zelt", "f\u00fchrst", "du", "sie", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit Vaterhuld und Mutterarmen,", "tokens": ["Mit", "Va\u00b7ter\u00b7huld", "und", "Mut\u00b7ter\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umfassest du die gro\u00dfe, gro\u00dfe Welt,", "tokens": ["Um\u00b7fas\u00b7sest", "du", "die", "gro\u00b7\u00dfe", ",", "gro\u00b7\u00dfe", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast mit dem Thiere selbst Erbarmen,", "tokens": ["Hast", "mit", "dem", "Thie\u00b7re", "selbst", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es darf an deiner Brust erwarmen \u2013", "tokens": ["Es", "darf", "an", "dei\u00b7ner", "Brust", "er\u00b7war\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst auch die Erkenntni\u00dfarmen", "tokens": ["Du", "wirst", "auch", "die", "Er\u00b7kennt\u00b7ni\u00df\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchren in Zions Zelt.", "tokens": ["F\u00fch\u00b7ren", "in", "Zi\u00b7ons", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Hallelujah!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Kaffern, Mohren, Hottentotten,", "tokens": ["Kaf\u00b7fern", ",", "Moh\u00b7ren", ",", "Hot\u00b7ten\u00b7tot\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aller Heiden S\u00fcnderrotten", "tokens": ["Al\u00b7ler", "Hei\u00b7den", "S\u00fcn\u00b7der\u00b7rot\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In Zions Zelt f\u00fchrst du sie ja!", "tokens": ["In", "Zi\u00b7ons", "Zelt", "f\u00fchrst", "du", "sie", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}