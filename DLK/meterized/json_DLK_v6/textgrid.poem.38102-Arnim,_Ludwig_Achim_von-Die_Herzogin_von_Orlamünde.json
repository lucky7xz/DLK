{"textgrid.poem.38102": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Herzogin von Orlam\u00fcnde", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Albert Graf von N\u00fcrnberg spricht:", "tokens": ["Al\u00b7bert", "Graf", "von", "N\u00fcrn\u00b7berg", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbherzogin ich liebe nicht;", "tokens": ["\u00bb", "her\u00b7zo\u00b7gin", "ich", "lie\u00b7be", "nicht", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bin ein Kind von achtzehn Jahren", "tokens": ["Bin", "ein", "Kind", "von", "acht\u00b7zehn", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im Lieben unerfahren,", "tokens": ["Und", "im", "Lie\u00b7ben", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00fcrde doch zum Weib dich nehmen,", "tokens": ["W\u00fcr\u00b7de", "doch", "zum", "Weib", "dich", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch vier Augen mich besch\u00e4men;", "tokens": ["Doch", "vier", "Au\u00b7gen", "mich", "be\u00b7sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn nicht hier vier Augen w\u00e4ren,", "tokens": ["Wenn", "nicht", "hier", "vier", "Au\u00b7gen", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "CARD", "NN", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die das Herze mein beschweren.\u00ab", "tokens": ["Die", "das", "Her\u00b7ze", "mein", "be\u00b7schwe\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PDS", "VVFIN", "PPOSAT", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Orlam\u00fcndens Herzogin", "tokens": ["Or\u00b7la\u00b7m\u00fcn\u00b7dens", "Her\u00b7zo\u00b7gin"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht zu sich in ihrem Sinn:", "tokens": ["Spricht", "zu", "sich", "in", "ih\u00b7rem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbwitwe bin ich sch\u00f6n vor allen,", "tokens": ["\u00bb", "wit\u00b7we", "bin", "ich", "sch\u00f6n", "vor", "al\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller F\u00fcrsten Wohlgefallen;", "tokens": ["Al\u00b7ler", "F\u00fcrs\u00b7ten", "Wohl\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn nicht hier vier Augen w\u00e4ren,", "tokens": ["Wenn", "nicht", "hier", "vier", "Au\u00b7gen", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "CARD", "NN", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "W\u00fcrde seine Lieb mich ehren.\u00ab", "tokens": ["W\u00fcr\u00b7de", "sei\u00b7ne", "Lieb", "mich", "eh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbkinder ihr vom schlechten Mann,", "tokens": ["\u00bb", "kin\u00b7der", "ihr", "vom", "schlech\u00b7ten", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mich hielt in strengem Bann;", "tokens": ["Der", "mich", "hielt", "in", "stren\u00b7gem", "Bann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weil ihr meine Land ererbet", "tokens": ["Weil", "ihr", "mei\u00b7ne", "Land", "er\u00b7er\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr nicht unm\u00fcndig sterbet.\u00ab", "tokens": ["Wenn", "ihr", "nicht", "un\u00b7m\u00fcn\u00b7dig", "ster\u00b7bet", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Also Oehl in Flammen w\u00fcthet,", "tokens": ["Al\u00b7so", "O\u00b7ehl", "in", "Flam\u00b7men", "w\u00fct\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das statt Wasser aufgesch\u00fcttet.", "tokens": ["Das", "statt", "Was\u00b7ser", "auf\u00b7ge\u00b7sch\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Also deutet sie die Rede", "tokens": ["Al\u00b7so", "deu\u00b7tet", "sie", "die", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf zwey eigne Kinder schn\u00f6de,", "tokens": ["Auf", "zwey", "eig\u00b7ne", "Kin\u00b7der", "schn\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Die im Saal zum Spiel abz\u00e4hlen,", "tokens": ["Die", "im", "Saal", "zum", "Spiel", "ab\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPRART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter sich den Engel w\u00e4hlen:", "tokens": ["Un\u00b7ter", "sich", "den", "En\u00b7gel", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbengel, Bengel, la\u00df mich leben,", "tokens": ["\u00bb", "en\u00b7gel", ",", "Ben\u00b7gel", ",", "la\u00df", "mich", "le\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich will dir den Vogel geben.\u00ab", "tokens": ["Ich", "will", "dir", "den", "Vo\u00b7gel", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Nadeln aus dem Wittibschleyer", "tokens": ["Na\u00b7deln", "aus", "dem", "Wit\u00b7tib\u00b7schle\u00b7yer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Zieht sie, da\u00df er falle freyer,", "tokens": ["Zieht", "sie", ",", "da\u00df", "er", "fal\u00b7le", "frey\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Zu dem wilden Hager spricht:", "tokens": ["Zu", "dem", "wil\u00b7den", "Ha\u00b7ger", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbnimm die Nadeln und verricht,", "tokens": ["\u00bb", "nimm", "die", "Na\u00b7deln", "und", "ver\u00b7richt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Schwarzer Hager, du mein Freyer", "tokens": ["Schwar\u00b7zer", "Ha\u00b7ger", ",", "du", "mein", "Frey\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcrchtest nicht den schwarzen Schleyer,", "tokens": ["F\u00fcrch\u00b7test", "nicht", "den", "schwar\u00b7zen", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.17": {"line.1": {"text": "F\u00fcrchtest du nicht auch vier Augen,", "tokens": ["F\u00fcrch\u00b7test", "du", "nicht", "auch", "vier", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die zum Zusehn hier nicht taugen,", "tokens": ["Die", "zum", "Zu\u00b7sehn", "hier", "nicht", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Setz' dich mit zu ihren Spielen,", "tokens": ["Setz'", "dich", "mit", "zu", "ih\u00b7ren", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie keine Schmerzen f\u00fchlen,", "tokens": ["Da\u00df", "sie", "kei\u00b7ne", "Schmer\u00b7zen", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Da\u00df die Wunden niemals sprechen,", "tokens": ["Da\u00df", "die", "Wun\u00b7den", "nie\u00b7mals", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Must du in das Hirn sie stechen.\u00ab", "tokens": ["Must", "du", "in", "das", "Hirn", "sie", "ste\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Herulus zum Hager spricht,", "tokens": ["He\u00b7ru\u00b7lus", "zum", "Ha\u00b7ger", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh der ihm das Hirn einsticht:", "tokens": ["Eh", "der", "ihm", "das", "Hirn", "ein\u00b7sticht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "\u00bblieber Hager, la\u00df mich leben,", "tokens": ["\u00bb", "lie\u00b7ber", "Ha\u00b7ger", ",", "la\u00df", "mich", "le\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will dir Orlam\u00fcnde geben,", "tokens": ["Will", "dir", "Or\u00b7la\u00b7m\u00fcn\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Auch die Plassenburg die neue,", "tokens": ["Auch", "die", "Plas\u00b7sen\u00b7burg", "die", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es soll mich nicht gereuen.\u00ab", "tokens": ["Und", "es", "soll", "mich", "nicht", "ge\u00b7reu\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Herula zum Hager spricht,", "tokens": ["He\u00b7ru\u00b7la", "zum", "Ha\u00b7ger", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh er ihr das Hirn einsticht:", "tokens": ["Eh", "er", "ihr", "das", "Hirn", "ein\u00b7sticht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "\u00bblieber Hager la\u00df mich leben,", "tokens": ["\u00bb", "lie\u00b7ber", "Ha\u00b7ger", "la\u00df", "mich", "le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will dir meine Docken geben,", "tokens": ["Will", "dir", "mei\u00b7ne", "Do\u00b7cken", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Engel, Bengel la\u00df mich leben,", "tokens": ["En\u00b7gel", ",", "Ben\u00b7gel", "la\u00df", "mich", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will dir meinen Vogel geben.\u00ab", "tokens": ["Will", "dir", "mei\u00b7nen", "Vo\u00b7gel", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Hager sich als M\u00f6rder nennt,", "tokens": ["Ha\u00b7ger", "sich", "als", "M\u00f6r\u00b7der", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh er sich das Hirn einrennt.", "tokens": ["Eh", "er", "sich", "das", "Hirn", "ein\u00b7rennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "\u00bbgott ach Gott, wo werd ich ruhen,", "tokens": ["\u00bb", "gott", "ach", "Gott", ",", "wo", "werd", "ich", "ru\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "XY", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re schon den Vogel rufen,", "tokens": ["H\u00f6\u00b7re", "schon", "den", "Vo\u00b7gel", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Gott ach Gott, wo soll ich fliehen,", "tokens": ["Gott", "ach", "Gott", ",", "wo", "soll", "ich", "flie\u00b7hen", ","], "token_info": ["word", "word", "wor