{"textgrid.poem.49444": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[nicht k\u00f6nnen die Dome]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht k\u00f6nnen die Dome", "tokens": ["Nicht", "k\u00f6n\u00b7nen", "die", "Do\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem Meer sich vergleichen,", "tokens": ["Dem", "Meer", "sich", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In wiegendem Strome", "tokens": ["In", "wie\u00b7gen\u00b7dem", "Stro\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hegt sanft es die Leichen.", "tokens": ["Hegt", "sanft", "es", "die", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Es geben die Meere", "tokens": ["Es", "ge\u00b7ben", "die", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Jungfrau die Ehre.", "tokens": ["Der", "Jung\u00b7frau", "die", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Alle Gaben sind sein,", "tokens": ["Al\u00b7le", "Ga\u00b7ben", "sind", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VAINF", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ob schrecklich, ob hold,", "tokens": ["Ob", "schreck\u00b7lich", ",", "ob", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mild rauscht sein Verzeihn,", "tokens": ["Mild", "rauscht", "sein", "Ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sein Zornbrausen grollt.", "tokens": ["Sein", "Zorn\u00b7brau\u00b7sen", "grollt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie atmet es sacht", "tokens": ["Wie", "at\u00b7met", "es", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "In endloser Pracht.", "tokens": ["In", "end\u00b7lo\u00b7ser", "Pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie ist es geduldig,", "tokens": ["Wie", "ist", "es", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ob schlimm auch und schuldig,", "tokens": ["Ob", "schlimm", "auch", "und", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vom Wind \u00fcberflogen", "tokens": ["Vom", "Wind", "\u00fc\u00b7berf\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "H\u00f6rt singen die Wogen:", "tokens": ["H\u00f6rt", "sin\u00b7gen", "die", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "\u00bbihr trostlosen Herzen", "tokens": ["\u00bb", "ihr", "trost\u00b7lo\u00b7sen", "Her\u00b7zen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sterbt hin ohne Schmerzen.\u00ab", "tokens": ["Sterbt", "hin", "oh\u00b7ne", "Schmer\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Im l\u00e4chelnden Schein", "tokens": ["Im", "l\u00e4\u00b7cheln\u00b7den", "Schein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Des \u00c4thers erbl\u00fchn", "tokens": ["Des", "\u00c4\u00b7thers", "er\u00b7bl\u00fchn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Farben so rein,", "tokens": ["Die", "Far\u00b7ben", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Blau, rosig und gr\u00fcn.", "tokens": ["Blau", ",", "ro\u00b7sig", "und", "gr\u00fcn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "In herrlichster Zier", "tokens": ["In", "herr\u00b7lichs\u00b7ter", "Zier"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und besser als wir!", "tokens": ["Und", "bes\u00b7ser", "als", "wir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}