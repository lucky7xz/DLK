{"textgrid.poem.41331": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00fcngling", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wird der junge Herr von seinem Mentor frei.", "tokens": ["Nun", "wird", "der", "jun\u00b7ge", "Herr", "von", "sei\u00b7nem", "Men\u00b7tor", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie froh ist ihm die Welt, und die Natur wie neu!", "tokens": ["Wie", "froh", "ist", "ihm", "die", "Welt", ",", "und", "die", "Na\u00b7tur", "wie", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun sucht er Luft und Lust, schweift aus, flucht allem Zwange:", "tokens": ["Nun", "sucht", "er", "Luft", "und", "Lust", ",", "schweift", "aus", ",", "flucht", "al\u00b7lem", "Zwan\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verschwendet hoffnungsreich: ist zornig, doch nicht lange;", "tokens": ["Ver\u00b7schwen\u00b7det", "hoff\u00b7nungs\u00b7reich", ":", "ist", "zor\u00b7nig", ",", "doch", "nicht", "lan\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "VAFIN", "ADJD", "$,", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Oft scherzhaft, selten klug: voll Spr\u00fcnge, wie sein Gaul.", "tokens": ["Oft", "scherz\u00b7haft", ",", "sel\u00b7ten", "klug", ":", "voll", "Spr\u00fcn\u00b7ge", ",", "wie", "sein", "Gaul", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "ADJD", "$.", "ADJD", "NN", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf Tanz und Jagd erhitzt: zu k\u00fchler Arbeit faul.", "tokens": ["Auf", "Tanz", "und", "Jagd", "er\u00b7hitzt", ":", "zu", "k\u00fch\u00b7ler", "Ar\u00b7beit", "faul", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur Chloris unterthan, die ihn so sch\u00f6n regieret,", "tokens": ["Nur", "Chlo\u00b7ris", "un\u00b7ter\u00b7than", ",", "die", "ihn", "so", "sch\u00f6n", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJD", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bis ihren Augen ihn Serpinens Wink entf\u00fchret,", "tokens": ["Bis", "ih\u00b7ren", "Au\u00b7gen", "ihn", "Ser\u00b7pi\u00b7nens", "Wink", "ent\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Dem ihn Elisa raubt. Sein Herz wird \u00fcbereilt,", "tokens": ["Dem", "ihn", "E\u00b7li\u00b7sa", "raubt", ".", "Sein", "Herz", "wird", "\u00fc\u00b7be\u00b7reilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "Das seine Weichlichkeit mit zwanzig Freunden theilt.", "tokens": ["Das", "sei\u00b7ne", "Weich\u00b7lich\u00b7keit", "mit", "zwan\u00b7zig", "Freun\u00b7den", "theilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er w\u00e4hlt un\u00fcberlegt, bleibt keiner Wahl ergeben,", "tokens": ["Er", "w\u00e4hlt", "un\u00b7\u00fc\u00b7ber\u00b7legt", ",", "bleibt", "kei\u00b7ner", "Wahl", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und denkt kaum an den Tod, und lebt nur, um zu leben.", "tokens": ["Und", "denkt", "kaum", "an", "den", "Tod", ",", "und", "lebt", "nur", ",", "um", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}