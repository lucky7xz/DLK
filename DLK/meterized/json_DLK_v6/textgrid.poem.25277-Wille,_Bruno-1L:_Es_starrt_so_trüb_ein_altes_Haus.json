{"textgrid.poem.25277": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es starrt so tr\u00fcb ein altes Haus", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es starrt so tr\u00fcb ein altes Haus", "tokens": ["Es", "starrt", "so", "tr\u00fcb", "ein", "al\u00b7tes", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wintergraue Fernen hinaus.", "tokens": ["In", "win\u00b7ter\u00b7grau\u00b7e", "Fer\u00b7nen", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "In sich versunken, erbebt es bang", "tokens": ["In", "sich", "ver\u00b7sun\u00b7ken", ",", "er\u00b7bebt", "es", "bang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "VVINF", "$,", "VVFIN", "PPER", "ADJD"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von dumpf verschollenem Glockenklang.", "tokens": ["Von", "dumpf", "ver\u00b7schol\u00b7le\u00b7nem", "Glo\u00b7cken\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf einmal gleitet z\u00e4rtliches Licht", "tokens": ["Auf", "ein\u00b7mal", "glei\u00b7tet", "z\u00e4rt\u00b7li\u00b7ches", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihm tastend \u00fcber das Angesicht.", "tokens": ["Ihm", "tas\u00b7tend", "\u00fc\u00b7ber", "das", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Wintersonne mit mattem Schein", "tokens": ["Die", "Win\u00b7ter\u00b7son\u00b7ne", "mit", "mat\u00b7tem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00fc\u00dft scheidend Giebel und Fensterlein.", "tokens": ["K\u00fc\u00dft", "schei\u00b7dend", "Gie\u00b7bel", "und", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wehm\u00fctig l\u00e4chelt ihr zitternder Strahl:", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "l\u00e4\u00b7chelt", "ihr", "zit\u00b7tern\u00b7der", "Strahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "\u00bbgedenkst du noch? Es war einmal ...\u00ab", "tokens": ["\u00bb", "ge\u00b7denkst", "du", "noch", "?", "Es", "war", "ein\u00b7mal", "...", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "ADV", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann h\u00fcllt sie das Haupt in Schneegew\u00f6lk ein", "tokens": ["Dann", "h\u00fcllt", "sie", "das", "Haupt", "in", "Schnee\u00b7ge\u00b7w\u00f6lk", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "ART"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und l\u00e4\u00dft das Haus im Dunkeln, allein.", "tokens": ["Und", "l\u00e4\u00dft", "das", "Haus", "im", "Dun\u00b7keln", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$,", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}