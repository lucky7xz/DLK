{"textgrid.poem.57865": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcssekraut", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den gr\u00fcnen Wald bin ich gegangen,", "tokens": ["In", "den", "gr\u00fc\u00b7nen", "Wald", "bin", "ich", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo das Rotkehlchen sang,", "tokens": ["Wo", "das", "Rot\u00b7kehl\u00b7chen", "sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein St\u00fcndlein, kleines St\u00fcndlein,", "tokens": ["Ein", "St\u00fcnd\u00b7lein", ",", "klei\u00b7nes", "St\u00fcnd\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch zwei dreie lang.", "tokens": ["Auch", "zwei", "drei\u00b7e", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Unterm Liebholz hab' ich gesessen,", "tokens": ["Un\u00b7term", "Lieb\u00b7holz", "hab'", "ich", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Habe K\u00fcssekraut gepfl\u00fcckt;", "tokens": ["Ha\u00b7be", "K\u00fcs\u00b7se\u00b7kraut", "ge\u00b7pfl\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat mein Liebster, Allerliebster", "tokens": ["Hat", "mein", "Liebs\u00b7ter", ",", "Al\u00b7ler\u00b7liebs\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An das Herz mich gedr\u00fcckt.", "tokens": ["An", "das", "Herz", "mich", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und er hat mich liebkoset", "tokens": ["Und", "er", "hat", "mich", "lieb\u00b7ko\u00b7set"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Mund und mit Hand;", "tokens": ["Mit", "Mund", "und", "mit", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sang ein V\u00f6glein, kleines V\u00f6glein", "tokens": ["Sang", "ein", "V\u00f6\u00b7glein", ",", "klei\u00b7nes", "V\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Lied ich verstand.", "tokens": ["Und", "das", "Lied", "ich", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und das Lied hat gehei\u00dfen,", "tokens": ["Und", "das", "Lied", "hat", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und das Lied und das hie\u00df:", "tokens": ["Und", "das", "Lied", "und", "das", "hie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "PDS", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach die Liebe, s\u00fc\u00dfe Liebe", "tokens": ["Ach", "die", "Lie\u00b7be", ",", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ART", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die schmeckt ja so s\u00fc\u00df.", "tokens": ["Und", "die", "schmeckt", "ja", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Will jetzt K\u00fcssekraut pfl\u00fccken", "tokens": ["Will", "jetzt", "K\u00fcs\u00b7se\u00b7kraut", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Bei Tag und bei Nacht;", "tokens": ["Bei", "Tag", "und", "bei", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Denn zum K\u00fcssen, ach K\u00fcssen", "tokens": ["Denn", "zum", "K\u00fcs\u00b7sen", ",", "ach", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "XY", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sind wir M\u00e4dchen gemacht.", "tokens": ["Sind", "wir", "M\u00e4d\u00b7chen", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}