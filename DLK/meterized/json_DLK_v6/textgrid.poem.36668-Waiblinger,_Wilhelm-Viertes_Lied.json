{"textgrid.poem.36668": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Viertes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Stunde des Tages aber weiht' ich", "tokens": ["Ei\u00b7ne", "Stun\u00b7de", "des", "Ta\u00b7ges", "a\u00b7ber", "weiht'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "PPER"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dir, o Loggia! Des Morgens, wenn die Sonne", "tokens": ["Dir", ",", "o", "Log\u00b7gia", "!", "Des", "Mor\u00b7gens", ",", "wenn", "die", "Son\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "FM", "$.", "ART", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Aus den Hernikerfelsen, \u00fcberm kahlen", "tokens": ["Aus", "den", "Her\u00b7ni\u00b7ker\u00b7fel\u00b7sen", ",", "\u00fc\u00b7berm", "kah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sanft umdufteten Haupte des Serone", "tokens": ["Sanft", "um\u00b7duf\u00b7te\u00b7ten", "Haup\u00b7te", "des", "Se\u00b7ro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sich erh\u00fcb', und die Purpurflamme gl\u00fchend", "tokens": ["Sich", "er\u00b7h\u00fcb'", ",", "und", "die", "Pur\u00b7pur\u00b7flam\u00b7me", "gl\u00fc\u00b7hend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "KON", "ART", "NN", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Um Olevano's H\u00e4userpyramide", "tokens": ["Um", "O\u00b7le\u00b7va\u00b7no's", "H\u00e4u\u00b7ser\u00b7py\u00b7ra\u00b7mi\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "H\u00f6h're Sch\u00f6nheit erg\u00f6sse, s\u00e4\u00df ich l\u00e4ngst schon", "tokens": ["H\u00f6h'\u00b7re", "Sch\u00f6n\u00b7heit", "er\u00b7g\u00f6s\u00b7se", ",", "s\u00e4\u00df", "ich", "l\u00e4ngst", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Auf des Hauses Balkon, an dem das Weinlaub", "tokens": ["Auf", "des", "Hau\u00b7ses", "Bal\u00b7kon", ",", "an", "dem", "das", "Wein\u00b7laub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Schwellend volle Gewinde hoch emporrankt,", "tokens": ["Schwel\u00b7lend", "vol\u00b7le", "Ge\u00b7win\u00b7de", "hoch", "em\u00b7por\u00b7rankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Ueberquellend vom Geist des Freudengottes", "tokens": ["Ue\u00b7ber\u00b7quel\u00b7lend", "vom", "Geist", "des", "Freu\u00b7den\u00b7got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Schon die Traube dem s\u00fc\u00dfen Lichte zulacht,", "tokens": ["Schon", "die", "Trau\u00b7be", "dem", "s\u00fc\u00b7\u00dfen", "Lich\u00b7te", "zu\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Wo in m\u00e4chtigen Bl\u00e4ttern aus der Mauer", "tokens": ["Wo", "in", "m\u00e4ch\u00b7ti\u00b7gen", "Bl\u00e4t\u00b7tern", "aus", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Mit der reifenden Frucht die Feige vorgr\u00fcnt,", "tokens": ["Mit", "der", "rei\u00b7fen\u00b7den", "Frucht", "die", "Fei\u00b7ge", "vor\u00b7gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Saftig schon die Citrone lacht, die goldne,", "tokens": ["Saf\u00b7tig", "schon", "die", "Cit\u00b7ro\u00b7ne", "lacht", ",", "die", "gold\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.15": {"text": "Die Melon' ihr Gew\u00e4chs zur Erde senket,", "tokens": ["Die", "Me\u00b7lon'", "ihr", "Ge\u00b7w\u00e4chs", "zur", "Er\u00b7de", "sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und zur Seite der einsamen Cypresse,", "tokens": ["Und", "zur", "Sei\u00b7te", "der", "ein\u00b7sa\u00b7men", "Cyp\u00b7res\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Aus dem Busche die Goldcitrone blinket.", "tokens": ["Aus", "dem", "Bu\u00b7sche", "die", "Gold\u00b7cit\u00b7ro\u00b7ne", "blin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Helle s\u00e4h' ich die wind'gen Schl\u00f6sser blinken,", "tokens": ["Hel\u00b7le", "s\u00e4h'", "ich", "die", "win\u00b7d'\u00b7gen", "Schl\u00f6s\u00b7ser", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "S\u00e4he Rocca di Cavi, morgenheiter", "tokens": ["S\u00e4\u00b7he", "Roc\u00b7ca", "di", "Ca\u00b7vi", ",", "mor\u00b7gen\u00b7hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NE", "NE", "NE", "$,", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Der Capranica Burg, Kastanienh\u00fcgel", "tokens": ["Der", "Cap\u00b7ra\u00b7ni\u00b7ca", "Burg", ",", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NE", "NE", "$,", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "F\u00fchrten nun mir den Blick in der Campagna", "tokens": ["F\u00fchr\u00b7ten", "nun", "mir", "den", "Blick", "in", "der", "Cam\u00b7pag\u00b7na"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Bunte, schimmernde Gr\u00fcnde weit zur Ferne,", "tokens": ["Bun\u00b7te", ",", "schim\u00b7mern\u00b7de", "Gr\u00fcn\u00b7de", "weit", "zur", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Bis wo durch die Elysiumshaine Cavi's", "tokens": ["Bis", "wo", "durch", "die", "E\u00b7ly\u00b7si\u00b7ums\u00b7hai\u00b7ne", "Ca\u00b7vi's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "APPR", "ART", "NE", "NE"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Palestrina der Schattenpfad sich n\u00e4hert, ", "tokens": ["Pa\u00b7le\u00b7stri\u00b7na", "der", "Schat\u00b7ten\u00b7pfad", "sich", "n\u00e4\u00b7hert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "Zu der Volsker Gebirge, Cavignano,", "tokens": ["Zu", "der", "Vols\u00b7ker", "Ge\u00b7bir\u00b7ge", ",", "Ca\u00b7vi\u00b7gna\u00b7no", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "NE", "$,"], "meter": "--+--+----+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Bis zur Scurcola und Anagni's Tempe. ", "tokens": ["Bis", "zur", "Scur\u00b7co\u00b7la", "und", "A\u00b7na\u00b7gni's", "Tem\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NE", "KON", "NE", "NE", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und die volle Erinn'rung schweifte manchmal", "tokens": ["Und", "die", "vol\u00b7le", "Er\u00b7inn'\u00b7rung", "schweif\u00b7te", "manch\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "In mein Latium hin, das ewig theure,", "tokens": ["In", "mein", "La\u00b7ti\u00b7um", "hin", ",", "das", "e\u00b7wig", "theu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zu den Hainen Albano's, zu Gandolfo's", "tokens": ["Zu", "den", "Hai\u00b7nen", "Al\u00b7ba\u00b7no's", ",", "zu", "Gan\u00b7dol\u00b7fo's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "$,", "APPR", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Klarem, erlenbekr\u00e4nzten See, zu Nemi's", "tokens": ["Kla\u00b7rem", ",", "er\u00b7len\u00b7be\u00b7kr\u00e4nz\u00b7ten", "See", ",", "zu", "Ne\u00b7mi's"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "APPR", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Altem, dunkeln Dianenwald, Genzano's", "tokens": ["Al\u00b7tem", ",", "dun\u00b7keln", "Di\u00b7a\u00b7nen\u00b7wald", ",", "Genza\u00b7no's"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "NE"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Meeresaussicht, und zu des Monte Cavo,", "tokens": ["Mee\u00b7re\u00b7saus\u00b7sicht", ",", "und", "zu", "des", "Mon\u00b7te", "Ca\u00b7vo", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "ART", "NN", "NE", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Weltbeherrschenden Haupt, wo oft mein Auge", "tokens": ["Welt\u00b7be\u00b7herr\u00b7schen\u00b7den", "Haupt", ",", "wo", "oft", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Von Oreste, von Tibur's Paradiese", "tokens": ["Von", "O\u00b7res\u00b7te", ",", "von", "Ti\u00b7bur's", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das unendliche Meer bis zu der Circe", "tokens": ["Das", "un\u00b7end\u00b7li\u00b7che", "Meer", "bis", "zu", "der", "Cir\u00b7ce"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Fernem, bl\u00e4ulichen Vorgebirg', hinunter", "tokens": ["Fer\u00b7nem", ",", "bl\u00e4u\u00b7li\u00b7chen", "Vor\u00b7ge\u00b7bir\u00b7g'", ",", "hin\u00b7un\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Zu Parthenope's Zauberinseln schaute,", "tokens": ["Zu", "Par\u00b7the\u00b7no\u00b7pe's", "Zau\u00b7be\u00b7rin\u00b7seln", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Schweifte gerne zum rebenvollen H\u00fcgel,", "tokens": ["Schweif\u00b7te", "ger\u00b7ne", "zum", "re\u00b7ben\u00b7vol\u00b7len", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Wo die Stadt der Lavinia, fabelheilig,", "tokens": ["Wo", "die", "Stadt", "der", "La\u00b7vi\u00b7nia", ",", "fa\u00b7bel\u00b7hei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NE", "$,", "ADJD", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Drei Jahrtausende bald sich schon im Lichte", "tokens": ["Drei", "Jahr\u00b7tau\u00b7sen\u00b7de", "bald", "sich", "schon", "im", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "PRF", "ADV", "APPRART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Des hesperischen Himmels sonnt; sie schweifte", "tokens": ["Des", "hes\u00b7pe\u00b7ri\u00b7schen", "Him\u00b7mels", "sonnt", ";", "sie", "schweif\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Nach des ewigen Fr\u00fchlings Wollusthainen,", "tokens": ["Nach", "des", "e\u00b7wi\u00b7gen", "Fr\u00fch\u00b7lings", "Wol\u00b7lust\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Frascatanischen G\u00e4rten zu, und bliebe", "tokens": ["Fras\u00b7ca\u00b7ta\u00b7ni\u00b7schen", "G\u00e4r\u00b7ten", "zu", ",", "und", "blie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Tr\u00e4umend stehn an der Einzigen, der Hehren,", "tokens": ["Tr\u00e4u\u00b7mend", "stehn", "an", "der", "Ein\