{"textgrid.poem.59694": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An die deutschen F\u00fcrsten", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schaut den deutschen Michel", "tokens": ["Ihr", "schaut", "den", "deut\u00b7schen", "Mi\u00b7chel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er tr\u00e4gt nicht mehr den Stamm der Tannen,", "tokens": ["Er", "tr\u00e4gt", "nicht", "mehr", "den", "Stamm", "der", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist er noch der ", "tokens": ["Doch", "ist", "er", "noch", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der nicht viel ", "tokens": ["Der", "nicht", "viel"], "token_info": ["word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das Riesenkind im alten Traum,", "tokens": ["Das", "Rie\u00b7sen\u00b7kind", "im", "al\u00b7ten", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor dessen Faust die Welt mu\u00df strauchen;", "tokens": ["Vor", "des\u00b7sen", "Faust", "die", "Welt", "mu\u00df", "strau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nimmt er sich den Weberbaum,", "tokens": ["Und", "nimmt", "er", "sich", "den", "We\u00b7ber\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er wei\u00df wie weiland ihn zu brauchen.", "tokens": ["Er", "wei\u00df", "wie", "wei\u00b7land", "ihn", "zu", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr schaut den deutschen Michel an?", "tokens": ["Ihr", "schaut", "den", "deut\u00b7schen", "Mi\u00b7chel", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O meinet nicht mit ihm zu scherzen!", "tokens": ["O", "mei\u00b7net", "nicht", "mit", "ihm", "zu", "scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Er ist noch heut der wilde Mann,", "tokens": ["Er", "ist", "noch", "heut", "der", "wil\u00b7de", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der viel im Arm hat, mehr im Herzen.", "tokens": ["Der", "viel", "im", "Arm", "hat", ",", "mehr", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VAFIN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Traut nicht zuviel auf seinen Traum,", "tokens": ["Traut", "nicht", "zu\u00b7viel", "auf", "sei\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er tr\u00e4umet hart am Morgentore,", "tokens": ["Er", "tr\u00e4u\u00b7met", "hart", "am", "Mor\u00b7gen\u00b7to\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein solcher Traum wird nimmer Schaum,", "tokens": ["Ein", "sol\u00b7cher", "Traum", "wird", "nim\u00b7mer", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hat die volle Lichtaurore.", "tokens": ["Er", "hat", "die", "vol\u00b7le", "Licht\u00b7au\u00b7ro\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, schaut euch nur den Michel an,", "tokens": ["Ja", ",", "schaut", "euch", "nur", "den", "Mi\u00b7chel", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er reibt die Augen zum Erwachen,", "tokens": ["Er", "reibt", "die", "Au\u00b7gen", "zum", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm tr\u00e4umte, wie er ein Gespann", "tokens": ["Ihm", "tr\u00e4um\u00b7te", ",", "wie", "er", "ein", "Ge\u00b7spann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem Riesen schlug und Drachen \u2013", "tokens": ["Von", "ei\u00b7nem", "Rie\u00b7sen", "schlug", "und", "Dra\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O schaut, wie ihm des Schlafes Sand", "tokens": ["O", "schaut", ",", "wie", "ihm", "des", "Schla\u00b7fes", "Sand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom lichtbestrahlten Auge flie\u00dfet,", "tokens": ["Vom", "licht\u00b7be\u00b7strahl\u00b7ten", "Au\u00b7ge", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie er halb tr\u00e4umend mit der Hand", "tokens": ["Wie", "er", "halb", "tr\u00e4u\u00b7mend", "mit", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie durch die L\u00fcfte Speere schie\u00dfet.", "tokens": ["Wie", "durch", "die", "L\u00fcf\u00b7te", "Spee\u00b7re", "schie\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, schaut euch nur den Michel an,", "tokens": ["Ja", ",", "schaut", "euch", "nur", "den", "Mi\u00b7chel", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Faust, das Herz, das Speereschie\u00dfen,", "tokens": ["Die", "Faust", ",", "das", "Herz", ",", "das", "Spee\u00b7re\u00b7schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der schwere Schlaf gottlob! wird dann", "tokens": ["Der", "schwe\u00b7re", "Schlaf", "gott\u00b7lob", "!", "wird", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch euch wie ihm im Licht zerflie\u00dfen \u2013", "tokens": ["Auch", "euch", "wie", "ihm", "im", "Licht", "zer\u00b7flie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PWAV", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kommt, schaut den Traum, des Tr\u00e4umers Spiel,", "tokens": ["Kommt", ",", "schaut", "den", "Traum", ",", "des", "Tr\u00e4u\u00b7mers", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und traut nicht, da\u00df er nur will spielen:", "tokens": ["Und", "traut", "nicht", ",", "da\u00df", "er", "nur", "will", "spie\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weil er mit Geistern spielt zum Ziel,", "tokens": ["Weil", "er", "mit", "Geis\u00b7tern", "spielt", "zum", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wird er desto sch\u00e4rfer zielen.", "tokens": ["So", "wird", "er", "des\u00b7to", "sch\u00e4r\u00b7fer", "zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, schaut euch nur den Michel an", "tokens": ["Ja", ",", "schaut", "euch", "nur", "den", "Mi\u00b7chel", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "ART", "NE", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lernt im Michel euch erkennen,", "tokens": ["Und", "lernt", "im", "Mi\u00b7chel", "euch", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NE", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lernt mit dem deutschen, starken Mann", "tokens": ["Lernt", "mit", "dem", "deut\u00b7schen", ",", "star\u00b7ken", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie weiland f\u00fcr die Freiheit brennen,", "tokens": ["Wie", "wei\u00b7land", "f\u00fcr", "die", "Frei\u00b7heit", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr deutsche Ehre, deutsches Recht,", "tokens": ["F\u00fcr", "deut\u00b7sche", "Eh\u00b7re", ",", "deut\u00b7sches", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr deutsche Wahrheit, deutsche Freude \u2013", "tokens": ["F\u00fcr", "deut\u00b7sche", "Wahr\u00b7heit", ",", "deut\u00b7sche", "Freu\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lernt das, dann weidet eu'r Geschlecht", "tokens": ["Lernt", "das", ",", "dann", "wei\u00b7det", "eu'r", "Ge\u00b7schlecht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch k\u00fcnftig mit auf deutscher Weide.", "tokens": ["Auch", "k\u00fcnf\u00b7tig", "mit", "auf", "deut\u00b7scher", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, schaut den deutschen Michel an,", "tokens": ["Ja", ",", "schaut", "den", "deut\u00b7schen", "Mi\u00b7chel", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was soll ich F\u00fcrsten Wahrheit f\u00e4lschen?", "tokens": ["Was", "soll", "ich", "F\u00fcrs\u00b7ten", "Wahr\u00b7heit", "f\u00e4l\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht an den vollen deutschen Mann,", "tokens": ["Zieht", "an", "den", "vol\u00b7len", "deut\u00b7schen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werft weg den bunten Rock der Welschen,", "tokens": ["Werft", "weg", "den", "bun\u00b7ten", "Rock", "der", "Wel\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Werft weg den welschen L\u00fcgenschein,", "tokens": ["Werft", "weg", "den", "wel\u00b7schen", "L\u00fc\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "All eure welschen Feinereien \u2013", "tokens": ["All", "eu\u00b7re", "wel\u00b7schen", "Fei\u00b7ne\u00b7rei\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann tritt der deutsche Held herein,", "tokens": ["Dann", "tritt", "der", "deut\u00b7sche", "Held", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der erste Freie unter Freien.", "tokens": ["Der", "ers\u00b7te", "Frei\u00b7e", "un\u00b7ter", "Frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, schaut den deutschen Michel an \u2013", "tokens": ["Ja", ",", "schaut", "den", "deut\u00b7schen", "Mi\u00b7chel", "an", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O w\u00e4rt ihr ganz aus seinem Holze!", "tokens": ["O", "w\u00e4rt", "ihr", "ganz", "aus", "sei\u00b7nem", "Hol\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich st\u00fcnde da der ganze Mann,", "tokens": ["Gleich", "st\u00fcn\u00b7de", "da", "der", "gan\u00b7ze", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stille, Tapfre, Freie, Stolze,", "tokens": ["Der", "Stil\u00b7le", ",", "Tapf\u00b7re", ",", "Frei\u00b7e", ",", "Stol\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der winkte durch die Welt hinaus:", "tokens": ["Der", "wink\u00b7te", "durch", "die", "Welt", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word",