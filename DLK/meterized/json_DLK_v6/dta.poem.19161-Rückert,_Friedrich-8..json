{"dta.poem.19161": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Licht ist leicht, es ist die umgekehrte Schwere;", "tokens": ["Das", "Licht", "ist", "leicht", ",", "es", "ist", "die", "um\u00b7ge\u00b7kehr\u00b7te", "Schwe\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einleuchten wird dirs leicht, wenn ich dirs klar erkl\u00e4re.", "tokens": ["Ein\u00b7leuch\u00b7ten", "wird", "dirs", "leicht", ",", "wenn", "ich", "dirs", "klar", "er\u00b7kl\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADJD", "$,", "KOUS", "PPER", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Licht von oben nimmt, wenn es hernieder schwimmt,", "tokens": ["Das", "Licht", "von", "o\u00b7ben", "nimmt", ",", "wenn", "es", "her\u00b7nie\u00b7der", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In gleichen Ma\u00dfen ab, wie zu die Schwere nimmt.", "tokens": ["In", "glei\u00b7chen", "Ma\u00b7\u00dfen", "ab", ",", "wie", "zu", "die", "Schwe\u00b7re", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Am schwersten alles ist der dunklen Erd' am n\u00e4chsten;", "tokens": ["Am", "schwers\u00b7ten", "al\u00b7les", "ist", "der", "dunk\u00b7len", "Erd'", "am", "n\u00e4chs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "PIS", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sonn' am fernsten wirkt des Lichtes Kraft am schw\u00e4chsten.", "tokens": ["Der", "Sonn'", "am", "ferns\u00b7ten", "wirkt", "des", "Lich\u00b7tes", "Kraft", "am", "schw\u00e4chs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das leichte Licht ist hoch, tief ist die schwere Schw\u00e4rze,", "tokens": ["Das", "leich\u00b7te", "Licht", "ist", "hoch", ",", "tief", "ist", "die", "schwe\u00b7re", "Schw\u00e4r\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zwischen beiden bl\u00fchn der T\u00f6n' und Farben Scherze.", "tokens": ["Und", "zwi\u00b7schen", "bei\u00b7den", "bl\u00fchn", "der", "T\u00f6n'", "und", "Far\u00b7ben", "Scher\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der h\u00f6chste Ton ist Licht, der tiefste Ton ist Nacht,", "tokens": ["Der", "h\u00f6chs\u00b7te", "Ton", "ist", "Licht", ",", "der", "tiefs\u00b7te", "Ton", "ist", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der endlich ganz erlischt, entschl\u00e4ft und neu erwacht;", "tokens": ["Der", "end\u00b7lich", "ganz", "er\u00b7lischt", ",", "ent\u00b7schl\u00e4ft", "und", "neu", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$,", "VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie dir der Schlaf bei Nacht schwer dr\u00fcckt die Augenlieder,", "tokens": ["Wie", "dir", "der", "Schlaf", "bei", "Nacht", "schwer", "dr\u00fcckt", "die", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die leicht der lichte Stral des Morgens aufschlie\u00dft wieder.", "tokens": ["Die", "leicht", "der", "lich\u00b7te", "Stral", "des", "Mor\u00b7gens", "auf\u00b7schlie\u00dft", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "ART", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}