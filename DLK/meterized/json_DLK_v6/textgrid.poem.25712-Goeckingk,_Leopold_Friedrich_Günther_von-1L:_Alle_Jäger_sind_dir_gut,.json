{"textgrid.poem.25712": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alle J\u00e4ger sind dir gut,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle J\u00e4ger sind dir gut,", "tokens": ["Al\u00b7le", "J\u00e4\u00b7ger", "sind", "dir", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Um der Sechszehnender willen;", "tokens": ["Um", "der", "Sechs\u00b7ze\u00b7hnen\u00b7der", "wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber sollt' ich wohl durch Blut", "tokens": ["A\u00b7ber", "sollt'", "ich", "wohl", "durch", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Magens W\u00fcnsche stillen?", "tokens": ["Mei\u00b7nes", "Ma\u00b7gens", "W\u00fcn\u00b7sche", "stil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trieb zum Leben wird so gut", "tokens": ["Trieb", "zum", "Le\u00b7ben", "wird", "so", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Hirsch, als Dichter f\u00fcllen.", "tokens": ["Ei\u00b7nen", "Hirsch", ",", "als", "Dich\u00b7ter", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da ich selbst ein wenig viel", "tokens": ["Da", "ich", "selbst", "ein", "we\u00b7nig", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "PIS", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Aus dem St\u00fcndchen Leben mache,", "tokens": ["Aus", "dem", "St\u00fcnd\u00b7chen", "Le\u00b7ben", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Treib' auch meinethalb ihr Spiel,", "tokens": ["Treib'", "auch", "mei\u00b7net\u00b7halb", "ihr", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie der Keuler, so die Bache;", "tokens": ["Wie", "der", "Keu\u00b7ler", ",", "so", "die", "Ba\u00b7che", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn der Thiere froh Gew\u00fchl", "tokens": ["Denn", "der", "Thie\u00b7re", "froh", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "War von jeher meine Sache.", "tokens": ["War", "von", "je\u00b7her", "mei\u00b7ne", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Selbst den F\u00fcrsten bist du lieb,", "tokens": ["Selbst", "den", "F\u00fcrs\u00b7ten", "bist", "du", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Lieb um deiner Berge willen,", "tokens": ["Lieb", "um", "dei\u00b7ner", "Ber\u00b7ge", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die mit Gold das weite Sieb", "tokens": ["Die", "mit", "Gold", "das", "wei\u00b7te", "Sieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Ihrer Habsucht nimmer f\u00fcllen.", "tokens": ["Ih\u00b7rer", "Hab\u00b7sucht", "nim\u00b7mer", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ohne Gold nehm' ich vorlieb;", "tokens": ["Oh\u00b7ne", "Gold", "nehm'", "ich", "vor\u00b7lieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Wer es hat, hat viele Grillen.", "tokens": ["Wer", "es", "hat", ",", "hat", "vie\u00b7le", "Gril\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Was kein Andrer haben mag:", "tokens": ["Was", "kein", "A\u00b7ndrer", "ha\u00b7ben", "mag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Blumen, Moose, Schw\u00e4mm' und Kr\u00e4uter,", "tokens": ["Blu\u00b7men", ",", "Moo\u00b7se", ",", "Schw\u00e4mm'", "und", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Gibst du, Harz, mir Tag f\u00fcr Tag;", "tokens": ["Gibst", "du", ",", "Harz", ",", "mir", "Tag", "f\u00fcr", "Tag", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "PPER", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Auch verlangt mein Herz nichts weiter;", "tokens": ["Auch", "ver\u00b7langt", "mein", "Herz", "nichts", "wei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Denn mir macht ein Trinkgelag", "tokens": ["Denn", "mir", "macht", "ein", "Trink\u00b7ge\u00b7lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Stirn und Auge kaum so heiter.", "tokens": ["Stirn", "und", "Au\u00b7ge", "kaum", "so", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Auf den Bergen steigt und f\u00e4llt,", "tokens": ["Auf", "den", "Ber\u00b7gen", "steigt", "und", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "In der reinern Atmosph\u00e4re,", "tokens": ["In", "der", "rei\u00b7nern", "At\u00b7mo\u00b7sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Meine Brust so leicht, und h\u00e4lt", "tokens": ["Mei\u00b7ne", "Brust", "so", "leicht", ",", "und", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Nur Zufriedenheit f\u00fcr Ehre,", "tokens": ["Nur", "Zu\u00b7frie\u00b7den\u00b7heit", "f\u00fcr", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Gleich als wenn bereits die Welt", "tokens": ["Gleich", "als", "wenn", "be\u00b7reits", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Um mich her versunken w\u00e4re!", "tokens": ["Um", "mich", "her", "ver\u00b7sun\u00b7ken", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Kommt der Mond noch obenein", "tokens": ["Kommt", "der", "Mond", "noch", "o\u00b7be\u00b7nein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Durch die D\u00e4mmerung geschwommen,", "tokens": ["Durch", "die", "D\u00e4m\u00b7me\u00b7rung", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Ist's, als w\u00fcrde mein Gebein", "tokens": ["Ist's", ",", "als", "w\u00fcr\u00b7de", "mein", "Ge\u00b7bein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOKOM", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Von dem Boden aufgenommen,", "tokens": ["Von", "dem", "Bo\u00b7den", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und als z\u00f6g' ich lebend ein", "tokens": ["Und", "als", "z\u00f6g'", "ich", "le\u00b7bend", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "VVFIN", "PPER", "ADJD", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "In das stille Land der Frommen.", "tokens": ["In", "das", "stil\u00b7le", "Land", "der", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Welch ein Leben! Ist doch mir", "tokens": ["Welch", "ein", "Le\u00b7ben", "!", "Ist", "doch", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "VAFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Ach! so wohl an Seel' und Leibe!", "tokens": ["Ach", "!", "so", "wohl", "an", "Seel'", "und", "Lei\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Wem geb\u00fchrt mein Dank daf\u00fcr?", "tokens": ["Wem", "ge\u00b7b\u00fchrt", "mein", "Dank", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "O, n\u00e4chst Gott, so lang ich's treibe,", "tokens": ["O", ",", "n\u00e4chst", "Gott", ",", "so", "lang", "ich's", "trei\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "$,", "ADV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Dank' ich's, Harz! zur H\u00e4lfte dir,", "tokens": ["Dank'", "ich's", ",", "Harz", "!", "zur", "H\u00e4lf\u00b7te", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "NE", "$.", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Und zur H\u00e4lfte meinem Weibe.", "tokens": ["Und", "zur", "H\u00e4lf\u00b7te", "mei\u00b7nem", "Wei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}