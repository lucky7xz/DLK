{"textgrid.poem.48331": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wo Bismarck liegen soll", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht in Dom oder F\u00fcrstengruft,", "tokens": ["Nicht", "in", "Dom", "o\u00b7der", "F\u00fcrs\u00b7ten\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er ruh' in Gottes freier Luft", "tokens": ["Er", "ruh'", "in", "Got\u00b7tes", "frei\u00b7er", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drau\u00dfen auf Berg und Halde,", "tokens": ["Drau\u00b7\u00dfen", "auf", "Berg", "und", "Hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Noch besser: tief, tief im Walde;", "tokens": ["Noch", "bes\u00b7ser", ":", "tief", ",", "tief", "im", "Wal\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADJD", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Widukind l\u00e4dt ihn zu sich ein:", "tokens": ["Wi\u00b7du\u00b7kind", "l\u00e4dt", "ihn", "zu", "sich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "\u00bbein Sachse war er, drum ist er mein,", "tokens": ["\u00bb", "ein", "Sach\u00b7se", "war", "er", ",", "drum", "ist", "er", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "$,", "PAV", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Der Leib zerf\u00e4llt, der Stein zerf\u00e4llt,", "tokens": ["Der", "Leib", "zer\u00b7f\u00e4llt", ",", "der", "Stein", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aber der Sachsenwald, der h\u00e4lt.", "tokens": ["A\u00b7ber", "der", "Sach\u00b7sen\u00b7wald", ",", "der", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und kommen nach dreitausend Jahren", "tokens": ["Und", "kom\u00b7men", "nach", "drei\u00b7tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fremde hier des Weges gefahren", "tokens": ["Frem\u00b7de", "hier", "des", "We\u00b7ges", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und sehen, geborgen vorm Licht der Sonnen,", "tokens": ["Und", "se\u00b7hen", ",", "ge\u00b7bor\u00b7gen", "vorm", "Licht", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Den Waldgrund in Efeu tief eingesponnen", "tokens": ["Den", "Wald\u00b7grund", "in", "E\u00b7feu", "tief", "ein\u00b7ge\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und staunen der Sch\u00f6nheit und jauchzen froh,", "tokens": ["Und", "stau\u00b7nen", "der", "Sch\u00f6n\u00b7heit", "und", "jauch\u00b7zen", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So gebietet einer: \u00bbL\u00e4rmt nicht so! \u2013", "tokens": ["So", "ge\u00b7bie\u00b7tet", "ei\u00b7ner", ":", "\u00bb", "L\u00e4rmt", "nicht", "so", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$.", "$(", "VVFIN", "PTKNEG", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}