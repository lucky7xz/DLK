{"dta.poem.13451": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Geiz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Der Harpax kauft sich Augen,", "tokens": ["Der", "Har\u00b7pax", "kauft", "sich", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und pflanzt sie auf die Nase,", "tokens": ["Und", "pflanzt", "sie", "auf", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn z\u00e4hlet er Ducaten", "tokens": ["Denn", "z\u00e4h\u00b7let", "er", "Du\u00b7ca\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit doppeltem Gesichte;", "tokens": ["Mit", "dop\u00b7pel\u00b7tem", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Er l\u00e4sset lauter Helden", "tokens": ["Er", "l\u00e4s\u00b7set", "lau\u00b7ter", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier durch die Mustrung gehen.", "tokens": ["Hier", "durch", "die", "Must\u00b7rung", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Nasen voller Kupfer,", "tokens": ["Die", "Na\u00b7sen", "vol\u00b7ler", "Kup\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die rothgewordnen Locken", "tokens": ["Die", "roth\u00b7ge\u00b7word\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "In denen Staats-Perucken", "tokens": ["In", "de\u00b7nen", "Staats\u00b7Pe\u00b7ru\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "An seinen gelben Leuten", "tokens": ["An", "sei\u00b7nen", "gel\u00b7ben", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Kann er ohnm\u00f6glich leiden.", "tokens": ["Kann", "er", "ohn\u00b7m\u00f6g\u00b7lich", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sieh! wie er das Gesichte", "tokens": ["Sieh", "!", "wie", "er", "das", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Bey ieglichem verziehet,", "tokens": ["Bey", "ieg\u00b7li\u00b7chem", "ver\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Da\u00df die gekauften Augen", "tokens": ["Da\u00df", "die", "ge\u00b7kauf\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Sich vor der Stirne regen.", "tokens": ["Sich", "vor", "der", "Stir\u00b7ne", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Nun hebt er voller Unmuth", "tokens": ["Nun", "hebt", "er", "vol\u00b7ler", "Un\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Sie aus den gleichen Reihen,", "tokens": ["Sie", "aus", "den", "glei\u00b7chen", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und stellet sie bey Seite,", "tokens": ["Und", "stel\u00b7let", "sie", "bey", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Gedenkt auch schon mit Wucher", "tokens": ["Ge\u00b7denkt", "auch", "schon", "mit", "Wu\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Sie andern abzutreten.", "tokens": ["Sie", "an\u00b7dern", "ab\u00b7zu\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Hingegen bey Gesichtern,", "tokens": ["Hin\u00b7ge\u00b7gen", "bey", "Ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.22": {"text": "Die fein und lieblich spielen,", "tokens": ["Die", "fein", "und", "lieb\u00b7lich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Verliehrt er seine Runzeln,", "tokens": ["Ver\u00b7liehrt", "er", "sei\u00b7ne", "Run\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Wird heftig drein verliebet,", "tokens": ["Wird", "hef\u00b7tig", "drein", "ver\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Wie Harpax es kann werden,", "tokens": ["Wie", "Har\u00b7pax", "es", "kann", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Da\u00df er sie br\u00fcnstig k\u00fcsset,", "tokens": ["Da\u00df", "er", "sie", "br\u00fcns\u00b7tig", "k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Auch an sein Herze dr\u00fccket,", "tokens": ["Auch", "an", "sein", "Her\u00b7ze", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wor\u00fcber ihm der Hunger", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "ihm", "der", "Hun\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Und auch der Durst vergehet.", "tokens": ["Und", "auch", "der", "Durst", "ver\u00b7ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Jedoch ich sehe sch\u00e4rfer", "tokens": ["Je\u00b7doch", "ich", "se\u00b7he", "sch\u00e4r\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADJD"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.31": {"text": "Mit meinen eignen Augen,", "tokens": ["Mit", "mei\u00b7nen", "eig\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Indem ich Sch\u00e4tze z\u00e4hle,", "tokens": ["In\u00b7dem", "ich", "Sch\u00e4t\u00b7ze", "z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Und kann die Augen sparen,", "tokens": ["Und", "kann", "die", "Au\u00b7gen", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die man erst mu\u00df bezahlen.", "tokens": ["Die", "man", "erst", "mu\u00df", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Doch sind es keine Helden", "tokens": ["Doch", "sind", "es", "kei\u00b7ne", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Mit goldenen Gesichtern,", "tokens": ["Mit", "gol\u00b7de\u00b7nen", "Ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "O nein! ich z\u00e4hle M\u00e4dchen,", "tokens": ["O", "nein", "!", "ich", "z\u00e4h\u00b7le", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Brunetten und Blondinen,", "tokens": ["Bru\u00b7net\u00b7ten", "und", "Blon\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Mit wei\u00df und schwarzen Haaren,", "tokens": ["Mit", "wei\u00df", "und", "schwar\u00b7zen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Mit solchen Rosenwangen,", "tokens": ["Mit", "sol\u00b7chen", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Als du, o Christiane!", "tokens": ["Als", "du", ",", "o", "Chris\u00b7ti\u00b7a\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "So lieblichreizend wiesest.", "tokens": ["So", "lieb\u00b7lich\u00b7rei\u00b7zend", "wie\u00b7sest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Nur niedliche Gesichter,", "tokens": ["Nur", "nied\u00b7li\u00b7che", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Die ich sodenn besehe,", "tokens": ["Die", "ich", "so\u00b7denn", "be\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Worein ich mich verliebe,", "tokens": ["Wo\u00b7rein", "ich", "mich", "ver\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Sodenn gleich geizig werde,", "tokens": ["So\u00b7denn", "gleich", "gei\u00b7zig", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Um sie nur zu besitzen,", "tokens": ["Um", "sie", "nur", "zu", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Sie nach und nach zu k\u00fcssen,", "tokens": ["Sie", "nach", "und", "nach", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "KON", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Sie an mein Herz zu dr\u00fccken,", "tokens": ["Sie", "an", "mein", "Herz", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Sie tugendhaft zu lieben,", "tokens": ["Sie", "tu\u00b7gend\u00b7haft", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Wie geizige Poeten", "tokens": ["Wie", "gei\u00b7zi\u00b7ge", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Es sonst zu machen pflegen,", "tokens": ["Es", "sonst", "zu", "ma\u00b7chen", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Da\u00df ihnen Wein und Essen", "tokens": ["Da\u00df", "ih\u00b7nen", "Wein", "und", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Um desto besser schmecke.", "tokens": ["Um", "des\u00b7to", "bes\u00b7ser", "schme\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Wer ist von uns nun kl\u00fcger?", "tokens": ["Wer", "ist", "von", "uns", "nun", "kl\u00fc\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}