{"textgrid.poem.61654": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was der Wissenschaft gef\u00e4llt,", "tokens": ["Was", "der", "Wis\u00b7sen\u00b7schaft", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird darum der Kunst nicht taugen;", "tokens": ["Wird", "da\u00b7rum", "der", "Kunst", "nicht", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Beide schaun dieselbe Welt,", "tokens": ["Bei\u00b7de", "schaun", "die\u00b7sel\u00b7be", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch mit ganz verschiednen Augen.", "tokens": ["Doch", "mit", "ganz", "ver\u00b7schied\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}