{"textgrid.poem.32189": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "102. Das Auge und Herze leiden nichts", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herz ist wie das Aug; ein einzigs Gr\u00e4nelein,", "tokens": ["Das", "Herz", "ist", "wie", "das", "Aug", ";", "ein", "ein\u00b7zigs", "Gr\u00e4\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo dus im Herzen hast, verursacht dir schon Pein.", "tokens": ["Wo", "dus", "im", "Her\u00b7zen", "hast", ",", "ver\u00b7ur\u00b7sacht", "dir", "schon", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}