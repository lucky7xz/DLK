{"dta.poem.12945": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er entdeckt sein verlangen nach ei-  \n nem ku\u00dfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe wol geh\u00f6rt, o Venus! da\u00df dein sohn", "tokens": ["Ich", "ha\u00b7be", "wol", "ge\u00b7h\u00f6rt", ",", "o", "Ve\u00b7nus", "!", "da\u00df", "dein", "sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$,", "FM", "FM", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir aus der schoos entflogen,", "tokens": ["Dir", "aus", "der", "schoos", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und deine wachsamkeit betrogen.", "tokens": ["Und", "dei\u00b7ne", "wach\u00b7sam\u00b7keit", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist des verr\u00e4thers lohn", "tokens": ["Ist", "des", "ver\u00b7r\u00e4\u00b7thers", "lohn"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun, wie man sagt, ein ku\u00df? so gieb dich doch zufrieden,", "tokens": ["Nun", ",", "wie", "man", "sagt", ",", "ein", "ku\u00df", "?", "so", "gieb", "dich", "doch", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "$.", "ADV", "VVIMP", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und such ihn weiter nicht.", "tokens": ["Und", "such", "ihn", "wei\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ertheile mir nur den versprochnen prei\u00df,", "tokens": ["Er\u00b7thei\u00b7le", "mir", "nur", "den", "ver\u00b7sproch\u00b7nen", "prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Oder mache, da\u00df der mund, der sich stets von mir geschieden,", "tokens": ["O\u00b7der", "ma\u00b7che", ",", "da\u00df", "der", "mund", ",", "der", "sich", "stets", "von", "mir", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+--+---+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Mir l\u00e4nger nicht ihn abzuschlagen wei\u00df;", "tokens": ["Mir", "l\u00e4n\u00b7ger", "nicht", "ihn", "ab\u00b7zu\u00b7schla\u00b7gen", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKNEG", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Denn deines sohnes aufenthalt", "tokens": ["Denn", "dei\u00b7nes", "soh\u00b7nes", "auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ist kein entlegner wald;", "tokens": ["Ist", "kein", "ent\u00b7leg\u00b7ner", "wald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Nein, sondern Flavia, mein paradies und licht.", "tokens": ["Nein", ",", "son\u00b7dern", "Fla\u00b7via", ",", "mein", "pa\u00b7ra\u00b7dies", "und", "licht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "NE", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}