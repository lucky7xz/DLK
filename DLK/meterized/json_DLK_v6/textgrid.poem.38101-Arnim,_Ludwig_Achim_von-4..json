{"textgrid.poem.38101": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wollt ihr h\u00f6ren, wie lang die Frau des Lebens pflag,", "tokens": ["Nun", "wollt", "ihr", "h\u00f6\u00b7ren", ",", "wie", "lang", "die", "Frau", "des", "Le\u00b7bens", "pflag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ohn Essen und Trinken hat sie kein Noth,", "tokens": ["Ohn", "Es\u00b7sen", "und", "Trin\u00b7ken", "hat", "sie", "kein", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als ich euch will bescheiden.", "tokens": ["Als", "ich", "euch", "will", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcrwahr sie lebt bis an den eilften Tag,", "tokens": ["F\u00fcr\u00b7wahr", "sie", "lebt", "bis", "an", "den", "eilf\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da schied die Zart, die Werth davon,", "tokens": ["Da", "schied", "die", "Zart", ",", "die", "Werth", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Herrn geschah gro\u00df Leiden.", "tokens": ["Dem", "Herrn", "ge\u00b7schah", "gro\u00df", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbach Gott wie soll es mir ergahn,", "tokens": ["\u00bb", "ach", "Gott", "wie", "soll", "es", "mir", "er\u00b7gahn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "KOKOM", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich die liebste Fraue mein", "tokens": ["Da\u00df", "ich", "die", "liebs\u00b7te", "Frau\u00b7e", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So unehrlich hab verrathen", "tokens": ["So", "un\u00b7ehr\u00b7lich", "hab", "ver\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Und ihren werthen Dienstmann,", "tokens": ["Und", "ih\u00b7ren", "wert\u00b7hen", "Dienst\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ich f\u00fcrcht es wird mir viel zu schwer", "tokens": ["Ich", "f\u00fcrcht", "es", "wird", "mir", "viel", "zu", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mein Seel die mu\u00df leiden Noth.\u00ab", "tokens": ["Mein", "Seel", "die", "mu\u00df", "lei\u00b7den", "Noth", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ART", "VMFIN", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Der Herr der stand und sah den grossen Jammer an:", "tokens": ["Der", "Herr", "der", "stand", "und", "sah", "den", "gros\u00b7sen", "Jam\u00b7mer", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u00bbo Herre Gott, da\u00df ich sie beyde samt verrathen han!\u00ab", "tokens": ["\u00bb", "o", "Her\u00b7re", "Gott", ",", "da\u00df", "ich", "sie", "bey\u00b7de", "samt", "ver\u00b7ra\u00b7then", "han", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "NN", "$,", "KOUS", "PPER", "PPER", "PIS", "ADJD", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Der Herr ein Messer in sein eigen Herz stach,", "tokens": ["Der", "Herr", "ein", "Mes\u00b7ser", "in", "sein", "ei\u00b7gen", "Herz", "stach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Es wende dann Maria und ihr liebes Kind", "tokens": ["Es", "wen\u00b7de", "dann", "Ma\u00b7ria", "und", "ihr", "lie\u00b7bes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Sein Seel mu\u00df leiden Ungemach.", "tokens": ["Sein", "Seel", "mu\u00df", "lei\u00b7den", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}