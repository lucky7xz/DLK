{"textgrid.poem.42934": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Trostworte an einen Luftkranken", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Recht so! Speie, lieber Mitgast, speie!", "tokens": ["Recht", "so", "!", "Spei\u00b7e", ",", "lie\u00b7ber", "Mit\u00b7gast", ",", "spei\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$.", "NN", "$,", "ADV", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Speie dreist und ungeniert und laut,", "tokens": ["Spei\u00b7e", "dreist", "und", "un\u00b7ge\u00b7niert", "und", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df sich einmal andersrum befreie,", "tokens": ["Da\u00df", "sich", "ein\u00b7mal", "an\u00b7ders\u00b7rum", "be\u00b7frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was f\u00fcr dich pass\u00e9 ist und verdaut.", "tokens": ["Was", "f\u00fcr", "dich", "pass\u00e9", "ist", "und", "ver\u00b7daut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "NE", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Speie froh. Es wird dir polizeilich", "tokens": ["Spei\u00b7e", "froh", ".", "Es", "wird", "dir", "po\u00b7li\u00b7zei\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$.", "PPER", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und moralisch jederzeit verziehn. \u2013", "tokens": ["Und", "mo\u00b7ra\u00b7lisch", "je\u00b7der\u00b7zeit", "ver\u00b7ziehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ADV", "VVINF", "$.", "$("], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ja, ich gebe zu: Ich habe freilich", "tokens": ["Ja", ",", "ich", "ge\u00b7be", "zu", ":", "Ich", "ha\u00b7be", "frei\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da leicht reden, weil ich nie gespien.", "tokens": ["Da", "leicht", "re\u00b7den", ",", "weil", "ich", "nie", "ge\u00b7spi\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und der Himmel m\u00f6ge auch verh\u00fcten,", "tokens": ["Und", "der", "Him\u00b7mel", "m\u00f6\u00b7ge", "auch", "ver\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df es je geschieht. Ich stell mir blo\u00df", "tokens": ["Da\u00df", "es", "je", "ge\u00b7schieht", ".", "Ich", "stell", "mir", "blo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Vor, wie unten deine T\u00fcten", "tokens": ["Vor", ",", "wie", "un\u00b7ten", "dei\u00b7ne", "T\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Landen in der Mutter Erde Scho\u00df.", "tokens": ["Lan\u00b7den", "in", "der", "Mut\u00b7ter", "Er\u00b7de", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Andern Luft und Appetit verderben,", "tokens": ["An\u00b7dern", "Luft", "und", "Ap\u00b7pe\u00b7tit", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kann ein schadenfr\u00f6hlich freier Sport", "tokens": ["Kann", "ein", "scha\u00b7den\u00b7fr\u00f6h\u00b7lich", "frei\u00b7er", "Sport"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sein. Und niemand wird deswegen sterben.", "tokens": ["Sein", ".", "Und", "nie\u00b7mand", "wird", "des\u00b7we\u00b7gen", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "KON", "PIS", "VAFIN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn der Magen ist wie ein Abort.", "tokens": ["Denn", "der", "Ma\u00b7gen", "ist", "wie", "ein", "Ab\u00b7ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "--+-++-+-", "measure": "anapaest.init"}}}}}