{"dta.poem.18281": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Aegypter- und Teutonenthum,", "tokens": ["A\u00b7egyp\u00b7ter", "und", "Teu\u00b7to\u00b7nen\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df dich der Teufel brate!", "tokens": ["Da\u00df", "dich", "der", "Teu\u00b7fel", "bra\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon l\u00e4ngst sind wir fascikelsatt,", "tokens": ["Schon", "l\u00e4ngst", "sind", "wir", "fas\u00b7ci\u00b7kel\u00b7satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Grinst doch durch jedes Titelblatt", "tokens": ["Grinst", "doch", "durch", "je\u00b7des", "Ti\u00b7tel\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Dante'sche \u201e", "tokens": ["Das", "Dan\u00b7te'\u00b7sche", "\u201e"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$("], "meter": "+-+-", "measure": "trochaic.di"}}}}}