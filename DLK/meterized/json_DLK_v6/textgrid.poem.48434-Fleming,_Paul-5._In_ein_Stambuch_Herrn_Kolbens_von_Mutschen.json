{"textgrid.poem.48434": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. In ein Stambuch Herrn Kolbens von Mutschen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel treibt die Luft, die Luft bewegt die Erde,", "tokens": ["Der", "Him\u00b7mel", "treibt", "die", "Luft", ",", "die", "Luft", "be\u00b7wegt", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Wasser eilet fort, die Sonn- und Mondenpferde", "tokens": ["das", "Was\u00b7ser", "ei\u00b7let", "fort", ",", "die", "Sonn", "und", "Mon\u00b7den\u00b7pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die steigen auf und ab, der Sternen P\u00f6fel tanzt,", "tokens": ["die", "stei\u00b7gen", "auf", "und", "ab", ",", "der", "Ster\u00b7nen", "P\u00f6\u00b7fel", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKVZ", "KON", "PTKVZ", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die Glut rei\u00dft \u00fcber sich und wird mit nichts umschanzt.", "tokens": ["die", "Glut", "rei\u00dft", "\u00fc\u00b7ber", "sich", "und", "wird", "mit", "nichts", "um\u00b7schanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "KON", "VAFIN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo H\u00e4user sind, war Flut, wo St\u00e4dte, sind nun W\u00e4lder;", "tokens": ["Wo", "H\u00e4u\u00b7ser", "sind", ",", "war", "Flut", ",", "wo", "St\u00e4d\u00b7te", ",", "sind", "nun", "W\u00e4l\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,", "VAFIN", "NN", "$,", "PWAV", "NN", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "vor H\u00fcgel, itzund Tal, vor Klippen, itzund Felder.", "tokens": ["vor", "H\u00fc\u00b7gel", ",", "it\u00b7zund", "Tal", ",", "vor", "Klip\u00b7pen", ",", "it\u00b7zund", "Fel\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "NN", "$,", "APPR", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Jahr ist niemals gleich, bald ist es kalt, bald hei\u00df.", "tokens": ["Das", "Jahr", "ist", "nie\u00b7mals", "gleich", ",", "bald", "ist", "es", "kalt", ",", "bald", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir \u00e4ndern uns mit ihm, itzt sind wir jung, bald greis.", "tokens": ["Wir", "\u00e4n\u00b7dern", "uns", "mit", "ihm", ",", "itzt", "sind", "wir", "jung", ",", "bald", "greis", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Schau an di\u00df gro\u00dfe Das, das Ph\u00f6bus G\u00e4ul' umrennen!", "tokens": ["Schau", "an", "di\u00df", "gro\u00b7\u00dfe", "Das", ",", "das", "Ph\u00f6\u00b7bus", "G\u00e4ul'", "um\u00b7ren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDS", "VVFIN", "PDS", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie stark es immer ist, noch wird es m\u00fcssen brennen.", "tokens": ["Wie", "stark", "es", "im\u00b7mer", "ist", ",", "noch", "wird", "es", "m\u00fcs\u00b7sen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man lebe, wie man soll, und brauche seiner Zeit!", "tokens": ["Man", "le\u00b7be", ",", "wie", "man", "soll", ",", "und", "brau\u00b7che", "sei\u00b7ner", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PIS", "VMFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hier ist best\u00e4ndig nichts als Unbest\u00e4ndigkeit.", "tokens": ["Hier", "ist", "be\u00b7st\u00e4n\u00b7dig", "nichts", "als", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}