{"textgrid.poem.40625": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie hat im letzten M\u00e4rzen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hat im letzten M\u00e4rzen", "tokens": ["Wie", "hat", "im", "letz\u00b7ten", "M\u00e4r\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sonnenbrand gekocht,", "tokens": ["Der", "Son\u00b7nen\u00b7brand", "ge\u00b7kocht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie habt ihr deutschen Herzen", "tokens": ["Wie", "habt", "ihr", "deut\u00b7schen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gelodert und gepocht!", "tokens": ["Ge\u00b7lo\u00b7dert", "und", "ge\u00b7pocht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eu'r Pochen, das zermalmte", "tokens": ["Eu'r", "Po\u00b7chen", ",", "das", "zer\u00b7malm\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ehrnen G\u00f6tzen im Fall,", "tokens": ["Die", "ehr\u00b7nen", "G\u00f6t\u00b7zen", "im", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Von eurem Lodern qualmte", "tokens": ["Von", "eu\u00b7rem", "Lo\u00b7dern", "qualm\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zerschmelzend Kronmetall.", "tokens": ["Zer\u00b7schmel\u00b7zend", "Kron\u00b7me\u00b7tall", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Frankfurt hie\u00df die Esse,", "tokens": ["Und", "Frank\u00b7furt", "hie\u00df", "die", "Es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort steigt aus Flammen wohl,", "tokens": ["Dort", "steigt", "aus", "Flam\u00b7men", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich's in Formen presse,", "tokens": ["Da\u00df", "sich's", "in", "For\u00b7men", "pres\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der neuen Zeit Symbol;", "tokens": ["Der", "neu\u00b7en", "Zeit", "Sym\u00b7bol", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Gluth verzehrt den Flitter,", "tokens": ["Die", "Gluth", "ver\u00b7zehrt", "den", "Flit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Womit sich Schmach umhing,", "tokens": ["Wo\u00b7mit", "sich", "Schmach", "um\u00b7hing", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schmilzt die Tr\u00fcmmer und Splitter", "tokens": ["Und", "schmilzt", "die", "Tr\u00fcm\u00b7mer", "und", "Split\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zum m\u00e4cht'gen Einheitsring.", "tokens": ["Zum", "m\u00e4cht'\u00b7gen", "Ein\u00b7heits\u00b7ring", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im neuen M\u00e4rzen ging es", "tokens": ["Im", "neu\u00b7en", "M\u00e4r\u00b7zen", "ging", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dunkler Form zu Tag:", "tokens": ["Aus", "dunk\u00b7ler", "Form", "zu", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da statt des m\u00e4cht'gen Ringes", "tokens": ["Da", "statt", "des", "m\u00e4cht'\u00b7gen", "Rin\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein machtlos Kr\u00f6nchen lag.", "tokens": ["Ein", "macht\u00b7los", "Kr\u00f6n\u00b7chen", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weh, ein mi\u00dfrathner Gu\u00df!", "tokens": ["Weh", ",", "ein", "mi\u00df\u00b7ra\u00b7th\u00b7ner", "Gu\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Solch' ungeheurem Brande", "tokens": ["Solch'", "un\u00b7ge\u00b7heu\u00b7rem", "Bran\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So jammervoller Schlu\u00df!", "tokens": ["So", "jam\u00b7mer\u00b7vol\u00b7ler", "Schlu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die\u00df Mi\u00dfgeschick zu heilen", "tokens": ["Die\u00df", "Mi\u00df\u00b7ge\u00b7schick", "zu", "hei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erlahmt noch manche Hand;", "tokens": ["Er\u00b7lahmt", "noch", "man\u00b7che", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lang m\u00fc\u00dft ihr feilen, feilen", "tokens": ["Lang", "m\u00fc\u00dft", "ihr", "fei\u00b7len", ",", "fei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zacken vom Kronenrand,", "tokens": ["Die", "Za\u00b7cken", "vom", "Kro\u00b7nen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn nicht, sie umzuschmelzen,", "tokens": ["Wenn", "nicht", ",", "sie", "um\u00b7zu\u00b7schmel\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aufs neu es lodern mu\u00df", "tokens": ["Aufs", "neu", "es", "lo\u00b7dern", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und eherne Wogen w\u00e4lzen", "tokens": ["Und", "e\u00b7her\u00b7ne", "Wo\u00b7gen", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zu neuem, bessern Gu\u00df!", "tokens": ["Zu", "neu\u00b7em", ",", "bes\u00b7sern", "Gu\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}