{"textgrid.poem.62591": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Heb ich in den freyen Felde meine Augen in die H\u00f6h", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heb ich in den freyen Felde meine Augen in die H\u00f6h,", "tokens": ["Heb", "ich", "in", "den", "frey\u00b7en", "Fel\u00b7de", "mei\u00b7ne", "Au\u00b7gen", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schau ich seitwerts, vor und hinter, ich doch nichts als Himmel seh", "tokens": ["Schau", "ich", "seit\u00b7werts", ",", "vor", "und", "hin\u00b7ter", ",", "ich", "doch", "nichts", "als", "Him\u00b7mel", "seh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "PTKVZ", "KON", "APPR", "$,", "PPER", "ADV", "PIS", "KOKOM", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dieses giebt mir zu verstehn, da\u00df ich mich in meinem Leben", "tokens": ["Die\u00b7ses", "giebt", "mir", "zu", "ver\u00b7stehn", ",", "da\u00df", "ich", "mich", "in", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Um den Himmel nur allein soll bewerben und bestreben.", "tokens": ["Um", "den", "Him\u00b7mel", "nur", "al\u00b7lein", "soll", "be\u00b7wer\u00b7ben", "und", "be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "ADV", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}