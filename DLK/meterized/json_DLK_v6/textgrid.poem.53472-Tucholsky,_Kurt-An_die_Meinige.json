{"textgrid.poem.53472": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An die Meinige", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Legt man die Hand jetzt auf die Gummiwaren?", "tokens": ["Legt", "man", "die", "Hand", "jetzt", "auf", "die", "Gum\u00b7mi\u00b7wa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erinnre, Claire, dich an deine Pflicht!", "tokens": ["E\u00b7rinn\u00b7re", ",", "Clai\u00b7re", ",", "dich", "an", "dei\u00b7ne", "Pflicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das geht nicht so wie in den letzten Jahren:", "tokens": ["Das", "geht", "nicht", "so", "wie", "in", "den", "letz\u00b7ten", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "KOKOM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Du bist steril, und du vermehrst dich nicht!", "tokens": ["Du", "bist", "ste\u00b7ril", ",", "und", "du", "ver\u00b7mehrst", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohlauf! Wohlan! Zu Deutschlands Ruhm und Ehren!", "tokens": ["Wohl\u00b7auf", "!", "Wo\u00b7hlan", "!", "Zu", "Deutschlands", "Ruhm", "und", "Eh\u00b7ren", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Vorbei ist nun der Liebe gr\u00fcner Mai \u2013", "tokens": ["Vor\u00b7bei", "ist", "nun", "der", "Lie\u00b7be", "gr\u00fc\u00b7ner", "Mai", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da hilft nun nichts: du mu\u00dft etwas geb\u00e4ren,", "tokens": ["da", "hilft", "nun", "nichts", ":", "du", "mu\u00dft", "et\u00b7was", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "einmal, vielleicht auch zweimal oder drei!", "tokens": ["ein\u00b7mal", ",", "viel\u00b7leicht", "auch", "zwei\u00b7mal", "o\u00b7der", "drei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "ADV", "KON", "CARD", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.3": {"line.1": {"text": "Wir Deutschen sind die Allerallerersten,", "tokens": ["Wir", "Deut\u00b7schen", "sind", "die", "Al\u00b7le\u00b7ral\u00b7le\u00b7rers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "voran der Kronprinz als Eins-A-Papa.", "tokens": ["vo\u00b7ran", "der", "Kron\u00b7prinz", "als", "Eins\u00b7A\u00b7Pa\u00b7pa", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Gallier faucht \u2013 wir haben doch die mehrsten,", "tokens": ["Der", "Gal\u00b7lier", "faucht", "\u2013", "wir", "ha\u00b7ben", "doch", "die", "mehrs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und hungern sie, mein Gott, sie sind doch da!", "tokens": ["und", "hun\u00b7gern", "sie", ",", "mein", "Gott", ",", "sie", "sind", "doch", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn sieh: die Babys brauchen Medizinen", "tokens": ["Denn", "sieh", ":", "die", "Ba\u00b7bys", "brau\u00b7chen", "Me\u00b7di\u00b7zi\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und manchmal auch ein wei\u00df Get\u00f6pf aus Ton,", "tokens": ["und", "manch\u00b7mal", "auch", "ein", "wei\u00df", "Ge\u00b7t\u00f6pf", "aus", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geb\u00e4ck, das Milchgetr\u00e4nk \u2013 man kauft es ihnen,", "tokens": ["Ge\u00b7b\u00e4ck", ",", "das", "Milch\u00b7ge\u00b7tr\u00e4nk", "\u2013", "man", "kauft", "es", "ih\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$(", "PIS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und dann vor allem, Kind, die Konfektion!", "tokens": ["und", "dann", "vor", "al\u00b7lem", ",", "Kind", ",", "die", "Kon\u00b7fek\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wer soll in des Kaisers R\u00f6cken dienen,", "tokens": ["Und", "wer", "soll", "in", "des", "Kai\u00b7sers", "R\u00f6\u00b7cken", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "umbr\u00fcllt vom Leutnant und vom General?", "tokens": ["um\u00b7br\u00fcllt", "vom", "Leut\u00b7nant", "und", "vom", "Ge\u00b7ne\u00b7ral", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stell du das her: es mu\u00df nur maskulinen", "tokens": ["Stell", "du", "das", "her", ":", "es", "mu\u00df", "nur", "mas\u00b7ku\u00b7li\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PDS", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geschlechtes sein \u2013 der Sch\u00e4del ist egal.", "tokens": ["Ge\u00b7schlech\u00b7tes", "sein", "\u2013", "der", "Sch\u00e4\u00b7del", "ist", "e\u00b7gal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$(", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ins Bett! Hier hast du deine Wickelbinden!", "tokens": ["Ins", "Bett", "!", "Hier", "hast", "du", "dei\u00b7ne", "Wi\u00b7ckel\u00b7bin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schenk mir den Leo nebst der Annmarei!", "tokens": ["Schenk", "mir", "den", "Leo", "nebst", "der", "A\u00b7nnma\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und z\u00e4hlt man nach, wird man voll Freude finden", "tokens": ["Und", "z\u00e4hlt", "man", "nach", ",", "wird", "man", "voll", "Freu\u00b7de", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,", "VAFIN", "PIS", "ADJD", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "sechzig Millionen, und von uns", "tokens": ["sech\u00b7zig", "Mil\u00b7lion\u00b7en", ",", "und", "von", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "KON", "APPR", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "die zwei!", "tokens": ["die", "zwei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "CARD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}