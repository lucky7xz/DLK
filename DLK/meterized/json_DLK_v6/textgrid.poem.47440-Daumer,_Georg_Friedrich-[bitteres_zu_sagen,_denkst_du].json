{"textgrid.poem.47440": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[bitteres zu sagen, denkst du]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bitteres zu sagen, denkst du;", "tokens": ["Bit\u00b7te\u00b7res", "zu", "sa\u00b7gen", ",", "denkst", "du", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Aber nun und nimmer kr\u00e4nkst du,", "tokens": ["A\u00b7ber", "nun", "und", "nim\u00b7mer", "kr\u00e4nkst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob du noch so b\u00f6se bist.", "tokens": ["Ob", "du", "noch", "so", "b\u00f6\u00b7se", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine herben Redethaten", "tokens": ["Dei\u00b7ne", "her\u00b7ben", "Re\u00b7de\u00b7tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Scheitern an korallner Klippe,", "tokens": ["Schei\u00b7tern", "an", "ko\u00b7rall\u00b7ner", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werden all zu reinen Gnaden,", "tokens": ["Wer\u00b7den", "all", "zu", "rei\u00b7nen", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn sie m\u00fcssen, um zu schaden,", "tokens": ["Denn", "sie", "m\u00fcs\u00b7sen", ",", "um", "zu", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schiffen \u00fcber eine Lippe,", "tokens": ["Schif\u00b7fen", "\u00fc\u00b7ber", "ei\u00b7ne", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die die S\u00fc\u00dfe selber ist.", "tokens": ["Die", "die", "S\u00fc\u00b7\u00dfe", "sel\u00b7ber", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}