{"textgrid.poem.57403": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Verh\u00e4ngnisse", "genre": "verse", "period": "N.A.", "pub_year": 1747, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nigen gab der Olympier Stolz, und sklavischen P\u00f6bel", "tokens": ["K\u00f6\u00b7ni\u00b7gen", "gab", "der", "O\u00b7lym\u00b7pier", "Stolz", ",", "und", "skla\u00b7vi\u00b7schen", "P\u00f6\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Um den gef\u00fcrchteten Thron:", "tokens": ["Um", "den", "ge\u00b7f\u00fcrch\u00b7te\u00b7ten", "Thron", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weisheit gab er den K\u00f6nigen nicht; sonst hielten sie Menschen", "tokens": ["Weis\u00b7heit", "gab", "er", "den", "K\u00f6\u00b7ni\u00b7gen", "nicht", ";", "sonst", "hiel\u00b7ten", "sie", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$.", "ADV", "VVFIN", "PPER", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Nicht f\u00fcr w\u00fcrgbares Vieh.", "tokens": ["Nicht", "f\u00fcr", "w\u00fcrg\u00b7ba\u00b7res", "Vieh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Philosophen gab er den Traum, da Wahrheit zu suchen,", "tokens": ["Phi\u00b7lo\u00b7so\u00b7phen", "gab", "er", "den", "Traum", ",", "da", "Wahr\u00b7heit", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-++--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wo sie zu finden nicht ist:", "tokens": ["Wo", "sie", "zu", "fin\u00b7den", "nicht", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "PTKNEG", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Priestern den Wahn, die g\u00f6ttlichste Wahrheit durch alles zu lehren,", "tokens": ["Pries\u00b7tern", "den", "Wahn", ",", "die", "g\u00f6tt\u00b7lichs\u00b7te", "Wahr\u00b7heit", "durch", "al\u00b7les", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Nur durch Tugenden nicht.", "tokens": ["Nur", "durch", "Tu\u00b7gen\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Alles dies gab er im Zorn. Sehr wenig K\u00f6nige weihen", "tokens": ["Al\u00b7les", "dies", "gab", "er", "im", "Zorn", ".", "Sehr", "we\u00b7nig", "K\u00f6\u00b7ni\u00b7ge", "wei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "PDS", "VVFIN", "PPER", "APPRART", "NN", "$.", "ADV", "PIAT", "NN", "VVINF"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Ihr erhabenes Amt", "tokens": ["Ihr", "er\u00b7ha\u00b7be\u00b7nes", "Amt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Durch ein Gott nachahmendes Wohltun, das \u00fcber die Menschheit", "tokens": ["Durch", "ein", "Gott", "nac\u00b7hah\u00b7men\u00b7des", "Wohl\u00b7tun", ",", "das", "\u00fc\u00b7ber", "die", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Sterbliche Menschen erh\u00f6ht.", "tokens": ["Sterb\u00b7li\u00b7che", "Men\u00b7schen", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Wenig Philosophen erreichen die n\u00e4here Weisheit,", "tokens": ["We\u00b7nig", "Phi\u00b7lo\u00b7so\u00b7phen", "er\u00b7rei\u00b7chen", "die", "n\u00e4\u00b7he\u00b7re", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Die Gl\u00fcckseligkeit ist.", "tokens": ["Die", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Selten wandeln Priester dem nach, der lebend sie lehrte,", "tokens": ["Sel\u00b7ten", "wan\u00b7deln", "Pries\u00b7ter", "dem", "nach", ",", "der", "le\u00b7bend", "sie", "lehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "APPR", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Und viel weniger sprach.", "tokens": ["Und", "viel", "we\u00b7ni\u00b7ger", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Tugend gab er nicht Menschen, die gab er Engeln. Ihr Bildnis", "tokens": ["Tu\u00b7gend", "gab", "er", "nicht", "Men\u00b7schen", ",", "die", "gab", "er", "En\u00b7geln", ".", "Ihr", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "NN", "$,", "PRELS", "VVFIN", "PPER", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Lie\u00df er den Sterblichen nur.", "tokens": ["Lie\u00df", "er", "den", "Sterb\u00b7li\u00b7chen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Mir gab er die singende Leier, und redliche Freunde.", "tokens": ["Mir", "gab", "er", "die", "sin\u00b7gen\u00b7de", "Lei\u00b7er", ",", "und", "red\u00b7li\u00b7che", "Freun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.20": {"text": "Wollt ich, was gr\u00f6\u00dfer noch ist,", "tokens": ["Wollt", "ich", ",", "was", "gr\u00f6\u00b7\u00dfer", "noch", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "ADJD", "ADV", "VAFIN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Wollt ich der Himmlischen Gl\u00fcck, die selige Liebe noch bitten,", "tokens": ["Wollt", "ich", "der", "Himm\u00b7li\u00b7schen", "Gl\u00fcck", ",", "die", "se\u00b7li\u00b7ge", "Lie\u00b7be", "noch", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "O so b\u00e4t ich zu viel!", "tokens": ["O", "so", "b\u00e4t", "ich", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PTKA", "PIS", "$."], "meter": "-----+", "measure": "unknown.measure.single"}, "line.23": {"text": "O so b\u00e4t ich auch Tugend! Die gab er Engeln! Ihr Bildnis", "tokens": ["O", "so", "b\u00e4t", "ich", "auch", "Tu\u00b7gend", "!", "Die", "gab", "er", "En\u00b7geln", "!", "Ihr", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "NN", "$.", "ART", "VVFIN", "PPER", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Lie\u00df er den Sterblichen nur.", "tokens": ["Lie\u00df", "er", "den", "Sterb\u00b7li\u00b7chen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Ist die Leier der Weisheit nicht heilig, und singet sie jemals", "tokens": ["Ist", "die", "Lei\u00b7er", "der", "Weis\u00b7heit", "nicht", "hei\u00b7lig", ",", "und", "sin\u00b7get", "sie", "je\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Was Geringers als sie;", "tokens": ["Was", "Ge\u00b7rin\u00b7gers", "als", "sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOUS", "PPER", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Lieb ich die Freunde nicht treu, die so voll Freundschaft mich lieben,", "tokens": ["Lieb", "ich", "die", "Freun\u00b7de", "nicht", "treu", ",", "die", "so", "voll", "Freund\u00b7schaft", "mich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$,", "PRELS", "ADV", "ADJD", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+--++--+-", "measure": "dactylic.tri.plus"}, "line.28": {"text": "O so sind mir von ihm,", "tokens": ["O", "so", "sind", "mir", "von", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.29": {"text": "Alles was er mir gab, auch die unvergeltbarsten Gaben,", "tokens": ["Al\u00b7les", "was", "er", "mir", "gab", ",", "auch", "die", "un\u00b7ver\u00b7gelt\u00b7bars\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "PPER", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Auch im Zorne verliehn.", "tokens": ["Auch", "im", "Zor\u00b7ne", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}