{"dta.poem.17905": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "148.  Wer hier satt wird/ kan dort nicht essen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie da\u00df der Fra\u00df nicht kommt zum ewgen Abendessen/", "tokens": ["Wie", "da\u00df", "der", "Fra\u00df", "nicht", "kommt", "zum", "ew\u00b7gen", "A\u00b7ben\u00b7des\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er mag nicht weil er hier sich hat zu satt gefressen.", "tokens": ["Er", "mag", "nicht", "weil", "er", "hier", "sich", "hat", "zu", "satt", "ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "KOUS", "PPER", "ADV", "PRF", "VAFIN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}