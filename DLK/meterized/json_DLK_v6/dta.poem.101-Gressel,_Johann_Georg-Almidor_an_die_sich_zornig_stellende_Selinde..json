{"dta.poem.101": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Almidor  an die sich zornig stellende  Selinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Selinde siehe doch/ und lencke jene Strahlen", "tokens": ["Se\u00b7lin\u00b7de", "sie\u00b7he", "doch", "/", "und", "len\u00b7cke", "je\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$(", "KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6nen Augen her auf dieses Blat Papier/", "tokens": ["Der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "her", "auf", "die\u00b7ses", "Blat", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PDAT", "NN", "NN", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Lie\u00df es nur obenhin/ denn wirff es weg von dir/", "tokens": ["Lie\u00df", "es", "nur", "o\u00b7ben\u00b7hin", "/", "denn", "wirff", "es", "weg", "von", "dir", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$(", "KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn es den Augen kan die M\u00fche nicht bezahlen/", "tokens": ["Wenn", "es", "den", "Au\u00b7gen", "kan", "die", "M\u00fc\u00b7he", "nicht", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sie es angesehn.", "tokens": ["Da\u00df", "sie", "es", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wann aber/ sch\u00f6nstes Kind/ die Worte dir gefallen/", "tokens": ["Wann", "a\u00b7ber", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "die", "Wor\u00b7te", "dir", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$(", "ADJA", "NN", "$(", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So lie\u00df sie g\u00e4ntzlich durch/ dein Z\u00fcrnen stelle ein/", "tokens": ["So", "lie\u00df", "sie", "g\u00e4ntz\u00b7lich", "durch", "/", "dein", "Z\u00fcr\u00b7nen", "stel\u00b7le", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "$(", "PPOSAT", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und la\u00df sie zwischen uns die Friedens-Stiffter seyn;", "tokens": ["Und", "la\u00df", "sie", "zwi\u00b7schen", "uns", "die", "Frie\u00b7dens\u00b7Stiff\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hernach so stecke es zu deinen Marmor-Ballen", "tokens": ["Her\u00b7nach", "so", "ste\u00b7cke", "es", "zu", "dei\u00b7nen", "Mar\u00b7mor\u00b7Bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sonsten wol geschehn.", "tokens": ["Wie", "sons\u00b7ten", "wol", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wenn eine treue Hand der Liebsten etwas schicket/", "tokens": ["Wenn", "ei\u00b7ne", "treu\u00b7e", "Hand", "der", "Liebs\u00b7ten", "et\u00b7was", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das/ weil es heimlich ist/ nicht jeder darf besehn/", "tokens": ["Das", "/", "weil", "es", "heim\u00b7lich", "ist", "/", "nicht", "je\u00b7der", "darf", "be\u00b7sehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "PTKNEG", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So mu\u00df der Busen ihm zur Frey-Stadt offen stehn/", "tokens": ["So", "mu\u00df", "der", "Bu\u00b7sen", "ihm", "zur", "Frey\u00b7Stadt", "of\u00b7fen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPRART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Eh denn das liebe Blat ihr wird hinweg ger\u00fccket", "tokens": ["Eh", "denn", "das", "lie\u00b7be", "Blat", "ihr", "wird", "hin\u00b7weg", "ge\u00b7r\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Von dem/ der H\u00fcter ist.", "tokens": ["Von", "dem", "/", "der", "H\u00fc\u00b7ter", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ist nun das Blat versteckt/ so darf dein Ohr nicht h\u00f6ren", "tokens": ["Ist", "nun", "das", "Blat", "ver\u00b7steckt", "/", "so", "darf", "dein", "Ohr", "nicht", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$(", "ADV", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das L\u00e4stern/ so der Mund der Mutter sonst ausspeit/", "tokens": ["Das", "L\u00e4s\u00b7tern", "/", "so", "der", "Mund", "der", "Mut\u00b7ter", "sonst", "aus\u00b7speit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die auf dich Achtung giebt bey Nacht und Tages-Zeit/", "tokens": ["Die", "auf", "dich", "Ach\u00b7tung", "giebt", "bey", "Nacht", "und", "Ta\u00b7ges\u00b7Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und als ein ", "tokens": ["Und", "als", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Zu dir/ die ich erkie\u00dft.", "tokens": ["Zu", "dir", "/", "die", "ich", "er\u00b7kie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Sie aber wird von uns arglistiglich betrogen/", "tokens": ["Sie", "a\u00b7ber", "wird", "von", "uns", "arg\u00b7lis\u00b7tig\u00b7lich", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn du/ mein Kind/ und ich nur eines Sinnes sind;", "tokens": ["Wenn", "du", "/", "mein", "Kind", "/", "und", "ich", "nur", "ei\u00b7nes", "Sin\u00b7nes", "sind", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPOSAT", "NN", "$(", "KON", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie wolte ", "tokens": ["Sie", "wol\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Als dein ", "tokens": ["Als", "dein"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Und in dein Bette kahm.", "tokens": ["Und", "in", "dein", "Bet\u00b7te", "kahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Da hat dein Enge-Land ihn inniglich erg\u00f6tzet/", "tokens": ["Da", "hat", "dein", "En\u00b7ge\u00b7Land", "ihn", "in\u00b7nig\u00b7lich", "er\u00b7g\u00f6t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mit mehr als ", "tokens": ["Mit", "mehr", "als"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Jhn hat das s\u00fcsse Safft der Lippen Honig-Kost", "tokens": ["Jhn", "hat", "das", "s\u00fcs\u00b7se", "Safft", "der", "Lip\u00b7pen", "Ho\u00b7nig\u00b7Kost"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Entz\u00fccket/ und die Lust an jenen Ort gesetzet/", "tokens": ["Ent\u00b7z\u00fc\u00b7cket", "/", "und", "die", "Lust", "an", "je\u00b7nen", "Ort", "ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Bey der ", "tokens": ["Bey", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Da fandt er sich vergn\u00fcgt als eine Biene ein/", "tokens": ["Da", "fandt", "er", "sich", "ver\u00b7gn\u00fcgt", "als", "ei\u00b7ne", "Bie\u00b7ne", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "KOKOM", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach! m\u00f6chte diese Lust mir bald erlaubet seyn/", "tokens": ["Ach", "!", "m\u00f6ch\u00b7te", "die\u00b7se", "Lust", "mir", "bald", "er\u00b7lau\u00b7bet", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PDAT", "NN", "PPER", "ADV", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und da\u00df ich jenen Zorn/ mein Kind/ verbannet w\u00fcste/", "tokens": ["Und", "da\u00df", "ich", "je\u00b7nen", "Zorn", "/", "mein", "Kind", "/", "ver\u00b7ban\u00b7net", "w\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der mir ein Ungel\u00fcck.", "tokens": ["Der", "mir", "ein", "Un\u00b7ge\u00b7l\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Dis hat ", "tokens": ["Dis", "hat"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Zu sehen ob dein Zorn sich noch nicht hat gestillt/", "tokens": ["Zu", "se\u00b7hen", "ob", "dein", "Zorn", "sich", "noch", "nicht", "hat", "ge\u00b7stillt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOUS", "PPOSAT", "NN", "PRF", "ADV", "PTKNEG", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und ob du/ Engels-Kind/ mich ewig hassen wilt/", "tokens": ["Und", "ob", "du", "/", "En\u00b7gels\u00b7Kind", "/", "mich", "e\u00b7wig", "has\u00b7sen", "wilt", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$(", "NE", "$(", "PPER", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn du ", "tokens": ["Wenn", "du"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "So schreibe doch zur\u00fcck.", "tokens": ["So", "schrei\u00b7be", "doch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}