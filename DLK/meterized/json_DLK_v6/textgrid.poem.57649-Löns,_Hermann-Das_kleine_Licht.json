{"textgrid.poem.57649": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das kleine Licht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den wei\u00dfen, mondbeschienenen Weg", "tokens": ["Den", "wei\u00b7\u00dfen", ",", "mond\u00b7be\u00b7schie\u00b7ne\u00b7nen", "Weg"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwei schwarze Mauern s\u00e4umen,", "tokens": ["Zwei", "schwar\u00b7ze", "Mau\u00b7ern", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein schwarzes, schweigendes Geheg", "tokens": ["Ein", "schwar\u00b7zes", ",", "schwei\u00b7gen\u00b7des", "Ge\u00b7heg"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von schwarzen, schweigenden B\u00e4umen.", "tokens": ["Von", "schwar\u00b7zen", ",", "schwei\u00b7gen\u00b7den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So weit ist der todeseinsame Pfad,", "tokens": ["So", "weit", "ist", "der", "to\u00b7de\u00b7sein\u00b7sa\u00b7me", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So eng die N\u00e4he dunkelt,", "tokens": ["So", "eng", "die", "N\u00e4\u00b7he", "dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort hinten, wo schwarz sich die Wolke naht,", "tokens": ["Dort", "hin\u00b7ten", ",", "wo", "schwarz", "sich", "die", "Wol\u00b7ke", "naht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein glimmendes Lichtchen funkelt.", "tokens": ["Ein", "glim\u00b7men\u00b7des", "Licht\u00b7chen", "fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Nacht immer schwerer herniederbricht,", "tokens": ["Die", "Nacht", "im\u00b7mer", "schwe\u00b7rer", "her\u00b7nie\u00b7der\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "M\u00fcdigkeit meine Stirne befeuchtet,", "tokens": ["M\u00fc\u00b7dig\u00b7keit", "mei\u00b7ne", "Stir\u00b7ne", "be\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So ferne zittert das kleine Licht", "tokens": ["So", "fer\u00b7ne", "zit\u00b7tert", "das", "klei\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ich wei\u00df nicht einmal, wo es leuchtet.", "tokens": ["Und", "ich", "wei\u00df", "nicht", "ein\u00b7mal", ",", "wo", "es", "leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}