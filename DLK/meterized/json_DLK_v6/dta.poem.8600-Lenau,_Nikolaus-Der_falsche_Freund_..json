{"dta.poem.8600": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der falsche Freund .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eo sey mein Freund!\u201c so schallt's vom Heuchelmunde", "tokens": ["\u201e", "o", "sey", "mein", "Freund", "!", "\u201c", "so", "schallt's", "vom", "Heu\u00b7chel\u00b7mun\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "FM", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Falschen, der mit heimlichem Behagen", "tokens": ["Dem", "Fal\u00b7schen", ",", "der", "mit", "heim\u00b7li\u00b7chem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Vortheil \u00fcberz\u00e4hlt von solchem Bunde.", "tokens": ["Den", "Vor\u00b7theil", "\u00fc\u00b7berz\u00b7\u00e4hlt", "von", "sol\u00b7chem", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du traust ihm, und \u2014 schon hast du eingeschlagen,", "tokens": ["Du", "traust", "ihm", ",", "und", "schon", "hast", "du", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "$(", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein edler Thor! naht einst die Wetterstunde,", "tokens": ["Ein", "ed\u00b7ler", "Thor", "!", "naht", "einst", "die", "Wet\u00b7ter\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So siehst den Schurken du mit bleichem Zagen", "tokens": ["So", "siehst", "den", "Schur\u00b7ken", "du", "mit", "blei\u00b7chem", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In seines Ichs bequeme H\u00fctte springen,", "tokens": ["In", "sei\u00b7nes", "Ichs", "be\u00b7que\u00b7me", "H\u00fct\u00b7te", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hinausgesperrt magst mit dem Sturm du ringen.", "tokens": ["Hin\u00b7aus\u00b7ge\u00b7sperrt", "magst", "mit", "dem", "Sturm", "du", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}