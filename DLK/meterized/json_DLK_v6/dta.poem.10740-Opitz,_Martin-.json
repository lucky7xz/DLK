{"dta.poem.10740": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Rot R\u00f6slein wolt' ich brechen", "tokens": ["Rot", "R\u00f6s\u00b7lein", "wolt'", "ich", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum h\u00fcbschen Krentzelein:", "tokens": ["Zum", "h\u00fcb\u00b7schen", "Krent\u00b7zel\u00b7ein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich D\u00f6rner thaten stechen", "tokens": ["Mich", "D\u00f6r\u00b7ner", "tha\u00b7ten", "ste\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hart in die finger mein.", "tokens": ["Hart", "in", "die", "fin\u00b7ger", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch wolt\u2019 ich nit lan ab.", "tokens": ["Noch", "wolt'", "ich", "nit", "lan", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich gunt mich weiter stecken", "tokens": ["Ich", "gunt", "mich", "wei\u00b7ter", "ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In Stauden vnd in Hecken:", "tokens": ["In", "Stau\u00b7den", "vnd", "in", "He\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Darin mirs wunden gab.", "tokens": ["Da\u00b7rin", "mirs", "wun\u00b7den", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}