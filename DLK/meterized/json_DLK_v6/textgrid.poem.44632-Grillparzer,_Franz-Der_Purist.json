{"textgrid.poem.44632": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Purist", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was nach Gallien klingt \u2013 fort aus dem Kreise der Deutschen!", "tokens": ["Was", "nach", "Gal\u00b7li\u00b7en", "klingt", "\u2013", "fort", "aus", "dem", "Krei\u00b7se", "der", "Deut\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VVFIN", "$(", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fort mit dem Sens comun, dann folgt von selbst das Genie.", "tokens": ["Fort", "mit", "dem", "Sens", "co\u00b7mun", ",", "dann", "folgt", "von", "selbst", "das", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,", "ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}}}}