{"dta.poem.6416": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein saures Theilchen ist von spitziger Figur,", "tokens": ["Ein", "sau\u00b7res", "Theil\u00b7chen", "ist", "von", "spit\u00b7zi\u00b7ger", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dr\u00e4ngt sich in die Zung hinein,", "tokens": ["Und", "dr\u00e4ngt", "sich", "in", "die", "Zung", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es gehet tieff in ihre Z\u00e4serlein,", "tokens": ["Es", "ge\u00b7het", "tieff", "in", "ih\u00b7re", "Z\u00e4\u00b7ser\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4sset eine tieffe Spur.", "tokens": ["Und", "l\u00e4s\u00b7set", "ei\u00b7ne", "tief\u00b7fe", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was bitter aber, ist demselben gantz entgegen,", "tokens": ["Was", "bit\u00b7ter", "a\u00b7ber", ",", "ist", "dem\u00b7sel\u00b7ben", "gantz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "$,", "VAFIN", "PDS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn das, was eigentlich die Bitterkeit,", "tokens": ["Denn", "das", ",", "was", "ei\u00b7gent\u00b7lich", "die", "Bit\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sind Theile, welche platt und von Beschaffenheit", "tokens": ["Sind", "Thei\u00b7le", ",", "wel\u00b7che", "platt", "und", "von", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verwirrt, stumpf, ungleich, schwer, und die sich nicht bewegen:", "tokens": ["Ver\u00b7wirrt", ",", "stumpf", ",", "un\u00b7gleich", ",", "schwer", ",", "und", "die", "sich", "nicht", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "KON", "PRELS", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr Druck ist plump und sonder Krafft und Geist,", "tokens": ["Ihr", "Druck", "ist", "plump", "und", "son\u00b7der", "Krafft", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Spitzen sind nicht spitz, da keine sticht und beist.", "tokens": ["Die", "Spit\u00b7zen", "sind", "nicht", "spitz", ",", "da", "kei\u00b7ne", "sticht", "und", "beist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PIAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Nerven, die ihr Druck versehret,", "tokens": ["Die", "Ner\u00b7ven", ",", "die", "ihr", "Druck", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sind sehr dadurch beleidigt und beschweret.", "tokens": ["Sind", "sehr", "da\u00b7durch", "be\u00b7lei\u00b7digt", "und", "be\u00b7schwe\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}