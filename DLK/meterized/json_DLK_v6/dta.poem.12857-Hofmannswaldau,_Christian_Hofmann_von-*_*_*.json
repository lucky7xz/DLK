{"dta.poem.12857": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So in mienen, als im hertzen", "tokens": ["So", "in", "mie\u00b7nen", ",", "als", "im", "hert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jmmer fr\u00f6lich seyn und lachen,", "tokens": ["Jm\u00b7mer", "fr\u00f6\u00b7lich", "seyn", "und", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein traurig auge machen,", "tokens": ["Und", "kein", "trau\u00b7rig", "au\u00b7ge", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dft nicht lieben, sondern schertzen.", "tokens": ["Hei\u00dft", "nicht", "lie\u00b7ben", ",", "son\u00b7dern", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}