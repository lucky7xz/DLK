{"textgrid.poem.26964": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Moralisten", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lehret! das ziemet euch wohl, auch wir verehren die Sitte,", "tokens": ["Leh\u00b7ret", "!", "das", "zie\u00b7met", "euch", "wohl", ",", "auch", "wir", "ver\u00b7eh\u00b7ren", "die", "Sit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PDS", "VVFIN", "PPER", "ADV", "$,", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber die Muse l\u00e4\u00dft sich nicht gebieten von euch.", "tokens": ["A\u00b7ber", "die", "Mu\u00b7se", "l\u00e4\u00dft", "sich", "nicht", "ge\u00b7bie\u00b7ten", "von", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVPP", "APPR", "PPER", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Nicht von dem Architekt erwart ich melodische Weisen,", "tokens": ["Nicht", "von", "dem", "Ar\u00b7chi\u00b7tekt", "er\u00b7wart", "ich", "me\u00b7lo\u00b7di\u00b7sche", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und, Moralist, von dir nicht zu dem Epos den Plan.", "tokens": ["Und", ",", "Mo\u00b7ra\u00b7list", ",", "von", "dir", "nicht", "zu", "dem", "E\u00b7pos", "den", "Plan", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "APPR", "PPER", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Vielfach sind die Kr\u00e4fte des Menschen, o da\u00df sich doch jede", "tokens": ["Viel\u00b7fach", "sind", "die", "Kr\u00e4f\u00b7te", "des", "Men\u00b7schen", ",", "o", "da\u00df", "sich", "doch", "je\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,", "FM", "KOUS", "PRF", "ADV", "PIAT"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Selbst beherrsche, sich selbst bilde zum herrlichsten aus!", "tokens": ["Selbst", "be\u00b7herr\u00b7sche", ",", "sich", "selbst", "bil\u00b7de", "zum", "herr\u00b7lichs\u00b7ten", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRF", "ADV", "VVFIN", "APPRART", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}