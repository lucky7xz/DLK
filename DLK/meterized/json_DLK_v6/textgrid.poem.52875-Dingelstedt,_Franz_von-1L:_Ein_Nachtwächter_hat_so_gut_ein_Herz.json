{"textgrid.poem.52875": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Nachtw\u00e4chter hat so gut ein Herz", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Nachtw\u00e4chter hat so gut ein Herz", "tokens": ["Ein", "Nacht\u00b7w\u00e4ch\u00b7ter", "hat", "so", "gut", "ein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie ein schmachtender Held der Frauen,", "tokens": ["Wie", "ein", "schmach\u00b7ten\u00b7der", "Held", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auch er f\u00fchlt Liebeslust und Schmerz", "tokens": ["Auch", "er", "f\u00fchlt", "Lie\u00b7bes\u00b7lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn die Kater im M\u00e4rze miauen.", "tokens": ["Wenn", "die", "Ka\u00b7ter", "im", "M\u00e4r\u00b7ze", "mi\u00b7au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Drum, wann ich abends ausw\u00e4rts geh'", "tokens": ["Drum", ",", "wann", "ich", "a\u00b7bends", "aus\u00b7w\u00e4rts", "geh'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mein Weib in der ganzen Nacht nicht seh',", "tokens": ["Und", "mein", "Weib", "in", "der", "gan\u00b7zen", "Nacht", "nicht", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Verlangt mich's nach Mariandel sehr;", "tokens": ["Ver\u00b7langt", "mich's", "nach", "Ma\u00b7ri\u00b7an\u00b7del", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NE", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ja, wenn sie nur nicht so garstig w\u00e4r'!", "tokens": ["Ja", ",", "wenn", "sie", "nur", "nicht", "so", "gars\u00b7tig", "w\u00e4r'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sie ist eine gute, alte Haut", "tokens": ["Sie", "ist", "ei\u00b7ne", "gu\u00b7te", ",", "al\u00b7te", "Haut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mit mehr Runzeln als just notwendig,", "tokens": ["Mit", "mehr", "Run\u00b7zeln", "als", "just", "not\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KOKOM", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihr Vater hat sie mir angetraut", "tokens": ["Ihr", "Va\u00b7ter", "hat", "sie", "mir", "an\u00b7ge\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Haus und Ger\u00e4t vollst\u00e4ndig;", "tokens": ["Mit", "Haus", "und", "Ge\u00b7r\u00e4t", "voll\u00b7st\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das Amt und dreihundert Gulden dazu, \u2013", "tokens": ["Das", "Amt", "und", "drei\u00b7hun\u00b7dert", "Gul\u00b7den", "da\u00b7zu", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "CARD", "NN", "PAV", "$,", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gott schenke dem Alten ewige Ruh'!", "tokens": ["Gott", "schen\u00b7ke", "dem", "Al\u00b7ten", "e\u00b7wi\u00b7ge", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ich liebte auch seine Tochter mehr,", "tokens": ["Ich", "lieb\u00b7te", "auch", "sei\u00b7ne", "Toch\u00b7ter", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ja, wenn sie nur nicht so garstig w\u00e4r'!", "tokens": ["Ja", ",", "wenn", "sie", "nur", "nicht", "so", "gars\u00b7tig", "w\u00e4r'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wir leben wie zwo Engelein", "tokens": ["Wir", "le\u00b7ben", "wie", "zwo", "En\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Paradies vor dem Falle;", "tokens": ["Im", "Pa\u00b7ra\u00b7dies", "vor", "dem", "Fal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Keine Ehe kann so gl\u00fccklich sein,", "tokens": ["Kei\u00b7ne", "E\u00b7he", "kann", "so", "gl\u00fcck\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als unsre, ein Muster f\u00fcr alle.", "tokens": ["Als", "uns\u00b7re", ",", "ein", "Mus\u00b7ter", "f\u00fcr", "al\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "$,", "ART", "NN", "APPR", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie schl\u00e4ft des Nachts, ich schlaf' am Tag,", "tokens": ["Sie", "schl\u00e4ft", "des", "Nachts", ",", "ich", "schlaf'", "am", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie nimmt den Schluck, den ich nicht mag,", "tokens": ["Sie", "nimmt", "den", "Schluck", ",", "den", "ich", "nicht", "mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das einigste P\u00e4rlein weit umher,", "tokens": ["Das", "ei\u00b7nigs\u00b7te", "P\u00e4r\u00b7lein", "weit", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ja, wenn sie nur nicht so garstig w\u00e4r'!", "tokens": ["Ja", ",", "wenn", "sie", "nur", "nicht", "so", "gars\u00b7tig", "w\u00e4r'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "So oft ich Nachts in mein Haus geguckt,", "tokens": ["So", "oft", "ich", "Nachts", "in", "mein", "Haus", "ge\u00b7guckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "War's ruhig allerwegen.", "tokens": ["Wa\u00b7r's", "ru\u00b7hig", "al\u00b7ler\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch nie hat's mich an der Stirne gejuckt,", "tokens": ["Noch", "nie", "hat's", "mich", "an", "der", "Stir\u00b7ne", "ge\u00b7juckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie so viele meiner Kollegen;", "tokens": ["Wie", "so", "vie\u00b7le", "mei\u00b7ner", "Kol\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Bei denen geht's wie ein Taubenschlag,", "tokens": ["Bei", "de\u00b7nen", "geht's", "wie", "ein", "Tau\u00b7ben\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hinein bei der Nacht, heraus am Tag,", "tokens": ["Hin\u00b7ein", "bei", "der", "Nacht", ",", "he\u00b7raus", "am", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und ein Nachtw\u00e4chter h\u00e4lt doch auch auf Ehr',", "tokens": ["Und", "ein", "Nacht\u00b7w\u00e4ch\u00b7ter", "h\u00e4lt", "doch", "auch", "auf", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ja, wenn sie nur nicht so garstig w\u00e4r'!", "tokens": ["Ja", ",", "wenn", "sie", "nur", "nicht", "so", "gars\u00b7tig", "w\u00e4r'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}