{"textgrid.poem.38915": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit tr\u00fcbem Auge", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit tr\u00fcbem Auge", "tokens": ["Mit", "tr\u00fc\u00b7bem", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In finstrer Nacht,", "tokens": ["In", "finst\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Geht durch das Leben", "tokens": ["Geht", "durch", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das Kind geleitet", "tokens": ["Das", "Kind", "ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Vom ernsten F\u00fchrer,", "tokens": ["Vom", "erns\u00b7ten", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den es nicht kennt.", "tokens": ["Den", "es", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Im Thal, am lauten Wasserfall", "tokens": ["Im", "Thal", ",", "am", "lau\u00b7ten", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stehn beide Wandrer still,", "tokens": ["Stehn", "bei\u00b7de", "Wand\u00b7rer", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der F\u00fchrer spricht zum Horchenden:", "tokens": ["Der", "F\u00fch\u00b7rer", "spricht", "zum", "Hor\u00b7chen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, hier bl\u00fchen alle Blumen,", "tokens": ["Sieh", ",", "hier", "bl\u00fc\u00b7hen", "al\u00b7le", "Blu\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle W\u00fcnsche, alle Freuden,", "tokens": ["Al\u00b7le", "W\u00fcn\u00b7sche", ",", "al\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfl\u00fccke, denn wie flie\u00dfend Wasser", "tokens": ["Pfl\u00fc\u00b7cke", ",", "denn", "wie", "flie\u00b7\u00dfend", "Was\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWAV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauscht das Leben dir vor\u00fcber.", "tokens": ["Rauscht", "das", "Le\u00b7ben", "dir", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fort weicht die Gestalt,", "tokens": ["Fort", "weicht", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und tiefbek\u00fcmmert", "tokens": ["Und", "tief\u00b7be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sieht ihr mit langem Blicke", "tokens": ["Sieht", "ihr", "mit", "lan\u00b7gem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der einsam Verla\u00dfne schmachtend nach.", "tokens": ["Der", "ein\u00b7sam", "Ver\u00b7la\u00df\u00b7ne", "schmach\u00b7tend", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wind s\u00e4uselt in den Blumen,", "tokens": ["Wind", "s\u00e4u\u00b7selt", "in", "den", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wellen murmeln so wie zum fr\u00f6hlichen Tanz,", "tokens": ["Wel\u00b7len", "mur\u00b7meln", "so", "wie", "zum", "fr\u00f6h\u00b7li\u00b7chen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da beugt sich der Fremdling", "tokens": ["Da", "beugt", "sich", "der", "Fremd\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und m\u00e4ht mit raschen zitternden H\u00e4nden", "tokens": ["Und", "m\u00e4ht", "mit", "ra\u00b7schen", "zit\u00b7tern\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die kleine Stelle", "tokens": ["Die", "klei\u00b7ne", "Stel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auf der er steht.", "tokens": ["Auf", "der", "er", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und Blumen und Gr\u00e4ser", "tokens": ["Und", "Blu\u00b7men", "und", "Gr\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und giftiges Unkraut", "tokens": ["Und", "gif\u00b7ti\u00b7ges", "Un\u00b7kraut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und stachlicht Gew\u00fcrme", "tokens": ["Und", "stach\u00b7licht", "Ge\u00b7w\u00fcr\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fchlt zitternd die Hand,", "tokens": ["F\u00fchlt", "zit\u00b7ternd", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und halb erschrocken", "tokens": ["Und", "halb", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und halb entschlossen", "tokens": ["Und", "halb", "ent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wirft Gr\u00e4ser und Unkraut,", "tokens": ["Wirft", "Gr\u00e4\u00b7ser", "und", "Un\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Gew\u00fcrme und Blumen", "tokens": ["Ge\u00b7w\u00fcr\u00b7me", "und", "Blu\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Das Kind mit Gewinsel", "tokens": ["Das", "Kind", "mit", "Ge\u00b7win\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "In die Fluthen des lauten abrollenden Stroms.", "tokens": ["In", "die", "Flut\u00b7hen", "des", "lau\u00b7ten", "ab\u00b7rol\u00b7len\u00b7den", "Stroms", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+---+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbwo sind die Freuden?", "tokens": ["\u00bb", "wo", "sind", "die", "Freu\u00b7den", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo sind meine W\u00fcnsche?", "tokens": ["Wo", "sind", "mei\u00b7ne", "W\u00fcn\u00b7sche", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du hast mich betrogen", "tokens": ["Du", "hast", "mich", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und einsam verlassen", "tokens": ["Und", "ein\u00b7sam", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zittr' ich noch einmal", "tokens": ["Zit\u00b7tr'", "ich", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Hand nach den t\u00e4uschenden Blumen zu strecken.\u00ab", "tokens": ["Die", "Hand", "nach", "den", "t\u00e4u\u00b7schen\u00b7den", "Blu\u00b7men", "zu", "stre\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Da flie\u00dft des Mondes goldnes Licht", "tokens": ["Da", "flie\u00dft", "des", "Mon\u00b7des", "gold\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Thal' und Wies' und \u00fcber den Strom", "tokens": ["Durch", "Thal'", "und", "Wies'", "und", "\u00fc\u00b7ber", "den", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und r\u00e4thselhaft steht rings die Gegend", "tokens": ["Und", "r\u00e4th\u00b7sel\u00b7haft", "steht", "rings", "die", "Ge\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Glanz des Abends.", "tokens": ["Im", "Glanz", "des", "A\u00b7bends", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "\u00bbwo find' ich die Heimath?", "tokens": ["\u00bb", "wo", "find'", "ich", "die", "Hei\u00b7math", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo sind die Gef\u00e4hrten?", "tokens": ["Wo", "sind", "die", "Ge\u00b7f\u00e4hr\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich sehe nur Schatten,", "tokens": ["Ich", "se\u00b7he", "nur", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die dunkel und dunkler", "tokens": ["Die", "dun\u00b7kel", "und", "dunk\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vom Strom her\u00fcber,", "tokens": ["Vom", "Strom", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bald hierhin, bald dorthin", "tokens": ["Bald", "hier\u00b7hin", ",", "bald", "dor\u00b7thin"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "PAV", "$,", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie Wolken gehn.", "tokens": ["Wie", "Wol\u00b7ken", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Liegt alles jenseits,", "tokens": ["Liegt", "al\u00b7les", "jen\u00b7seits", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Was ich mir w\u00fcnsche", "tokens": ["Was", "ich", "mir", "w\u00fcn\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und herzlich suche?", "tokens": ["Und", "herz\u00b7lich", "su\u00b7che", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ich h\u00f6re T\u00f6ne,", "tokens": ["Ich", "h\u00f6\u00b7re", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Sind's ferne Wasser,", "tokens": ["Sin\u00b7d's", "fer\u00b7ne", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Sind's t\u00f6nende W\u00e4lder,", "tokens": ["Sin\u00b7d's", "t\u00f6\u00b7nen\u00b7de", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Sind's Menschenstimmen?", "tokens": ["Sin\u00b7d's", "Men\u00b7schen\u00b7stim\u00b7men", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "So fremd und vertraulich,", "tokens": ["So", "fremd", "und", "ver\u00b7trau\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "So ernst und so freundlich,", "tokens": ["So", "ernst", "und", "so", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Klingt's fern her\u00fcber.", "tokens": ["Klingt's", "fern", "her\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Ach wie trotzig braust der Strom sein Lied fort,", "tokens": ["Ach", "wie", "trot\u00b7zig", "braust", "der", "Strom", "sein", "Lied", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Ziehende V\u00f6gel spotten meiner in der Ferne,", "tokens": ["Zie\u00b7hen\u00b7de", "V\u00f6\u00b7gel", "spot\u00b7ten", "mei\u00b7ner", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Wolken sammeln sich um den Mond und nehmen ihn mit sich,", "tokens": ["Wol\u00b7ken", "sam\u00b7meln", "sich", "um", "den", "Mond", "und", "neh\u00b7men", "ihn", "mit", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "PRF", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Ach kein Wesen, das meiner sich erbarmte.\u00ab", "tokens": ["Ach", "kein", "We\u00b7sen", ",", "das", "mei\u00b7ner", "sich", "er\u00b7barm\u00b7te", "."