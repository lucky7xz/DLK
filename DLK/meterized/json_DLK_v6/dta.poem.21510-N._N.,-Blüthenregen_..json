{"dta.poem.21510": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Bl\u00fcthenregen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "en:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Welch frohes Wallen!", "tokens": ["Welch", "fro\u00b7hes", "Wal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Welch bunter Gru\u00df!", "tokens": ["Welch", "bun\u00b7ter", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Bl\u00fcthen fallen", "tokens": ["Die", "Bl\u00fc\u00b7then", "fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vor deinen Fu\u00df.", "tokens": ["Vor", "dei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Doch was dies Blinken,", "tokens": ["Doch", "was", "dies", "Blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hast du\u2019s bedacht?", "tokens": ["Hast", "du's", "be\u00b7dacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein seufzend Sinken", "tokens": ["Ein", "seuf\u00b7zend", "Sin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In Todesnacht."