{"dta.poem.14972": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Vor Rauch's B\u00fcste der K\u00f6nigin Louise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Noch deines Lebens sch\u00f6ne Tr\u00e4ume wieder,", "tokens": ["Noch", "dei\u00b7nes", "Le\u00b7bens", "sch\u00f6\u00b7ne", "Tr\u00e4u\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schlummer nur senkt seine Fl\u00fcgel nieder,", "tokens": ["Der", "Schlum\u00b7mer", "nur", "senkt", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und heil'ger Frieden schlie\u00dft die klaren Augen.", "tokens": ["Und", "heil'\u00b7ger", "Frie\u00b7den", "schlie\u00dft", "die", "kla\u00b7ren", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So schlummre fort, bis Deines Volkes Br\u00fcder,", "tokens": ["So", "schlumm\u00b7re", "fort", ",", "bis", "Dei\u00b7nes", "Vol\u00b7kes", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn Flammenzeichen von den Bergen rauchen,", "tokens": ["Wenn", "Flam\u00b7men\u00b7zei\u00b7chen", "von", "den", "Ber\u00b7gen", "rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Gott vers\u00f6hnt die rost'gen Schwerdter brauchen,", "tokens": ["Mit", "Gott", "ver\u00b7s\u00f6hnt", "die", "rost'\u00b7gen", "Schwerd\u00b7ter", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Leben opfernd f\u00fcr die h\u00f6chsten G\u00fcter.", "tokens": ["Das", "Le\u00b7ben", "op\u00b7fernd", "f\u00fcr", "die", "h\u00f6chs\u00b7ten", "G\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Tief f\u00fchrt der Herr durch Nacht und durch Verderben,", "tokens": ["Tief", "f\u00fchrt", "der", "Herr", "durch", "Nacht", "und", "durch", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So sollen wir im Kampf das Heil erwerben,", "tokens": ["So", "sol\u00b7len", "wir", "im", "Kampf", "das", "Heil", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df unsre Enkel freie M\u00e4nner sterben.", "tokens": ["Da\u00df", "uns\u00b7re", "En\u00b7kel", "frei\u00b7e", "M\u00e4n\u00b7ner", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kommt dann der Tag der Freiheit und der Rache,", "tokens": ["Kommt", "dann", "der", "Tag", "der", "Frei\u00b7heit", "und", "der", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dann ruft ", "tokens": ["Dann", "ruft"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Ein guter Engel f\u00fcr die gute Sache.", "tokens": ["Ein", "gu\u00b7ter", "En\u00b7gel", "f\u00fcr", "die", "gu\u00b7te", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}