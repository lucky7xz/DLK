{"textgrid.poem.61803": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In blauer Nacht bei Vollmondschein", "tokens": ["In", "blau\u00b7er", "Nacht", "bei", "Voll\u00b7mond\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was rauscht und singt so s\u00fc\u00dfe?", "tokens": ["Was", "rauscht", "und", "singt", "so", "s\u00fc\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drei Nixen sitzen am M\u00f6wenstein", "tokens": ["Drei", "Ni\u00b7xen", "sit\u00b7zen", "am", "M\u00f6\u00b7wen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und baden die wei\u00dfen F\u00fc\u00dfe.", "tokens": ["Und", "ba\u00b7den", "die", "wei\u00b7\u00dfen", "F\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es hat der blonde Fischerknab'", "tokens": ["Es", "hat", "der", "blon\u00b7de", "Fi\u00b7scher\u00b7knab'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh\u00f6rt das Singen und Rauschen,", "tokens": ["Ge\u00b7h\u00f6rt", "das", "Sin\u00b7gen", "und", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihm brennt das Herz, er schleicht hinab,", "tokens": ["Ihm", "brennt", "das", "Herz", ",", "er", "schleicht", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Feien zu belauschen.", "tokens": ["Die", "Fei\u00b7en", "zu", "be\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da sausen empor im Mondenlicht", "tokens": ["Da", "sau\u00b7sen", "em\u00b7por", "im", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drei wei\u00dfe wilde Schw\u00e4ne \u2013", "tokens": ["Drei", "wei\u00b7\u00dfe", "wil\u00b7de", "Schw\u00e4\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Wasser spritzt ihm ins Gesicht,", "tokens": ["Das", "Was\u00b7ser", "spritzt", "ihm", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verklungen sind die T\u00f6ne.", "tokens": ["Ver\u00b7klun\u00b7gen", "sind", "die", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}