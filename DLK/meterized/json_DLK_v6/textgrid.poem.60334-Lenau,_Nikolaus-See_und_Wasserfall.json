{"textgrid.poem.60334": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "See und Wasserfall", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Felsen schroff und wild,", "tokens": ["Die", "Fel\u00b7sen", "schroff", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der See, die Waldumnachtung", "tokens": ["Der", "See", ",", "die", "Wal\u00b7dum\u00b7nach\u00b7tung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind dir ein stilles Bild", "tokens": ["Sind", "dir", "ein", "stil\u00b7les", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tiefsinniger Betrachtung.", "tokens": ["Tief\u00b7sin\u00b7ni\u00b7ger", "Be\u00b7trach\u00b7tung", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und dort, mit Donnerhall", "tokens": ["Und", "dort", ",", "mit", "Don\u00b7ner\u00b7hall"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hineilend zwischen Steinen,", "tokens": ["Hi\u00b7nei\u00b7lend", "zwi\u00b7schen", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4\u00dft dir der Wasserfall", "tokens": ["L\u00e4\u00dft", "dir", "der", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die k\u00fchne Tat erscheinen.", "tokens": ["Die", "k\u00fch\u00b7ne", "Tat", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du sollst, gleich jenem Teich,", "tokens": ["Du", "sollst", ",", "gleich", "je\u00b7nem", "Teich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Betrachtend dich verschlie\u00dfen;", "tokens": ["Be\u00b7trach\u00b7tend", "dich", "ver\u00b7schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann k\u00fchn, dem Bache gleich,", "tokens": ["Dann", "k\u00fchn", ",", "dem", "Ba\u00b7che", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur Tat hinunterschie\u00dfen.", "tokens": ["Zur", "Tat", "hin\u00b7un\u00b7ter\u00b7schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}