{"textgrid.poem.55966": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "R\u00f6mische Font\u00e4ne", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Becken, eins das andre \u00fcbersteigend", "tokens": ["Zwei", "Be\u00b7cken", ",", "eins", "das", "and\u00b7re", "\u00fc\u00b7bers\u00b7tei\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "PIS", "ART", "ADJA", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus einem alten runden Marmorrand,", "tokens": ["aus", "ei\u00b7nem", "al\u00b7ten", "run\u00b7den", "Mar\u00b7mor\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und aus dem oberen Wasser leis sich neigend", "tokens": ["und", "aus", "dem", "o\u00b7be\u00b7ren", "Was\u00b7ser", "leis", "sich", "nei\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJD", "PRF", "ADJD"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "zum Wasser, welches unten wartend stand,", "tokens": ["zum", "Was\u00b7ser", ",", "wel\u00b7ches", "un\u00b7ten", "war\u00b7tend", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "dem leise redenden entgegenschweigend", "tokens": ["dem", "lei\u00b7se", "re\u00b7den\u00b7den", "ent\u00b7ge\u00b7gen\u00b7schwei\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVINF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und heimlich, gleichsam in der hohlen Hand,", "tokens": ["und", "heim\u00b7lich", ",", "gleich\u00b7sam", "in", "der", "hoh\u00b7len", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ihm Himmel hinter Gr\u00fcn und Dunkel zeigend", "tokens": ["ihm", "Him\u00b7mel", "hin\u00b7ter", "Gr\u00fcn", "und", "Dun\u00b7kel", "zei\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie einen unbekannten Gegenstand;", "tokens": ["wie", "ei\u00b7nen", "un\u00b7be\u00b7kann\u00b7ten", "Ge\u00b7gen\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "sich selber ruhig in der sch\u00f6nen Schale", "tokens": ["sich", "sel\u00b7ber", "ru\u00b7hig", "in", "der", "sch\u00f6\u00b7nen", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verbreitend ohne Heimweh, Kreis aus Kreis,", "tokens": ["ver\u00b7brei\u00b7tend", "oh\u00b7ne", "Heim\u00b7weh", ",", "Kreis", "aus", "Kreis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nur manchmal tr\u00e4umerisch und tropfenweis", "tokens": ["nur", "manch\u00b7mal", "tr\u00e4u\u00b7me\u00b7risch", "und", "trop\u00b7fen\u00b7weis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "sich niederlassend an den Moosbeh\u00e4ngen", "tokens": ["sich", "nie\u00b7der\u00b7las\u00b7send", "an", "den", "Moos\u00b7be\u00b7h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zum letzten Spiegel, der sein Becken leis", "tokens": ["zum", "letz\u00b7ten", "Spie\u00b7gel", ",", "der", "sein", "Be\u00b7cken", "leis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "von unten l\u00e4cheln macht mit \u00dcberg\u00e4ngen.", "tokens": ["von", "un\u00b7ten", "l\u00e4\u00b7cheln", "macht", "mit", "\u00dc\u00b7ber\u00b7g\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}