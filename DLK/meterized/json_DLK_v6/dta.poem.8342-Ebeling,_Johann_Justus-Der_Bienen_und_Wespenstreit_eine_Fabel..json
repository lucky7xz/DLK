{"dta.poem.8342": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Bienen und Wespenstreit  \n eine Fabel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bienenschwarm und Wespen\nHauffen,", "tokens": ["Ein", "Bie\u00b7nen\u00b7schwarm", "und", "Wes\u00b7pen", "Hauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam einst zum Wettstreit herge-", "tokens": ["Kam", "einst", "zum", "Wett\u00b7streit", "her\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es sprach der Wespen Nation,", "tokens": ["Es", "sprach", "der", "Wes\u00b7pen", "Na\u00b7tion", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dem Bienen-Volke trozzig Hohn.", "tokens": ["Dem", "Bie\u00b7nen\u00b7Vol\u00b7ke", "troz\u00b7zig", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Bienen sprachen sie, ihr tadelt,", "tokens": ["Ihr", "Bie\u00b7nen", "spra\u00b7chen", "sie", ",", "ihr", "ta\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wespen die euch gleich geadelt,", "tokens": ["Die", "Wes\u00b7pen", "die", "euch", "gleich", "ge\u00b7a\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was habt ihr woll zu eurer Ehr", "tokens": ["Was", "habt", "ihr", "woll", "zu", "eu\u00b7rer", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Voraus, f\u00fcr einem Wespen-Heer?", "tokens": ["Vo\u00b7raus", ",", "f\u00fcr", "ei\u00b7nem", "Wes\u00b7pen\u00b7Heer", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Jhr seid bem\u00fcht herum zu fliegen,", "tokens": ["Ihr", "seid", "be\u00b7m\u00fcht", "he\u00b7rum", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und darin finden wir vergn\u00fcgen.", "tokens": ["Und", "da\u00b7rin", "fin\u00b7den", "wir", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Jhr sauget eure Nahrung ein,", "tokens": ["Ihr", "sau\u00b7get", "eu\u00b7re", "Nah\u00b7rung", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Aus wundersch\u00f6nen Bl\u00fcmelein:", "tokens": ["Aus", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Bl\u00fc\u00b7me\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wir durchl\u00f6chern sch\u00f6ne Fr\u00fcchte,", "tokens": ["Und", "wir", "durch\u00b7l\u00f6\u00b7chern", "sch\u00f6\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Geniessen manches s\u00fc\u00df Gerichte.", "tokens": ["Ge\u00b7nies\u00b7sen", "man\u00b7ches", "s\u00fc\u00df", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Jhr habet Stacheln, die gewezt,", "tokens": ["Ihr", "ha\u00b7bet", "Sta\u00b7cheln", ",", "die", "ge\u00b7wezt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Womit ihr manche Haut verlezt:", "tokens": ["Wo\u00b7mit", "ihr", "man\u00b7che", "Haut", "ver\u00b7lezt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wir sind damit auch ausger\u00fcstet,", "tokens": ["Wir", "sind", "da\u00b7mit", "auch", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dies f\u00fchlen die, den es gel\u00fcstet,", "tokens": ["Dies", "f\u00fch\u00b7len", "die", ",", "den", "es", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Uns unsre Nester zu zerst\u00f6hrn,", "tokens": ["Uns", "uns\u00b7re", "Nes\u00b7ter", "zu", "zer\u00b7st\u00f6hrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Uns zu erstikken, zu verheern.", "tokens": ["Uns", "zu", "ers\u00b7tik\u00b7ken", ",", "zu", "ver\u00b7heern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sagt an, was k\u00f6nnt ihr mehr aufweisen", "tokens": ["Sagt", "an", ",", "was", "k\u00f6nnt", "ihr", "mehr", "auf\u00b7wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Warum ihr seid vielmehr zu preisen?", "tokens": ["Wa\u00b7rum", "ihr", "seid", "viel\u00b7mehr", "zu", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die Bienen sprachen: O! wie weit,", "tokens": ["Die", "Bie\u00b7nen", "spra\u00b7chen", ":", "O", "!", "wie", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$.", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ist zwischen uns der Unterscheid,", "tokens": ["Ist", "zwi\u00b7schen", "uns", "der", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Jhr m\u00fcsset uns drum billig weichen,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "uns", "drum", "bil\u00b7lig", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PAV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ob wir in \u00e4usern schon zu gleichen.", "tokens": ["Ob", "wir", "in", "\u00e4u\u00b7sern", "schon", "zu", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Es schadet zwar der Stachel Spiz", "tokens": ["Es", "scha\u00b7det", "zwar", "der", "Sta\u00b7chel", "Spiz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Den Menschen, doch wir sind auch n\u00fcz:", "tokens": ["Den", "Men\u00b7schen", ",", "doch", "wir", "sind", "auch", "n\u00fcz", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wir schenken ihnen S\u00fc\u00dfigkeiten,", "tokens": ["Wir", "schen\u00b7ken", "ih\u00b7nen", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Da wir den Honig zubereiten;", "tokens": ["Da", "wir", "den", "Ho\u00b7nig", "zu\u00b7be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und was der Stachel Gifft verlezt,", "tokens": ["Und", "was", "der", "Sta\u00b7chel", "Gifft", "ver\u00b7lezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wird durch dem Honig-Bau ersezt:", "tokens": ["Wird", "durch", "dem", "Ho\u00b7nig\u00b7Bau", "er\u00b7sezt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wir schw\u00e4rmen in den holden Gr\u00fcnen,", "tokens": ["Wir", "schw\u00e4r\u00b7men", "in", "den", "hol\u00b7den", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Nicht uns, wie ihr, allein zu dienen.", "tokens": ["Nicht", "uns", ",", "wie", "ihr", ",", "al\u00b7lein", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "PWAV", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Die H\u00e4user, die ihr auferbaut,", "tokens": ["Die", "H\u00e4u\u00b7ser", ",", "die", "ihr", "au\u00b7fer\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sind k\u00fcnstlich, wenn man sie anschaut:", "tokens": ["Sind", "k\u00fcnst\u00b7lich", ",", "wenn", "man", "sie", "an\u00b7schaut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Doch lange nicht mit unsern Zellen", "tokens": ["Doch", "lan\u00b7ge", "nicht", "mit", "un\u00b7sern", "Zel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Nach dem Verh\u00e4ltnis gleich zu stellen:", "tokens": ["Nach", "dem", "Ver\u00b7h\u00e4lt\u00b7nis", "gleich", "zu", "stel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Das Wachs woraus wir sie formirt,", "tokens": ["Das", "Wachs", "wo\u00b7raus", "wir", "sie", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Gebildet, wunderbar geziert,", "tokens": ["Ge\u00b7bil\u00b7det", ",", "wun\u00b7der\u00b7bar", "ge\u00b7ziert", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ist brauchbar zu so vielen Sachen,", "tokens": ["Ist", "brauch\u00b7bar", "zu", "so", "vie\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Die Menschen aus denselben machen.", "tokens": ["Die", "Men\u00b7schen", "aus", "den\u00b7sel\u00b7ben", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Darum erlanget unser Fleis,", "tokens": ["Da\u00b7rum", "er\u00b7lan\u00b7get", "un\u00b7ser", "Fleis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "F\u00fcr euch den wollverdienten Preis.", "tokens": ["F\u00fcr", "euch", "den", "woll\u00b7ver\u00b7dien\u00b7ten", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}