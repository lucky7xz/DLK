{"textgrid.poem.31581": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jesu / meines Lebens Ziel / du mein ganzes Alls in Allen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesu / meines Lebens Ziel / du mein ganzes Alls in Allen /", "tokens": ["Je\u00b7su", "/", "mei\u00b7nes", "Le\u00b7bens", "Ziel", "/", "du", "mein", "gan\u00b7zes", "Alls", "in", "Al\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "NN", "$(", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "NE", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "du mein Leben in dem Sterben / s\u00fcsser Fried in Krieg und Streit /", "tokens": ["du", "mein", "Le\u00b7ben", "in", "dem", "Ster\u00b7ben", "/", "s\u00fcs\u00b7ser", "Fried", "in", "Krieg", "und", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$(", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "in der Welt mein halber Himmel / Lust in Widerw\u00e4rtigkeit /", "tokens": ["in", "der", "Welt", "mein", "hal\u00b7ber", "Him\u00b7mel", "/", "Lust", "in", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$(", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "in der Erden-D\u00fcsterheit mein Begeistertes bestrahlen!", "tokens": ["in", "der", "Er\u00b7den\u00b7D\u00fcs\u00b7ter\u00b7heit", "mein", "Be\u00b7geis\u00b7ter\u00b7tes", "be\u00b7strah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "nichtes / (bistu meine H\u00fclff / ) kan unm\u00fcglichschwer mir fallen.", "tokens": ["nich\u00b7tes", "/", "(", "bis\u00b7tu", "mei\u00b7ne", "H\u00fclff", "/", ")", "kan", "un\u00b7m\u00fcg\u00b7lich\u00b7schwer", "mir", "fal\u00b7len", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "$(", "APPR", "PPOSAT", "NN", "$(", "$(", "VMFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "wann der Drach all' Vngl\u00fccks Kugeln / Hauffenwei\u00df auf mich ausspeit /", "tokens": ["wann", "der", "Drach", "all'", "Vn\u00b7gl\u00fccks", "Ku\u00b7geln", "/", "Hauf\u00b7fen\u00b7wei\u00df", "auf", "mich", "aus\u00b7speit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN", "NN", "$(", "NN", "APPR", "PPER", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "bin ich in der Jesus-Vestung der Verletzlichkeit befreyt.", "tokens": ["bin", "ich", "in", "der", "Je\u00b7sus\u00b7Ves\u00b7tung", "der", "Ver\u00b7letz\u00b7lich\u00b7keit", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Alles Vbel gibt die Flucht / macht man diesen Nam erschallen.", "tokens": ["Al\u00b7les", "Vbel", "gibt", "die", "Flucht", "/", "macht", "man", "die\u00b7sen", "Nam", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$(", "VVFIN", "PIS", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Er sey meines Hauptes Cron / meines Herzens inners Herze /", "tokens": ["Er", "sey", "mei\u00b7nes", "Haup\u00b7tes", "Cron", "/", "mei\u00b7nes", "Her\u00b7zens", "in\u00b7ners", "Her\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NE", "$(", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "meiner Zungen ganzes Tichten / meines Sinnes Gegenstand /", "tokens": ["mei\u00b7ner", "Zun\u00b7gen", "gan\u00b7zes", "Tich\u00b7ten", "/", "mei\u00b7nes", "Sin\u00b7nes", "Ge\u00b7gen\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "meines Lebens Vnterhald / meines Schiffleins Sternen Kerze /", "tokens": ["mei\u00b7nes", "Le\u00b7bens", "Vn\u00b7ter\u00b7hald", "/", "mei\u00b7nes", "Schif\u00b7fleins", "Ster\u00b7nen", "Ker\u00b7ze", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$(", "PPOSAT", "NN", "NN", "NN", "$("], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.12": {"text": "meine Allheit / wann ich nichtes! dieses ist mein Haubt Gewinn", "tokens": ["mei\u00b7ne", "All\u00b7heit", "/", "wann", "ich", "nich\u00b7tes", "!", "die\u00b7ses", "ist", "mein", "Haubt", "Ge\u00b7winn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PWAV", "PPER", "PIS", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "da\u00df ich in mir selbst aufh\u00f6re / und dann in dir alles bin.", "tokens": ["da\u00df", "ich", "in", "mir", "selbst", "auf\u00b7h\u00f6\u00b7re", "/", "und", "dann", "in", "dir", "al\u00b7les", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$(", "KON", "ADV", "APPR", "PPER", "PIS", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}