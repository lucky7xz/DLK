{"dta.poem.20998": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxII.  \n Eines Kupffernen/ so im Kupfferwe-  \n sen gestorben.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Es war zu meiner Zeit das Kupffer hochgeacht/", "tokens": ["Es", "war", "zu", "mei\u00b7ner", "Zeit", "das", "Kupf\u00b7fer", "hoch\u00b7ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mancher Ofentopff zu Pfennigen gemacht.", "tokens": ["Und", "man\u00b7cher", "O\u00b7fen\u00b7topff", "zu", "Pfen\u00b7ni\u00b7gen", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Furcht ich m\u00f6chte noch umb meine Nase kom-", "tokens": ["Aus", "Furcht", "ich", "m\u00f6ch\u00b7te", "noch", "umb", "mei\u00b7ne", "Na\u00b7se", "kom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "men/", "tokens": ["men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "So hab ich meinen Sitz hier unten eingenommen.", "tokens": ["So", "hab", "ich", "mei\u00b7nen", "Sitz", "hier", "un\u00b7ten", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}