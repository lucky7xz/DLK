{"textgrid.poem.62432": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Marsch nach Franken", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Franken hin, nach Franken", "tokens": ["Nach", "Fran\u00b7ken", "hin", ",", "nach", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum k\u00fchlen Werrastrand;", "tokens": ["Zum", "k\u00fch\u00b7len", "Wer\u00b7rast\u00b7rand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort weilen die Gedanken", "tokens": ["Dort", "wei\u00b7len", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An alter H\u00fcgel Rand.", "tokens": ["An", "al\u00b7ter", "H\u00fc\u00b7gel", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo deutsche eben bl\u00fchen", "tokens": ["Wo", "deut\u00b7sche", "e\u00b7ben", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und deutscher M\u00e4dchen Mund,", "tokens": ["Und", "deut\u00b7scher", "M\u00e4d\u00b7chen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo deutsche Herzen gl\u00fchen", "tokens": ["Wo", "deut\u00b7sche", "Her\u00b7zen", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr ernsten Todesbund.", "tokens": ["F\u00fcr", "erns\u00b7ten", "To\u00b7des\u00b7bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da wollen wir es pflanzen", "tokens": ["Da", "wol\u00b7len", "wir", "es", "pflan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Freiheit edles Reis,", "tokens": ["Der", "Frei\u00b7heit", "ed\u00b7les", "Reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wald von jungen Lanzen", "tokens": ["Ein", "Wald", "von", "jun\u00b7gen", "Lan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umbl\u00fcht es gr\u00fcn und wei\u00df.", "tokens": ["Um\u00b7bl\u00fcht", "es", "gr\u00fcn", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verwebet euch, ihr Aeste", "tokens": ["Ver\u00b7we\u00b7bet", "euch", ",", "ihr", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Th\u00fcrings dunkelm Wald,", "tokens": ["In", "Th\u00fc\u00b7rings", "dun\u00b7kelm", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr gebet Schmuck zum Feste,", "tokens": ["Ihr", "ge\u00b7bet", "Schmuck", "zum", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir kommen, kommen bald.", "tokens": ["Wir", "kom\u00b7men", ",", "kom\u00b7men", "bald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nach Franken hin, nach Franken", "tokens": ["Nach", "Fran\u00b7ken", "hin", ",", "nach", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du morgendliches Heer,", "tokens": ["Du", "mor\u00b7gend\u00b7li\u00b7ches", "Heer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es mu\u00df die Welt dir danken,", "tokens": ["Es", "mu\u00df", "die", "Welt", "dir", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du f\u00fchrest Gottes Wehr!", "tokens": ["Du", "f\u00fch\u00b7rest", "Got\u00b7tes", "Wehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}