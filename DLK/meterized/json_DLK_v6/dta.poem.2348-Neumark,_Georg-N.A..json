{"dta.poem.2348": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wessen ist der s\u00fcsse Tohn?", "tokens": ["Wes\u00b7sen", "ist", "der", "s\u00fcs\u00b7se", "Tohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wessen sind die sch\u00f6ne Lieder?", "tokens": ["Wes\u00b7sen", "sind", "die", "sch\u00f6\u00b7ne", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebet unser Opitz wieder?", "tokens": ["Le\u00b7bet", "un\u00b7ser", "O\u00b7pitz", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es nicht Euagers Sohn?", "tokens": ["Ist", "es", "nicht", "Eu\u00b7a\u00b7gers", "Sohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein. Es ist die Zier der Zeit/", "tokens": ["Nein", ".", "Es", "ist", "die", "Zier", "der", "Zeit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser Neumark/ der durch Tichten", "tokens": ["Un\u00b7ser", "Neu\u00b7mark", "/", "der", "durch", "Tich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ART", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jhm wei\u00df k\u00fcnstlich zuzurichten", "tokens": ["Jhm", "wei\u00df", "k\u00fcnst\u00b7lich", "zu\u00b7zu\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Krantz der Ewigkeit.", "tokens": ["Ei\u00b7nen", "Krantz", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Du der Tichter Prei\u00df und Licht/", "tokens": ["Du", "der", "Tich\u00b7ter", "Prei\u00df", "und", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Billich wirst du hoch gesch\u00e4tzet:", "tokens": ["Bil\u00b7lich", "wirst", "du", "hoch", "ge\u00b7sch\u00e4t\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dein gelehrtes Spiel erg\u00f6tzet.", "tokens": ["Dein", "ge\u00b7lehr\u00b7tes", "Spiel", "er\u00b7g\u00f6t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo die Seeten/ wie man spricht/", "tokens": ["Wo", "die", "See\u00b7ten", "/", "wie", "man", "spricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$(", "PWAV", "PIS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Fahren au\u00df und wieder ein", "tokens": ["Fah\u00b7ren", "au\u00df", "und", "wie\u00b7der", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADV", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wird in deine Leibesh\u00f6le", "tokens": ["Wird", "in", "dei\u00b7ne", "Lei\u00b7bes\u00b7h\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Orpheus und des Opitz Seele/", "tokens": ["Or\u00b7pheus", "und", "des", "O\u00b7pitz", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Warlich eingezogen seyn.", "tokens": ["War\u00b7lich", "ein\u00b7ge\u00b7zo\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}