{"dta.poem.3309": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die fr\u00f6hliche Dichtkunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Oschattigter Parna\u00df! ihr heiligen Gestr\u00e4uche,", "tokens": ["O\u00b7schat\u00b7tig\u00b7ter", "Par\u00b7na\u00df", "!", "ihr", "hei\u00b7li\u00b7gen", "Ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo oft um Mitternacht ich einsam wachend schlei-", "tokens": ["Wo", "oft", "um", "Mit\u00b7ter\u00b7nacht", "ich", "ein\u00b7sam", "wa\u00b7chend", "schlei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "PPER", "ADJD", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "che!", "tokens": ["che", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Nie hab ich klagend euch entweiht.", "tokens": ["Nie", "hab", "ich", "kla\u00b7gend", "euch", "ent\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Scherz mit heitrem Angesichte,", "tokens": ["Nur", "Scherz", "mit", "heit\u00b7rem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Wein und freye Z\u00e4rtlichkeit", "tokens": ["Nur", "Wein", "und", "frey\u00b7e", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Begeistern mich, gef\u00e4llig, wenn ich dichte.", "tokens": ["Be\u00b7geis\u00b7tern", "mich", ",", "ge\u00b7f\u00e4l\u00b7lig", ",", "wenn", "ich", "dich\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wann mich ein Kummer dr\u00fcckt, so mag die Mu-", "tokens": ["Wann", "mich", "ein", "Kum\u00b7mer", "dr\u00fcckt", ",", "so", "mag", "die", "Mu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "se schweigen,", "tokens": ["se", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Den Nachtigallen gleich, die auf begr\u00fcnten Zweigen", "tokens": ["Den", "Nach\u00b7ti\u00b7gal\u00b7len", "gleich", ",", "die", "auf", "be\u00b7gr\u00fcn\u00b7ten", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur singen, wenn sie sich erfreun.", "tokens": ["Nur", "sin\u00b7gen", ",", "wenn", "sie", "sich", "er\u00b7freun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch \u00e4chter Priester froher Musen", "tokens": ["Welch", "\u00e4ch\u00b7ter", "Pries\u00b7ter", "fro\u00b7her", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vermischt mit Thr\u00e4nen seinen Wein,", "tokens": ["Ver\u00b7mischt", "mit", "Thr\u00e4\u00b7nen", "sei\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00e4chzet stets, auch an der Daphne Busen?", "tokens": ["Und", "\u00e4ch\u00b7zet", "stets", ",", "auch", "an", "der", "Daph\u00b7ne", "Bu\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einst lag ich sorgenvoll im Schatten finstrer Buchen,", "tokens": ["Einst", "lag", "ich", "sor\u00b7gen\u00b7voll", "im", "Schat\u00b7ten", "finst\u00b7rer", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sich ein tr\u00e4ger Bach, den Faunen blo\u00df besuchen,", "tokens": ["Wo", "sich", "ein", "tr\u00e4\u00b7ger", "Bach", ",", "den", "Fau\u00b7nen", "blo\u00df", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch einsames Gefilde wand.", "tokens": ["Durch", "ein\u00b7sa\u00b7mes", "Ge\u00b7fil\u00b7de", "wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mein Saitenspiel verga\u00df der Sch\u00f6nen,", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "ver\u00b7ga\u00df", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine scherzgewohnte Hand", "tokens": ["Und", "mei\u00b7ne", "scherz\u00b7ge\u00b7wohn\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verirrte sich zu trauervollen T\u00f6nen.", "tokens": ["Ver\u00b7irr\u00b7te", "sich", "zu", "trau\u00b7er\u00b7vol\u00b7len", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bereits entschlo\u00df mein Mund sich unvergn\u00fcgter Kla-", "tokens": ["Be\u00b7reits", "ent\u00b7schlo\u00df", "mein", "Mund", "sich", "un\u00b7ver\u00b7gn\u00fcg\u00b7ter", "Kla"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ge,", "tokens": ["ge", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Als mit entw\u00f6lkter Stirn, gleich einem Fr\u00fchlingstage,", "tokens": ["Als", "mit", "ent\u00b7w\u00f6lk\u00b7ter", "Stirn", ",", "gleich", "ei\u00b7nem", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die holde Muse mir erschien.", "tokens": ["Die", "hol\u00b7de", "Mu\u00b7se", "mir", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Lippen Anmuth war den Rosen,", "tokens": ["Der", "Lip\u00b7pen", "An\u00b7muth", "war", "den", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Morgen Rosen vorzuziehn,", "tokens": ["Den", "Mor\u00b7gen", "Ro\u00b7sen", "vor\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ieder Blick schien l\u00e4chelnd liebzukosen.", "tokens": ["Und", "ie\u00b7der", "Blick", "schien", "l\u00e4\u00b7chelnd", "lieb\u00b7zu\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mein Geist erwachte schnell aus allen tr\u00fcben Sorgen:", "tokens": ["Mein", "Geist", "er\u00b7wach\u00b7te", "schnell", "aus", "al\u00b7len", "tr\u00fc\u00b7ben", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie, wann im rothen Ost der angenehme Morgen", "tokens": ["Wie", ",", "wann", "im", "ro\u00b7then", "Ost", "der", "an\u00b7ge\u00b7neh\u00b7me", "Mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt in Aurorens Arm erwacht;", "tokens": ["Jtzt", "in", "Au\u00b7ro\u00b7rens", "Arm", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Alsdann die bangen Tr\u00e4ume fliehen", "tokens": ["Als\u00b7dann", "die", "ban\u00b7gen", "Tr\u00e4u\u00b7me", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schwarzgefl\u00fcgelt, wie die Nacht,", "tokens": ["Und", "schwarz\u00b7ge\u00b7fl\u00fc\u00b7gelt", ",", "wie", "die", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihr zugleich in ihre Grotte ziehen.", "tokens": ["Mit", "ihr", "zu\u00b7gleich", "in", "ih\u00b7re", "Grot\u00b7te", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Soll Unmuth, schalt sie mich, dein Saitenspiel ver-", "tokens": ["Soll", "Un\u00b7muth", ",", "schalt", "sie", "mich", ",", "dein", "Sai\u00b7ten\u00b7spiel", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "stimmen?", "tokens": ["stim\u00b7men", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sieh auf! Anakreon, den Wein und Alter kr\u00fcmmen,", "tokens": ["Sieh", "auf", "!", "A\u00b7nak\u00b7re\u00b7on", ",", "den", "Wein", "und", "Al\u00b7ter", "kr\u00fcm\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "NE", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Scheucht singend eitler Sorgen Heer!", "tokens": ["Scheucht", "sin\u00b7gend", "eit\u00b7ler", "Sor\u00b7gen", "Heer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weicht auch die Freude von Alk\u00e4en?", "tokens": ["Weicht", "auch", "die", "Freu\u00b7de", "von", "Al\u00b7k\u00e4\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schwimmt ihm nach durchs rauhe Meer,", "tokens": ["Sie", "schwimmt", "ihm", "nach", "durchs", "rau\u00b7he", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und singt mit ihm von Amorn und Ly\u00e4en.", "tokens": ["Und", "singt", "mit", "ihm", "von", "A\u00b7morn", "und", "Ly\u00b7\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}}, "stanza.7": {"line.1": {"text": "Horaz trinkt Chier-Wein und jauchzt bey seinem", "tokens": ["Ho\u00b7raz", "trinkt", "Chier\u00b7Wein", "und", "jauchzt", "bey", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "KON", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weine:", "tokens": ["Wei\u00b7ne", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sein ewiger Gesang ert\u00f6nt in Tiburs Hayne", "tokens": ["Sein", "e\u00b7wi\u00b7ger", "Ge\u00b7sang", "er\u00b7t\u00f6nt", "in", "Ti\u00b7burs", "Hay\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur an der weisen Wollust Brust.", "tokens": ["Nur", "an", "der", "wei\u00b7sen", "Wol\u00b7lust", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wollust weihe deine Leyer!", "tokens": ["Der", "Wol\u00b7lust", "wei\u00b7he", "dei\u00b7ne", "Le\u00b7yer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Blo\u00df diese Mutter wahrer Lust", "tokens": ["Blo\u00df", "die\u00b7se", "Mut\u00b7ter", "wah\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beseelt ein Lied mit \u00e4chtem Reiz und Feuer.", "tokens": ["Be\u00b7seelt", "ein", "Lied", "mit", "\u00e4ch\u00b7tem", "Reiz", "und", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die wache Sorge mag an schlechten Seelen nagen!", "tokens": ["Die", "wa\u00b7che", "Sor\u00b7ge", "mag", "an", "schlech\u00b7ten", "See\u00b7len", "na\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Thoren fehlt es nie an selbstgemachten Plagen:", "tokens": ["Dem", "Tho\u00b7ren", "fehlt", "es", "nie", "an", "selbst\u00b7ge\u00b7mach\u00b7ten", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhn qu\u00e4lt ein Tand, ein dunkler Traum.", "tokens": ["Jhn", "qu\u00e4lt", "ein", "Tand", ",", "ein", "dunk\u00b7ler", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Weise kann das Gl\u00fcck betr\u00fcgen:", "tokens": ["Der", "Wei\u00b7se", "kann", "das", "Gl\u00fcck", "be\u00b7tr\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch wahres Uebel f\u00fchlt er kaum;", "tokens": ["Auch", "wah\u00b7res", "Ue