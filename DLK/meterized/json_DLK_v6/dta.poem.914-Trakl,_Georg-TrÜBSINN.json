{"dta.poem.914": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00dcBSINN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Weltungl\u00fcck geistert durch den Nachmittag.", "tokens": ["Wel\u00b7tun\u00b7gl\u00fcck", "geis\u00b7tert", "durch", "den", "Nach\u00b7mit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Baraken fliehn durch G\u00e4rtchen braun und w\u00fcst.", "tokens": ["Ba\u00b7ra\u00b7ken", "fliehn", "durch", "G\u00e4rt\u00b7chen", "braun", "und", "w\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lichtschnuppen gaukeln um verbrannten Mist,", "tokens": ["Licht\u00b7schnup\u00b7pen", "gau\u00b7keln", "um", "ver\u00b7brann\u00b7ten", "Mist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zwei Schl\u00e4fer schwanken heimw\u00e4rts, grau und vag.", "tokens": ["Zwei", "Schl\u00e4\u00b7fer", "schwan\u00b7ken", "heim\u00b7w\u00e4rts", ",", "grau", "und", "vag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf der verdorrten Wiese l\u00e4uft ein Kind", "tokens": ["Auf", "der", "ver\u00b7dorr\u00b7ten", "Wie\u00b7se", "l\u00e4uft", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und spielt mit seinen Augen schwarz und glatt.", "tokens": ["Und", "spielt", "mit", "sei\u00b7nen", "Au\u00b7gen", "schwarz", "und", "glatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Gold tropft von den B\u00fcschen tr\u00fcb und matt.", "tokens": ["Das", "Gold", "tropft", "von", "den", "B\u00fc\u00b7schen", "tr\u00fcb", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein alter Mann dreht traurig sich im Wind.", "tokens": ["Ein", "al\u00b7ter", "Mann", "dreht", "trau\u00b7rig", "sich", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Abend wieder \u00fcber meinem Haupt", "tokens": ["Am", "A\u00b7bend", "wie\u00b7der", "\u00fc\u00b7ber", "mei\u00b7nem", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Saturn lenkt stumm ein elendes Geschick.", "tokens": ["Sa\u00b7turn", "lenkt", "stumm", "ein", "e\u00b7len\u00b7des", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein Baum, ein Hund tritt hinter sich zur\u00fcck", "tokens": ["Ein", "Baum", ",", "ein", "Hund", "tritt", "hin\u00b7ter", "sich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schwarz schwankt Gottes Himmel und entlaubt.", "tokens": ["Und", "schwarz", "schwankt", "Got\u00b7tes", "Him\u00b7mel", "und", "ent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NN", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Fischlein gleitet schnell hinab den Bach;", "tokens": ["Ein", "Fisc\u00b7hlein", "glei\u00b7tet", "schnell", "hin\u00b7ab", "den", "Bach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leise r\u00fchrt des toten Freundes Hand", "tokens": ["Und", "lei\u00b7se", "r\u00fchrt", "des", "to\u00b7ten", "Freun\u00b7des", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gl\u00e4ttet liebend Stirne und Gewand.", "tokens": ["Und", "gl\u00e4t\u00b7tet", "lie\u00b7bend", "Stir\u00b7ne", "und", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Licht ruft Schatten in den Zimmern wach.", "tokens": ["Ein", "Licht", "ruft", "Schat\u00b7ten", "in", "den", "Zim\u00b7mern", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}