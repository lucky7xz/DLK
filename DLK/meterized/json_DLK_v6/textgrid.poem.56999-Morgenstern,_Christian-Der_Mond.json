{"textgrid.poem.56999": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Mond", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Gott den lieben Mond erschuf,", "tokens": ["Als", "Gott", "den", "lie\u00b7ben", "Mond", "er\u00b7schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gab er ihm folgenden Beruf:", "tokens": ["gab", "er", "ihm", "fol\u00b7gen\u00b7den", "Be\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Beim Zu- sowohl wie beim Abnehmen", "tokens": ["Beim", "Zu", "so\u00b7wohl", "wie", "beim", "Ab\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sich deutschen Lesern zu bequemen,", "tokens": ["sich", "deut\u00b7schen", "Le\u00b7sern", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "ein [A] formierend und ein [Z] \u2013", "tokens": ["ein", "A", "for\u00b7mie\u00b7rend", "und", "ein", "Z", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "$(", "NE", "$(", "VVPP", "KON", "ART", "$(", "NN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df keiner gro\u00df zu denken h\u00e4tt.", "tokens": ["da\u00df", "kei\u00b7ner", "gro\u00df", "zu", "den\u00b7ken", "h\u00e4tt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Befolgend dies, ward der Trabant", "tokens": ["Be\u00b7fol\u00b7gend", "dies", ",", "ward", "der", "Tra\u00b7bant"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "PDS", "$,", "VAFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "ein v\u00f6llig deutscher Gegenstand.", "tokens": ["ein", "v\u00f6l\u00b7lig", "deut\u00b7scher", "Ge\u00b7gen\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}