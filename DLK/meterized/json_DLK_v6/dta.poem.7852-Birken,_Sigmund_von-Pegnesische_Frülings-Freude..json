{"dta.poem.7852": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Solang ein Sch\u00e4ferspiel in W\u00e4ldern wird", "tokens": ["So\u00b7lang", "ein", "Sch\u00e4\u00b7fer\u00b7spiel", "in", "W\u00e4l\u00b7dern", "wird"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "solang ein Halmen Schilf am gr\u00fcnen Ufer", "tokens": ["so\u00b7lang", "ein", "Hal\u00b7men", "Schilf", "am", "gr\u00fc\u00b7nen", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "solange soll von Euch st\u00e4ts unser Singen sin-", "tokens": ["so\u00b7lan\u00b7ge", "soll", "von", "Euch", "st\u00e4ts", "un\u00b7ser", "Sin\u00b7gen", "sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Euch sich die Fl\u00f6te schenkt", "tokens": ["Euch", "sich", "die", "Fl\u00f6\u00b7te", "schenkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}