{"textgrid.poem.26486": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit ahnungsvollem Lauschen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit ahnungsvollem Lauschen,", "tokens": ["Mit", "ah\u00b7nungs\u00b7vol\u00b7lem", "Lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das gern an Zeichen glaubt,", "tokens": ["Das", "gern", "an", "Zei\u00b7chen", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6r' ich jetzt leise rauschen,", "tokens": ["H\u00f6r'", "ich", "jetzt", "lei\u00b7se", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Gl\u00fcck, dich um mein Haupt.", "tokens": ["O", "Gl\u00fcck", ",", "dich", "um", "mein", "Haupt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als Lohn f\u00fcr muth'ges Hoffen,", "tokens": ["Als", "Lohn", "f\u00fcr", "muth'\u00b7ges", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommst sp\u00e4t du \u2013 nicht zu sp\u00e4t;", "tokens": ["Kommst", "sp\u00e4t", "du", "\u2013", "nicht", "zu", "sp\u00e4t", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "$(", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch ist die Brust dir offen,", "tokens": ["Noch", "ist", "die", "Brust", "dir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die lang umsonst gefleht.", "tokens": ["Die", "lang", "um\u00b7sonst", "ge\u00b7fleht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gezwungen, zu entsagen,", "tokens": ["Ge\u00b7zwun\u00b7gen", ",", "zu", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich gedarbt, entbehrt,", "tokens": ["Hab'", "ich", "ge\u00b7darbt", ",", "ent\u00b7behrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hab' in kr\u00e4ft'gen Tagen", "tokens": ["Und", "hab'", "in", "kr\u00e4ft'\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom eig'nen Mark gezehrt.", "tokens": ["Vom", "eig'\u00b7nen", "Mark", "ge\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch Manches ist geblieben,", "tokens": ["Doch", "Man\u00b7ches", "ist", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Manches sp\u00e4t gereift:", "tokens": ["Und", "Man\u00b7ches", "sp\u00e4t", "ge\u00b7reift", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Herz noch, um zu lieben,", "tokens": ["Ein", "Herz", "noch", ",", "um", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Geist, der dich begreift.", "tokens": ["Ein", "Geist", ",", "der", "dich", "be\u00b7greift", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So lohne jetzt mein Hoffen,", "tokens": ["So", "loh\u00b7ne", "jetzt", "mein", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn sp\u00e4t auch \u2013 nicht zu sp\u00e4t:", "tokens": ["Wenn", "sp\u00e4t", "auch", "\u2013", "nicht", "zu", "sp\u00e4t", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "$(", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch ist die Brust dir offen,", "tokens": ["Noch", "ist", "die", "Brust", "dir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die lang umsonst gefleht!", "tokens": ["Die", "lang", "um\u00b7sonst", "ge\u00b7fleht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}