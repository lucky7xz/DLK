{"textgrid.poem.44642": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Justitia Distributiva", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie der Erstgeborne unter Br\u00fcdern,", "tokens": ["So", "wie", "der", "Erst\u00b7ge\u00b7bor\u00b7ne", "un\u00b7ter", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ward der Kopf vor allen Gliedern", "tokens": ["So", "ward", "der", "Kopf", "vor", "al\u00b7len", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dunkler Feudalzeit einst gesch\u00e4tzt.", "tokens": ["In", "dunk\u00b7ler", "Feu\u00b7dal\u00b7zeit", "einst", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch unsern Zeiten war es vorbehalten,", "tokens": ["Doch", "un\u00b7sern", "Zei\u00b7ten", "war", "es", "vor\u00b7be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Recht mit gleichen H\u00e4nden zu verwalten,", "tokens": ["Das", "Recht", "mit", "glei\u00b7chen", "H\u00e4n\u00b7den", "zu", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Kopf ist seines Ansehns nun entsetzt.", "tokens": ["Der", "Kopf", "ist", "sei\u00b7nes", "An\u00b7sehns", "nun", "ent\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein Vorrang gleich so manchem mu\u00dfte wandern,", "tokens": ["Sein", "Vor\u00b7rang", "gleich", "so", "man\u00b7chem", "mu\u00df\u00b7te", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "PIS", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er usurpiert nicht mehr des Schicksals Topf,", "tokens": ["Er", "u\u00b7sur\u00b7piert", "nicht", "mehr", "des", "Schick\u00b7sals", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und hilft auch einem hie und da sein Kopf,", "tokens": ["Und", "hilft", "auch", "ei\u00b7nem", "hie", "und", "da", "sein", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADV", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So hilft daf\u00fcr ihr Hintern hundert andern.", "tokens": ["So", "hilft", "da\u00b7f\u00fcr", "ihr", "Hin\u00b7tern", "hun\u00b7dert", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PPOSAT", "NN", "CARD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}