{"textgrid.poem.28530": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heuchler und Hunde belecken die Teller;", "tokens": ["Heuch\u00b7ler", "und", "Hun\u00b7de", "be\u00b7le\u00b7cken", "die", "Tel\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Jene sind Schmeichler, und diese sind Beller;", "tokens": ["Je\u00b7ne", "sind", "Schmeich\u00b7ler", ",", "und", "die\u00b7se", "sind", "Bel\u00b7ler", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "KON", "PDS", "VAFIN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Diese bewahren, bey denen sie zehren;", "tokens": ["Die\u00b7se", "be\u00b7wah\u00b7ren", ",", "bey", "de\u00b7nen", "sie", "zeh\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Jene verzehren die, welche sie n\u00e4hren.", "tokens": ["Je\u00b7ne", "ver\u00b7zeh\u00b7ren", "die", ",", "wel\u00b7che", "sie", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}