{"dta.poem.8912": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Der dich durch Freude soll und Schmerz geleiten,", "tokens": ["Der", "dich", "durch", "Freu\u00b7de", "soll", "und", "Schmerz", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VMFIN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00e4hle mich, du findest keinen zweyten,", "tokens": ["So", "w\u00e4h\u00b7le", "mich", ",", "du", "fin\u00b7dest", "kei\u00b7nen", "zwey\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "PPER", "VVFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und keinen f\u00e4higern, sich hinzugeben.", "tokens": ["Und", "kei\u00b7nen", "f\u00e4\u00b7hi\u00b7gern", ",", "sich", "hin\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVINF", "$,", "PRF", "VVIZU", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar kann er nicht, wie du, ein Wonnebeben", "tokens": ["Zwar", "kann", "er", "nicht", ",", "wie", "du", ",", "ein", "Won\u00b7ne\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch seine Sch\u00f6nheit um sich her verbreiten;", "tokens": ["Durch", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "um", "sich", "her", "ver\u00b7brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PRF", "APZR", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Doch Alle horchen gern den Lieblichkeiten,", "tokens": ["Doch", "Al\u00b7le", "hor\u00b7chen", "gern", "den", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ihm begeistert auf der Lippe schweben.", "tokens": ["Die", "ihm", "be\u00b7geis\u00b7tert", "auf", "der", "Lip\u00b7pe", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fcrchte nur, es m\u00f6chte dich erbittern,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nur", ",", "es", "m\u00f6ch\u00b7te", "dich", "er\u00b7bit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich mir selbst so hohes Lob verstatte,", "tokens": ["Wenn", "ich", "mir", "selbst", "so", "ho\u00b7hes", "Lob", "ver\u00b7stat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blos um vor dir in falschem Glanz zu flittern;", "tokens": ["Blos", "um", "vor", "dir", "in", "fal\u00b7schem", "Glanz", "zu", "flit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sonst w\u00fcrd' ich sagen, da\u00df auf diese glatte,", "tokens": ["Sonst", "w\u00fcrd'", "ich", "sa\u00b7gen", ",", "da\u00df", "auf", "die\u00b7se", "glat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch junge Stirn, mit ungewissem Zittern,", "tokens": ["Noch", "jun\u00b7ge", "Stirn", ",", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schatten f\u00e4llt von einem Lorbeerblatte.", "tokens": ["Der", "Schat\u00b7ten", "f\u00e4llt", "von", "ei\u00b7nem", "Lor\u00b7beer\u00b7blat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}