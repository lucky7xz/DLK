{"textgrid.poem.32453": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "132. Die Vorspiele der Vers\u00f6hnung", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Korinne schwur, mich zu vergessen:", "tokens": ["Ko\u00b7rin\u00b7ne", "schwur", ",", "mich", "zu", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch kann sie mich nicht vergessen.", "tokens": ["Und", "doch", "kann", "sie", "mich", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sie mich sieht, und wo sie kann,", "tokens": ["Wo", "sie", "mich", "sieht", ",", "und", "wo", "sie", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "KON", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4ngt sie auf mich zu l\u00e4stern an.", "tokens": ["F\u00e4ngt", "sie", "auf", "mich", "zu", "l\u00e4s\u00b7tern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Doch warum tut sie das? warum erhitzt sie sich?", "tokens": ["Doch", "wa\u00b7rum", "tut", "sie", "das", "?", "wa\u00b7rum", "er\u00b7hitzt", "sie", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PDS", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wette was, noch liebt sie mich.", "tokens": ["Ich", "wet\u00b7te", "was", ",", "noch", "liebt", "sie", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich schwur, Korinnen zu vergessen:", "tokens": ["Ich", "schwur", ",", "Ko\u00b7rin\u00b7nen", "zu", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch kann ich sie nicht vergessen.", "tokens": ["Und", "doch", "kann", "ich", "sie", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich sie seh, und wo ich kann,", "tokens": ["Wo", "ich", "sie", "seh", ",", "und", "wo", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "KON", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fang' ich mich zu entschuld'gen an.", "tokens": ["Fang'", "ich", "mich", "zu", "ent\u00b7schuld'\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch warum tu ich das? und warum schweig' ich nie?", "tokens": ["Doch", "wa\u00b7rum", "tu", "ich", "das", "?", "und", "wa\u00b7rum", "schweig'", "ich", "nie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PDS", "$.", "KON", "PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wette was, noch lieb' ich sie.", "tokens": ["Ich", "wet\u00b7te", "was", ",", "noch", "lieb'", "ich", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}