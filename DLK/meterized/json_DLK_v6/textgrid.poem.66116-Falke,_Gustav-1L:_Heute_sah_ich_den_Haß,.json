{"textgrid.poem.66116": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heute sah ich den Ha\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute sah ich den Ha\u00df,", "tokens": ["Heu\u00b7te", "sah", "ich", "den", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den herrlichen nackten Ha\u00df.", "tokens": ["Den", "herr\u00b7li\u00b7chen", "nack\u00b7ten", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So dacht ich mir", "tokens": ["So", "dacht", "ich", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die trotzige Sch\u00f6nheit gefallener Engel:", "tokens": ["Die", "trot\u00b7zi\u00b7ge", "Sch\u00f6n\u00b7heit", "ge\u00b7fal\u00b7le\u00b7ner", "En\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wildheit ganz", "tokens": ["Wild\u00b7heit", "ganz"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und knirschender Stolz.", "tokens": ["Und", "knir\u00b7schen\u00b7der", "Stolz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbwie sch\u00f6n du bist\u00ab,", "tokens": ["\u00bb", "wie", "sch\u00f6n", "du", "bist", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "PPER", "VAFIN", "$(", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Betete ich an.", "tokens": ["Be\u00b7te\u00b7te", "ich", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u00bbmillionen", "tokens": ["\u00bb", "mil\u00b7li\u00b7o\u00b7nen"], "token_info": ["punct", "word"], "pos": ["$(", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Preisen mich\u00ab, l\u00e4chelte er,", "tokens": ["Prei\u00b7sen", "mich", "\u00ab", ",", "l\u00e4\u00b7chel\u00b7te", "er", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-++", "measure": "iambic.tetra.invert"}, "line.5": {"text": "\u00bbmein ist das Reich.\u00ab", "tokens": ["\u00bb", "mein", "ist", "das", "Reich", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und ich sah auf und sah", "tokens": ["Und", "ich", "sah", "auf", "und", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zwischen den Nachtbrauen", "tokens": ["Zwi\u00b7schen", "den", "Nacht\u00b7brau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Schmerzfalte,", "tokens": ["Die", "Schmerz\u00b7fal\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Senkrecht,", "tokens": ["Sen\u00b7krecht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Tief eingefurcht.", "tokens": ["Tief", "ein\u00b7ge\u00b7furcht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u00bbwarum diese Falte?\u00ab", "tokens": ["\u00bb", "wa\u00b7rum", "die\u00b7se", "Fal\u00b7te", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PDAT", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Abgewandt schwieg er.", "tokens": ["Ab\u00b7ge\u00b7wandt", "schwieg", "er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbwarum diese Falte?\u00ab", "tokens": ["\u00bb", "wa\u00b7rum", "die\u00b7se", "Fal\u00b7te", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PDAT", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Leise,", "tokens": ["Lei\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Verqu\u00e4lt klang es zur\u00fcck:", "tokens": ["Ver\u00b7qu\u00e4lt", "klang", "es", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbweil ich nicht lieben darf.\u00ab", "tokens": ["\u00bb", "weil", "ich", "nicht", "lie\u00b7ben", "darf", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}