{"textgrid.poem.26436": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Schiffen auf hoher See,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Schiffen auf hoher See,", "tokens": ["In", "Schif\u00b7fen", "auf", "ho\u00b7her", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ringsum das unbegrenzte Blau;", "tokens": ["Ring\u00b7sum", "das", "un\u00b7be\u00b7grenz\u00b7te", "Blau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Windespfeifen und Musik der Wellen, der gro\u00dfen k\u00f6niglichen Wellen ...", "tokens": ["Win\u00b7des\u00b7pfei\u00b7fen", "und", "Mu\u00b7sik", "der", "Wel\u00b7len", ",", "der", "gro\u00b7\u00dfen", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Wel\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+--+--+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Ein Fahrzeug einsam schaukelnd auf dem Wogenschwall,", "tokens": ["Ein", "Fahr\u00b7zeug", "ein\u00b7sam", "schau\u00b7kelnd", "auf", "dem", "Wo\u00b7gen\u00b7schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo freudig voller Zuversicht sich wei\u00dfe Segel bl\u00e4hen,", "tokens": ["Wo", "freu\u00b7dig", "vol\u00b7ler", "Zu\u00b7ver\u00b7sicht", "sich", "wei\u00b7\u00dfe", "Se\u00b7gel", "bl\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Durchschneidet es den \u00c4ther, im Glitzern und Schaum des Tages,", "tokens": ["Durch\u00b7schnei\u00b7det", "es", "den", "\u00c4\u00b7ther", ",", "im", "Glit\u00b7zern", "und", "Schaum", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPRART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Oder in der Nacht, unter unz\u00e4hligen Sternen.", "tokens": ["O\u00b7der", "in", "der", "Nacht", ",", "un\u00b7ter", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "----+--+---+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Dort werde ich von Seefahrern, jung und alt, vielleicht gelesen werden \u2013 in Erinnerung an das Land \u2013", "tokens": ["Dort", "wer\u00b7de", "ich", "von", "See\u00b7fah\u00b7rern", ",", "jung", "und", "alt", ",", "viel\u00b7leicht", "ge\u00b7le\u00b7sen", "wer\u00b7den", "\u2013", "in", "E\u00b7rin\u00b7ne\u00b7rung", "an", "das", "Land", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "ADV", "VVPP", "VAINF", "$(", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Endlich im vollen seelischen Rapport!", "tokens": ["End\u00b7lich", "im", "vol\u00b7len", "see\u00b7li\u00b7schen", "Rap\u00b7port", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Dann m\u00f6gen sie wohl sagen: ", "tokens": ["Dann", "m\u00f6\u00b7gen", "sie", "wohl", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht nur eine Erinnerung an das Land bist du,", "tokens": ["Nicht", "nur", "ei\u00b7ne", "E\u00b7rin\u00b7ne\u00b7rung", "an", "das", "Land", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Auch du eine einsame Barke durchschneidest den \u00c4ther.", "tokens": ["Auch", "du", "ei\u00b7ne", "ein\u00b7sa\u00b7me", "Bar\u00b7ke", "durch\u00b7schnei\u00b7dest", "den", "\u00c4\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohin bestimmt? ich wei\u00df es nicht, doch voller Zuversicht", "tokens": ["Wo\u00b7hin", "be\u00b7stimmt", "?", "ich", "wei\u00df", "es", "nicht", ",", "doch", "vol\u00b7ler", "Zu\u00b7ver\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Begleite jedes Schiff, das segelt,", "tokens": ["Be\u00b7glei\u00b7te", "je\u00b7des", "Schiff", ",", "das", "se\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Bruderschiff f\u00fcr alle, segle du.", "tokens": ["Ein", "Bru\u00b7der\u00b7schiff", "f\u00fcr", "al\u00b7le", ",", "seg\u00b7le", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Trage in dir meine Liebe zu euch, ihr Seefahrer (ich falte sie in jedes Blatt hinein),", "tokens": ["Tra\u00b7ge", "in", "dir", "mei\u00b7ne", "Lie\u00b7be", "zu", "euch", ",", "ihr", "See\u00b7fah\u00b7rer", "(", "ich", "fal\u00b7te", "sie", "in", "je\u00b7des", "Blatt", "hin\u00b7ein", ")", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$(", "$,"], "meter": "+-+-+-+--+-++--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Eile mein Buch! Spanne deine wei\u00dfen Segel, meine kleine Barke, \u00fcber den k\u00f6niglichen Wellen,", "tokens": ["Ei\u00b7le", "mein", "Buch", "!", "Span\u00b7ne", "dei\u00b7ne", "wei\u00b7\u00dfen", "Se\u00b7gel", ",", "mei\u00b7ne", "klei\u00b7ne", "Bar\u00b7ke", ",", "\u00fc\u00b7ber", "den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-+-+--+---+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Singe weiter, segle dahin \u00fcber das grenzenlose Blau und bringe hinaus auf jedes Meer", "tokens": ["Sin\u00b7ge", "wei\u00b7ter", ",", "seg\u00b7le", "da\u00b7hin", "\u00fc\u00b7ber", "das", "gren\u00b7zen\u00b7lo\u00b7se", "Blau", "und", "brin\u00b7ge", "hin\u00b7aus", "auf", "je\u00b7des", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PAV", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+--+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Dieses Lied f\u00fcr die Seefahrer und ihre Schiffe!", "tokens": ["Die\u00b7ses", "Lied", "f\u00fcr", "die", "See\u00b7fah\u00b7rer", "und", "ih\u00b7re", "Schif\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}