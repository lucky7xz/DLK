{"dta.poem.976": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "V", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn solch ein sausen in den wipfeln w&#252;hlt                 ", "tokens": ["Wenn", "solch", "ein", "sau\u00b7sen", "in", "den", "wip\u00b7feln", "w", "&#252;", "hlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PIAT", "ART", "ADJD", "APPR", "ART", "NN", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ist es nicht mehr als dass ein sehnen drohe                 ", "tokens": ["Ist", "es", "nicht", "mehr", "als", "dass", "ein", "seh\u00b7nen", "dro\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als blaue blicke blumen blonde frohe?               ", "tokens": ["Als", "blau\u00b7e", "bli\u00b7cke", "blu\u00b7men", "blon\u00b7de", "fro\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn solch ein branden um die festen sp&#252;lt                 ", "tokens": ["Wenn", "solch", "ein", "bran\u00b7den", "um", "die", "fes\u00b7ten", "sp", "&#252;", "lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PIAT", "ART", "ADJA", "APPR", "ART", "ADJA", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dass du verlassen irrend an dem strand                 ", "tokens": ["Dass", "du", "ver\u00b7las\u00b7sen", "ir\u00b7rend", "an", "dem", "strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die rettung suchst in leerer himmel brand?               ", "tokens": ["Die", "ret\u00b7tung", "suchst", "in", "lee\u00b7rer", "him\u00b7mel", "brand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dass ich wie nie dich blass und bebend finde                 ", "tokens": ["Dass", "ich", "wie", "nie", "dich", "blass", "und", "be\u00b7bend", "fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "ADV", "PPER", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kaum mehr noch als am wegesrand die blinde                 ", "tokens": ["Kaum", "mehr", "noch", "als", "am", "we\u00b7ges\u00b7rand", "die", "blin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "KOKOM", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die unbeachtet ruft im lauten winde.               ", "tokens": ["Die", "un\u00b7be\u00b7ach\u00b7tet", "ruft", "im", "lau\u00b7ten", "win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}