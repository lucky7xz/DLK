{"dta.poem.19434": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "An die Schwalbe.  \n Anakreons zw\u00f6lfte Ode.  \u03a4\u03b9\u03c3\u03bf\u03b9 \u03d1\u03ad\u03bb\u03b5\u03b9\u03c2 \u03c0\u03bf\u03b9\u03ae\u03c3\u03c9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Wie soll ich dich bestrafen,", "tokens": ["Wie", "soll", "ich", "dich", "be\u00b7stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du plauderhafte Schwalbe?", "tokens": ["Du", "plau\u00b7der\u00b7haf\u00b7te", "Schwal\u00b7be", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Soll ich die leichten Schwingen", "tokens": ["Soll", "ich", "die", "leich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir k\u00fcrzen? oder soll ich,", "tokens": ["Dir", "k\u00fcr\u00b7zen", "?", "o\u00b7der", "soll", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "KON", "VMFIN", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wie Tereus that, die Zunge", "tokens": ["Wie", "Te\u00b7reus", "that", ",", "die", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dir aus dem Schnabel reissen?", "tokens": ["Dir", "aus", "dem", "Schna\u00b7bel", "reis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Aus meinen sch\u00f6nen Tr\u00e4umen,", "tokens": ["Aus", "mei\u00b7nen", "sch\u00f6\u00b7nen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit deiner fr\u00fchen Stimme,", "tokens": ["Mit", "dei\u00b7ner", "fr\u00fc\u00b7hen", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mein M\u00e4dchen mir zu rauben!", "tokens": ["Mein", "M\u00e4d\u00b7chen", "mir", "zu", "rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}