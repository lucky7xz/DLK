{"textgrid.poem.47302": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl endet Tod des Lebens Not,", "tokens": ["Wohl", "en\u00b7det", "Tod", "des", "Le\u00b7bens", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch schauert Leben vor dem Tod.", "tokens": ["Doch", "schau\u00b7ert", "Le\u00b7ben", "vor", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Leben sieht die dunkle Hand,", "tokens": ["Das", "Le\u00b7ben", "sieht", "die", "dunk\u00b7le", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den hellen Kelch nicht, den sie bot.", "tokens": ["Den", "hel\u00b7len", "Kelch", "nicht", ",", "den", "sie", "bot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So schauert vor der Lieb' ein Herz,", "tokens": ["So", "schau\u00b7ert", "vor", "der", "Lieb'", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie von Untergang bedroht.", "tokens": ["Als", "wie", "von", "Un\u00b7ter\u00b7gang", "be\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wo die Lieb' erwachet, stirbt", "tokens": ["Denn", "wo", "die", "Lieb'", "er\u00b7wa\u00b7chet", ",", "stirbt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Ich, der dunkele Despot.", "tokens": ["Das", "Ich", ",", "der", "dun\u00b7ke\u00b7le", "Des\u00b7pot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du la\u00df ihn sterben in der Nacht,", "tokens": ["Du", "la\u00df", "ihn", "ster\u00b7ben", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und atme frei im Morgenrot.", "tokens": ["Und", "at\u00b7me", "frei", "im", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}