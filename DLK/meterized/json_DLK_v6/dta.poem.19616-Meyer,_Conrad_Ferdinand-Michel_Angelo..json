{"dta.poem.19616": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Michel Angelo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In der Sistine d\u00e4mmerhohem Raum,", "tokens": ["In", "der", "Sis\u00b7ti\u00b7ne", "d\u00e4m\u00b7mer\u00b7ho\u00b7hem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das Bibelbuch in seiner nerv'gen Hand,", "tokens": ["Das", "Bi\u00b7bel\u00b7buch", "in", "sei\u00b7ner", "ner\u00b7v'\u00b7gen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sitzt Michel Angelo in wachem Traum,", "tokens": ["Sitzt", "Mi\u00b7chel", "An\u00b7ge\u00b7lo", "in", "wa\u00b7chem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umhellt von einer kleinen Ampel Brand.", "tokens": ["Um\u00b7hellt", "von", "ei\u00b7ner", "klei\u00b7nen", "Am\u00b7pel", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Laut spricht hinein er in die Mitternacht,", "tokens": ["Laut", "spricht", "hin\u00b7ein", "er", "in", "die", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als lauscht' ein Gast ihm gegen\u00fcber hier,", "tokens": ["Als", "lauscht'", "ein", "Gast", "ihm", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald wie mit einer allgewalt'gen Macht,", "tokens": ["Bald", "wie", "mit", "ei\u00b7ner", "all\u00b7ge\u00b7walt'\u00b7gen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald wieder wie mit Seinesgleichen schier:", "tokens": ["Bald", "wie\u00b7der", "wie", "mit", "Sei\u00b7nes\u00b7glei\u00b7chen", "schier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u201eumfa\u00dft, umgrenzt hab' ich Dich, ewig Sein,", "tokens": ["\u201e", "um\u00b7fa\u00dft", ",", "um\u00b7grenzt", "hab'", "ich", "Dich", ",", "e\u00b7wig", "Sein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVPP", "VAFIN", "PPER", "PPER", "$,", "ADJD", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit meinen gro\u00dfen Linien f\u00fcnfmal dort!", "tokens": ["Mit", "mei\u00b7nen", "gro\u00b7\u00dfen", "Li\u00b7ni\u00b7en", "f\u00fcnf\u00b7mal", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich h\u00fcllte Dich in lichte M\u00e4ntel ein", "tokens": ["Ich", "h\u00fcll\u00b7te", "Dich", "in", "lich\u00b7te", "M\u00e4n\u00b7tel", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gab Dir Leib, wie dieses Bibelwort.", "tokens": ["Und", "gab", "Dir", "Leib", ",", "wie", "die\u00b7ses", "Bi\u00b7bel\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "PWAV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit weh'nden Haaren st\u00fcrmst Du feurig wild", "tokens": ["Mit", "weh'n\u00b7den", "Haa\u00b7ren", "st\u00fcrmst", "Du", "feu\u00b7rig", "wild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Sonnen immer neuen Sonnen zu,", "tokens": ["Von", "Son\u00b7nen", "im\u00b7mer", "neu\u00b7en", "Son\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Deinen Menschen bist in meinem Bild", "tokens": ["F\u00fcr", "Dei\u00b7nen", "Men\u00b7schen", "bist", "in", "mei\u00b7nem", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entgegenschwebend und barmherzig Du!", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7schwe\u00b7bend", "und", "barm\u00b7her\u00b7zig", "Du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PPER", "$."], "meter": "---+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "So schuf ich Dich mit meiner nicht'gen Kraft:", "tokens": ["So", "schuf", "ich", "Dich", "mit", "mei\u00b7ner", "nicht'\u00b7gen", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Damit ich nicht der gr\u00f6\u00dfre K\u00fcnstler sei,", "tokens": ["Da\u00b7mit", "ich", "nicht", "der", "gr\u00f6\u00df\u00b7re", "K\u00fcnst\u00b7ler", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schaff mich \u2014 ich bin ein Knecht der Leidenschaft \u2014", "tokens": ["Schaff", "mich", "ich", "bin", "ein", "Knecht", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nach Deinem Bilde schaff mich rein und frei!", "tokens": ["Nach", "Dei\u00b7nem", "Bil\u00b7de", "schaff", "mich", "rein", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Den ersten Menschen formtest Du aus Thon,", "tokens": ["Den", "ers\u00b7ten", "Men\u00b7schen", "form\u00b7test", "Du", "aus", "Thon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich werde schon von h\u00e4rterm Stoffe sein,", "tokens": ["Ich", "wer\u00b7de", "schon", "von", "h\u00e4r\u00b7term", "Stof\u00b7fe", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da, Meister, brauchst Du Deinen Hammer schon,", "tokens": ["Da", ",", "Meis\u00b7ter", ",", "brauchst", "Du", "Dei\u00b7nen", "Ham\u00b7mer", "schon", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bildhauer Gott, schlag zu! Ich bin der Stein.", "tokens": ["Bild\u00b7hau\u00b7er", "Gott", ",", "schlag", "zu", "!", "Ich", "bin", "der", "Stein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}