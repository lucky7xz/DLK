{"textgrid.poem.47802": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir harren nicht mehr ahnungsvoll", "tokens": ["Wir", "har\u00b7ren", "nicht", "mehr", "ah\u00b7nungs\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sonst auf blaue M\u00e4rchenwunder;", "tokens": ["Wie", "sonst", "auf", "blau\u00b7e", "M\u00e4r\u00b7chen\u00b7wun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sich das Buch entwickeln soll,", "tokens": ["Wie", "sich", "das", "Buch", "ent\u00b7wi\u00b7ckeln", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wissen's ganz genau jetzunder.", "tokens": ["Wir", "wis\u00b7sen's", "ganz", "ge\u00b7nau", "je\u00b7tzun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir bl\u00e4tterten schon hin und her", "tokens": ["Wir", "bl\u00e4t\u00b7ter\u00b7ten", "schon", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 Denn ruchlos wurden unsre H\u00e4nde \u2013,", "tokens": ["\u2013", "Denn", "ruch\u00b7los", "wur\u00b7den", "uns\u00b7re", "H\u00e4n\u00b7de", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf der letzten Seite sahn", "tokens": ["Und", "auf", "der", "letz\u00b7ten", "Sei\u00b7te", "sahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir schon das schlimme W\u00f6rtlein Ende.", "tokens": ["Wir", "schon", "das", "schlim\u00b7me", "W\u00f6rt\u00b7lein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}