{"textgrid.poem.61007": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Sonne hoch und heiter", "tokens": ["Wenn", "die", "Son\u00b7ne", "hoch", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelt, wenn der Tag sich neigt,", "tokens": ["L\u00e4\u00b7chelt", ",", "wenn", "der", "Tag", "sich", "neigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe bleibt die goldne Leiter,", "tokens": ["Lie\u00b7be", "bleibt", "die", "gold\u00b7ne", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drauf das Herz zum Himmel steigt;", "tokens": ["Drauf", "das", "Herz", "zum", "Him\u00b7mel", "steigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob der J\u00fcngling sie empfinde,", "tokens": ["Ob", "der", "J\u00fcng\u00b7ling", "sie", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den es zur Geliebten zieht,", "tokens": ["Den", "es", "zur", "Ge\u00b7lieb\u00b7ten", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob die Mutter sie dem Kinde", "tokens": ["Ob", "die", "Mut\u00b7ter", "sie", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sing' als s\u00fc\u00dfes Wiegenlied,", "tokens": ["Sing'", "als", "s\u00fc\u00b7\u00dfes", "Wie\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob der Freund dem Freund sie spende,", "tokens": ["Ob", "der", "Freund", "dem", "Freund", "sie", "spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den er fest im Arme h\u00e4lt,", "tokens": ["Den", "er", "fest", "im", "Ar\u00b7me", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob der hohe Greis sie wende", "tokens": ["Ob", "der", "ho\u00b7he", "Greis", "sie", "wen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den weiten Kreis der Welt", "tokens": ["Auf", "den", "wei\u00b7ten", "Kreis", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob der Heimat sie der Streiter", "tokens": ["Ob", "der", "Hei\u00b7mat", "sie", "der", "Strei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zolle, wenn er wund sich neigt:", "tokens": ["Zol\u00b7le", ",", "wenn", "er", "wund", "sich", "neigt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe bleibt die goldne Leiter,", "tokens": ["Lie\u00b7be", "bleibt", "die", "gold\u00b7ne", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drauf das Herz zum Himmel steigt.", "tokens": ["Drauf", "das", "Herz", "zum", "Him\u00b7mel", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}