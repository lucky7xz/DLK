{"textgrid.poem.36530": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2. Der Hund und der Wolf", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein armer magrer Wolf, der wenig L\u00e4mmer stahl,", "tokens": ["Ein", "ar\u00b7mer", "mag\u00b7rer", "Wolf", ",", "der", "we\u00b7nig", "L\u00e4m\u00b7mer", "stahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begegnete, bergab, in einem engen Thal,", "tokens": ["Be\u00b7ge\u00b7gne\u00b7te", ",", "berg\u00b7ab", ",", "in", "ei\u00b7nem", "en\u00b7gen", "Thal", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Einst eines reichen Mannes Hund,", "tokens": ["Einst", "ei\u00b7nes", "rei\u00b7chen", "Man\u00b7nes", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Namen Sigismund.", "tokens": ["Mit", "Na\u00b7men", "Si\u00b7gis\u00b7mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ei! denkt der Wolf, w\u00e4r' ich entkr\u00e4ftet nicht;", "tokens": ["Ei", "!", "denkt", "der", "Wolf", ",", "w\u00e4r'", "ich", "ent\u00b7kr\u00e4f\u00b7tet", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NE", "$,", "VAFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An diesem Herrn wollt' ich mich r\u00e4chen", "tokens": ["An", "die\u00b7sem", "Herrn", "wollt'", "ich", "mich", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr manchen b\u00f6sen Bi\u00df. O du! du B\u00f6sewicht!", "tokens": ["F\u00fcr", "man\u00b7chen", "b\u00f6\u00b7sen", "Bi\u00df", ".", "O", "du", "!", "du", "B\u00f6\u00b7se\u00b7wicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$.", "NE", "NE", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er denkt's! er wagt's nicht auszusprechen.", "tokens": ["Er", "denkt's", "!", "er", "wagt's", "nicht", "aus\u00b7zu\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So freundlich, als wenn er", "tokens": ["So", "freund\u00b7lich", ",", "als", "wenn", "er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Sein Freund, sein treuer Bruder w\u00e4r',", "tokens": ["Sein", "Freund", ",", "sein", "treu\u00b7er", "Bru\u00b7der", "w\u00e4r'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Spricht er zum Hunde:", "tokens": ["Spricht", "er", "zum", "Hun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Und geht sogleich den Weg zum Herrn des Hundes mit.", "tokens": ["Und", "geht", "sog\u00b7leich", "den", "Weg", "zum", "Herrn", "des", "Hun\u00b7des", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesellig gehen sie, wie Br\u00fcder, einen Schritt.", "tokens": ["Ge\u00b7sel\u00b7lig", "ge\u00b7hen", "sie", ",", "wie", "Br\u00fc\u00b7der", ",", "ei\u00b7nen", "Schritt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWAV", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht lange: denn der Wolf, der so gesellig trabt,", "tokens": ["Nicht", "lan\u00b7ge", ":", "denn", "der", "Wolf", ",", "der", "so", "ge\u00b7sel\u00b7lig", "trabt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "KON", "ART", "NE", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Betrachtet seinen Freund, sieht seinen Hals geschabt,", "tokens": ["Be\u00b7trach\u00b7tet", "sei\u00b7nen", "Freund", ",", "sieht", "sei\u00b7nen", "Hals", "ge\u00b7schabt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Fragt: ", "tokens": ["Fragt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Rief Sigismund. Der Wolf, in seinem Elemente,", "tokens": ["Rief", "Si\u00b7gis\u00b7mund", ".", "Der", "Wolf", ",", "in", "sei\u00b7nem", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ART", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sah sich nicht um, lief fort, und l\u00e4uft wohl noch!", "tokens": ["Sah", "sich", "nicht", "um", ",", "lief", "fort", ",", "und", "l\u00e4uft", "wohl", "noch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}