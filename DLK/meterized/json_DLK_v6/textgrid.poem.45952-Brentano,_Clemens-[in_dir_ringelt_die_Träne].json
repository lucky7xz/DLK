{"textgrid.poem.45952": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[in dir ringelt die Tr\u00e4ne]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dir ringelt die Tr\u00e4ne, auf dir l\u00e4chelt das Mondlicht,", "tokens": ["In", "dir", "rin\u00b7gelt", "die", "Tr\u00e4\u00b7ne", ",", "auf", "dir", "l\u00e4\u00b7chelt", "das", "Mond\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Welle, bald Woge, bald Strom, wie dich das Ufer umkr\u00e4nzt,", "tokens": ["Wel\u00b7le", ",", "bald", "Wo\u00b7ge", ",", "bald", "Strom", ",", "wie", "dich", "das", "U\u00b7fer", "um\u00b7kr\u00e4nzt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Gifttrank und lieblicher Wein, wie dich die Schale umfa\u00dft.", "tokens": ["Gift\u00b7trank", "und", "lieb\u00b7li\u00b7cher", "Wein", ",", "wie", "dich", "die", "Scha\u00b7le", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lethe wird nimmer in dir, Psychen ein Spiegel wohl oft,", "tokens": ["Le\u00b7the", "wird", "nim\u00b7mer", "in", "dir", ",", "Psy\u00b7chen", "ein", "Spie\u00b7gel", "wohl", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "PPER", "$,", "NN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Aber es tauchet der Schwan ins heilign\u00fcchterne Wasser", "tokens": ["A\u00b7ber", "es", "tau\u00b7chet", "der", "Schwan", "ins", "hei\u00b7lig\u00b7n\u00fcch\u00b7ter\u00b7ne", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Trunken das Haupt, und singt sterbend dem Sternbild den Gru\u00df.", "tokens": ["Trun\u00b7ken", "das", "Haupt", ",", "und", "singt", "ster\u00b7bend", "dem", "Stern\u00b7bild", "den", "Gru\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}}}}