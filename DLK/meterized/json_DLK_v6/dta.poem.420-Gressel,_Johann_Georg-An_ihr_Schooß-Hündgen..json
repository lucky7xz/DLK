{"dta.poem.420": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An ihr Schoo\u00df-H\u00fcndgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00f6nne mir den Platz wo deine Knochen ruhn/", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "den", "Platz", "wo", "dei\u00b7ne", "Kno\u00b7chen", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PWAV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df mich einst im Schoo\u00df der sch\u00f6nen ", "tokens": ["Und", "la\u00df", "mich", "einst", "im", "Schoo\u00df", "der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wil ihr lieblicher als deine Zunge thun/", "tokens": ["Ich", "wil", "ihr", "lieb\u00b7li\u00b7cher", "als", "dei\u00b7ne", "Zun\u00b7ge", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "KOKOM", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Marmor-Schoo\u00df mit Perlen Milch vergn\u00fcgen/", "tokens": ["Und", "ih\u00b7re", "Mar\u00b7mor\u00b7Schoo\u00df", "mit", "Per\u00b7len", "Milch", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dich aber ", "tokens": ["Dich", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und zur Belohnung vor ein fettes S\u00fcpgen setzen.", "tokens": ["Und", "zur", "Be\u00b7loh\u00b7nung", "vor", "ein", "fet\u00b7tes", "S\u00fcp\u00b7gen", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}