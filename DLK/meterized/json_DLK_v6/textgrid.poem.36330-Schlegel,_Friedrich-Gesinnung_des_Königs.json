{"textgrid.poem.36330": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gesinnung des K\u00f6nigs", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mannes Herz in starker Brust,", "tokens": ["Man\u00b7nes", "Herz", "in", "star\u00b7ker", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fern von weib'scher Sitt' und Lust,", "tokens": ["Fern", "von", "weib'\u00b7scher", "Sitt'", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So wie edle Krieger sind,", "tokens": ["So", "wie", "ed\u00b7le", "Krie\u00b7ger", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sei der K\u00f6nig uns gesinnt;", "tokens": ["Sei", "der", "K\u00f6\u00b7nig", "uns", "ge\u00b7sinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer f\u00fcr das Recht bem\u00fcht,", "tokens": ["Im\u00b7mer", "f\u00fcr", "das", "Recht", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alte Satzung treu beh\u00fct't,", "tokens": ["Al\u00b7te", "Sat\u00b7zung", "treu", "be\u00b7h\u00fct't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott vor allen stets gedient,", "tokens": ["Gott", "vor", "al\u00b7len", "stets", "ge\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dessen Lorbeer ewig gr\u00fcnt.", "tokens": ["Des\u00b7sen", "Lor\u00b7beer", "e\u00b7wig", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}