{"textgrid.poem.28453": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00f6cke nemen zu; die Schafe m\u00fcssen weichen", "tokens": ["Die", "B\u00f6\u00b7cke", "ne\u00b7men", "zu", ";", "die", "Scha\u00b7fe", "m\u00fcs\u00b7sen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auff den letzten Zug von ihrem stossen keichen.", "tokens": ["Und", "auff", "den", "letz\u00b7ten", "Zug", "von", "ih\u00b7rem", "stos\u00b7sen", "kei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kumm, kumm, Herr Jesu, kumm! mach Ordnung und theil ein", "tokens": ["Kumm", ",", "kumm", ",", "Herr", "Je\u00b7su", ",", "kumm", "!", "mach", "Ord\u00b7nung", "und", "theil", "ein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "$,", "NN", "NE", "$,", "ADJD", "$.", "VVFIN", "NN", "KON", "NN", "ART"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Schaf in deine Lust, die B\u00f6ck ins Teuffels Pein.", "tokens": ["Die", "Schaf", "in", "dei\u00b7ne", "Lust", ",", "die", "B\u00f6ck", "ins", "Teuf\u00b7fels", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}