{"textgrid.poem.39464": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du nahtest nur, uns wieder zu verla\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du nahtest nur, uns wieder zu verla\u00dfen,", "tokens": ["Du", "nah\u00b7test", "nur", ",", "uns", "wie\u00b7der", "zu", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein rascher Weg hat dich vorbeigetragen!", "tokens": ["Dein", "ra\u00b7scher", "Weg", "hat", "dich", "vor\u00b7bei\u00b7ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von deiner Gegenwart begl\u00fcckten Tagen", "tokens": ["Von", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "be\u00b7gl\u00fcck\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sah ich zu bald den heitern Strahl erbla\u00dfen.", "tokens": ["Sah", "ich", "zu", "bald", "den", "hei\u00b7tern", "Strahl", "er\u00b7bla\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die\u00df kleine Blatt, das du zur\u00fcckgela\u00dfen,", "tokens": ["Die\u00df", "klei\u00b7ne", "Blatt", ",", "das", "du", "zu\u00b7r\u00fcck\u00b7ge\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es soll dir meine W\u00fcnsche, meine Klagen,", "tokens": ["Es", "soll", "dir", "mei\u00b7ne", "W\u00fcn\u00b7sche", ",", "mei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Bild in mir, dein Angedenken sagen:", "tokens": ["Dein", "Bild", "in", "mir", ",", "dein", "An\u00b7ge\u00b7den\u00b7ken", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie k\u00f6nnt' es so viel gro\u00dfe Dinge fa\u00dfen?", "tokens": ["Wie", "k\u00f6nnt'", "es", "so", "viel", "gro\u00b7\u00dfe", "Din\u00b7ge", "fa\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum die\u00df nur: wird's in deiner N\u00e4he wohnen,", "tokens": ["Drum", "die\u00df", "nur", ":", "wird's", "in", "dei\u00b7ner", "N\u00e4\u00b7he", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PDS", "ADV", "$.", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird manchmal seinen Sinn dein Blick entsiegeln,", "tokens": ["Wird", "manch\u00b7mal", "sei\u00b7nen", "Sinn", "dein", "Blick", "ent\u00b7sie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So neid' ich ihm sein gl\u00fcckliches Gelingen.", "tokens": ["So", "neid'", "ich", "ihm", "sein", "gl\u00fcck\u00b7li\u00b7ches", "Ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O m\u00f6cht' ein T\u00e4ubchen dir es \u00fcberbringen,", "tokens": ["O", "m\u00f6cht'", "ein", "T\u00e4ub\u00b7chen", "dir", "es", "\u00fc\u00b7berb\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und n\u00e4hmest du's ihm schmeichelnd von den Fl\u00fcgeln,", "tokens": ["Und", "n\u00e4h\u00b7mest", "du's", "ihm", "schmei\u00b7chelnd", "von", "den", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00f6cht' ein Ku\u00df die kleine Botin lohnen!", "tokens": ["Und", "m\u00f6cht'", "ein", "Ku\u00df", "die", "klei\u00b7ne", "Bo\u00b7tin", "loh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}