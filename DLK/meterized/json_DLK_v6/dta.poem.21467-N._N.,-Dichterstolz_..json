{"dta.poem.21467": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Dichterstolz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ja blicket stolz ihr Enkel des Helios,               ", "tokens": ["Ja", "bli\u00b7cket", "stolz", "ihr", "En\u00b7kel", "des", "He\u00b7li\u00b7os", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADJD", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Seele hei\u00df von gro\u00dfer Gedanken Glut!", "tokens": ["Die", "See\u00b7le", "hei\u00df", "von", "gro\u00b7\u00dfer", "Ge\u00b7dan\u00b7ken", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein bl\u00fchend Thal zu euren F\u00fc\u00dfen", "tokens": ["Ein", "bl\u00fc\u00b7hend", "Thal", "zu", "eu\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Breite sich schmiegend die weite Erde!", "tokens": ["Brei\u00b7te", "sich", "schmie\u00b7gend", "die", "wei\u00b7te", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Der Sterne Kranz, ein leuchtendes Diadem,", "tokens": ["Der", "Ster\u00b7ne", "Kranz", ",", "ein", "leuch\u00b7ten\u00b7des", "Di\u00b7a\u00b7dem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umbl\u00fche glanzvoll euere Stirnen! Ha,", "tokens": ["Um\u00b7bl\u00fc\u00b7he", "glanz\u00b7voll", "eu\u00b7e\u00b7re", "Stir\u00b7nen", "!", "Ha", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$.", "ITJ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vergesset nie, von oben str\u00f6met", "tokens": ["Ver\u00b7ges\u00b7set", "nie", ",", "von", "o\u00b7ben", "str\u00f6\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nieder das Licht in des Dunkels Feuchte!", "tokens": ["Nie\u00b7der", "das", "Licht", "in", "des", "Dun\u00b7kels", "Feuch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Vergesset niemals, Priester des Ewigen,", "tokens": ["Ver\u00b7ges\u00b7set", "nie\u00b7mals", ",", "Pries\u00b7ter", "des", "E\u00b7wi\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Von eurem Mund nur t\u00f6net Unsterblichkeit", "tokens": ["Von", "eu\u00b7rem", "Mund", "nur", "t\u00f6\u00b7net", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der armen Menschheit: Viel noch lebten", "tokens": ["Der", "ar\u00b7men", "Menschheit", ":", "Viel", "noch", "leb\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach Agamemnon der tapf'ren Helden,             ", "tokens": ["Nach", "A\u00b7ga\u00b7mem\u00b7non", "der", "tapf'\u00b7ren", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Die namenlos nun schlafen den ew\u2019gen Schlaf,", "tokens": ["Die", "na\u00b7men\u00b7los", "nun", "schla\u00b7fen", "den", "ew'\u00b7gen", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil ihnen nachzog nimmer in\u2019s Kampfgefild", "tokens": ["Weil", "ih\u00b7nen", "nach\u00b7zog", "nim\u00b7mer", "in's", "Kampf\u00b7ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der S\u00e4nger, leicht zu Fu\u00dfe schwebend,", "tokens": ["Der", "S\u00e4n\u00b7ger", ",", "leicht", "zu", "Fu\u00b7\u00dfe", "schwe\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Singend den herrlichsten aller Tode!", "tokens": ["Sin\u00b7gend", "den", "herr\u00b7lichs\u00b7ten", "al\u00b7ler", "To\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Von Mord und Raubgier, schn\u00f6de vergoss\u2019nem Blut,", "tokens": ["Von", "Mord", "und", "Raub\u00b7gier", ",", "schn\u00f6\u00b7de", "ver\u00b7goss'\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nichts von Achilleus w\u00fc\u00dfte die Welt, wenn nicht", "tokens": ["Nichts", "von", "A\u00b7chil\u00b7leus", "w\u00fc\u00df\u00b7te", "die", "Welt", ",", "wenn", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "APPR", "NE", "VVFIN", "ART", "NN", "$,", "KOUS", "PTKNEG"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Homer geliehn ihm h\u00e4tte seine", "tokens": ["Ho\u00b7mer", "ge\u00b7liehn", "ihm", "h\u00e4t\u00b7te", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VAFIN", "PPOSAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Eigene, g\u00f6ttliche Feuerseele!             ", "tokens": ["Ei\u00b7ge\u00b7ne", ",", "g\u00f6tt\u00b7li\u00b7che", "Feu\u00b7er\u00b7see\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Und w\u00e4hnet heut auch manche gekr\u00f6nte Stirn,", "tokens": ["Und", "w\u00e4h\u00b7net", "heut", "auch", "man\u00b7che", "ge\u00b7kr\u00f6n\u00b7te", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des S\u00e4ngers Beifall wiege so leicht wie Hauch", "tokens": ["Des", "S\u00e4n\u00b7gers", "Bei\u00b7fall", "wie\u00b7ge", "so", "leicht", "wie", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In jener Wagschal\u2019, welche sp\u00e4t\u2019re", "tokens": ["In", "je\u00b7ner", "Wag\u00b7schal'", ",", "wel\u00b7che", "sp\u00e4t'\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Folgegeschlechter zu H\u00e4nden nehmen,", "tokens": ["Fol\u00b7ge\u00b7ge\u00b7schlech\u00b7ter", "zu", "H\u00e4n\u00b7den", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "O lasset sie hinleben und \u2014 sterben auch", "tokens": ["O", "las\u00b7set", "sie", "hin\u00b7le\u00b7ben", "und", "ster\u00b7ben", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "KON", "$(", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dem dumpfen Traumwahn! Stillet den edlen Zorn,", "tokens": ["Dem", "dum\u00b7pfen", "Traum\u00b7wahn", "!", "Stil\u00b7let", "den", "ed\u00b7len", "Zorn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der heimlich aufbraust: eure Rache", "tokens": ["Der", "heim\u00b7lich", "auf\u00b7braust", ":", "eu\u00b7re", "Ra\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibe das ruhige, gro\u00dfe Schweigen.", "tokens": ["Blei\u00b7be", "das", "ru\u00b7hi\u00b7ge", ",", "gro\u00b7\u00dfe", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Was ewig lebt und lebend erfreuen soll", "tokens": ["Was", "e\u00b7wig", "lebt", "und", "le\u00b7bend", "er\u00b7freu\u00b7en", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die arme Menschheit, legen die Dichter nur", "tokens": ["Die", "ar\u00b7me", "Menschheit", ",", "le\u00b7gen", "die", "Dich\u00b7ter", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr an das Herz, da\u00df wie die Mutter", "tokens": ["Ihr", "an", "das", "Herz", ",", "da\u00df", "wie", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freudig sie staune der Vielgeliebten!", "tokens": ["Freu\u00b7dig", "sie", "stau\u00b7ne", "der", "Viel\u00b7ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}