{"dta.poem.910": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4MMERUNG", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Im Hof, verhext von milchigem D\u00e4mmerschein,", "tokens": ["Im", "Hof", ",", "ver\u00b7hext", "von", "mil\u00b7chi\u00b7gem", "D\u00e4m\u00b7mer\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch Herbstgebr\u00e4untes weiche Kranke gleiten.", "tokens": ["Durch", "Herbst\u00b7ge\u00b7br\u00e4un\u00b7tes", "wei\u00b7che", "Kran\u00b7ke", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr w\u00e4chsern-runder Blick sinnt goldner Zeiten,", "tokens": ["Ihr", "w\u00e4ch\u00b7sern\u00b7run\u00b7der", "Blick", "sinnt", "gold\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erf\u00fcllt von Tr\u00e4umerei und Ruh und Wein.", "tokens": ["Er\u00b7f\u00fcllt", "von", "Tr\u00e4u\u00b7me\u00b7rei", "und", "Ruh", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Siechentum schlie\u00dft geisterhaft sich ein.", "tokens": ["Ihr", "Sie\u00b7chen\u00b7tum", "schlie\u00dft", "geis\u00b7ter\u00b7haft", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sterne wei\u00dfe Traurigkeit verbreiten.", "tokens": ["Die", "Ster\u00b7ne", "wei\u00b7\u00dfe", "Trau\u00b7rig\u00b7keit", "ver\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Grau, erf\u00fcllt von T\u00e4uschung und Gel\u00e4uten,", "tokens": ["Im", "Grau", ",", "er\u00b7f\u00fcllt", "von", "T\u00e4u\u00b7schung", "und", "Ge\u00b7l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sieh, wie die Schrecklichen sich wirr zerstreun.", "tokens": ["Sieh", ",", "wie", "die", "Schreck\u00b7li\u00b7chen", "sich", "wirr", "zer\u00b7streun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Formlose Spottgestalten huschen, kauern", "tokens": ["Form\u00b7lo\u00b7se", "Spott\u00b7ge\u00b7stal\u00b7ten", "hu\u00b7schen", ",", "kau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "VVINF", "$,", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und flattern sie auf schwarz-gekreuzten Pfaden.", "tokens": ["Und", "flat\u00b7tern", "sie", "auf", "schwa\u00b7rz\u00b7ge\u00b7kreuz\u00b7ten", "Pfa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O! trauervolle Schatten an den Mauern.", "tokens": ["O", "!", "trau\u00b7er\u00b7vol\u00b7le", "Schat\u00b7ten", "an", "den", "Mau\u00b7ern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Die andern fliehn durch dunkelnde Arkaden;", "tokens": ["Die", "an\u00b7dern", "fliehn", "durch", "dun\u00b7keln\u00b7de", "Ar\u00b7ka\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und n\u00e4chtens st\u00fcrzen sie aus roten Schauern", "tokens": ["Und", "n\u00e4ch\u00b7tens", "st\u00fcr\u00b7zen", "sie", "aus", "ro\u00b7ten", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Sternenwinds, gleich rasenden M\u00e4naden.", "tokens": ["Des", "Ster\u00b7nen\u00b7winds", ",", "gleich", "ra\u00b7sen\u00b7den", "M\u00e4\u00b7na\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}