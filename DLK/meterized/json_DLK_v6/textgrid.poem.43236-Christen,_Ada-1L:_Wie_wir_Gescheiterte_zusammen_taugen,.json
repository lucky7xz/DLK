{"textgrid.poem.43236": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wir Gescheiterte zusammen taugen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wir Gescheiterte zusammen taugen,", "tokens": ["Wie", "wir", "Ge\u00b7schei\u00b7ter\u00b7te", "zu\u00b7sam\u00b7men", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich ernster Mann und dieses junge Leben!", "tokens": ["Ich", "erns\u00b7ter", "Mann", "und", "die\u00b7ses", "jun\u00b7ge", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchle oft ein tief-geheimes Beben,", "tokens": ["Ich", "f\u00fch\u00b7le", "oft", "ein", "tie\u00b7fge\u00b7hei\u00b7mes", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schaut es mich an mit l\u00e4ngstbekannten Augen.", "tokens": ["Schaut", "es", "mich", "an", "mit", "l\u00e4ngst\u00b7be\u00b7kann\u00b7ten", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Deinen Augen! \u2013 ja \u2013 sie gleichen Deinen,", "tokens": ["Mit", "Dei\u00b7nen", "Au\u00b7gen", "!", "\u2013", "ja", "\u2013", "sie", "glei\u00b7chen", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "PTKANT", "$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kinderblick, er zerrt an meinem Herzen,", "tokens": ["Der", "Kin\u00b7der\u00b7blick", ",", "er", "zerrt", "an", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schautest Du, so h\u00fclflos, stumm, in Schmerzen,", "tokens": ["So", "schau\u00b7test", "Du", ",", "so", "h\u00fcl\u00b7flos", ",", "stumm", ",", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$,", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Oh weinen m\u00f6cht' ich, Dir zu F\u00fc\u00dfen weinen ...", "tokens": ["Oh", "wei\u00b7nen", "m\u00f6cht'", "ich", ",", "Dir", "zu", "F\u00fc\u00b7\u00dfen", "wei\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVINF", "VMFIN", "PPER", "$,", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}