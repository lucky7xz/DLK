{"dta.poem.199": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Man bittet ein Frauen-Zimmer zur  \n  masquir ten Schlitten-Fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt/ da der rauhe Nord/ mit blanckem Ey\u00df bezogen/", "tokens": ["Jetzt", "/", "da", "der", "rau\u00b7he", "Nord", "/", "mit", "blan\u00b7ckem", "Ey\u00df", "be\u00b7zo\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Pfeil-geschwinden Lauff des strengen Flusses wehrt/", "tokens": ["Den", "Pfeil\u00b7ge\u00b7schwin\u00b7den", "Lauff", "des", "stren\u00b7gen", "Flus\u00b7ses", "wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Da nichts als weisser Schnee kommt aus der Lufft geflogen/", "tokens": ["Da", "nichts", "als", "weis\u00b7ser", "Schnee", "kommt", "aus", "der", "Lufft", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der uns das todte Feld zu lassen hat bewogen/", "tokens": ["Der", "uns", "das", "tod\u00b7te", "Feld", "zu", "las\u00b7sen", "hat", "be\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da man ein ", "tokens": ["Da", "man", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Das in der Winters-Zeit Lust und Vergn\u00fcgung mehrt/", "tokens": ["Das", "in", "der", "Win\u00b7ter\u00b7sZeit", "Lust", "und", "Ver\u00b7gn\u00fc\u00b7gung", "mehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Denn in dem kalten Schnee erhitzet ", "tokens": ["Denn", "in", "dem", "kal\u00b7ten", "Schnee", "er\u00b7hit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hat eine ", "tokens": ["Hat", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Darinnen man verkleidt so Mond-als Jahres-Zeiten", "tokens": ["Da\u00b7rin\u00b7nen", "man", "ver\u00b7kleidt", "so", "Mon\u00b7dals", "Jah\u00b7res\u00b7Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PIS", "VVFIN", "ADV", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als T\u00fcrcken und Hufarn ist willens zu begleiten/", "tokens": ["Als", "T\u00fcr\u00b7cken", "und", "Hu\u00b7farn", "ist", "wil\u00b7lens", "zu", "be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Weil nun ihr Angesicht den bunten Lentz abgiebt/", "tokens": ["Weil", "nun", "ihr", "An\u00b7ge\u00b7sicht", "den", "bun\u00b7ten", "Lentz", "ab\u00b7giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So bittet diese Zunfft/ wenn es so kan geschehen", "tokens": ["So", "bit\u00b7tet", "die\u00b7se", "Zunfft", "/", "wenn", "es", "so", "kan", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df man sie mit darbey als Fr\u00fchling m\u00f6ge sehen.", "tokens": ["Da\u00df", "man", "sie", "mit", "dar\u00b7bey", "als", "Fr\u00fch\u00b7ling", "m\u00f6\u00b7ge", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "PAV", "KOUS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}