{"textgrid.poem.24736": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwere Sonne lag im Anfang auf den Scheiben", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwere Sonne lag im Anfang auf den Scheiben", "tokens": ["Schwe\u00b7re", "Son\u00b7ne", "lag", "im", "An\u00b7fang", "auf", "den", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mit dem schweren Glanz \u2013 es standen gro\u00dfe Blumen", "tokens": ["Mit", "dem", "schwe\u00b7ren", "Glanz", "\u2013", "es", "stan\u00b7den", "gro\u00b7\u00dfe", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Auf den Tischen rings: Schwertlilien, volle Rosen,", "tokens": ["Auf", "den", "Ti\u00b7schen", "rings", ":", "Schwert\u00b7li\u00b7li\u00b7en", ",", "vol\u00b7le", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$.", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Haselzweige dumpf wie braune Erze funkelnd \u2013 vergeblich", "tokens": ["Ha\u00b7sel\u00b7zwei\u00b7ge", "dumpf", "wie", "brau\u00b7ne", "Er\u00b7ze", "fun\u00b7kelnd", "\u2013", "ver\u00b7geb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADJD", "KOKOM", "ADJA", "NN", "VVPP", "$(", "ADJD"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Rang die Stimme schwach wie eine matte Flamme", "tokens": ["Rang", "die", "Stim\u00b7me", "schwach", "wie", "ei\u00b7ne", "mat\u00b7te", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Von Unsicherm umschwebt: von Schatten, halben Gestalten.", "tokens": ["Von", "Un\u00b7si\u00b7cherm", "um\u00b7schwebt", ":", "von", "Schat\u00b7ten", ",", "hal\u00b7ben", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$.", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Aber m\u00e4hlich mit der D\u00e4mmerung wurden jene", "tokens": ["A\u00b7ber", "m\u00e4h\u00b7lich", "mit", "der", "D\u00e4m\u00b7me\u00b7rung", "wur\u00b7den", "je\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VAFIN", "PDS"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Getr\u00e4umten Schatten stark: und sie zerdrangen siegreich", "tokens": ["Ge\u00b7tr\u00e4um\u00b7ten", "Schat\u00b7ten", "stark", ":", "und", "sie", "zer\u00b7dran\u00b7gen", "sieg\u00b7reich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "$.", "KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die sch\u00f6ne Luft und da \u2013 wie gro\u00dfe kluge V\u00f6gel", "tokens": ["Die", "sch\u00f6\u00b7ne", "Luft", "und", "da", "\u2013", "wie", "gro\u00b7\u00dfe", "klu\u00b7ge", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "$(", "KOKOM", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sa\u00dfen sie schon unter uns und banden uns mit den Augen.", "tokens": ["sa\u00b7\u00dfen", "sie", "schon", "un\u00b7ter", "uns", "und", "ban\u00b7den", "uns", "mit", "den", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}}}}}