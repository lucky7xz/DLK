{"textgrid.poem.46225": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von Herren Axeln Oxenstiern, schwedischen Reichscanzlern", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man findet kein gestirn, das mit so klarem brand", "tokens": ["Man", "fin\u00b7det", "kein", "ge\u00b7stirn", ",", "das", "mit", "so", "kla\u00b7rem", "brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und starkem gegenschein als diser nordstern scheinet,", "tokens": ["und", "star\u00b7kem", "ge\u00b7gen\u00b7schein", "als", "di\u00b7ser", "nords\u00b7tern", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KOKOM", "PDS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "stracks wider die zu weit und hart greiffende hand,", "tokens": ["stracks", "wi\u00b7der", "die", "zu", "weit", "und", "hart", "greif\u00b7fen\u00b7de", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "PTKA", "ADJD", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "darunder das numehr gejochte Teutschland weinet.", "tokens": ["da\u00b7run\u00b7der", "das", "nu\u00b7mehr", "ge\u00b7joch\u00b7te", "Teutschland", "wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Man findet keine stirn, darunder mehr verstand,", "tokens": ["Man", "fin\u00b7det", "kei\u00b7ne", "stirn", ",", "da\u00b7run\u00b7der", "mehr", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$,", "PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "erfahrung, weisheit, kunst, des risen, der vermeinet,", "tokens": ["er\u00b7fah\u00b7rung", ",", "weis\u00b7heit", ",", "kunst", ",", "des", "ri\u00b7sen", ",", "der", "ver\u00b7mei\u00b7net", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "$,", "PTKVZ", "$,", "ART", "ADJA", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df ihm gehorchen solt ein jedes meer, volk, land,", "tokens": ["da\u00df", "ihm", "ge\u00b7hor\u00b7chen", "solt", "ein", "je\u00b7des", "meer", ",", "volk", ",", "land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "PIAT", "NN", "$,", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu weit g\u00e4hnenden munds weltweiten lust verneinet", "tokens": ["zu", "weit", "g\u00e4h\u00b7nen\u00b7den", "munds", "welt\u00b7wei\u00b7ten", "lust", "ver\u00b7nei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Hat schon dein Herkules, Gustav der gro\u00df, mit mut", "tokens": ["Hat", "schon", "dein", "Her\u00b7ku\u00b7les", ",", "Gus\u00b7tav", "der", "gro\u00df", ",", "mit", "mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "NE", "ART", "ADJD", "$,", "APPR", "NN"], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "sich durch der Risen heer den g\u00f6ttern zugeschlagen,", "tokens": ["sich", "durch", "der", "Ri\u00b7sen", "heer", "den", "g\u00f6t\u00b7tern", "zu\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so f\u00f6rcht, o frommer hauf, du doch nicht ihre wut;", "tokens": ["so", "f\u00f6rcht", ",", "o", "from\u00b7mer", "hauf", ",", "du", "doch", "nicht", "ih\u00b7re", "wut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "ADJA", "NN", "$,", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dan solt der himmel sich vil f\u00f6rchten oder klagen,", "tokens": ["Dan", "solt", "der", "him\u00b7mel", "sich", "vil", "f\u00f6rch\u00b7ten", "o\u00b7der", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "alslang des h\u00f6chsten hand, allm\u00e4chtig und allgut", "tokens": ["als\u00b7lang", "des", "h\u00f6chs\u00b7ten", "hand", ",", "all\u00b7m\u00e4ch\u00b7tig", "und", "all\u00b7gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "bewahret dise seul und ", "tokens": ["be\u00b7wah\u00b7ret", "di\u00b7se", "seul", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}