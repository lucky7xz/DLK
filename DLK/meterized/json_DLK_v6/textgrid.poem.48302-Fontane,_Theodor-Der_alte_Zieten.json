{"textgrid.poem.48302": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Zieten", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Joachim Hans von Zieten,", "tokens": ["Joa\u00b7ch\u00b7im", "Hans", "von", "Zie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Husarengeneral,", "tokens": ["Hu\u00b7sa\u00b7ren\u00b7ge\u00b7ne\u00b7ral", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+----+", "measure": "dactylic.init"}, "line.3": {"text": "Dem Feind die Stirne bieten,", "tokens": ["Dem", "Feind", "die", "Stir\u00b7ne", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er tat's wohl hundert Mal;", "tokens": ["Er", "tat's", "wohl", "hun\u00b7dert", "Mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben's all' erfahren,", "tokens": ["Sie", "ha\u00b7ben's", "all'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie er die Pelze wusch,", "tokens": ["Wie", "er", "die", "Pel\u00b7ze", "wusch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit seinen Leibhusaren", "tokens": ["Mit", "sei\u00b7nen", "Leib\u00b7hu\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Hei, wie den Feind sie bl\u00e4uten", "tokens": ["Hei", ",", "wie", "den", "Feind", "sie", "bl\u00e4u\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Bei Hennersdorf und Prag,", "tokens": ["Bei", "Hen\u00b7ners\u00b7dorf", "und", "Prag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei Liegnitz und bei Leuthen,", "tokens": ["Bei", "Lie\u00b7gnitz", "und", "bei", "Leu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weiter Schlag auf Schlag;", "tokens": ["Und", "wei\u00b7ter", "Schlag", "auf", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bei Torgau, Tag der Ehre,", "tokens": ["Bei", "Tor\u00b7gau", ",", "Tag", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ritt selbst der ", "tokens": ["Ritt", "selbst", "der"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Erst noch mein Schlachtfeld aus.\u00ab", "tokens": ["Erst", "noch", "mein", "Schlacht\u00b7feld", "aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie kamen nie alleine,", "tokens": ["Sie", "ka\u00b7men", "nie", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der Donner war der eine,", "tokens": ["Der", "Don\u00b7ner", "war", "der", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der andre war der Blitz.", "tokens": ["Der", "and\u00b7re", "war", "der", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wies sich keiner tr\u00e4ge,", "tokens": ["Es", "wies", "sich", "kei\u00b7ner", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum schlug's auch immer ein,", "tokens": ["Drum", "schlug's", "auch", "im\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob warm', ob kalte Schl\u00e4ge,", "tokens": ["Ob", "warm'", ",", "ob", "kal\u00b7te", "Schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie pflegten gut zu sein. \u2013", "tokens": ["Sie", "pfleg\u00b7ten", "gut", "zu", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Friede war geschlossen,", "tokens": ["Der", "Frie\u00b7de", "war", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch Krieges Lust und Qual,", "tokens": ["Doch", "Krie\u00b7ges", "Lust", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die alten Schlachtgenossen", "tokens": ["Die", "al\u00b7ten", "Schlacht\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchlebten's noch einmal.", "tokens": ["Durch\u00b7leb\u00b7ten's", "noch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Marschall ", "tokens": ["Wie", "Mar\u00b7schall"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Es ward jetzt durchgeplaudert", "tokens": ["Es", "ward", "jetzt", "durch\u00b7ge\u00b7plau\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bei Tisch, in Sanssouci.", "tokens": ["Bei", "Tisch", ",", "in", "Sans\u00b7sou\u00b7ci", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Einst mocht' es ihm nicht schmecken,", "tokens": ["Einst", "mocht'", "es", "ihm", "nicht", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sieh, der Zieten schlief,", "tokens": ["Und", "sieh", ",", "der", "Zie\u00b7ten", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein H\u00f6fling wollt' ihn wecken,", "tokens": ["Ein", "H\u00f6f\u00b7ling", "wollt'", "ihn", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der K\u00f6nig aber rief:", "tokens": ["Der", "K\u00f6\u00b7nig", "a\u00b7ber", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbla\u00dft schlafen mir den Alten,", "tokens": ["\u00bb", "la\u00dft", "schla\u00b7fen", "mir", "den", "Al\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er hat in mancher Nacht", "tokens": ["Er", "hat", "in", "man\u00b7cher", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr uns sich wach gehalten,", "tokens": ["F\u00fcr", "uns", "sich", "wach", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der hat genug gewacht.\u00ab \u2013", "tokens": ["Der", "hat", "ge\u00b7nug", "ge\u00b7wacht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als die Zeit erf\u00fcllet", "tokens": ["Und", "als", "die", "Zeit", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des alten Helden war,", "tokens": ["Des", "al\u00b7ten", "Hel\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lag einst, schlicht eingeh\u00fcllet,", "tokens": ["Lag", "einst", ",", "schlicht", "ein\u00b7ge\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie selber er genommen", "tokens": ["Wie", "sel\u00b7ber", "er", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Feinde stets im Husch,", "tokens": ["Die", "Fein\u00b7de", "stets", "im", "Husch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So war der Tod gekommen", "tokens": ["So", "war", "der", "Tod", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie Zieten aus dem Busch.", "tokens": ["Wie", "Zie\u00b7ten", "aus", "dem", "Busch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}