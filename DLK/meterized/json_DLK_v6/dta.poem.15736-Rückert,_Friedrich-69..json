{"dta.poem.15736": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von zweien Welten will die wahre jede seyn,", "tokens": ["Von", "zwei\u00b7en", "Wel\u00b7ten", "will", "die", "wah\u00b7re", "je\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VMFIN", "ART", "ADJA", "PIAT", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wirft der andern vor, sie sei ein leerer Schein.", "tokens": ["Und", "wirft", "der", "an\u00b7dern", "vor", ",", "sie", "sei", "ein", "lee\u00b7rer", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du die Wirklichkeit als wirklich anerkennst,", "tokens": ["Wenn", "du", "die", "Wirk\u00b7lich\u00b7keit", "als", "wirk\u00b7lich", "an\u00b7er\u00b7kennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist das Ideal dagegen ein Gespenst.", "tokens": ["So", "ist", "das", "I\u00b7deal", "da\u00b7ge\u00b7gen", "ein", "Ge\u00b7spenst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch wenn mit ew'gem Stral dich trifft das Ideal,", "tokens": ["Doch", "wenn", "mit", "ew'\u00b7gem", "Stral", "dich", "trifft", "das", "I\u00b7deal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist das Verg\u00e4ngliche dagegen dumpf und kahl.", "tokens": ["Ist", "das", "Ver\u00b7g\u00e4ng\u00b7li\u00b7che", "da\u00b7ge\u00b7gen", "dumpf", "und", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PAV", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Nicht wenn das eine durch das andre du verneinst,", "tokens": ["Nicht", "wenn", "das", "ei\u00b7ne", "durch", "das", "and\u00b7re", "du", "ver\u00b7neinst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PDS", "ART", "APPR", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist begl\u00fcckt, wenn du die beiden sch\u00f6n vereinst;", "tokens": ["Du", "bist", "be\u00b7gl\u00fcckt", ",", "wenn", "du", "die", "bei\u00b7den", "sch\u00f6n", "ver\u00b7einst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ART", "PIAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn Geistiges f\u00fcr dich Gestalt und Leib annimmt,", "tokens": ["Wenn", "Geis\u00b7ti\u00b7ges", "f\u00fcr", "dich", "Ge\u00b7stalt", "und", "Leib", "an\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im Verg\u00e4nglichen der ew'ge Funke glimmt.", "tokens": ["Und", "im", "Ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "der", "ew'\u00b7ge", "Fun\u00b7ke", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}