{"textgrid.poem.53867": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Einigkeit und Recht und Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was die Freiheit ist bei den Germanen,", "tokens": ["Was", "die", "Frei\u00b7heit", "ist", "bei", "den", "Ger\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "die bleibt meistens schwer inkognito.", "tokens": ["die", "bleibt", "meis\u00b7tens", "schwer", "in\u00b7kog\u00b7ni\u00b7to", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Manche sind die ewigen Untertanen,", "tokens": ["Man\u00b7che", "sind", "die", "e\u00b7wi\u00b7gen", "Un\u00b7ter\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "m\u00f6chten gern und k\u00f6nnen blo\u00df nicht so.", "tokens": ["m\u00f6ch\u00b7ten", "gern", "und", "k\u00f6n\u00b7nen", "blo\u00df", "nicht", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "KON", "VMFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn schon hundert Jahr", "tokens": ["Denn", "schon", "hun\u00b7dert", "Jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "CARD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "trifft dich immerdar", "tokens": ["trifft", "dich", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "ein geduldiger Schafsblick durch die Brillen.", "tokens": ["ein", "ge\u00b7dul\u00b7di\u00b7ger", "Schafs\u00b7blick", "durch", "die", "Bril\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Doof ist doof.", "tokens": ["Doof", "ist", "doof", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Da helfen keine Pillen.", "tokens": ["Da", "hel\u00b7fen", "kei\u00b7ne", "Pil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was Justitia ist bei den Teutonen,", "tokens": ["Was", "Jus\u00b7ti\u00b7tia", "ist", "bei", "den", "Teu\u00b7to\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die hat eine Binde obenrum.", "tokens": ["die", "hat", "ei\u00b7ne", "Bin\u00b7de", "o\u00b7ben\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch sie tut die Binde gerne schonen,", "tokens": ["Doch", "sie", "tut", "die", "Bin\u00b7de", "ger\u00b7ne", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und da bindt sie sie nicht immer um.", "tokens": ["und", "da", "bindt", "sie", "sie", "nicht", "im\u00b7mer", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Unten winseln die", "tokens": ["Un\u00b7ten", "win\u00b7seln", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "wie das liebe Vieh.", "tokens": ["wie", "das", "lie\u00b7be", "Vieh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Manche glauben noch an guten Willen . . .", "tokens": ["Man\u00b7che", "glau\u00b7ben", "noch", "an", "gu\u00b7ten", "Wil\u00b7len", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doof ist doof.", "tokens": ["Doof", "ist", "doof", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Da helfen keine Pillen.", "tokens": ["Da", "hel\u00b7fen", "kei\u00b7ne", "Pil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was die Einigkeit ist bei den Hiesigen,", "tokens": ["Was", "die", "Ei\u00b7nig\u00b7keit", "ist", "bei", "den", "Hie\u00b7si\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "die ist vierundzwanzigfach verteilt.", "tokens": ["die", "ist", "vie\u00b7rund\u00b7zwan\u00b7zig\u00b7fach", "ver\u00b7teilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr die L\u00e4nder hat man einen riesigen", "tokens": ["F\u00fcr", "die", "L\u00e4n\u00b7der", "hat", "man", "ei\u00b7nen", "rie\u00b7si\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "ART", "ADJA"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Schreibeapparat gefeilt:", "tokens": ["Schrei\u00b7be\u00b7ap\u00b7pa\u00b7rat", "ge\u00b7feilt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hamburg schie\u00dft beinah", "tokens": ["Ham\u00b7burg", "schie\u00dft", "bei\u00b7nah"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "sich mit Altona;", "tokens": ["sich", "mit", "Al\u00b7to\u00b7na", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Bayern zeigt sich barsch,", "tokens": ["Bay\u00b7ern", "zeigt", "sich", "barsch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "ruft: \u00bbEs lebe die Republik!\u00ab", "tokens": ["ruft", ":", "\u00bb", "Es", "le\u00b7be", "die", "Re\u00b7pub\u00b7lik", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Jeder denkt nur gleich", "tokens": ["Je\u00b7der", "denkt", "nur", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "an sein privates Reich . . .", "tokens": ["an", "sein", "pri\u00b7va\u00b7tes", "Reich", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Eine Republike wider Willen.", "tokens": ["Ei\u00b7ne", "Re\u00b7pub\u00b7li\u00b7ke", "wi\u00b7der", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Deutsch ist deutsch.", "tokens": ["Deutsch", "ist", "deutsch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Da helfen keine Pillen.", "tokens": ["Da", "hel\u00b7fen", "kei\u00b7ne", "Pil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}