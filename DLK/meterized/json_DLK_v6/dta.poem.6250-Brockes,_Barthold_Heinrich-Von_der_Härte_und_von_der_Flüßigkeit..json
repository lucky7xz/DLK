{"dta.poem.6250": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn jemand ohngefehr bey dunckler Nacht spatziert,", "tokens": ["Wenn", "je\u00b7mand", "ohn\u00b7ge\u00b7fehr", "bey", "dunck\u00b7ler", "Nacht", "spat\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihrem schwartzen Flor umgeben;", "tokens": ["Von", "ih\u00b7rem", "schwart\u00b7zen", "Flor", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wovon wird er zuerst getroffen und ber\u00fchrt?", "tokens": ["Wo\u00b7von", "wird", "er", "zu\u00b7erst", "ge\u00b7trof\u00b7fen", "und", "be\u00b7r\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Er f\u00fchlt, wenn er darauf wird Achtung geben,", "tokens": ["Er", "f\u00fchlt", ",", "wenn", "er", "da\u00b7rauf", "wird", "Ach\u00b7tung", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PAV", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was c\u00f6rperlichs, das ihm entweder weicht,", "tokens": ["Was", "c\u00f6r\u00b7per\u00b7lichs", ",", "das", "ihm", "ent\u00b7we\u00b7der", "weicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo nicht, ihm widersteht.", "tokens": ["Wo", "nicht", ",", "ihm", "wi\u00b7der\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jm C\u00f6rper, welcher ihm nicht aus dem Wege geht,", "tokens": ["Jm", "C\u00f6r\u00b7per", ",", "wel\u00b7cher", "ihm", "nicht", "aus", "dem", "We\u00b7ge", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Wenn man nach ihm die H\u00e4nde strecket,", "tokens": ["Wenn", "man", "nach", "ihm", "die", "H\u00e4n\u00b7de", "stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wird Rauhigkeit und Festigkeit entdecket;", "tokens": ["Wird", "Rau\u00b7hig\u00b7keit", "und", "Fes\u00b7tig\u00b7keit", "ent\u00b7de\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der aber, welcher weicht, entdeckt uns allezeit", "tokens": ["Der", "a\u00b7ber", ",", "wel\u00b7cher", "weicht", ",", "ent\u00b7deckt", "uns", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Weichheit und die Fl\u00fc\u00dfigkeit.", "tokens": ["Die", "Weich\u00b7heit", "und", "die", "Fl\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Woher nun k\u00f6mmt es doch, da\u00df sie, wie wir versp\u00fcren,", "tokens": ["Wo\u00b7her", "nun", "k\u00f6mmt", "es", "doch", ",", "da\u00df", "sie", ",", "wie", "wir", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Uns so gar unterschiedlich r\u00fchren?", "tokens": ["Uns", "so", "gar", "un\u00b7ter\u00b7schied\u00b7lich", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Eigenschafften sind es eben,", "tokens": ["Die", "Ei\u00b7gen\u00b7schaff\u00b7ten", "sind", "es", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die wir anitzt zu suchen uns bestreben.", "tokens": ["Die", "wir", "a\u00b7nitzt", "zu", "su\u00b7chen", "uns", "be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}