{"textgrid.poem.54521": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mehr kalt denn Scytisch Ei\u00df ist mein erfr\u00f6rtes Hertz:", "tokens": ["Mehr", "kalt", "denn", "Scy\u00b7tisch", "Ei\u00df", "ist", "mein", "er\u00b7fr\u00f6r\u00b7tes", "Hertz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NE", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wei\u00df vom Leben nichts/ es starrt ohn alles Regen/", "tokens": ["Es", "wei\u00df", "vom", "Le\u00b7ben", "nichts", "/", "es", "starrt", "ohn", "al\u00b7les", "Re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PIS", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es kan das Blut nicht mehr durch Leib und Glied bewegen;", "tokens": ["Es", "kan", "das", "Blut", "nicht", "mehr", "durch", "Leib", "und", "Glied", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch steckt mich an ein feurig-fressend Schmertz.", "tokens": ["Und", "den\u00b7noch", "steckt", "mich", "an", "ein", "feu\u00b7rig\u00b7fres\u00b7send", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein wilder kalter Brand/ als wenn die Todten-Kertz", "tokens": ["Ein", "wil\u00b7der", "kal\u00b7ter", "Brand", "/", "als", "wenn", "die", "Tod\u00b7ten\u00b7Kertz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sich in die Leichen macht/ die Rom auffs Holtz hie\u00df legen;", "tokens": ["Sich", "in", "die", "Lei\u00b7chen", "macht", "/", "die", "Rom", "auffs", "Holtz", "hie\u00df", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$(", "ART", "NE", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als wenn die heisse Seuch' pflegt Gluth im Blut zu hegen/", "tokens": ["Als", "wenn", "die", "heis\u00b7se", "Seuch'", "pflegt", "Gluth", "im", "Blut", "zu", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "VVFIN", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch die das Fleisch verf\u00e4llt/ wie Schnee bey sp\u00e4tem Mertz.", "tokens": ["Durch", "die", "das", "Fleisch", "ver\u00b7f\u00e4llt", "/", "wie", "Schnee", "bey", "sp\u00e4\u00b7tem", "Mertz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVPP", "$(", "KOKOM", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es hitzt in mir/ es hitzt ein h\u00f6llisch-kaltes Brennen/", "tokens": ["Es", "hitzt", "in", "mir", "/", "es", "hitzt", "ein", "h\u00f6l\u00b7lischkal\u00b7tes", "Bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$(", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "O Flamme/ die wir Gott und lebend-machen nennen/", "tokens": ["O", "Flam\u00b7me", "/", "die", "wir", "Gott", "und", "le\u00b7ben\u00b7dma\u00b7chen", "nen\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PRELS", "PPER", "NN", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O komm und z\u00fcnd' in mir dein sch\u00fctternd Feuer an!", "tokens": ["O", "komm", "und", "z\u00fcnd'", "in", "mir", "dein", "sch\u00fct\u00b7ternd", "Feu\u00b7er", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verzehr/ O reine Gluth/ die Zunder meiner Flammen/", "tokens": ["Ver\u00b7zehr", "/", "O", "rei\u00b7ne", "Gluth", "/", "die", "Zun\u00b7der", "mei\u00b7ner", "Flam\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Fackeln/ die in mir/ mir leuchten zum Verdammen/", "tokens": ["Die", "Fa\u00b7ckeln", "/", "die", "in", "mir", "/", "mir", "leuch\u00b7ten", "zum", "Ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPER", "$(", "PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil vor dir Seuch und H\u00f6ll und Todt nicht stehen kan.", "tokens": ["Weil", "vor", "dir", "Seuch", "und", "H\u00f6ll", "und", "Todt", "nicht", "ste\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "NN", "KON", "NN", "KON", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}