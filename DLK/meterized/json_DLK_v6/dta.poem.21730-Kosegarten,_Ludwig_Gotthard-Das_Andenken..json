{"dta.poem.21730": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Andenken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich denk' an Dich, Geliebte,", "tokens": ["Ich", "denk'", "an", "Dich", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom fr\u00fchsten D\u00e4mmerstrahl,", "tokens": ["Vom", "fr\u00fchs\u00b7ten", "D\u00e4m\u00b7mer\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis Kron' und Leyer funkeln", "tokens": ["Bis", "Kron'", "und", "Le\u00b7yer", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am ew'gen Himmelssaal.", "tokens": ["Am", "ew'\u00b7gen", "Him\u00b7mels\u00b7saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im lichten Mittagsglanze,", "tokens": ["Im", "lich\u00b7ten", "Mit\u00b7tags\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Graun der Mitternacht,", "tokens": ["Im", "Graun", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stehst Du mir klar vor Augen", "tokens": ["Stehst", "Du", "mir", "klar", "vor", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In jedes Reizes Pracht.", "tokens": ["In", "je\u00b7des", "Rei\u00b7zes", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mir winkt das Lied des Dichters,", "tokens": ["Mir", "winkt", "das", "Lied", "des", "Dich\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich lockt des Denkers Buch.", "tokens": ["Mich", "lockt", "des", "Den\u00b7kers", "Buch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "S\u00fcss klingt des S\u00e4ngers Harfe,", "tokens": ["S\u00fcss", "klingt", "des", "S\u00e4n\u00b7gers", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ernst des Weisen Spruch.", "tokens": ["Und", "ernst", "des", "Wei\u00b7sen", "Spruch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Umsonst. Hinweggezogen", "tokens": ["Um\u00b7sonst", ".", "Hin\u00b7weg\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Folgt der entz\u00fcckte Geist", "tokens": ["Folgt", "der", "ent\u00b7z\u00fcck\u00b7te", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Dem Strom, der ihn magnetisch", "tokens": ["Dem", "Strom", ",", "der", "ihn", "mag\u00b7ne\u00b7tisch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In seine Wirbel reisst.", "tokens": ["In", "sei\u00b7ne", "Wir\u00b7bel", "reisst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn Nachts aus halbem Schlummer", "tokens": ["Wenn", "Nachts", "aus", "hal\u00b7bem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sehnsucht Sturm mich weckt,", "tokens": ["Der", "Sehn\u00b7sucht", "Sturm", "mich", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mich der Schlag der Wachtel", "tokens": ["Wenn", "mich", "der", "Schlag", "der", "Wach\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus s\u00fcssen Tr\u00e4umen neckt,", "tokens": ["Aus", "s\u00fcs\u00b7sen", "Tr\u00e4u\u00b7men", "neckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Breit' ich den Arm und dr\u00fccke", "tokens": ["Breit'", "ich", "den", "Arm", "und", "dr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich w\u00e4hnend an mein Herz;", "tokens": ["Dich", "w\u00e4h\u00b7nend", "an", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Wahn zerrinnt, und einsam", "tokens": ["Der", "Wahn", "zer\u00b7rinnt", ",", "und", "ein\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bin ich mit meinem Schmerz.", "tokens": ["Bin", "ich", "mit", "mei\u00b7nem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich fl\u00fcchte sehnsuchtm\u00fcde", "tokens": ["Ich", "fl\u00fcch\u00b7te", "sehn\u00b7sucht\u00b7m\u00fc\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Busen der Natur.", "tokens": ["Zum", "Bu\u00b7sen", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ach, Dein Bild, Geliebte,", "tokens": ["Doch", "ach", ",", "Dein", "Bild", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "XY", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Folgt mir auf jeder Spur.", "tokens": ["Folgt", "mir", "auf", "je\u00b7der", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es fl\u00f6tet Deinen Namen", "tokens": ["Es", "fl\u00f6\u00b7tet", "Dei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das V\u00f6glein auf dem Zweig.", "tokens": ["Das", "V\u00f6\u00b7glein", "auf", "dem", "Zweig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihn schwirrt die Grill' im Grase,", "tokens": ["Ihn", "schwirrt", "die", "Grill'", "im", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihn ruft die Unk' im Teich.", "tokens": ["Ihn", "ruft", "die", "Un\u00b7k'", "im", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "An Deines Auges Bl\u00e4ue", "tokens": ["An", "Dei\u00b7nes", "Au\u00b7ges", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mahnt mich des \u00c4thers Blau;", "tokens": ["Mahnt", "mich", "des", "\u00c4\u00b7thers", "Blau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An Deiner Locken F\u00fclle", "tokens": ["An", "Dei\u00b7ner", "Lo\u00b7cken", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Nebels str\u00f6mend Grau.", "tokens": ["Des", "Ne\u00b7bels", "str\u00f6\u00b7mend", "Grau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich mahnt an Deine Wangen", "tokens": ["Mich", "mahnt", "an", "Dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Rose keusche Gluth.", "tokens": ["Der", "Ro\u00b7se", "keu\u00b7sche", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich an den Wein der Lippen", "tokens": ["Mich", "an", "den", "Wein", "der", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Beere quellend Blut.", "tokens": ["Der", "Bee\u00b7re", "quel\u00b7lend", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Besch\u00e4mt dein Schwanenbusen", "tokens": ["Be\u00b7sch\u00e4mt", "dein", "Schwa\u00b7nen\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht der Narzisse Schnee?", "tokens": ["Nicht", "der", "Nar\u00b7zis\u00b7se", "Schnee", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Weicht nicht der Milch der Arme", "tokens": ["Weicht", "nicht", "der", "Milch", "der", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Milch der Lilie?", "tokens": ["Die", "Milch", "der", "Li\u00b7lie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Umhaucht mich nicht Dein Athem", "tokens": ["Um\u00b7haucht", "mich", "nicht", "Dein", "A\u00b7them"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Nachtviolenduft?", "tokens": ["Im", "Nacht\u00b7vi\u00b7o\u00b7len\u00b7duft", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ists nicht Dein s\u00fcsser Name,", "tokens": ["Ists", "nicht", "Dein", "s\u00fcs\u00b7ser", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den jedes Echo ruft? \u2014 \u2014", "tokens": ["Den", "je\u00b7des", "E\u00b7cho", "ruft", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohin, wohin mich retten", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "mich", "ret\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor der verborgnen Macht,", "tokens": ["Vor", "der", "ver\u00b7borg\u00b7nen", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die mich verfolgt vom Morgen", "tokens": ["Die", "mich", "ver\u00b7folgt", "vom", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis in das Graun der Nacht! \u2014 \u2014", "tokens": ["Bis", "in", "das", "Graun", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bey Dir, bey Dir nur, Traute,", "tokens": ["Bey", "Dir", ",", "bey", "Dir", "nur", ",", "Trau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist Rettung mir bewusst \u2014 \u2014", "tokens": ["Ist", "Ret\u00b7tung", "mir", "be\u00b7wusst"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVFIN", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach, nur in Deinen Armen,", "tokens": ["Ach", ",", "nur", "in", "Dei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ach, nur an Deiner Brust.", "tokens": ["Ach", ",", "nur", "an", "Dei\u00b7ner", "Brust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}