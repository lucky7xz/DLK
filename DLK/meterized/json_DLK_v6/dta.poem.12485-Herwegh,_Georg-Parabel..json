{"dta.poem.12485": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Parabel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erlaubt mir, da\u00df ich 'mal berichte", "tokens": ["Er\u00b7laubt", "mir", ",", "da\u00df", "ich", "'mal", "be\u00b7rich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Euch eine alberne Geschichte:", "tokens": ["Euch", "ei\u00b7ne", "al\u00b7ber\u00b7ne", "Ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kommt mir eben in den Sinn,", "tokens": ["Sie", "kommt", "mir", "e\u00b7ben", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geduld ist deutsch, drum nehmt sie hin.", "tokens": ["Ge\u00b7duld", "ist", "deutsch", ",", "drum", "nehmt", "sie", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "PAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "War eine brave, brave Frau,", "tokens": ["War", "ei\u00b7ne", "bra\u00b7ve", ",", "bra\u00b7ve", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nahm's im Dienste wohl genau,", "tokens": ["Die", "nahm's", "im", "Diens\u00b7te", "wohl", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und macht', so brav sie auch gewesen,", "tokens": ["Und", "macht'", ",", "so", "brav", "sie", "auch", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch niemals vieles Federlesen.", "tokens": ["Doch", "nie\u00b7mals", "vie\u00b7les", "Fe\u00b7der\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Frau hatt' einen muntern Hahn,", "tokens": ["Die", "Frau", "hatt'", "ei\u00b7nen", "mun\u00b7tern", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kr\u00e4ht' ihr stets den Morgen an,", "tokens": ["Der", "kr\u00e4ht'", "ihr", "stets", "den", "Mor\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und war, nach seiner Hahn-Natur,", "tokens": ["Und", "war", ",", "nach", "sei\u00b7ner", "Hahn\u00b7Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr sie die allerbeste Uhr.", "tokens": ["F\u00fcr", "sie", "die", "al\u00b7ler\u00b7bes\u00b7te", "Uhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sobald den Tag er angesagt,", "tokens": ["So\u00b7bald", "den", "Tag", "er", "an\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da weckt' die Frau die faule Magd,", "tokens": ["Da", "weckt'", "die", "Frau", "die", "fau\u00b7le", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was unsre Magd gar schwer verdro\u00df,", "tokens": ["Was", "uns\u00b7re", "Magd", "gar", "schwer", "ver\u00b7dro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie im Grimme einst beschlo\u00df,", "tokens": ["Da\u00df", "sie", "im", "Grim\u00b7me", "einst", "be\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem Vogel zu stutzen seine Schwingen,", "tokens": ["Dem", "Vo\u00b7gel", "zu", "stut\u00b7zen", "sei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und, meld' ich's kurz, ihn umzubringen.", "tokens": ["Und", ",", "meld'", "ich's", "kurz", ",", "ihn", "um\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PIS", "ADJD", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es war gedacht, es war gethan,", "tokens": ["Es", "war", "ge\u00b7dacht", ",", "es", "war", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die G\u00f6tter bekamen einen Hahn.", "tokens": ["Die", "G\u00f6t\u00b7ter", "be\u00b7ka\u00b7men", "ei\u00b7nen", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Was aber hat die Magd gewonnen?", "tokens": ["Was", "a\u00b7ber", "hat", "die", "Magd", "ge\u00b7won\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sonst geweckt ward mit der Sonnen,", "tokens": ["Die", "sonst", "ge\u00b7weckt", "ward", "mit", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ward nun geweckt um Mitternacht,", "tokens": ["Ward", "nun", "ge\u00b7weckt", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem den Hahn sie umgebracht.", "tokens": ["Nach\u00b7dem", "den", "Hahn", "sie", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, sprach die Magd, die schwer Beth\u00f6rte,", "tokens": ["Ach", ",", "sprach", "die", "Magd", ",", "die", "schwer", "Be\u00b7th\u00f6r\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich den Hahn doch kr\u00e4hen h\u00f6rte!", "tokens": ["Wenn", "ich", "den", "Hahn", "doch", "kr\u00e4\u00b7hen", "h\u00f6r\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Kr\u00e4hen hat so sch\u00f6n geklungen,", "tokens": ["Sein", "Kr\u00e4\u00b7hen", "hat", "so", "sch\u00f6n", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00e4tt' eine Nachtigall gesungen.", "tokens": ["Als", "h\u00e4tt'", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "\u201eund nun der Witz? wir bitten dich!\u201c", "tokens": ["\u201e", "und", "nun", "der", "Witz", "?", "wir", "bit\u00b7ten", "dich", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr kennt die Frau so gut, wie ich;", "tokens": ["Ihr", "kennt", "die", "Frau", "so", "gut", ",", "wie", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist die sch\u00f6nste weit und breit,", "tokens": ["Sie", "ist", "die", "sch\u00f6ns\u00b7te", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Anblick die volle Seligkeit.", "tokens": ["Ihr", "An\u00b7blick", "die", "vol\u00b7le", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Ihr kennt wohl auch des Nachbars Hahn,", "tokens": ["Ihr", "kennt", "wohl", "auch", "des", "Nach\u00b7bars", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ihr so viel zu Leid gethan;", "tokens": ["Dem", "ihr", "so", "viel", "zu", "Leid", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ihr mich nach dem dritten fragt:", "tokens": ["Und", "wenn", "ihr", "mich", "nach", "dem", "drit\u00b7ten", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du, deutsches Volk, du bist die Magd!", "tokens": ["Du", ",", "deut\u00b7sches", "Volk", ",", "du", "bist", "die", "Magd", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch wenn ihr den Hahn auch mordet, ihr Sklaven,", "tokens": ["Doch", "wenn", "ihr", "den", "Hahn", "auch", "mor\u00b7det", ",", "ihr", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So denkt darum nicht l\u00e4nger zu schlafen!", "tokens": ["So", "denkt", "da\u00b7rum", "nicht", "l\u00e4n\u00b7ger", "zu", "schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erst weckt' euch die Frau nach dem Hahnenschrei,", "tokens": ["Erst", "weckt'", "euch", "die", "Frau", "nach", "dem", "Hah\u00b7nen\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nun ist's mit dem Schlummer auf ewig vorbei.", "tokens": ["Nun", "ist's", "mit", "dem", "Schlum\u00b7mer", "auf", "e\u00b7wig", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "APPR", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Die Freiheit kommt wie ein Dieb in der Nacht", "tokens": ["Die", "Frei\u00b7heit", "kommt", "wie", "ein", "Dieb", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ruft euch zu: Erwacht! erwacht!", "tokens": ["Und", "ruft", "euch", "zu", ":", "Er\u00b7wacht", "!", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}