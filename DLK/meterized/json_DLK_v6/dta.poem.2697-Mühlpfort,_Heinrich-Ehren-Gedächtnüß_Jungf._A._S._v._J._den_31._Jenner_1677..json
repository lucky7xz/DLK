{"dta.poem.2697": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtn\u00fc\u00df/  \n  Jungf. A. S. v. J. den 31. Jenner  1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hochedle/ da\u00df ihr itzt mit anverwantem Klagen/", "tokens": ["Ho\u00b7ched\u00b7le", "/", "da\u00df", "ihr", "itzt", "mit", "an\u00b7ver\u00b7wan\u00b7tem", "Kla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den letzten Ehren-Dienst und Liebes-Pflicht legt ab", "tokens": ["Den", "letz\u00b7ten", "Eh\u00b7ren\u00b7Dienst", "und", "Lie\u00b7bes\u00b7Pflicht", "legt", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und schickt die kalte Leich\u2019/ ins Haus der Ruh/ das Grab/", "tokens": ["Und", "schickt", "die", "kal\u00b7te", "Leich'", "/", "ins", "Haus", "der", "Ruh", "/", "das", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "APPRART", "NN", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verdient nicht schlechtes Lob. Es heischt es das Gesetze/", "tokens": ["Ver\u00b7di\u00b7ent", "nicht", "schlech\u00b7tes", "Lob", ".", "Es", "heischt", "es", "das", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So GOtt und die Natur uns in die Brust gelegt/", "tokens": ["So", "Gott", "und", "die", "Na\u00b7tur", "uns", "in", "die", "Brust", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df man der ", "tokens": ["Da\u00df", "man", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Und da\u00df in Seel und Hertz man ihr Ged\u00e4chtn\u00fc\u00df tr\u00e4gt.", "tokens": ["Und", "da\u00df", "in", "Seel", "und", "Hertz", "man", "ihr", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es ist ein toller Wahn von den verdammten Heyden/", "tokens": ["Es", "ist", "ein", "tol\u00b7ler", "Wahn", "von", "den", "ver\u00b7damm\u00b7ten", "Hey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die die Beerdigung am wenigsten geacht/", "tokens": ["Die", "die", "Be\u00b7er\u00b7di\u00b7gung", "am", "we\u00b7nigs\u00b7ten", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "PIS", "VVPP", "$("], "meter": "+-+----+---+", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Und nichts darnach gefragt/ ob brennen/ reissen/ schneiden", "tokens": ["Und", "nichts", "dar\u00b7nach", "ge\u00b7fragt", "/", "ob", "bren\u00b7nen", "/", "reis\u00b7sen", "/", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PIS", "PAV", "VVPP", "$(", "KOUS", "VVINF", "$(", "VVINF", "$(", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat au\u00df dem todten Leib ein lauter nichts gemacht.", "tokens": ["Hat", "au\u00df", "dem", "tod\u00b7ten", "Leib", "ein", "lau\u00b7ter", "nichts", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "PIAT", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nein/ Gottes Eben-Bild und seiner Hand Gesch\u00f6pffe", "tokens": ["Nein", "/", "Got\u00b7tes", "E\u00b7ben\u00b7Bild", "und", "sei\u00b7ner", "Hand", "Ge\u00b7sch\u00f6pf\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "NN", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mu\u00df nicht wie schlechter Staub seyn in die Lufft gestreut/", "tokens": ["Mu\u00df", "nicht", "wie", "schlech\u00b7ter", "Staub", "seyn", "in", "die", "Lufft", "ge\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "KOKOM", "ADJA", "NN", "VAINF", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hat das noch blinde Rom geehrt sein\u2019 Aschen-T\u00f6pffe", "tokens": ["Hat", "das", "noch", "blin\u00b7de", "Rom", "ge\u00b7ehrt", "sein'", "A\u00b7schen\u00b7T\u00f6pf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ADV", "ADJA", "NE", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und gar das Grab genennt ein Haus der Ewigkeit:", "tokens": ["Und", "gar", "das", "Grab", "ge\u00b7nennt", "ein", "Haus", "der", "E\u00b7wig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie vielmehr sollen wir die abgelebten Glieder/", "tokens": ["Wie", "viel\u00b7mehr", "sol\u00b7len", "wir", "die", "ab\u00b7ge\u00b7leb\u00b7ten", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der allgemeinen Scho\u00df der Erden anvertraun.", "tokens": ["Der", "all\u00b7ge\u00b7mei\u00b7nen", "Scho\u00df", "der", "Er\u00b7den", "an\u00b7ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gesichert durch den Trost/ der H\u00f6chste giebt sie wieder/", "tokens": ["Ge\u00b7si\u00b7chert", "durch", "den", "Trost", "/", "der", "H\u00f6chs\u00b7te", "giebt", "sie", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die H\u00fctte/ die er bricht/ die kan er neu auffbann.", "tokens": ["Die", "H\u00fct\u00b7te", "/", "die", "er", "bricht", "/", "die", "kan", "er", "neu", "auff\u00b7bann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "PDS", "VMFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn sind die Gr\u00e4ber auch ein Denckmahl unsrer Ehren/", "tokens": ["Denn", "sind", "die", "Gr\u00e4\u00b7ber", "auch", "ein", "Denck\u00b7mahl", "uns\u00b7rer", "Eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wobey ein ewig Ruhm mit hellen Augen wacht/", "tokens": ["Wo\u00b7bey", "ein", "e\u00b7wig", "Ruhm", "mit", "hel\u00b7len", "Au\u00b7gen", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wo offt die Tugend pflegt den Wanders-Mann zu lehren/", "tokens": ["Wo", "offt", "die", "Tu\u00b7gend", "pflegt", "den", "Wan\u00b7der\u00b7sMann", "zu", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df hoher Thaten Prei\u00df nicht fault ins Grabes Nacht.", "tokens": ["Da\u00df", "ho\u00b7her", "Tha\u00b7ten", "Prei\u00df", "nicht", "fault", "ins", "Gra\u00b7bes", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "PTKNEG", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und weiter sind sie auch ein kennbahr Liebes-Zeichen/", "tokens": ["Und", "wei\u00b7ter", "sind", "sie", "auch", "ein", "kenn\u00b7bahr", "Lie\u00b7bes\u00b7Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So Angesipptes Blut einander setzen sol/", "tokens": ["So", "An\u00b7ge\u00b7sipp\u00b7tes", "Blut", "ein\u00b7an\u00b7der", "set\u00b7zen", "sol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer wei\u00df nicht wie so hoch Egyptens S\u00e4ulen reichen", "tokens": ["Wer", "wei\u00df", "nicht", "wie", "so", "hoch", "E\u00b7gyp\u00b7tens", "S\u00e4u\u00b7len", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "KOKOM", "ADV", "ADJD", "NE", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und was vor Schrifften trug das g\u00f6ldne Capitol/", "tokens": ["Und", "was", "vor", "Schriff\u00b7ten", "trug", "das", "g\u00f6ld\u00b7ne", "Ca\u00b7pi\u00b7tol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir Ertz und Marmel noch die reine Treu benennen", "tokens": ["Wir", "Ertz", "und", "Mar\u00b7mel", "noch", "die", "rei\u00b7ne", "Treu", "be\u00b7nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "NN", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Uhrhebern ihrer Zier beharrlich danckbar seyn/", "tokens": ["Uhr\u00b7he\u00b7bern", "ih\u00b7rer", "Zier", "be\u00b7harr\u00b7lich", "dan\u00b7ck\u00b7bar", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "ADJD", "VAINF", "$("], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Wie man die Liebe sieht noch au\u00df der Asche brennen", "tokens": ["Wie", "man", "die", "Lie\u00b7be", "sieht", "noch", "au\u00df", "der", "A\u00b7sche", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und wo sonst nichts mehr redt/ so redt der Leichenstein.", "tokens": ["Und", "wo", "sonst", "nichts", "mehr", "redt", "/", "so", "redt", "der", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PIS", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hoch-edle/ wenn ihr nun mit Adlichem Gepr\u00e4nge ", "tokens": ["Hoch\u00b7ed\u00b7le", "/", "wenn", "ihr", "nun", "mit", "Ad\u00b7li\u00b7chem", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "(ster ehrt", "tokens": ["(", "ster", "ehrt"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ADJD", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.35": {"text": "Nach Stand und nach ", "tokens": ["Nach", "Stand", "und", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "So ists der Ahnen Ruhm und ihrer Tugend M\u00e4nge", "tokens": ["So", "ists", "der", "Ah\u00b7nen", "Ruhm", "und", "ih\u00b7rer", "Tu\u00b7gend", "M\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die euch/ den werthen Sarch zu schm\u00fccken hat gelehrt.", "tokens": ["Die", "euch", "/", "den", "wert\u00b7hen", "Sarch", "zu", "schm\u00fc\u00b7cken", "hat", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Jhr keuscher Jungfern-Krantz sucht di\u00df zum Eigenthume/", "tokens": ["Ihr", "keu\u00b7scher", "Jung\u00b7fern\u00b7Krantz", "sucht", "di\u00df", "zum", "Ei\u00b7gen\u00b7thu\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PDS", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wozu die Gottes-Furcht die weisse Seiden schenckt/", "tokens": ["Wo\u00b7zu", "die", "Got\u00b7tes\u00b7Furcht", "die", "weis\u00b7se", "Sei\u00b7den", "schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Engel heissen sie itzt auserwehlte Muhme/", "tokens": ["Die", "En\u00b7gel", "heis\u00b7sen", "sie", "itzt", "au\u00b7ser\u00b7wehl\u00b7te", "Muh\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die an die Lust der Welt ihr Hertze nie gelenckt.", "tokens": ["Die", "an", "die", "Lust", "der", "Welt", "ihr", "Hert\u00b7ze", "nie", "ge\u00b7lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Was ist die Keuschheit selbst? Ein Zweig vom Paradiese/", "tokens": ["Was", "ist", "die", "Keuschheit", "selbst", "?", "Ein", "Zweig", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$.", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Ein Kleinod das den Leib doch mehr die Seele ziert.", "tokens": ["Ein", "Klei\u00b7nod", "das", "den", "Leib", "doch", "mehr", "die", "See\u00b7le", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.