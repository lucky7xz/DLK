{"dta.poem.21712": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Jahrhundert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey o Jungfrau gegr&#252;sst, du j&#252;ngste Tochter des", "tokens": ["Sey", "o", "Jung\u00b7frau", "ge\u00b7gr", "&#252;", "sst", ",", "du", "j", "&#252;", "ngs\u00b7te", "Toch\u00b7ter", "des"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["VAFIN", "FM", "FM", "FM", "FM", "FM", "$,", "PPER", "NE", "$(", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Chronos!", "tokens": ["Chro\u00b7nos", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Freudigen Muthes betritt, Heldinn, die st\u00e4u-", "tokens": ["Freu\u00b7di\u00b7gen", "Mu\u00b7thes", "be\u00b7tritt", ",", "Hel\u00b7dinn", ",", "die", "st\u00e4u"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "NN", "$,", "ART", "TRUNC"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Manche zwar sind schon gewonnen der Preise. Den", "tokens": ["Man\u00b7che", "zwar", "sind", "schon", "ge\u00b7won\u00b7nen", "der", "Prei\u00b7se", ".", "Den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "ADV", "VAFIN", "ADV", "VVPP", "ART", "NN", "$.", "ART"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Apfel der ", "tokens": ["Ap\u00b7fel", "der"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Raubte dir Phidias Zeit, Luthers die Keule der", "tokens": ["Raub\u00b7te", "dir", "Phi\u00b7di\u00b7as", "Zeit", ",", "Lu\u00b7thers", "die", "Keu\u00b7le", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "NN", "$,", "NE", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Kraft;", "tokens": ["Kraft", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Aber noch schimmert in d\u00e4mmernder Ferne der", "tokens": ["A\u00b7ber", "noch", "schim\u00b7mert", "in", "d\u00e4m\u00b7mern\u00b7der", "Fer\u00b7ne", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "ART"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Kranz der ", "tokens": ["Kranz", "der"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Selige, schimmert er einst dir in dem wehenden", "tokens": ["Se\u00b7li\u00b7ge", ",", "schim\u00b7mert", "er", "einst", "dir", "in", "dem", "we\u00b7hen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "+--+--+++-+--", "measure": "dactylic.di.plus"}}}}}