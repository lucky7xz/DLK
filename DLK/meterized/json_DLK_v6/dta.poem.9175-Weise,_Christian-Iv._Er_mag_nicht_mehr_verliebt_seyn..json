{"dta.poem.9175": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n Er mag nicht mehr verliebt seyn.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Nun bin ich wieder frey/", "tokens": ["Nun", "bin", "ich", "wie\u00b7der", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nun darff ich an die pein/", "tokens": ["Nun", "darff", "ich", "an", "die", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an die liebes-treu/", "tokens": ["Und", "an", "die", "lie\u00b7bes\u00b7treu", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mehr gebunden seyn:", "tokens": ["Nicht", "mehr", "ge\u00b7bun\u00b7den", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein ander mag sich sehnen/", "tokens": ["Ein", "an\u00b7der", "mag", "sich", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie ich bi\u00dfher gethan/", "tokens": ["Wie", "ich", "bi\u00df\u00b7her", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich blicke meine sch\u00f6nen/", "tokens": ["Ich", "bli\u00b7cke", "mei\u00b7ne", "sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit kalten augen an.", "tokens": ["Mit", "kal\u00b7ten", "au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Nun bin ich wieder frey/", "tokens": ["Nun", "bin", "ich", "wie\u00b7der", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und spotte der gefahr/", "tokens": ["Und", "spot\u00b7te", "der", "ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der strick ist schon entzwey/", "tokens": ["Der", "strick", "ist", "schon", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der mein gef\u00e4ngni\u00df war/", "tokens": ["Der", "mein", "ge\u00b7f\u00e4ng\u00b7ni\u00df", "war", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die sch\u00f6nsten lust-gedancken/", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "lust\u00b7ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vergn\u00fcgen meinen sinn/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "mei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dieweil ich aus den schrancken/", "tokens": ["Die\u00b7weil", "ich", "aus", "den", "schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der knechtschafft kommen bin.", "tokens": ["Der", "knecht\u00b7schafft", "kom\u00b7men", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Nun bin ich wieder frey!", "tokens": ["Nun", "bin", "ich", "wie\u00b7der", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor lie\u00df das eitele thun/", "tokens": ["Vor", "lie\u00df", "das", "ei\u00b7te\u00b7le", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der blinden fantasey", "tokens": ["Der", "blin\u00b7den", "fan\u00b7ta\u00b7sey"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich wenig stunden ruh\u2019n/", "tokens": ["Mich", "we\u00b7nig", "stun\u00b7den", "ruh'n", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da stell ich mir der wangen/", "tokens": ["Da", "stell", "ich", "mir", "der", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und da der augen-zier/", "tokens": ["Und", "da", "der", "au\u00b7gen\u00b7zier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit sehnlichem verlangen/", "tokens": ["Mit", "sehn\u00b7li\u00b7chem", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In einem traume f\u00fcr.", "tokens": ["In", "ei\u00b7nem", "trau\u00b7me", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Nun bin ich wieder frey/", "tokens": ["Nun", "bin", "ich", "wie\u00b7der", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und achte dieses spiel/", "tokens": ["Und", "ach\u00b7te", "die\u00b7ses", "spiel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der blossen l\u00f6ffeley/", "tokens": ["Der", "blos\u00b7sen", "l\u00f6f\u00b7fe\u00b7ley", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als einen biernen-stiel.", "tokens": ["Als", "ei\u00b7nen", "bier\u00b7nen\u00b7stiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ade ihr sch\u00f6nen kinder/", "tokens": ["A\u00b7de", "ihr", "sch\u00f6\u00b7nen", "kin\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Gedenckt an euren freund/", "tokens": ["Ge\u00b7denckt", "an", "eu\u00b7ren", "freund", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich scheide nun geschwinder/", "tokens": ["Ich", "schei\u00b7de", "nun", "ge\u00b7schwin\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als ich es selbst gemeint.", "tokens": ["Als", "ich", "es", "selbst", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Nun bin ich wieder frey.", "tokens": ["Nun", "bin", "ich", "wie\u00b7der", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das liebes-feuer streicht", "tokens": ["Das", "lie\u00b7bes\u00b7feu\u00b7er", "streicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey mir umsonst vorbey:", "tokens": ["Bey", "mir", "um\u00b7sonst", "vor\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Drum weicht/ ihr m\u00e4dgen weicht/", "tokens": ["Drum", "weicht", "/", "ihr", "m\u00e4d\u00b7gen", "weicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sucht andre m\u00e4nner-sinnen/", "tokens": ["Sucht", "and\u00b7re", "m\u00e4n\u00b7ner\u00b7sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich sag es ohne scheu/", "tokens": ["Ich", "sag", "es", "oh\u00b7ne", "scheu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich k\u00f6nnt ihr nicht gewinnen/", "tokens": ["Mich", "k\u00f6nnt", "ihr", "nicht", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denn ich bin wieder frey.", "tokens": ["Denn", "ich", "bin", "wie\u00b7der", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}