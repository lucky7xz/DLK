{"textgrid.poem.33675": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesundes Leben kann erkranken \u00fcber Nacht,", "tokens": ["Ge\u00b7sun\u00b7des", "Le\u00b7ben", "kann", "er\u00b7kran\u00b7ken", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Fels, auf den wir bau'n, kann wanken \u00fcber Nacht.", "tokens": ["Ein", "Fels", ",", "auf", "den", "wir", "bau'n", ",", "kann", "wan\u00b7ken", "\u00fc\u00b7ber", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es kommen, wenn wir fest entschlossen scheinen,", "tokens": ["Es", "kom\u00b7men", ",", "wenn", "wir", "fest", "ent\u00b7schlos\u00b7sen", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns oft ganz andere Gedanken \u00fcber Nacht.", "tokens": ["Uns", "oft", "ganz", "an\u00b7de\u00b7re", "Ge\u00b7dan\u00b7ken", "\u00fc\u00b7ber", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer heute lacht, kann morgen m\u00fcssen weinen,", "tokens": ["Wer", "heu\u00b7te", "lacht", ",", "kann", "mor\u00b7gen", "m\u00fcs\u00b7sen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die gr\u00f6\u00dften Reiche schon versanken \u00fcber Nacht,", "tokens": ["Die", "gr\u00f6\u00df\u00b7ten", "Rei\u00b7che", "schon", "ver\u00b7san\u00b7ken", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nichts \u00fcbrig lassend als ein Mal in Steinen.", "tokens": ["Nichts", "\u00fcb\u00b7rig", "las\u00b7send", "als", "ein", "Mal", "in", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}