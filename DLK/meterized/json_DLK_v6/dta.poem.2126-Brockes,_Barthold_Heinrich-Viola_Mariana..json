{"dta.poem.2126": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Viola Mariana.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich die schon welke Staude der blaulichten", "tokens": ["Nach\u00b7dem", "ich", "die", "schon", "wel\u00b7ke", "Stau\u00b7de", "der", "blau\u00b7lich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADV", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Campanula,", "tokens": ["Cam\u00b7pa\u00b7nu\u00b7la", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.3": {"text": "(wof\u00fcr ich sie zu anfangs hielte; da doch der Blume rech-", "tokens": ["(", "wo\u00b7f\u00fcr", "ich", "sie", "zu", "an\u00b7fangs", "hiel\u00b7te", ";", "da", "doch", "der", "Blu\u00b7me", "rech"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PPER", "PPER", "APPR", "ADV", "VVFIN", "$.", "ADV", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "ter Name", "tokens": ["ter", "Na\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "---", "measure": "unknown.measure.zero"}, "line.5": {"text": "Viola Mariana hei\u00dft, den sie vor Alters \u00fcberkame)", "tokens": ["Vi\u00b7o\u00b7la", "Ma\u00b7ri\u00b7a\u00b7na", "hei\u00dft", ",", "den", "sie", "vor", "Al\u00b7ters", "\u00fc\u00b7ber\u00b7ka\u00b7me", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Mit einiger Aufmerksamkeit in meinem Garten, j\u00fcngst", "tokens": ["Mit", "ei\u00b7ni\u00b7ger", "Auf\u00b7merk\u00b7sam\u00b7keit", "in", "mei\u00b7nem", "Gar\u00b7ten", ",", "j\u00fcngst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "besah,", "tokens": ["be\u00b7sah", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Die eben, durch ihr schmuzigs Ansehn, und falbes Braun-", "tokens": ["Die", "e\u00b7ben", ",", "durch", "ihr", "schmu\u00b7zigs", "An\u00b7sehn", ",", "und", "fal\u00b7bes", "Braun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "roth, meine Blicke", "tokens": ["roth", ",", "mei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Von andern sch\u00f6nen gr\u00fcnen Pflanzen, wovon sie abstach,", "tokens": ["Von", "an\u00b7dern", "sch\u00f6\u00b7nen", "gr\u00fc\u00b7nen", "Pflan\u00b7zen", ",", "wo\u00b7von", "sie", "ab\u00b7stach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "weg, zur\u00fccke", "tokens": ["weg", ",", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "punct", "word"], "pos": ["PTKVZ", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Und mit Gewalt fast auf sich zog,", "tokens": ["Und", "mit", "Ge\u00b7walt", "fast", "auf", "sich", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und ich, um sie recht zu betrachten, mich schnell nach ihr", "tokens": ["Und", "ich", ",", "um", "sie", "recht", "zu", "be\u00b7trach\u00b7ten", ",", "mich", "schnell", "nach", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$,", "PPER", "ADJD", "APPR", "PPOSAT"], "meter": "-+--+--+--+--", "measure": "amphibrach.tetra.plus"}, "line.14": {"text": "herunter bog,", "tokens": ["her\u00b7un\u00b7ter", "bog", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Erschrack ich fast, wie an derselben ich von dem Finger", "tokens": ["Er\u00b7schrack", "ich", "fast", ",", "wie", "an", "der\u00b7sel\u00b7ben", "ich", "von", "dem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PWAV", "APPR", "PDAT", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "der Natur", "tokens": ["der", "Na\u00b7tur"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.17": {"text": "Ein\u2019 au\u00dferordentlich formirt\u2019 und nett gebogene Figur,", "tokens": ["Ein'", "au\u00b7\u00dfer\u00b7or\u00b7dent\u00b7lich", "for\u00b7mirt'", "und", "nett", "ge\u00b7bo\u00b7ge\u00b7ne", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.18": {"text": "Die fremd und seltsam, und dennoch von zierlicher Erfin-", "tokens": ["Die", "fremd", "und", "selt\u00b7sam", ",", "und", "den\u00b7noch", "von", "zier\u00b7li\u00b7cher", "Er\u00b7fin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "KON", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "dung, sah:", "tokens": ["dung", ",", "sah", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Die, sonder Farbe, doch gef\u00e4llig. Denn ihre vormals", "tokens": ["Die", ",", "son\u00b7der", "Far\u00b7be", ",", "doch", "ge\u00b7f\u00e4l\u00b7lig", ".", "Denn", "ih\u00b7re", "vor\u00b7mals"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KON", "NN", "$,", "ADV", "ADJD", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "sch\u00f6ne Bl\u00fcte,", "tokens": ["sch\u00f6\u00b7ne", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Die j\u00fcngst in purpurf\u00e4rbigem und gleichsam blauem Feuer", "tokens": ["Die", "j\u00fcngst", "in", "pur\u00b7pur\u00b7f\u00e4r\u00b7bi\u00b7gem", "und", "gleich\u00b7sam", "blau\u00b7em", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}, "line.23": {"text": "gl\u00fchte,", "tokens": ["gl\u00fch\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "War abgefallen, welk, verschrumpft und ihre Sch\u00f6nheit", "tokens": ["War", "ab\u00b7ge\u00b7fal\u00b7len", ",", "welk", ",", "ver\u00b7schrumpft", "und", "ih\u00b7re", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "ADJD", "$,", "VVPP", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "nicht mehr da.", "tokens": ["nicht", "mehr", "da", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Allein die Bildung ihres Fu\u00dfes war so verwunderlich", "tokens": ["Al\u00b7lein", "die", "Bil\u00b7dung", "ih\u00b7res", "Fu\u00b7\u00dfes", "war", "so", "ver\u00b7wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD"], "meter": "---+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "formiret,", "tokens": ["for\u00b7mi\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.28": {"text": "Da\u00df sie fast gar nicht zu beschreiben. Von oben formt ein", "tokens": ["Da\u00df", "sie", "fast", "gar", "nicht", "zu", "be\u00b7schrei\u00b7ben", ".", "Von", "o\u00b7ben", "formt", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "$.", "APPR", "ADV", "VVFIN", "ART"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "f\u00fcnffach Blatt", "tokens": ["f\u00fcnf\u00b7fach", "Blatt"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}}}}}