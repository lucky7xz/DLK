{"textgrid.poem.44358": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "St\u00e4ndchen", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brim blim, klang kling,", "tokens": ["Brim", "blim", ",", "klang", "kling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "H\u00f6re, M\u00e4dchen, was ich sing!", "tokens": ["H\u00f6\u00b7re", ",", "M\u00e4d\u00b7chen", ",", "was", "ich", "sing", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh mich hier vor deinem Fenster", "tokens": ["Sieh", "mich", "hier", "vor", "dei\u00b7nem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauschend mit der Zither stehn,", "tokens": ["Lau\u00b7schend", "mit", "der", "Zi\u00b7ther", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Stunde, wo Gespenster", "tokens": ["In", "der", "Stun\u00b7de", ",", "wo", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur und Liebende noch gehn.", "tokens": ["Nur", "und", "Lie\u00b7ben\u00b7de", "noch", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles ruht im trauten Zimmer,", "tokens": ["Al\u00b7les", "ruht", "im", "trau\u00b7ten", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur die Liebe ruhet nimmer.", "tokens": ["Nur", "die", "Lie\u00b7be", "ru\u00b7het", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Brim blim, klang kling,", "tokens": ["Brim", "blim", ",", "klang", "kling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was ist die Liebe f\u00fcr ein Ding!", "tokens": ["Was", "ist", "die", "Lie\u00b7be", "f\u00fcr", "ein", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "St\u00fcrme brausen durch die Gassen,", "tokens": ["St\u00fcr\u00b7me", "brau\u00b7sen", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief verh\u00fcllt in Schnee und Eis,", "tokens": ["Tief", "ver\u00b7h\u00fcllt", "in", "Schnee", "und", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, und doch, kaum kann ichs fassen,", "tokens": ["Ach", ",", "und", "doch", ",", "kaum", "kann", "ichs", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kalt die Hand, der Busen hei\u00df.", "tokens": ["Kalt", "die", "Hand", ",", "der", "Bu\u00b7sen", "hei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Innre Gluten, w\u00e4rmt die Finger,", "tokens": ["Inn\u00b7re", "Glu\u00b7ten", ",", "w\u00e4rmt", "die", "Fin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fchl, o Eis, den Minnesinger.", "tokens": ["K\u00fchl", ",", "o", "Eis", ",", "den", "Min\u00b7ne\u00b7sin\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Brim blim, klang kling,", "tokens": ["Brim", "blim", ",", "klang", "kling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was ist die Liebe f\u00fcr ein Ding!", "tokens": ["Was", "ist", "die", "Lie\u00b7be", "f\u00fcr", "ein", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mutig, wenn ich dich nicht sehe,", "tokens": ["Mu\u00b7tig", ",", "wenn", "ich", "dich", "nicht", "se\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinn ich aus manch Liebeswort,", "tokens": ["Sinn", "ich", "aus", "manch", "Lie\u00b7bes\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber kaum in deiner N\u00e4he,", "tokens": ["A\u00b7ber", "kaum", "in", "dei\u00b7ner", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die Sprache eilends fort.", "tokens": ["Ist", "die", "Spra\u00b7che", "ei\u00b7lends", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ferne mutig, nahe bl\u00f6de?", "tokens": ["Fer\u00b7ne", "mu\u00b7tig", ",", "na\u00b7he", "bl\u00f6\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kannst du denken, Lieb, so rede!", "tokens": ["Kannst", "du", "den\u00b7ken", ",", "Lieb", ",", "so", "re\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Brim blim, klang kling,", "tokens": ["Brim", "blim", ",", "klang", "kling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was ist die Liebe f\u00fcr ein Ding!", "tokens": ["Was", "ist", "die", "Lie\u00b7be", "f\u00fcr", "ein", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nur, ergreif ich meine Zither,", "tokens": ["Nur", ",", "er\u00b7greif", "ich", "mei\u00b7ne", "Zi\u00b7ther", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird das Herz mir weit und gro\u00df,", "tokens": ["Wird", "das", "Herz", "mir", "weit", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das br\u00fctende Gewitter", "tokens": ["Und", "das", "br\u00fc\u00b7ten\u00b7de", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bricht in hundert Strahlen los.", "tokens": ["Bricht", "in", "hun\u00b7dert", "Strah\u00b7len", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, mags noch so seltsam klingen,", "tokens": ["Ja", ",", "mags", "noch", "so", "selt\u00b7sam", "klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Reden kann ich nicht, doch singen.", "tokens": ["Re\u00b7den", "kann", "ich", "nicht", ",", "doch", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Brim blim, klang kling,", "tokens": ["Brim", "blim", ",", "klang", "kling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was ist die Liebe f\u00fcr ein Ding!", "tokens": ["Was", "ist", "die", "Lie\u00b7be", "f\u00fcr", "ein", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Drum, das Saitenspiel in H\u00e4nden,", "tokens": ["Drum", ",", "das", "Sai\u00b7ten\u00b7spiel", "in", "H\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruf ich k\u00fchn zu dir hinauf:", "tokens": ["Ruf", "ich", "k\u00fchn", "zu", "dir", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df den spr\u00f6den Sinn sich wenden,", "tokens": ["La\u00df", "den", "spr\u00f6\u00b7den", "Sinn", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tu mir Herz und Fenster auf!", "tokens": ["Tu", "mir", "Herz", "und", "Fens\u00b7ter", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber still: denn wird sies innen,", "tokens": ["A\u00b7ber", "still", ":", "denn", "wird", "sies", "in\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "ADV", "VAFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00fcrnt sie etwa dem Beginnen,", "tokens": ["Z\u00fcrnt", "sie", "et\u00b7wa", "dem", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Schilt, da\u00df ichs mich unterfing,", "tokens": ["Schilt", ",", "da\u00df", "ichs", "mich", "un\u00b7ter\u00b7fing", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ist die Liebe f\u00fcr ein Ding!", "tokens": ["Was", "ist", "die", "Lie\u00b7be", "f\u00fcr", "ein", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch was schm\u00e4h ich diese Wonne,", "tokens": ["Doch", "was", "schm\u00e4h", "ich", "die\u00b7se", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein Innres s\u00fc\u00df bewegt,", "tokens": ["Die", "mein", "Inn\u00b7res", "s\u00fc\u00df", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Sonne minder Sonne,", "tokens": ["Ist", "die", "Son\u00b7ne", "min\u00b7der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil kein Aug ihr Schaun ertr\u00e4gt?", "tokens": ["Weil", "kein", "Aug", "ihr", "Schaun", "er\u00b7tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleibt, wenn nichts auch \u00fcbrig bliebe,", "tokens": ["Bleibt", ",", "wenn", "nichts", "auch", "\u00fcb\u00b7rig", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Gef\u00fchl doch, da\u00df ich liebe,", "tokens": ["Das", "Ge\u00b7f\u00fchl", "doch", ",", "da\u00df", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach und \u2013", "tokens": ["Ach", "und", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "KON", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.12": {"line.1": {"text": "Brim blim, klang kling,", "tokens": ["Brim", "blim", ",", "klang", "kling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Liebe bleibt ein s\u00fc\u00dfes Ding!", "tokens": ["Lie\u00b7be", "bleibt", "ein", "s\u00fc\u00b7\u00dfes", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}