ter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dem falschen Katzenmaul zum neuen Unterhalt.", "tokens": ["Dem", "fal\u00b7schen", "Kat\u00b7zen\u00b7maul", "zum", "neu\u00b7en", "Un\u00b7ter\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was k\u00f6nnen b\u00f6se Zungen nicht", "tokens": ["Was", "k\u00f6n\u00b7nen", "b\u00f6\u00b7se", "Zun\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leichtgl\u00e4ubigen f\u00fcr Stacheln hinterlassen?", "tokens": ["Leicht\u00b7gl\u00e4u\u00b7bi\u00b7gen", "f\u00fcr", "Sta\u00b7cheln", "hin\u00b7ter\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Was richten sie nicht an? Wer ist wol mehr zu hassen,", "tokens": ["Was", "rich\u00b7ten", "sie", "nicht", "an", "?", "Wer", "ist", "wol", "mehr", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "PWS", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als der von Frommen \u00fcbel spricht?", "tokens": ["Als", "der", "von", "From\u00b7men", "\u00fc\u00b7bel", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O k\u00f6nnt' ich dieses hier in kurze Worte fassen!", "t