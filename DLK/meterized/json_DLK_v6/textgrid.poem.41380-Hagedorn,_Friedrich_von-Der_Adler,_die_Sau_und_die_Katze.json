{"textgrid.poem.41380": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Adler, die Sau und die Katze", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tyrannin! die du jung und alt", "tokens": ["Ty\u00b7ran\u00b7nin", "!", "die", "du", "jung", "und", "alt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PRELS", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit unumschr\u00e4nkter Macht regierest!", "tokens": ["Mit", "un\u00b7um\u00b7schr\u00e4nk\u00b7ter", "Macht", "re\u00b7gie\u00b7rest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dich mit der weiblichen Gestalt", "tokens": ["Dich", "mit", "der", "weib\u00b7li\u00b7chen", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der meisten Mode-Laster zierest,", "tokens": ["Der", "meis\u00b7ten", "Mo\u00b7de\u00b7Las\u00b7ter", "zie\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und bald des Stolzes, bald der List,", "tokens": ["Und", "bald", "des", "Stol\u00b7zes", ",", "bald", "der", "List", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch oft der Einfalt Zuflucht bist,", "tokens": ["Auch", "oft", "der", "Ein\u00b7falt", "Zu\u00b7flucht", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verleumdung! deren Mund die Wahrheit selbst bet\u00e4ubet,", "tokens": ["Ver\u00b7leum\u00b7dung", "!", "de\u00b7ren", "Mund", "die", "Wahr\u00b7heit", "selbst", "be\u00b7t\u00e4u\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Mund, den Zucht und Unschuld scheut;", "tokens": ["Der", "Mund", ",", "den", "Zucht", "und", "Un\u00b7schuld", "scheut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dir sei zum ersten Mal ein Blatt von mir geweiht,", "tokens": ["Dir", "sei", "zum", "ers\u00b7ten", "Mal", "ein", "Blatt", "von", "mir", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das jetzt ein Meisterst\u00fcck, das du vollf\u00fchrt, beschreibet!", "tokens": ["Das", "jetzt", "ein", "Meis\u00b7ter\u00b7st\u00fcck", ",", "das", "du", "voll\u00b7f\u00fchrt", ",", "be\u00b7schrei\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es hatt' auf einem hohen Baum", "tokens": ["Es", "hatt'", "auf", "ei\u00b7nem", "ho\u00b7hen", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der V\u00f6gel K\u00f6nigin den Obersitz genommen,", "tokens": ["Der", "V\u00f6\u00b7gel", "K\u00f6\u00b7ni\u00b7gin", "den", "O\u00b7ber\u00b7sitz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Katze w\u00e4hlte sich der Eiche mittlern Raum.", "tokens": ["Die", "Kat\u00b7ze", "w\u00e4hl\u00b7te", "sich", "der", "Ei\u00b7che", "mitt\u00b7lern", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den untersten hatt' eine Sau bekommen.", "tokens": ["Den", "un\u00b7ters\u00b7ten", "hatt'", "ei\u00b7ne", "Sau", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die hielten gute Nachbarschaft;", "tokens": ["Die", "hiel\u00b7ten", "gu\u00b7te", "Nach\u00b7bar\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Argwohn war noch nie die Eintracht unterbrochen;", "tokens": ["Durch", "Arg\u00b7wohn", "war", "noch", "nie", "die", "Ein\u00b7tracht", "un\u00b7ter\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch endlich trennte sie der Bosheit H\u00f6llenkraft,", "tokens": ["Doch", "end\u00b7lich", "trenn\u00b7te", "sie", "der", "Bos\u00b7heit", "H\u00f6l\u00b7len\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Katze kam zum Adler hingekrochen,", "tokens": ["Die", "Kat\u00b7ze", "kam", "zum", "Ad\u00b7ler", "hin\u00b7ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und sprach: H\u00f6rt! unsrer Kinder Tod,", "tokens": ["Und", "sprach", ":", "H\u00f6rt", "!", "uns\u00b7rer", "Kin\u00b7der", "Tod", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVIMP", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo nicht der unsere, (doch, das zu unterscheiden,", "tokens": ["Wo", "nicht", "der", "un\u00b7se\u00b7re", ",", "(", "doch", ",", "das", "zu", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "ADJA", "$,", "$(", "ADV", "$,", "PRELS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "F\u00e4llt Mutterherzen schwer) scheint gar nicht zu vermeiden.", "tokens": ["F\u00e4llt", "Mut\u00b7ter\u00b7her\u00b7zen", "schwer", ")", "scheint", "gar", "nicht", "zu", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "$(", "VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein guter Freund warnt in der Noth.", "tokens": ["Ein", "gu\u00b7ter", "Freund", "warnt", "in", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Seht, ach! ich bitte, seht! wie w\u00fchlt die wilde Sau!", "tokens": ["Seht", ",", "ach", "!", "ich", "bit\u00b7te", ",", "seht", "!", "wie", "w\u00fchlt", "die", "wil\u00b7de", "Sau", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ITJ", "$.", "PPER", "ADV", "$,", "VVFIN", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie gr\u00e4bt, und will den Baum ganz aus der Wurzel heben.", "tokens": ["Sie", "gr\u00e4bt", ",", "und", "will", "den", "Baum", "ganz", "aus", "der", "Wur\u00b7zel", "he\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VMFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Trau', schaue wem; wie mu\u00df ich arme Frau", "tokens": ["Trau'", ",", "schau\u00b7e", "wem", ";", "wie", "mu\u00df", "ich", "ar\u00b7me", "Frau"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PIS", "$.", "PWAV", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "An unsern Kindern das erleben!", "tokens": ["An", "un\u00b7sern", "Kin\u00b7dern", "das", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ihr kennt nicht die Gefahr; mir aber, mir ist bange!", "tokens": ["Ihr", "kennt", "nicht", "die", "Ge\u00b7fahr", ";", "mir", "a\u00b7ber", ",", "mir", "ist", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$.", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sobald die Eiche f\u00e4llt, die schon besch\u00e4digt ist,", "tokens": ["So\u00b7bald", "die", "Ei\u00b7che", "f\u00e4llt", ",", "die", "schon", "be\u00b7sch\u00e4\u00b7digt", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So seh' ich's, wie die Sau die lieben K\u00e4tzchen fri\u00dft,", "tokens": ["So", "seh'", "ich's", ",", "wie", "die", "Sau", "die", "lie\u00b7ben", "K\u00e4tz\u00b7chen", "fri\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ich verlass'nes Weib noch voller Furcht umfange.", "tokens": ["Die", "ich", "ver\u00b7lass'\u00b7nes", "Weib", "noch", "vol\u00b7ler", "Furcht", "um\u00b7fan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich bin den L\u00fcgen gram; ich suche keinen Zwist;", "tokens": ["Ich", "bin", "den", "L\u00fc\u00b7gen", "gram", ";", "ich", "su\u00b7che", "kei\u00b7nen", "Zwist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nein, ehrlich, ehrlich w\u00e4hret lange.", "tokens": ["Nein", ",", "ehr\u00b7lich", ",", "ehr\u00b7lich", "w\u00e4h\u00b7ret", "lan\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "$,", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nachdem sie das gesagt, und mit verstelltem Sinn", "tokens": ["Nach\u00b7dem", "sie", "das", "ge\u00b7sagt", ",", "und", "mit", "ver\u00b7stell\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVPP", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Argwohn gleich erweckt, auf den ihr Reden zielte,", "tokens": ["Den", "Arg\u00b7wohn", "gleich", "er\u00b7weckt", ",", "auf", "den", "ihr", "Re\u00b7den", "ziel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schlich die schlaue Frau stracks zu der Bache hin,", "tokens": ["So", "schlich", "die", "schlau\u00b7e", "Frau", "stracks", "zu", "der", "Ba\u00b7che", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die unten ihre Wochen hielte.", "tokens": ["Die", "un\u00b7ten", "ih\u00b7re", "Wo\u00b7chen", "hiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! allerliebste Nachbarin,", "tokens": ["Ach", "!", "al\u00b7ler\u00b7liebs\u00b7te", "Nach\u00b7ba\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch ahnt's wol nimmermehr, warum ich traurig bin.", "tokens": ["Euch", "ahnt's", "wol", "nim\u00b7mer\u00b7mehr", ",", "wa\u00b7rum", "ich", "trau\u00b7rig", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Kinder jammern mich, die eure Br\u00fcste saugen.", "tokens": ["Die", "Kin\u00b7der", "jam\u00b7mern", "mich", ",", "die", "eu\u00b7re", "Br\u00fcs\u00b7te", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man traue keinen Adleraugen!", "tokens": ["Man", "trau\u00b7e", "kei\u00b7nen", "Ad\u00b7le\u00b7rau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "K\u00f6nnt ihr auch schweigen? Gebt doch Acht,", "tokens": ["K\u00f6nnt", "ihr", "auch", "schwei\u00b7gen", "?", "Gebt", "doch", "Acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$.", "VVIMP", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie \u00fcber uns der b\u00f6se Vogel wacht.", "tokens": ["Wie", "\u00fc\u00b7ber", "uns", "der", "b\u00f6\u00b7se", "Vo\u00b7gel", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich wei\u00df es nur zu wohl, er sch\u00e4rfet schon die Klauen,", "tokens": ["Ich", "wei\u00df", "es", "nur", "zu", "wohl", ",", "er", "sch\u00e4r\u00b7fet", "schon", "die", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und raubet, wenn ihr euch aus eurem Lager macht,", "tokens": ["Und", "rau\u00b7bet", ",", "wenn", "ihr", "euch", "aus", "eu\u00b7rem", "La\u00b7ger", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die sch\u00f6nen Kinderchen; doch alles im Vertrauen.", "tokens": ["Die", "sch\u00f6\u00b7nen", "Kin\u00b7der\u00b7chen", ";", "doch", "al\u00b7les", "im", "Ver\u00b