{"textgrid.poem.58353": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "391. Die g\u00f6ttliche Keuschheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Jesu keusche Braut will sein,", "tokens": ["Wer", "Je\u00b7su", "keu\u00b7sche", "Braut", "will", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df keinen Trost auf Erden haben;", "tokens": ["Mu\u00df", "kei\u00b7nen", "Trost", "auf", "Er\u00b7den", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lust darf auch nicht gehen ein", "tokens": ["Die", "Lust", "darf", "auch", "nicht", "ge\u00b7hen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "VVINF", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Licht, noch Trost, noch andre Gaben.", "tokens": ["In", "Licht", ",", "noch", "Trost", ",", "noch", "and\u00b7re", "Ga\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Gott und nicht in seinen Sch\u00e4tzen", "tokens": ["In", "Gott", "und", "nicht", "in", "sei\u00b7nen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df sich der keusche Sinn erg\u00f6tzen.", "tokens": ["Mu\u00df", "sich", "der", "keu\u00b7sche", "Sinn", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}