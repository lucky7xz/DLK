{"dta.poem.15645": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es gibt nichts einfaches, ein kleinstes gibt es nicht;", "tokens": ["Es", "gibt", "nichts", "ein\u00b7fa\u00b7ches", ",", "ein", "kleins\u00b7tes", "gibt", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PIS", "$,", "ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn scharf und fein genug Gedank' ist und Gesicht,", "tokens": ["Wenn", "scharf", "und", "fein", "ge\u00b7nug", "Ge\u00b7dank'", "ist", "und", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ADV", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nimmst du viel kleines noch im einfach kleinsten wahr;", "tokens": ["Nimmst", "du", "viel", "klei\u00b7nes", "noch", "im", "ein\u00b7fach", "kleins\u00b7ten", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "ADV", "APPRART", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein was hilft es dir, zu spalten Haar um Haar?", "tokens": ["Al\u00b7lein", "was", "hilft", "es", "dir", ",", "zu", "spal\u00b7ten", "Haar", "um", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dis metaphysische Gesch\u00e4ft la\u00df einer Milbe;", "tokens": ["Dis", "me\u00b7ta\u00b7phy\u00b7si\u00b7sche", "Ge\u00b7sch\u00e4ft", "la\u00df", "ei\u00b7ner", "Mil\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVIMP", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Erfreue dich des Worts, und stich nicht jede Silbe.", "tokens": ["Er\u00b7freu\u00b7e", "dich", "des", "Worts", ",", "und", "stich", "nicht", "je\u00b7de", "Sil\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KON", "ADJD", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}