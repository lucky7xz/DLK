{"textgrid.poem.63421": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Scholmeister Bors", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scholmeister Bors \u2013 je de was echt,", "tokens": ["Schol\u00b7meis\u00b7ter", "Bors", "\u2013", "je", "de", "was", "echt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "++-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "un de verst\u00fcnn to backsen,", "tokens": ["un", "de", "ver\u00b7st\u00fcnn", "to", "back\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "un \u00fcmmer hett sin Spr\u00fcch he seggt,", "tokens": ["un", "\u00fcm\u00b7mer", "hett", "sin", "Spr\u00fcch", "he", "seggt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wull he \u00fcns af eens jacksen.", "tokens": ["wull", "he", "\u00fcns", "af", "e\u00b7ens", "jack\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eens set bi't s\u0153wt Gebot ick wi\u00df", "tokens": ["E\u00b7ens", "set", "bi't", "s\u0153wt", "Ge\u00b7bot", "ick", "wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "un k\u00fcnn nich rut mi trecken:", "tokens": ["un", "k\u00fcnn", "nich", "rut", "mi", "tre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u00bbje\u00ab, r\u00f6p he dunn, \u00bball wat nich is,", "tokens": ["\u00bb", "je", "\u00ab", ",", "r\u00f6p", "he", "dunn", ",", "\u00bb", "all", "wat", "nich", "is", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "FM.la", "FM.la", "FM.la", "$,", "$(", "PIAT", "NN", "PTKNEG", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dor kann'n ok nicks vun seggen.", "tokens": ["dor", "kann'n", "ok", "nicks", "vun", "seg\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Na kumm eens rut dor ut din B\u00e4nk,", "tokens": ["Na", "kumm", "e\u00b7ens", "rut", "dor", "ut", "din", "B\u00e4nk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "wat helpt all dat Besinnen!", "tokens": ["wat", "helpt", "all", "dat", "Be\u00b7sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIAT", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Denn wat een nich verl\u00fcst, dat, denk", "tokens": ["Denn", "wat", "e\u00b7en", "nich", "ver\u00b7l\u00fcst", ",", "dat", ",", "denk"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PWS", "VVFIN", "PTKNEG", "VVFIN", "$,", "PRELS", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "ick, kann een ok nich finnen!\u00ab", "tokens": ["ick", ",", "kann", "e\u00b7en", "ok", "nich", "fin\u00b7nen", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sin Fust was knakendr\u00f6g un swer;", "tokens": ["Sin", "Fust", "was", "kna\u00b7ken\u00b7dr\u00f6g", "un", "swer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "irst hett se af mi knuffelt,", "tokens": ["irst", "hett", "se", "af", "mi", "knuf\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "backst linksch un rechtsch mi nahst un s\u00e4r:", "tokens": ["backst", "link\u00b7sch", "un", "rechtsch", "mi", "nahst", "un", "s\u00e4r", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00bbs\u00fch, \u0153wer Kr\u00fcz h\u00f6llt duwwelt!\u00ab", "tokens": ["\u00bb", "s\u00fch", ",", "\u0153wer", "Kr\u00fcz", "h\u00f6llt", "duw\u00b7welt", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ick sn\u00f6w un speg; man he h\u00f6ll fast,", "tokens": ["Ick", "sn\u00f6w", "un", "speg", ";", "man", "he", "h\u00f6ll", "fast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "FM", "FM", "FM", "FM", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "grad as mit isern Klannern,", "tokens": ["grad", "as", "mit", "i\u00b7sern", "Klan\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "un r\u00f6p dorto: \u00bbSonn Amt, sonn Last \u2013", "tokens": ["un", "r\u00f6p", "dor\u00b7to", ":", "\u00bb", "Sonn", "Amt", ",", "sonn", "Last", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "NN", "$,", "KON", "NE", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "een D\u00fcwel 's \u0153wer'n annern!", "tokens": ["e\u00b7en", "D\u00fc\u00b7wel", "'s", "\u0153wer'n", "an\u00b7nern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wen alltoneg an'n Graben f\u00fchrt,", "tokens": ["Wen", "all\u00b7to\u00b7neg", "an'n", "Gra\u00b7ben", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "is oft all rinner schaten \u2013", "tokens": ["is", "oft", "all", "rin\u00b7ner", "scha\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "un wen de Koh to eegen h\u00fcrt,", "tokens": ["un", "wen", "de", "Koh", "to", "e\u00b7e\u00b7gen", "h\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "m\u00f6t s' ok an'n Swanz anfaten!", "tokens": ["m\u00f6t", "s'", "ok", "an'n", "Swanz", "an\u00b7fa\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Man nich so \u00e4ngstlich, dreig di r\u00fcnd,", "tokens": ["Man", "nich", "so", "\u00e4ngst\u00b7lich", ",", "dreig", "di", "r\u00fcnd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "ADV", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat geiht di nich an't L\u00e4ben;", "tokens": ["dat", "geiht", "di", "nich", "an't", "L\u00e4\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "PTKNEG", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "s\u00fch, Murrjan was'n steenolt Hund", "tokens": ["s\u00fch", ",", "Murr\u00b7jan", "was'n", "stee\u00b7nolt", "Hund"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "NE", "NE", "VVFIN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "un m\u00fc\u00df sick doch noch g\u00e4ben.", "tokens": ["un", "m\u00fc\u00df", "sick", "doch", "noch", "g\u00e4\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Sonn Pott, sonn St\u00fclp, sonn Bom, sonn Block,", "tokens": ["Sonn", "Pott", ",", "sonn", "St\u00fclp", ",", "sonn", "Bom", ",", "sonn", "Block", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "NN", "$,", "KON", "NE", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sonn Wor, min S\u0153hn, sonn Dr\u00fcttel!", "tokens": ["sonn", "Wor", ",", "min", "S\u0153hn", ",", "sonn", "Dr\u00fct\u00b7tel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dat Hemd is neger as de Rock \u2013", "tokens": ["Dat", "Hemd", "is", "ne\u00b7ger", "as", "de", "Rock", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "nu treck eens af din'n Kittel!\u00ab", "tokens": ["nu", "treck", "e\u00b7ens", "af", "din'n", "Kit\u00b7tel", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Un as ick dunn verspr\u00e4ken der,", "tokens": ["Un", "as", "ick", "dunn", "ver\u00b7spr\u00e4\u00b7ken", "der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "wat ick nu b\u00e4tern wull mi,", "tokens": ["wat", "ick", "nu", "b\u00e4\u00b7tern", "wull", "mi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "dor langt he flink den Tagel her", "tokens": ["dor", "langt", "he", "flink", "den", "Ta\u00b7gel", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "VVFIN", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un sl\u00f6g f\u00f6r blind un dull mi.", "tokens": ["un", "sl\u00f6g", "f\u00f6r", "blind", "un", "dull", "mi."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbs\u00fch, W\u00fcr, min S\u0153hn, de s\u00fcnd nich d\u00fcr,", "tokens": ["\u00bb", "s\u00fch", ",", "W\u00fcr", ",", "min", "S\u0153hn", ",", "de", "s\u00fcnd", "nich", "d\u00fcr", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat k\u00fcnn mi doch belur'n;", "tokens": ["dat", "k\u00fcnn", "mi", "doch", "be\u00b7lur'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "verspr\u00e4ken don de Edell\u00fcr,", "tokens": ["ver\u00b7spr\u00e4\u00b7ken", "don", "de", "E\u00b7del\u00b7l\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "man hollen don de Bur'n!\u00ab", "tokens": ["man", "hol\u00b7len", "don", "de", "Bur'n", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "NE", "NE", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ick b\u00f6lkt luddhals. Dunn schreg he: \u00bbJung,", "tokens": ["Ick", "b\u00f6lkt", "ludd\u00b7hals", ".", "Dunn", "schreg", "he", ":", "\u00bb", "Jung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "schri drist to, du schast blarren!", "tokens": ["schri", "drist", "to", ",", "du", "schast", "blar\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ungl\u00fcck hadd stets 'ne scharpe Tung,", "tokens": ["Un\u00b7gl\u00fcck", "hadd", "stets", "'ne", "schar\u00b7pe", "Tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un de peddt Pogg' de quarren.", "tokens": ["un", "de", "peddt", "Pog\u00b7g'", "de", "quar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "So heet, as ick di dat upf\u00fcll,", "tokens": ["So", "heet", ",", "as", "ick", "di", "dat", "up\u00b7f\u00fcll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "brukst du't jo nich to \u00e4ten;", "tokens": ["brukst", "du't", "jo", "nich", "to", "\u00e4\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wenn sick din Tung verbrennen sch\u00fcll,", "tokens": ["wenn", "sick", "din", "Tung", "ver\u00b7bren\u00b7nen", "sch\u00fcll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "m\u00f6st du di't s\u00fclst bim\u00e4ten!\u00ab", "tokens": ["m\u00f6st", "du", "di't", "s\u00fclst", "bi\u00b7m\u00e4\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbmin Puckel\u00ab, schreg'ck, \u00bbm\u00f6t g\u00e4l un gr\u00f6n,", "tokens": ["\u00bb", "min", "Pu\u00b7ckel", "\u00ab", ",", "schreg'ck", ",", "\u00bb", "m\u00f6t", "g\u00e4l", "un", "gr\u00f6n", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$(", "$,", "VVFIN", "$,", "$(", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "min leew Herr Bors, all w\u00e4sen!\u00ab", "tokens": ["min", "leew", "Herr", "Bors", ",", "all", "w\u00e4\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$,", "PIAT", "VVINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dunn s\u00e4d he: \u00bbUnwennt Arbeit, S\u0153hn,", "tokens": ["Dunn", "s\u00e4d