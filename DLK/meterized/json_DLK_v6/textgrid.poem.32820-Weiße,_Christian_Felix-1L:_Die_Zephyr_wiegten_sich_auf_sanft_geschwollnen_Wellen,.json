{"textgrid.poem.32820": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Zephyr wiegten sich auf sanft geschwollnen Wellen,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zephyr wiegten sich auf sanft geschwollnen Wellen,", "tokens": ["Die", "Ze\u00b7phyr", "wieg\u00b7ten", "sich", "auf", "sanft", "ge\u00b7schwoll\u00b7nen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Fr\u00fchling war ums stille Meer:", "tokens": ["Und", "Fr\u00fch\u00b7ling", "war", "ums", "stil\u00b7le", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der leichten Scherze fl\u00fcchtigs Heer,", "tokens": ["Der", "leich\u00b7ten", "Scher\u00b7ze", "fl\u00fcch\u00b7tigs", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die jungen Freuden, ihre Gesellen,", "tokens": ["Die", "jun\u00b7gen", "Freu\u00b7den", ",", "ih\u00b7re", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Grazien mit sanftumschlungner Hand", "tokens": ["Und", "Gra\u00b7zi\u00b7en", "mit", "sanf\u00b7tumsc\u00b7hlung\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Umringten den bebl\u00fcmten Strand:", "tokens": ["Um\u00b7ring\u00b7ten", "den", "be\u00b7bl\u00fcm\u00b7ten", "Strand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sah die Fabel Cytheren", "tokens": ["Da", "sah", "die", "Fa\u00b7bel", "Cy\u00b7the\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vom Schaume des Meeres geb\u00e4hren.", "tokens": ["Vom", "Schau\u00b7me", "des", "Mee\u00b7res", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Doch Damon st\u00f6ret k\u00fchn den alten Aberglauben;", "tokens": ["Doch", "Da\u00b7mon", "st\u00f6\u00b7ret", "k\u00fchn", "den", "al\u00b7ten", "A\u00b7berg\u00b7lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey einem Glase blanken Wein", "tokens": ["Bey", "ei\u00b7nem", "Gla\u00b7se", "blan\u00b7ken", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah er das Ding weit besser ein:", "tokens": ["Sah", "er", "das", "Ding", "weit", "bes\u00b7ser", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die frohen Winzer kelterten Trauben;", "tokens": ["Die", "fro\u00b7hen", "Win\u00b7zer", "kel\u00b7ter\u00b7ten", "Trau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es sch\u00e4umete der Most mit Ungest\u00fcm,", "tokens": ["Es", "sch\u00e4u\u00b7me\u00b7te", "der", "Most", "mit", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Chloe zeigt es l\u00e4chelnd ihm:", "tokens": ["Und", "Chloe", "zeigt", "es", "l\u00e4\u00b7chelnd", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "PPER", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Da sah er ganz deutlich Cytheren", "tokens": ["Da", "sah", "er", "ganz", "deut\u00b7lich", "Cy\u00b7the\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Vom Schaume des Weines geb\u00e4hren.", "tokens": ["Vom", "Schau\u00b7me", "des", "Wei\u00b7nes", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}