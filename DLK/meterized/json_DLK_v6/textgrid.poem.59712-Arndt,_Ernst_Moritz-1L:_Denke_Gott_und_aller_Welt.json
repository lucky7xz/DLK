{"textgrid.poem.59712": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denke Gott und aller Welt", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denke Gott und aller Welt", "tokens": ["Den\u00b7ke", "Gott", "und", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Millionen Sonnenstra\u00dfen,", "tokens": ["Mil\u00b7lion\u00b7en", "Son\u00b7nen\u00b7stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mi\u00df, was diese Erde h\u00e4lt,", "tokens": ["Mi\u00df", ",", "was", "die\u00b7se", "Er\u00b7de", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mi\u00df es dir mit Sonnenma\u00dfen,", "tokens": ["Mi\u00df", "es", "dir", "mit", "Son\u00b7nen\u00b7ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tritt den Staub dir ganz zu Staub,", "tokens": ["Tritt", "den", "Staub", "dir", "ganz", "zu", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tritt ihn mit Prometheus' Sohlen;", "tokens": ["Tritt", "ihn", "mit", "Pro\u00b7me\u00b7theus'", "Soh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "So nur kannst du Himmelsraub", "tokens": ["So", "nur", "kannst", "du", "Him\u00b7mels\u00b7raub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Mit Prometheus' Mut dir holen.", "tokens": ["Mit", "Pro\u00b7me\u00b7theus'", "Mut", "dir", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hoch und niedrig, gro\u00df und klein \u2013", "tokens": ["Hoch", "und", "nied\u00b7rig", ",", "gro\u00df", "und", "klein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Stolz, dies Ma\u00df mu\u00df schwinden,", "tokens": ["Die\u00b7ser", "Stolz", ",", "dies", "Ma\u00df", "mu\u00df", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDS", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann nur kannst du Flieger sein", "tokens": ["Dann", "nur", "kannst", "du", "Flie\u00b7ger", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Adler \u00fcber Winden:", "tokens": ["Mit", "dem", "Ad\u00b7ler", "\u00fc\u00b7ber", "Win\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Federn schweben still", "tokens": ["Sei\u00b7ne", "Fe\u00b7dern", "schwe\u00b7ben", "still"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaukelnd \u00fcber Sonnenscheiben,", "tokens": ["Schau\u00b7kelnd", "\u00fc\u00b7ber", "Son\u00b7nen\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo kein Sehnen weiter will,", "tokens": ["Wo", "kein", "Seh\u00b7nen", "wei\u00b7ter", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da nur ist ein selig Bleiben.", "tokens": ["Da", "nur", "ist", "ein", "se\u00b7lig", "Blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Hehrer Aufblick! H\u00f6chstes Ziel!", "tokens": ["Heh\u00b7rer", "Auf\u00b7blick", "!", "H\u00f6chs\u00b7tes", "Ziel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ma\u00dfe schwinden und Gewichte,", "tokens": ["Ma\u00b7\u00dfe", "schwin\u00b7den", "und", "Ge\u00b7wich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Geist im zarten Spiel", "tokens": ["Und", "der", "Geist", "im", "zar\u00b7ten", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Schwelgt und jauchzt im heitern Lichte:", "tokens": ["Schwelgt", "und", "jauchzt", "im", "hei\u00b7tern", "Lich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn um keine Majest\u00e4t,", "tokens": ["Denn", "um", "kei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Um kein Gl\u00fcck wird mehr gestritten,", "tokens": ["Um", "kein", "Gl\u00fcck", "wird", "mehr", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeder Punkt, auf dem er steht,", "tokens": ["Je\u00b7der", "Punkt", ",", "auf", "dem", "er", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist ein Punkt der Weltenmitten.", "tokens": ["Ist", "ein", "Punkt", "der", "Wel\u00b7ten\u00b7mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}