{"textgrid.poem.36538": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst trug auf seinem schmalen R\u00fccken", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst trug auf seinem schmalen R\u00fccken", "tokens": ["Einst", "trug", "auf", "sei\u00b7nem", "schma\u00b7len", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Esel eine schwere Last,", "tokens": ["Ein", "E\u00b7sel", "ei\u00b7ne", "schwe\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die f\u00e4hig war, ihn tot zu dr\u00fccken.", "tokens": ["Die", "f\u00e4\u00b7hig", "war", ",", "ihn", "tot", "zu", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ledig Pferd ging neben ihm.", "tokens": ["Ein", "le\u00b7dig", "Pferd", "ging", "ne\u00b7ben", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Du hast", "tokens": ["Du", "hast"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Auf deinem R\u00fccken nichts, sprach das geplagte Tier;", "tokens": ["Auf", "dei\u00b7nem", "R\u00fc\u00b7cken", "nichts", ",", "sprach", "das", "ge\u00b7plag\u00b7te", "Tier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIS", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Hilf, liebes Pferdchen, hilf, ich bitte dich, hilf mir!", "tokens": ["Hilf", ",", "lie\u00b7bes", "Pferd\u00b7chen", ",", "hilf", ",", "ich", "bit\u00b7te", "dich", ",", "hilf", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVIMP", "$,", "PPER", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was helfen! sagt der grobe Gaul;", "tokens": ["Was", "hel\u00b7fen", "!", "sagt", "der", "gro\u00b7be", "Gaul", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du bist der rechte Gast, du bist ein wenig faul!", "tokens": ["Du", "bist", "der", "rech\u00b7te", "Gast", ",", "du", "bist", "ein", "we\u00b7nig", "faul", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Trag zu! \u2013 \u2013", "tokens": ["Trag", "zu", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["NN", "PTKVZ", "$.", "$(", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ich sterbe, liebes Pferd \u2013", "tokens": ["Ich", "ster\u00b7be", ",", "lie\u00b7bes", "Pferd", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Last erdr\u00fcckt mich, rette mich!", "tokens": ["Die", "Last", "er\u00b7dr\u00fcckt", "mich", ",", "ret\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die H\u00e4lfte w\u00e4r' ein Spiel f\u00fcr dich!", "tokens": ["Die", "H\u00e4lf\u00b7te", "w\u00e4r'", "ein", "Spiel", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kann nicht! sprach das Pferd.", "tokens": ["Ich", "kann", "nicht", "!", "sprach", "das", "Pferd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kurz, unter dem zu schweren Sack", "tokens": ["Kurz", ",", "un\u00b7ter", "dem", "zu", "schwe\u00b7ren", "Sack"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erlag der Esel. Sack und Pack", "tokens": ["Er\u00b7lag", "der", "E\u00b7sel", ".", "Sack", "und", "Pack"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schmi\u00df man dem groben Rappen auf;", "tokens": ["Sch\u00b7mi\u00df", "man", "dem", "gro\u00b7ben", "Rap\u00b7pen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Des Esels Haut noch oben drauf.", "tokens": ["Des", "E\u00b7sels", "Haut", "noch", "o\u00b7ben", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}