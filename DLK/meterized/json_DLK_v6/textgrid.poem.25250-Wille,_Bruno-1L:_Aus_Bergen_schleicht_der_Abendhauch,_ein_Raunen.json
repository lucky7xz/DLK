{"textgrid.poem.25250": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus Bergen schleicht der Abendhauch, ein Raunen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Bergen schleicht der Abendhauch, ein Raunen", "tokens": ["Aus", "Ber\u00b7gen", "schleicht", "der", "A\u00b7bend\u00b7hauch", ",", "ein", "Rau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im w\u00fcsten Hain.", "tokens": ["Im", "w\u00fcs\u00b7ten", "Hain", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Tannenvolk umringt mit scheuem Staunen", "tokens": ["Das", "Tan\u00b7nen\u00b7volk", "um\u00b7ringt", "mit", "scheu\u00b7em", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Sagenstein.", "tokens": ["Den", "Sa\u00b7gen\u00b7stein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Hier stund ein Schlo\u00df; sein Glitzern machte trunken", "tokens": ["Hier", "stund", "ein", "Schlo\u00df", ";", "sein", "Glit\u00b7zern", "mach\u00b7te", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Abendstrahl.", "tokens": ["Wie", "A\u00b7bend\u00b7strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verwunschen wards. Und wo die Pracht versunken,", "tokens": ["Ver\u00b7wun\u00b7schen", "wards", ".", "Und", "wo", "die", "Pracht", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bezeugt dies Mal.", "tokens": ["Be\u00b7zeugt", "dies", "Mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Verd\u00fcstert hockt der Stein/ wie seinen Sorgen", "tokens": ["Ver\u00b7d\u00fcs\u00b7tert", "hockt", "der", "Stein", "/", "wie", "sei\u00b7nen", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$(", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bettler grollt.", "tokens": ["Ein", "Bett\u00b7ler", "grollt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verkappter F\u00fcrst! Im Grunde dir geborgen", "tokens": ["Ver\u00b7kapp\u00b7ter", "F\u00fcrst", "!", "Im", "Grun\u00b7de", "dir", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPRART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ruht Perl und Gold.", "tokens": ["Ruht", "Perl", "und", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Kein Gr\u00e4ber drang noch durch die Felsenrinde", "tokens": ["Kein", "Gr\u00e4\u00b7ber", "drang", "noch", "durch", "die", "Fel\u00b7sen\u00b7rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum g\u00fcldnen Schacht.", "tokens": ["Zum", "g\u00fcld\u00b7nen", "Schacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Glimmen winkt nur dem Johanniskinde", "tokens": ["Ein", "Glim\u00b7men", "winkt", "nur", "dem", "Jo\u00b7han\u00b7nis\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Zaubernacht.", "tokens": ["In", "Zau\u00b7ber\u00b7nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sein Tr\u00e4umeraug erschaut in H\u00f6hlenwildnis", "tokens": ["Sein", "Tr\u00e4u\u00b7me\u00b7raug", "er\u00b7schaut", "in", "H\u00f6h\u00b7len\u00b7wild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Perlenschrein,", "tokens": ["Den", "Per\u00b7len\u00b7schrein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auch marmorwei\u00df ein K\u00f6niginnen-Bildnis", "tokens": ["Auch", "mar\u00b7mor\u00b7wei\u00df", "ein", "K\u00f6\u00b7ni\u00b7gin\u00b7nen\u00b7Bild\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Dom von Stein./", "tokens": ["Im", "Dom", "von", "Stein", ".", "/"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich kenne sie, die heilgen Heimlichkeiten", "tokens": ["Ich", "ken\u00b7ne", "sie", ",", "die", "heil\u00b7gen", "Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Innenschau.", "tokens": ["Der", "In\u00b7nen\u00b7schau", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verwunschen sank auch mir ins Grab der Zeiten", "tokens": ["Ver\u00b7wun\u00b7schen", "sank", "auch", "mir", "ins", "Grab", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein K\u00f6nigsbau.", "tokens": ["Mein", "K\u00f6\u00b7nigs\u00b7bau", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Doch was dereinst an Seligkeit erbl\u00fchte,", "tokens": ["Doch", "was", "de\u00b7reinst", "an", "Se\u00b7lig\u00b7keit", "er\u00b7bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist nimmer tot;", "tokens": ["Ist", "nim\u00b7mer", "tot", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es bleibt mein Schatz, versunken im Gem\u00fcte,", "tokens": ["Es", "bleibt", "mein", "Schatz", ",", "ver\u00b7sun\u00b7ken", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der magisch loht.", "tokens": ["Der", "ma\u00b7gisch", "loht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ich selber bin das Schlo\u00df mit g\u00fcldner Tiefe,", "tokens": ["Ich", "sel\u00b7ber", "bin", "das", "Schlo\u00df", "mit", "g\u00fcld\u00b7ner", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sagenstein.", "tokens": ["Der", "Sa\u00b7gen\u00b7stein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und ob ich ganz der Oberwelt entschliefe,", "tokens": ["Und", "ob", "ich", "ganz", "der", "O\u00b7ber\u00b7welt", "ent\u00b7schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Traum ist mein.", "tokens": ["Der", "Traum", "ist", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Die K\u00f6nigin ward diesen hei\u00dfen Sinnen", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "ward", "die\u00b7sen", "hei\u00b7\u00dfen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinweggebannt.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7bannt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verkl\u00e4rt zum Engel weiht sie nun mein Minnen", "tokens": ["Ver\u00b7kl\u00e4rt", "zum", "En\u00b7gel", "weiht", "sie", "nun", "mein", "Min\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Geisterland.", "tokens": ["Dem", "Geis\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Als Dom von Tropfgestein soll mich umflechten", "tokens": ["Als", "Dom", "von", "Tropf\u00b7ge\u00b7stein", "soll", "mich", "um\u00b7flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Innenwelt.", "tokens": ["Die", "In\u00b7nen\u00b7welt", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Braut meiner Jugend, throne mir zur Rechten", "tokens": ["Braut", "mei\u00b7ner", "Ju\u00b7gend", ",", "thro\u00b7ne", "mir", "zur", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Im H\u00f6hlenzelt!", "tokens": ["Im", "H\u00f6h\u00b7len\u00b7zelt", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}