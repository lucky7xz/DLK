{"dta.poem.18790": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dichter will so gerne Knecht seyn,", "tokens": ["Dich\u00b7ter", "will", "so", "ger\u00b7ne", "Knecht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADV", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Herrschaft draus entspringet;", "tokens": ["Weil", "die", "Herr\u00b7schaft", "draus", "ent\u00b7sprin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch vor allem sollt\u2019 ihm recht seyn,", "tokens": ["Doch", "vor", "al\u00b7lem", "sollt'", "ihm", "recht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wenn das Liebchen selber singet.", "tokens": ["Wenn", "das", "Lieb\u00b7chen", "sel\u00b7ber", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist sie denn des Liedes m\u00e4chtig?", "tokens": ["Ist", "sie", "denn", "des", "Lie\u00b7des", "m\u00e4ch\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie\u2019s auf unsern Lippen waltet:", "tokens": ["Wie's", "auf", "un\u00b7sern", "Lip\u00b7pen", "wal\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es macht sie gar verd\u00e4chtig", "tokens": ["Denn", "es", "macht", "sie", "gar", "ver\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass sie im Verborgnen schaltet.", "tokens": ["Dass", "sie", "im", "Ver\u00b7borg\u00b7nen", "schal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}