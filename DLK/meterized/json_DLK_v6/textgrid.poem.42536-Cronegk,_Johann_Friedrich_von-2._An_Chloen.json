{"textgrid.poem.42536": {"metadata": {"author": {"name": "Cronegk, Johann Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2. An Chloen", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts unterbreche hier die stille heil'ge Nacht", "tokens": ["Nichts", "un\u00b7ter\u00b7bre\u00b7che", "hier", "die", "stil\u00b7le", "heil'\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als, Nachtigall, dein Lied und mein verliebtes Leiden!", "tokens": ["Als", ",", "Nach\u00b7ti\u00b7gall", ",", "dein", "Lied", "und", "mein", "ver\u00b7lieb\u00b7tes", "Lei\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie reizend rauscht der West! Die Welt ist f\u00fcr die Freuden,", "tokens": ["Wie", "rei\u00b7zend", "rauscht", "der", "West", "!", "Die", "Welt", "ist", "f\u00fcr", "die", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ich bin nicht f\u00fcr sie gemacht.", "tokens": ["Doch", "ich", "bin", "nicht", "f\u00fcr", "sie", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu einer schlimmen Zeit in diese Welt versetzt,", "tokens": ["Zu", "ei\u00b7ner", "schlim\u00b7men", "Zeit", "in", "die\u00b7se", "Welt", "ver\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Chloens K\u00fcssen fern, ist einsam, ungenossen", "tokens": ["Von", "Chloens", "K\u00fcs\u00b7sen", "fern", ",", "ist", "ein\u00b7sam", ",", "un\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$,", "VAFIN", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Der Jugend traur'ge Zeit mir unvermerkt verflossen,", "tokens": ["Der", "Ju\u00b7gend", "traur'\u00b7ge", "Zeit", "mir", "un\u00b7ver\u00b7merkt", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur, Dichtkunst! nur durch dich erg\u00f6tzt.", "tokens": ["Nur", ",", "Dicht\u00b7kunst", "!", "nur", "durch", "dich", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch dich erh\u00f6ht der Geist sich \u00fcber seinen Schmerz;", "tokens": ["Durch", "dich", "er\u00b7h\u00f6ht", "der", "Geist", "sich", "\u00fc\u00b7ber", "sei\u00b7nen", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es trocknen nach und nach der Wehmut Tr\u00e4nen wieder.", "tokens": ["Es", "trock\u00b7nen", "nach", "und", "nach", "der", "Weh\u00b7mut", "Tr\u00e4\u00b7nen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun z\u00fcrn' ich nicht, o Gl\u00fcck! du gibst mir sanfte Lieder", "tokens": ["Nun", "z\u00fcrn'", "ich", "nicht", ",", "o", "Gl\u00fcck", "!", "du", "gibst", "mir", "sanf\u00b7te", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meiner Chloen z\u00e4rtlich Herz.", "tokens": ["Und", "mei\u00b7ner", "Chloen", "z\u00e4rt\u00b7lich", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie liebt mich \u2013 dies ist g'nug, nie unbegl\u00fcckt zu sein!", "tokens": ["Sie", "liebt", "mich", "\u2013", "dies", "ist", "g'\u00b7nug", ",", "nie", "un\u00b7be\u00b7gl\u00fcckt", "zu", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PDS", "VAFIN", "ADV", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Stets sah ich sie vor mir, versenkt in stillem Sehnen.", "tokens": ["Stets", "sah", "ich", "sie", "vor", "mir", ",", "ver\u00b7senkt", "in", "stil\u00b7lem", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will, geliebtes Kind! ich will statt meiner Tr\u00e4nen", "tokens": ["Ich", "will", ",", "ge\u00b7lieb\u00b7tes", "Kind", "!", "ich", "will", "statt", "mei\u00b7ner", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "ADJA", "NN", "$.", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir ew'ge treue Lieder weihn.", "tokens": ["Dir", "ew'\u00b7ge", "treu\u00b7e", "Lie\u00b7der", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Fern von der H\u00f6fe Pracht, lockt meiner Lieder Ton", "tokens": ["Fern", "von", "der", "H\u00f6\u00b7fe", "Pracht", ",", "lockt", "mei\u00b7ner", "Lie\u00b7der", "Ton"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nymphen aus dem Busch zu leicht verschlungnen Reihen.", "tokens": ["Die", "Nym\u00b7phen", "aus", "dem", "Busch", "zu", "leicht", "ver\u00b7schlung\u00b7nen", "Rei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergn\u00fcgt vergangene Zeit empfind' ich hier vom neuen,", "tokens": ["Ver\u00b7gn\u00fcgt", "ver\u00b7gan\u00b7ge\u00b7ne", "Zeit", "emp\u00b7find'", "ich", "hier", "vom", "neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und dich, o k\u00fcnft'ge, seh' ich schon.", "tokens": ["Und", "dich", ",", "o", "k\u00fcnft'\u00b7ge", ",", "seh'", "ich", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So \u00f6ffnet l\u00e4chelnd sich der Rosen schlummernd Haupt,", "tokens": ["So", "\u00f6ff\u00b7net", "l\u00e4\u00b7chelnd", "sich", "der", "Ro\u00b7sen", "schlum\u00b7mernd", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Morgens s\u00fc\u00dfen Tau sanft lechzend zu empfangen:", "tokens": ["Des", "Mor\u00b7gens", "s\u00fc\u00b7\u00dfen", "Tau", "sanft", "lech\u00b7zend", "zu", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So l\u00e4chelt' einst ihr Mund, so gl\u00fchten ihre Wangen,", "tokens": ["So", "l\u00e4\u00b7chelt'", "einst", "ihr", "Mund", ",", "so", "gl\u00fch\u00b7ten", "ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als sie mir einen Ku\u00df erlaubt.", "tokens": ["Als", "sie", "mir", "ei\u00b7nen", "Ku\u00df", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wann mich die g\u00fcnst'ge Zeit der Nachwelt \u00fcbergibt,", "tokens": ["Wann", "mich", "die", "g\u00fcnst'\u00b7ge", "Zeit", "der", "Nach\u00b7welt", "\u00fc\u00b7berg\u00b7ibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oh, so beneiden dich der k\u00fcnft'gen Sch\u00f6nen Triebe:", "tokens": ["Oh", ",", "so", "be\u00b7nei\u00b7den", "dich", "der", "k\u00fcnft'\u00b7gen", "Sch\u00f6\u00b7nen", "Trie\u00b7be", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie sch\u00f6n war Chloe wohl! wie z\u00e4rtlich seine Liebe!", "tokens": ["Wie", "sch\u00f6n", "war", "Chloe", "wohl", "!", "wie", "z\u00e4rt\u00b7lich", "sei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NE", "ADV", "$.", "PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich selbst, ich h\u00e4tt' ihn auch geliebt!", "tokens": ["Ich", "selbst", ",", "ich", "h\u00e4tt'", "ihn", "auch", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein J\u00fcngling findet einst der Lieder \u00dcberrest;", "tokens": ["Ein", "J\u00fcng\u00b7ling", "fin\u00b7det", "einst", "der", "Lie\u00b7der", "\u00dc\u00b7ber\u00b7rest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sagt alsdann ger\u00fchrt von traurig sanften T\u00f6nen:", "tokens": ["Er", "sagt", "als\u00b7dann", "ge\u00b7r\u00fchrt", "von", "trau\u00b7rig", "sanf\u00b7ten", "T\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ungl\u00fccklicher! \u2013 auch dir hat edle Schwermut Tr\u00e4nen,", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "!", "\u2013", "auch", "dir", "hat", "ed\u00b7le", "Schwer\u00b7mut", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ADV", "PPER", "VAFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verliebte Tr\u00e4nen ausgepre\u00dft!", "tokens": ["Ver\u00b7lieb\u00b7te", "Tr\u00e4\u00b7nen", "aus\u00b7ge\u00b7pre\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So, Hebrus! lag bei dir am klagerf\u00fcllten Strand", "tokens": ["So", ",", "Heb\u00b7rus", "!", "lag", "bei", "dir", "am", "kla\u00b7ger\u00b7f\u00fcll\u00b7ten", "Strand"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NE", "$.", "VVFIN", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Dichters Leier da, der an dem Styx gesungen,", "tokens": ["Des", "Dich\u00b7ters", "Lei\u00b7er", "da", ",", "der", "an", "dem", "Styx", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und selbst dem schwarzen Styx Mitleiden abgezwungen,", "tokens": ["Und", "selbst", "dem", "schwar\u00b7zen", "Styx", "Mit\u00b7lei\u00b7den", "ab\u00b7ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch nicht bei zorn'gen Sch\u00f6nen fand.", "tokens": ["Doch", "nicht", "bei", "zorn'\u00b7gen", "Sch\u00f6\u00b7nen", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Saiten zitterten, mit halbgebrochnem Ton,", "tokens": ["Die", "Sai\u00b7ten", "zit\u00b7ter\u00b7ten", ",", "mit", "halb\u00b7ge\u00b7broch\u00b7nem", "Ton", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Von dir, Eurydice, von dem gewohnten Namen:", "tokens": ["Von", "dir", ",", "Eu\u00b7ry\u00b7di\u00b7ce", ",", "von", "dem", "ge\u00b7wohn\u00b7ten", "Na\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Wirbelwinde bald, sie zu erheben, kamen;", "tokens": ["Als", "Wir\u00b7bel\u00b7win\u00b7de", "bald", ",", "sie", "zu", "er\u00b7he\u00b7ben", ",", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unter Sternen gl\u00e4nzt sie schon.", "tokens": ["Und", "un\u00b7ter", "Ster\u00b7nen", "gl\u00e4nzt", "sie", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Nach ihr sieht, durch die Nacht, wachsamer Weisen Blick:", "tokens": ["Nach", "ihr", "sieht", ",", "durch", "die", "Nacht", ",", "wach\u00b7sa\u00b7mer", "Wei\u00b7sen", "Blick", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihr Flei\u00df bemerket noch der Dichtkunst Lohn vom weiten.", "tokens": ["Ihr", "Flei\u00df", "be\u00b7mer\u00b7ket", "noch", "der", "Dicht\u00b7kunst", "Lohn", "vom", "wei\u00b7ten", "."], "token_info": ["word", "word", "word"