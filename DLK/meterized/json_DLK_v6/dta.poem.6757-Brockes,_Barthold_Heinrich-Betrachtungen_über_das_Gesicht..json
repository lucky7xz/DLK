{"dta.poem.6757": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von unserem Begriff die grosse Fertigkeit", "tokens": ["Von", "un\u00b7se\u00b7rem", "Be\u00b7griff", "die", "gros\u00b7se", "Fer\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht, da\u00df wir so geneigt, dem Werckzeug ihn zu schencken,", "tokens": ["Macht", ",", "da\u00df", "wir", "so", "ge\u00b7neigt", ",", "dem", "Wer\u00b7ck\u00b7zeug", "ihn", "zu", "schen\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Und da\u00df wir, durch Gewohnheit und durch Zeit,", "tokens": ["Und", "da\u00df", "wir", ",", "durch", "Ge\u00b7wohn\u00b7heit", "und", "durch", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu f\u00fchlen glauben, ohn zu dencken.", "tokens": ["Zu", "f\u00fch\u00b7len", "glau\u00b7ben", ",", "ohn", "zu", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Seel ist allemal dabey,", "tokens": ["Die", "Seel", "ist", "al\u00b7le\u00b7mal", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn, da\u00df sie gegenw\u00e4rtig sey,", "tokens": ["Wenn", ",", "da\u00df", "sie", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie selber gleich nicht glaubt. Jhr heimlichs Uberlegen,", "tokens": ["Sie", "sel\u00b7ber", "gleich", "nicht", "glaubt", ".", "Ihr", "heim\u00b7lichs", "U\u00b7ber\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So durch Gewohnheit wir noch zu verst\u00e4rcken pflegen;", "tokens": ["So", "durch", "Ge\u00b7wohn\u00b7heit", "wir", "noch", "zu", "ver\u00b7st\u00e4r\u00b7cken", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Begleitet allemal die Sinnen", "tokens": ["Be\u00b7glei\u00b7tet", "al\u00b7le\u00b7mal", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch dann schon, wenn wir kaum zu seyn beginnen.", "tokens": ["Auch", "dann", "schon", ",", "wenn", "wir", "kaum", "zu", "seyn", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VAINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Doch f\u00fcgt man zum Begriff, der einfach, schnell entsteht,", "tokens": ["Doch", "f\u00fcgt", "man", "zum", "Be\u00b7griff", ",", "der", "ein\u00b7fach", ",", "schnell", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPRART", "NN", "$,", "PRELS", "ADV", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch einen andern meist, der tieffer geht,", "tokens": ["Noch", "ei\u00b7nen", "an\u00b7dern", "meist", ",", "der", "tief\u00b7fer", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und welcher uns so leicht nicht irren l\u00e4sst. Es bindet", "tokens": ["Und", "wel\u00b7cher", "uns", "so", "leicht", "nicht", "ir\u00b7ren", "l\u00e4sst", ".", "Es", "bin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sich der Verstand mit dem, was man zuerst empfindet.", "tokens": ["Sich", "der", "Ver\u00b7stand", "mit", "dem", ",", "was", "man", "zu\u00b7erst", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "ART", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}