{"textgrid.poem.56264": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der vortreffliche Mantel", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Tochter, was klagst du so sehr", "tokens": ["Lie\u00b7be", "Toch\u00b7ter", ",", "was", "klagst", "du", "so", "sehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Um diesen Einen?", "tokens": ["Um", "die\u00b7sen", "Ei\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "'s gibt ja der h\u00fcbschen J\u00fcnglinge mehr,", "tokens": ["'s", "gibt", "ja", "der", "h\u00fcb\u00b7schen", "J\u00fcng\u00b7lin\u00b7ge", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "La\u00df ab zu weinen.", "tokens": ["La\u00df", "ab", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Liebe Mutter, es f\u00e4llt mir nicht ein", "tokens": ["Lie\u00b7be", "Mut\u00b7ter", ",", "es", "f\u00e4llt", "mir", "nicht", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ART"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Um ihn zu klagen;", "tokens": ["Um", "ihn", "zu", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Um den Mantel klag ich allein,", "tokens": ["Um", "den", "Man\u00b7tel", "klag", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ich will's dir sagen.", "tokens": ["Ich", "will's", "dir", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ach der gute Mantel, beschwert", "tokens": ["Ach", "der", "gu\u00b7te", "Man\u00b7tel", ",", "be\u00b7schwert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit silbernen Ketten!", "tokens": ["Mit", "sil\u00b7ber\u00b7nen", "Ket\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den behielt er noch unverzehrt,", "tokens": ["Den", "be\u00b7hielt", "er", "noch", "un\u00b7ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}