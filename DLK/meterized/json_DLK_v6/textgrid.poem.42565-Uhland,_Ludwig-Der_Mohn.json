{"textgrid.poem.42565": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Mohn", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie dort, gewiegt von Westen,", "tokens": ["Wie", "dort", ",", "ge\u00b7wiegt", "von", "Wes\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Mohnes Bl\u00fcte gl\u00e4nzt!", "tokens": ["Des", "Moh\u00b7nes", "Bl\u00fc\u00b7te", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blume, die am besten", "tokens": ["Die", "Blu\u00b7me", ",", "die", "am", "bes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Traumgotts Schl\u00e4fe kr\u00e4nzt;", "tokens": ["Des", "Traum\u00b7gotts", "Schl\u00e4\u00b7fe", "kr\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald purpurhell, als spiele", "tokens": ["Bald", "pur\u00b7pur\u00b7hell", ",", "als", "spie\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Abendr\u00f6te Schein,", "tokens": ["Der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bald wei\u00df und bleich, als fiele", "tokens": ["Bald", "wei\u00df", "und", "bleich", ",", "als", "fie\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Mondes Schimmer ein.", "tokens": ["Des", "Mon\u00b7des", "Schim\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zur Warnung h\u00f6rt ich sagen,", "tokens": ["Zur", "War\u00b7nung", "h\u00f6rt", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df, der im Mohne schlief,", "tokens": ["Da\u00df", ",", "der", "im", "Moh\u00b7ne", "schlief", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinunter ward getragen", "tokens": ["Hin\u00b7un\u00b7ter", "ward", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Tr\u00e4ume, schwer und tief;", "tokens": ["In", "Tr\u00e4u\u00b7me", ",", "schwer", "und", "tief", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Wachen selbst geblieben", "tokens": ["Dem", "Wa\u00b7chen", "selbst", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sei irren Wahnes Spur,", "tokens": ["Sei", "ir\u00b7ren", "Wah\u00b7nes", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Nahen und die Lieben", "tokens": ["Die", "Na\u00b7hen", "und", "die", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Halt' er f\u00fcr Schemen nur.", "tokens": ["Halt'", "er", "f\u00fcr", "Sche\u00b7men", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "In meiner Tage Morgen,", "tokens": ["In", "mei\u00b7ner", "Ta\u00b7ge", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lag auch ich einmal,", "tokens": ["Da", "lag", "auch", "ich", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Blumen ganz verborgen,", "tokens": ["Von", "Blu\u00b7men", "ganz", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einem sch\u00f6nen Tal.", "tokens": ["In", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Tal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie dufteten so milde!", "tokens": ["Sie", "duf\u00b7te\u00b7ten", "so", "mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da ward, ich f\u00fchlt es kaum,", "tokens": ["Da", "ward", ",", "ich", "f\u00fchlt", "es", "kaum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Leben mir zum Bilde,", "tokens": ["Das", "Le\u00b7ben", "mir", "zum", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Wirkliche zum Traum.", "tokens": ["Das", "Wirk\u00b7li\u00b7che", "zum", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seitdem ist mir best\u00e4ndig,", "tokens": ["Seit\u00b7dem", "ist", "mir", "be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Als w'r es so nur recht,", "tokens": ["Als", "w'r", "es", "so", "nur", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Bild der Welt lebendig,", "tokens": ["Mein", "Bild", "der", "Welt", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Traum nur wahr und echt;", "tokens": ["Mein", "Traum", "nur", "wahr", "und", "echt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Schatten, die ich sehe,", "tokens": ["Die", "Schat\u00b7ten", ",", "die", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie sind wie Sterne klar.", "tokens": ["Sie", "sind", "wie", "Ster\u00b7ne", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Mohn der Dichtung! wehe", "tokens": ["O", "Mohn", "der", "Dich\u00b7tung", "!", "we\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "ART", "NN", "$.", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ums Haupt mir immerdar!", "tokens": ["Ums", "Haupt", "mir", "im\u00b7mer\u00b7dar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}