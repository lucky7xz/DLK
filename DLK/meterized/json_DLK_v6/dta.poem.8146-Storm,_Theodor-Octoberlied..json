{"dta.poem.8146": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Octoberlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Schenk' ein den Wein, den holden!", "tokens": ["Schenk'", "ein", "den", "Wein", ",", "den", "hol\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir wollen uns den grauen Tag", "tokens": ["Wir", "wol\u00b7len", "uns", "den", "grau\u00b7en", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergolden, ja vergolden.", "tokens": ["Ver\u00b7gol\u00b7den", ",", "ja", "ver\u00b7gol\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und geht es drau\u00dfen noch so toll,", "tokens": ["Und", "geht", "es", "drau\u00b7\u00dfen", "noch", "so", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unchristlich oder christlich,", "tokens": ["Un\u00b7christ\u00b7lich", "o\u00b7der", "christ\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist doch die Welt, die sch\u00f6ne Welt,", "tokens": ["Ist", "doch", "die", "Welt", ",", "die", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So g\u00e4nzlich unverw\u00fcstlich!", "tokens": ["So", "g\u00e4nz\u00b7lich", "un\u00b7ver\u00b7w\u00fcst\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wimmert auch einmal das Herz, \u2014", "tokens": ["Und", "wim\u00b7mert", "auch", "ein\u00b7mal", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sto\u00df' an, und la\u00df es klingen!", "tokens": ["Sto\u00df'", "an", ",", "und", "la\u00df", "es", "klin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wissen's doch, ein rechtes Herz", "tokens": ["Wir", "wis\u00b7sen's", "doch", ",", "ein", "rech\u00b7tes", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist gar nicht umzubringen.", "tokens": ["Ist", "gar", "nicht", "um\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Nebel steigt, es f\u00e4llt das Laub;", "tokens": ["Der", "Ne\u00b7bel", "steigt", ",", "es", "f\u00e4llt", "das", "Laub", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schenk' ein den Wein, den holden!", "tokens": ["Schenk'", "ein", "den", "Wein", ",", "den", "hol\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen uns den grauen Tag", "tokens": ["Wir", "wol\u00b7len", "uns", "den", "grau\u00b7en", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergolden, ja vergolden.", "tokens": ["Ver\u00b7gol\u00b7den", ",", "ja", "ver\u00b7gol\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl ist es Herbst; doch warte nur,", "tokens": ["Wohl", "ist", "es", "Herbst", ";", "doch", "war\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch warte nur ein Weilchen!", "tokens": ["Doch", "war\u00b7te", "nur", "ein", "Weil\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Fr\u00fchling kommt, der Himmel lacht,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", ",", "der", "Him\u00b7mel", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es steht die Welt in Veilchen.", "tokens": ["Es", "steht", "die", "Welt", "in", "Veil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die blauen Tage brechen an;", "tokens": ["Die", "blau\u00b7en", "Ta\u00b7ge", "bre\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ehe sie verflie\u00dfen,", "tokens": ["Und", "e\u00b7he", "sie", "ver\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen sie, mein wackrer Freund,", "tokens": ["Wir", "wol\u00b7len", "sie", ",", "mein", "wack\u00b7rer", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Genie\u00dfen, ja genie\u00dfen!", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", ",", "ja", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}