{"dta.poem.2185": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorzug des Geistes f\u00fcr die Sch\u00f6n-  \n heit des K\u00f6rpers.  \n Aus dem Englischen \u00fcbersetzt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es haben mich nicht Z\u00fcg' und Farben dich zu bewun-", "tokens": ["Es", "ha\u00b7ben", "mich", "nicht", "Z\u00fcg'", "und", "Far\u00b7ben", "dich", "zu", "be\u00b7wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "dern angetrieben,", "tokens": ["dern", "an\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es ist der Reiz so fl\u00fccht\u2019ger Sch\u00f6nheit, um mich zu r\u00fch-", "tokens": ["Es", "ist", "der", "Reiz", "so", "fl\u00fccht'\u00b7ger", "Sch\u00f6n\u00b7heit", ",", "um", "mich", "zu", "r\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJA", "NN", "$,", "KOUI", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "ren, viel zu matt,", "tokens": ["ren", ",", "viel", "zu", "matt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es sehn an solchen Niedlichkeiten die Augen sich gar", "tokens": ["Es", "sehn", "an", "sol\u00b7chen", "Nied\u00b7lich\u00b7kei\u00b7ten", "die", "Au\u00b7gen", "sich", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "leichtlich satt:", "tokens": ["leicht\u00b7lich", "satt", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Allein, wenn man die Seele liebt, so h\u00f6ret man nicht auf", "tokens": ["Al\u00b7lein", ",", "wenn", "man", "die", "See\u00b7le", "liebt", ",", "so", "h\u00f6\u00b7ret", "man", "nicht", "auf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "PTKNEG", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "zu lieben.", "tokens": ["zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}