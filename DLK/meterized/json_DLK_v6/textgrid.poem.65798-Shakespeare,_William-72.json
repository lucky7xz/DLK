{"textgrid.poem.65798": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "72", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, da\u00df die Welt dir nicht mit Fragen droht,", "tokens": ["O", ",", "da\u00df", "die", "Welt", "dir", "nicht", "mit", "Fra\u00b7gen", "droht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Welch ein Verdienst du in mir lieben k\u00f6nnen,", "tokens": ["Welch", "ein", "Ver\u00b7dienst", "du", "in", "mir", "lie\u00b7ben", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "APPR", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergi\u00df mich, Lieber, ganz nach meinem Tod;", "tokens": ["Ver\u00b7gi\u00df", "mich", ",", "Lie\u00b7ber", ",", "ganz", "nach", "mei\u00b7nem", "Tod", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJD", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn nichts Vollkommnes kannst du an mir nennen:", "tokens": ["Denn", "nichts", "Voll\u00b7komm\u00b7nes", "kannst", "du", "an", "mir", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es w\u00e4re denn, da\u00df fromme L\u00fcgen du", "tokens": ["Es", "w\u00e4\u00b7re", "denn", ",", "da\u00df", "from\u00b7me", "L\u00fc\u00b7gen", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erf\u00e4ndest, mehr als mein Verdienst ertr\u00fcge;", "tokens": ["Er\u00b7f\u00e4n\u00b7dest", ",", "mehr", "als", "mein", "Ver\u00b7dienst", "er\u00b7tr\u00fc\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit Kr\u00e4nzen schm\u00fccktest meine Totentruh,", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", "schm\u00fcck\u00b7test", "mei\u00b7ne", "To\u00b7ten\u00b7truh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die karge Wahrheit gern herunterschl\u00fcge.", "tokens": ["Die", "kar\u00b7ge", "Wahr\u00b7heit", "gern", "her\u00b7un\u00b7ter\u00b7schl\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O, da\u00df nicht falsch dein wahres Lieben nun,", "tokens": ["O", ",", "da\u00df", "nicht", "falsch", "dein", "wah\u00b7res", "Lie\u00b7ben", "nun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PTKNEG", "ADJD", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wenn du nun Liebe l\u00f6gest, wird erfunden,", "tokens": ["Wenn", "du", "nun", "Lie\u00b7be", "l\u00f6\u00b7gest", ",", "wird", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "La\u00df bei dem Leibe meinen Namen ruhn!", "tokens": ["La\u00df", "bei", "dem", "Lei\u00b7be", "mei\u00b7nen", "Na\u00b7men", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Uns beiden zum Gewinn sei er verschwunden.", "tokens": ["Uns", "bei\u00b7den", "zum", "Ge\u00b7winn", "sei", "er", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Denn meine Fr\u00fcchte, sie besch\u00e4men mich;", "tokens": ["Denn", "mei\u00b7ne", "Fr\u00fcch\u00b7te", ",", "sie", "be\u00b7sch\u00e4\u00b7men", "mich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und so w\u00e4r Tand zu lieben, Schmach f\u00fcr dich.", "tokens": ["Und", "so", "w\u00e4r", "Tand", "zu", "lie\u00b7ben", ",", "Schmach", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "PTKZU", "VVINF", "$,", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}