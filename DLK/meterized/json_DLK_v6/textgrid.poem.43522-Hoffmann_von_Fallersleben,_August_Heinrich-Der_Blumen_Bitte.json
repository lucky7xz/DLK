{"textgrid.poem.43522": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Blumen Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging ganz fr\u00fch im k\u00fchlen Thau", "tokens": ["Ich", "ging", "ganz", "fr\u00fch", "im", "k\u00fch\u00b7len", "Thau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur gr\u00fcnen Au", "tokens": ["Zur", "gr\u00fc\u00b7nen", "Au"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wollte Blumen pfl\u00fccken.", "tokens": ["Und", "woll\u00b7te", "Blu\u00b7men", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wollte mich zum Abendtanz", "tokens": ["Ich", "woll\u00b7te", "mich", "zum", "A\u00b7bend\u00b7tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Kranz", "tokens": ["Mit", "ei\u00b7nem", "Kranz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von sch\u00f6nen Blumen schm\u00fccken.", "tokens": ["Von", "sch\u00f6\u00b7nen", "Blu\u00b7men", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da war es mir als weinten sie,", "tokens": ["Da", "war", "es", "mir", "als", "wein\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "KOKOM", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als flehten sie:", "tokens": ["Als", "fleh\u00b7ten", "sie", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbo la\u00df uns, la\u00df uns stehen!", "tokens": ["\u00bb", "o", "la\u00df", "uns", ",", "la\u00df", "uns", "ste\u00b7hen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVIMP", "PPER", "$,", "VVIMP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Eh's Abend wird und wohl noch eh'r", "tokens": ["Eh's", "A\u00b7bend", "wird", "und", "wohl", "noch", "eh'r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "KON", "ADV", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir nicht mehr,", "tokens": ["Sind", "wir", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dann ist's um uns geschehen.\u00ab", "tokens": ["Dann", "ist's", "um", "uns", "ge\u00b7sche\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Abend ward es wiederum,", "tokens": ["Und", "A\u00b7bend", "ward", "es", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und still und stumm", "tokens": ["Und", "still", "und", "stumm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sah ich die Bl\u00fcmlein stehen.", "tokens": ["Sah", "ich", "die", "Bl\u00fcm\u00b7lein", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie senkten all' ihr K\u00f6pfelein", "tokens": ["Sie", "senk\u00b7ten", "all'", "ihr", "K\u00f6p\u00b7fel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schliefen ein \u2013", "tokens": ["Und", "schlie\u00b7fen", "ein", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es war um sie geschehen.", "tokens": ["Es", "war", "um", "sie", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}