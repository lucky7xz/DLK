{"dta.poem.13684": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So felsenfester sei Dein gro\u00dfes Trachten,", "tokens": ["So", "fel\u00b7sen\u00b7fes\u00b7ter", "sei", "Dein", "gro\u00b7\u00dfes", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hau' klingend Luft Dir, ritterlich Gem\u00fcthe!", "tokens": ["Hau'", "klin\u00b7gend", "Luft", "Dir", ",", "rit\u00b7ter\u00b7lich", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "NN", "PPER", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir wollen bei Dir bleiben bis zum Tode.", "tokens": ["Wir", "wol\u00b7len", "bei", "Dir", "blei\u00b7ben", "bis", "zum", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}