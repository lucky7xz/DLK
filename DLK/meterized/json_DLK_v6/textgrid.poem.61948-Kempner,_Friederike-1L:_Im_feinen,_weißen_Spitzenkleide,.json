{"textgrid.poem.61948": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im feinen, wei\u00dfen Spitzenkleide,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im feinen, wei\u00dfen Spitzenkleide,", "tokens": ["Im", "fei\u00b7nen", ",", "wei\u00b7\u00dfen", "Spit\u00b7zen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im blonden Haar Kamelienkranz,", "tokens": ["Im", "blon\u00b7den", "Haar", "Ka\u00b7me\u00b7li\u00b7en\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Steht heut Madam', 'ne Augenweide", "tokens": ["Steht", "heut", "Ma\u00b7dam'", ",", "'ne", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Macht Toilett' beim Kerzenglanz.", "tokens": ["Macht", "Toi\u00b7lett'", "beim", "Ker\u00b7zen\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vier H\u00e4nde sind bem\u00fcht zu schm\u00fccken", "tokens": ["Vier", "H\u00e4n\u00b7de", "sind", "be\u00b7m\u00fcht", "zu", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr selig l\u00e4chelnd Angesicht,", "tokens": ["Ihr", "se\u00b7lig", "l\u00e4\u00b7chelnd", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Dies und Jenes recht zu r\u00fccken,", "tokens": ["Ihr", "Dies", "und", "Je\u00b7nes", "recht", "zu", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "KON", "PDS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch die ihr'gen ruhen nicht.", "tokens": ["Und", "auch", "die", "ih\u00b7r'\u00b7gen", "ru\u00b7hen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PPOSS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sie geht zum Ball, und dreist ich sage,", "tokens": ["Sie", "geht", "zum", "Ball", ",", "und", "dreist", "ich", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Frau ist reizend, wundersch\u00f6n,", "tokens": ["Die", "Frau", "ist", "rei\u00b7zend", ",", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie gef\u00e4llt ist keine Frage,", "tokens": ["Da\u00df", "sie", "ge\u00b7f\u00e4llt", "ist", "kei\u00b7ne", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mu\u00df ihr selbst der Neid gesteh'n!", "tokens": ["Das", "mu\u00df", "ihr", "selbst", "der", "Neid", "ge\u00b7steh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn auch nicht eingeh\u00fcllt in Flimmer,", "tokens": ["Wenn", "auch", "nicht", "ein\u00b7ge\u00b7h\u00fcllt", "in", "Flim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So spielt doch ganz dieselbe Szen'", "tokens": ["So", "spielt", "doch", "ganz", "die\u00b7sel\u00b7be", "Szen'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Herr Gemahl im Nebenzimmer,", "tokens": ["Ihr", "Herr", "Ge\u00b7mahl", "im", "Ne\u00b7ben\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der freilich etwas minder sch\u00f6n.", "tokens": ["Der", "frei\u00b7lich", "et\u00b7was", "min\u00b7der", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sehr fein ist seine Toilette,", "tokens": ["Sehr", "fein", "ist", "sei\u00b7ne", "To\u00b7i\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es gl\u00e4nzt der Ring an seiner Hand,", "tokens": ["Es", "gl\u00e4nzt", "der", "Ring", "an", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er putzt die goldene Lorgnette,", "tokens": ["Er", "putzt", "die", "gol\u00b7de\u00b7ne", "Lorg\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Setzt eine Cigarett' in Brand.", "tokens": ["Setzt", "ei\u00b7ne", "Ci\u00b7ga\u00b7rett'", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er ist schon fertig, spricht mit W\u00fcrde:", "tokens": ["Er", "ist", "schon", "fer\u00b7tig", ",", "spricht", "mit", "W\u00fcr\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbder Wagen steht f\u00fcr uns bereit,", "tokens": ["\u00bb", "der", "Wa\u00b7gen", "steht", "f\u00fcr", "uns", "be\u00b7reit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist sehr sch\u00f6n, genug der Zierde,", "tokens": ["Du", "bist", "sehr", "sch\u00f6n", ",", "ge\u00b7nug", "der", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Kind, es ist die h\u00f6chste Zeit!\u00ab", "tokens": ["Mein", "Kind", ",", "es", "ist", "die", "h\u00f6chs\u00b7te", "Zeit", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbwie gl\u00fccklich bin ich\u00ab, \u2013 ruft sie leise,", "tokens": ["\u00bb", "wie", "gl\u00fcck\u00b7lich", "bin", "ich", "\u00ab", ",", "\u2013", "ruft", "sie", "lei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PPER", "$(", "$,", "$(", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbauch ich\u00ab, \u2013 sagt lauter ihr Gemahl,", "tokens": ["\u00bb", "auch", "ich", "\u00ab", ",", "\u2013", "sagt", "lau\u00b7ter", "ihr", "Ge\u00b7mahl", ","], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "$(", "$,", "$(", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbes macht mich Deine Art und Weise", "tokens": ["\u00bb", "es", "macht", "mich", "Dei\u00b7ne", "Art", "und", "Wei\u00b7se"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr stolz auf meine gute Wahl!\u00ab", "tokens": ["Sehr", "stolz", "auf", "mei\u00b7ne", "gu\u00b7te", "Wahl", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbkomm\u00ab \u2013 sagt er, froh an Faro denkend, \u2013", "tokens": ["\u00bb", "komm", "\u00ab", "\u2013", "sagt", "er", ",", "froh", "an", "Fa\u00b7ro", "den\u00b7kend", ",", "\u2013"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$(", "$(", "VVFIN", "PPER", "$,", "ADJD", "APPR", "NE", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdir alles, alles, herrlich steht\u00ab, \u2013", "tokens": ["\u00bb", "dir", "al\u00b7les", ",", "al\u00b7les", ",", "herr\u00b7lich", "steht", "\u00ab", ",", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "PIS", "$,", "PIS", "$,", "ADJD", "VVFIN", "$(", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seinen Kopf bedenklich senkend \u2013", "tokens": ["Und", "sei\u00b7nen", "Kopf", "be\u00b7denk\u00b7lich", "sen\u00b7kend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwir kommen wahrlich heut zu sp\u00e4t!\u00ab", "tokens": ["\u00bb", "wir", "kom\u00b7men", "wahr\u00b7lich", "heut", "zu", "sp\u00e4t", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nur noch das Halsband von Demanten,", "tokens": ["Nur", "noch", "das", "Hals\u00b7band", "von", "De\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur noch die Brosche mit dem Opal,", "tokens": ["Nur", "noch", "die", "Bro\u00b7sche", "mit", "dem", "O\u00b7pal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Das Taschentuch mit den Brabanten,", "tokens": ["Das", "Ta\u00b7schen\u00b7tuch", "mit", "den", "Bra\u00b7ban\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Bl\u00fctenstrau\u00df und dann den Shawl!", "tokens": ["Den", "Bl\u00fc\u00b7ten\u00b7strau\u00df", "und", "dann", "den", "Shawl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Zu Ende ist die Toilette,", "tokens": ["Zu", "En\u00b7de", "ist", "die", "To\u00b7i\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Wahrheit ein possierlich Bild,", "tokens": ["In", "Wahr\u00b7heit", "ein", "pos\u00b7sier\u00b7lich", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Solch' Torheitseifer um die Wette,", "tokens": ["Solch'", "Tor\u00b7hei\u00b7tsei\u00b7fer", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Stets aus beschr\u00e4nktem Geiste quillt.", "tokens": ["Stets", "aus", "be\u00b7schr\u00e4nk\u00b7tem", "Geis\u00b7te", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So jung, so sch\u00f6n, so voller Freuden,", "tokens": ["So", "jung", ",", "so", "sch\u00f6n", ",", "so", "vol\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So voller Anmut und so reich,", "tokens": ["So", "vol\u00b7ler", "An\u00b7mut", "und", "so", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So eilen nun zum Ball die Beiden,", "tokens": ["So", "ei\u00b7len", "nun", "zum", "Ball", "die", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Eleganz sich selber gleich.", "tokens": ["An", "E\u00b7le\u00b7ganz", "sich", "sel\u00b7ber", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die reichen Menschenkindchen tr\u00e4umen", "tokens": ["Die", "rei\u00b7chen", "Men\u00b7schen\u00b7kind\u00b7chen", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem Moment von Ungl\u00fcck nicht,", "tokens": ["In", "dem", "Mo\u00b7ment", "von", "Un\u00b7gl\u00fcck", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieh, sich scheu die Rosse b\u00e4umen", "tokens": ["Da", "sieh", ",", "sich", "scheu", "die", "Ros\u00b7se", "b\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PRF", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor eines Mannes Angesicht.", "tokens": ["Vor", "ei\u00b7nes", "Man\u00b7nes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ein armer Mann, die Stirn voll Falten,", "tokens": ["Ein", "ar\u00b7mer", "Mann", ",", "die", "Stirn", "voll", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit stierem Auge, hohler Wang',", "tokens": ["Mit", "stie\u00b7rem", "Au\u00b7ge", ",", "hoh\u00b7ler", "Wang'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Lippen, d\u00fcnnen, bleichen, kalten,", "tokens": ["Mit", "Lip\u00b7pen", ",", "d\u00fcn\u00b7nen", ",", "blei\u00b7chen", ",", "kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch