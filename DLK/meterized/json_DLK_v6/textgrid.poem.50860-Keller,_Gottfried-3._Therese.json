{"textgrid.poem.50860": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3. Therese", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du milchjunger Knabe,", "tokens": ["Du", "milch\u00b7jun\u00b7ger", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wie schaust du mich an?", "tokens": ["Wie", "schaust", "du", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was haben deine Augen", "tokens": ["Was", "ha\u00b7ben", "dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr eine Frage getan!", "tokens": ["F\u00fcr", "ei\u00b7ne", "Fra\u00b7ge", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Alle Ratsherrn in der Stadt", "tokens": ["Al\u00b7le", "Rats\u00b7herrn", "in", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und alle Weisen der Welt", "tokens": ["Und", "al\u00b7le", "Wei\u00b7sen", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bleiben stumm auf die Frage,", "tokens": ["Blei\u00b7ben", "stumm", "auf", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die deine Augen gestellt!", "tokens": ["Die", "dei\u00b7ne", "Au\u00b7gen", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Eine Meermuschel liegt", "tokens": ["Ei\u00b7ne", "Meer\u00b7mu\u00b7schel", "liegt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auf dem Schrank meiner Bas' \u2013", "tokens": ["Auf", "dem", "Schrank", "mei\u00b7ner", "Bas'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da halte dein Ohr dran,", "tokens": ["Da", "hal\u00b7te", "dein", "Ohr", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dann h\u00f6rst du etwas!", "tokens": ["Dann", "h\u00f6rst", "du", "et\u00b7was", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}