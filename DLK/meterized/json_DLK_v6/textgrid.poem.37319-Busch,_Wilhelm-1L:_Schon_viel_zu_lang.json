{"textgrid.poem.37319": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon viel zu lang", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon viel zu lang", "tokens": ["Schon", "viel", "zu", "lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKA", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hab ich der Bosheit mich ergeben.", "tokens": ["Hab", "ich", "der", "Bos\u00b7heit", "mich", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lasse t\u00f6ten, um zu leben,", "tokens": ["Ich", "las\u00b7se", "t\u00f6\u00b7ten", ",", "um", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und b\u00f6s macht bang.", "tokens": ["Und", "b\u00f6s", "macht", "bang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Denn niemals ruht", "tokens": ["Denn", "nie\u00b7mals", "ruht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Stimme in des Herzens Tiefe,", "tokens": ["Die", "Stim\u00b7me", "in", "des", "Her\u00b7zens", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob es z\u00e4rtlich klagend riefe:", "tokens": ["Als", "ob", "es", "z\u00e4rt\u00b7lich", "kla\u00b7gend", "rie\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei wieder gut.", "tokens": ["Sei", "wie\u00b7der", "gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und frisch vom Baum", "tokens": ["Und", "frisch", "vom", "Baum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den allersch\u00f6nsten Apfel brach ich.", "tokens": ["Den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Ap\u00b7fel", "brach", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bi\u00df hinein, und seufzend sprach ich", "tokens": ["Ich", "bi\u00df", "hin\u00b7ein", ",", "und", "seuf\u00b7zend", "sprach", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie halb im Traum:", "tokens": ["Wie", "halb", "im", "Traum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du erstes Gl\u00fcck,", "tokens": ["Du", "ers\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du alter Paradiesesfrieden,", "tokens": ["Du", "al\u00b7ter", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da noch kein Lamm den Wolf gemieden,", "tokens": ["Da", "noch", "kein", "Lamm", "den", "Wolf", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O komm zur\u00fcck!", "tokens": ["O", "komm", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}