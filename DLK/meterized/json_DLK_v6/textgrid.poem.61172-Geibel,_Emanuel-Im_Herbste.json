{"textgrid.poem.61172": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbste", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf des Gartens Mauerzinne", "tokens": ["Auf", "des", "Gar\u00b7tens", "Mau\u00b7er\u00b7zin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bebt noch eine einz'ge Ranke:", "tokens": ["Bebt", "noch", "ei\u00b7ne", "einz'\u00b7ge", "Ran\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also bebt in meinem Sinne", "tokens": ["Al\u00b7so", "bebt", "in", "mei\u00b7nem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmerzlich nur noch ein Gedanke.", "tokens": ["Schmerz\u00b7lich", "nur", "noch", "ein", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum vermag ich ihn zu fassen,", "tokens": ["Kaum", "ver\u00b7mag", "ich", "ihn", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber dennoch von mir lassen", "tokens": ["A\u00b7ber", "den\u00b7noch", "von", "mir", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will er, ach, zu keiner Frist;", "tokens": ["Will", "er", ",", "ach", ",", "zu", "kei\u00b7ner", "Frist", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ITJ", "$,", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so denk' ich ihn und trage", "tokens": ["Und", "so", "denk'", "ich", "ihn", "und", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle N\u00e4chte, alle Tage", "tokens": ["Al\u00b7le", "N\u00e4ch\u00b7te", ",", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit mir fort die dumpfe Klage,", "tokens": ["Mit", "mir", "fort", "die", "dump\u00b7fe", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df du mir verloren bist.", "tokens": ["Da\u00df", "du", "mir", "ver\u00b7lo\u00b7ren", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}