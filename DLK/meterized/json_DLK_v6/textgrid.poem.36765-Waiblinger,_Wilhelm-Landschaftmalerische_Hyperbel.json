{"textgrid.poem.36765": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Landschaftmalerische Hyperbel", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du den Himmel gesehn heut' Abend? \u2013 \u00bbNein, und wie war er?\u00ab \u2013", "tokens": ["Hast", "du", "den", "Him\u00b7mel", "ge\u00b7sehn", "heut'", "A\u00b7bend", "?", "\u2013", "\u00bb", "Nein", ",", "und", "wie", "war", "er", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "ADV", "NN", "$.", "$(", "$(", "PTKANT", "$,", "KON", "PWAV", "VAFIN", "PPER", "$.", "$(", "$("], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O welch herrlicher Claud, ach welch ein himmlischer Ton!", "tokens": ["O", "welch", "herr\u00b7li\u00b7cher", "Claud", ",", "ach", "welch", "ein", "himm\u00b7li\u00b7scher", "Ton", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NE", "$,", "XY", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "\u00bbund die Campagna, wie war sie?\u00ab \u2013 Nur Claud in Himmel und Erde,", "tokens": ["\u00bb", "und", "die", "Cam\u00b7pag\u00b7na", ",", "wie", "war", "sie", "?", "\u00ab", "\u2013", "Nur", "Claud", "in", "Him\u00b7mel", "und", "Er\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "$.", "$(", "$(", "ADV", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jeglichen Pinselstrich hab' in der Luft ich gesehn!", "tokens": ["Jeg\u00b7li\u00b7chen", "Pin\u00b7sel\u00b7strich", "hab'", "in", "der", "Luft", "ich", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}}}}