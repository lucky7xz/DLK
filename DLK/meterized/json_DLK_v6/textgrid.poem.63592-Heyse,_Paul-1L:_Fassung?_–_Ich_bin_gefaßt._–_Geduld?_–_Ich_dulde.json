{"textgrid.poem.63592": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fassung? \u2013 Ich bin gefa\u00dft. \u2013 Geduld? \u2013 Ich dulde", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fassung? \u2013 Ich bin gefa\u00dft. \u2013 Geduld? \u2013 Ich dulde", "tokens": ["Fas\u00b7sung", "?", "\u2013", "Ich", "bin", "ge\u00b7fa\u00dft", ".", "\u2013", "Ge\u00b7duld", "?", "\u2013", "Ich", "dul\u00b7de"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "PPER", "VAFIN", "VVPP", "$.", "$(", "NN", "$.", "$(", "PPER", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aufb\u00e4umen wider das gewalt'ge Mu\u00df", "tokens": ["Auf\u00b7b\u00e4u\u00b7men", "wi\u00b7der", "das", "ge\u00b7walt'\u00b7ge", "Mu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist eine Torheit, die ich nicht verschulde.", "tokens": ["Ist", "ei\u00b7ne", "Tor\u00b7heit", ",", "die", "ich", "nicht", "ver\u00b7schul\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df, in strenger Kette, Schlu\u00df an Schlu\u00df,", "tokens": ["Ich", "wei\u00df", ",", "in", "stren\u00b7ger", "Ket\u00b7te", ",", "Schlu\u00df", "an", "Schlu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Reiht sich der Wandel aller ird'schen Dinge,", "tokens": ["Reiht", "sich", "der", "Wan\u00b7del", "al\u00b7ler", "ird'\u00b7schen", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und unaufhaltsam rinnt des Werdens Flu\u00df.", "tokens": ["Und", "un\u00b7auf\u00b7halt\u00b7sam", "rinnt", "des", "Wer\u00b7dens", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur da\u00df zum ", "tokens": ["Nur", "da\u00df", "zum"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART"], "meter": "++-", "measure": "unknown.measure.di"}, "line.8": {"text": "Wenn ich beraubt ward, da\u00df ich, wenn der Geier", "tokens": ["Wenn", "ich", "be\u00b7raubt", "ward", ",", "da\u00df", "ich", ",", "wenn", "der", "Gei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "An meiner Leber zehrt, Tedeum singe,", "tokens": ["An", "mei\u00b7ner", "Le\u00b7ber", "zehrt", ",", "Te\u00b7de\u00b7um", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Da\u00df hinter jenem niegehobnen Schleier", "tokens": ["Da\u00df", "hin\u00b7ter", "je\u00b7nem", "nie\u00b7ge\u00b7hob\u00b7nen", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich eine Macht mir tr\u00e4umte ", "tokens": ["Ich", "ei\u00b7ne", "Macht", "mir", "tr\u00e4um\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und Huldigung ihr stamml' in frommer Feier:", "tokens": ["Und", "Hul\u00b7di\u00b7gung", "ihr", "stamml'", "in", "from\u00b7mer", "Fei\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das fordre niemand. Weder Ha\u00df noch Groll,", "tokens": ["Das", "ford\u00b7re", "nie\u00b7mand", ".", "We\u00b7der", "Ha\u00df", "noch", "Groll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$.", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Noch minder Liebe trag' ich jenem Einen,", "tokens": ["Noch", "min\u00b7der", "Lie\u00b7be", "trag'", "ich", "je\u00b7nem", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "PPER", "PDAT", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der alles ist und wirket, was er soll.", "tokens": ["Der", "al\u00b7les", "ist", "und", "wir\u00b7ket", ",", "was", "er", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "KON", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ich bin ein Teil von ihm, samt allem Meinen.", "tokens": ["Ich", "bin", "ein", "Teil", "von", "ihm", ",", "samt", "al\u00b7lem", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "APPR", "PIS", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wie winzig ihm, der auf das Ganze denkt,", "tokens": ["Wie", "win\u00b7zig", "ihm", ",", "der", "auf", "das", "Gan\u00b7ze", "denkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mu\u00df des Atoms, des St\u00e4ubchens Weh erscheinen!", "tokens": ["Mu\u00df", "des", "A\u00b7toms", ",", "des", "St\u00e4ub\u00b7chens", "Weh", "er\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "\u00c4onenlang hat er das Sein gelenkt", "tokens": ["\u00c4\u00b7o\u00b7nen\u00b7lang", "hat", "er", "das", "Sein", "ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "An seiner Brauen Wink. Soll er's nun achten,", "tokens": ["An", "sei\u00b7ner", "Brau\u00b7en", "Wink", ".", "Soll", "er's", "nun", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wenn eine M\u00fccke sich am Licht versengt?", "tokens": ["Wenn", "ei\u00b7ne", "M\u00fc\u00b7cke", "sich", "am", "Licht", "ver\u00b7sengt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Urew'ger Ziele Bahn mu\u00df er betrachten,", "tokens": ["U\u00b7re\u00b7w'\u00b7ger", "Zie\u00b7le", "Bahn", "mu\u00df", "er", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "Vielleicht unselig selbst, unfroh gewi\u00df;", "tokens": ["Viel\u00b7leicht", "un\u00b7se\u00b7lig", "selbst", ",", "un\u00b7froh", "ge\u00b7wi\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "ADJD", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Denn wo sind Freuden, die ihn jauchzen machten?", "tokens": ["Denn", "wo", "sind", "Freu\u00b7den", ",", "die", "ihn", "jauch\u00b7zen", "mach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "NN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und darum h\u00fcllt er sich in Finsternis,", "tokens": ["Und", "da\u00b7rum", "h\u00fcllt", "er", "sich", "in", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Als scheu' er sich, sein Angesicht zu zeigen", "tokens": ["Als", "scheu'", "er", "sich", ",", "sein", "An\u00b7ge\u00b7sicht", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Elenden, die er in das Sein verstie\u00df,", "tokens": ["E\u00b7len\u00b7den", ",", "die", "er", "in", "das", "Sein", "ver\u00b7stie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.28": {"text": "Unwissend, nur gewissem Tod zu eigen.", "tokens": ["Un\u00b7wis\u00b7send", ",", "nur", "ge\u00b7wis\u00b7sem", "Tod", "zu", "ei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJA", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und ihm, dem Unerforschlichen, der nie", "tokens": ["Und", "ihm", ",", "dem", "Un\u00b7er\u00b7for\u00b7schli\u00b7chen", ",", "der", "nie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Mir brechen will sein unnahbares Schweigen,", "tokens": ["Mir", "bre\u00b7chen", "will", "sein", "un\u00b7nah\u00b7ba\u00b7res", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Ihm sollt' ich kindlich liebewarm das Knie", "tokens": ["Ihm", "sollt'", "ich", "kind\u00b7lich", "lie\u00b7be\u00b7warm", "das", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Umfassen, gut' und b\u00f6se Gabe danken,", "tokens": ["Um\u00b7fas\u00b7sen", ",", "gut'", "und", "b\u00f6\u00b7se", "Ga\u00b7be", "dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Im Wahn, da\u00df er sie ", "tokens": ["Im", "Wahn", ",", "da\u00df", "er", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.34": {"text": "Niemals! Uns trennen himmelhohe Schranken.", "tokens": ["Nie\u00b7mals", "!", "Uns", "tren\u00b7nen", "him\u00b7mel\u00b7ho\u00b7he", "Schran\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVINF", "ADJA", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.35": {"text": "Mu\u00df er mich leiden lassen, sei's darum!", "tokens": ["Mu\u00df", "er", "mich", "lei\u00b7den", "las\u00b7sen", ",", "sei's", "da\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$,", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Dem Weltall dient vielleicht des Wurmes Kranken.", "tokens": ["Dem", "Wel\u00b7tall", "dient", "viel\u00b7leicht", "des", "Wur\u00b7mes", "Kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Doch eh' mir seine Weisheit das Warum", "tokens": ["Doch", "eh'", "mir", "sei\u00b7ne", "Weis\u00b7heit", "das", "Wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Nicht offenbart, schweigt mir von Vaterg\u00fcte!", "tokens": ["Nicht", "of\u00b7fen\u00b7bart", ",", "schweigt", "mir", "von", "Va\u00b7ter\u00b7g\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Wo blieb' ein Vater seinem Kinde stumm,", "tokens": ["Wo", "blieb'", "ein", "Va\u00b7ter", "sei\u00b7nem", "Kin\u00b7de", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Wenn schon aus einem Wort ihm Trost erbl\u00fchte?", "tokens": ["Wenn", "schon", "aus", "ei\u00b7nem", "Wort", "ihm", "Trost", "er\u00b7bl\u00fch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}