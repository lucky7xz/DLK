{"textgrid.poem.60774": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "24. An die Gr\u00e4fin Pieri in Siena", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nheit fielen und Reiz wenigen Fraun anheim,", "tokens": ["Sch\u00f6n\u00b7heit", "fie\u00b7len", "und", "Reiz", "we\u00b7ni\u00b7gen", "Fraun", "an\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auch Reicht\u00fcmer verschenkt selten ein g\u00fcnstig Los;", "tokens": ["Auch", "Reich\u00b7t\u00fc\u00b7mer", "ver\u00b7schenkt", "sel\u00b7ten", "ein", "g\u00fcns\u00b7tig", "Los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADJD", "ART", "ADJD", "NE", "$."], "meter": "-+---+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Doch viel seltener gibt es", "tokens": ["Doch", "viel", "sel\u00b7te\u00b7ner", "gibt", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein teilnehmendes, gro\u00dfes Herz,", "tokens": ["Ein", "teil\u00b7neh\u00b7men\u00b7des", ",", "gro\u00b7\u00dfes", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Dem Sch\u00f6nheit es und auch Gaben des Gl\u00fccks gesellt:", "tokens": ["Dem", "Sch\u00f6n\u00b7heit", "es", "und", "auch", "Ga\u00b7ben", "des", "Gl\u00fccks", "ge\u00b7sellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "ADV", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Also seh ich vereint w\u00fcrdigem Gatten dich,", "tokens": ["Al\u00b7so", "seh", "ich", "ver\u00b7eint", "w\u00fcr\u00b7di\u00b7gem", "Gat\u00b7ten", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "VAFIN", "NN", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Rastlos t\u00e4tigem Dasein", "tokens": ["Rast\u00b7los", "t\u00e4\u00b7ti\u00b7gem", "Da\u00b7sein"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Prunk nicht, aber Gehalt verleihn.", "tokens": ["Prunk", "nicht", ",", "a\u00b7ber", "Ge\u00b7halt", "ver\u00b7leihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KON", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Dichtkunst hebt und Musik, wahre Geselligkeit", "tokens": ["Dicht\u00b7kunst", "hebt", "und", "Mu\u00b7sik", ",", "wah\u00b7re", "Ge\u00b7sel\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "NN", "$,", "ADJA", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hebt dein Leben empor (wie es der Deutschen ziemt)", "tokens": ["Hebt", "dein", "Le\u00b7ben", "em\u00b7por", "(", "wie", "es", "der", "Deut\u00b7schen", "ziemt", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "PWAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aus einf\u00f6rmigem Kreislauf,", "tokens": ["Aus", "ein\u00b7f\u00f6r\u00b7mi\u00b7gem", "Kreis\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Den schlaftrunken Italien tr\u00e4umt.", "tokens": ["Den", "schlaf\u00b7trun\u00b7ken", "I\u00b7ta\u00b7li\u00b7en", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Gastfreundschaftlichen Sinns nahmst du den Dichter auf,", "tokens": ["Gast\u00b7freund\u00b7schaft\u00b7li\u00b7chen", "Sinns", "nahmst", "du", "den", "Dich\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dankbar bietet er dir liebenden Scheidegru\u00df,", "tokens": ["Dank\u00b7bar", "bie\u00b7tet", "er", "dir", "lie\u00b7ben\u00b7den", "Schei\u00b7de\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Weil aufs neue der Fr\u00fchling", "tokens": ["Weil", "aufs", "neu\u00b7e", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihn zum fl\u00fcchtigen Wandrer macht.", "tokens": ["Ihn", "zum", "fl\u00fcch\u00b7ti\u00b7gen", "Wand\u00b7rer", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n ist's, h\u00e4uslichen Kreis sammeln umher, wiewohl", "tokens": ["Sch\u00f6n", "ist's", ",", "h\u00e4us\u00b7li\u00b7chen", "Kreis", "sam\u00b7meln", "um\u00b7her", ",", "wie\u00b7wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "KOUS"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sch\u00f6n nicht minder, sich selbst lebend und frei von Zwang", "tokens": ["Sch\u00f6n", "nicht", "min\u00b7der", ",", "sich", "selbst", "le\u00b7bend", "und", "frei", "von", "Zwang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "$,", "PRF", "ADV", "ADJD", "KON", "ADJD", "APPR", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Anschaun St\u00e4dte der Menschen,", "tokens": ["An\u00b7schaun", "St\u00e4d\u00b7te", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Stehn auf hohem Verdeck zu Schiff.", "tokens": ["Stehn", "auf", "ho\u00b7hem", "Ver\u00b7deck", "zu", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}