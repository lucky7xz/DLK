{"textgrid.poem.58406": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "444. Bleib stille daheim!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ungern bleibt man doch zu Haus,", "tokens": ["Wie", "un\u00b7gern", "bleibt", "man", "doch", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tausendk\u00fcnstler lockt heraus!", "tokens": ["Der", "Tau\u00b7send\u00b7k\u00fcnst\u00b7ler", "lockt", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man meint viel Gutes zu verrichten,", "tokens": ["Man", "meint", "viel", "Gu\u00b7tes", "zu", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir schaden nur durch unser Tun,", "tokens": ["Wir", "scha\u00b7den", "nur", "durch", "un\u00b7ser", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir n\u00fctzen, wenn wir stille ruhn", "tokens": ["Wir", "n\u00fct\u00b7zen", ",", "wenn", "wir", "stil\u00b7le", "ruhn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lassen uns von Gott vernichten.", "tokens": ["Und", "las\u00b7sen", "uns", "von", "Gott", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}