{"textgrid.poem.56317": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod des R\u00e4ubers", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem S\u00f6ldner zahlt den ausgerufnen Preis! \u2013", "tokens": ["Dem", "S\u00f6ld\u00b7ner", "zahlt", "den", "aus\u00b7ge\u00b7ruf\u00b7nen", "Preis", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sonst um Romas Mauern weit im Kreis", "tokens": ["Der", "sonst", "um", "Ro\u00b7mas", "Mau\u00b7ern", "weit", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NE", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gemordet und geraubt, liegt \u00fcberwunden;", "tokens": ["Ge\u00b7mor\u00b7det", "und", "ge\u00b7raubt", ",", "liegt", "\u00fc\u00b7berw\u00b7un\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Schreckliche verspritzt aus tiefen Wunden", "tokens": ["Der", "Schreck\u00b7li\u00b7che", "ver\u00b7spritzt", "aus", "tie\u00b7fen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein Blut so hei\u00df.", "tokens": ["Sein", "Blut", "so", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Seinen haben ihn hinabgetragen", "tokens": ["Die", "Sei\u00b7nen", "ha\u00b7ben", "ihn", "hin\u00b7ab\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSS", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In ihre H\u00f6hle, wo beim Fackelschein", "tokens": ["In", "ih\u00b7re", "H\u00f6h\u00b7le", ",", "wo", "beim", "Fa\u00b7ckel\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Um den Gefallnen sie gekauert klagen;", "tokens": ["Um", "den", "Ge\u00b7fall\u00b7nen", "sie", "ge\u00b7kau\u00b7ert", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Alte liegt besinnungslos, allein", "tokens": ["Der", "Al\u00b7te", "liegt", "be\u00b7sin\u00b7nungs\u00b7los", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Pulse schlagen.", "tokens": ["Die", "Pul\u00b7se", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der sp\u00e4ht, indem den Brand er n\u00e4her schiebt,", "tokens": ["Der", "sp\u00e4ht", ",", "in\u00b7dem", "den", "Brand", "er", "n\u00e4\u00b7her", "schiebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob er kein Lebenszeichen von sich gibt;", "tokens": ["Ob", "er", "kein", "Le\u00b7bens\u00b7zei\u00b7chen", "von", "sich", "gibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der spricht, indem er geht das Grab zu graben", "tokens": ["Der", "spricht", ",", "in\u00b7dem", "er", "geht", "das", "Grab", "zu", "gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seine Tr\u00e4nen er verschluckt: \u00bbWie haben", "tokens": ["Und", "sei\u00b7ne", "Tr\u00e4\u00b7nen", "er", "ver\u00b7schluckt", ":", "\u00bb", "Wie", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVPP", "$.", "$(", "PWAV", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir ihn geliebt!", "tokens": ["Wir", "ihn", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die um das Sterbebett des Pabstes weilen,", "tokens": ["Die", "um", "das", "Ster\u00b7be\u00b7bett", "des", "Pabs\u00b7tes", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie haben nicht f\u00fcr ihn die Herzlichkeit.", "tokens": ["Sie", "ha\u00b7ben", "nicht", "f\u00fcr", "ihn", "die", "Herz\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie wu\u00dft er zu der Pl\u00fcnderung zu eilen!", "tokens": ["Wie", "wu\u00dft", "er", "zu", "der", "Pl\u00fcn\u00b7de\u00b7rung", "zu", "ei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie stark im Kampf und welche Ehrlichkeit", "tokens": ["Wie", "stark", "im", "Kampf", "und", "wel\u00b7che", "Ehr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "KON", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sodann beim Teilen!", "tokens": ["So\u00b7dann", "beim", "Tei\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er war ein echter Christ vom alten Schlag,", "tokens": ["Er", "war", "ein", "ech\u00b7ter", "Christ", "vom", "al\u00b7ten", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er hielt die Fasten, wie nur einer mag,", "tokens": ["Er", "hielt", "die", "Fas\u00b7ten", ",", "wie", "nur", "ei\u00b7ner", "mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die heil'ge Kirche nebst den Heil'gen ehrt' er,", "tokens": ["Die", "heil'\u00b7ge", "Kir\u00b7che", "nebst", "den", "Heil'\u00b7gen", "ehrt'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Raub und Mord, und jedes Werk verwehrt' er", "tokens": ["Und", "Raub", "und", "Mord", ",", "und", "je\u00b7des", "Werk", "ver\u00b7wehrt'", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Feiertag.", "tokens": ["Am", "Fei\u00b7er\u00b7tag", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da hatte nicht ein Christenkind zu beben,", "tokens": ["Da", "hat\u00b7te", "nicht", "ein", "Chris\u00b7ten\u00b7kind", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Ketzer durfte nur, wie sich's geb\u00fchrt,", "tokens": ["Der", "Ket\u00b7zer", "durf\u00b7te", "nur", ",", "wie", "sich's", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Engel\u00e4nder uns zu schaffen geben. \u2013", "tokens": ["Der", "En\u00b7ge\u00b7l\u00e4n\u00b7der", "uns", "zu", "schaf\u00b7fen", "ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Beeifert euch, wenn's so zu sterben f\u00fchrt,", "tokens": ["Be\u00b7ei\u00b7fert", "euch", ",", "wenn's", "so", "zu", "ster\u00b7ben", "f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Noch fromm zu leben!", "tokens": ["Noch", "fromm", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nun regt er sich, erwartet sein Gebot!\u00ab \u2013", "tokens": ["Nun", "regt", "er", "sich", ",", "er\u00b7war\u00b7tet", "sein", "Ge\u00b7bot", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er streckt die Hand aus, breit und blutig rot,", "tokens": ["Er", "streckt", "die", "Hand", "aus", ",", "breit", "und", "blu\u00b7tig", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie suchet seine Flinte noch zu fassen;", "tokens": ["Sie", "su\u00b7chet", "sei\u00b7ne", "Flin\u00b7te", "noch", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht will er von der alten Waffe lassen,", "tokens": ["Nicht", "will", "er", "von", "der", "al\u00b7ten", "Waf\u00b7fe", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht in den Tod.", "tokens": ["Nicht", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Sie war so manche Jahre sein getreuer,", "tokens": ["Sie", "war", "so", "man\u00b7che", "Jah\u00b7re", "sein", "ge\u00b7treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sein einziger Besch\u00fctzer und Geno\u00df;", "tokens": ["Sein", "ein\u00b7zi\u00b7ger", "Be\u00b7sch\u00fct\u00b7zer", "und", "Ge\u00b7no\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Er freut sich ihrer, die er h\u00e4lt so teuer,", "tokens": ["Er", "freut", "sich", "ih\u00b7rer", ",", "die", "er", "h\u00e4lt", "so", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "$,", "PRELS", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Versucht mit starrem Finger noch das Schlo\u00df \u2013", "tokens": ["Ver\u00b7sucht", "mit", "star\u00b7rem", "Fin\u00b7ger", "noch", "das", "Schlo\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da gibt sie Feuer.", "tokens": ["Da", "gibt", "sie", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbschon gut, du kennst mich noch; \u2013 indessen rafft", "tokens": ["\u00bb", "schon", "gut", ",", "du", "kennst", "mich", "noch", ";", "\u2013", "in\u00b7des\u00b7sen", "rafft"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der S\u00f6ldner mich inmitten meiner Kraft;", "tokens": ["Der", "S\u00f6ld\u00b7ner", "mich", "in\u00b7mit\u00b7ten", "mei\u00b7ner", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann nicht selber meine Rache nehmen;", "tokens": ["Ich", "kann", "nicht", "sel\u00b7ber", "mei\u00b7ne", "Ra\u00b7che", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du mu\u00dft dich einer st\u00e4rkern Hand bequemen,", "tokens": ["Du", "mu\u00dft", "dich", "ei\u00b7ner", "st\u00e4r\u00b7kern", "Hand", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Rache schafft.", "tokens": ["Die", "Ra\u00b7che", "schafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Durch dich getroffen mu\u00df der Wicht erstarren,", "tokens": ["Durch", "dich", "ge\u00b7trof\u00b7fen", "mu\u00df", "der", "Wicht", "er\u00b7star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Den schuldest du mir noch, versage nicht;", "tokens": ["Den", "schul\u00b7dest", "du", "mir", "noch", ",", "ver\u00b7sa\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADV", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie werden in die Erde mich verscharren,", "tokens": ["Sie", "wer\u00b7den", "in", "die", "Er\u00b7de", "mich", "ver\u00b7schar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Drei Tage geb ich Zeit, tu deine Pflicht,", "tokens": ["Drei", "Ta\u00b7ge", "geb", "ich", "Zeit", ",", "tu", "dei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich werde harren.\u00ab", "tokens": ["Ich", "wer\u00b7de", "har\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Des Weges zog ein M\u00f6nch von ungef\u