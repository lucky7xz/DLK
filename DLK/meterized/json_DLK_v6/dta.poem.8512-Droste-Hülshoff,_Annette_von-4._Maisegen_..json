{"dta.poem.8512": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "4.  \n  Maisegen .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mai ist eingezogen,", "tokens": ["Der", "Mai", "ist", "ein\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon pflanzt\u2019 er sein Panier", "tokens": ["Schon", "pflanzt'", "er", "sein", "Pa\u00b7nier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am dunklen Himmelsbogen", "tokens": ["Am", "dunk\u00b7len", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit blanker Sterne Zier.", "tokens": ["Mit", "blan\u00b7ker", "Ster\u00b7ne", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die wilden Wasser brausen", "tokens": ["Die", "wil\u00b7den", "Was\u00b7ser", "brau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und r\u00fctteln aus den Klausen", "tokens": ["Und", "r\u00fct\u00b7teln", "aus", "den", "Klau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Rellmaus und Murmelthier.", "tokens": ["Rell\u00b7maus", "und", "Mur\u00b7melt\u00b7hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eob wohl das Gletschereis den Strom ged\u00e4mmt?", "tokens": ["\u201e", "ob", "wohl", "das", "Glet\u00b7sche\u00b7reis", "den", "Strom", "ge\u00b7d\u00e4mmt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von mancher H\u00fctte geht\u2019s auf schlimmen Wegen,", "tokens": ["Von", "man\u00b7cher", "H\u00fct\u00b7te", "geht's", "auf", "schlim\u00b7men", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sturm hat alle Firnen kahl gek\u00e4mmt,", "tokens": ["Der", "Sturm", "hat", "al\u00b7le", "Fir\u00b7nen", "kahl", "ge\u00b7k\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gestern wie aus R\u00f6hren scho\u00df der Regen.", "tokens": ["Und", "ge\u00b7stern", "wie", "aus", "R\u00f6h\u00b7ren", "scho\u00df", "der", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Adieu, Jeanette, nicht l\u00e4nger mich gehemmt!", "tokens": ["A\u00b7die\u00b7u", ",", "Jea\u00b7net\u00b7te", ",", "nicht", "l\u00e4n\u00b7ger", "mich", "ge\u00b7hemmt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "PTKNEG", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Adieu, ich mu\u00df, es gilt den Maiensegen;", "tokens": ["A\u00b7die\u00b7u", ",", "ich", "mu\u00df", ",", "es", "gilt", "den", "Mai\u00b7en\u00b7se\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Wenn vier es schl\u00e4gt im Thurme zu Escout,", "tokens": ["Wenn", "vier", "es", "schl\u00e4gt", "im", "Thur\u00b7me", "zu", "Es\u00b7cout", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mu\u00df jeder Senner stehen am Pointe de Droux.\u201c", "tokens": ["Mu\u00df", "je\u00b7der", "Sen\u00b7ner", "ste\u00b7hen", "am", "Poin\u00b7te", "de", "Droux", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVFIN", "APPRART", "NN", "NE", "NE", "$.", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie trunken schau\u2019n die Klippen,", "tokens": ["Wie", "trun\u00b7ken", "schau'n", "die", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie taumelnd in die Schlucht!", "tokens": ["Wie", "tau\u00b7melnd", "in", "die", "Schlucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als nickten sie, zu nippen", "tokens": ["Als", "nick\u00b7ten", "sie", ",", "zu", "nip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Sturzbach auf der Flucht.", "tokens": ["Vom", "Sturz\u00b7bach", "auf", "der", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da ist ein rasselnd Klingen,", "tokens": ["Da", "ist", "ein", "ras\u00b7selnd", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man h\u00f6rt die Schollen springen,", "tokens": ["Man", "h\u00f6rt", "die", "Schol\u00b7len", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und brechen an der Bucht.", "tokens": ["Und", "bre\u00b7chen", "an", "der", "Bucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf allen Wegen zieh\u2019n Laternen um", "tokens": ["Auf", "al\u00b7len", "We\u00b7gen", "zieh'n", "La\u00b7ter\u00b7nen", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jedes Passes Echo wecken Schritte.", "tokens": ["Und", "je\u00b7des", "Pas\u00b7ses", "E\u00b7cho", "we\u00b7cken", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Habt Acht, habt Acht, die Nacht ist blind und stumm,", "tokens": ["Habt", "Acht", ",", "habt", "Acht", ",", "die", "Nacht", "ist", "blind", "und", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "$,", "VAFIN", "CARD", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Schneefluth fra\u00df an manches Blockes Kitte;", "tokens": ["Die", "Schnee\u00b7fluth", "fra\u00df", "an", "man\u00b7ches", "Blo\u00b7ckes", "Kit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Habt Acht, h\u00f6rt ihr des B\u00e4ren tief Gebrumm?", "tokens": ["Habt", "Acht", ",", "h\u00f6rt", "ihr", "des", "B\u00e4\u00b7ren", "tief", "Ge\u00b7brumm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dort ist sein Lager an des Riffes Mitte;", "tokens": ["Dort", "ist", "sein", "La\u00b7ger", "an", "des", "Rif\u00b7fes", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und dort die schiefe Klippenbank, f\u00fcrwahr!", "tokens": ["Und", "dort", "die", "schie\u00b7fe", "Klip\u00b7pen\u00b7bank", ",", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie hing schon los am ersten Februar.", "tokens": ["Sie", "hing", "schon", "los", "am", "ers\u00b7ten", "Feb\u00b7ru\u00b7ar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun sprie\u00dfen blasse Rosen", "tokens": ["Nun", "sprie\u00b7\u00dfen", "blas\u00b7se", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Gletscherbord hervor,", "tokens": ["Am", "Glet\u00b7scher\u00b7bord", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit der D\u00e4mmrung kosen", "tokens": ["Und", "mit", "der", "D\u00e4mm\u00b7rung", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will schon das Klippenthor;", "tokens": ["Will", "schon", "das", "Klip\u00b7pen\u00b7thor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon schwimmen lichte Streifen,", "tokens": ["Schon", "schwim\u00b7men", "lich\u00b7te", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es lockt der Gemse Pfeifen", "tokens": ["Es", "lockt", "der", "Gem\u00b7se", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Blick zum Grat empor.", "tokens": ["Den", "Blick", "zum", "Grat", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Verl\u00f6scht sind die Laternen, und im Kreis", "tokens": ["Ver\u00b7l\u00f6scht", "sind", "die", "La\u00b7ter\u00b7nen", ",", "und", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Steht eine Hirtenschaar auf breiter Platte,", "tokens": ["Steht", "ei\u00b7ne", "Hir\u00b7ten\u00b7schaar", "auf", "brei\u00b7ter", "Plat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Voran der Patriarch, wie Silber wei\u00df", "tokens": ["Vo\u00b7ran", "der", "Pat\u00b7ri\u00b7arch", ",", "wie", "Sil\u00b7ber", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4ngt um sein tiefgebr\u00e4unt Gesicht das glatte,", "tokens": ["H\u00e4ngt", "um", "sein", "tief\u00b7ge\u00b7br\u00e4unt", "Ge\u00b7sicht", "das", "glat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gestr\u00e4hlte Haar, und Alle beten leis", "tokens": ["Ge\u00b7str\u00e4hl\u00b7te", "Haar", ",", "und", "Al\u00b7le", "be\u00b7ten", "leis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach Osten schauend, wo das farbensatte", "tokens": ["Nach", "Os\u00b7ten", "schau\u00b7end", ",", "wo", "das", "far\u00b7ben\u00b7sat\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Rubingew\u00f6lk mit glitzerndem Geroll", "tokens": ["Ru\u00b7bin\u00b7ge\u00b7w\u00f6lk", "mit", "glit\u00b7zern\u00b7dem", "Ge\u00b7roll"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Die stolze Sonnenkugel bringen soll.", "tokens": ["Die", "stol\u00b7ze", "Son\u00b7nen\u00b7ku\u00b7gel", "brin\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da kommt sie aufgefahren", "tokens": ["Da", "kommt", "sie", "auf\u00b7ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In strenger Majest\u00e4t,", "tokens": ["In", "stren\u00b7ger", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von den Firnaltaren", "tokens": ["Und", "von", "den", "Fir\u00b7nal\u00b7ta\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Opferflamme weht:", "tokens": ["Die", "Op\u00b7fer\u00b7flam\u00b7me", "weht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sinken in der Runde", "tokens": ["Da", "sin\u00b7ken", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So Knie an Knie, dem Munde", "tokens": ["So", "Knie", "an", "Knie", ",", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Entstr\u00f6mt das Maigebet:", "tokens": ["Ent\u00b7str\u00f6mt", "das", "Mai\u00b7ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201eherr Gott, der an des Maien erstem Tag", "tokens": ["\u201e", "herr", "Gott", ",", "der", "an", "des", "Mai\u00b7en", "ers\u00b7tem", "Tag"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Strahl begabt mit sonderlichem Segen,", "tokens": ["Den", "Strahl", "be\u00b7gabt", "mit", "son\u00b7der\u00b7li\u00b7chem", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den sich der s\u00fcnd\u2019ge Mensch gewinnen mag", "tokens": ["Den", "sich", "der", "s\u00fcn\u00b7d'\u00b7ge", "Mensch", "ge\u00b7win\u00b7nen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "ADJA", "NN", "