{"textgrid.poem.56256": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Das Dampfro\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnell! schnell, mein Schmidt, mit des Rosses Beschlag!", "tokens": ["Schnell", "!", "schnell", ",", "mein", "Schmidt", ",", "mit", "des", "Ros\u00b7ses", "Be\u00b7schlag", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Derweil du zauderst, verstreicht der Tag. \u2013", "tokens": ["Der\u00b7weil", "du", "zau\u00b7derst", ",", "ver\u00b7streicht", "der", "Tag", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwie dampfet dein ungeheures Pferd!", "tokens": ["\u00bb", "wie", "damp\u00b7fet", "dein", "un\u00b7ge\u00b7heu\u00b7res", "Pferd", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo eilst du so hin, mein Ritter wert?\u00ab \u2013", "tokens": ["Wo", "eilst", "du", "so", "hin", ",", "mein", "Rit\u00b7ter", "wert", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schnell! schnell, mein Schmidt! Wer die Erde umkreist", "tokens": ["Schnell", "!", "schnell", ",", "mein", "Schmidt", "!", "Wer", "die", "Er\u00b7de", "um\u00b7kreist"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "ADJD", "$,", "PPOSAT", "NN", "$.", "PWS", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Ost in West, wie die Schule beweist,", "tokens": ["Von", "Ost", "in", "West", ",", "wie", "die", "Schu\u00b7le", "be\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der kommt, das hat er von seiner M\u00fch,", "tokens": ["Der", "kommt", ",", "das", "hat", "er", "von", "sei\u00b7ner", "M\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ans Ziel um einen Tag zu fr\u00fch.", "tokens": ["Ans", "Ziel", "um", "ei\u00b7nen", "Tag", "zu", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Dampfro\u00df, Muster der Schnelligkeit,", "tokens": ["Mein", "Dam\u00b7pfro\u00df", ",", "Mus\u00b7ter", "der", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00e4\u00dft hinter sich die laufende Zeit,", "tokens": ["L\u00e4\u00dft", "hin\u00b7ter", "sich", "die", "lau\u00b7fen\u00b7de", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und nimmt's zur Stunde nach Westen den Lauf,", "tokens": ["Und", "nimmt's", "zur", "Stun\u00b7de", "nach", "Wes\u00b7ten", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kommt's gestern von Osten schon wieder herauf.", "tokens": ["Kommt's", "ge\u00b7stern", "von", "Os\u00b7ten", "schon", "wie\u00b7der", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich habe der Zeit ihr Geheimnis geraubt,", "tokens": ["Ich", "ha\u00b7be", "der", "Zeit", "ihr", "Ge\u00b7heim\u00b7nis", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Von Gestern zu Gestern zur\u00fcck sie geschraubt,", "tokens": ["Von", "Ge\u00b7stern", "zu", "Ge\u00b7stern", "zu\u00b7r\u00fcck", "sie", "ge\u00b7schraubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PTKVZ", "PPER", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und schraube zur\u00fcck sie von Tag zu Tag,", "tokens": ["Und", "schrau\u00b7be", "zu\u00b7r\u00fcck", "sie", "von", "Tag", "zu", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie einst ich zu Adam gelangen mag.", "tokens": ["Wie", "einst", "ich", "zu", "A\u00b7dam", "ge\u00b7lan\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "ADV", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ich habe die Mutter, sonderbar!", "tokens": ["Ich", "ha\u00b7be", "die", "Mut\u00b7ter", ",", "son\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der Stunde besucht, da sie mich gebar,", "tokens": ["In", "der", "Stun\u00b7de", "be\u00b7sucht", ",", "da", "sie", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ich selber stand der Krei\u00dfenden bei,", "tokens": ["Ich", "sel\u00b7ber", "stand", "der", "Krei\u00b7\u00dfen\u00b7den", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und habe vernommen mein erstes Geschrei.", "tokens": ["Und", "ha\u00b7be", "ver\u00b7nom\u00b7men", "mein", "ers\u00b7tes", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Viel tausend Mal, der Sonne voran,", "tokens": ["Viel", "tau\u00b7send", "Mal", ",", "der", "Son\u00b7ne", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vollbracht ich im Fluge noch meine Bahn,", "tokens": ["Voll\u00b7bracht", "ich", "im", "Flu\u00b7ge", "noch", "mei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Bis heut ich hier zu besuchen kam", "tokens": ["Bis", "heut", "ich", "hier", "zu", "be\u00b7su\u00b7chen", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gro\u00dfvater als gl\u00fccklichen Br\u00e4utigam.", "tokens": ["Gro\u00df\u00b7va\u00b7ter", "als", "gl\u00fcck\u00b7li\u00b7chen", "Br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Gro\u00dfmutter ist die lieblichste Braut,", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "ist", "die", "lieb\u00b7lichs\u00b7te", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die je mit Augen ich noch erschaut;", "tokens": ["Die", "je", "mit", "Au\u00b7gen", "ich", "noch", "er\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er aber, gr\u00e4mlich, zu eifern geneigt,", "tokens": ["Er", "a\u00b7ber", ",", "gr\u00e4m\u00b7lich", ",", "zu", "ei\u00b7fern", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "$,", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat ohne weitres die T\u00fcr mir gezeigt.", "tokens": ["Hat", "oh\u00b7ne", "weit\u00b7res", "die", "T\u00fcr", "mir", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Schnell! schnell, mein Schmidt! mich ekelt schier,", "tokens": ["Schnell", "!", "schnell", ",", "mein", "Schmidt", "!", "mich", "e\u00b7kelt", "schier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die jetzt verl\u00e4uft, die Zeit von Papier;", "tokens": ["Die", "jetzt", "ver\u00b7l\u00e4uft", ",", "die", "Zeit", "von", "Pa\u00b7pier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Zur\u00fcck hindurch! es verlangt mich schon", "tokens": ["Zu\u00b7r\u00fcck", "hin\u00b7durch", "!", "es", "ver\u00b7langt", "mich", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu sehen den Kaiser Napoleon.", "tokens": ["Zu", "se\u00b7hen", "den", "Kai\u00b7ser", "Na\u00b7po\u00b7le\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Ich sprech ihn zuerst auf Helena,", "tokens": ["Ich", "sprech", "ihn", "zu\u00b7erst", "auf", "He\u00b7le\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Gru\u00df der Nachwelt bring ich ihm da;", "tokens": ["Den", "Gru\u00df", "der", "Nach\u00b7welt", "bring", "ich", "ihm", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann sprech ich ihn fr\u00fcher beim Kr\u00f6nungsfest,", "tokens": ["Dann", "sprech", "ich", "ihn", "fr\u00fc\u00b7her", "beim", "Kr\u00f6\u00b7nungs\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und warn ihn, \u2013 o hielt' er die Warnung fest!", "tokens": ["Und", "warn", "ihn", ",", "\u2013", "o", "hielt'", "er", "die", "War\u00b7nung", "fest", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "$(", "FM", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Bist fertig, mein Schmidt? nimm deinen Sold,", "tokens": ["Bist", "fer\u00b7tig", ",", "mein", "Schmidt", "?", "nimm", "dei\u00b7nen", "Sold", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$.", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Tausend Neunhundert gepr\u00e4gtes Gold.", "tokens": ["Ein", "Tau\u00b7send", "Neun\u00b7hun\u00b7dert", "ge\u00b7pr\u00e4g\u00b7tes", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Ro\u00df! Hurrah! nach Westen gejagt,", "tokens": ["Zu", "Ro\u00df", "!", "Hur\u00b7rah", "!", "nach", "Wes\u00b7ten", "ge\u00b7jagt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "$.", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hier wieder vor\u00fcber, wann gestern es tagt! \u2013", "tokens": ["Hier", "wie\u00b7der", "vor\u00b7\u00fc\u00b7ber", ",", "wann", "ge\u00b7stern", "es", "tagt", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbmein Ritter, mein Ritter, du kommst daher,", "tokens": ["\u00bb", "mein", "Rit\u00b7ter", ",", "mein", "Rit\u00b7ter", ",", "du", "kommst", "da\u00b7her", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wohin wir gehen, erz\u00e4hle noch mehr;", "tokens": ["Wo\u00b7hin", "wir", "ge\u00b7hen", ",", "er\u00b7z\u00e4h\u00b7le", "noch", "mehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du wei\u00dft, o sag es, ob f\u00e4llt, ob steigt", "tokens": ["Du", "wei\u00dft", ",", "o", "sag", "es", ",", "ob", "f\u00e4llt", ",", "ob", "steigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "VVFIN", "PPER", "$,", "KOUS", "VVFIN", "$,", "KOUS", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Cours, der jetzt so schwankend sich zeigt?", "tokens": ["Der", "Cours", ",", "der", "jetzt", "so", "schwan\u00b7kend", "sich", "zeigt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Ein Wort, ein Wort nur im Vertraun!", "tokens": ["Ein", "Wort", ",", "ein", "Wort", "nur", "im", "Ver\u00b7traun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist's weis auf Rothschild H\u00e4user zu baun?\u00ab \u2013", "tokens": ["Ist's", "weis", "auf", "Roth\u00b7schild", "H\u00e4u\u00b7ser", "zu", "baun", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "PTKVZ", "APPR", "NN", "NN", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+--+", "measu