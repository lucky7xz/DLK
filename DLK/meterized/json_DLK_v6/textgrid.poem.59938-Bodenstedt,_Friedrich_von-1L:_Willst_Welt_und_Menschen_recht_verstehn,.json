{"textgrid.poem.59938": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst Welt und Menschen recht verstehn,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst Welt und Menschen recht verstehn,", "tokens": ["Willst", "Welt", "und", "Men\u00b7schen", "recht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00dft du ins eigne Herz dir sehn.", "tokens": ["Mu\u00dft", "du", "ins", "eig\u00b7ne", "Herz", "dir", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du dich selbst recht kennenlernen,", "tokens": ["Willst", "du", "dich", "selbst", "recht", "ken\u00b7nen\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00dft du dich aus dir selbst entfernen.", "tokens": ["Mu\u00dft", "du", "dich", "aus", "dir", "selbst", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer sich beurteilt nur nach sich,", "tokens": ["Wer", "sich", "beur\u00b7teilt", "nur", "nach", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "ADV", "APPR", "PRF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Gelangt zu falschen Schl\u00fcssen \u2013", "tokens": ["Ge\u00b7langt", "zu", "fal\u00b7schen", "Schl\u00fcs\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du selbst erkennst so wenig dich,", "tokens": ["Du", "selbst", "er\u00b7kennst", "so", "we\u00b7nig", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PIS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als du dich selbst kannst k\u00fcssen.", "tokens": ["Als", "du", "dich", "selbst", "kannst", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}