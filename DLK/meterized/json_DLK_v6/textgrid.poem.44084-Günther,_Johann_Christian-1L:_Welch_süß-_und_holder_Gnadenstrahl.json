{"textgrid.poem.44084": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch s\u00fc\u00df- und holder Gnadenstrahl", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch s\u00fc\u00df- und holder Gnadenstrahl", "tokens": ["Welch", "s\u00fc\u00df", "und", "hol\u00b7der", "Gna\u00b7den\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verwandelt mich von innen?", "tokens": ["Ver\u00b7wan\u00b7delt", "mich", "von", "in\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was raubt mir so bald auf einmahl", "tokens": ["Was", "raubt", "mir", "so", "bald", "auf", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten W\u00fcntsch und Sinnen?", "tokens": ["Die", "al\u00b7ten", "W\u00fcnt\u00b7sch", "und", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mein Herz ist froh, mein Geist wird frey", "tokens": ["Mein", "Herz", "ist", "froh", ",", "mein", "Geist", "wird", "frey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rei\u00dft der L\u00fcste Band entzwey,", "tokens": ["Und", "rei\u00dft", "der", "L\u00fcs\u00b7te", "Band", "ent\u00b7zwey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An dem er starck gehangen.", "tokens": ["An", "dem", "er", "starck", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach Gott, erhalt den guten Trieb", "tokens": ["Ach", "Gott", ",", "er\u00b7halt", "den", "gu\u00b7ten", "Trieb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und treib aus Funcken Flammen;", "tokens": ["Und", "treib", "aus", "Fun\u00b7cken", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jezt hab ich deine Rechte lieb,", "tokens": ["Jezt", "hab", "ich", "dei\u00b7ne", "Rech\u00b7te", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jezt lern ich mich verdammen,", "tokens": ["Jezt", "lern", "ich", "mich", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jezt find ich Lust in Creuz und Pein:", "tokens": ["Jezt", "find", "ich", "Lust", "in", "Creuz", "und", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seele mu\u00df gel\u00e4utert seyn", "tokens": ["Die", "See\u00b7le", "mu\u00df", "ge\u00b7l\u00e4u\u00b7tert", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber Felsen steigen.", "tokens": ["Und", "\u00fc\u00b7ber", "Fel\u00b7sen", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Las jezo die Barmherzigkeit,", "tokens": ["Las", "je\u00b7zo", "die", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Vater, dich nicht halten,", "tokens": ["Mein", "Va\u00b7ter", ",", "dich", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nein, las vielmehr durch Schlag und Leid", "tokens": ["Nein", ",", "las", "viel\u00b7mehr", "durch", "Schlag", "und", "Leid"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein festes Herz zerspalten,", "tokens": ["Mein", "fes\u00b7tes", "Herz", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schmeis deinen Zorn in Fleisch und Blut,", "tokens": ["Schmeis", "dei\u00b7nen", "Zorn", "in", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil so ein Schmerzen linder thut", "tokens": ["Weil", "so", "ein", "Schmer\u00b7zen", "lin\u00b7der", "thut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Balsam auf der Scheitel.", "tokens": ["Als", "Bal\u00b7sam", "auf", "der", "Schei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein ewig Gl\u00fccke kan kaum bl\u00fchn,", "tokens": ["Mein", "e\u00b7wig", "Gl\u00fc\u00b7cke", "kan", "kaum", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wofern ich ruhig lebe", "tokens": ["Wo\u00b7fern", "ich", "ru\u00b7hig", "le\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und, dort den rechten Schaz zu ziehn,", "tokens": ["Und", ",", "dort", "den", "rech\u00b7ten", "Schaz", "zu", "ziehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nicht der Welt begebe.", "tokens": ["Mich", "nicht", "der", "Welt", "be\u00b7ge\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gewohnheit ist ein eisern Kleid,", "tokens": ["Ge\u00b7wohn\u00b7heit", "ist", "ein", "ei\u00b7sern", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zerrei\u00df es durch die Traurigkeit", "tokens": ["Zer\u00b7rei\u00df", "es", "durch", "die", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gewaltig starcker Pfeile.", "tokens": ["Ge\u00b7wal\u00b7tig", "star\u00b7cker", "Pfei\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verflucht sey Sorgen, Flei\u00df und Zeit,", "tokens": ["Ver\u00b7flucht", "sey", "Sor\u00b7gen", ",", "Flei\u00df", "und", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich der Welt verpf\u00e4ndet", "tokens": ["Die", "ich", "der", "Welt", "ver\u00b7pf\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und auf den Dienst der Eitelkeit", "tokens": ["Und", "auf", "den", "Dienst", "der", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sinnlos angewendet,", "tokens": ["So", "sinn\u00b7los", "an\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verflucht sey alle Wi\u00dfenschaft,", "tokens": ["Ver\u00b7flucht", "sey", "al\u00b7le", "Wi\u00b7\u00dfen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nicht mit deiner Wei\u00dfheit Kraft", "tokens": ["Die", "nicht", "mit", "dei\u00b7ner", "Wei\u00df\u00b7heit", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Nechsten Heil gebe\u00dfert!", "tokens": ["Des", "Nechs\u00b7ten", "Heil", "ge\u00b7be\u00b7\u00dfert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Mein Heiland, hilf mir wider mich", "tokens": ["Mein", "Hei\u00b7land", ",", "hilf", "mir", "wi\u00b7der", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deiner Demuth k\u00e4mpfen", "tokens": ["Mit", "dei\u00b7ner", "De\u00b7muth", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lehre mich vern\u00fcnftiglich", "tokens": ["Und", "leh\u00b7re", "mich", "ver\u00b7n\u00fcnf\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch fremde Schwachheit d\u00e4mpfen!", "tokens": ["Auch", "frem\u00b7de", "Schwach\u00b7heit", "d\u00e4mp\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Komm, stelle meine S\u00fcnd ans Licht", "tokens": ["Komm", ",", "stel\u00b7le", "mei\u00b7ne", "S\u00fcnd", "ans", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und las dein holdes Angesicht", "tokens": ["Und", "las", "dein", "hol\u00b7des", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich stets zur Be\u00dfrung reizen.", "tokens": ["Mich", "stets", "zur", "Be\u00df\u00b7rung", "rei\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}