{"dta.poem.2194": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der N\u00e4chster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verlanget ihr durch seltne Gaben, durch Sch\u00f6nheit,", "tokens": ["Ver\u00b7lan\u00b7get", "ihr", "durch", "selt\u00b7ne", "Ga\u00b7ben", ",", "durch", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "durch Gelehrsamkeit,", "tokens": ["durch", "Ge\u00b7lehr\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Durch gutes Ansehn, durch Verstand, durch Schriften,", "tokens": ["Durch", "gu\u00b7tes", "An\u00b7sehn", ",", "durch", "Ver\u00b7stand", ",", "durch", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "durch ein sch\u00f6nes Kleid", "tokens": ["durch", "ein", "sch\u00f6\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der Welt Bewundrung zu erhalten; verliert ihr eure", "tokens": ["Der", "Welt", "Be\u00b7wund\u00b7rung", "zu", "er\u00b7hal\u00b7ten", ";", "ver\u00b7liert", "ihr", "eu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$.", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "M\u00fch bey allen.", "tokens": ["M\u00fch", "bey", "al\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die Menschen wollen nicht bewundern, sie wollen alle", "tokens": ["Die", "Men\u00b7schen", "wol\u00b7len", "nicht", "be\u00b7wun\u00b7dern", ",", "sie", "wol\u00b7len", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,", "PPER", "VMFIN", "PIS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "selbst gefallen.", "tokens": ["selbst", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}