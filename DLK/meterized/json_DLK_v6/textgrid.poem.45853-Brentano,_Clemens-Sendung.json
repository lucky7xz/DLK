{"textgrid.poem.45853": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sendung", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie war vor mir so tief hinabgeschwunden", "tokens": ["Sie", "war", "vor", "mir", "so", "tief", "hin\u00b7ab\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tief, so tief hinab mit ihr mein Sinn,", "tokens": ["Und", "tief", ",", "so", "tief", "hin\u00b7ab", "mit", "ihr", "mein", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sendet sie mich schnell zur Wiege hin,", "tokens": ["Da", "sen\u00b7det", "sie", "mich", "schnell", "zur", "Wie\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis Auferstehung sie dem Tod entbunden.", "tokens": ["Bis", "Auf\u00b7er\u00b7ste\u00b7hung", "sie", "dem", "Tod", "ent\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und an der Wiege brechen alle Wunden", "tokens": ["Und", "an", "der", "Wie\u00b7ge", "bre\u00b7chen", "al\u00b7le", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Seele auf, vom zartesten Beginn", "tokens": ["Der", "See\u00b7le", "auf", ",", "vom", "zar\u00b7tes\u00b7ten", "Be\u00b7ginn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird ihres Lebens Bild mir zum Gewinn,", "tokens": ["Wird", "ih\u00b7res", "Le\u00b7bens", "Bild", "mir", "zum", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Rosen und mit Dornen dicht umwunden.", "tokens": ["Mit", "Ro\u00b7sen", "und", "mit", "Dor\u00b7nen", "dicht", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und sehnsuchtsvoll wandl' ich auf allen Wegen,", "tokens": ["Und", "sehn\u00b7suchts\u00b7voll", "wandl'", "ich", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sie betrat, und harre sie zu sehen", "tokens": ["Die", "sie", "be\u00b7trat", ",", "und", "har\u00b7re", "sie", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nimmer, nimmer will sie auferstehen.", "tokens": ["Doch", "nim\u00b7mer", ",", "nim\u00b7mer", "will", "sie", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VMFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da tritt ein tiefes Bild mir still entgegen.", "tokens": ["Da", "tritt", "ein", "tie\u00b7fes", "Bild", "mir", "still", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Bildes Augen heil'ge Lichter drangen", "tokens": ["Aus", "Bil\u00b7des", "Au\u00b7gen", "heil'\u00b7ge", "Lich\u00b7ter", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und spannen Netze, nahmen mich gefangen.", "tokens": ["Und", "span\u00b7nen", "Net\u00b7ze", ",", "nah\u00b7men", "mich", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}