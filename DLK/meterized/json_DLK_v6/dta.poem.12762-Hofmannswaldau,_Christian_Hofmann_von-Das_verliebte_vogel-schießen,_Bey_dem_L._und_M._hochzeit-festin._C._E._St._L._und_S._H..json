{"dta.poem.12762": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das verliebte vogel-schie\u00dfen,  \n Bey dem L. und M. hochzeit-festin.  \n C. E. St. L. und S. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje kommt es doch, da\u00df sich das vogel-schie\u00dfen-fest", "tokens": ["Wie", "kommt", "es", "doch", ",", "da\u00df", "sich", "das", "vo\u00b7gel\u00b7schie\u00b7\u00dfen\u00b7fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit einer hochzeit-lust so wohl vergleichen l\u00e4st?", "tokens": ["Mit", "ei\u00b7ner", "hoch\u00b7zeit\u00b7lust", "so", "wohl", "ver\u00b7glei\u00b7chen", "l\u00e4st", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist vogel und gesch\u00fctz denn irgend beym verm\u00e4hlen?", "tokens": ["Ist", "vo\u00b7gel", "und", "ge\u00b7sch\u00fctz", "denn", "ir\u00b7gend", "beym", "ver\u00b7m\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "KON", "ADJD", "KON", "ADV", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es mu\u00df ja allerdings also beschaffen seyn,", "tokens": ["Es", "mu\u00df", "ja", "al\u00b7ler\u00b7dings", "al\u00b7so", "be\u00b7schaf\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn sonsten stimmte nicht so that als namen ein;", "tokens": ["Denn", "sons\u00b7ten", "stimm\u00b7te", "nicht", "so", "that", "als", "na\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKNEG", "ADV", "VVFIN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die that, die w\u00fcrdig hier mit kurtzem zu erzehlen:", "tokens": ["Die", "that", ",", "die", "w\u00fcr\u00b7dig", "hier", "mit", "kurt\u00b7zem", "zu", "er\u00b7zeh\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "ADJD", "ADV", "APPR", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn der Johannis-tag nun vor der th\u00fcre steht,", "tokens": ["Wenn", "der", "Jo\u00b7han\u00b7nis\u00b7tag", "nun", "vor", "der", "th\u00fc\u00b7re", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und die bekante lust des schie\u00dfens vor sich geht,", "tokens": ["Und", "die", "be\u00b7kan\u00b7te", "lust", "des", "schie\u00b7\u00dfens", "vor", "sich", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So schauet man zu erst, wie um die vogel-stange", "tokens": ["So", "schau\u00b7et", "man", "zu", "erst", ",", "wie", "um", "die", "vo\u00b7gel\u00b7stan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKZU", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man tieffe gruben gr\u00e4bt, in welchen bier und wein;", "tokens": ["Man", "tief\u00b7fe", "gru\u00b7ben", "gr\u00e4bt", ",", "in", "wel\u00b7chen", "bier", "und", "wein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VVFIN", "$,", "APPR", "PWAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wurst, und was man hat, kan frey und sicher seyn;", "tokens": ["Und", "wurst", ",", "und", "was", "man", "hat", ",", "kan", "frey", "und", "si\u00b7cher", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KON", "PWS", "PIS", "VAFIN", "$,", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wann uns der sonnen-gluth will machen allzu bange.", "tokens": ["Wann", "uns", "der", "son\u00b7nen\u00b7gluth", "will", "ma\u00b7chen", "all\u00b7zu", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "VVINF", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch wird vorhero auch der vogel aufgesetzt,", "tokens": ["Doch", "wird", "vor\u00b7he\u00b7ro", "auch", "der", "vo\u00b7gel", "auf\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "An welchem sich hernach so mancher sch\u00fctz ergetzt,", "tokens": ["An", "wel\u00b7chem", "sich", "her\u00b7nach", "so", "man\u00b7cher", "sch\u00fctz", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADV", "ADV", "PIAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da offt sch\u00e4rffer als bey seiner liebsten schie\u00dfet:", "tokens": ["Und", "da", "offt", "sch\u00e4rf\u00b7fer", "als", "bey", "sei\u00b7ner", "liebs\u00b7ten", "schie\u00b7\u00dfet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "KOKOM", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nach diesem werden dann die h\u00fctten aufgebaut,", "tokens": ["Nach", "die\u00b7sem", "wer\u00b7den", "dann", "die", "h\u00fct\u00b7ten", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "ADV", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Darinn das frauen-volck dieselbe lust anschaut,", "tokens": ["Da\u00b7rinn", "das", "frau\u00b7en\u00b7volck", "die\u00b7sel\u00b7be", "lust", "an\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und durch ihr gegenseyn der m\u00e4nner m\u00fch vers\u00fc\u00dfet.", "tokens": ["Und", "durch", "ihr", "ge\u00b7gen\u00b7seyn", "der", "m\u00e4n\u00b7ner", "m\u00fch", "ver\u00b7s\u00fc\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geht denn hernach, wenns zeit, die kirme\u00df v\u00f6llig an,", "tokens": ["Geht", "denn", "her\u00b7nach", ",", "wenns", "zeit", ",", "die", "kir\u00b7me\u00df", "v\u00f6l\u00b7lig", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "NN", "$,", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So wandert da hinaus, was offt kaum kriechen kan,", "tokens": ["So", "wan\u00b7dert", "da", "hin\u00b7aus", ",", "was", "offt", "kaum", "krie\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APZR", "$,", "PRELS", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die m\u00e4gdgen gehn zuerst, hernach die meisten frauen,", "tokens": ["Die", "m\u00e4gd\u00b7gen", "gehn", "zu\u00b7erst", ",", "her\u00b7nach", "die", "meis\u00b7ten", "frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und diese f\u00fchren offt die v\u00f6gel bey der hand,", "tokens": ["Und", "die\u00b7se", "f\u00fch\u00b7ren", "offt", "die", "v\u00f6\u00b7gel", "bey", "der", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die ihnen hat ihr sch\u00fctz\u2019 in liebe zugewandt,", "tokens": ["Die", "ih\u00b7nen", "hat", "ihr", "sch\u00fctz'", "in", "lie\u00b7be", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn er hat seine kunst im schie\u00dfen lassen schauen.", "tokens": ["Wenn", "er", "hat", "sei\u00b7ne", "kunst", "im", "schie\u00b7\u00dfen", "las\u00b7sen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "APPRART", "VVINF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da ist kein junger mensch, und wer er immer sey,", "tokens": ["Da", "ist", "kein", "jun\u00b7ger", "mensch", ",", "und", "wer", "er", "im\u00b7mer", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,", "KON", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn ihn ein m\u00e4gdgen kennt, vom zeichen-l\u00f6sen frey,", "tokens": ["Wenn", "ihn", "ein", "m\u00e4gd\u00b7gen", "kennt", ",", "vom", "zei\u00b7chen\u00b7l\u00f6\u00b7sen", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "(wiewol die frauen auch sich dieser lust nicht sch\u00e4men)", "tokens": ["(", "wie\u00b7wol", "die", "frau\u00b7en", "auch", "sich", "die\u00b7ser", "lust", "nicht", "sch\u00e4\u00b7men", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "ADV", "PRF", "PDAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wofern er anders sie und seiner liebe ziel", "tokens": ["Wo\u00b7fern", "er", "an\u00b7ders", "sie", "und", "sei\u00b7ner", "lie\u00b7be", "ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nicht durch ein grobes nein im buy verkehren will,", "tokens": ["Nicht", "durch", "ein", "gro\u00b7bes", "nein", "im", "buy", "ver\u00b7keh\u00b7ren", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "PTKANT", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wie zuvor die kost von ihren lippen nehmen.", "tokens": ["Und", "wie", "zu\u00b7vor", "die", "kost", "von", "ih\u00b7ren", "lip\u00b7pen", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wird dann manch scharffer pfeil dem vogel zugestellt;", "tokens": ["Wird", "dann", "manch", "scharf\u00b7fer", "pfeil", "dem", "vo\u00b7gel", "zu\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und trifft sichs, da\u00df ein span von solchem runter f\u00e4llt;", "tokens": ["Und", "trifft", "sichs", ",", "da\u00df", "ein", "span", "von", "sol\u00b7chem", "run\u00b7ter", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "ART", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da wird so mann als frau durch diese that vergn\u00fcget,", "tokens": ["Da", "wird", "so", "mann", "als", "frau", "durch", "die\u00b7se", "that", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "KOKOM", "ADJD", "APPR", "PDS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die frau darum, da\u00df sonst niemand, als nur ihr mann,", "tokens": ["Die", "frau", "da\u00b7rum", ",", "da\u00df", "sonst", "nie\u00b7mand", ",", "als", "nur", "ihr", "mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "KOUS", "ADV", "PIS", "$,", "KOUS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Den vogel auf den peltz so artig treffen kan;", "tokens": ["Den", "vo\u00b7gel", "auf", "den", "peltz", "so", "ar\u00b7tig", "tref\u00b7fen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der mann, da\u00df er hierdurch die andern hat besieget.", "tokens": ["Der", "mann", ",", "da\u00df", "er", "hier\u00b7durch", "die", "an\u00b7dern", "hat", "be\u00b7sie\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PAV", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich wei\u00df, da\u00df \u00f6ffters da manch frauenzimmer denckt:", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "\u00f6ff\u00b7ters", "da", "manch", "frau\u00b7en\u00b7zim\u00b7mer", "denckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ach w\u00fcrde mancher pfeil doch auch nach mir geleuckt!", "tokens": ["Ach", "w\u00fcr\u00b7de", "man\u00b7cher", "pfeil", "doch", "auch", "nach", "mir", "ge\u00b7leuckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PIAT", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und zwar ein pfeil, der sich zu solchen v\u00f6geln schicket,", "tokens": ["Und", "zwar", "ein", "pfeil", ",", "der", "sich", "zu", "sol\u00b7chen", "v\u00f6\u00b7geln", "schi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die meines gleichen sind, ich h\u00e4tte gr\u00f6\u00dfre lust,", "tokens": ["Die", "mei\u00b7nes", "glei\u00b7chen", "sind", ",", "ich", "h\u00e4t\u00b7te", "gr\u00f6\u00df\u00b7re", "lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "VAFIN", "$,", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und es vergn\u00fcgte sich auch mehr des sch\u00fctzens brust,", "tokens": ["Und", "es", "ver\u00b7gn\u00fcg\u00b7te", "sich", "auch", "mehr", "des", "sch\u00fct\u00b7zens", "brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Als wenn er da ein holtz auf einer stang erblicket.", "tokens": ["Als", "wenn", "er", "da", "ein", "holtz", "auf", "ei\u00b7ner", "stang", "er\u00b7bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So lustig dieses spiel sich nun beschauen l\u00e4st;", "tokens": ["So", "lus\u00b7tig", "die\u00b7ses", "spiel", "sich", "nun", "be\u00b7schau\u00b7en", "l\u00e4st", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDS", "VVFIN", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So artig ist es auch um euer hochzeit-fest,", "tokens": ["So", "ar\u00b7tig", "ist", "es", "auch", "um", "eu\u00b7er", "hoch\u00b7zeit\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Verliebte, die ein band itzt hat vergn\u00fcgt verbunden!", "tokens": ["Ver\u00b7lieb\u00b7te", ",", "die", "ein", "band", "itzt", "hat", "ver\u00b7gn\u00fcgt", "ver\u00b7bun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "VVFIN", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Meynt ihr, es reime sich hieher nicht allzuwohl?", "tokens": ["Meynt", "ihr", ",", "es", "rei\u00b7me", "sich", "hie\u00b7her", "nicht", "all\u00b7zu\u00b7wohl", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PRF", "PAV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So h\u00f6ret, was dis blat euch ferner sagen soll,", "tokens": ["So", "h\u00f6\u00b7ret", ",", "was", "dis", "blat", "euch", "fer\u00b7ner", "sa\u00b7gen", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PDS", "VVFIN", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und wo es einen schutz vor diesen spruch gefunden.", "tokens": ["Und", "wo", "es", "ei\u00b7nen", "schutz", "vor", "die\u00b7sen", "spruch", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Doch ehe noch mein mund vergleichet dieses spiel,", "tokens": ["Doch", "e\u00b7he", "noch", "mein", "mund", "ver\u00b7glei\u00b7chet", "die\u00b7ses", "spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So gebt mir erstlich zu, was ich hier haben will.", "tokens": ["So", "gebt", "mir", "erst\u00b7lich", "zu", ",", "was", "ich", "hier", "ha\u00b7ben", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PWS", "PPER", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Des vogels stelle soll die Jungfer Braut vertreten,", "tokens": ["Des", "vo\u00b7gels", "stel\u00b7le", "soll", "die", "Jung\u00b7fer", "Braut", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die gruben, welche vor der vogel-stange seyn,", "tokens": ["Die", "gru\u00b7ben", ",", "wel\u00b7che", "vor", "der", "vo\u00b7gel\u00b7stan\u00b7ge", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und wo man tr\u00e4gt die kost vor die verliebten ein,", "tokens": ["Und", "wo", "man", "tr\u00e4gt", "die", "kost", "vor", "die", "ver\u00b7lieb\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Die hat der Br\u00e4utigam allein ihm ausgebeten.", "tokens": ["Die", "hat", "der", "Br\u00e4u\u00b7ti\u00b7gam", "al\u00b7lein", "ihm", "aus\u00b7ge\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und dieser bleibet auch der sch\u00fctze hier bestellt,", "tokens": ["Und", "die\u00b7ser", "blei\u00b7bet", "auch", "der", "sch\u00fct\u00b7ze", "hier", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Weil l\u00e4ngsten seiner brust vor andern wohlgef\u00e4llt,", "tokens": ["Weil", "l\u00e4ngs\u00b7ten", "sei\u00b7ner", "brust", "vor", "an\u00b7dern", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Wenn er gesch\u00fctz und pfeil soll mit der hand regieren:", "tokens": ["Wenn", "er", "ge\u00b7sch\u00fctz", "und", "pfeil", "soll", "mit", "der", "hand", "re\u00b7gie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Sein bogen ist gewi\u00df, da\u00df auch die folge-zeit,", "tokens": ["Sein", "bo\u00b7gen", "ist", "ge\u00b7wi\u00df", ",", "da\u00df", "auch", "die", "fol\u00b7ge\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Was er vor sp\u00e4ne hat vom vogel abgestreut,", "tokens": ["Was", "er", "vor", "sp\u00e4\u00b7ne", "hat", "vom", "vo\u00b7gel", "ab\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Ein wahres zeugni\u00df wird von seinem schie\u00dfen f\u00fchren.", "tokens": ["Ein", "wah\u00b7res", "zeug\u00b7ni\u00df", "wird", "von", "sei\u00b7nem", "schie\u00b7\u00dfen", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die pfeile, welche nur verliebten brauchbar sind,", "tokens": ["Die", "pfei\u00b7le", ",", "wel\u00b7che", "nur", "ver\u00b7lieb\u00b7ten", "brauch\u00b7bar", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die ladung, welche man nur bey verliebten findt,", "tokens": ["Die", "la\u00b7dung", ",", "wel\u00b7che", "man", "nur", "bey", "ver\u00b7lieb\u00b7ten", "findt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die will mein kiel aus furcht nicht allzudeutlich sagen;", "tokens": ["Die", "will", "mein", "kiel", "aus", "furcht", "nicht", "all\u00b7zu\u00b7deut\u00b7lich", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Doch wer es noch nicht wei\u00df, und dennoch wissen will,", "tokens": ["Doch", "wer", "es", "noch", "nicht", "wei\u00df", ",", "und", "den\u00b7noch", "wis\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "KON", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Der kan, wann vor vorbey der hochzeit lustig spiel,", "tokens": ["Der", "kan", ",", "wann", "vor", "vor\u00b7bey", "der", "hoch\u00b7zeit", "lus\u00b7tig", "spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "PWAV", "APPR", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Den Herren Br\u00e4utigam und auch die g\u00e4ste fragen.", "tokens": ["Den", "Her\u00b7ren", "Br\u00e4u\u00b7ti\u00b7gam", "und", "auch", "die", "g\u00e4s\u00b7te", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Du, fruchtbares Dobin! bist das begl\u00fcckte feld,", "tokens": ["Du", ",", "frucht\u00b7ba\u00b7res", "Do\u00b7bin", "!", "bist", "das", "be\u00b7gl\u00fcck\u00b7te", "feld", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.68": {"text": "Wo di\u00df verbundne paar sein vogel-schie\u00dfen h\u00e4lt:", "tokens": ["Wo", "di\u00df", "ver\u00b7bund\u00b7ne", "paar", "sein", "vo\u00b7gel\u00b7schie\u00b7\u00dfen", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "PIAT", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Die g\u00e4ste sind, die hier die lust vergn\u00fcgter machen:", "tokens": ["Die", "g\u00e4s\u00b7te", "sind", ",", "die", "hier", "die", "lust", "ver\u00b7gn\u00fcg\u00b7ter", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "L\u00f6st irgend heut\u2019 iemand vor ein\u2019 ein zeichen ein,", "tokens": ["L\u00f6st", "ir\u00b7gend", "heut'", "ie\u00b7mand", "vor", "ein'", "ein", "zei\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIS", "APPR", "ART", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.71": {"text": "Kan k\u00fcnfftig noch einmal dergleichen schie\u00dfen seyn,", "tokens": ["Kan", "k\u00fcnff\u00b7tig", "noch", "ein\u00b7mal", "derg\u00b7lei\u00b7chen", "schie\u00b7\u00dfen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ADV", "PIS", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und wir dabey, wie itzt, von neuem wieder lachen.", "tokens": ["Und", "wir", "da\u00b7bey", ",", "wie", "itzt", ",", "von", "neu\u00b7em", "wie\u00b7der", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "$,", "PWAV", "ADV", "$,", "APPR", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Doch ist es einmal zeit, da\u00df auch mein bogen schlie\u00dft,", "tokens": ["Doch", "ist", "es", "ein\u00b7mal", "zeit", ",", "da\u00df", "auch", "mein", "bo\u00b7gen", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Verliebte! die der thau der liebe nun begie\u00dft;", "tokens": ["Ver\u00b7lieb\u00b7te", "!", "die", "der", "thau", "der", "lie\u00b7be", "nun", "be\u00b7gie\u00dft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "ART", "NN", "ART", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Lebt, liebt und bl\u00fchet wohl! der himmel sey dem bogen,", "tokens": ["Lebt", ",", "liebt", "und", "bl\u00fc\u00b7het", "wohl", "!", "der", "him\u00b7mel", "sey", "dem", "bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$.", "ART", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Mit dem ihr, was euch spei\u00dft, und di\u00df, was euch vergn\u00fcgt,", "tokens": ["Mit", "dem", "ihr", ",", "was", "euch", "spei\u00dft", ",", "und", "di\u00df", ",", "was", "euch", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Und dieses, was ihr w\u00fcnscht, nach aller lust besiegt,", "tokens": ["Und", "die\u00b7ses", ",", "was", "ihr", "w\u00fcnscht", ",", "nach", "al\u00b7ler", "lust", "be\u00b7siegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Und diesem, der ihn f\u00fchrt, zu aller zeit gewogen!", "tokens": ["Und", "die\u00b7sem", ",", "der", "ihn", "f\u00fchrt", ",", "zu", "al\u00b7ler", "zeit", "ge\u00b7wo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Der wunsch soll zum beschlu\u00df der treuen zeilen stehn:", "tokens": ["Der", "wunsch", "soll", "zum", "be\u00b7schlu\u00df", "der", "treu\u00b7en", "zei\u00b7len", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Wenn k\u00fcnfftig wir das fest das andre mal begehn,", "tokens": ["Wenn", "k\u00fcnff\u00b7tig", "wir", "das", "fest", "das", "and\u00b7re", "mal", "be\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PDS", "VVFIN", "ART", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Da\u00df unsre vogel-stang\u2019 ein vogel soll bezieren,", "tokens": ["Da\u00df", "uns\u00b7re", "vo\u00b7gel\u00b7stang'", "ein", "vo\u00b7gel", "soll", "be\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Da\u00df man von deiner kunst schon einen neuen span", "tokens": ["Da\u00df", "man", "von", "dei\u00b7ner", "kunst", "schon", "ei\u00b7nen", "neu\u00b7en", "span"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Bey unsrer sch\u00fctzen-zunfft erfreuet schauen kan,", "tokens": ["Bey", "uns\u00b7rer", "sch\u00fct\u00b7zen\u00b7zunfft", "er\u00b7freu\u00b7et", "schau\u00b7en", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Der seines sch\u00fctzens sehn\u2019 und bogen pflegt zu f\u00fchren.", "tokens": ["Der", "sei\u00b7nes", "sch\u00fct\u00b7zens", "sehn'", "und", "bo\u00b7gen", "pflegt", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}