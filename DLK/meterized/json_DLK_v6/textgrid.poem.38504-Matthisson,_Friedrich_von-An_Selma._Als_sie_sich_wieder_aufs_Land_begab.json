{"textgrid.poem.38504": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Selma. Als sie sich wieder aufs Land begab", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der goldnen Stadt Get\u00fcmmel", "tokens": ["Wenn", "der", "gold\u00b7nen", "Stadt", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du, o Beste, nun entfliehst,", "tokens": ["Du", ",", "o", "Bes\u00b7te", ",", "nun", "ent\u00b7fliehst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den heitren Segenshimmel", "tokens": ["Und", "den", "hei\u00b7tren", "Se\u00b7gens\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines D\u00f6rfchens wieder siehst,", "tokens": ["Dei\u00b7nes", "D\u00f6rf\u00b7chens", "wie\u00b7der", "siehst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der holde Herzensfriede", "tokens": ["Und", "der", "hol\u00b7de", "Her\u00b7zens\u00b7frie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebend dir entgegenwallt,", "tokens": ["Lie\u00b7bend", "dir", "ent\u00b7ge\u00b7gen\u00b7wallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir mit jedem Fr\u00fchlingsliede", "tokens": ["Dir", "mit", "je\u00b7dem", "Fr\u00fch\u00b7lings\u00b7lie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In die sanfte Seele schallt:", "tokens": ["In", "die", "sanf\u00b7te", "See\u00b7le", "schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, dann will ich zu dir eilen,", "tokens": ["O", ",", "dann", "will", "ich", "zu", "dir", "ei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich des Fr\u00fchlings mit dir freun,", "tokens": ["Mich", "des", "Fr\u00fch\u00b7lings", "mit", "dir", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freud und Wonne mit dir theilen,", "tokens": ["Freud", "und", "Won\u00b7ne", "mit", "dir", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unaussprechlich gl\u00fccklich seyn!", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7lich", "gl\u00fcck\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6ner wird der Hain uns l\u00e4cheln,", "tokens": ["Sch\u00f6\u00b7ner", "wird", "der", "Hain", "uns", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lenzgeschm\u00fcckt und aufgebl\u00fcht,", "tokens": ["Lenz\u00b7ge\u00b7schm\u00fcckt", "und", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Linder uns die K\u00fchlung f\u00e4cheln,", "tokens": ["Lin\u00b7der", "uns", "die", "K\u00fch\u00b7lung", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann die Abendr\u00f6the flieht.", "tokens": ["Wann", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fcsser jede Blume d\u00fcften", "tokens": ["S\u00fcs\u00b7ser", "je\u00b7de", "Blu\u00b7me", "d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Pappelweidenthal,", "tokens": ["In", "dem", "Pap\u00b7pel\u00b7wei\u00b7dent\u00b7hal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo, umweht von Mayenl\u00fcften,", "tokens": ["Wo", ",", "um\u00b7weht", "von", "Ma\u00b7yen\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du uns gr\u00fcssest, Nachtigall!", "tokens": ["Du", "uns", "gr\u00fcs\u00b7sest", ",", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn, o Unschuld, du begl\u00fcckest", "tokens": ["Denn", ",", "o", "Un\u00b7schuld", ",", "du", "be\u00b7gl\u00fc\u00b7ckest"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns mit Engelheiterkeit,", "tokens": ["Uns", "mit", "En\u00b7gel\u00b7hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Winkst zur Freude, und entz\u00fcckest", "tokens": ["Winkst", "zur", "Freu\u00b7de", ",", "und", "ent\u00b7z\u00fc\u00b7ckest"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis zur Himmelsseligkeit!", "tokens": ["Bis", "zur", "Him\u00b7mels\u00b7se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gutes M\u00e4dchen, wann uns immer", "tokens": ["Gu\u00b7tes", "M\u00e4d\u00b7chen", ",", "wann", "uns", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Leben heiter lacht,", "tokens": ["Un\u00b7ser", "Le\u00b7ben", "hei\u00b7ter", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie des Mondes milder Schimmer", "tokens": ["Wie", "des", "Mon\u00b7des", "mil\u00b7der", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der stillen Sommernacht,", "tokens": ["In", "der", "stil\u00b7len", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie sollen unsrer Jugend", "tokens": ["O", "wie", "sol\u00b7len", "uns\u00b7rer", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wonnetage dann entfliehn,", "tokens": ["Won\u00b7ne\u00b7ta\u00b7ge", "dann", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stets geleitet von der Tugend,", "tokens": ["Stets", "ge\u00b7lei\u00b7tet", "von", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieser Freudenk\u00f6niginn!", "tokens": ["Die\u00b7ser", "Freu\u00b7den\u00b7k\u00f6\u00b7ni\u00b7ginn", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}