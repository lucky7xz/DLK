{"dta.poem.6063": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sp\u00fcrt, es thu der Mond in eines Jahres Creise", "tokens": ["Man", "sp\u00fcrt", ",", "es", "thu", "der", "Mond", "in", "ei\u00b7nes", "Jah\u00b7res", "Crei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit unsrer Erden Rund dieselbe Reise:", "tokens": ["Mit", "uns\u00b7rer", "Er\u00b7den", "Rund", "die\u00b7sel\u00b7be", "Rei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch hat derselbige, in einer kleinern R\u00fcnde,", "tokens": ["Doch", "hat", "der\u00b7sel\u00b7bi\u00b7ge", ",", "in", "ei\u00b7ner", "klei\u00b7nern", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch seinen eignen Lauf,", "tokens": ["Noch", "sei\u00b7nen", "eig\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der h\u00f6rt in drey\u00dfig Tagen auf.", "tokens": ["Der", "h\u00f6rt", "in", "drey\u00b7\u00dfig", "Ta\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Monath-L\u00e4uffer eilt geschwinde,", "tokens": ["Der", "Mo\u00b7na\u00b7th\u00b7L\u00e4uf\u00b7fer", "eilt", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und legt in einem Jahr zw\u00f6lfmal den Weg zur\u00fcck.", "tokens": ["Und", "legt", "in", "ei\u00b7nem", "Jahr", "zw\u00f6lf\u00b7mal", "den", "Weg", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es w\u00e4chst der Mond, wird voll, nimmt ab. Wenn sein Gesicht,", "tokens": ["Es", "w\u00e4chst", "der", "Mond", ",", "wird", "voll", ",", "nimmt", "ab", ".", "Wenn", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "$,", "VVFIN", "PTKVZ", "$.", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indem er voll, recht zu dem Sonnen-Licht", "tokens": ["In\u00b7dem", "er", "voll", ",", "recht", "zu", "dem", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.10": {"text": "Gekehrt ist; schickt er uns der Sonnen Schein", "tokens": ["Ge\u00b7kehrt", "ist", ";", "schickt", "er", "uns", "der", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$.", "VVFIN", "PPER", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Zur\u00fcck, und scheint sodann ein silbern Rund zu seyn.", "tokens": ["Zu\u00b7r\u00fcck", ",", "und", "scheint", "so\u00b7dann", "ein", "sil\u00b7bern", "Rund", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wol, wenn er erscheint, als wenn er von uns eilt;", "tokens": ["So", "wol", ",", "wenn", "er", "er\u00b7scheint", ",", "als", "wenn", "er", "von", "uns", "eilt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist sein Gesicht getheilt.", "tokens": ["Ist", "sein", "Ge\u00b7sicht", "ge\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Das Dunckle weicht dem Lichte nach und nach,", "tokens": ["Das", "Dunck\u00b7le", "weicht", "dem", "Lich\u00b7te", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Das Licht dem Duncklen allgemach,", "tokens": ["Das", "Licht", "dem", "Dunck\u00b7len", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nachdem der Sonnen Glantz ihm ihre Strahlen schenckt,", "tokens": ["Nach\u00b7dem", "der", "Son\u00b7nen", "Glantz", "ihm", "ih\u00b7re", "Strah\u00b7len", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und wieder von der Kugel lenckt.", "tokens": ["Und", "wie\u00b7der", "von", "der", "Ku\u00b7gel", "lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bis da\u00df zuletzt er zur gewissen Zeit,", "tokens": ["Bis", "da\u00df", "zu\u00b7letzt", "er", "zur", "ge\u00b7wis\u00b7sen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ADV", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wenn er der Sonnen nah, sich gantz verlieret.", "tokens": ["Wenn", "er", "der", "Son\u00b7nen", "nah", ",", "sich", "gantz", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$,", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Es wird sodann kein Strahl herab gef\u00fchret.", "tokens": ["Es", "wird", "so\u00b7dann", "kein", "Strahl", "her\u00b7ab", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Der Ober-Theil empf\u00e4nget gantz", "tokens": ["Der", "O\u00b7ber\u00b7T\u00b7heil", "emp\u00b7f\u00e4n\u00b7get", "gantz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Sonnen Glantz.", "tokens": ["Der", "Son\u00b7nen", "Glantz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Bald aber wird er sichtbar, neu gebohren,", "tokens": ["Bald", "a\u00b7ber", "wird", "er", "sicht\u00b7bar", ",", "neu", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und hat er bald am Schein gewonnen, bald verlohren.", "tokens": ["Und", "hat", "er", "bald", "am", "Schein", "ge\u00b7won\u00b7nen", ",", "bald", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}