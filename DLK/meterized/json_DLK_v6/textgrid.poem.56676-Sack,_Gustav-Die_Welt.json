{"textgrid.poem.56676": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus eins ward zwei, dann strichen wir die zwei", "tokens": ["Aus", "eins", "ward", "zwei", ",", "dann", "stri\u00b7chen", "wir", "die", "zwei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "CARD", "$,", "ADV", "VVFIN", "PPER", "ART", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und schrieben: wahrlich! es ist eine Welt,", "tokens": ["und", "schrie\u00b7ben", ":", "wahr\u00b7lich", "!", "es", "ist", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die in sich Stoff und Geist zusammen h\u00e4lt,", "tokens": ["die", "in", "sich", "Stoff", "und", "Geist", "zu\u00b7sam\u00b7men", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und auch kein Pfaffe bricht sie mehr entzwei.", "tokens": ["und", "auch", "kein", "Pfaf\u00b7fe", "bricht", "sie", "mehr", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann aber: es ist alles Bilderei,", "tokens": ["Dann", "a\u00b7ber", ":", "es", "ist", "al\u00b7les", "Bil\u00b7de\u00b7rei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was sich so bunt vor unsre Sinne stellt,", "tokens": ["was", "sich", "so", "bunt", "vor", "uns\u00b7re", "Sin\u00b7ne", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ein X, von dem niemals der Schleier f\u00e4llt,", "tokens": ["ein", "X", ",", "von", "dem", "nie\u00b7mals", "der", "Schlei\u00b7er", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "XY", "$,", "APPR", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "ja unsre Sinne selbst sind Malerei,", "tokens": ["ja", "uns\u00b7re", "Sin\u00b7ne", "selbst", "sind", "Ma\u00b7le\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "die Welt, das Ding, die Folge, Zeit und Raum", "tokens": ["die", "Welt", ",", "das", "Ding", ",", "die", "Fol\u00b7ge", ",", "Zeit", "und", "Raum"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "alles ein schwerer, r\u00e4tselwirrer Traum.", "tokens": ["al\u00b7les", "ein", "schwe\u00b7rer", ",", "r\u00e4t\u00b7sel\u00b7wir\u00b7rer", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und heute schreit man laut auf allen Gassen:", "tokens": ["Und", "heu\u00b7te", "schreit", "man", "laut", "auf", "al\u00b7len", "Gas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "nein, sie ist da, ist harte Wirklichkeit \u2013 \u2013", "tokens": ["nein", ",", "sie", "ist", "da", ",", "ist", "har\u00b7te", "Wirk\u00b7lich\u00b7keit", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "$,", "VAFIN", "ADJA", "NN", "$(", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "fortrollt die Welt im wilden Strom der Zeit,", "tokens": ["fort\u00b7rollt", "die", "Welt", "im", "wil\u00b7den", "Strom", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wir rollen mit und k\u00f6nnen sie nicht fassen.", "tokens": ["wir", "rol\u00b7len", "mit", "und", "k\u00f6n\u00b7nen", "sie", "nicht", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}