{"textgrid.poem.40310": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel g\u00e4hnt, der Tag ist auferstanden,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel g\u00e4hnt, der Tag ist auferstanden,", "tokens": ["Der", "Him\u00b7mel", "g\u00e4hnt", ",", "der", "Tag", "ist", "auf\u00b7er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich habe nun genug geschaut nach Osten;", "tokens": ["ich", "ha\u00b7be", "nun", "ge\u00b7nug", "ge\u00b7schaut", "nach", "Os\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Seele will in ihren Abendlanden", "tokens": ["die", "See\u00b7le", "will", "in", "ih\u00b7ren", "A\u00b7bend\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vollendung kosten.", "tokens": ["Vol\u00b7len\u00b7dung", "kos\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "An dem Thor des neuen Evagartens", "tokens": ["An", "dem", "Thor", "des", "neu\u00b7en", "E\u00b7va\u00b7gar\u00b7tens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "steht ein kn\u00f6chernes Gerippe,", "tokens": ["steht", "ein", "kn\u00f6\u00b7cher\u00b7nes", "Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "mit dem Ausdruck des Erwartens,", "tokens": ["mit", "dem", "Aus\u00b7druck", "des", "Er\u00b7war\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "aber nicht mehr in der Faust die Hippe.", "tokens": ["a\u00b7ber", "nicht", "mehr", "in", "der", "Faust", "die", "Hip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sein Scheitel schimmert; eine Pfauenfeder", "tokens": ["Sein", "Schei\u00b7tel", "schim\u00b7mert", ";", "ei\u00b7ne", "Pfau\u00b7en\u00b7fe\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "ragt aus der Rechten steil zum Himmelsrand,", "tokens": ["ragt", "aus", "der", "Rech\u00b7ten", "steil", "zum", "Him\u00b7mels\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "drin sonnt sich tausendfarbig, was ein Jeder", "tokens": ["drin", "sonnt", "sich", "tau\u00b7send\u00b7far\u00b7big", ",", "was", "ein", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "$,", "PRELS", "ART", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "war und empfand.", "tokens": ["war", "und", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "In der Stunde einer neuen Frucht", "tokens": ["In", "der", "Stun\u00b7de", "ei\u00b7ner", "neu\u00b7en", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "perlt ein Strahl aus diesem Spiegel,", "tokens": ["perlt", "ein", "Strahl", "aus", "die\u00b7sem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "dann verglimmt die Wonnesucht,", "tokens": ["dann", "ver\u00b7glimmt", "die", "Won\u00b7ne\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "still empf\u00e4ngt der dunkle Keim sein Siegel.", "tokens": ["still", "emp\u00b7f\u00e4ngt", "der", "dunk\u00b7le", "Keim", "sein", "Sie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Schon d\u00e4mmert Glanz; krystallne Ketten h\u00e4ngen", "tokens": ["Schon", "d\u00e4m\u00b7mert", "Glanz", ";", "krys\u00b7tall\u00b7ne", "Ket\u00b7ten", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$.", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "klar her zu dir aus v\u00e4terlichen Sph\u00e4ren.", "tokens": ["klar", "her", "zu", "dir", "aus", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "So sollst auch Du dich aus der D\u00e4mmrung dr\u00e4ngen", "tokens": ["So", "sollst", "auch", "Du", "dich", "aus", "der", "D\u00e4mm\u00b7rung", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "und dich verkl\u00e4ren,", "tokens": ["und", "dich", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Seele, bis dein starr Gehirn sich lichtet,", "tokens": ["See\u00b7le", ",", "bis", "dein", "starr", "Ge\u00b7hirn", "sich", "lich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "wie die Sonne scheint durch Eis,", "tokens": ["wie", "die", "Son\u00b7ne", "scheint", "durch", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "und dir deine Brunst beschwichtet", "tokens": ["und", "dir", "dei\u00b7ne", "Brunst", "be\u00b7schwich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "und im Traum selbst deinen Willen wei\u00df.", "tokens": ["und", "im", "Traum", "selbst", "dei\u00b7nen", "Wil\u00b7len", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Noch flimmert's nur; tief lockt die alte Nacht", "tokens": ["Noch", "flim\u00b7mert's", "nur", ";", "tief", "lockt", "die", "al\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$.", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "mit ihrer Schaar verworrner Muttergluten.", "tokens": ["mit", "ih\u00b7rer", "Schaar", "ver\u00b7worr\u00b7ner", "Mut\u00b7ter\u00b7glu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Doch du wirst wiederkehren! du bist Macht!", "tokens": ["Doch", "du", "wirst", "wie\u00b7der\u00b7keh\u00b7ren", "!", "du", "bist", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVINF", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "sieh, rings sind Fluten:", "tokens": ["sieh", ",", "rings", "sind", "Flu\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "wenn zwei Liebende zusammensinken,", "tokens": ["wenn", "zwei", "Lie\u00b7ben\u00b7de", "zu\u00b7sam\u00b7men\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VVINF", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.30": {"text": "die du Einmal nur erleuchtet,", "tokens": ["die", "du", "Ein\u00b7mal", "nur", "er\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "und im Rausche blind ertrinken,", "tokens": ["und", "im", "Rau\u00b7sche", "blind", "er\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "wird die Frucht von Deinem Licht befeuchtet.", "tokens": ["wird", "die", "Frucht", "von", "Dei\u00b7nem", "Licht", "be\u00b7feuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "So tagt es. Mit dem Ausdruck des Ver\u00e4chters", "tokens": ["So", "tagt", "es", ".", "Mit", "dem", "Aus\u00b7druck", "des", "Ver\u00b7\u00e4ch\u00b7ters"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "sollst du dem alten Garten kalt entschreiten:", "tokens": ["sollst", "du", "dem", "al\u00b7ten", "Gar\u00b7ten", "kalt", "ent\u00b7schrei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "dir weist die Pfauenfeder unsres W\u00e4chters", "tokens": ["dir", "weist", "die", "Pfau\u00b7en\u00b7fe\u00b7der", "uns\u00b7res", "W\u00e4ch\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Unsterblichkeiten.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}