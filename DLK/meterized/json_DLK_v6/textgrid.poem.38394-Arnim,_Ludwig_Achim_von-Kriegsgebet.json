{"textgrid.poem.38394": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Kriegsgebet", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bet' Kinder bet',", "tokens": ["Bet'", "Kin\u00b7der", "bet'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Morge kommt der Schwed,", "tokens": ["Mor\u00b7ge", "kommt", "der", "Schwed", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Morge kommt der Oxestern,", "tokens": ["Mor\u00b7ge", "kommt", "der", "O\u00b7xes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der wird die Kinder bete lern.", "tokens": ["Der", "wird", "die", "Kin\u00b7der", "be\u00b7te", "lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}