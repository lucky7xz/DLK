{"textgrid.poem.34812": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Merlin, der eitle Weise,", "tokens": ["Wie", "Mer\u00b7lin", ",", "der", "eit\u00b7le", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich armer Nekromant", "tokens": ["Bin", "ich", "ar\u00b7mer", "Ne\u00b7kro\u00b7mant"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun am Ende festgebannt", "tokens": ["Nun", "am", "En\u00b7de", "fest\u00b7ge\u00b7bannt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die eignen Zauberkreise.", "tokens": ["In", "die", "eig\u00b7nen", "Zau\u00b7ber\u00b7krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Festgebannt zu ihren F\u00fc\u00dfen", "tokens": ["Fest\u00b7ge\u00b7bannt", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieg ich nun, und immerdar", "tokens": ["Lieg", "ich", "nun", ",", "und", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau ich in ihr Augenpaar;", "tokens": ["Schau", "ich", "in", "ihr", "Au\u00b7gen\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Stunden, sie verflie\u00dfen.", "tokens": ["Und", "die", "Stun\u00b7den", ",", "sie", "ver\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stunden, Tage, ganze Wochen,", "tokens": ["Stun\u00b7den", ",", "Ta\u00b7ge", ",", "gan\u00b7ze", "Wo\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie verflie\u00dfen wie ein Traum,", "tokens": ["Sie", "ver\u00b7flie\u00b7\u00dfen", "wie", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich rede, wei\u00df ich kaum,", "tokens": ["Was", "ich", "re\u00b7de", ",", "wei\u00df", "ich", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wei\u00df auch nicht, was sie gesprochen.", "tokens": ["Wei\u00df", "auch", "nicht", ",", "was", "sie", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Manchmal ist mir, als ber\u00fchren", "tokens": ["Manch\u00b7mal", "ist", "mir", ",", "als", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Lippen meinen Mund \u2013", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "mei\u00b7nen", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis in meiner Seele Grund", "tokens": ["Bis", "in", "mei\u00b7ner", "See\u00b7le", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ich dann die Flammen sp\u00fcren.", "tokens": ["Kann", "ich", "dann", "die", "Flam\u00b7men", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}