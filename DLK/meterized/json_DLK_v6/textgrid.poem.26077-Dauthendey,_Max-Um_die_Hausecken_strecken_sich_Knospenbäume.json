{"textgrid.poem.26077": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Um die Hausecken strecken sich Knospenb\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Hausecken strecken sich Knospenb\u00e4ume,", "tokens": ["Um", "die", "Haus\u00b7e\u00b7cken", "stre\u00b7cken", "sich", "Knos\u00b7pen\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PRF", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Recken sich goldene Knospenhecken,", "tokens": ["Re\u00b7cken", "sich", "gol\u00b7de\u00b7ne", "Knos\u00b7pen\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gehen hell hervor aus ihren Verstecken;", "tokens": ["Ge\u00b7hen", "hell", "her\u00b7vor", "aus", "ih\u00b7ren", "Ver\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sehen schnell \u00fcbers Dach,", "tokens": ["Se\u00b7hen", "schnell", "\u00fc\u00b7bers", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Aus finsterem Hof und steinernem Fach,", "tokens": ["Aus", "fins\u00b7te\u00b7rem", "Hof", "und", "stei\u00b7ner\u00b7nem", "Fach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wollen um die Mauer der Sonne nach.", "tokens": ["Wol\u00b7len", "um", "die", "Mau\u00b7er", "der", "Son\u00b7ne", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sie stehen lusthoch in den Himmel verstiegen.", "tokens": ["Sie", "ste\u00b7hen", "lust\u00b7hoch", "in", "den", "Him\u00b7mel", "ver\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Eines Abends beginnen sich alle zu wiegen", "tokens": ["Ei\u00b7nes", "A\u00b7bends", "be\u00b7gin\u00b7nen", "sich", "al\u00b7le", "zu", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PIS", "PTKZU", "VVINF"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und lehren Verliebten im Nachtrausch zu fliegen.", "tokens": ["Und", "leh\u00b7ren", "Ver\u00b7lieb\u00b7ten", "im", "Nach\u00b7trausch", "zu", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}