{"textgrid.poem.31370": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Welt Lauf", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr und Petrus oft, in ihrer Liebe beide,", "tokens": ["Der", "Herr", "und", "Pet\u00b7rus", "oft", ",", "in", "ih\u00b7rer", "Lie\u00b7be", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "FM", "ADV", "$,", "APPR", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begegneten im Streite sich,", "tokens": ["Be\u00b7ge\u00b7gne\u00b7ten", "im", "Strei\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn von der Menschen Heil die Rede war;", "tokens": ["Wenn", "von", "der", "Men\u00b7schen", "Heil", "die", "Re\u00b7de", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dieser nannte zwar die Gnade Gottes gro\u00df,", "tokens": ["Und", "die\u00b7ser", "nann\u00b7te", "zwar", "die", "Gna\u00b7de", "Got\u00b7tes", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch w\u00e4r er Herr der Welt, meint' er,", "tokens": ["Doch", "w\u00e4r", "er", "Herr", "der", "Welt", ",", "meint'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "W\u00fcrd er sich ihrer mehr erbarmen.", "tokens": ["W\u00fcrd", "er", "sich", "ih\u00b7rer", "mehr", "er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da trat, zu einer Zeit, als l\u00e4ngst, in beider Herzen,", "tokens": ["Da", "trat", ",", "zu", "ei\u00b7ner", "Zeit", ",", "als", "l\u00e4ngst", ",", "in", "bei\u00b7der", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "$,", "KOUS", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Streit vergessen schien, und just,", "tokens": ["Der", "Streit", "ver\u00b7ges\u00b7sen", "schien", ",", "und", "just", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Um welcher Ursach wei\u00df ich nicht,", "tokens": ["Um", "wel\u00b7cher", "Ur\u00b7sach", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Himmel oben auch voll Wolken hing,", "tokens": ["Der", "Him\u00b7mel", "o\u00b7ben", "auch", "voll", "Wol\u00b7ken", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Sanktus, mi\u00dfgestimmt, den Heiland an, und sprach:", "tokens": ["Der", "Sank\u00b7tus", ",", "mi\u00df\u00b7ge\u00b7stimmt", ",", "den", "Hei\u00b7land", "an", ",", "und", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u00bbherr, la\u00df, auf eine Handvoll Zeit,", "tokens": ["\u00bb", "herr", ",", "la\u00df", ",", "auf", "ei\u00b7ne", "Hand\u00b7voll", "Zeit", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVIMP", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mich, aus dem Himmelreich, auf Erden niederfahren,", "tokens": ["Mich", ",", "aus", "dem", "Him\u00b7mel\u00b7reich", ",", "auf", "Er\u00b7den", "nie\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ich des Unmuts, der mich griff,", "tokens": ["Da\u00df", "ich", "des", "Un\u00b7muts", ",", "der", "mich", "griff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Verge\u00df und mich einmal, von Sorgen frei, erg\u00f6tze,", "tokens": ["Ver\u00b7ge\u00df", "und", "mich", "ein\u00b7mal", ",", "von", "Sor\u00b7gen", "frei", ",", "er\u00b7g\u00f6t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "PPER", "ADV", "$,", "APPR", "NN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil es jetzt grad vor Fastnacht ist.\u00ab", "tokens": ["Weil", "es", "jetzt", "grad", "vor", "Fast\u00b7nacht", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der Herr, des Streits noch sinnig eingedenk,", "tokens": ["Der", "Herr", ",", "des", "Streits", "noch", "sin\u00b7nig", "ein\u00b7ge\u00b7denk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Spricht: \u00bbGut; acht Tag geb ich dir Zeit,", "tokens": ["Spricht", ":", "\u00bb", "Gut", ";", "acht", "Tag", "geb", "ich", "dir", "Zeit", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "ADJD", "$.", "CARD", "NN", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Feier, die mir dort beginnt, dich beizumischen;", "tokens": ["Der", "Fei\u00b7er", ",", "die", "mir", "dort", "be\u00b7ginnt", ",", "dich", "bei\u00b7zu\u00b7mi\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jedoch, sobald das Fest vorbei,", "tokens": ["Je\u00b7doch", ",", "so\u00b7bald", "das", "Fest", "vor\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Kommst du mir zur gesetzten Stunde wieder.\u00ab", "tokens": ["Kommst", "du", "mir", "zur", "ge\u00b7setz\u00b7ten", "Stun\u00b7de", "wie\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Acht volle Tage doch, zwei Wochen schon, und mehr,", "tokens": ["Acht", "vol\u00b7le", "Ta\u00b7ge", "doch", ",", "zwei", "Wo\u00b7chen", "schon", ",", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADV", "$,", "CARD", "NN", "ADV", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein abgez\u00e4hlter Mond vergeht,", "tokens": ["Ein", "ab\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Mond", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Bevor der Sankt zum Himmel wiederkehrt.", "tokens": ["Be\u00b7vor", "der", "Sankt", "zum", "Him\u00b7mel", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "\u00bbei, Petre\u00ab, spricht der Herr, \u00bbwo weiltest du so lange?", "tokens": ["\u00bb", "ei", ",", "Pe\u00b7tre", "\u00ab", ",", "spricht", "der", "Herr", ",", "\u00bb", "wo", "weil\u00b7test", "du", "so", "lan\u00b7ge", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "$(", "$,", "VVFIN", "ART", "NN", "$,", "$(", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Gefiels auch nieden dir so wohl?\u00ab", "tokens": ["Ge\u00b7fiels", "auch", "nie\u00b7den", "dir", "so", "wohl", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PIS", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Sanktus, mit noch schwerem Kopfe, spricht:", "tokens": ["Der", "Sank\u00b7tus", ",", "mit", "noch", "schwe\u00b7rem", "Kop\u00b7fe", ",", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "\u00bbach, Herr! Das war ein Jubel unten \u2013!", "tokens": ["\u00bb", "ach", ",", "Herr", "!", "Das", "war", "ein", "Ju\u00b7bel", "un\u00b7ten", "\u2013", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "ADV", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der Himmel selbst beseliget nicht besser.", "tokens": ["Der", "Him\u00b7mel", "selbst", "be\u00b7se\u00b7li\u00b7get", "nicht", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die Ernte, reich, du wei\u00dft, wie keine je gewesen,", "tokens": ["Die", "Ern\u00b7te", ",", "reich", ",", "du", "wei\u00dft", ",", "wie", "kei\u00b7ne", "je", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "PPER", "VVFIN", "$,", "PWAV", "PIAT", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gab alles was das Herz nur w\u00fcnscht,", "tokens": ["Gab", "al\u00b7les", "was", "das", "Herz", "nur", "w\u00fcnscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Getreide, wei\u00df und s\u00fc\u00df, Most, sag ich dir, wie Honig,", "tokens": ["Ge\u00b7trei\u00b7de", ",", "wei\u00df", "und", "s\u00fc\u00df", ",", "Most", ",", "sag", "ich", "dir", ",", "wie", "Ho\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "ADJD", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Fleisch fett, dem Speck gleich, von der Brust des Rindes;", "tokens": ["Fleisch", "fett", ",", "dem", "Speck", "gleich", ",", "von", "der", "Brust", "des", "Rin\u00b7des", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.34": {"text": "Kurz, von der Erde jeglichem Erzeugnis", "tokens": ["Kurz", ",", "von", "der", "Er\u00b7de", "jeg\u00b7li\u00b7chem", "Er\u00b7zeug\u00b7nis"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.35": {"text": "Zum Brechen alle Tafeln voll.", "tokens": ["Zum", "Bre\u00b7chen", "al\u00b7le", "Ta\u00b7feln", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Da lie\u00df ichs, schier, zu wohl mir sein,", "tokens": ["Da", "lie\u00df", "ichs", ",", "schier", ",", "zu", "wohl", "mir", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADJD", "$,", "APPR", "ADV", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und h\u00e4tte bald des Himmels gar vergessen.\u00ab", "tokens": ["Und", "h\u00e4t\u00b7te", "bald", "des", "Him\u00b7mels", "gar", "ver\u00b7ges\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Der Herr erwidert: \u00bbGut! Doch Petre sag mir an,", "tokens": ["Der", "Herr", "er\u00b7wi\u00b7dert", ":", "\u00bb", "Gut", "!", "Doch", "Pe\u00b7tre", "sag", "mir", "an", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$(", "ADJD", "$.", "KON", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bei soviel Segen, den ich ausgesch\u00fcttet,", "tokens": ["Bei", "so\u00b7viel", "Se\u00b7gen", ",", "den", "ich", "aus\u00b7ge\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Hat man auch dankbar mein gedacht?", "tokens": ["Hat", "man", "auch", "dank\u00b7bar", "mein", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Sahst du die Kirchen auch von Menschen voll?\u00ab \u2013", "tokens": ["Sahst", "du", "die", "Kir\u00b7chen", "auch", "von", "Men\u00b7schen", "voll", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Der Sankt, best\u00fcrzt hierauf, nachdem er sich besonnen,", "tokens": ["Der", "Sankt", ",", "be\u00b7st\u00fcrzt", "hier\u00b7auf", ",", "nach\u00b7dem", "er", "sich", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "PAV", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "\u00bbo Herr\u00ab, spricht er, \u00bbbei meiner Liebe,", "tokens": ["\u00bb", "o", "Herr", "\u00ab", ",", "spricht", "er", ",", "\u00bb", "bei", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Den ganzen Fastmond durch, wo ich mich hingewendet,", "tokens": ["Den", "gan\u00b7zen", "Fast\u00b7mond", "durch", ",", "wo", "ich", "mich", "hin\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.45": {"text": "Nicht deinen Namen h\u00f6rt ich nennen.", "tokens": ["Nicht", "dei\u00b7nen", "Na\u00b7men", "h\u00f6rt", "ich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Ein einzger Mann sa\u00df murmelnd in der Kirche:", "tokens": ["Ein", "einz\u00b7ger", "Mann", "sa\u00df", "mur\u00b7melnd", "in", "der", "Kir\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Der aber war ein Wucherer,", "tokens": ["Der", "a\u00b7ber", "war", "ein", "Wu\u00b7che\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.48": {"text": "Und hatte Korn, im Herbst erstanden,", "tokens": ["Und", "hat\u00b7te", "Korn", ",", "im", "Herbst", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "F\u00fcr M\u00e4us und Ratzen hungrig aufgesch\u00fcttet.\u00ab \u2013", "tokens": ["F\u00fcr", "M\u00e4us", "und", "Rat\u00b7zen", "hung\u00b7rig", "auf\u00b7ge\u00b7sch\u00fct\u00b7tet", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "w