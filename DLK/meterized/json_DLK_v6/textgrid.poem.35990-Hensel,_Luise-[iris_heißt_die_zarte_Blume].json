{"textgrid.poem.35990": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[iris hei\u00dft die zarte Blume]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die der Erde still erbl\u00fcht,", "tokens": ["Die", "der", "Er\u00b7de", "still", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch mit ihren blauen Augen", "tokens": ["Doch", "mit", "ih\u00b7ren", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zum Blau des Himmels sieht.", "tokens": ["Nur", "zum", "Blau", "des", "Him\u00b7mels", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deinen Lebenspfad zu schm\u00fccken", "tokens": ["Dei\u00b7nen", "Le\u00b7ben\u00b7spfad", "zu", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier im dunklen Schattenland,", "tokens": ["Hier", "im", "dunk\u00b7len", "Schat\u00b7ten\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der gute ", "tokens": ["Hat", "der", "gu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}}}}