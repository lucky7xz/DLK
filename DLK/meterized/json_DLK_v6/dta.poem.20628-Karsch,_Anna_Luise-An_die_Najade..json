{"dta.poem.20628": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die Najade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Melpomene, die selbst den Fischgeschlechtern,                 ", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", ",", "die", "selbst", "den", "Fischge\u00b7schlech\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Selbst deinen Schw\u00e4nen Stimme giebt,", "tokens": ["Selbst", "dei\u00b7nen", "Schw\u00e4\u00b7nen", "Stim\u00b7me", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empfing am Havelstrom von allen F\u00fcrstent\u00f6chtern", "tokens": ["Emp\u00b7fing", "am", "Ha\u00b7vel\u00b7strom", "von", "al\u00b7len", "F\u00fcrs\u00b7ten\u00b7t\u00f6ch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sch\u00f6nste, die der Halbgott liebt,", "tokens": ["Die", "Sch\u00f6ns\u00b7te", ",", "die", "der", "Halb\u00b7gott", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Pallas und Dein erster Schutzheld auferzogen;", "tokens": ["Den", "Pal\u00b7las", "und", "Dein", "ers\u00b7ter", "Schutz\u00b7held", "auf\u00b7er\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und jezt befiehlt die Muse mir", "tokens": ["Und", "jezt", "be\u00b7fiehlt", "die", "Mu\u00b7se", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gesang an Dich: Komm! unter jenen Ehrenbogen", "tokens": ["Ge\u00b7sang", "an", "Dich", ":", "Komm", "!", "un\u00b7ter", "je\u00b7nen", "Eh\u00b7ren\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "VVFIN", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von Myrten sing ich Dir.", "tokens": ["Von", "Myr\u00b7ten", "sing", "ich", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie k\u00f6mmt \u2014 Sie k\u00f6mmt, o g\u00f6ttliche Najade!", "tokens": ["Sie", "k\u00f6mmt", "Sie", "k\u00f6mmt", ",", "o", "g\u00f6tt\u00b7li\u00b7che", "Na\u00b7ja\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die neue Deidamia!", "tokens": ["Die", "neu\u00b7e", "De\u00b7i\u00b7da\u00b7mia", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So kam mit Grazien aus ihrem Silberbade", "tokens": ["So", "kam", "mit", "Gra\u00b7zi\u00b7en", "aus", "ih\u00b7rem", "Sil\u00b7ber\u00b7ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Venus Acidalia;", "tokens": ["Die", "Ve\u00b7nus", "A\u00b7ci\u00b7da\u00b7lia", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So br\u00e4utlich ward die Jugendg\u00f6ttinn Hebe", "tokens": ["So", "br\u00e4ut\u00b7lich", "ward", "die", "Ju\u00b7gend\u00b7g\u00f6t\u00b7tinn", "He\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Sohn Alkmenens zugef\u00fchrt,", "tokens": ["Dem", "Sohn", "A\u00b7lkme\u00b7nens", "zu\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und so liebreizend ist Apollens Schwester Ph\u00f6be,", "tokens": ["Und", "so", "lieb\u00b7rei\u00b7zend", "ist", "A\u00b7pol\u00b7lens", "Schwes\u00b7ter", "Ph\u00f6\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "NE", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn sie die N\u00e4chte ziert.", "tokens": ["Wenn", "sie", "die", "N\u00e4ch\u00b7te", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Auge gleicht dem hellen Morgensterne,", "tokens": ["Ihr", "Au\u00b7ge", "gleicht", "dem", "hel\u00b7len", "Mor\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der im Azurnen Aether schwimmt,", "tokens": ["Der", "im", "A\u00b7zur\u00b7nen", "A\u00b7e\u00b7ther", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Zum Trost des Steuermanns, der noch vom Hafen ferne", "tokens": ["Zum", "Trost", "des", "Steu\u00b7er\u00b7manns", ",", "der", "noch", "vom", "Ha\u00b7fen", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Fahrt durch wilde Wellen nimmt.", "tokens": ["Die", "Fahrt", "durch", "wil\u00b7de", "Wel\u00b7len", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In s\u00fc\u00dfen Blicken spricht Sie mit des Prinzen Seele,", "tokens": ["In", "s\u00fc\u00b7\u00dfen", "Bli\u00b7cken", "spricht", "Sie", "mit", "des", "Prin\u00b7zen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der feurigen Gef\u00fchles voll", "tokens": ["Der", "feu\u00b7ri\u00b7gen", "Ge\u00b7f\u00fch\u00b7les", "voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem jungen Helden gleicht, der einst in Cirons H\u00f6hle", "tokens": ["Dem", "jun\u00b7gen", "Hel\u00b7den", "gleicht", ",", "der", "einst", "in", "Ci\u00b7rons", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Beschenkt ward vom Apoll.", "tokens": ["Be\u00b7schenkt", "ward", "vom", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Er geizet gleich der Thetis tapferm Sohne", "tokens": ["Er", "gei\u00b7zet", "gleich", "der", "The\u00b7tis", "tap\u00b7ferm", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr nach dem Herzen Seiner Braut,", "tokens": ["Mehr", "nach", "dem", "Her\u00b7zen", "Sei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Als k\u00fcnftig nach dem Glanz der diamantnen Krone,", "tokens": ["Als", "k\u00fcnf\u00b7tig", "nach", "dem", "Glanz", "der", "di\u00b7a\u00b7mant\u00b7nen", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Ihm das Schicksal anvertraut,", "tokens": ["Die", "Ihm", "das", "Schick\u00b7sal", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Sein fernefunkelnd Ziel erreicht,", "tokens": ["Sein", "fer\u00b7ne\u00b7fun\u00b7kelnd", "Ziel", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, die bekannte Zahl der G\u00f6tter zu vermehren,", "tokens": ["Und", ",", "die", "be\u00b7kann\u00b7te", "Zahl", "der", "G\u00f6t\u00b7ter", "zu", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach dem Olympus fleucht:", "tokens": ["Nach", "dem", "O\u00b7lym\u00b7pus", "fleucht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nachdem Sein Blick von jungen Antoninen,", "tokens": ["Nach\u00b7dem", "Sein", "Blick", "von", "jun\u00b7gen", "An\u00b7to\u00b7ni\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und Marc Aurelen, seinen Thron", "tokens": ["Und", "Marc", "Au\u00b7re\u00b7len", ",", "sei\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Genug umgeben sieht, die mit den J\u00fcnglingsmienen", "tokens": ["Ge\u00b7nug", "um\u00b7ge\u00b7ben", "sieht", ",", "die", "mit", "den", "J\u00fcng\u00b7lings\u00b7mie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und stolzen M\u00e4nnerstirnen drohn", "tokens": ["Und", "stol\u00b7zen", "M\u00e4n\u00b7ners\u00b7tir\u00b7nen", "drohn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Feinden Deines Ruhms, und Muth zum Siege", "tokens": ["Den", "Fein\u00b7den", "Dei\u00b7nes", "Ruhms", ",", "und", "Muth", "zum", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "f\u00fchlen;", "tokens": ["f\u00fch\u00b7len", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und wenn der Friede, Land und Heer", "tokens": ["Und", "wenn", "der", "Frie\u00b7de", ",", "Land", "und", "Heer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Ruhe tr\u00e4nket, des Achilles Leyer spielen,", "tokens": ["Mit", "Ru\u00b7he", "tr\u00e4n\u00b7ket", ",", "des", "A\u00b7chil\u00b7les", "Le\u00b7yer", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ART", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}}}}}