{"textgrid.poem.38165": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Jerusalem", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat Lust mit mir zu ziehen", "tokens": ["Wer", "hat", "Lust", "mit", "mir", "zu", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "NN", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Stadt Jerusalem,", "tokens": ["Nach", "der", "Stadt", "Je\u00b7ru\u00b7sa\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Denn darinnen kann man sehen", "tokens": ["Denn", "da\u00b7rin\u00b7nen", "kann", "man", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der weise Salomon", "tokens": ["Was", "der", "wei\u00b7se", "Sa\u00b7lo\u00b7mon"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat gebauet Schl\u00f6sser, Kirchen,", "tokens": ["Hat", "ge\u00b7bau\u00b7et", "Schl\u00f6s\u00b7ser", ",", "Kir\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "VVPP", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles ist von Stein und Holz,", "tokens": ["Al\u00b7les", "ist", "von", "Stein", "und", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles \u00fcberzogen worden", "tokens": ["Al\u00b7les", "\u00fc\u00b7berz\u00b7o\u00b7gen", "wor\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVPP", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem Silber und rothen Gold.", "tokens": ["Mit", "dem", "Sil\u00b7ber", "und", "ro\u00b7then", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Der Glanz ist nicht auszusprechen", "tokens": ["Der", "Glanz", "ist", "nicht", "aus\u00b7zu\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von der Stadt Jerusalem,", "tokens": ["Von", "der", "Stadt", "Je\u00b7ru\u00b7sa\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "David spielet auf der Harfe,", "tokens": ["Da\u00b7vid", "spie\u00b7let", "auf", "der", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Benjamin spielt Fl\u00f6ttraw\u00e4r,", "tokens": ["Ben\u00b7ja\u00b7min", "spielt", "Fl\u00f6t\u00b7tra\u00b7w\u00e4r", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Isaak tanzet mit Rebekka,", "tokens": ["I\u00b7saak", "tan\u00b7zet", "mit", "Re\u00b7bek\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Jakob mit der sch\u00f6nen Rahel.", "tokens": ["Ja\u00b7kob", "mit", "der", "sch\u00f6\u00b7nen", "Ra\u00b7hel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Zu der grossen Freudenwonne,", "tokens": ["Zu", "der", "gros\u00b7sen", "Freu\u00b7den\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dem Schlo\u00df Jerusalem.", "tokens": ["Auf", "dem", "Schlo\u00df", "Je\u00b7ru\u00b7sa\u00b7lem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "O Jerusalem du sch\u00f6ne,", "tokens": ["O", "Je\u00b7ru\u00b7sa\u00b7lem", "du", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie sch\u00f6ne gl\u00e4nzest du,", "tokens": ["O", "wie", "sch\u00f6\u00b7ne", "gl\u00e4n\u00b7zest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJA", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ey w\u00e4rst du nur in der N\u00e4he,", "tokens": ["Ey", "w\u00e4rst", "du", "nur", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So w\u00e4r ich schon l\u00e4ngst bey dir.", "tokens": ["So", "w\u00e4r", "ich", "schon", "l\u00e4ngst", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach wenn ich ein Engel w\u00e4re,", "tokens": ["Ach", "wenn", "ich", "ein", "En\u00b7gel", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich heut noch fliegen k\u00f6nnt,", "tokens": ["Da\u00df", "ich", "heut", "noch", "flie\u00b7gen", "k\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In die H\u00f6h wollt ich mich schwingen,", "tokens": ["In", "die", "H\u00f6h", "wollt", "ich", "mich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und fliegen nach Jerusalem.", "tokens": ["Und", "flie\u00b7gen", "nach", "Je\u00b7ru\u00b7sa\u00b7lem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "So lang sich mein Herz beweget,", "tokens": ["So", "lang", "sich", "mein", "Herz", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ein warmes Tr\u00f6pflein Blut,", "tokens": ["Und", "ein", "war\u00b7mes", "Tr\u00f6pf\u00b7lein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den blauen Adern reget,", "tokens": ["In", "den", "blau\u00b7en", "A\u00b7dern", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleib ich dir mein Engel gut.", "tokens": ["Bleib", "ich", "dir", "mein", "En\u00b7gel", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}