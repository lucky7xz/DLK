{"textgrid.poem.48217": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "9. Nur nicht loben", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schreibt wer in Deutschland historische St\u00fccke,", "tokens": ["Schreibt", "wer", "in", "Deutschland", "his\u00b7to\u00b7ri\u00b7sche", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So steht er auf der Schiller-Br\u00fccke.", "tokens": ["So", "steht", "er", "auf", "der", "Schil\u00b7ler\u00b7Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Macht er den Helden zugleich zum Dam\u00f6te,", "tokens": ["Macht", "er", "den", "Hel\u00b7den", "zu\u00b7gleich", "zum", "Da\u00b7m\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "So hei\u00dft es: Egmont, siehe Goethe.", "tokens": ["So", "hei\u00dft", "es", ":", "Eg\u00b7mont", ",", "sie\u00b7he", "Goe\u00b7the", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NE", "$,", "VVIMP", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schildert er Juden, ernst oder witzig,", "tokens": ["Schil\u00b7dert", "er", "Ju\u00b7den", ",", "ernst", "o\u00b7der", "wit\u00b7zig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist es Schmock oder Veitel Itzig.", "tokens": ["Ist", "es", "Schmock", "o\u00b7der", "Vei\u00b7tel", "It\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NE", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Schildert er einige h\u00fcbsche Damen,", "tokens": ["Schil\u00b7dert", "er", "ei\u00b7ni\u00b7ge", "h\u00fcb\u00b7sche", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hei\u00dft es: Dumas ... Ehebruchsdramen.", "tokens": ["Hei\u00dft", "es", ":", "Du\u00b7mas", "...", "E\u00b7heb\u00b7ruchs\u00b7dra\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NE", "$(", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jeder Einfall, statt ihn zu loben,", "tokens": ["Je\u00b7der", "Ein\u00b7fall", ",", "statt", "ihn", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wird einem andern zugeschoben.", "tokens": ["Wird", "ei\u00b7nem", "an\u00b7dern", "zu\u00b7ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Gl\u00fcck, so hab' ich oft gedacht,", "tokens": ["Ein", "Gl\u00fcck", ",", "so", "hab'", "ich", "oft", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Zola keine Balladen gemacht.", "tokens": ["Da\u00df", "Zo\u00b7la", "kei\u00b7ne", "Bal\u00b7la\u00b7den", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}