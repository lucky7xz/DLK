, "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Heut sei der Rheinstrom treuer Bote mir!", "tokens": ["Heut", "sei", "der", "Rhein\u00b7strom", "treu\u00b7er", "Bo\u00b7te", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dieselbe Flut, die jetzt zu meinen F\u00fc\u00dfen", "tokens": ["Die\u00b7sel\u00b7be", "Flut", ",", "die", "jetzt", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ans Ufer schl\u00e4gt, wird morgen dich begr\u00fc\u00dfen!", "tokens": ["Ans", "U\u00b7fer", "schl\u00e4gt", ",", "wird", "mor\u00b7gen", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Ich wei\u00df es nicht, mir ist so k\u00fchn ums Herz;", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "mir", "ist", "so", "k\u00fchn", "ums", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hell durch die Brust mir bebt ein mut'ger Klang:", "tokens": ["Hell", "durch", "die", "Brust", "mir", "bebt", "ein", "mut'\u00b7ger", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"