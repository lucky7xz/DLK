{"textgrid.poem.52209": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1L: So lehnt' er fromm dort seinen Wanderstab,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lehnt' er fromm dort seinen Wanderstab,", "tokens": ["So", "lehnt'", "er", "fromm", "dort", "sei\u00b7nen", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Heros selbst, an der Heroen Grab;", "tokens": ["Ein", "He\u00b7ros", "selbst", ",", "an", "der", "He\u00b7roen", "Grab", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gesenkt das Haupt, ein ernster Pilgersmann,", "tokens": ["Ge\u00b7senkt", "das", "Haupt", ",", "ein", "erns\u00b7ter", "Pil\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Trat an die S\u00e4rge dienend er heran,", "tokens": ["Trat", "an", "die", "S\u00e4r\u00b7ge", "die\u00b7nend", "er", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und lie\u00df voll Mut Unsterblichkeitsgedanken", "tokens": ["Und", "lie\u00df", "voll", "Mut", "U\u00b7nsterb\u00b7lich\u00b7keits\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als Totenkranz um ihren Staub sich ranken.", "tokens": ["Als", "To\u00b7ten\u00b7kranz", "um", "ih\u00b7ren", "Staub", "sich", "ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Opfer, wie er's bringen mu\u00dfte! \u2013 Keins,", "tokens": ["Ein", "Op\u00b7fer", ",", "wie", "er's", "brin\u00b7gen", "mu\u00df\u00b7te", "!", "\u2013", "Keins", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das w\u00fcrd'ger w\u00e4re! \u2013 Tief ergreift nur eins!", "tokens": ["Das", "w\u00fcrd'\u00b7ger", "w\u00e4\u00b7re", "!", "\u2013", "Tief", "er\u00b7greift", "nur", "eins", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "$.", "$(", "ADJD", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er, der Hohe selbst, der es gebracht,", "tokens": ["Da\u00df", "er", ",", "der", "Ho\u00b7he", "selbst", ",", "der", "es", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sobald schon einging in die \u00bbgro\u00dfe Nacht\u00ab;", "tokens": ["So\u00b7bald", "schon", "ein\u00b7ging", "in", "die", "\u00bb", "gro\u00b7\u00dfe", "Nacht", "\u00ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "APPR", "ART", "$(", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df er es brachte nur, um uns zu lehren,", "tokens": ["Da\u00df", "er", "es", "brach\u00b7te", "nur", ",", "um", "uns", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie wir ihn selbst im Tode w\u00fcrdig ehren!", "tokens": ["Wie", "wir", "ihn", "selbst", "im", "To\u00b7de", "w\u00fcr\u00b7dig", "eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gescheh' es denn! \u2013 Wir fassen uns ein Herz!", "tokens": ["Ge\u00b7scheh'", "es", "denn", "!", "\u2013", "Wir", "fas\u00b7sen", "uns", "ein", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verwunden jetzt der erste j\u00e4he Schmerz!", "tokens": ["Ver\u00b7wun\u00b7den", "jetzt", "der", "ers\u00b7te", "j\u00e4\u00b7he", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir wissen es, ein Gott hat ihn gef\u00e4llt,", "tokens": ["Wir", "wis\u00b7sen", "es", ",", "ein", "Gott", "hat", "ihn", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Boden reglos liegt der starke Held;", "tokens": ["Am", "Bo\u00b7den", "reg\u00b7los", "liegt", "der", "star\u00b7ke", "Held", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch eisenadrig trotzt er der Vernichtung,", "tokens": ["Doch", "ei\u00b7se\u00b7nad\u00b7rig", "trotzt", "er", "der", "Ver\u00b7nich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein edler Fels im Walde deutscher Dichtung.", "tokens": ["Ein", "ed\u00b7ler", "Fels", "im", "Wal\u00b7de", "deut\u00b7scher", "Dich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drin wird er ragen \u2013 jetzt und immerdar!", "tokens": ["Drin", "wird", "er", "ra\u00b7gen", "\u2013", "jetzt", "und", "im\u00b7mer\u00b7dar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$(", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr viele noch ein schroffes R\u00e4tsel zwar;", "tokens": ["F\u00fcr", "vie\u00b7le", "noch", "ein", "schrof\u00b7fes", "R\u00e4t\u00b7sel", "zwar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Runenstein, mit Moose rauh bedeckt,", "tokens": ["Ein", "Ru\u00b7nen\u00b7stein", ",", "mit", "Moo\u00b7se", "rauh", "be\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der den Verzagten und den Bl\u00f6den schreckt;", "tokens": ["Der", "den", "Ver\u00b7zag\u00b7ten", "und", "den", "Bl\u00f6\u00b7den", "schreckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Doch stets des Volkes Edelsten und Gr\u00f6\u00dften", "tokens": ["Doch", "stets", "des", "Vol\u00b7kes", "E\u00b7dels\u00b7ten", "und", "Gr\u00f6\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-----+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Ein ernster Freund, zu wecken und zu tr\u00f6sten!", "tokens": ["Ein", "erns\u00b7ter", "Freund", ",", "zu", "we\u00b7cken", "und", "zu", "tr\u00f6s\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie um ihn her auch toben mag der Streit,", "tokens": ["Wie", "um", "ihn", "her", "auch", "to\u00b7ben", "mag", "der", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie unterm Beil der Jahre Baum an Baum", "tokens": ["Wie", "un\u00b7term", "Beil", "der", "Jah\u00b7re", "Baum", "an", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "NE", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zusammenrasselt \u2013 er vernimmt es kaum!", "tokens": ["Zu\u00b7sam\u00b7men\u00b7ras\u00b7selt", "\u2013", "er", "ver\u00b7nimmt", "es", "kaum", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Aar des Ruhmes zieht in treuen Kreisen", "tokens": ["Der", "Aar", "des", "Ruh\u00b7mes", "zieht", "in", "treu\u00b7en", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um seine Stirn: \u2013 la\u00dft uns ihn gl\u00fccklich preisen!", "tokens": ["Um", "sei\u00b7ne", "Stirn", ":", "\u2013", "la\u00dft", "uns", "ihn", "gl\u00fcck\u00b7lich", "prei\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "$(", "VVIMP", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und doppelt gl\u00fccklich, weil mit ehrnem Tritt,", "tokens": ["Und", "dop\u00b7pelt", "gl\u00fcck\u00b7lich", ",", "weil", "mit", "ehr\u00b7nem", "Tritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Recht als ein Sieger, er von dannen schritt;", "tokens": ["Recht", "als", "ein", "Sie\u00b7ger", ",", "er", "von", "dan\u00b7nen", "schritt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil, eh' er ihn verlie\u00df, auf seinem Pfad", "tokens": ["Weil", ",", "eh'", "er", "ihn", "ver\u00b7lie\u00df", ",", "auf", "sei\u00b7nem", "Pfad"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sieg noch auf Sieg, Tat folgte noch auf Tat,", "tokens": ["Sieg", "noch", "auf", "Sieg", ",", "Tat", "folg\u00b7te", "noch", "auf", "Tat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und weil, die sp\u00e4t noch in sein Leben gl\u00e4nzte,", "tokens": ["Und", "weil", ",", "die", "sp\u00e4t", "noch", "in", "sein", "Le\u00b7ben", "gl\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PRELS", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weinend die Liebe seinen Tod bekr\u00e4nzte!", "tokens": ["Wei\u00b7nend", "die", "Lie\u00b7be", "sei\u00b7nen", "Tod", "be\u00b7kr\u00e4nz\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "So wurden die Heroen einst entr\u00fcckt!", "tokens": ["So", "wur\u00b7den", "die", "He\u00b7roen", "einst", "ent\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So die Propheten! \u2013 Nachsah tief geb\u00fcckt", "tokens": ["So", "die", "Pro\u00b7phe\u00b7ten", "!", "\u2013", "Nach\u00b7sah", "tief", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "$(", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Volks, der N\u00e4chsten kummervolle Schar!", "tokens": ["Des", "Volks", ",", "der", "N\u00e4chs\u00b7ten", "kum\u00b7mer\u00b7vol\u00b7le", "Schar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald aber senkte Tr\u00f6stung wunderbar", "tokens": ["Bald", "a\u00b7ber", "senk\u00b7te", "Tr\u00f6s\u00b7tung", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In ihre Brust sich! Sie erhuben Steine", "tokens": ["In", "ih\u00b7re", "Brust", "sich", "!", "Sie", "er\u00b7hu\u00b7ben", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und legten Kr\u00e4nze drauf! \u2013 Wo steht der seine?", "tokens": ["Und", "leg\u00b7ten", "Kr\u00e4n\u00b7ze", "drauf", "!", "\u2013", "Wo", "steht", "der", "sei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$.", "$(", "PWAV", "VVFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sucht ihn nicht auf in einer F\u00fcrstengruft!", "tokens": ["Sucht", "ihn", "nicht", "auf", "in", "ei\u00b7ner", "F\u00fcrs\u00b7ten\u00b7gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er hat ein Grab in frischer Rheinesluft;", "tokens": ["Er", "hat", "ein", "Grab", "in", "fri\u00b7scher", "Rhei\u00b7nes\u00b7luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Land der Berge sendet Waldeshauch", "tokens": ["Das", "Land", "der", "Ber\u00b7ge", "sen\u00b7det", "Wal\u00b7des\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem jungen Gras, dem jungen Rosenstrauch,", "tokens": ["Dem", "jun\u00b7gen", "Gras", ",", "dem", "jun\u00b7gen", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die es umwehn; frei netzt es Tau und Wolke \u2013", "tokens": ["Die", "es", "um\u00b7wehn", ";", "frei", "netzt", "es", "Tau", "und", "Wol\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$.", "ADJD", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bei F\u00fcrsten nicht, er ruht bei seinem Volke.", "tokens": ["Bei", "F\u00fcrs\u00b7ten", "nicht", ",", "er", "ruht", "bei", "sei\u00b7nem", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sei es ein Zeichen! \u2013 Wie wir ruhn ihn sehn", "tokens": ["Sei", "es", "ein", "Zei\u00b7chen", "!", "\u2013", "Wie", "wir", "ruhn", "ihn", "sehn"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "$(", "PWAV", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei allem Volke, wird er auferstehn", "tokens": ["Bei", "al\u00b7lem", "Vol\u00b7ke", ",", "wird", "er", "auf\u00b7er\u00b7stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "$,", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Herzen auch des Volks: \u2013 er selbst, verkl\u00e4rt", "tokens": ["Im", "Her\u00b7zen", "auch", "des", "Volks", ":", "\u2013", "er", "selbst", ",", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$.", "$(", "PPER", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In uns, in andern! Ew'gen Lebens Herd", "tokens": ["In", "uns", ",", "in", "an\u00b7dern", "!", "Ew'\u00b7gen", "Le\u00b7bens", "Herd"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "$.", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dies stumme Grab, auf das wir sinnend blicken,", "tokens": ["Dies", "stum\u00b7me", "Grab", ",", "auf", "das", "wir", "sin\u00b7nend", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und es nach Kr\u00e4ften w\u00fcrdig m\u00f6chten schm\u00fccken!", "tokens": ["Und", "es", "nach", "Kr\u00e4f\u00b7ten", "w\u00fcr\u00b7dig", "m\u00f6ch\u00b7ten", "schm\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sein bester Schmuck, was er uns selbst vermacht!", "tokens": ["Sein", "bes\u00b7ter", "Schmuck", ",", "was", "er", "uns", "selbst", "ver\u00b7macht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was er im Herzen frisch uns angefacht:", "tokens": ["Was", "er", "im", "Her\u00b7zen", "frisch", "uns", "an\u00b7ge\u00b7facht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erinnerung, Gedanke, Bild und Wort,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", ",", "Ge\u00b7dan\u00b7ke", ",", "Bild", "und", "Wort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weih' es in Andacht jeder diesem Ort!", "tokens": ["Weih'", "es", "in", "An\u00b7dacht", "je\u00b7der", "die\u00b7sem", "Ort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PIS", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kehr' es ihm wieder, rein und ohne Fehle \u2013", "tokens": ["Kehr'", "es", "ihm", "wie\u00b7der", ",", "rein", "und", "oh\u00b7ne", "Feh\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "$,", "ADJD", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mir klingt es also recht in tiefer Seele:", "tokens": ["Mir", "klingt", "es", "al\u00b7so", "recht", "in", "tie\u00b7fer", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "O, schweift' ich wieder, wo ein Bursch' ich war,", "tokens": ["O", ",", "schweift'", "ich", "wie\u00b7der", ",", "wo", "ein", "Bur\u00b7sch'", "ich", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf meiner Heimat waldbewachsner Haar,", "tokens": ["Auf", "mei\u00b7ner", "Hei\u00b7mat", "wald\u00b7be\u00b7wachs\u00b7ner", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O, st\u00e4nd' ich wieder, wenn die Drossel schl\u00e4gt,", "tokens": ["O", ",", "st\u00e4nd'", "ich", "wie\u00b7der", ",", "wenn", "die", "Dros\u00b7sel", "schl\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf Lisbeths, Oswalds, meinem eignen Boden \u2013", "tokens": ["Auf", "Lis\u00b7beths", ",", "Os\u00b7walds", ",", "mei\u00b7nem", "eig\u00b7nen", "Bo\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da br\u00e4ch' ich still des Holzes gr\u00fcnste Loden!", "tokens": ["Da", "br\u00e4ch'", "ich", "still", "des", "Hol\u00b7zes", "gr\u00fcns\u00b7te", "Lo\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und fl\u00f6chte sie zum schattenreichen Kranz;", "tokens": ["Und", "fl\u00f6ch\u00b7te", "sie", "zum", "schat\u00b7ten\u00b7rei\u00b7chen", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den sollt' er haben, frisch und voll und ganz;", "tokens": ["Den", "sollt'", "er", "ha\u00b7ben", ",", "frisch", "und", "voll", "und", "ganz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAFIN", "$,", "ADJD", "KON", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den legt' ich fromm auf seinen schlichten Stein!", "tokens": ["Den", "legt'", "ich", "fromm", "auf", "sei\u00b7nen", "schlich\u00b7ten", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Westf\u00e4lisch Laub! Es m\u00fc\u00dft' ihn doch erfreun!", "tokens": ["West\u00b7f\u00e4\u00b7lisch", "Laub", "!", "Es", "m\u00fc\u00dft'", "ihn", "doch", "er\u00b7freun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gewi\u00df, er n\u00e4hm' ihn \u2013 aus der Bl\u00e4tterf\u00fclle", "tokens": ["Ge\u00b7wi\u00df", ",", "er", "n\u00e4hm'", "ihn", "\u2013", "aus", "der", "Bl\u00e4t\u00b7ter\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Eichkamps seiner pr\u00e4chtigen Idylle!", "tokens": ["Des", "Eich\u00b7kamps", "sei\u00b7ner", "pr\u00e4ch\u00b7ti\u00b7gen", "I\u00b7dyl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und zu des Kranzes Rauschen spr\u00e4ch' ich dann:", "tokens": ["Und", "zu", "des", "Kran\u00b7zes", "Rau\u00b7schen", "spr\u00e4ch'", "ich", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das soll ein Dank sein, du gewalt'ger Mann!", "tokens": ["Das", "soll", "ein", "Dank", "sein", ",", "du", "ge\u00b7walt'\u00b7ger", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du Mann der Liebe, wie der schroffen Kraft,", "tokens": ["Du", "Mann", "der", "Lie\u00b7be", ",", "wie", "der", "schrof\u00b7fen", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wahr, fest, beharrlich, eisern-eichenhaft,", "tokens": ["Wahr", ",", "fest", ",", "be\u00b7harr\u00b7lich", ",", "ei\u00b7sern\u00b7ei\u00b7chen\u00b7haft", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fast wie dein Hofschulz! einen stillen Segen", "tokens": ["Fast", "wie", "dein", "Hof\u00b7schulz", "!", "ei\u00b7nen", "stil\u00b7len", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und diesen Kranz la\u00df auf dein Grab mich legen!", "tokens": ["Und", "die\u00b7sen", "Kranz", "la\u00df", "auf", "dein", "Grab", "mich", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Du wei\u00dft es nicht, was ich dir schuldig bin!", "tokens": ["Du", "wei\u00dft", "es", "nicht", ",", "was", "ich", "dir", "schul\u00b7dig", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf dich, als Leuchtturm, blick' ich t\u00e4glich hin!", "tokens": ["Auf", "dich", ",", "als", "Leucht\u00b7turm", ",", "blick'", "ich", "t\u00e4g\u00b7lich", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Kunst und Leben irrt' ich, ach, schon viel:", "tokens": ["In", "Kunst", "und", "Le\u00b7ben", "irrt'", "ich", ",", "ach", ",", "schon", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "$,", "ITJ", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein hohes Bild gab Richtung mir und Ziel!", "tokens": ["Dein", "ho\u00b7hes", "Bild", "gab", "Rich\u00b7tung", "mir", "und", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus deinem Grabe noch vor wenig Wochen", "tokens": ["Aus", "dei\u00b7nem", "Gra\u00b7be", "noch", "vor", "we\u00b7nig", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT