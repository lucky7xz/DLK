{"textgrid.poem.66861": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Krieg dem Kriege", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Dichter schrieb: \u00bbDer Krieg mu\u00df bleiben,", "tokens": ["Ein", "Dich\u00b7ter", "schrieb", ":", "\u00bb", "Der", "Krieg", "mu\u00df", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist notwendig, menschlich, sch\u00f6n!\u00ab", "tokens": ["Er", "ist", "not\u00b7wen\u00b7dig", ",", "menschlich", ",", "sch\u00f6n", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mich aber lehrt die Wahrheit schreiben", "tokens": ["Mich", "a\u00b7ber", "lehrt", "die", "Wahr\u00b7heit", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Trotz dem tr\u00fcgenden Get\u00f6n:", "tokens": ["Zum", "Trotz", "dem", "tr\u00fc\u00b7gen\u00b7den", "Ge\u00b7t\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Krieg mu\u00df fallen und verderben,", "tokens": ["Der", "Krieg", "mu\u00df", "fal\u00b7len", "und", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist unmenschlich, h\u00e4\u00dflich, faul,", "tokens": ["Er", "ist", "un\u00b7menschlich", ",", "h\u00e4\u00df\u00b7lich", ",", "faul", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An seiner F\u00e4ulnis mu\u00df er sterben,", "tokens": ["An", "sei\u00b7ner", "F\u00e4ul\u00b7nis", "mu\u00df", "er", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wahnsinnig wie der K\u00f6nig Saul.", "tokens": ["Wahn\u00b7sin\u00b7nig", "wie", "der", "K\u00f6\u00b7nig", "Saul", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Erbteil blutbefleckter Ahnen,", "tokens": ["Ein", "Erb\u00b7teil", "blut\u00b7be\u00b7fleck\u00b7ter", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll r\u00e4uberischer M\u00f6rdergier", "tokens": ["Voll", "r\u00e4u\u00b7be\u00b7ri\u00b7scher", "M\u00f6r\u00b7der\u00b7gier"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begleitet er der Menschheit Bahnen", "tokens": ["Be\u00b7glei\u00b7tet", "er", "der", "Menschheit", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Bis in der Gegenwart Revier.", "tokens": ["Bis", "in", "der", "Ge\u00b7gen\u00b7wart", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er pfeift und winselt noch nach Beute,", "tokens": ["Er", "pfeift", "und", "win\u00b7selt", "noch", "nach", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verpestend unsrer Erde Flur,", "tokens": ["Ver\u00b7pes\u00b7tend", "uns\u00b7rer", "Er\u00b7de", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er h\u00fcllt sich schlau in feinere H\u00e4ute", "tokens": ["Er", "h\u00fcllt", "sich", "schlau", "in", "fei\u00b7ne\u00b7re", "H\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schleicht auf frischen Aases Spur.", "tokens": ["Und", "schleicht", "auf", "fri\u00b7schen", "Aa\u00b7ses", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ist's kein Traum mehr, ist kein Plunder", "tokens": ["Doch", "ist's", "kein", "Traum", "mehr", ",", "ist", "kein", "Plun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ADV", "$,", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bet\u00f6rter Hoffnungsseligkeit,", "tokens": ["Be\u00b7t\u00f6r\u00b7ter", "Hoff\u00b7nungs\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist kein Wahnwitz und kein Wunder,", "tokens": ["Es", "ist", "kein", "Wahn\u00b7witz", "und", "kein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist ein Ziel der Wendezeit!", "tokens": ["Es", "ist", "ein", "Ziel", "der", "Wen\u00b7de\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Menschheit ward des Schlachtens m\u00fcde,", "tokens": ["Die", "Menschheit", "ward", "des", "Schlach\u00b7tens", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Selbstzerfleischung Schwertzahn bricht,", "tokens": ["Der", "Selbst\u00b7zer\u00b7flei\u00b7schung", "Schwert\u00b7zahn", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Friede! klingt es, Friede, Friede!", "tokens": ["Und", "Frie\u00b7de", "!", "klingt", "es", ",", "Frie\u00b7de", ",", "Frie\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$.", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Angesicht zu Angesicht.", "tokens": ["Von", "An\u00b7ge\u00b7sicht", "zu", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar kein Gebet, kein Lied der Lieder", "tokens": ["Zwar", "kein", "Ge\u00b7bet", ",", "kein", "Lied", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschw\u00f6rt das Scheusal alter Nacht,", "tokens": ["Be\u00b7schw\u00f6rt", "das", "Scheu\u00b7sal", "al\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Menschheit wirft die Waffen nieder,", "tokens": ["Die", "Menschheit", "wirft", "die", "Waf\u00b7fen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und siehe da! Es ist vollbracht.", "tokens": ["Und", "sie\u00b7he", "da", "!", "Es", "ist", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn unser Wille Macht geworden,", "tokens": ["Wenn", "un\u00b7ser", "Wil\u00b7le", "Macht", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wille, wahrhaft zu befrein,", "tokens": ["Der", "Wil\u00b7le", ",", "wahr\u00b7haft", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wird ewiger Urlaub allem Morden,", "tokens": ["Wird", "e\u00b7wi\u00b7ger", "Ur\u00b7laub", "al\u00b7lem", "Mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PIS", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wird Friede, Friede, Friede sein.", "tokens": ["Wird", "Frie\u00b7de", ",", "Frie\u00b7de", ",", "Frie\u00b7de", "sein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Dichter schrieb: \u00bbDer Krieg mu\u00df bleiben,", "tokens": ["Ein", "Dich\u00b7ter", "schrieb", ":", "\u00bb", "Der", "Krieg", "mu\u00df", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heroisch, wie er immer war,\u00ab", "tokens": ["He\u00b7ro\u00b7isch", ",", "wie", "er", "im\u00b7mer", "war", ",", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber mu\u00df zur Wahrheit schreiben:", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "zur", "Wahr\u00b7heit", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichter ist ein Versbarbar!", "tokens": ["Der", "Dich\u00b7ter", "ist", "ein", "Vers\u00b7bar\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er tr\u00fcgt in seiner sch\u00f6nen Ode,", "tokens": ["Er", "tr\u00fcgt", "in", "sei\u00b7ner", "sch\u00f6\u00b7nen", "O\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil er nicht horcht, weil er nicht lauscht,", "tokens": ["Weil", "er", "nicht", "horcht", ",", "weil", "er", "nicht", "lauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie zu der alten Menschheit Tode", "tokens": ["Wie", "zu", "der", "al\u00b7ten", "Menschheit", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Der neuen Menschheit Taufpsalm rauscht.", "tokens": ["Der", "neu\u00b7en", "Menschheit", "Tauf\u00b7psalm", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}