{"dta.poem.21023": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXVII.  \n Einer unkeuschen Ehefrauen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh erstlich deinen Leib und denn die Grabschrifft", "tokens": ["Sieh", "erst\u00b7lich", "dei\u00b7nen", "Leib", "und", "denn", "die", "Grab\u00b7schrifft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an/", "tokens": ["an", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "In dem nicht iederman alhier bestehen kan.", "tokens": ["In", "dem", "nicht", "ie\u00b7der\u00b7man", "al\u00b7hier", "be\u00b7ste\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier liegt ein geiles Weib so schmertzlich hat gebe-", "tokens": ["Hier", "liegt", "ein", "gei\u00b7les", "Weib", "so", "schmertz\u00b7lich", "hat", "ge\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "ten/", "tokens": ["ten", "/"], "token_info": ["word", "punct"], "pos": ["FM", "$("], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Es sol kein Joseph nicht zu ihrem Grabe treten.", "tokens": ["Es", "sol", "kein", "Jo\u00b7se\u00b7ph", "nicht", "zu", "ih\u00b7rem", "Gra\u00b7be", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NE", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}