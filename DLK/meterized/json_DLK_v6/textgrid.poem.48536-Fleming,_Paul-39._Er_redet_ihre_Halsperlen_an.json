{"textgrid.poem.48536": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "39. Er redet ihre Halsperlen an", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bildet ihr euch ein, ihr Muschelt\u00f6chter ihr?", "tokens": ["Was", "bil\u00b7det", "ihr", "euch", "ein", ",", "ihr", "Mu\u00b7schel\u00b7t\u00f6ch\u00b7ter", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vermeint ihr, da\u00df mein Lieb euch tr\u00e4gt zu ihrer Zier?", "tokens": ["Ver\u00b7meint", "ihr", ",", "da\u00df", "mein", "Lieb", "euch", "tr\u00e4gt", "zu", "ih\u00b7rer", "Zier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein, darumb tr\u00e4gt sie euch, darmit ihr selbsten schaut,", "tokens": ["Nein", ",", "da\u00b7rumb", "tr\u00e4gt", "sie", "euch", ",", "dar\u00b7mit", "ihr", "selbs\u00b7ten", "schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PAV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wie viel ihr dunkler seid als ihre klare Haut!", "tokens": ["wie", "viel", "ihr", "dunk\u00b7ler", "seid", "als", "ih\u00b7re", "kla\u00b7re", "Haut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "ADJA", "VAFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}