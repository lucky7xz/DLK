{"textgrid.poem.29371": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob das Jahr gleich alle Jahre sich gewohnt ist zu verj\u00fcngen,", "tokens": ["Ob", "das", "Jahr", "gleich", "al\u00b7le", "Jah\u00b7re", "sich", "ge\u00b7wohnt", "ist", "zu", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PIAT", "NN", "PRF", "VVPP", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dennoch kan der Jahre Jugend Menschen nichts als Alter bringen.", "tokens": ["Den\u00b7noch", "kan", "der", "Jah\u00b7re", "Ju\u00b7gend", "Men\u00b7schen", "nichts", "als", "Al\u00b7ter", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "NN", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}