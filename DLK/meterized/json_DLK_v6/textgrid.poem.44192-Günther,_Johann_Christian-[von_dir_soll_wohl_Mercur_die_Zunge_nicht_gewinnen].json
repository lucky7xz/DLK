{"textgrid.poem.44192": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[von dir soll wohl Mercur die Zunge nicht gewinnen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dir soll wohl Mercur die Zunge nicht gewinnen,", "tokens": ["Von", "dir", "soll", "wohl", "Mer\u00b7cur", "die", "Zun\u00b7ge", "nicht", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ADV", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies Opfer ist vor ihn zu kostbar zugericht", "tokens": ["Dies", "Op\u00b7fer", "ist", "vor", "ihn", "zu", "kost\u00b7bar", "zu\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und schickt sich vor den Schwezer nicht.", "tokens": ["Und", "schickt", "sich", "vor", "den", "Schwe\u00b7zer", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist und bleibt den Charitinnen", "tokens": ["Es", "ist", "und", "bleibt", "den", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wem denn mehr geweiht?", "tokens": ["Und", "wem", "denn", "mehr", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der heiligen Beredsamkeit.", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Be\u00b7red\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}