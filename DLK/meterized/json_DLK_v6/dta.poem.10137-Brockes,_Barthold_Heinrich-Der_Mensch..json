{"dta.poem.10137": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mensch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beym Eintritt in die Zeitlichkeit,", "tokens": ["Beym", "Ein\u00b7tritt", "in", "die", "Zeit\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist, an Verstand, ein neu gebohren Kind,", "tokens": ["Ist", ",", "an", "Ver\u00b7stand", ",", "ein", "neu", "ge\u00b7boh\u00b7ren", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "NN", "$,", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fast mehr, als andre Thiere, blind.", "tokens": ["Fast", "mehr", ",", "als", "and\u00b7re", "Thie\u00b7re", ",", "blind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darauf verspielet es die bald verspielte Zeit.", "tokens": ["Da\u00b7rauf", "ver\u00b7spie\u00b7let", "es", "die", "bald", "ver\u00b7spiel\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beym Fortgang w\u00e4chset zwar sein Leib und sein Verstand", "tokens": ["Beym", "Fort\u00b7gang", "w\u00e4ch\u00b7set", "zwar", "sein", "Leib", "und", "sein", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch ieder hat sein Ziel. Nichts ist ihm recht bekannt,", "tokens": ["Doch", "ie\u00b7der", "hat", "sein", "Ziel", ".", "Nichts", "ist", "ihm", "recht", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$.", "PIS", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von allem, was Natur, was Geist und C\u00f6rper heisset,", "tokens": ["Von", "al\u00b7lem", ",", "was", "Na\u00b7tur", ",", "was", "Geist", "und", "C\u00f6r\u00b7per", "heis\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "NN", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie sehr er sich zerdenckt, wie sehr er sich befleisset.", "tokens": ["Wie", "sehr", "er", "sich", "zer\u00b7denckt", ",", "wie", "sehr", "er", "sich", "be\u00b7fleis\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "VVPP", "$,", "PWAV", "ADV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es will kein Element, es will kein K\u00f6rnchen Sand", "tokens": ["Es", "will", "kein", "E\u00b7le\u00b7ment", ",", "es", "will", "kein", "K\u00f6rn\u00b7chen", "Sand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "$,", "PPER", "VMFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von ihm sich recht begreiffen lassen.", "tokens": ["Von", "ihm", "sich", "recht", "be\u00b7greif\u00b7fen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Selbst das, was in ihm denckt, womit er alles fassen", "tokens": ["Selbst", "das", ",", "was", "in", "ihm", "denckt", ",", "wo\u00b7mit", "er", "al\u00b7les", "fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und gr\u00fcndlich kennen will, ist ihm so wenig kund,", "tokens": ["Und", "gr\u00fcnd\u00b7lich", "ken\u00b7nen", "will", ",", "ist", "ihm", "so", "we\u00b7nig", "kund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als alles, was er sucht. Er, dessen Hertz ein Tempel", "tokens": ["Als", "al\u00b7les", ",", "was", "er", "sucht", ".", "Er", ",", "des\u00b7sen", "Hertz", "ein", "Tem\u00b7pel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PWS", "PPER", "VVFIN", "$.", "PPER", "$,", "PRELAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des Sch\u00f6pfers sollte seyn, ein Wohn-Platz Seiner Ehr,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "soll\u00b7te", "seyn", ",", "ein", "Wohn\u00b7Platz", "Sei\u00b7ner", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VAINF", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Folgt, wann er \u00e4lter wird, dem str\u00e4fflichen Exempel", "tokens": ["Folgt", ",", "wann", "er", "\u00e4l\u00b7ter", "wird", ",", "dem", "str\u00e4ff\u00b7li\u00b7chen", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Der gantz verderbten Welt, betritt ie mehr und mehr", "tokens": ["Der", "gantz", "ver\u00b7derb\u00b7ten", "Welt", ",", "be\u00b7tritt", "ie", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die freche Laster-Bahn, macht gleichsam einen Bund,", "tokens": ["Die", "fre\u00b7che", "Las\u00b7ter\u00b7Bahn", ",", "macht", "gleich\u00b7sam", "ei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zu thun, was er nicht soll, und was er soll, zu lassen,", "tokens": ["Zu", "thun", ",", "was", "er", "nicht", "soll", ",", "und", "was", "er", "soll", ",", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "$,", "KON", "PWS", "PPER", "VMFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zu lieben, was nicht gut, was liebens wehrt, zu hassen.", "tokens": ["Zu", "lie\u00b7ben", ",", "was", "nicht", "gut", ",", "was", "lie\u00b7bens", "wehrt", ",", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PTKNEG", "ADJD", "$,", "PRELS", "ADV", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dann, kommt sein Leib und Geist zu etwas mehrern Kr\u00e4fften", "tokens": ["Dann", ",", "kommt", "sein", "Leib", "und", "Geist", "zu", "et\u00b7was", "meh\u00b7rern", "Kr\u00e4ff\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Stat da\u00df er im Gesch\u00f6pf den Sch\u00f6pfer finden sollt,", "tokens": ["Stat", "da\u00df", "er", "im", "Ge\u00b7sch\u00f6pf", "den", "Sch\u00f6p\u00b7fer", "fin\u00b7den", "sollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So \u00fcberh\u00e4ufft er sich mit eitelen Gesch\u00e4fften.", "tokens": ["So", "\u00fc\u00b7berh\u00b7\u00e4ufft", "er", "sich", "mit", "ei\u00b7te\u00b7len", "Ge\u00b7sch\u00e4ff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sein Ziel, sein einzger Wunsch, sein Herr, sein Gott,", "tokens": ["Sein", "Ziel", ",", "sein", "einz\u00b7ger", "Wunsch", ",", "sein", "Herr", ",", "sein", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Gold.", "tokens": ["Gold", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}