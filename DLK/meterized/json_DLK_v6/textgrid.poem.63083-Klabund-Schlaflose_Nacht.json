{"textgrid.poem.63083": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Schlaflose Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberm\u00fcdet, schlaflos lieg ich in den Decken,", "tokens": ["\u00dc\u00b7ber\u00b7m\u00fc\u00b7det", ",", "schlaf\u00b7los", "lieg", "ich", "in", "den", "De\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Schon malt der junge Tag lichtgraue Flecken", "tokens": ["Schon", "malt", "der", "jun\u00b7ge", "Tag", "licht\u00b7grau\u00b7e", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Ofen, Stuhl und Lampenknauf.", "tokens": ["Auf", "O\u00b7fen", ",", "Stuhl", "und", "Lam\u00b7pen\u00b7knauf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fenster steht sperrangelauf.", "tokens": ["Das", "Fens\u00b7ter", "steht", "sper\u00b7ran\u00b7ge\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Hund l\u00e4uft \u00fcber den Asphalt, sein Halsband klappert.", "tokens": ["Ein", "Hund", "l\u00e4uft", "\u00fc\u00b7ber", "den", "As\u00b7phalt", ",", "sein", "Hals\u00b7band", "klap\u00b7pert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Es tickt wo eine Uhr. Der B\u00e4ckerjunge tappert", "tokens": ["Es", "tickt", "wo", "ei\u00b7ne", "Uhr", ".", "Der", "B\u00e4\u00b7cker\u00b7jun\u00b7ge", "tap\u00b7pert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWAV", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und schleppt im Sack Verschlafenheit und Bemme.", "tokens": ["Und", "schleppt", "im", "Sack", "Ver\u00b7schla\u00b7fen\u00b7heit", "und", "Bem\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von nebenan schwirrt, summt aus der Kaschemme", "tokens": ["Von", "ne\u00b7be\u00b7nan", "schwirrt", ",", "summt", "aus", "der", "Ka\u00b7schem\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Ein tr\u00fcbes Lied auf tr\u00fcbgestimmter Zither.", "tokens": ["Ein", "tr\u00fc\u00b7bes", "Lied", "auf", "tr\u00fcb\u00b7ge\u00b7stimm\u00b7ter", "Zi\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Zunge jappt im Gaumen rauh und bitter,", "tokens": ["Die", "Zun\u00b7ge", "jappt", "im", "Gau\u00b7men", "rauh", "und", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich hole d\u00fcrstend Glas mir und Karaffe \u2013", "tokens": ["Ich", "ho\u00b7le", "d\u00fcrs\u00b7tend", "Glas", "mir", "und", "Ka\u00b7raf\u00b7fe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "PPER", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da ist die Sonne jenseit aufgetaucht,", "tokens": ["Da", "ist", "die", "Son\u00b7ne", "jen\u00b7seit", "auf\u00b7ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von rosagelbem Wolkendampf umraucht,", "tokens": ["Von", "ro\u00b7sa\u00b7gel\u00b7bem", "Wol\u00b7ken\u00b7dampf", "um\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und formt im Glase eine Goldagraffe,", "tokens": ["Und", "formt", "im", "Gla\u00b7se", "ei\u00b7ne", "Gol\u00b7da\u00b7graf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Als wolle sie die letzten grauen Schlangen", "tokens": ["Als", "wol\u00b7le", "sie", "die", "letz\u00b7ten", "grau\u00b7en", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der Nacht mit einer goldnen Schlinge fangen.", "tokens": ["Der", "Nacht", "mit", "ei\u00b7ner", "gold\u00b7nen", "Schlin\u00b7ge", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}