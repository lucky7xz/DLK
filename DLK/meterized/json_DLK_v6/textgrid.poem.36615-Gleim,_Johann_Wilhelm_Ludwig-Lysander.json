{"textgrid.poem.36615": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lysander", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Spiel, der Wein, und Phillilette,", "tokens": ["Das", "Spiel", ",", "der", "Wein", ",", "und", "Phil\u00b7li\u00b7let\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschaefftigten Lysandern um die Wette;", "tokens": ["Be\u00b7schaeff\u00b7tig\u00b7ten", "Ly\u00b7san\u00b7dern", "um", "die", "Wet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, als die Hoell ihm ins Gedaechtni\u00df fiel,", "tokens": ["Doch", ",", "als", "die", "Hoell", "ihm", "ins", "Ge\u00b7daecht\u00b7ni\u00df", "fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Floh er den Wein, das Maedchen, und das Spiel.", "tokens": ["Floh", "er", "den", "Wein", ",", "das", "Maed\u00b7chen", ",", "und", "das", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Er beichtete, der Priester sprach ihn lo\u00df", "tokens": ["Er", "beich\u00b7te\u00b7te", ",", "der", "Pries\u00b7ter", "sprach", "ihn", "lo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von aller Schuld, und nannte sie: nicht gro\u00df.", "tokens": ["Von", "al\u00b7ler", "Schuld", ",", "und", "nann\u00b7te", "sie", ":", "nicht", "gro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "VVFIN", "PPER", "$.", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn Herr Lysander war kein armer S\u00fcnder;", "tokens": ["Denn", "Herr", "Ly\u00b7san\u00b7der", "war", "kein", "ar\u00b7mer", "S\u00fcn\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Itzt schreckt die H\u00f6ll ihn minder als vorher,", "tokens": ["Itzt", "schreckt", "die", "H\u00f6ll", "ihn", "min\u00b7der", "als", "vor\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Itzt mischt er schon die Charten viel geschwinder,", "tokens": ["Itzt", "mischt", "er", "schon", "die", "Char\u00b7ten", "viel", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und k\u00fcsst und trinkt noch mehr.", "tokens": ["Und", "k\u00fcsst", "und", "trinkt", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}