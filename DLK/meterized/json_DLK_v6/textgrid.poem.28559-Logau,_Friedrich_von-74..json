{"textgrid.poem.28559": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Reim ist offt was frey; noch freyer ist mein Mut", "tokens": ["Mein", "Reim", "ist", "offt", "was", "frey", ";", "noch", "frey\u00b7er", "ist", "mein", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PWS", "ADJD", "$.", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff das, was lasterhafft, von deme, was nicht gut.", "tokens": ["Auff", "das", ",", "was", "las\u00b7ter\u00b7hafft", ",", "von", "de\u00b7me", ",", "was", "nicht", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "VVFIN", "$,", "APPR", "ART", "$,", "PRELS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich rede frey von dem, was Schande heist und bringt;", "tokens": ["Ich", "re\u00b7de", "frey", "von", "dem", ",", "was", "Schan\u00b7de", "heist", "und", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "$,", "PWS", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vielleicht ist wer, den Scham von Schanden abezwingt.", "tokens": ["Viel\u00b7leicht", "ist", "wer", ",", "den", "Scham", "von", "Schan\u00b7den", "a\u00b7be\u00b7zwingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}