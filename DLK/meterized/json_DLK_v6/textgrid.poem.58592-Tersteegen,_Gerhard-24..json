{"textgrid.poem.58592": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habe Dank, mein guter Hirt, da\u00df du mich willst selber weiden;", "tokens": ["Ha\u00b7be", "Dank", ",", "mein", "gu\u00b7ter", "Hirt", ",", "da\u00df", "du", "mich", "willst", "sel\u00b7ber", "wei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Andre Hirten m\u00f6gen dann mich von ihrer Herde scheiden,", "tokens": ["And\u00b7re", "Hir\u00b7ten", "m\u00f6\u00b7gen", "dann", "mich", "von", "ih\u00b7rer", "Her\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Weide mich in dir, mit dir, in der d\u00fcrren W\u00fcstenei,", "tokens": ["Wei\u00b7de", "mich", "in", "dir", ",", "mit", "dir", ",", "in", "der", "d\u00fcr\u00b7ren", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Lehr mich kennen deine Stimm' und ihr folgen ganz getreu!", "tokens": ["Lehr", "mich", "ken\u00b7nen", "dei\u00b7ne", "Stimm'", "und", "ihr", "fol\u00b7gen", "ganz", "ge\u00b7treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "W\u00e4r' ich wie ein L\u00e4mmlein nur, so unschuldig, sanft und kleine!", "tokens": ["W\u00e4r'", "ich", "wie", "ein", "L\u00e4mm\u00b7lein", "nur", ",", "so", "un\u00b7schul\u00b7dig", ",", "sanft", "und", "klei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "ADV", "$,", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "-+-++-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Sammle mich in deinen Arm aus der Vielheit in das Eine,", "tokens": ["Samm\u00b7le", "mich", "in", "dei\u00b7nen", "Arm", "aus", "der", "Viel\u00b7heit", "in", "das", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Trage mich in deinem Scho\u00df, trag dein dummes L\u00e4mmlein doch,", "tokens": ["Tra\u00b7ge", "mich", "in", "dei\u00b7nem", "Scho\u00df", ",", "trag", "dein", "dum\u00b7mes", "L\u00e4mm\u00b7lein", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "So kann ich, wenn du es willst, auch wohl andre tragen noch!", "tokens": ["So", "kann", "ich", ",", "wenn", "du", "es", "willst", ",", "auch", "wohl", "and\u00b7re", "tra\u00b7gen", "noch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VMFIN", "$,", "ADV", "ADV", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}