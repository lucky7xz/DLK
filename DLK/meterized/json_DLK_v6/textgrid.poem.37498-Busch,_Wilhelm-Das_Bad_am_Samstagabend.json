{"textgrid.poem.37498": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Bad am Samstagabend", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sieht man Bruder Franz und Fritzen", "tokens": ["Hier", "sieht", "man", "Bru\u00b7der", "Franz", "und", "Frit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu zweit in einer Wanne sitzen.", "tokens": ["Zu", "zweit", "in", "ei\u00b7ner", "Wan\u00b7ne", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die alte Lene geht; \u2013 und gleich", "tokens": ["Die", "al\u00b7te", "Le\u00b7ne", "geht", ";", "\u2013", "und", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$.", "$(", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da treibt man lauter dummes Zeug.", "tokens": ["Da", "treibt", "man", "lau\u00b7ter", "dum\u00b7mes", "Zeug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn Reinlichkeit ist f\u00fcr die zwei", "tokens": ["Denn", "Rein\u00b7lich\u00b7keit", "ist", "f\u00fcr", "die", "zwei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "APPR", "ART", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ende doch nur Spielerei. \u2013", "tokens": ["Am", "En\u00b7de", "doch", "nur", "Spie\u00b7le\u00b7rei", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt will der Fritz beim Untertauchen", "tokens": ["Jetzt", "will", "der", "Fritz", "beim", "Un\u00b7ter\u00b7tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur seinen einen Finger brauchen.", "tokens": ["Nur", "sei\u00b7nen", "ei\u00b7nen", "Fin\u00b7ger", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nat\u00fcrlich l\u00e4uft ihm was ins Ohr,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "l\u00e4uft", "ihm", "was", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Franz kommt dieses lustig vor.", "tokens": ["Dem", "Franz", "kommt", "die\u00b7ses", "lus\u00b7tig", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PDAT", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das \u00e4rgert aber Bruder Fritzen,", "tokens": ["Das", "\u00e4r\u00b7gert", "a\u00b7ber", "Bru\u00b7der", "Frit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum f\u00e4ngt er an den Franz zu spritzen.", "tokens": ["Drum", "f\u00e4ngt", "er", "an", "den", "Franz", "zu", "sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch der mit seiner gro\u00dfen Zehe", "tokens": ["Doch", "der", "mit", "sei\u00b7ner", "gro\u00b7\u00dfen", "Ze\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tut Fritzen an der Nase wehe;", "tokens": ["Tut", "Frit\u00b7zen", "an", "der", "Na\u00b7se", "we\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Daf\u00fcr taucht Fritz den Kopf ihm nieder,", "tokens": ["Da\u00b7f\u00fcr", "taucht", "Fritz", "den", "Kopf", "ihm", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was so im Wasser sehr zuwider.", "tokens": ["Was", "so", "im", "Was\u00b7ser", "sehr", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Franz aber zieht an Fritzens Bein;", "tokens": ["Franz", "a\u00b7ber", "zieht", "an", "Frit\u00b7zens", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zappelt sehr und kann nicht schrein.", "tokens": ["Der", "zap\u00b7pelt", "sehr", "und", "kann", "nicht", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "In Mund und Auge, zornentbrannt,", "tokens": ["In", "Mund", "und", "Au\u00b7ge", ",", "zor\u00b7nent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greift jetzt die rachbegier'ge Hand.", "tokens": ["Greift", "jetzt", "die", "rach\u00b7be\u00b7gier'\u00b7ge", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die Wanne wird zu enge", "tokens": ["Die", "Wan\u00b7ne", "wird", "zu", "en\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr dieses Kampfgedr\u00e4nge.", "tokens": ["F\u00fcr", "die\u00b7ses", "Kampf\u00b7ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Perdatsch!! \u2013 Die alte, brave Lene", "tokens": ["Per\u00b7datsch", "!!", "\u2013", "Die", "al\u00b7te", ",", "bra\u00b7ve", "Le\u00b7ne"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt leider grad zu dieser Szene.", "tokens": ["Kommt", "lei\u00b7der", "grad", "zu", "die\u00b7ser", "Sze\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Sie spricht voll W\u00fcrde und voll Schmerz:", "tokens": ["Sie", "spricht", "voll", "W\u00fcr\u00b7de", "und", "voll", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdie Reinlichkeit ist nicht zum Scherz!!\u00ab", "tokens": ["\u00bb", "die", "Rein\u00b7lich\u00b7keit", "ist", "nicht", "zum", "Scherz", "!!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und die Moral von der Geschicht':", "tokens": ["Und", "die", "Mo\u00b7ral", "von", "der", "Ge\u00b7schicht'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bad\u00ab zwei in einer Wanne nicht!", "tokens": ["Bad", "\u00ab", "zwei", "in", "ei\u00b7ner", "Wan\u00b7ne", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "CARD", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}