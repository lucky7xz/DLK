{"textgrid.poem.31409": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Solt nicht das Gnaden-Meer dem rotem Meer gebieten?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solt nicht das Gnaden-Meer dem rotem Meer gebieten?", "tokens": ["Solt", "nicht", "das", "Gna\u00b7den\u00b7Meer", "dem", "ro\u00b7tem", "Meer", "ge\u00b7bie\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja billich! dieses hat aus jenes Krafft den Safft.", "tokens": ["Ja", "bil\u00b7lich", "!", "die\u00b7ses", "hat", "aus", "je\u00b7nes", "Krafft", "den", "Safft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$.", "PDS", "VAFIN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Flut wird Felsen fest / der Fel\u00df mit Safft behafft.", "tokens": ["die", "Flut", "wird", "Fel\u00b7sen", "fest", "/", "der", "Fel\u00df", "mit", "Safft", "be\u00b7hafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PTKVZ", "$(", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott kan die Sinnen auch verstocken und beg\u00fcten.", "tokens": ["Gott", "kan", "die", "Sin\u00b7nen", "auch", "ver\u00b7sto\u00b7cken", "und", "be\u00b7g\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "unm\u00f6glichheit nit jrrt / die seinen zu beh\u00fcten.", "tokens": ["un\u00b7m\u00f6g\u00b7lich\u00b7heit", "nit", "jrrt", "/", "die", "sei\u00b7nen", "zu", "be\u00b7h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "ADJD", "$(", "ART", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit volles Wesens Herz / durch seine Krafft / er schafft.", "tokens": ["mit", "vol\u00b7les", "We\u00b7sens", "Herz", "/", "durch", "sei\u00b7ne", "Krafft", "/", "er", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$(", "APPR", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "sein Allseyn / theilet aus die unschadhaffte Krafft.", "tokens": ["sein", "All\u00b7seyn", "/", "thei\u00b7let", "aus", "die", "un\u00b7schad\u00b7haff\u00b7te", "Krafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "umsonst Feind / Feur und Meer / will er beh\u00fcten / w\u00fcten.", "tokens": ["um\u00b7sonst", "Feind", "/", "Feur", "und", "Meer", "/", "will", "er", "be\u00b7h\u00fc\u00b7ten", "/", "w\u00fc\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$(", "NN", "KON", "NN", "$(", "VMFIN", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "geh / Gott-geleites Volk / den sondern Wunder Weg!", "tokens": ["geh", "/", "Got\u00b7tge\u00b7lei\u00b7tes", "Volk", "/", "den", "son\u00b7dern", "Wun\u00b7der", "Weg", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sein' Allmacht ist dein Pfad / die Gnaden Hand der Steg.", "tokens": ["Sein'", "All\u00b7macht", "ist", "dein", "Pfad", "/", "die", "Gna\u00b7den", "Hand", "der", "Steg", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "geh' unverg\u00e4nglichkeit / des Herren Lob zubringen /", "tokens": ["geh'", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "/", "des", "Her\u00b7ren", "Lob", "zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$(", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das selb' aus Meeres Tieff in Sonnen Thron zuschwingen.", "tokens": ["das", "selb'", "aus", "Mee\u00b7res", "Tieff", "in", "Son\u00b7nen", "Thron", "zu\u00b7schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "NN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein mehr als Meersands-G\u00fct' hau' in den Felsen ein.", "tokens": ["Sein", "mehr", "als", "Meer\u00b7sands\u00b7G\u00fct'", "hau'", "in", "den", "Fel\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "KOKOM", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "dann wird voll seiner Ehr' ein jedes Tr\u00f6pflein seyn.", "tokens": ["dann", "wird", "voll", "sei\u00b7ner", "Ehr'", "ein", "je\u00b7des", "Tr\u00f6pf\u00b7lein", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "ART", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}