{"textgrid.poem.53895": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Kupplerin", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Suchen zwei", "tokens": ["Su\u00b7chen", "zwei"], "token_info": ["word", "word"], "pos": ["NN", "CARD"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "nachts um drei \u2013", "tokens": ["nachts", "um", "drei", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "\u00bbpst\u00ab mach ick \u2013 \u00bbhier is 'n Zimmer frei \u2013!\u00ab", "tokens": ["\u00bb", "pst", "\u00ab", "mach", "ick", "\u2013", "\u00bb", "hier", "is", "'n", "Zim\u00b7mer", "frei", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$(", "VVFIN", "PPER", "$(", "$(", "ADV", "FM", "ART", "NN", "ADJD", "$(", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Treppe kracht,", "tokens": ["Trep\u00b7pe", "kracht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "in dunkler Nacht \u2013", "tokens": ["in", "dunk\u00b7ler", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "\u00bbhe \u2013 fall mir keener in den Fahrstuhlschacht!\u00ab", "tokens": ["\u00bb", "he", "\u2013", "fall", "mir", "kee\u00b7ner", "in", "den", "Fahr\u00b7stuhl\u00b7schacht", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$(", "VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Rin ins Zimmer.", "tokens": ["Rin", "ins", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Matter Schimmer.", "tokens": ["Mat\u00b7ter", "Schim\u00b7mer", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ick davor.", "tokens": ["Ick", "da\u00b7vor", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PAV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "In 'n Korridor . . .", "tokens": ["In", "'n", "Kor\u00b7ri\u00b7dor", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$.", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Feiern die die Orchideen \u2013", "tokens": ["Fei\u00b7ern", "die", "die", "Or\u00b7chi\u00b7de\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ick stopp mein'n Mann seine Str\u00fcmpe.", "tokens": ["Ick", "stopp", "mein'n", "Mann", "sei\u00b7ne", "Str\u00fcm\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ick will den Rummel jahnich sehn \u2013", "tokens": ["Ick", "will", "den", "Rum\u00b7mel", "jah\u00b7nich", "sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ick stopp bis frieh um fimfe.", "tokens": ["ick", "stopp", "bis", "frieh", "um", "fim\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Masochisten,", "tokens": ["Ma\u00b7so\u00b7chis\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Homosexwalisten,", "tokens": ["Ho\u00b7mo\u00b7sex\u00b7wa\u00b7lis\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "frisch jelehrje", "tokens": ["frisch", "je\u00b7lehr\u00b7je"], "token_info": ["word", "word"], "pos": ["ADJD", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Minderj\u00e4hrje", "tokens": ["Min\u00b7der\u00b7j\u00e4hr\u00b7je"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "vaitressiern mir nich so sehr", "tokens": ["vait\u00b7res\u00b7siern", "mir", "nich", "so", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "als wie mein Mann seine Str\u00fcmpe.", "tokens": ["als", "wie", "mein", "Mann", "sei\u00b7ne", "Str\u00fcm\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Ick sitz stur.", "tokens": ["Ick", "sitz", "stur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Manchmal nur", "tokens": ["Manch\u00b7mal", "nur"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schl\u00e4gt in unsan Salon die Uhr.", "tokens": ["Schl\u00e4gt", "in", "un\u00b7san", "Sa\u00b7lon", "die", "Uhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "W\u00e4sche bauscht", "tokens": ["W\u00e4\u00b7sche", "bauscht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "sich \u2013 Wasser rauscht \u2013", "tokens": ["sich", "\u2013", "Was\u00b7ser", "rauscht", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PRF", "$(", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "ick h\u00f6r, wie eena K\u00fcsskens tauscht.", "tokens": ["ick", "h\u00f6r", ",", "wie", "e\u00b7e\u00b7na", "K\u00fcss\u00b7kens", "tauscht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Da jehts hart auf hart . . .", "tokens": ["Da", "jehts", "hart", "auf", "hart", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ADJD", "$.", "$.", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Matratze knarrt.", "tokens": ["Ma\u00b7trat\u00b7ze", "knarrt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Nebenbei", "tokens": ["Ne\u00b7ben\u00b7bei"], "token_info": ["word"], "pos": ["XY"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "ein doller Schrei \u2013!", "tokens": ["ein", "dol\u00b7ler", "Schrei", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "na, na . . .", "tokens": ["na", ",", "na", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "$.", "$."], "meter": "OO", "measure": "unknown.measure.zero"}, "line.12": {"text": "Ich gucke durch keen Schl\u00fcsselloch", "tokens": ["Ich", "gu\u00b7cke", "durch", "ke\u00b7en", "Schl\u00fcs\u00b7sel\u00b7loch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Ick stopp mein Mann seine Str\u00fcmpe.", "tokens": ["Ick", "stopp", "mein", "Mann", "sei\u00b7ne", "Str\u00fcm\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Ick la\u00df sie muddeln noch und noch \u2013", "tokens": ["Ick", "la\u00df", "sie", "mud\u00b7deln", "noch", "und", "noch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "ick stopp bis frieh um fimfe.", "tokens": ["ick", "stopp", "bis", "frieh", "um", "fim\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Junge Meise", "tokens": ["Jun\u00b7ge", "Mei\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Zittergreise \u2013", "tokens": ["Zit\u00b7ter\u00b7grei\u00b7se", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Rennbanditen \u2013", "tokens": ["Renn\u00b7ban\u00b7di\u00b7ten", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+---", "measure": "dactylic.init"}, "line.19": {"text": "Transchvestiten \u2013", "tokens": ["Transchves\u00b7ti\u00b7ten", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+--", "measure": "dactylic.init"}, "line.20": {"text": "lauter Bruch aus 'n Ausverkauf \u2013", "tokens": ["lau\u00b7ter", "Bruch", "aus", "'n", "Aus\u00b7ver\u00b7kauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "wie mein Mann seine Str\u00fcmpe.", "tokens": ["wie", "mein", "Mann", "sei\u00b7ne", "Str\u00fcm\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Jahr f\u00fcr Jahr \u2013", "tokens": ["Jahr", "f\u00fcr", "Jahr", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "det is klar \u2013", "tokens": ["det", "is", "klar", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$("], "meter": "--+", "measure": "anapaest.init"}, "line.3": {"text": "horch ick, wat in die Stuben war.", "tokens": ["horch", "ick", ",", "wat", "in", "die", "Stu\u00b7ben", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie sich qu\u00e4ln,", "tokens": ["Wie", "sie", "sich", "qu\u00e4ln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "und krakeehln \u2013", "tokens": ["und", "kra\u00b7keehln", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "mir kann keiner was erz\u00e4hln.", "tokens": ["mir", "kann", "kei\u00b7ner", "was", "er\u00b7z\u00e4hln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PWS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neulich kam vorbei", "tokens": ["Neu\u00b7lich", "kam", "vor\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "eener von die Polissei.", "tokens": ["e\u00b7e\u00b7ner", "von", "die", "Po\u00b7lis\u00b7sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und statt Platz zunehm,", "tokens": ["Und", "statt", "Platz", "zu\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "sagt er: \u00bbSie solln sich was sch\u00e4m \u2013!\u00ab", "tokens": ["sagt", "er", ":", "\u00bb", "Sie", "solln", "sich", "was", "sch\u00e4m", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VMFIN", "PRF", "PWS", "VVFIN", "$(", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Ick bin eine brave Frau \u2013", "tokens": ["Ick", "bin", "ei\u00b7ne", "bra\u00b7ve", "Frau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ick stopp mein Mann seine Str\u00fcmpe.", "tokens": ["ick", "stopp", "mein", "Mann", "sei\u00b7ne", "Str\u00fcm\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Mit die Wirtschaft nehm icks ganz jenau \u2013", "tokens": ["Mit", "die", "Wirt\u00b7schaft", "nehm", "icks", "ganz", "je\u00b7nau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "ick stopp bis fr\u00fch um fimfe.", "tokens": ["ick", "stopp", "bis", "fr\u00fch", "um", "fim\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Det Jelichter,", "tokens": ["Det", "Je\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "die B\u00fchnendichter,", "tokens": ["die", "B\u00fch\u00b7nen\u00b7dich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "wat die da schreihm", "tokens": ["wat", "die", "da", "schreihm"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADV", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "von unsan Treihm \u2013:", "tokens": ["von", "un\u00b7san", "Treihm", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "die ham ja keene Ahnung nich", "tokens": ["die", "ham", "ja", "kee\u00b7ne", "Ah\u00b7nung", "nich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "von mein Mann seine Str\u00fcmpe \u2013!", "tokens": ["von", "mein", "Mann", "sei\u00b7ne", "Str\u00fcm\u00b7pe", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$(", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}