{"textgrid.poem.24692": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Kind mit fiebernden Wangen lag,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Kind mit fiebernden Wangen lag,", "tokens": ["Das", "Kind", "mit", "fie\u00b7bern\u00b7den", "Wan\u00b7gen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rotgolden versank im Laub der Tag.", "tokens": ["Rot\u00b7gol\u00b7den", "ver\u00b7sank", "im", "Laub", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Fenster hing voller wildem Wein,", "tokens": ["Das", "Fens\u00b7ter", "hing", "vol\u00b7ler", "wil\u00b7dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da sah ein fremder J\u00fcngling herein.", "tokens": ["Da", "sah", "ein", "frem\u00b7der", "J\u00fcng\u00b7ling", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbla\u00df, Mutter, den sch\u00f6nen Knaben ein,", "tokens": ["\u00bb", "la\u00df", ",", "Mut\u00b7ter", ",", "den", "sch\u00f6\u00b7nen", "Kna\u00b7ben", "ein", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er beut mir die Schale mit leuchtendem Wein,", "tokens": ["Er", "beut", "mir", "die", "Scha\u00b7le", "mit", "leuch\u00b7ten\u00b7dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Seine Lippen sind wie Blumen rot,", "tokens": ["Sei\u00b7ne", "Lip\u00b7pen", "sind", "wie", "Blu\u00b7men", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus seinen Augen ein Feuer loht.\u00ab", "tokens": ["Aus", "sei\u00b7nen", "Au\u00b7gen", "ein", "Feu\u00b7er", "loht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der n\u00e4chste Tag verglomm im Teich,", "tokens": ["Der", "n\u00e4chs\u00b7te", "Tag", "ver\u00b7glomm", "im", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stand am Fenster der J\u00fcngling, bleich,", "tokens": ["Da", "stand", "am", "Fens\u00b7ter", "der", "J\u00fcng\u00b7ling", ",", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit Lippen wie giftige Blumen rot", "tokens": ["Mit", "Lip\u00b7pen", "wie", "gif\u00b7ti\u00b7ge", "Blu\u00b7men", "rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM", "ADJA", "NN", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und einem L\u00e4cheln, das lockt und droht.", "tokens": ["Und", "ei\u00b7nem", "L\u00e4\u00b7cheln", ",", "das", "lockt", "und", "droht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbschick, Mutter, den fremden Knaben fort,", "tokens": ["\u00bb", "schick", ",", "Mut\u00b7ter", ",", "den", "frem\u00b7den", "Kna\u00b7ben", "fort", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich zehrt die Glut und mein Leib verdorrt,", "tokens": ["Mich", "zehrt", "die", "Glut", "und", "mein", "Leib", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mich \u00e4ngstigt sein L\u00e4cheln, er h\u00e4lt mir her", "tokens": ["Mich", "\u00e4ngs\u00b7tigt", "sein", "L\u00e4\u00b7cheln", ",", "er", "h\u00e4lt", "mir", "her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "APZR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Schale mit Wein, der ist hei\u00df und schwer!", "tokens": ["Die", "Scha\u00b7le", "mit", "Wein", ",", "der", "ist", "hei\u00df", "und", "schwer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Ach Mutter, was bist du nicht erwacht!", "tokens": ["Ach", "Mut\u00b7ter", ",", "was", "bist", "du", "nicht", "er\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er kam geschlichen ans Bett bei Nacht:", "tokens": ["Er", "kam", "ge\u00b7schli\u00b7chen", "ans", "Bett", "bei", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und, weh, seinen Wein ich getrunken hab", "tokens": ["Und", ",", "weh", ",", "sei\u00b7nen", "Wein", "ich", "ge\u00b7trun\u00b7ken", "hab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "PPER", "VVPP", "VAFIN"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und morgen k\u00f6nnt ihr mir graben das Grab!\u00ab", "tokens": ["Und", "mor\u00b7gen", "k\u00f6nnt", "ihr", "mir", "gra\u00b7ben", "das", "Grab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}