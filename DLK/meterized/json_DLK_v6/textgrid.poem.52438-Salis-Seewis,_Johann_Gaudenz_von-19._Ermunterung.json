{"textgrid.poem.52438": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "19. Ermunterung", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht! wie die Tage sich sonnig verkl\u00e4ren!", "tokens": ["Seht", "!", "wie", "die", "Ta\u00b7ge", "sich", "son\u00b7nig", "ver\u00b7kl\u00e4\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Blau ist der Himmel und gr\u00fcnend das Land.", "tokens": ["Blau", "ist", "der", "Him\u00b7mel", "und", "gr\u00fc\u00b7nend", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Klag' ist ein Mi\u00dfton im Chore der Sph\u00e4ren!", "tokens": ["Klag'", "ist", "ein", "Mi\u00df\u00b7ton", "im", "Cho\u00b7re", "der", "Sph\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Tr\u00e4gt denn die Sch\u00f6pfung ein Trauergewand?", "tokens": ["Tr\u00e4gt", "denn", "die", "Sch\u00f6p\u00b7fung", "ein", "Trau\u00b7er\u00b7ge\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Hebet die Blicke, die tr\u00fcbe sich senken,", "tokens": ["He\u00b7bet", "die", "Bli\u00b7cke", ",", "die", "tr\u00fc\u00b7be", "sich", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "Hebet die Blicke, des Sch\u00f6nen ist viel.", "tokens": ["He\u00b7bet", "die", "Bli\u00b7cke", ",", "des", "Sch\u00f6\u00b7nen", "ist", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Tugend wird selber zu Freuden uns lenken;", "tokens": ["Tu\u00b7gend", "wird", "sel\u00b7ber", "zu", "Freu\u00b7den", "uns", "len\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Freud' ist der Weisheit belohnendes Ziel.", "tokens": ["Freud'", "ist", "der", "Weis\u00b7heit", "be\u00b7loh\u00b7nen\u00b7des", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00d6ffnet die Seele dem Lichte der Freude,", "tokens": ["\u00d6ff\u00b7net", "die", "See\u00b7le", "dem", "Lich\u00b7te", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Horcht! ihr ert\u00f6net des H\u00e4nflings Gesang.", "tokens": ["Horcht", "!", "ihr", "er\u00b7t\u00f6\u00b7net", "des", "H\u00e4nf\u00b7lings", "Ge\u00b7sang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Atmet! sie duftet im Rosengest\u00e4ude,", "tokens": ["At\u00b7met", "!", "sie", "duf\u00b7tet", "im", "Ro\u00b7sen\u00b7ge\u00b7st\u00e4u\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "F\u00fchlet! sie s\u00e4uselt am B\u00e4chlein entlang.", "tokens": ["F\u00fch\u00b7let", "!", "sie", "s\u00e4u\u00b7selt", "am", "B\u00e4ch\u00b7lein", "ent\u00b7lang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Kostet! sie gl\u00fcht uns im Safte der Traube,", "tokens": ["Kos\u00b7tet", "!", "sie", "gl\u00fcht", "uns", "im", "Saf\u00b7te", "der", "Trau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "W\u00fcrzet die Fr\u00fcchte beim l\u00e4ndlichen Mahl.", "tokens": ["W\u00fcr\u00b7zet", "die", "Fr\u00fcch\u00b7te", "beim", "l\u00e4nd\u00b7li\u00b7chen", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Schauet! sie gr\u00fcnet in Kr\u00e4utern und Laube,", "tokens": ["Schau\u00b7et", "!", "sie", "gr\u00fc\u00b7net", "in", "Kr\u00e4u\u00b7tern", "und", "Lau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Malt uns die Aussicht ins blumichte Thal.", "tokens": ["Malt", "uns", "die", "Aus\u00b7sicht", "ins", "blu\u00b7mich\u00b7te", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Freunde! was gleiten euch weibische Thr\u00e4nen", "tokens": ["Freun\u00b7de", "!", "was", "glei\u00b7ten", "euch", "wei\u00b7bi\u00b7sche", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "\u00dcber die bl\u00fchenden Wangen herab?", "tokens": ["\u00dc\u00b7ber", "die", "bl\u00fc\u00b7hen\u00b7den", "Wan\u00b7gen", "her\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ziemt sich f\u00fcr M\u00e4nner das weichliche Sehnen?", "tokens": ["Ziemt", "sich", "f\u00fcr", "M\u00e4n\u00b7ner", "das", "weich\u00b7li\u00b7che", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "W\u00fcnscht ihr verzagend zu modern im Grab?", "tokens": ["W\u00fcnscht", "ihr", "ver\u00b7za\u00b7gend", "zu", "mo\u00b7dern", "im", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Edleres bleibt uns noch viel zu verrichten,", "tokens": ["Ed\u00b7le\u00b7res", "bleibt", "uns", "noch", "viel", "zu", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Viel auch des Guten ist noch nicht gethan;", "tokens": ["Viel", "auch", "des", "Gu\u00b7ten", "ist", "noch", "nicht", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Heiterkeit lohnt die Erf\u00fcllung der Pflichten,", "tokens": ["Hei\u00b7ter\u00b7keit", "lohnt", "die", "Er\u00b7f\u00fcl\u00b7lung", "der", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Ruhe beschattet das Ende der Bahn.", "tokens": ["Ru\u00b7he", "be\u00b7schat\u00b7tet", "das", "En\u00b7de", "der", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Mancherlei Sorgen und mancherlei Schmerzen", "tokens": ["Man\u00b7cher\u00b7lei", "Sor\u00b7gen", "und", "man\u00b7cher\u00b7lei", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Qu\u00e4len uns wahrlich aus eigener Schuld.", "tokens": ["Qu\u00e4\u00b7len", "uns", "wahr\u00b7lich", "aus", "ei\u00b7ge\u00b7ner", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Hoffnung ist Labsal dem wundesten Herzen,", "tokens": ["Hoff\u00b7nung", "ist", "Lab\u00b7sal", "dem", "wun\u00b7des\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Duldende st\u00e4rket gela\u00dfne Geduld.", "tokens": ["Dul\u00b7den\u00b7de", "st\u00e4r\u00b7ket", "ge\u00b7la\u00df\u00b7ne", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wenn euch die Nebel des Tr\u00fcbsinns umgrauen,", "tokens": ["Wenn", "euch", "die", "Ne\u00b7bel", "des", "Tr\u00fcb\u00b7sinns", "um\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Hebt zu den Sternen den sinkenden Mut;", "tokens": ["Hebt", "zu", "den", "Ster\u00b7nen", "den", "sin\u00b7ken\u00b7den", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Heget nur m\u00e4nnliches, hohes Vertrauen,", "tokens": ["He\u00b7get", "nur", "m\u00e4nn\u00b7li\u00b7ches", ",", "ho\u00b7hes", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Guten ergeht es am Schlusse noch gut.", "tokens": ["Gu\u00b7ten", "er\u00b7geht", "es", "am", "Schlus\u00b7se", "noch", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Lasset uns fr\u00f6hlich die Sch\u00f6pfungen sehen:", "tokens": ["Las\u00b7set", "uns", "fr\u00f6h\u00b7lich", "die", "Sch\u00f6p\u00b7fun\u00b7gen", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gottes Natur ist entz\u00fcckend und hehr!", "tokens": ["Got\u00b7tes", "Na\u00b7tur", "ist", "ent\u00b7z\u00fc\u00b7ckend", "und", "hehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Aber auch stillen des D\u00fcrftigen Flehen:", "tokens": ["A\u00b7ber", "auch", "stil\u00b7len", "des", "D\u00fcrf\u00b7ti\u00b7gen", "Fle\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Freuden des Wohlthuns entz\u00fccken noch mehr.", "tokens": ["Freu\u00b7den", "des", "Wohl\u00b7thuns", "ent\u00b7z\u00fc\u00b7cken", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Liebet! die Lieb' ist die sch\u00f6nste der Triebe:", "tokens": ["Lie\u00b7bet", "!", "die", "Lieb'", "ist", "die", "sch\u00f6ns\u00b7te", "der", "Trie\u00b7be", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Weiht nur der Unschuld die heilige Glut.", "tokens": ["Weiht", "nur", "der", "Un\u00b7schuld", "die", "hei\u00b7li\u00b7ge", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Aber dann liebt auch mit weiserer Liebe", "tokens": ["A\u00b7ber", "dann", "liebt", "auch", "mit", "wei\u00b7se\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Alles, was edel und sch\u00f6n ist und gut.", "tokens": ["Al\u00b7les", ",", "was", "e\u00b7del", "und", "sch\u00f6n", "ist", "und", "gut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Handelt! durch Handlungen zeigt sich der Weise,", "tokens": ["Han\u00b7delt", "!", "durch", "Hand\u00b7lun\u00b7gen", "zeigt", "sich", "der", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ruhm und Unsterblichkeit sind ihr Geleit.", "tokens": ["Ruhm", "und", "U\u00b7nsterb\u00b7lich\u00b7keit", "sind", "ihr", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zeichnet mit Thaten die schwindenden Gleise", "tokens": ["Zeich\u00b7net", "mit", "Tha\u00b7ten", "die", "schwin\u00b7den\u00b7den", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Unserer fl\u00fcchtig entrollenden Zeit.", "tokens": ["Un\u00b7se\u00b7rer", "fl\u00fcch\u00b7tig", "ent\u00b7rol\u00b7len\u00b7den", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Den uns umschlie\u00dfenden Zirkel begl\u00fccken,", "tokens": ["Den", "uns", "um\u00b7schlie\u00b7\u00dfen\u00b7den", "Zir\u00b7kel", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "N\u00fctzen, so viel als ein jeder vermag,", "tokens": ["N\u00fct\u00b7zen", ",", "so", "viel", "als", "ein", "je\u00b7der", "ver\u00b7mag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PIAT", "KOKOM", "ART", "PIS", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "O das erf\u00fcllet mit stillem Entz\u00fccken!", "tokens": ["O", "das", "er\u00b7f\u00fcl\u00b7let", "mit", "stil\u00b7lem", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "O das entw\u00f6lket den d\u00fcstersten Tag!", "tokens": ["O", "das", "ent\u00b7w\u00f6l\u00b7ket", "den", "d\u00fcs\u00b7ters\u00b7ten", "Tag", "!"], "tok