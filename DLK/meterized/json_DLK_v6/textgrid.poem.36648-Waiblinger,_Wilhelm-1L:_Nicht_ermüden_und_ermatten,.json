{"textgrid.poem.36648": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht erm\u00fcden und ermatten,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht erm\u00fcden und ermatten,", "tokens": ["Nicht", "er\u00b7m\u00fc\u00b7den", "und", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch wenn kaum ein St\u00fcndchen Schlummer", "tokens": ["Auch", "wenn", "kaum", "ein", "St\u00fcnd\u00b7chen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen Morgen dich erquicket!", "tokens": ["Ge\u00b7gen", "Mor\u00b7gen", "dich", "er\u00b7quic\u00b7ket", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So die lustige Gef\u00e4hrtin,", "tokens": ["So", "die", "lus\u00b7ti\u00b7ge", "Ge\u00b7f\u00e4hr\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heut' am letzten Freudentage", "tokens": ["Heut'", "am", "letz\u00b7ten", "Freu\u00b7den\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir als trefflicher Paino,", "tokens": ["Mir", "als", "treff\u00b7li\u00b7cher", "Pai\u00b7no", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Fein in schwarzem Kleid und Hut", "tokens": ["Fein", "in", "schwar\u00b7zem", "Kleid", "und", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und im Busenstrich erscheinend.", "tokens": ["Und", "im", "Bu\u00b7sen\u00b7strich", "er\u00b7schei\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heut' am allerletzten Tage", "tokens": ["Heut'", "am", "al\u00b7ler\u00b7letz\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollte man nicht ausgelassen,", "tokens": ["Soll\u00b7te", "man", "nicht", "aus\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich dem Faune, gleich dem Satyr,", "tokens": ["Gleich", "dem", "Fau\u00b7ne", ",", "gleich", "dem", "Sa\u00b7tyr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eine tolle Nymph' im Arme,", "tokens": ["Ei\u00b7ne", "tol\u00b7le", "Nym\u00b7ph'", "im", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Jubelnd seinen Thyrsus schwingen?", "tokens": ["Ju\u00b7belnd", "sei\u00b7nen", "Thyr\u00b7sus", "schwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und warum nicht? Rennt mit H\u00f6rnern,", "tokens": ["Und", "wa\u00b7rum", "nicht", "?", "Rennt", "mit", "H\u00f6r\u00b7nern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "$.", "VVFIN", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Pferdefu\u00df, in schwarz und roth", "tokens": ["Pfer\u00b7de\u00b7fu\u00df", ",", "in", "schwarz", "und", "roth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lucifer nicht im Gedr\u00e4nge?", "tokens": ["Lu\u00b7ci\u00b7fer", "nicht", "im", "Ge\u00b7dr\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie man von dem Liebchen scheidend,", "tokens": ["Wie", "man", "von", "dem", "Lieb\u00b7chen", "schei\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in Einem langen Kusse", "tokens": ["Noch", "in", "Ei\u00b7nem", "lan\u00b7gen", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wonn' und Lust auf ewig trinken,", "tokens": ["Wonn'", "und", "Lust", "auf", "e\u00b7wig", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trost f\u00fcr immer saugen m\u00f6chte,", "tokens": ["Trost", "f\u00fcr", "im\u00b7mer", "sau\u00b7gen", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie dem Vaterland entwandernd,", "tokens": ["Wie", "dem", "Va\u00b7ter\u00b7land", "ent\u00b7wan\u00b7dernd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo man Kind war, wo man liebte,", "tokens": ["Wo", "man", "Kind", "war", ",", "wo", "man", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Man des Lebewohls Moment", "tokens": ["Man", "des", "Le\u00b7be\u00b7wohls", "Mo\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gerne noch verl\u00e4ngern m\u00f6chte:", "tokens": ["Ger\u00b7ne", "noch", "ver\u00b7l\u00e4n\u00b7gern", "m\u00f6ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So das wilde Rom, man taumelt", "tokens": ["So", "das", "wil\u00b7de", "Rom", ",", "man", "tau\u00b7melt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NE", "$,", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Taumelnden; es regnet", "tokens": ["Un\u00b7ter", "Tau\u00b7meln\u00b7den", ";", "es", "reg\u00b7net"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut' zum letzten Male Blumen", "tokens": ["Heut'", "zum", "letz\u00b7ten", "Ma\u00b7le", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf ein gl\u00fccklich Volk, und Zucker.", "tokens": ["Auf", "ein", "gl\u00fcck\u00b7lich", "Volk", ",", "und", "Zu\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Goldne Tage des Saturnus", "tokens": ["Gold\u00b7ne", "Ta\u00b7ge", "des", "Sa\u00b7tur\u00b7nus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lebt man noch; es w\u00e4re Fabel,", "tokens": ["Lebt", "man", "noch", ";", "es", "w\u00e4\u00b7re", "Fa\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$.", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so viele tausend Frau'n", "tokens": ["Und", "so", "vie\u00b7le", "tau\u00b7send", "Frau'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "CARD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Predigen die holde Wahrheit?", "tokens": ["Pre\u00b7di\u00b7gen", "die", "hol\u00b7de", "Wahr\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch es neigt sich schon die Sonne,", "tokens": ["Doch", "es", "neigt", "sich", "schon", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon erbraust es in der Menge,", "tokens": ["Schon", "er\u00b7braust", "es", "in", "der", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meilenweit vom Obeliskus", "tokens": ["Mei\u00b7len\u00b7weit", "vom", "O\u00b7be\u00b7lis\u00b7kus"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Capitol \u2013 sie kommen \u2013", "tokens": ["Bis", "zum", "Ca\u00b7pi\u00b7tol", "\u2013", "sie", "kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$(", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein! sie fliegen \u2013 kaum vernimmst du", "tokens": ["Nein", "!", "sie", "flie\u00b7gen", "\u2013", "kaum", "ver\u00b7nimmst", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVINF", "$(", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihren Hufschlag \u2013 Alles jubelt", "tokens": ["Ih\u00b7ren", "Huf\u00b7schlag", "\u2013", "Al\u00b7les", "ju\u00b7belt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "L\u00e4ngst sind alle schon verschwunden.", "tokens": ["L\u00e4ngst", "sind", "al\u00b7le", "schon", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie ersehnt steigt jetzt die D\u00e4mm'rung", "tokens": ["Wie", "er\u00b7sehnt", "steigt", "jetzt", "die", "D\u00e4m\u00b7m'\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "ADV", "ART", "NN"], "meter": "+-++--++-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Von den m\u00e4chtigen Pal\u00e4sten", "tokens": ["Von", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Pa\u00b7l\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nieder in die tiefe Stra\u00dfe.", "tokens": ["Nie\u00b7der", "in", "die", "tie\u00b7fe", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein St\u00fcndchen, Kind der Liebe,", "tokens": ["Noch", "ein", "St\u00fcnd\u00b7chen", ",", "Kind", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch das k\u00f6stlichste der Erde!", "tokens": ["Doch", "das", "k\u00f6st\u00b7lichs\u00b7te", "der", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimm' dir einen Sitz, ein Lichtchen,", "tokens": ["Nimm'", "dir", "ei\u00b7nen", "Sitz", ",", "ein", "Licht\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn dem Weibe ziemt ein Licht,", "tokens": ["Denn", "dem", "Wei\u00b7be", "ziemt", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dem Manne ziemt's zu l\u00f6schen.", "tokens": ["Und", "dem", "Man\u00b7ne", "ziemt's", "zu", "l\u00f6\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und schon flammet nah' und ferne", "tokens": ["Und", "schon", "flam\u00b7met", "nah'", "und", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Balkonen und von Fenstern,", "tokens": ["Von", "Bal\u00b7ko\u00b7nen", "und", "von", "Fens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Aus Carossen, von den Sitzen", "tokens": ["Aus", "Ca\u00b7ros\u00b7sen", ",", "von", "den", "Sit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In unz\u00e4hlbar vielen H\u00e4nden", "tokens": ["In", "un\u00b7z\u00e4hl\u00b7bar", "vie\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Durch den Nachtduft ein beweglich", "tokens": ["Durch", "den", "Nacht\u00b7duft", "ein", "be\u00b7weg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Muntres Heer von kleinen Feuern,", "tokens": ["Mun\u00b7tres", "Heer", "von", "klei\u00b7nen", "Feu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein neuer Zaubertag", "tokens": ["Und", "ein", "neu\u00b7er", "Zau\u00b7ber\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hebt nun an, dem Fest zu leuchten.", "tokens": ["Hebt", "nun", "an", ",", "dem", "Fest", "zu", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Welch ein \u00fcbersinnlich M\u00e4rchen,", "tokens": ["Welch", "ein", "\u00fc\u00b7ber\u00b7sinn\u00b7lich", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie man's oft von leichten Sylphen,", "tokens": ["Wie", "man's", "oft", "von", "leich\u00b7ten", "Syl\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gnomen und von Salamandern,", "tokens": ["Gno\u00b7men", "und", "von", "Sa\u00b7la\u00b7man\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$