{"textgrid.poem.37076": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine Nachtigall,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gie\u00df nicht so laut die liebgl\u00fchnden Lieder,", "tokens": ["Gie\u00df", "nicht", "so", "laut", "die", "lieb\u00b7gl\u00fchn\u00b7den", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zu meiner Quaal,", "tokens": ["Zu", "mei\u00b7ner", "Qua\u00b7al", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vom Bl\u00fcthenast des Apfelbaums hernieder,", "tokens": ["Vom", "Bl\u00fc\u00b7the\u00b7nast", "des", "Ap\u00b7fel\u00b7baums", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Nachtigall!", "tokens": ["O", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie t\u00f6nen mir, o liebe Philomele,", "tokens": ["Sie", "t\u00f6\u00b7nen", "mir", ",", "o", "lie\u00b7be", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Bildni\u00df wach,", "tokens": ["Das", "Bild\u00b7ni\u00df", "wach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das lange schon, in meiner tr\u00fcben Seele,", "tokens": ["Das", "lan\u00b7ge", "schon", ",", "in", "mei\u00b7ner", "tr\u00fc\u00b7ben", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Schlummer lag.", "tokens": ["Im", "Schlum\u00b7mer", "lag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.4": {"line.1": {"text": "Die Unholdinn verbannet Rast und Schlummer", "tokens": ["Die", "Un\u00b7hol\u00b7dinn", "ver\u00b7ban\u00b7net", "Rast", "und", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch ihren Stab,", "tokens": ["Durch", "ih\u00b7ren", "Stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und st\u00fcrzet uns, nach jahrelangem Kummer,", "tokens": ["Und", "st\u00fcr\u00b7zet", "uns", ",", "nach", "jah\u00b7re\u00b7lan\u00b7gem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohl gar ins Grab.", "tokens": ["Wohl", "gar", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sie trinkt voll Gier von unserm Herzensblute,", "tokens": ["Sie", "trinkt", "voll", "Gier", "von", "un\u00b7serm", "Her\u00b7zens\u00b7blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schwelgt sich satt;", "tokens": ["Und", "schwelgt", "sich", "satt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Giebt Dornen dem, der sonst auf Rosen ruhte,", "tokens": ["Giebt", "Dor\u00b7nen", "dem", ",", "der", "sonst", "auf", "Ro\u00b7sen", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Lagerstatt.", "tokens": ["Zur", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und machet ihm die Welt zum offnen Grabe,", "tokens": ["Und", "ma\u00b7chet", "ihm", "die", "Welt", "zum", "off\u00b7nen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das seiner harrt.", "tokens": ["Das", "sei\u00b7ner", "harrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ungl\u00fccklich, wer von ihrem Zauberstabe", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", ",", "wer", "von", "ih\u00b7rem", "Zau\u00b7ber\u00b7sta\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Getroffen ward!", "tokens": ["Ge\u00b7trof\u00b7fen", "ward", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Fleuch tiefer in die gr\u00fcnen Finsterni\u00dfe,", "tokens": ["Fleuch", "tie\u00b7fer", "in", "die", "gr\u00fc\u00b7nen", "Fins\u00b7ter\u00b7ni\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O S\u00e4ngerin,", "tokens": ["O", "S\u00e4n\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und spend im Nest der treuen Gattin K\u00fc\u00dfe,", "tokens": ["Und", "spend", "im", "Nest", "der", "treu\u00b7en", "Gat\u00b7tin", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fleuch hin, fleuch hin!", "tokens": ["Fleuch", "hin", ",", "fleuch", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}