{"dta.poem.18093": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ein Gebet, da\u00df Gott die Spanier wiede-  \n rum vom Rheinstrom wolle treiben, 1620.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlag doch, du starcker Held, die scheu\u00dflichen Maranen,", "tokens": ["Schlag", "doch", ",", "du", "star\u00b7cker", "Held", ",", "die", "scheu\u00df\u00b7li\u00b7chen", "Ma\u00b7ra\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So leyder! ihre Zelt und Blutgef\u00e4rbten Fahnen", "tokens": ["So", "ley\u00b7der", "!", "ih\u00b7re", "Zelt", "und", "Blut\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Fah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "PPOSAT", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch jetzt in Deutschland bracht, an unsern sch\u00f6nen Rhein,", "tokens": ["Auch", "jetzt", "in", "Deutschland", "bracht", ",", "an", "un\u00b7sern", "sch\u00f6\u00b7nen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Waffen tragen mu\u00df, vor seinen guten Wein.", "tokens": ["Der", "Waf\u00b7fen", "tra\u00b7gen", "mu\u00df", ",", "vor", "sei\u00b7nen", "gu\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist genug gespielt mit eisernen Ballonen,", "tokens": ["Es", "ist", "ge\u00b7nug", "ge\u00b7spielt", "mit", "ei\u00b7ser\u00b7nen", "Bal\u00b7lo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Du grosser Capitain, h\u00f6r auf, fang an zu schonen,", "tokens": ["Du", "gros\u00b7ser", "Ca\u00b7pi\u00b7tain", ",", "h\u00f6r", "auf", ",", "fang", "an", "zu", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist genug, genug, die G\u00f6tter sind verheert", "tokens": ["Es", "ist", "ge\u00b7nug", ",", "ge\u00b7nug", ",", "die", "G\u00f6t\u00b7ter", "sind", "ver\u00b7heert"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch die, so sie gemacht, Stadt, Dorff, und Feld verkehrt.", "tokens": ["Durch", "die", ",", "so", "sie", "ge\u00b7macht", ",", "Stadt", ",", "Dorff", ",", "und", "Feld", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ADV", "PPER", "VVPP", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df die, durch deren Grimm die Str\u00f6me kaum geflossen", "tokens": ["La\u00df", "die", ",", "durch", "de\u00b7ren", "Grimm", "die", "Str\u00f6\u00b7me", "kaum", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "$,", "APPR", "PRELAT", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Von Leichen zugestopfft, nicht ausgehn ungenossen,", "tokens": ["Von", "Lei\u00b7chen", "zu\u00b7ge\u00b7stopfft", ",", "nicht", "aus\u00b7gehn", "un\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und mache kund, da\u00df der, der dir zugegen strebt,", "tokens": ["Und", "ma\u00b7che", "kund", ",", "da\u00df", "der", ",", "der", "dir", "zu\u00b7ge\u00b7gen", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "ART", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "St\u00fcrtzt, oder bleibt er ja, ihm selbst zur Straffe lebt.", "tokens": ["St\u00fcrtzt", ",", "o\u00b7der", "bleibt", "er", "ja", ",", "ihm", "selbst", "zur", "Straf\u00b7fe", "lebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}