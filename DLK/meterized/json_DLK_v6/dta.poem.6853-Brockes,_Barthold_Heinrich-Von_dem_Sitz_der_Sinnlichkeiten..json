{"dta.poem.6853": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In andern Sinnen auch, sowol als im Gesicht,", "tokens": ["In", "an\u00b7dern", "Sin\u00b7nen", "auch", ",", "so\u00b7wol", "als", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht man ein gleiches sich erregen,", "tokens": ["Sieht", "man", "ein", "glei\u00b7ches", "sich", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann ein zu starck gemischt, verwirretes Bewegen,", "tokens": ["Wann", "ein", "zu", "starck", "ge\u00b7mischt", ",", "ver\u00b7wir\u00b7re\u00b7tes", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "PTKA", "ADJD", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Harmonie verwirrt und unterbricht.", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "ver\u00b7wirrt", "und", "un\u00b7ter\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Seele kan sodann nicht ferner unterscheiden,", "tokens": ["Die", "See\u00b7le", "kan", "so\u00b7dann", "nicht", "fer\u00b7ner", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und aller Eindruck ist verwirrt,", "tokens": ["Und", "al\u00b7ler", "Ein\u00b7druck", "ist", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja offt geschichts, wenn sie mu\u00df solch Ersch\u00fcttern leiden,", "tokens": ["Ja", "offt", "ge\u00b7schichts", ",", "wenn", "sie", "mu\u00df", "solch", "Er\u00b7sch\u00fct\u00b7tern", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVPP", "$,", "KOUS", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df durch Bewegungen, die sie in Sinnen stifft,", "tokens": ["Da\u00df", "durch", "Be\u00b7we\u00b7gun\u00b7gen", ",", "die", "sie", "in", "Sin\u00b7nen", "stifft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Der Druck zum Ziel nicht v\u00f6llig trifft.", "tokens": ["Der", "Druck", "zum", "Ziel", "nicht", "v\u00f6l\u00b7lig", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wann nun von dem, was ihre Ordnung st\u00f6hret,", "tokens": ["Wann", "nun", "von", "dem", ",", "was", "ih\u00b7re", "Ord\u00b7nung", "st\u00f6h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Jhr keines wiederf\u00e4hret;", "tokens": ["Ihr", "kei\u00b7nes", "wie\u00b7der\u00b7f\u00e4h\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wird ein gesund Gehirn von innen,", "tokens": ["Wird", "ein", "ge\u00b7sund", "Ge\u00b7hirn", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durch tausend Gegenst\u00e4nd\u2019 erregt und eingenommen,", "tokens": ["Durch", "tau\u00b7send", "Ge\u00b7gen\u00b7st\u00e4nd'", "er\u00b7regt", "und", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die alle durch verschiedne Sinnen", "tokens": ["Die", "al\u00b7le", "durch", "ver\u00b7schied\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "An einen Ort zusammen kommen.", "tokens": ["An", "ei\u00b7nen", "Ort", "zu\u00b7sam\u00b7men", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}