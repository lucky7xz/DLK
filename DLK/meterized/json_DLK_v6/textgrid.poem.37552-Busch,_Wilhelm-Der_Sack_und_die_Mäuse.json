{"textgrid.poem.37552": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Sack und die M\u00e4use", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein dicker Sack voll Weizen stand", "tokens": ["Ein", "di\u00b7cker", "Sack", "voll", "Wei\u00b7zen", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auf einem Speicher an der Wand. \u2013", "tokens": ["auf", "ei\u00b7nem", "Spei\u00b7cher", "an", "der", "Wand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam das schlaue Volk der M\u00e4use", "tokens": ["Da", "kam", "das", "schlau\u00b7e", "Volk", "der", "M\u00e4u\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und pfiff ihn an in dieser Weise:", "tokens": ["und", "pfiff", "ihn", "an", "in", "die\u00b7ser", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbo du da in der Ecke,", "tokens": ["\u00bb", "o", "du", "da", "in", "der", "E\u00b7cke", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gro\u00dfm\u00e4chtigster der S\u00e4cke!", "tokens": ["gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7ter", "der", "S\u00e4\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Du bist ja der Gescheitste,", "tokens": ["Du", "bist", "ja", "der", "Ge\u00b7scheits\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "der Dickste und der Breitste!", "tokens": ["der", "Dicks\u00b7te", "und", "der", "Breits\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Respekt und Reverenz", "tokens": ["Res\u00b7pekt", "und", "Re\u00b7ve\u00b7renz"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "vor Eurer Exzellenz!\u00ab", "tokens": ["vor", "Eu\u00b7rer", "Ex\u00b7zel\u00b7lenz", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit innigem Behagen h\u00f6rt", "tokens": ["Mit", "in\u00b7ni\u00b7gem", "Be\u00b7ha\u00b7gen", "h\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Sack, da\u00df man ihn so verehrt.", "tokens": ["der", "Sack", ",", "da\u00df", "man", "ihn", "so", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein M\u00e4uslein hat ihm unterdessen", "tokens": ["Ein", "M\u00e4us\u00b7lein", "hat", "ihm", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ganz unbemerkt ein Loch gefressen.", "tokens": ["ganz", "un\u00b7be\u00b7merkt", "ein", "Loch", "ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es rinnt das Korn in leisem Lauf.", "tokens": ["Es", "rinnt", "das", "Korn", "in", "lei\u00b7sem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4use knuspern's emsig auf.", "tokens": ["Die", "M\u00e4u\u00b7se", "knus\u00b7pern's", "em\u00b7sig", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon wird er faltig, krumm und matt.", "tokens": ["Schon", "wird", "er", "fal\u00b7tig", ",", "krumm", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4use werden fett und glatt.", "tokens": ["Die", "M\u00e4u\u00b7se", "wer\u00b7den", "fett", "und", "glatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zuletzt, man kennt ihn kaum noch mehr,", "tokens": ["Zu\u00b7letzt", ",", "man", "kennt", "ihn", "kaum", "noch", "mehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PIS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ist er kaputt und hohl und leer.", "tokens": ["ist", "er", "ka\u00b7putt", "und", "hohl", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Jetzt ziehn sie ihn von seinem Thron;", "tokens": ["Jetzt", "ziehn", "sie", "ihn", "von", "sei\u00b7nem", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jedes M\u00e4uslein spricht ihm Hohn;", "tokens": ["Ein", "je\u00b7des", "M\u00e4us\u00b7lein", "spricht", "ihm", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "und jedes, wie es geht, so spricht's:", "tokens": ["und", "je\u00b7des", ",", "wie", "es", "geht", ",", "so", "spricht's", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbempfehle mich, Herr Habenichts!\u00ab", "tokens": ["\u00bb", "emp\u00b7feh\u00b7le", "mich", ",", "Herr", "Ha\u00b7be\u00b7nichts", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}