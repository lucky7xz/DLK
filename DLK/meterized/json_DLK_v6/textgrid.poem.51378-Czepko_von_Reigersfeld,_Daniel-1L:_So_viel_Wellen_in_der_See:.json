{"textgrid.poem.51378": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: So viel Wellen in der See:", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So viel Wellen in der See:", "tokens": ["So", "viel", "Wel\u00b7len", "in", "der", "See", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann die Fluth steigt in die H\u00f6h,", "tokens": ["Wann", "die", "Fluth", "steigt", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So viel Stern in hellen N\u00e4chten:", "tokens": ["So", "viel", "Stern", "in", "hel\u00b7len", "N\u00e4ch\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Derer Zahl nicht zu verfechten.", "tokens": ["De\u00b7rer", "Zahl", "nicht", "zu", "ver\u00b7fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So viel Bl\u00e4tter in der Welt,", "tokens": ["So", "viel", "Bl\u00e4t\u00b7ter", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Herbst sie runter f\u00e4lt;", "tokens": ["Wenn", "der", "Herbst", "sie", "run\u00b7ter", "f\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So viel Stipchen in der Sonnen,", "tokens": ["So", "viel", "Stipc\u00b7hen", "in", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Ausflu\u00df kommt geronnen.", "tokens": ["Wenn", "der", "Aus\u00b7flu\u00df", "kommt", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So viel Anschl\u00e4g auf der Post,", "tokens": ["So", "viel", "An\u00b7schl\u00e4g", "auf", "der", "Post", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4ltz ich t\u00e4glich in der Brust:", "tokens": ["W\u00e4ltz", "ich", "t\u00e4g\u00b7lich", "in", "der", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ein Anschlag doch vor allen,", "tokens": ["Ist", "ein", "An\u00b7schlag", "doch", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn blo\u00df dir wil ich gefallen.", "tokens": ["Denn", "blo\u00df", "dir", "wil", "ich", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}