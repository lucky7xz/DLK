{"textgrid.poem.62178": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[die Wolken sich t\u00fcrmen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken sich t\u00fcrmen", "tokens": ["Die", "Wol\u00b7ken", "sich", "t\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am himmlischen Zelt,", "tokens": ["Am", "himm\u00b7li\u00b7schen", "Zelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gepeitschet von St\u00fcrmen", "tokens": ["Ge\u00b7peit\u00b7schet", "von", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Strahl sie erhellt;", "tokens": ["Ein", "Strahl", "sie", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein Sonnenstrahl eilet,", "tokens": ["Ein", "Son\u00b7nen\u00b7strahl", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zerstreuet sie bald,", "tokens": ["Zer\u00b7streu\u00b7et", "sie", "bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im Nu sie zerteilet", "tokens": ["Im", "Nu", "sie", "zer\u00b7tei\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADV", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Lichtes Gewalt.", "tokens": ["Des", "Lich\u00b7tes", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Im menschlichen Leben", "tokens": ["Im", "menschli\u00b7chen", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Erleuchtet ein Strahl", "tokens": ["Er\u00b7leuch\u00b7tet", "ein", "Strahl"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Des Friedens manchmal", "tokens": ["Des", "Frie\u00b7dens", "manch\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die menschliche Qual:", "tokens": ["Die", "menschli\u00b7che", "Qual", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Wolken entschweben,", "tokens": ["Die", "Wol\u00b7ken", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Freuden sich heben.", "tokens": ["Die", "Freu\u00b7den", "sich", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}