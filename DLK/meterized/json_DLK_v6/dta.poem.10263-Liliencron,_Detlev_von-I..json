{"dta.poem.10263": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Wir standen unter alten Riesenulmen,               ", "tokens": ["Wir", "stan\u00b7den", "un\u00b7ter", "al\u00b7ten", "Rie\u00b7sen\u00b7ul\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An unsers Gartens Rand. Mein Arm umschlang", "tokens": ["An", "un\u00b7sers", "Gar\u00b7tens", "Rand", ".", "Mein", "Arm", "um\u00b7schlang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die schlanke H\u00fcfte dir. Es lag dein Haupt,", "tokens": ["Die", "schlan\u00b7ke", "H\u00fcf\u00b7te", "dir", ".", "Es", "lag", "dein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das sch\u00f6ne, blasse, still an meiner Schulter.", "tokens": ["Das", "sch\u00f6\u00b7ne", ",", "blas\u00b7se", ",", "still", "an", "mei\u00b7ner", "Schul\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein kalter Hauch drang uns entgegen; fr\u00f6stelnd", "tokens": ["Ein", "kal\u00b7ter", "Hauch", "drang", "uns", "ent\u00b7ge\u00b7gen", ";", "fr\u00f6s\u00b7telnd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zogst fester du das Tuch um deinen Hals.", "tokens": ["Zogst", "fes\u00b7ter", "du", "das", "Tuch", "um", "dei\u00b7nen", "Hals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In grauer Luft, un\u00fcbersehbar, lag", "tokens": ["In", "grau\u00b7er", "Luft", ",", "un\u00b7\u00fc\u00b7ber\u00b7seh\u00b7bar", ",", "lag"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Wiesen gr\u00fcnes Flachland ausgebreitet.", "tokens": ["Der", "Wie\u00b7sen", "gr\u00fc\u00b7nes", "Flach\u00b7land", "aus\u00b7ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie deutlich h\u00f6rten wir den Jungen schelten", "tokens": ["Wie", "deut\u00b7lich", "h\u00f6r\u00b7ten", "wir", "den", "Jun\u00b7gen", "schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf seine K\u00fche, deutlich h\u00f6r\u2019 ich noch", "tokens": ["Auf", "sei\u00b7ne", "K\u00fc\u00b7he", ",", "deut\u00b7lich", "h\u00f6r'", "ich", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dein fr\u00f6hlich Lachen, als uns die gesunden,", "tokens": ["Dein", "fr\u00f6h\u00b7lich", "La\u00b7chen", ",", "als", "uns", "die", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vom Winde hergetragnen Worte trafen.", "tokens": ["Vom", "Win\u00b7de", "her\u00b7ge\u00b7trag\u00b7nen", "Wor\u00b7te", "tra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und eine Oede, nordisch unbehaglich,", "tokens": ["Und", "ei\u00b7ne", "O\u00b7e\u00b7de", ",", "nor\u00b7disch", "un\u00b7be\u00b7hag\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Durchfror die Landschaft. Kr\u00e4hen stolperten,", "tokens": ["Durch\u00b7fror", "die", "Land\u00b7schaft", ".", "Kr\u00e4\u00b7hen", "stol\u00b7per\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Laut kr\u00e4chzend, \u00fcber\u2019n Garten. Schl\u00e4frig zog", "tokens": ["Laut", "kr\u00e4ch\u00b7zend", ",", "\u00fc\u00b7ber'n", "Gar\u00b7ten", ".", "Schl\u00e4f\u00b7rig", "zog"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "VVPP", "$,", "ADJA", "NN", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Am Horizont die M\u00fchle ihre Kreise.", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "die", "M\u00fch\u00b7le", "ih\u00b7re", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und doch! Es lag auf Wegen fern und nah", "tokens": ["Und", "doch", "!", "Es", "lag", "auf", "We\u00b7gen", "fern", "und", "nah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Sonnenschein, der Sonnenschein des Gl\u00fccks.", "tokens": ["Der", "Son\u00b7nen\u00b7schein", ",", "der", "Son\u00b7nen\u00b7schein", "des", "Gl\u00fccks", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und langsam kehrten wir zur\u00fcck ins Haus.", "tokens": ["Und", "lang\u00b7sam", "kehr\u00b7ten", "wir", "zu\u00b7r\u00fcck", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}