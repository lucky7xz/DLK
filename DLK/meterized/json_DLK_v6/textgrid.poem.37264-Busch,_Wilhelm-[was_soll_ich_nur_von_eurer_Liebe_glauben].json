{"textgrid.poem.37264": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[was soll ich nur von eurer Liebe glauben]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich nur von eurer Liebe glauben?", "tokens": ["Was", "soll", "ich", "nur", "von", "eu\u00b7rer", "Lie\u00b7be", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was kriecht ihr immer so in dunkle Lauben?", "tokens": ["Was", "kriecht", "ihr", "im\u00b7mer", "so", "in", "dunk\u00b7le", "Lau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wozu das ew'ge Fl\u00fcstern und Gemunkel?", "tokens": ["Wo\u00b7zu", "das", "ew'\u00b7ge", "Fl\u00fcs\u00b7tern", "und", "Ge\u00b7mun\u00b7kel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das scheinen h\u00f6chst verd\u00e4chtige Geschichten.", "tokens": ["Das", "schei\u00b7nen", "h\u00f6chst", "ver\u00b7d\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und selbst die besten ehelichen Pflichten,", "tokens": ["Und", "selbst", "die", "bes\u00b7ten", "e\u00b7he\u00b7li\u00b7chen", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von allem Tun die sch\u00f6nste T\u00e4tigkeit,", "tokens": ["Von", "al\u00b7lem", "Tun", "die", "sch\u00f6ns\u00b7te", "T\u00e4\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In Tempeln von des Priesters Hand geweiht,", "tokens": ["In", "Tem\u00b7peln", "von", "des", "Pries\u00b7ters", "Hand", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihr h\u00fcllt sie in ein schuldbewu\u00dftes Dunkel.", "tokens": ["Ihr", "h\u00fcllt", "sie", "in", "ein", "schuld\u00b7be\u00b7wu\u00df\u00b7tes", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}