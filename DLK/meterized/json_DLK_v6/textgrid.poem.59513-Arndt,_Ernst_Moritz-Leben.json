{"textgrid.poem.59513": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Leben", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Kind,", "tokens": ["Ich", "war", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie Fr\u00fchlingss\u00e4usel flogen", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7s\u00e4u\u00b7sel", "flo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lebenssorgen spielend um meine Locken;", "tokens": ["Die", "Le\u00b7bens\u00b7sor\u00b7gen", "spie\u00b7lend", "um", "mei\u00b7ne", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das Gras gab weich die Blumendecke,", "tokens": ["Das", "Gras", "gab", "weich", "die", "Blu\u00b7men\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Himmel das ungemessene Aug' \u2013", "tokens": ["Der", "Him\u00b7mel", "das", "un\u00b7ge\u00b7mes\u00b7se\u00b7ne", "Aug'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Leben und Traum noch eins:", "tokens": ["Le\u00b7ben", "und", "Traum", "noch", "eins", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PIS", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Mich wiegte in beiden", "tokens": ["Mich", "wieg\u00b7te", "in", "bei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Wiege der Liebe.", "tokens": ["Die", "Wie\u00b7ge", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Knabe ward ich.", "tokens": ["Ein", "Kna\u00b7be", "ward", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Oft in den Hain der Eichen", "tokens": ["Oft", "in", "den", "Hain", "der", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nahm mich mein Vater unter die heiligen Lauben;", "tokens": ["Nahm", "mich", "mein", "Va\u00b7ter", "un\u00b7ter", "die", "hei\u00b7li\u00b7gen", "Lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Legte hinter die Garben des Feldes", "tokens": ["Leg\u00b7te", "hin\u00b7ter", "die", "Gar\u00b7ben", "des", "Fel\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Oft des M\u00fcden Ohr an des Meeres Sausen.", "tokens": ["Oft", "des", "M\u00fc\u00b7den", "Ohr", "an", "des", "Mee\u00b7res", "Sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich bebte unter den regen Eichenwipfeln,", "tokens": ["Ich", "beb\u00b7te", "un\u00b7ter", "den", "re\u00b7gen", "Ei\u00b7chen\u00b7wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Weinte ob des Meeres Sausen,", "tokens": ["Wein\u00b7te", "ob", "des", "Mee\u00b7res", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00fcckte vor dem Donner des Himmels", "tokens": ["Dr\u00fcck\u00b7te", "vor", "dem", "Don\u00b7ner", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Mit der Lerche, dem Reh mich hinter die B\u00fcsche.", "tokens": ["Mit", "der", "Ler\u00b7che", ",", "dem", "Reh", "mich", "hin\u00b7ter", "die", "B\u00fc\u00b7sche", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Doch bl\u00fchten mir Blumen,", "tokens": ["Doch", "bl\u00fch\u00b7ten", "mir", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Mir sangen die L\u00fcfte, die V\u00f6gel,", "tokens": ["Mir", "san\u00b7gen", "die", "L\u00fcf\u00b7te", ",", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Warm schien die Sonne, der Fruchtbaum golden,", "tokens": ["Warm", "schien", "die", "Son\u00b7ne", ",", "der", "Frucht\u00b7baum", "gol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJA", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Sanft trug das Meer oft des Schaukelnden Kahn.", "tokens": ["Sanft", "trug", "das", "Meer", "oft", "des", "Schau\u00b7keln\u00b7den", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich ward ein J\u00fcngling.", "tokens": ["Ich", "ward", "ein", "J\u00fcng\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "G\u00f6tter des Himmels all!", "tokens": ["G\u00f6t\u00b7ter", "des", "Him\u00b7mels", "all", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihr kamt herab mit eurem seligen Traum.", "tokens": ["Ihr", "kamt", "her\u00b7ab", "mit", "eu\u00b7rem", "se\u00b7li\u00b7gen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Schwellend stand ich am Meer wie Wogen,", "tokens": ["Schwel\u00b7lend", "stand", "ich", "am", "Meer", "wie", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "KOKOM", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wollte flie\u00dfen fort mit den Waffern,", "tokens": ["Woll\u00b7te", "flie\u00b7\u00dfen", "fort", "mit", "den", "Waf\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Stand lebendig unter dem Eichbaum,", "tokens": ["Stand", "le\u00b7ben\u00b7dig", "un\u00b7ter", "dem", "Eich\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "F\u00fchlte mich wie L\u00fcfte gefiedert.", "tokens": ["F\u00fchl\u00b7te", "mich", "wie", "L\u00fcf\u00b7te", "ge\u00b7fie\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Adler des Himmels, ihr trugt mich oft", "tokens": ["Ad\u00b7ler", "des", "Him\u00b7mels", ",", "ihr", "trugt", "mich", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Gl\u00e4nzend in eure Donnerwolken,", "tokens": ["Gl\u00e4n\u00b7zend", "in", "eu\u00b7re", "Don\u00b7ner\u00b7wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "In eurer Sonnen br\u00fcnstige Glut;", "tokens": ["In", "eu\u00b7rer", "Son\u00b7nen", "br\u00fcns\u00b7ti\u00b7ge", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Blumen der Erde, heiliger Mond,", "tokens": ["Blu\u00b7men", "der", "Er\u00b7de", ",", "hei\u00b7li\u00b7ger", "Mond", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Freundliche Nacht, wie liebt' ich euch,", "tokens": ["Freund\u00b7li\u00b7che", "Nacht", ",", "wie", "liebt'", "ich", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Meine erste Liebe, geheim!", "tokens": ["Mei\u00b7ne", "ers\u00b7te", "Lie\u00b7be", ",", "ge\u00b7heim", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Schimmernd flo\u00df mir des Lebens Wolke", "tokens": ["Schim\u00b7mernd", "flo\u00df", "mir", "des", "Le\u00b7bens", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Um die schuldlosen Locken noch;", "tokens": ["Um", "die", "schuld\u00b7lo\u00b7sen", "Lo\u00b7cken", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie prophetischer Raben Silberklang", "tokens": ["Wie", "pro\u00b7phe\u00b7ti\u00b7scher", "Ra\u00b7ben", "Sil\u00b7ber\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Aus einsamer Luft", "tokens": ["Aus", "ein\u00b7sa\u00b7mer", "Luft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Umklangen mich T\u00f6ne der Zukunft.", "tokens": ["Um\u00b7klan\u00b7gen", "mich", "T\u00f6\u00b7ne", "der", "Zu\u00b7kunft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.19": {"text": "Ich lebt' und war gl\u00fccklich.", "tokens": ["Ich", "lebt'", "und", "war", "gl\u00fcck\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Ich ward ein Mann.", "tokens": ["Ich", "ward", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die himmlischen G\u00f6tter all,", "tokens": ["Die", "himm\u00b7li\u00b7schen", "G\u00f6t\u00b7ter", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die spielenden all, in ernster Gestalt", "tokens": ["Die", "spie\u00b7len\u00b7den", "all", ",", "in", "erns\u00b7ter", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "PIAT", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stehen sie da: die \u00c4gide", "tokens": ["Ste\u00b7hen", "sie", "da", ":", "die", "\u00c4\u00b7gi\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00fcttelt Minerva, zum h\u00f6llischen Webstuhl", "tokens": ["Sch\u00fct\u00b7telt", "Mi\u00b7ner\u00b7va", ",", "zum", "h\u00f6l\u00b7li\u00b7schen", "Web\u00b7stuhl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Sah ich hinab ins Dunkel der Parzen:", "tokens": ["Sah", "ich", "hin\u00b7ab", "ins", "Dun\u00b7kel", "der", "Par\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie sa\u00dfen und webten", "tokens": ["Sie", "sa\u00b7\u00dfen", "und", "web\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Tr\u00e4nen und Freuden im schrecklichen Schweigen.", "tokens": ["Tr\u00e4\u00b7nen", "und", "Freu\u00b7den", "im", "schreck\u00b7li\u00b7chen", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Und des Blutes gefl\u00fcgelte", "tokens": ["Und", "des", "Blu\u00b7tes", "ge\u00b7fl\u00fc\u00b7gel\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "R\u00e4cherinnen, die Eumeniden,", "tokens": ["R\u00e4\u00b7che\u00b7rin\u00b7nen", ",", "die", "Eu\u00b7me\u00b7ni\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Standen umher, die grinsende Ate", "tokens": ["Stan\u00b7den", "um\u00b7her", ",", "die", "grin\u00b7sen\u00b7de", "A\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Flocht verworrene Knoten der Schuld,", "tokens": ["Flocht", "ver\u00b7wor\u00b7re\u00b7ne", "Kno\u00b7ten", "der", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Und meinem Donner droben", "tokens": ["Und", "mei\u00b7nem", "Don\u00b7ner", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Fehlte der Klang, doch fra\u00df", "tokens": ["Fehl\u00b7te", "der", "Klang", ",", "doch", "fra\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Mir sein Blitzstrahl die H\u00fctte.", "tokens": ["Mir", "sein", "Blitz\u00b7strahl", "die", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Flehend sah ich zum Himmel,", "tokens": ["Fle\u00b7hend", "sah", "ich", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPRART", "NN", 