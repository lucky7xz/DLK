{"dta.poem.4014": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Abendphantasie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Vor seiner H\u00fctte ruhigem Schatten sitzt", "tokens": ["Vor", "sei\u00b7ner", "H\u00fct\u00b7te", "ru\u00b7hi\u00b7gem", "Schat\u00b7ten", "sitzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Pfl\u00fcger, dem Gen\u00fcgsamen rauscht sein Heerd.", "tokens": ["Der", "Pfl\u00fc\u00b7ger", ",", "dem", "Ge\u00b7n\u00fcg\u00b7sa\u00b7men", "rauscht", "sein", "Heerd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gastfreundlich t\u00f6nt dem Wanderer im", "tokens": ["Gast\u00b7freund\u00b7lich", "t\u00f6nt", "dem", "Wan\u00b7de\u00b7rer", "im"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Friedlichen Dorfe die Abendglocke.", "tokens": ["Fried\u00b7li\u00b7chen", "Dor\u00b7fe", "die", "A\u00b7bend\u00b7glo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wohl kehren jetzt die Schiffer zum Hafen auch,", "tokens": ["Wohl", "keh\u00b7ren", "jetzt", "die", "Schif\u00b7fer", "zum", "Ha\u00b7fen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In fernen St\u00e4dten fr\u00f6hlich verrauscht des Markts", "tokens": ["In", "fer\u00b7nen", "St\u00e4d\u00b7ten", "fr\u00f6h\u00b7lich", "ver\u00b7rauscht", "des", "Markts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gesch\u00e4ft'ger L\u00e4rm; in stiller Laube", "tokens": ["Ge\u00b7sch\u00e4ft'\u00b7ger", "L\u00e4rm", ";", "in", "stil\u00b7ler", "Lau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzt das gesellige Mahl den Freunden.", "tokens": ["Gl\u00e4nzt", "das", "ge\u00b7sel\u00b7li\u00b7ge", "Mahl", "den", "Freun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wohin denn ich? Es leben die Sterblichen", "tokens": ["Wo\u00b7hin", "denn", "ich", "?", "Es", "le\u00b7ben", "die", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Lohn und Arbeit; wechselnd in M\u00fch' und Ruh'", "tokens": ["Von", "Lohn", "und", "Ar\u00b7beit", ";", "wech\u00b7selnd", "in", "M\u00fch'", "und", "Ruh'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist alles freudig; warum schl\u00e4ft denn", "tokens": ["Ist", "al\u00b7les", "freu\u00b7dig", ";", "wa\u00b7rum", "schl\u00e4ft", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADJD", "$.", "PWAV", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmer nur mir in der Brust der Stachel?", "tokens": ["Nim\u00b7mer", "nur", "mir", "in", "der", "Brust", "der", "Sta\u00b7chel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Am Abendhimmel bl\u00fcht ein Fr\u00fchling auf;", "tokens": ["Am", "A\u00b7bend\u00b7him\u00b7mel", "bl\u00fcht", "ein", "Fr\u00fch\u00b7ling", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unz\u00e4hlig bl\u00fch'n die Rosen und ruhig scheint", "tokens": ["Un\u00b7z\u00e4h\u00b7lig", "bl\u00fch'n", "die", "Ro\u00b7sen", "und", "ru\u00b7hig", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "ADJD", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die goldne Welt; o dorthin nehmt mich", "tokens": ["Die", "gold\u00b7ne", "Welt", ";", "o", "dor\u00b7thin", "nehmt", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "FM", "ADV", "VVFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Purpurne Wolken! und m\u00f6ge droben", "tokens": ["Pur\u00b7pur\u00b7ne", "Wol\u00b7ken", "!", "und", "m\u00f6\u00b7ge", "dro\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "VMFIN", "ADV"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "In Licht und Luft zerrinnen mir Lieb und Leid! \u2014", "tokens": ["In", "Licht", "und", "Luft", "zer\u00b7rin\u00b7nen", "mir", "Lieb", "und", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch, wie verscheucht von th\u00f6richter Bitte, flieht", "tokens": ["Doch", ",", "wie", "ver\u00b7scheucht", "von", "th\u00f6\u00b7rich\u00b7ter", "Bit\u00b7te", ",", "flieht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "PWAV", "VVPP", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Zauber; dunkel wird's, und einsam", "tokens": ["Der", "Zau\u00b7ber", ";", "dun\u00b7kel", "wird's", ",", "und", "ein\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ADJD", "VAFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter dem Himmel, wie immer, bin ich.", "tokens": ["Un\u00b7ter", "dem", "Him\u00b7mel", ",", "wie", "im\u00b7mer", ",", "bin", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADV", "$,", "VAFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Komm du nun, sanfter Schlummer! zu viel begehrt", "tokens": ["Komm", "du", "nun", ",", "sanf\u00b7ter", "Schlum\u00b7mer", "!", "zu", "viel", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$.", "PTKA", "PIS", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Herz; doch endlich, Jugend, vergl\u00fchst du ja,", "tokens": ["Das", "Herz", ";", "doch", "end\u00b7lich", ",", "Ju\u00b7gend", ",", "ver\u00b7gl\u00fchst", "du", "ja", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du ruhelose, tr\u00e4umerische!", "tokens": ["Du", "ru\u00b7he\u00b7lo\u00b7se", ",", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Friedlich und heiter ist dann das Alter.", "tokens": ["Fried\u00b7lich", "und", "hei\u00b7ter", "ist", "dann", "das", "Al\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}