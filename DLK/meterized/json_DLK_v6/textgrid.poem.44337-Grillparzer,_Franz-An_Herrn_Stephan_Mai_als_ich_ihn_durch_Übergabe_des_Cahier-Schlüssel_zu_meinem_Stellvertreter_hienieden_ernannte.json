{"textgrid.poem.44337": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Stephan Mai als ich ihn durch \u00dcbergabe des Cahier-Schl\u00fcssel zu meinem Stellvertreter hienieden ernannte", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, der ich hier so manchen Dienstag, mitten", "tokens": ["Ich", ",", "der", "ich", "hier", "so", "man\u00b7chen", "Diens\u00b7tag", ",", "mit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "PIAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einem Aktenhauf zum Kreuz verdammt,", "tokens": ["In", "ei\u00b7nem", "Ak\u00b7ten\u00b7hauf", "zum", "Kreuz", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr das gesamte Zollb\u00fcro gelitten,", "tokens": ["F\u00fcr", "das", "ge\u00b7sam\u00b7te", "Zoll\u00b7b\u00fc\u00b7ro", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich \u00fcbertrage dir mein Mittleramt.", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7tra\u00b7ge", "dir", "mein", "Mitt\u00b7le\u00b7ramt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nimm hin den Schl\u00fcssel, hochbegabtes Wesen,", "tokens": ["Nimm", "hin", "den", "Schl\u00fcs\u00b7sel", ",", "hoch\u00b7be\u00b7gab\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nimm ihn von meiner unnennbaren Huld,", "tokens": ["Nimm", "ihn", "von", "mei\u00b7ner", "un\u00b7nenn\u00b7ba\u00b7ren", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihm die Kraft zu binden und zu l\u00f6sen,", "tokens": ["Mit", "ihm", "die", "Kraft", "zu", "bin\u00b7den", "und", "zu", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wom\u00f6glich auch zu beidem die Geduld!", "tokens": ["Wo\u00b7m\u00f6g\u00b7lich", "auch", "zu", "bei\u00b7dem", "die", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und was du binden wirst, das sei versto\u00dfen,", "tokens": ["Und", "was", "du", "bin\u00b7den", "wirst", ",", "das", "sei", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VAFIN", "$,", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fern von dem Strahl des Himmelslichts,", "tokens": ["Fern", "von", "dem", "Strahl", "des", "Him\u00b7mels\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jenen schwarzen Schlund verschlossen,", "tokens": ["In", "je\u00b7nen", "schwar\u00b7zen", "Schlund", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis zu dem Tage des Gerichts.", "tokens": ["Bis", "zu", "dem", "Ta\u00b7ge", "des", "Ge\u00b7richts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Doch was du freigibst, wandre fr\u00f6hlich", "tokens": ["Doch", "was", "du", "frei\u00b7gibst", ",", "wand\u00b7re", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Fegefeur der Korrektur", "tokens": ["Durchs", "Fe\u00b7ge\u00b7feur", "der", "Kor\u00b7rek\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ruh als leichtes Kurrens selig", "tokens": ["Und", "ruh", "als", "leich\u00b7tes", "Kur\u00b7rens", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KOUS", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Himmel der Registratur.", "tokens": ["Im", "Him\u00b7mel", "der", "Re\u00b7gist\u00b7ra\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}