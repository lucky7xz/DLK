{"dta.poem.7914": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Zimmerspruch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Das neue Haus ist aufgericht\u2019t,", "tokens": ["Das", "neu\u00b7e", "Haus", "ist", "auf\u00b7ge\u00b7richt't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedeckt, gemauert ist es nicht,", "tokens": ["Ge\u00b7deckt", ",", "ge\u00b7mau\u00b7ert", "ist", "es", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch k\u00f6nnen Regen und Sonnenschein", "tokens": ["Noch", "k\u00f6n\u00b7nen", "Re\u00b7gen", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von oben und \u00fcberall herein:", "tokens": ["Von", "o\u00b7ben", "und", "\u00fc\u00b7be\u00b7rall", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Drum rufen wir zum Meister der Welt,", "tokens": ["Drum", "ru\u00b7fen", "wir", "zum", "Meis\u00b7ter", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Er wolle von dem Himmelszelt", "tokens": ["Er", "wol\u00b7le", "von", "dem", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur Heil und Segen gie\u00dfen aus", "tokens": ["Nur", "Heil", "und", "Se\u00b7gen", "gie\u00b7\u00dfen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hier \u00fcber dieses offne Haus.", "tokens": ["Hier", "\u00fc\u00b7ber", "die\u00b7ses", "off\u00b7ne", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zuoberst woll\u2019 er gut Gedeihn", "tokens": ["Zu\u00b7o\u00b7berst", "woll'", "er", "gut", "Ge\u00b7deihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In die Kornb\u00f6den uns verleihn;", "tokens": ["In", "die", "Korn\u00b7b\u00f6\u00b7den", "uns", "ver\u00b7leihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "In die Stube Flei\u00df und Fr\u00f6mmigkeit,", "tokens": ["In", "die", "Stu\u00b7be", "Flei\u00df", "und", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "In die K\u00fcche Ma\u00df und Reinlichkeit,", "tokens": ["In", "die", "K\u00fc\u00b7che", "Ma\u00df", "und", "Rein\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "In den Stall Gesundheit allermeist,", "tokens": ["In", "den", "Stall", "Ge\u00b7sund\u00b7heit", "al\u00b7ler\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "In den Keller dem Wein einen guten Geist;", "tokens": ["In", "den", "Kel\u00b7ler", "dem", "Wein", "ei\u00b7nen", "gu\u00b7ten", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Die Fenster und Pforten woll\u2019 er weihn,", "tokens": ["Die", "Fens\u00b7ter", "und", "Pfor\u00b7ten", "woll'", "er", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Da\u00df nichts Unseligs komm\u2019 herein,", "tokens": ["Da\u00df", "nichts", "Un\u00b7se\u00b7ligs", "komm'", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Und da\u00df aus dieser neuen Th\u00fcr", "tokens": ["Und", "da\u00df", "aus", "die\u00b7ser", "neu\u00b7en", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bald fromme Kindlein springen f\u00fcr.", "tokens": ["Bald", "from\u00b7me", "Kin\u00b7dlein", "sprin\u00b7gen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Nun, Maurer, decket und mauret aus!", "tokens": ["Nun", ",", "Mau\u00b7rer", ",", "de\u00b7cket", "und", "mau\u00b7ret", "aus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Der Segen Gottes ist im Haus.", "tokens": ["Der", "Se\u00b7gen", "Got\u00b7tes", "ist", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}