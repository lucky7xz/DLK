{"dta.poem.12136": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ehr' hat ihr Ungemach; oft ziehn mu\u00df seinen Hut", "tokens": ["Ehr'", "hat", "ihr", "Un\u00b7ge\u00b7mach", ";", "oft", "ziehn", "mu\u00df", "sei\u00b7nen", "Hut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "VVINF", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Mann, dem jedermann des Gr\u00fc\u00dfens Ehr' anthut.", "tokens": ["Ein", "Mann", ",", "dem", "je\u00b7der\u00b7mann", "des", "Gr\u00fc\u00b7\u00dfens", "Ehr'", "an\u00b7thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein solcher, wenn er fein will danken allen Gr\u00fc\u00dfen,", "tokens": ["Ein", "sol\u00b7cher", ",", "wenn", "er", "fein", "will", "dan\u00b7ken", "al\u00b7len", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird einen neuen Hut des Jahrs mehr haben m\u00fc\u00dfen.", "tokens": ["Wird", "ei\u00b7nen", "neu\u00b7en", "Hut", "des", "Jahrs", "mehr", "ha\u00b7ben", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "AR