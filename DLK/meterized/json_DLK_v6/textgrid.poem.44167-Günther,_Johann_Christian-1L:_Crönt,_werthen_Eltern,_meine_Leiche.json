{"textgrid.poem.44167": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Cr\u00f6nt, werthen Eltern, meine Leiche", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cr\u00f6nt, werthen Eltern, meine Leiche", "tokens": ["Cr\u00f6nt", ",", "wert\u00b7hen", "El\u00b7tern", ",", "mei\u00b7ne", "Lei\u00b7che"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Myrthen, Rosen und Jasmin,", "tokens": ["Mit", "Myr\u00b7then", ",", "Ro\u00b7sen", "und", "Jas\u00b7min", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und last die sch\u00f6nsten Blumenstr\u00e4uche", "tokens": ["Und", "last", "die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf meiner fr\u00fchen Bahre bl\u00fchn,", "tokens": ["Auf", "mei\u00b7ner", "fr\u00fc\u00b7hen", "Bah\u00b7re", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem der Engel Siegeswagen", "tokens": ["Nach\u00b7dem", "der", "En\u00b7gel", "Sie\u00b7ges\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich ins gelobte Land getragen.", "tokens": ["Mich", "ins", "ge\u00b7lob\u00b7te", "Land", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "An mir ersaht ihr mit Erbarmen", "tokens": ["An", "mir", "er\u00b7saht", "ihr", "mit", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den schwersten Kampf der lezten Noth.", "tokens": ["Den", "schwers\u00b7ten", "Kampf", "der", "lez\u00b7ten", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es rungen die geschw\u00e4chten Armen", "tokens": ["Es", "run\u00b7gen", "die", "ge\u00b7schw\u00e4ch\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Jammer, Unruh, Angst und Tod,", "tokens": ["Mit", "Jam\u00b7mer", ",", "Un\u00b7ruh", ",", "Angst", "und", "Tod", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und durch die abgezehrten Glieder", "tokens": ["Und", "durch", "die", "ab\u00b7ge\u00b7zehr\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Lief Schmerz und Elend hin und wieder.", "tokens": ["Lief", "Schmerz", "und", "E\u00b7lend", "hin", "und", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ri\u00df damahls euer Herz in St\u00fccken", "tokens": ["Ri\u00df", "da\u00b7mahls", "eu\u00b7er", "Herz", "in", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollt euch aller Trost entfliehn,", "tokens": ["Und", "wollt", "euch", "al\u00b7ler", "Trost", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da meiner Finger scharfes Z\u00fccken", "tokens": ["Da", "mei\u00b7ner", "Fin\u00b7ger", "schar\u00b7fes", "Z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Eitelkeit zu wincken schien,", "tokens": ["Der", "Ei\u00b7tel\u00b7keit", "zu", "win\u00b7cken", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So gebt euch jezo nur zufrieden,", "tokens": ["So", "gebt", "euch", "je\u00b7zo", "nur", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Elend ist mit mir verschieden.", "tokens": ["Das", "E\u00b7lend", "ist", "mit", "mir", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Last Perlen statt der Thr\u00e4nen fallen,", "tokens": ["Last", "Per\u00b7len", "statt", "der", "Thr\u00e4\u00b7nen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Unschuld braucht sie in mein Kleid.", "tokens": ["Die", "Un\u00b7schuld", "braucht", "sie", "in", "mein", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, h\u00f6rtet ihr die Lieder schallen,", "tokens": ["Ach", ",", "h\u00f6r\u00b7tet", "ihr", "die", "Lie\u00b7der", "schal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Woran sich jezt mein Ohr erfreut,", "tokens": ["Wo\u00b7ran", "sich", "jezt", "mein", "Ohr", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr w\u00fcrdet euch des Klagens sch\u00e4men", "tokens": ["Ihr", "w\u00fcr\u00b7det", "euch", "des", "Kla\u00b7gens", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und um mein Gl\u00fccke wohl nicht gr\u00e4men.", "tokens": ["Und", "um", "mein", "Gl\u00fc\u00b7cke", "wohl", "nicht", "gr\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was h\u00e4tt ich euch vor M\u00fch und Kummer", "tokens": ["Was", "h\u00e4tt", "ich", "euch", "vor", "M\u00fch", "und", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht auf Erden noch gemacht,", "tokens": ["Viel\u00b7leicht", "auf", "Er\u00b7den", "noch", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wofern mich nicht der lezte Schlummer", "tokens": ["Wo\u00b7fern", "mich", "nicht", "der", "lez\u00b7te", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So zeitig in die Ruh gebracht!", "tokens": ["So", "zei\u00b7tig", "in", "die", "Ruh", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mancher Sorgen und Beschwerden", "tokens": ["Wie", "man\u00b7cher", "Sor\u00b7gen", "und", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Entladet euch mein Grab auf Erden!", "tokens": ["Ent\u00b7la\u00b7det", "euch", "mein", "Grab", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Jezt bin ich der Gefahr entflogen,", "tokens": ["Jezt", "bin", "ich", "der", "Ge\u00b7fahr", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Womit die List der b\u00f6sen Welt", "tokens": ["Wo\u00b7mit", "die", "List", "der", "b\u00f6\u00b7sen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wie des wilden Meeres Wogen", "tokens": ["So", "wie", "des", "wil\u00b7den", "Mee\u00b7res", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Jugend oft in Abgrund schnellt.", "tokens": ["Die", "Ju\u00b7gend", "oft", "in", "Ab\u00b7grund", "schnellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jezt kan mich weiter nichts verf\u00fchren,", "tokens": ["Jezt", "kan", "mich", "wei\u00b7ter", "nichts", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr aber mich nicht mehr verlieren.", "tokens": ["Ihr", "a\u00b7ber", "mich", "nicht", "mehr", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Legt also dem entseelten Leibe", "tokens": ["Legt", "al\u00b7so", "dem", "ent\u00b7seel\u00b7ten", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kleid der gr\u00fcnen Hofnung an;", "tokens": ["Das", "Kleid", "der", "gr\u00fc\u00b7nen", "Hof\u00b7nung", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn weil ich euch zum Zeitvertreibe", "tokens": ["Denn", "weil", "ich", "euch", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Erden nicht mehr dienen kan,", "tokens": ["Auf", "Er\u00b7den", "nicht", "mehr", "die\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So werd ich hier bey Salems Sch\u00e4zen", "tokens": ["So", "werd", "ich", "hier", "bey", "Sa\u00b7lems", "Sch\u00e4\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Euch einmahl desto mehr erg\u00f6zen.", "tokens": ["Euch", "ein\u00b7mahl", "des\u00b7to", "mehr", "er\u00b7g\u00f6\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Liegt irgendwo in eurer Kammer", "tokens": ["Liegt", "ir\u00b7gend\u00b7wo", "in", "eu\u00b7rer", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Spielwerck oder Kleid von mir,", "tokens": ["Ein", "Spiel\u00b7werck", "o\u00b7der", "Kleid", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So denckt dabey an meinen Jammer", "tokens": ["So", "denckt", "da\u00b7bey", "an", "mei\u00b7nen", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit diesem Troste: Weit von hier,", "tokens": ["Mit", "die\u00b7sem", "Tros\u00b7te", ":", "Weit", "von", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von hier, wo Herrligkeit und Leben", "tokens": ["Von", "hier", ",", "wo", "Herr\u00b7lig\u00b7keit", "und", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein nicht mehr schwaches Haupt umgeben.", "tokens": ["Mein", "nicht", "mehr", "schwa\u00b7ches", "Haupt", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKNEG", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hier wird die eingefallne Scheitel", "tokens": ["Hier", "wird", "die", "ein\u00b7ge\u00b7fall\u00b7ne", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Glanz und Klarheit angef\u00fcllt,", "tokens": ["Mit", "Glanz", "und", "Klar\u00b7heit", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey euch ist aller Reichthum eitel,", "tokens": ["Bey", "euch", "ist", "al\u00b7ler", "Reicht\u00b7hum", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da hier mein Wechsel ewig gilt,", "tokens": ["Da", "hier", "mein", "Wech\u00b7sel", "e\u00b7wig", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Wechsel, der nach wenig Tagen", "tokens": ["Mein", "Wech\u00b7sel", ",", "der", "nach", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den besten Wucher eingetragen.", "tokens": ["Den", "bes\u00b7ten", "Wu\u00b7cher", "ein\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Es r\u00fchrt mich weder Qual noch Schr\u00f6cken", "tokens": ["Es", "r\u00fchrt", "mich", "we\u00b7der", "Qual", "noch", "Schr\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Gottes weiser Allmachtshand,", "tokens": ["In", "Got\u00b7tes", "wei\u00b7ser", "All\u00b7machts\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wir hier h\u00f6ren, sehn und schmecken,", "tokens": ["Was", "wir", "hier", "h\u00f6\u00b7ren", ",", "sehn", "und", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "$,", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist euren Sinnen unbekand.", "tokens": ["Ist", "eu\u00b7ren", "Sin\u00b7nen", "un\u00b7be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, g\u00f6nnt doch eurem lieben Sohne", "tokens": ["Ach", ",", "g\u00f6nnt", "doch", "eu\u00b7rem", "lie\u00b7ben", "Soh\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freyheit vor des Lammes Throne.", "tokens": ["Die", "Frey\u00b7heit", "vor", "des", "Lam\u00b7mes", "Thro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Lobt den, durch de\u00dfen Vaterg\u00fcte", "tokens": ["Lobt", "den", ",", "durch", "de\u00b7\u00dfen", "Va\u00b7ter\u00b7g\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mein zeitlich Creuz so bald vergeht,", "tokens": ["Mein", "zeit\u00b7lich", "Creuz", "so", "bald", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaubt, da\u00df mein getreu Gem\u00fcthe", "tokens": ["Und", "glaubt", ",", "da\u00df", "mein", "ge\u00b7treu", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Gott auch euer Lob erh\u00f6ht.", "tokens": ["Vor", "Gott", "auch", "eu\u00b7er", "Lob", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier r\u00fchm ich mit dem reinsten Triebe", "tokens": ["Hier", "r\u00fchm", "ich", "mit", "dem", "reins\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sorgfalt mir erwiesner Liebe.", "tokens": ["Die", "Sorg\u00b7falt", "mir", "er\u00b7wies\u00b7ner", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mit diesem Dancke nehmt vor Willen,", "tokens": ["Mit", "die\u00b7sem", "Dan\u00b7cke", "nehmt", "vor", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seht mir in den Himmel nach,", "tokens": ["Und", "seht", "mir", "in", "den", "Him\u00b7mel", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird sich alle Wehmuth stillen,", "tokens": ["So", "wird", "sich", "al\u00b7le", "Weh\u00b7muth", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wormit ich euch das Herze brach.", "tokens": ["Wor\u00b7mit", "ich", "euch", "das", "Her\u00b7ze", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lebt wohl! Und w\u00fcntscht ihr mehr zu h\u00f6ren,", "tokens": ["Lebt", "wohl", "!", "Und", "w\u00fcnt\u00b7scht", "ihr", "mehr", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "KON", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.6": {"text": "So kommt fein bald zu unsern Ch\u00f6ren!", "tokens": ["So", "kommt", "fein", "bald", "zu", "un\u00b7sern", "Ch\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}