{"textgrid.poem.42586": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "10. Neujahrswunsch", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer redlich h\u00e4lt zu seinem Volke,", "tokens": ["Wer", "red\u00b7lich", "h\u00e4lt", "zu", "sei\u00b7nem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der w\u00fcnsch ihm ein gesegnet Jahr!", "tokens": ["Der", "w\u00fcnsch", "ihm", "ein", "ge\u00b7seg\u00b7net", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Mi\u00dfwachs, Frost und Hagelwolke", "tokens": ["Vor", "Mi\u00df\u00b7wachs", ",", "Frost", "und", "Ha\u00b7gel\u00b7wol\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beh\u00fct uns aller Engel Schar!", "tokens": ["Be\u00b7h\u00fct", "uns", "al\u00b7ler", "En\u00b7gel", "Schar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit dem bang ersehnten Korne,", "tokens": ["Und", "mit", "dem", "bang", "er\u00b7sehn\u00b7ten", "Kor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit dem lang entbehrten Wein", "tokens": ["Und", "mit", "dem", "lang", "ent\u00b7behr\u00b7ten", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bring uns dies Jahr in seinem Horne", "tokens": ["Bring", "uns", "dies", "Jahr", "in", "sei\u00b7nem", "Hor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das alte, gute Recht herein!", "tokens": ["Das", "al\u00b7te", ",", "gu\u00b7te", "Recht", "her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man kann in W\u00fcnschen sich vergessen,", "tokens": ["Man", "kann", "in", "W\u00fcn\u00b7schen", "sich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man w\u00fcnschet leicht zum \u00dcberflu\u00df,", "tokens": ["Man", "w\u00fcn\u00b7schet", "leicht", "zum", "\u00dc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir aber w\u00fcnschen nicht vermessen,", "tokens": ["Wir", "a\u00b7ber", "w\u00fcn\u00b7schen", "nicht", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir w\u00fcnschen, was man w\u00fcnschen ", "tokens": ["Wir", "w\u00fcn\u00b7schen", ",", "was", "man", "w\u00fcn\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn soll der Mensch im Leibe leben,", "tokens": ["Denn", "soll", "der", "Mensch", "im", "Lei\u00b7be", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So brauchet er sein t\u00e4glich Brot,", "tokens": ["So", "brau\u00b7chet", "er", "sein", "t\u00e4g\u00b7lich", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und soll er sich zum Geist erheben,", "tokens": ["Und", "soll", "er", "sich", "zum", "Geist", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So ist ihm seine Freiheit not.", "tokens": ["So", "ist", "ihm", "sei\u00b7ne", "Frei\u00b7heit", "not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}