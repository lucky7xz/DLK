{"textgrid.poem.38952": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[verk\u00fcnden will ich wundervolle Wunder]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verk\u00fcnden will ich wundervolle Wunder,", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "will", "ich", "wun\u00b7der\u00b7vol\u00b7le", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer Ohren hat zu h\u00f6ren, der mag h\u00f6ren!", "tokens": ["Wer", "Oh\u00b7ren", "hat", "zu", "h\u00f6\u00b7ren", ",", "der", "mag", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$,", "PRELS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nichts zu entweihn mu\u00df er zuv\u00f6rderst schw\u00f6ren,", "tokens": ["Nichts", "zu", "ent\u00b7weihn", "mu\u00df", "er", "zu\u00b7v\u00f6r\u00b7derst", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann wird ihm alles klarer und auch runder.", "tokens": ["Dann", "wird", "ihm", "al\u00b7les", "kla\u00b7rer", "und", "auch", "run\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJA", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von neuem brennt der alte Liebeszunder", "tokens": ["Von", "neu\u00b7em", "brennt", "der", "al\u00b7te", "Lie\u00b7bes\u00b7zun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und droht das ganze Welt- All zu verzehren,", "tokens": ["Und", "droht", "das", "gan\u00b7ze", "Welt", "All", "zu", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "TRUNC", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Rumpel-Kammer mag sich bald verkehren", "tokens": ["Die", "Rum\u00b7pel\u00b7Kam\u00b7mer", "mag", "sich", "bald", "ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit allen Sch\u00e4tzen in gar nicht'gen Plunder.", "tokens": ["Mit", "al\u00b7len", "Sch\u00e4t\u00b7zen", "in", "gar", "nicht'\u00b7gen", "Plun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So lange Worte noch Gedanken tragen,", "tokens": ["So", "lan\u00b7ge", "Wor\u00b7te", "noch", "Ge\u00b7dan\u00b7ken", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird man an Worten was zu denken haben,", "tokens": ["Wird", "man", "an", "Wor\u00b7ten", "was", "zu", "den\u00b7ken", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "PIS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wie ich auch die Augen wisch und wasche,", "tokens": ["Doch", "wie", "ich", "auch", "die", "Au\u00b7gen", "wisch", "und", "wa\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wei\u00df ich doch, mein Seel, gar nichts zu sagen;", "tokens": ["So", "wei\u00df", "ich", "doch", ",", "mein", "Seel", ",", "gar", "nichts", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, Freunde, da, da liegt der Hund begraben,", "tokens": ["Ja", ",", "Freun\u00b7de", ",", "da", ",", "da", "liegt", "der", "Hund", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "KOUS", "$,", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geht Wandrer hin und weint auf seine Asche.", "tokens": ["Geht", "Wand\u00b7rer", "hin", "und", "weint", "auf", "sei\u00b7ne", "A\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}