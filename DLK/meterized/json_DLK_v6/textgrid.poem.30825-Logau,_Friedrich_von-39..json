{"textgrid.poem.30825": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Porus suff f\u00fcr gute Freunde mancherley Gesundheit ein,", "tokens": ["Po\u00b7rus", "suff", "f\u00fcr", "gu\u00b7te", "Freun\u00b7de", "man\u00b7cher\u00b7ley", "Ge\u00b7sund\u00b7heit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Bald an Biere, bald an Weine, bald an starckem Brantewein;", "tokens": ["Bald", "an", "Bie\u00b7re", ",", "bald", "an", "Wei\u00b7ne", ",", "bald", "an", "star\u00b7ckem", "Bran\u00b7te\u00b7wein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Als er seine nun verloren, fiel er in die tieffsten Sorgen,", "tokens": ["Als", "er", "sei\u00b7ne", "nun", "ver\u00b7lo\u00b7ren", ",", "fiel", "er", "in", "die", "tieffs\u00b7ten", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADV", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Keiner wolt ihm keine schencken, noch verkauffen, noch auch borgen.", "tokens": ["Kei\u00b7ner", "wolt", "ihm", "kei\u00b7ne", "schen\u00b7cken", ",", "noch", "ver\u00b7kauf\u00b7fen", ",", "noch", "auch", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PIAT", "VVINF", "$,", "ADV", "VVINF", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}