{"textgrid.poem.24067": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Welt ist reich, wie das Auge eines sch\u00f6nen M\u00e4dchens.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist reich, wie das Auge eines sch\u00f6nen M\u00e4dchens.", "tokens": ["Die", "Welt", "ist", "reich", ",", "wie", "das", "Au\u00b7ge", "ei\u00b7nes", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chens", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Tiefes Dunkel ist drin voll s\u00fc\u00dfer und schauriger R\u00e4tsel,", "tokens": ["Tie\u00b7fes", "Dun\u00b7kel", "ist", "drin", "voll", "s\u00fc\u00b7\u00dfer", "und", "schau\u00b7ri\u00b7ger", "R\u00e4t\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADJD", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-+-++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und die Seligkeit ist in ihr, die glitzernd", "tokens": ["Und", "die", "Se\u00b7lig\u00b7keit", "ist", "in", "ihr", ",", "die", "glit\u00b7zernd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPER", "$,", "PRELS", "ADJD"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ueber die Oberfl\u00e4che des feuchten Augapfels huscht.", "tokens": ["Ue\u00b7ber", "die", "O\u00b7berf\u00b7l\u00e4\u00b7che", "des", "feuch\u00b7ten", "Aug\u00b7ap\u00b7fels", "huscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Und das Herz dieser Welt voller Keime und hei\u00dfer Fluten \u2013", "tokens": ["Und", "das", "Herz", "die\u00b7ser", "Welt", "vol\u00b7ler", "Kei\u00b7me", "und", "hei\u00b7\u00dfer", "Flu\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wie das Herz eines verliebten M\u00e4dchens ist es, das unbewu\u00dft", "tokens": ["Wie", "das", "Herz", "ei\u00b7nes", "ver\u00b7lieb\u00b7ten", "M\u00e4d\u00b7chens", "ist", "es", ",", "das", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD"], "meter": "--+---+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sich nach Umarmung sehnt und schmerzlich seligem Geb\u00e4ren.", "tokens": ["Sich", "nach", "Um\u00b7ar\u00b7mung", "sehnt", "und", "schmerz\u00b7lich", "se\u00b7li\u00b7gem", "Ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Feuerstrahlender Gott, der du die Wolkenvorh\u00e4nge zerrei\u00dfest,", "tokens": ["Feu\u00b7er\u00b7strah\u00b7len\u00b7der", "Gott", ",", "der", "du", "die", "Wol\u00b7ken\u00b7vor\u00b7h\u00e4n\u00b7ge", "zer\u00b7rei\u00b7\u00dfest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-++--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Die Erde in ihrer Nacktheit zu schauen,", "tokens": ["Die", "Er\u00b7de", "in", "ih\u00b7rer", "Nackt\u00b7heit", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hei\u00dfblickender Mann, Held Helios, gie\u00dfe", "tokens": ["Hei\u00df\u00b7bli\u00b7cken\u00b7der", "Mann", ",", "Held", "He\u00b7li\u00b7os", ",", "gie\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "NE", "$,", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deine Lebensstr\u00f6me in den hei\u00df wartenden Schoo\u00df der Erde!", "tokens": ["Dei\u00b7ne", "Le\u00b7bens\u00b7str\u00f6\u00b7me", "in", "den", "hei\u00df", "war\u00b7ten\u00b7den", "Schoo\u00df", "der", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+---+-+-", "measure": "unknown.measure.septa"}}}}}