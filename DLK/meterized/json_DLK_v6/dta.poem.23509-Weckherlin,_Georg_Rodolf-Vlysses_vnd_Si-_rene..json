{"dta.poem.23509": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solcher flei\u00df/ vnruh/ m\u00fch vnd streit", "tokens": ["Sol\u00b7cher", "flei\u00df", "/", "vn\u00b7ruh", "/", "m\u00fch", "vnd", "streit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "NN", "$(", "ADJD", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Seind oft nohtwendig bey den St\u00e4nden/", "tokens": ["Seind", "oft", "noht\u00b7wen\u00b7dig", "bey", "den", "St\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die die Weiseste k\u00f6pf allzeit", "tokens": ["Die", "die", "Wei\u00b7ses\u00b7te", "k\u00f6pf", "all\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der welt zu den besten anwenden;", "tokens": ["Der", "welt", "zu", "den", "bes\u00b7ten", "an\u00b7wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Indem Sie schand/ gewalt vnd list", "tokens": ["In\u00b7dem", "Sie", "schand", "/", "ge\u00b7walt", "vnd", "list"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Strafen/ vnd das Recht recht verwalten;", "tokens": ["Stra\u00b7fen", "/", "vnd", "das", "Recht", "recht", "ver\u00b7wal\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Dan der Krieg selbs oft besser ist", "tokens": ["Dan", "der", "Krieg", "selbs", "oft", "bes\u00b7ser", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "ADV", "ADJD", "VAFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Dan mit schand vnd \u00fcbel frid-halten.", "tokens": ["Dan", "mit", "schand", "vnd", "\u00fc\u00b7bel", "fri\u00b7dhal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "KON", "ADJD", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}