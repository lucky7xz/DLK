{"textgrid.poem.48343": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zum K\u00f6lner Domfest", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ersehnter Tag! Inmitten lichten Glanzes", "tokens": ["Er\u00b7sehn\u00b7ter", "Tag", "!", "In\u00b7mit\u00b7ten", "lich\u00b7ten", "Glan\u00b7zes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhebt sich Pfeilerwald und Schiff und Chor,", "tokens": ["Er\u00b7hebt", "sich", "Pfei\u00b7ler\u00b7wald", "und", "Schiff", "und", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus der Umgrenzung eines Zinnenkranzes", "tokens": ["Aus", "der", "Um\u00b7gren\u00b7zung", "ei\u00b7nes", "Zin\u00b7nen\u00b7kran\u00b7zes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins Unbegrenzte steigt der Knauf empor;", "tokens": ["Ins", "Un\u00b7be\u00b7grenz\u00b7te", "steigt", "der", "Knauf", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Teil- und St\u00fcckwerk endlich ward ein Ganzes,", "tokens": ["Aus", "Teil", "und", "St\u00fcck\u00b7werk", "end\u00b7lich", "ward", "ein", "Gan\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Furcht erlag, und Zweifelsucht verlor,", "tokens": ["Und", "Furcht", "er\u00b7lag", ",", "und", "Zwei\u00b7fel\u00b7sucht", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mit den T\u00fcrmen schwingt sich auf nach oben", "tokens": ["Und", "mit", "den", "T\u00fcr\u00b7men", "schwingt", "sich", "auf", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "APPR", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Lobgesang: La\u00dft uns den Herren loben!", "tokens": ["Ein", "Lob\u00b7ge\u00b7sang", ":", "La\u00dft", "uns", "den", "Her\u00b7ren", "lo\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wer ihn h\u00f6rt, aufjubelnder erscholl er", "tokens": ["Und", "wer", "ihn", "h\u00f6rt", ",", "auf\u00b7ju\u00b7beln\u00b7der", "er\u00b7scholl", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In keiner Stund', an keiner Stelle wohl,", "tokens": ["In", "kei\u00b7ner", "Stund'", ",", "an", "kei\u00b7ner", "Stel\u00b7le", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alle Pulse schlagen freud'ger, voller:", "tokens": ["Und", "al\u00b7le", "Pul\u00b7se", "schla\u00b7gen", "freu\u00b7d'\u00b7ger", ",", "vol\u00b7ler", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "$,", "ADJA", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein Ideal, es ward uns zum Idol;", "tokens": ["Ein", "I\u00b7deal", ",", "es", "ward", "uns", "zum", "I\u00b7dol", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und dieser Dom ist dessen uns Symbol,", "tokens": ["Und", "die\u00b7ser", "Dom", "ist", "des\u00b7sen", "uns", "Sym\u00b7bol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PDS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wie nach Ma\u00df und Sch\u00f6nheit ohnegleichen,", "tokens": ["Und", "wie", "nach", "Ma\u00df", "und", "Sch\u00f6n\u00b7heit", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ist er zugleich uns unsrer Einheit Zeichen.", "tokens": ["Ist", "er", "zu\u00b7gleich", "uns", "uns\u00b7rer", "Ein\u00b7heit", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Einheits-Zeichen! Ach, und ", "tokens": ["Ein", "Ein\u00b7heits\u00b7Zei\u00b7chen", "!", "Ach", ",", "und"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "ITJ", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uneinheitlich des Volkes Herz und Sinn \u2013", "tokens": ["Un\u00b7ein\u00b7heit\u00b7lich", "des", "Vol\u00b7kes", "Herz", "und", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ersehnter Tag, in deines Mantels Falten", "tokens": ["Er\u00b7sehn\u00b7ter", "Tag", ",", "in", "dei\u00b7nes", "Man\u00b7tels", "Fal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nimm, eh' du scheidest, unsren Zwiespalt hin!", "tokens": ["Nimm", ",", "eh'", "du", "schei\u00b7dest", ",", "un\u00b7sren", "Zwies\u00b7palt", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df ", "tokens": ["La\u00df"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und klingst du, hohe Kaiserglocke, heute:", "tokens": ["Und", "klingst", "du", ",", "ho\u00b7he", "Kai\u00b7ser\u00b7glo\u00b7cke", ",", "heu\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vers\u00f6hnung, Friede sei dein erst Gel\u00e4ute!", "tokens": ["Ver\u00b7s\u00f6h\u00b7nung", ",", "Frie\u00b7de", "sei", "dein", "erst", "Ge\u00b7l\u00e4u\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "PPOSAT", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}