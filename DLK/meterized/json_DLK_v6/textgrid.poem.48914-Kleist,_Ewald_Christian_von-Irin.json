{"textgrid.poem.48914": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Irin", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem sch\u00f6nen Abend fuhr", "tokens": ["An", "ei\u00b7nem", "sch\u00f6\u00b7nen", "A\u00b7bend", "fuhr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Irin mit seinem Sohn, im Kahn", "tokens": ["I\u00b7rin", "mit", "sei\u00b7nem", "Sohn", ",", "im", "Kahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufs Meer, um Reusen in den Schilf", "tokens": ["Aufs", "Meer", ",", "um", "Reu\u00b7sen", "in", "den", "Schilf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUI", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu legen, der ringsum den Strand", "tokens": ["Zu", "le\u00b7gen", ",", "der", "ring\u00b7sum", "den", "Strand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Von nahen Eilanden umgab.", "tokens": ["Von", "na\u00b7hen", "Ei\u00b7lan\u00b7den", "um\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die Sonne tauchte sich bereits", "tokens": ["Die", "Son\u00b7ne", "tauch\u00b7te", "sich", "be\u00b7reits"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ins Meer, und Fluth und Himmel schien", "tokens": ["Ins", "Meer", ",", "und", "Fluth", "und", "Him\u00b7mel", "schien"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Feur zu gl\u00fchen. O wie sch\u00f6n", "tokens": ["Im", "Feur", "zu", "gl\u00fc\u00b7hen", ".", "O", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$.", "NE", "KOKOM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist jetzt die Gegend! sagt entz\u00fcckt", "tokens": ["Ist", "jetzt", "die", "Ge\u00b7gend", "!", "sagt", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "VVFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Knabe, den Irin gelehrt,", "tokens": ["Der", "Kna\u00b7be", ",", "den", "I\u00b7rin", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf jede Sch\u00f6nheit der Natur", "tokens": ["Auf", "je\u00b7de", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zu merken. Sieh! sagt er, den Schwan,", "tokens": ["Zu", "mer\u00b7ken", ".", "Sieh", "!", "sagt", "er", ",", "den", "Schwan", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "NE", "$.", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Umringt von seiner frohen Brut,", "tokens": ["Um\u00b7ringt", "von", "sei\u00b7ner", "fro\u00b7hen", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sich in den rothen Wiederschein", "tokens": ["Sich", "in", "den", "ro\u00b7then", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Des Himmels tauchen! Sieh! er schift,", "tokens": ["Des", "Him\u00b7mels", "tau\u00b7chen", "!", "Sieh", "!", "er", "schift", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zieht rothe Furchen in die Fluth", "tokens": ["Zieht", "ro\u00b7the", "Fur\u00b7chen", "in", "die", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und spannt des Fittigs Seegel auf. \u2013", "tokens": ["Und", "spannt", "des", "Fit\u00b7tigs", "See\u00b7gel", "auf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wie lieblich flistert dort im Hayn", "tokens": ["Wie", "lieb\u00b7lich", "flis\u00b7tert", "dort", "im", "Hayn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der schlanken Espen furchtsam Laub", "tokens": ["Der", "schlan\u00b7ken", "Es\u00b7pen", "furcht\u00b7sam", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Am Ufer, und wie reizend flie\u00dft", "tokens": ["Am", "U\u00b7fer", ",", "und", "wie", "rei\u00b7zend", "flie\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Saat in gr\u00fcnen Wellen fort,", "tokens": ["Die", "Saat", "in", "gr\u00fc\u00b7nen", "Wel\u00b7len", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und rauscht, vom Winde sanft bewegt. \u2013", "tokens": ["Und", "rauscht", ",", "vom", "Win\u00b7de", "sanft", "be\u00b7wegt", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "O, was f\u00fcr Anmuth haucht anjetzt", "tokens": ["O", ",", "was", "f\u00fcr", "An\u00b7muth", "haucht", "an\u00b7jetzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Gestad und Meer und Himmel aus!", "tokens": ["Ge\u00b7stad", "und", "Meer", "und", "Him\u00b7mel", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wie sch\u00f6n ist alles! und wie froh", "tokens": ["Wie", "sch\u00f6n", "ist", "al\u00b7les", "!", "und", "wie", "froh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PIS", "$.", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und gl\u00fccklich macht uns die Natur! \u2013", "tokens": ["Und", "gl\u00fcck\u00b7lich", "macht", "uns", "die", "Na\u00b7tur", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ja, sagt Irin, sie macht uns froh", "tokens": ["Ja", ",", "sagt", "I\u00b7rin", ",", "sie", "macht", "uns", "froh"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und gl\u00fccklich, und du wirst durch sie", "tokens": ["Und", "gl\u00fcck\u00b7lich", ",", "und", "du", "wirst", "durch", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KON", "PPER", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gl\u00fcckseelig seyn dein Lebelang,", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "seyn", "dein", "Le\u00b7be\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wenn du dabey rechtschaffen bist,", "tokens": ["Wenn", "du", "da\u00b7bey", "recht\u00b7schaf\u00b7fen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wenn wilde Leidenschaften nicht", "tokens": ["Wenn", "wil\u00b7de", "Lei\u00b7den\u00b7schaf\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Von sanfter Sch\u00f6nheit das Gef\u00fchl", "tokens": ["Von", "sanf\u00b7ter", "Sch\u00f6n\u00b7heit", "das", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Verhindern. O Geliebtester!", "tokens": ["Ver\u00b7hin\u00b7dern", ".", "O", "Ge\u00b7lieb\u00b7tes\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.34": {"text": "Ich werde nun in kurzem dich", "tokens": ["Ich", "wer\u00b7de", "nun", "in", "kur\u00b7zem", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Verlassen, und die sch\u00f6ne Welt,", "tokens": ["Ver\u00b7las\u00b7sen", ",", "und", "die", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und noch in sch\u00f6nern Gegenden", "tokens": ["Und", "noch", "in", "sch\u00f6\u00b7nern", "Ge\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.37": {"text": "Den Lohn der Redlichkeit empfahn.", "tokens": ["Den", "Lohn", "der", "Red\u00b7lich\u00b7keit", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "O, bleib der Tugend immer treu,", "tokens": ["O", ",", "bleib", "der", "Tu\u00b7gend", "im\u00b7mer", "treu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und weine mit den Weinenden,", "tokens": ["Und", "wei\u00b7ne", "mit", "den", "Wei\u00b7nen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.40": {"text": "Und gieb von deinem Vorrath gern", "tokens": ["Und", "gieb", "von", "dei\u00b7nem", "Vor\u00b7rath", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.41": {"text": "Den Armen! Hilf so viel du kanst,", "tokens": ["Den", "Ar\u00b7men", "!", "Hilf", "so", "viel", "du", "kanst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NE", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Zum Wohl der Welt! Sey arbeitsam,", "tokens": ["Zum", "Wohl", "der", "Welt", "!", "Sey", "ar\u00b7beit\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "VAIMP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Erheb zum Herren der Natur,", "tokens": ["Er\u00b7heb", "zum", "Her\u00b7ren", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Dem Wind und Meer gehorsam ist,", "tokens": ["Dem", "Wind", "und", "Meer", "ge\u00b7hor\u00b7sam", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Der alles lenkt zum Wohl der Welt,", "tokens": ["Der", "al\u00b7les", "lenkt", "zum", "Wohl", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Den Geist! W\u00e4hl lieber Schand und Tod,", "tokens": ["Den", "Geist", "!", "W\u00e4hl", "lie\u00b7ber", "Schand", "und", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Eh du in Bosheit willigest.", "tokens": ["Eh", "du", "in", "Bos\u00b7heit", "wil\u00b7li\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.48": {"text": "Ehr, \u00dcberflu\u00df und Pracht ist Tand;", "tokens": ["Ehr", ",", "\u00dc\u00b7berf\u00b7lu\u00df", "und", "Pracht", "ist", "Tand", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ein ruhig Herz ist unser Theil. \u2013", "tokens": ["Ein", "ru\u00b7hig", "Herz", "ist", "un\u00b7ser", "Theil", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Durch diese Denkungsart, mein Sohn!", "tokens": ["Durch", "die\u00b7se", "Den\u00b7kungs\u00b7art", ",", "mein", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.51": {"text": "Ist unter lauter Freuden mir", "tokens": ["Ist", "un\u00b7ter", "lau\u00b7ter", "Freu\u00b7den", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Das Haar verbleichet. Und wiewohl", "tokens": ["Das", "Haar", "ver\u00b7blei\u00b7chet", ".", "Und", "wie\u00b7wohl"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Ich achtzig mal bereits den Wald", "tokens": ["Ich", "acht\u00b7zig", "mal", "be\u00b7reits", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "CARD", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Um unsre H\u00fctte gr\u00fcnen sah;", "tokens": ["Um", "uns\u00b7re", "H\u00fct\u00b7te", "gr\u00fc\u00b7nen", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "So ist mein langes Leben doch", "tokens": ["So", "ist", "mein", "lan\u00b7ges", "Le\u00b7ben", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Gleich einem heitern Fr\u00fchlingstag", "tokens": ["Gleich", "ei\u00b7nem", "hei\u00b7tern", "Fr\u00fch\u00b7lings\