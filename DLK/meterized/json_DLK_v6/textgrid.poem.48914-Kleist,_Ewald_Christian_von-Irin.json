word", "punct"], "pos": ["PPER", "PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heilger Schauer \u00fcberfiel", "tokens": ["Ein", "heil\u00b7ger", "Schau\u00b7er", "\u00fc\u00b7berf\u00b7iel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn, wenn ihm seines Vaters Bild", "tokens": ["Ihn", ",", "wenn", "ihm", "sei\u00b7nes", "Va\u00b7ters", "Bild"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vors Antlitz trat. Er folgete", "tokens": ["Vors", "Ant\u00b7litz", "trat", ".", "Er", "fol\u00b7ge\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stets de\u00dfen Lehren. Seegen kam", "tokens": ["Stets", "de\u00b7\u00dfen", "Leh\u00b7ren", ".", "See\u00b7gen", "kam"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf ihn. Sein langes Leben d\u00fcnkt", "tokens": ["Auf", "ihn", ".", "Sein", "lan\u00b7ges", "Le\u00b7ben", "d\u00fcnkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihm auch ein Fr\u00fchlings-Tag zu seyn.", "tokens": ["Ihm", "auch", "ein", "Fr\u00fch\u00b7lings\u00b7Tag", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}