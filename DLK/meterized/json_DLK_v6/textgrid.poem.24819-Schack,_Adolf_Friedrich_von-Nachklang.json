{"textgrid.poem.24819": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Nachklang", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie ward ich, dir zu lauschen, m\u00fcde,", "tokens": ["Nie", "ward", "ich", ",", "dir", "zu", "lau\u00b7schen", ",", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchlte, wie in jedem Klang", "tokens": ["Ich", "f\u00fchl\u00b7te", ",", "wie", "in", "je\u00b7dem", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deinem Mund ein heil'ger Friede", "tokens": ["Von", "dei\u00b7nem", "Mund", "ein", "heil'\u00b7ger", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner Seele Tiefen drang.", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "Tie\u00b7fen", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur deine Stimme unter allen", "tokens": ["Nur", "dei\u00b7ne", "Stim\u00b7me", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erscholl so rein, als einte sie,", "tokens": ["Er\u00b7scholl", "so", "rein", ",", "als", "ein\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was andre nur gebrochen lallen,", "tokens": ["Was", "and\u00b7re", "nur", "ge\u00b7bro\u00b7chen", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur wundvollen Harmonie", "tokens": ["Zur", "wund\u00b7vol\u00b7len", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun sie verstummt zu ew'gem Schweigen,", "tokens": ["Nun", "sie", "ver\u00b7stummt", "zu", "ew'\u00b7gem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "T\u00f6nt mir wie Mi\u00dflaut jedes Wort,", "tokens": ["T\u00f6nt", "mir", "wie", "Mi\u00df\u00b7laut", "je\u00b7des", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcst und w\u00fcster braust der Reigen", "tokens": ["Und", "w\u00fcst", "und", "w\u00fcs\u00b7ter", "braust", "der", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des wilden Lebens um mich fort.", "tokens": ["Des", "wil\u00b7den", "Le\u00b7bens", "um", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur selten hallt im Weltgedr\u00e4nge", "tokens": ["Nur", "sel\u00b7ten", "hallt", "im", "Welt\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch all der Stimmen wirren Chor", "tokens": ["Durch", "all", "der", "Stim\u00b7men", "wir\u00b7ren", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Echo noch, wie Harfenkl\u00e4nge", "tokens": ["Ihr", "E\u00b7cho", "noch", ",", "wie", "Har\u00b7fen\u00b7kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Winde sterbend, an mein Ohr.", "tokens": ["Im", "Win\u00b7de", "ster\u00b7bend", ",", "an", "mein", "Ohr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}