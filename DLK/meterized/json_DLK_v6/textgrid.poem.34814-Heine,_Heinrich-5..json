{"textgrid.poem.34814": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe solche wei\u00dfe Glieder,", "tokens": ["Ich", "lie\u00b7be", "sol\u00b7che", "wei\u00b7\u00dfe", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der zarten Seele schlanke H\u00fclle,", "tokens": ["Der", "zar\u00b7ten", "See\u00b7le", "schlan\u00b7ke", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wildgro\u00dfe Augen und die Stirne", "tokens": ["Wild\u00b7gro\u00b7\u00dfe", "Au\u00b7gen", "und", "die", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umwogt von schwarzer Lockenf\u00fclle!", "tokens": ["Um\u00b7wogt", "von", "schwar\u00b7zer", "Lo\u00b7cken\u00b7f\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist so recht die rechte Sorte,", "tokens": ["Du", "bist", "so", "recht", "die", "rech\u00b7te", "Sor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich gesucht in allen Landen;", "tokens": ["Die", "ich", "ge\u00b7sucht", "in", "al\u00b7len", "Lan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch meinen Wert hat euresgleichen", "tokens": ["Auch", "mei\u00b7nen", "Wert", "hat", "eu\u00b7res\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So recht zu w\u00fcrdigen verstanden.", "tokens": ["So", "recht", "zu", "w\u00fcr\u00b7di\u00b7gen", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast an mir den Mann gefunden,", "tokens": ["Du", "hast", "an", "mir", "den", "Mann", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du ihn brauchst. Du wirst mich reichlich", "tokens": ["Wie", "du", "ihn", "brauchst", ".", "Du", "wirst", "mich", "reich\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begl\u00fccken mit Gef\u00fchl und K\u00fcssen,", "tokens": ["Be\u00b7gl\u00fc\u00b7cken", "mit", "Ge\u00b7f\u00fchl", "und", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann verraten, wie gebr\u00e4uchlich.", "tokens": ["Und", "dann", "ver\u00b7ra\u00b7ten", ",", "wie", "ge\u00b7br\u00e4uch\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}