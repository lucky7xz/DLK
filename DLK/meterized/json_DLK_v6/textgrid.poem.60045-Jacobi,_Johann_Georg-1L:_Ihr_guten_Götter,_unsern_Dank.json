{"textgrid.poem.60045": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr guten G\u00f6tter, unsern Dank", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr guten G\u00f6tter, unsern Dank", "tokens": ["Ihr", "gu\u00b7ten", "G\u00f6t\u00b7ter", ",", "un\u00b7sern", "Dank"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr eurer Weisheit ernste Lehren,", "tokens": ["F\u00fcr", "eu\u00b7rer", "Weis\u00b7heit", "erns\u00b7te", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die wir zum Trost im sp\u00e4ten Alter h\u00f6ren!", "tokens": ["Die", "wir", "zum", "Trost", "im", "sp\u00e4\u00b7ten", "Al\u00b7ter", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr G\u00f6tter! unsern Lobgesang", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "un\u00b7sern", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr jeden s\u00fc\u00dfen Wahn der frohen Jugendzeiten,", "tokens": ["F\u00fcr", "je\u00b7den", "s\u00fc\u00b7\u00dfen", "Wahn", "der", "fro\u00b7hen", "Ju\u00b7gend\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo sich in tausend Lieblichkeiten", "tokens": ["Wo", "sich", "in", "tau\u00b7send", "Lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Geist verirrt, und Alles K\u00fcsse giebt,", "tokens": ["Der", "Geist", "ver\u00b7irrt", ",", "und", "Al\u00b7les", "K\u00fcs\u00b7se", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und jedes Sonnenst\u00e4ubchen liebt!", "tokens": ["Und", "je\u00b7des", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}