{"dta.poem.10900": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob in diesem Kupferst\u00fcck ich zuerst das Pflanzen-Reich,", "tokens": ["Ob", "in", "die\u00b7sem", "Kup\u00b7fer\u00b7st\u00fcck", "ich", "zu\u00b7erst", "das", "Pflan\u00b7zen\u00b7Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Oder erst das Thier-Reich sehn, oder alle zwey zugleich", "tokens": ["O\u00b7der", "erst", "das", "Thier\u00b7Reich", "sehn", ",", "o\u00b7der", "al\u00b7le", "zwey", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,", "KON", "PIAT", "CARD", "ADV"], "meter": "+-+-++-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Schauen und bewundern wolle; zweifelt mein verwirrter", "tokens": ["Schau\u00b7en", "und", "be\u00b7wun\u00b7dern", "wol\u00b7le", ";", "zwei\u00b7felt", "mein", "ver\u00b7wirr\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$.", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Sucht mein Aug am k\u00fchlen Bach, unter Schatten-reichen B\u00fc-", "tokens": ["Sucht", "mein", "Aug", "am", "k\u00fch\u00b7len", "Bach", ",", "un\u00b7ter", "Schat\u00b7ten\u00b7rei\u00b7chen", "B\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Unter Bl\u00e4tter-reichen B\u00e4umen, in der Fern, sich zu erfrischen:", "tokens": ["Un\u00b7ter", "Bl\u00e4t\u00b7ter\u00b7rei\u00b7chen", "B\u00e4u\u00b7men", ",", "in", "der", "Fern", ",", "sich", "zu", "er\u00b7fri\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "ART", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Reisset ihn der edle Hirsch, mit Gewalt, auf sich zur\u00fcck,", "tokens": ["Reis\u00b7set", "ihn", "der", "ed\u00b7le", "Hirsch", ",", "mit", "Ge\u00b7walt", ",", "auf", "sich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "NN", "$,", "APPR", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Und indem ich seine Stellung, Anstand, pr\u00e4chtige Gestalt,", "tokens": ["Und", "in\u00b7dem", "ich", "sei\u00b7ne", "Stel\u00b7lung", ",", "An\u00b7stand", ",", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Sein auf mich gerichtet Aug und den festen Tritt beschau,", "tokens": ["Sein", "auf", "mich", "ge\u00b7rich\u00b7tet", "Aug", "und", "den", "fes\u00b7ten", "Tritt", "be\u00b7schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PPER", "VVPP", "NN", "KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Schweift der Blick aufs neue weg, in die von der klaren Au", "tokens": ["Schweift", "der", "Blick", "aufs", "neu\u00b7e", "weg", ",", "in", "die", "von", "der", "kla\u00b7ren", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "PTKVZ", "$,", "APPR", "ART", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Angenehm durchflo\u00dfne Gegend, wo sich, von bebl\u00fcmten H\u00fcgeln,", "tokens": ["An\u00b7ge\u00b7nehm", "durch\u00b7flo\u00df\u00b7ne", "Ge\u00b7gend", ",", "wo", "sich", ",", "von", "be\u00b7bl\u00fcm\u00b7ten", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PWAV", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Weisse St\u00e4mme, gr\u00fcne Bl\u00e4tter, in dem reinen Wasser, spiegeln.", "tokens": ["Weis\u00b7se", "St\u00e4m\u00b7me", ",", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter", ",", "in", "dem", "rei\u00b7nen", "Was\u00b7ser", ",", "spie\u00b7geln", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Doch des Hirsches sch\u00f6ne Zweig, und sein sich bewegend Ohr,", "tokens": ["Doch", "des", "Hir\u00b7sches", "sch\u00f6\u00b7ne", "Zweig", ",", "und", "sein", "sich", "be\u00b7we\u00b7gend", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,", "KON", "PPOSAT", "PRF", "ADJD", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Reissen mich aus gr\u00fcner Zweige ferner Tief aufs neu hervor,", "tokens": ["Reis\u00b7sen", "mich", "aus", "gr\u00fc\u00b7ner", "Zwei\u00b7ge", "fer\u00b7ner", "Tief", "aufs", "neu", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "ADV", "ADJD", "APPRART", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Beyder Urbild zeiget mir einen Gott, der K\u00f6rper macht;", "tokens": ["Bey\u00b7der", "Ur\u00b7bild", "zei\u00b7get", "mir", "ei\u00b7nen", "Gott", ",", "der", "K\u00f6r\u00b7per", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Die Copie beweist, da\u00df Gott auch den Geist hervor gebracht.", "tokens": ["Die", "Co\u00b7pie", "be\u00b7weist", ",", "da\u00df", "Gott", "auch", "den", "Geist", "her\u00b7vor", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "NN", "ADV", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}