{"textgrid.poem.60448": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der arme Jude", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Armer Jude, der du wandeln", "tokens": ["Ar\u00b7mer", "Ju\u00b7de", ",", "der", "du", "wan\u00b7deln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft von Dorf zu Dorf hausierend,", "tokens": ["Mu\u00dft", "von", "Dorf", "zu", "Dorf", "hau\u00b7sie\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlecht gen\u00e4hrt und bitter frierend,", "tokens": ["Schlecht", "ge\u00b7n\u00e4hrt", "und", "bit\u00b7ter", "frie\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allw\u00e4rts rufend: \u00bbNichts zu handeln?\u00ab!", "tokens": ["All\u00b7w\u00e4rts", "ru\u00b7fend", ":", "\u00bb", "Nichts", "zu", "han\u00b7deln", "?", "\u00ab", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVPP", "$.", "$(", "PIS", "PTKZU", "VVINF", "$.", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Holt die Seuche Mann und Frauen,", "tokens": ["Holt", "die", "Seu\u00b7che", "Mann", "und", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehst du nach auf ihrer F\u00e4hrte,", "tokens": ["Ziehst", "du", "nach", "auf", "ih\u00b7rer", "F\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kleider, die sie leerte,", "tokens": ["Und", "die", "Klei\u00b7der", ",", "die", "sie", "leer\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schleppst du fort, dir darf nicht grauen.", "tokens": ["Schleppst", "du", "fort", ",", "dir", "darf", "nicht", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf dem Baume kr\u00e4chzt der Rabe,", "tokens": ["Auf", "dem", "Bau\u00b7me", "kr\u00e4chzt", "der", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hunde zerren dich am Rocke,", "tokens": ["Hun\u00b7de", "zer\u00b7ren", "dich", "am", "Ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneegest\u00f6ber Flock an Flocke,", "tokens": ["Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "Flock", "an", "Flo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flei\u00dfig wanderst du am Stabe.", "tokens": ["Flei\u00b7\u00dfig", "wan\u00b7derst", "du", "am", "Sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Jerusalem, papieren,", "tokens": ["Ein", "Je\u00b7ru\u00b7sa\u00b7lem", ",", "pa\u00b7pie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "$,", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bauen deine Stammgenossen,", "tokens": ["Bau\u00b7en", "dei\u00b7ne", "Stamm\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch f\u00fcr dich ist es verschlossen,", "tokens": ["Doch", "f\u00fcr", "dich", "ist", "es", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wandern mu\u00dft du, darben, frieren.", "tokens": ["Wan\u00b7dern", "mu\u00dft", "du", ",", "dar\u00b7ben", ",", "frie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PAV", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jene habens hoch getrieben,", "tokens": ["Je\u00b7ne", "ha\u00b7bens", "hoch", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du verschacherst alte Kleider;", "tokens": ["Du", "ver\u00b7scha\u00b7cherst", "al\u00b7te", "Klei\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber alle seid ihr leider", "tokens": ["A\u00b7ber", "al\u00b7le", "seid", "ihr", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein geknicktes Volk geblieben.", "tokens": ["Ein", "ge\u00b7knick\u00b7tes", "Volk", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}