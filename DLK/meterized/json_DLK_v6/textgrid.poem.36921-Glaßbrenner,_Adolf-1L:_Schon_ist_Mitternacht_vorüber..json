{"textgrid.poem.36921": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon ist Mitternacht vor\u00fcber.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon ist Mitternacht vor\u00fcber.", "tokens": ["Schon", "ist", "Mit\u00b7ter\u00b7nacht", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen fl\u00f6tet Philomele;", "tokens": ["Drau\u00b7\u00dfen", "fl\u00f6\u00b7tet", "Phi\u00b7lo\u00b7me\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnsche, Hoffnungen, Gedanken", "tokens": ["W\u00fcn\u00b7sche", ",", "Hoff\u00b7nun\u00b7gen", ",", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehen wirr durch meine Seele.", "tokens": ["Zie\u00b7hen", "wirr", "durch", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wogend Herz, gib dich zur Ruhe,", "tokens": ["Wo\u00b7gend", "Herz", ",", "gib", "dich", "zur", "Ru\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df' die Sehnsucht endlich hafen!", "tokens": ["La\u00df'", "die", "Sehn\u00b7sucht", "end\u00b7lich", "ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df' den Steuermann, den Denker,", "tokens": ["La\u00df'", "den", "Steu\u00b7er\u00b7mann", ",", "den", "Den\u00b7ker", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df' den m\u00fcden S\u00e4nger schlafen!", "tokens": ["La\u00df'", "den", "m\u00fc\u00b7den", "S\u00e4n\u00b7ger", "schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber immer wilder wogt es,", "tokens": ["A\u00b7ber", "im\u00b7mer", "wil\u00b7der", "wogt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6her schl\u00e4gt es seine Wellen;", "tokens": ["H\u00f6\u00b7her", "schl\u00e4gt", "es", "sei\u00b7ne", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, am stumpfen, starren Felsen", "tokens": ["Ach", ",", "am", "stump\u00b7fen", ",", "star\u00b7ren", "Fel\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mein leichtes Schiff zerschellen!", "tokens": ["Wird", "mein", "leich\u00b7tes", "Schiff", "zer\u00b7schel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rettung! Rettung! Weh, verloren!", "tokens": ["Ret\u00b7tung", "!", "Ret\u00b7tung", "!", "Weh", ",", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weh, der gro\u00dfe Mast, er bricht!", "tokens": ["Weh", ",", "der", "gro\u00b7\u00dfe", "Mast", ",", "er", "bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Schiffe geh' ich unter,", "tokens": ["Mit", "dem", "Schif\u00b7fe", "geh'", "ich", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilfst du, Gott im Himmel, nicht!", "tokens": ["Hilfst", "du", ",", "Gott", "im", "Him\u00b7mel", ",", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und umher greif' ich verzweifelnd,", "tokens": ["Und", "um\u00b7her", "greif'", "ich", "ver\u00b7zwei\u00b7felnd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "VVPP", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ergreife das Register", "tokens": ["Und", "er\u00b7grei\u00b7fe", "das", "Re\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den neuen Ordensrittern,", "tokens": ["Von", "den", "neu\u00b7en", "Or\u00b7dens\u00b7rit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterzeichnet vom Minister.", "tokens": ["Un\u00b7ter\u00b7zeich\u00b7net", "vom", "Mi\u00b7nis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fort sind pl\u00f6tzlich die Gedanken;", "tokens": ["Fort", "sind", "pl\u00f6tz\u00b7lich", "die", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still und ruhig ist's im Herzen,", "tokens": ["Still", "und", "ru\u00b7hig", "ist's", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich, endlich kann ich schlafen!", "tokens": ["End\u00b7lich", ",", "end\u00b7lich", "kann", "ich", "schla\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so l\u00f6sch' ich denn die Kerzen.", "tokens": ["Und", "so", "l\u00f6sch'", "ich", "denn", "die", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}