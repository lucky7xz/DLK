{"textgrid.poem.61302": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eins ist noch schlimmer, als den Damm durchstechen", "tokens": ["Eins", "ist", "noch", "schlim\u00b7mer", ",", "als", "den", "Damm", "durch\u00b7ste\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und pl\u00f6tzlich dann die Sturmflut meistern wollen:", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "dann", "die", "Sturm\u00b7flut", "meis\u00b7tern", "wol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begeistrung wecken und, wenn angeschwollen", "tokens": ["Be\u00b7geis\u00b7trung", "we\u00b7cken", "und", ",", "wenn", "an\u00b7ge\u00b7schwol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "KON", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Volk sie herbraust, ihren Strom zerbrechen.", "tokens": ["Im", "Volk", "sie", "her\u00b7braust", ",", "ih\u00b7ren", "Strom", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn einmal aufgewogt aus tausend B\u00e4chen,", "tokens": ["Denn", "ein\u00b7mal", "auf\u00b7ge\u00b7wogt", "aus", "tau\u00b7send", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlangt sie stolz und siegreich hinzurollen;", "tokens": ["Ver\u00b7langt", "sie", "stolz", "und", "sieg\u00b7reich", "hin\u00b7zu\u00b7rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hemmst sie wohl, o F\u00fcrst, doch kehrt mit Grollen", "tokens": ["Du", "hemmst", "sie", "wohl", ",", "o", "F\u00fcrst", ",", "doch", "kehrt", "mit", "Grol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Schwall sich wider dich und deine Schw\u00e4chen.", "tokens": ["Ihr", "Schwall", "sich", "wi\u00b7der", "dich", "und", "dei\u00b7ne", "Schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Je sichrer sie dein Schifflein trug zur Stelle,", "tokens": ["Je", "sich\u00b7rer", "sie", "dein", "Schif\u00b7flein", "trug", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du sie nutztest, desto grimmer trachtet", "tokens": ["Wenn", "du", "sie", "nutz\u00b7test", ",", "des\u00b7to", "grim\u00b7mer", "trach\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich zu vernichten die ", "tokens": ["Dich", "zu", "ver\u00b7nich\u00b7ten", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVFIN", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Schon manches Volk hat sich dem Ruhm geschlachtet,", "tokens": ["Schon", "man\u00b7ches", "Volk", "hat", "sich", "dem", "Ruhm", "ge\u00b7schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch seines heiligsten Gef\u00fchles Quelle", "tokens": ["Doch", "sei\u00b7nes", "hei\u00b7ligs\u00b7ten", "Ge\u00b7f\u00fch\u00b7les", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "L\u00e4\u00dft keins vergeuden, das sich selbst noch achtet.", "tokens": ["L\u00e4\u00dft", "keins", "ver\u00b7geu\u00b7den", ",", "das", "sich", "selbst", "noch", "ach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "$,", "PRELS", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}