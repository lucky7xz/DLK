{"textgrid.poem.66464": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu deinen F\u00fc\u00dfen will ich ruhn,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu deinen F\u00fc\u00dfen will ich ruhn,", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "will", "ich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dir in's Auge schau'n,", "tokens": ["Und", "dir", "in's", "Au\u00b7ge", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die blaue Nacht mag leise nun", "tokens": ["Die", "blau\u00b7e", "Nacht", "mag", "lei\u00b7se", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf uns herniederthau'n.", "tokens": ["Auf", "uns", "her\u00b7nie\u00b7dert\u00b7hau'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon tauchet aus dem stillen See", "tokens": ["Schon", "tau\u00b7chet", "aus", "dem", "stil\u00b7len", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Mondes Bild empor,", "tokens": ["Des", "Mon\u00b7des", "Bild", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und k\u00fchner streift das scheue Reh", "tokens": ["Und", "k\u00fch\u00b7ner", "streift", "das", "scheu\u00b7e", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Wald und Wiesenmoor.", "tokens": ["Durch", "Wald", "und", "Wie\u00b7sen\u00b7moor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Haupt la\u00df ruhn auf deinem Schoo\u00df,", "tokens": ["Mein", "Haupt", "la\u00df", "ruhn", "auf", "dei\u00b7nem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ruht es sanft und weich.", "tokens": ["Da", "ruht", "es", "sanft", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ist der Himmel weit und gro\u00df,", "tokens": ["Wie", "ist", "der", "Him\u00b7mel", "weit", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ist die Erde reich!", "tokens": ["Wie", "ist", "die", "Er\u00b7de", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der sch\u00f6nste Stern in blauer Nacht,", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Stern", "in", "blau\u00b7er", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sch\u00f6nste Stern bist du,", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Stern", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In deines Lichtes sanfter Pracht", "tokens": ["In", "dei\u00b7nes", "Lich\u00b7tes", "sanf\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O g\u00f6nne mir die Ruh!", "tokens": ["O", "g\u00f6n\u00b7ne", "mir", "die", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "An deinem Herzen la\u00df mich ruhn", "tokens": ["An", "dei\u00b7nem", "Her\u00b7zen", "la\u00df", "mich", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur kurze, sel'ge Zeit!", "tokens": ["Nur", "kur\u00b7ze", ",", "sel'\u00b7ge", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Lauscher k\u00fcndet unser Thun,", "tokens": ["Kein", "Lau\u00b7scher", "k\u00fcn\u00b7det", "un\u00b7ser", "Thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt ist traumgefeit.", "tokens": ["Die", "Welt", "ist", "traum\u00b7ge\u00b7feit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, la\u00df mich ruhn an deinem Mund,", "tokens": ["Ach", ",", "la\u00df", "mich", "ruhn", "an", "dei\u00b7nem", "Mund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eh noch die Nacht verglimmt,", "tokens": ["Eh", "noch", "die", "Nacht", "ver\u00b7glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Bis Himmel uns und Erdenrund", "tokens": ["Bis", "Him\u00b7mel", "uns", "und", "Er\u00b7den\u00b7rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Seligkeit verschwimmt!", "tokens": ["In", "Se\u00b7lig\u00b7keit", "ver\u00b7schwimmt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}