{"textgrid.poem.44522": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Gegenwart", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, wer schilt die Jugend euch?", "tokens": ["Ei", ",", "wer", "schilt", "die", "Ju\u00b7gend", "euch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr sind alle Lebensg\u00fcter,", "tokens": ["Ihr", "sind", "al\u00b7le", "Le\u00b7bens\u00b7g\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor der Freuden Zauberreich", "tokens": ["Vor", "der", "Freu\u00b7den", "Zau\u00b7ber\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht sie als des Gartens H\u00fcter.", "tokens": ["Steht", "sie", "als", "des", "Gar\u00b7tens", "H\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie ist stolz und stark und k\u00fchn,", "tokens": ["Sie", "ist", "stolz", "und", "stark", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Reich an Streben und an Taten,", "tokens": ["Reich", "an", "Stre\u00b7ben", "und", "an", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brauchts doch auch erst Fr\u00fchlings Bl\u00fchn,", "tokens": ["Brauchts", "doch", "auch", "erst", "Fr\u00fch\u00b7lings", "Bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh der Sommer reift die Saaten.", "tokens": ["Eh", "der", "Som\u00b7mer", "reift", "die", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber eines wei\u00df sie nicht", "tokens": ["A\u00b7ber", "ei\u00b7nes", "wei\u00df", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wirds, oft get\u00e4uscht, erkennen:", "tokens": ["Und", "wirds", ",", "oft", "ge\u00b7t\u00e4uscht", ",", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ADV", "VVPP", "$,", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df, was heut am lautsten spricht,", "tokens": ["Da\u00df", ",", "was", "heut", "am", "lauts\u00b7ten", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wof\u00fcr alle Herzen brennen,", "tokens": ["Wo\u00b7f\u00fcr", "al\u00b7le", "Her\u00b7zen", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was in jeder Meinung steht", "tokens": ["Was", "in", "je\u00b7der", "Mei\u00b7nung", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als f\u00fcr ewig eingegraben,", "tokens": ["Als", "f\u00fcr", "e\u00b7wig", "ein\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum da\u00df ein Jahrzehnt vergeht,", "tokens": ["Kaum", "da\u00df", "ein", "Jahr\u00b7zehnt", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nur ein Spott noch ist der Knaben.", "tokens": ["Nur", "ein", "Spott", "noch", "ist", "der", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df, wie Mode formt das Kleid,", "tokens": ["Da\u00df", ",", "wie", "Mo\u00b7de", "formt", "das", "Kleid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der Geist tauscht seine Trachten,", "tokens": ["Auch", "der", "Geist", "tauscht", "sei\u00b7ne", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Als ein letzter sei zu achten.", "tokens": ["Als", "ein", "letz\u00b7ter", "sei", "zu", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Darum wirkt mit rascher Tat,", "tokens": ["Da\u00b7rum", "wirkt", "mit", "ra\u00b7scher", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "\u00dcbergebt euch Strom und L\u00fcften,", "tokens": ["\u00dc\u00b7ber\u00b7gebt", "euch", "Strom", "und", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch das Urteil und den Rat", "tokens": ["Doch", "das", "Ur\u00b7teil", "und", "den", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft den Reifern und Gepr\u00fcften.", "tokens": ["La\u00dft", "den", "Rei\u00b7fern", "und", "Ge\u00b7pr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}