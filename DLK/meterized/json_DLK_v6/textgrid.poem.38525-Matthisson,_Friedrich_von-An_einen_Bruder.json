{"textgrid.poem.38525": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, dessen Seele Feuerbegeisterung,", "tokens": ["Du", ",", "des\u00b7sen", "See\u00b7le", "Feu\u00b7er\u00b7be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im k\u00fchnen Taumel, zu den Gestirnen rei\u00dft,", "tokens": ["Im", "k\u00fch\u00b7nen", "Tau\u00b7mel", ",", "zu", "den", "Ge\u00b7stir\u00b7nen", "rei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Singst du, von Eichen rings umschauert,", "tokens": ["Singst", "du", ",", "von", "Ei\u00b7chen", "rings", "um\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fchend von Gott und dem Vaterlande:", "tokens": ["Gl\u00fc\u00b7hend", "von", "Gott", "und", "dem", "Va\u00b7ter\u00b7lan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Gesegnet, Theurer, dreimal gesegnet mir!", "tokens": ["Ge\u00b7seg\u00b7net", ",", "Theu\u00b7rer", ",", "drei\u00b7mal", "ge\u00b7seg\u00b7net", "mir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Ausblik k\u00fcndet flammende Seelenkraft,", "tokens": ["Dein", "Aus\u00b7blik", "k\u00fcn\u00b7det", "flam\u00b7men\u00b7de", "See\u00b7len\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu ringen nach dem Kranz des Lohnes,", "tokens": ["Zu", "rin\u00b7gen", "nach", "dem", "Kranz", "des", "Loh\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welcher am Ziele der Laufbahn schimmert.", "tokens": ["Wel\u00b7cher", "am", "Zie\u00b7le", "der", "Lauf\u00b7bahn", "schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn stets du singst, wozu dir dein Vaterland,", "tokens": ["Wenn", "stets", "du", "singst", ",", "wo\u00b7zu", "dir", "dein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Tugend dir und heilige Freiheitsglut,", "tokens": ["Die", "Tu\u00b7gend", "dir", "und", "hei\u00b7li\u00b7ge", "Frei\u00b7heits\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Eichenhain die Saiten stimmen,", "tokens": ["Im", "Ei\u00b7chen\u00b7hain", "die", "Sai\u00b7ten", "stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo du zuerst deine Harfe pr\u00fcftest:", "tokens": ["Wo", "du", "zu\u00b7erst", "dei\u00b7ne", "Har\u00b7fe", "pr\u00fcf\u00b7test", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "So schlingt sich einst, (o! wehre der Z\u00e4hre nicht,", "tokens": ["So", "schlingt", "sich", "einst", ",", "(", "o", "!", "weh\u00b7re", "der", "Z\u00e4h\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "$,", "$(", "FM", "$.", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Freudenz\u00e4hre, die dir im Auge blinkt!)", "tokens": ["Der", "Freu\u00b7den\u00b7z\u00e4h\u00b7re", ",", "die", "dir", "im", "Au\u00b7ge", "blinkt", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So wahr als du von Herman stammest,", "tokens": ["So", "wahr", "als", "du", "von", "Her\u00b7man", "stam\u00b7mest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um deine Loken der Eichenspr\u00f6sling!", "tokens": ["Um", "dei\u00b7ne", "Lo\u00b7ken", "der", "Ei\u00b7chen\u00b7spr\u00f6s\u00b7ling", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Voll Glut die Seele, walle die hohe Bahn!", "tokens": ["Voll", "Glut", "die", "See\u00b7le", ",", "wal\u00b7le", "die", "ho\u00b7he", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Sieger, wisse! lohnet Unsterblichkeit!", "tokens": ["Den", "Sie\u00b7ger", ",", "wis\u00b7se", "!", "loh\u00b7net", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$.", "VVFIN", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sie, deren ewiglichten Schimmer", "tokens": ["Sie", ",", "de\u00b7ren", "e\u00b7wig\u00b7lich\u00b7ten", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie die Gew\u00f6lke der Zeit verd\u00fcstern!", "tokens": ["Nie", "die", "Ge\u00b7w\u00f6l\u00b7ke", "der", "Zeit", "ver\u00b7d\u00fcs\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}