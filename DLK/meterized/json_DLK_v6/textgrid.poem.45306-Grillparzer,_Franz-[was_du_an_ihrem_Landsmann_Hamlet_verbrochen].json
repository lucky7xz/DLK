{"textgrid.poem.45306": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[was du an ihrem Landsmann Hamlet verbrochen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du an ihrem Landsmann Hamlet verbrochen,", "tokens": ["Was", "du", "an", "ih\u00b7rem", "Lands\u00b7mann", "Ham\u00b7let", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bleibt von den D\u00e4nen nicht ungerochen,", "tokens": ["Bleibt", "von", "den", "D\u00e4\u00b7nen", "nicht", "un\u00b7ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sie k\u00f6nnten f\u00fcr dein t\u00e4ppisch Sezieren", "tokens": ["Sie", "k\u00f6nn\u00b7ten", "f\u00fcr", "dein", "t\u00e4p\u00b7pisch", "Se\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Dich als spruchweisen Polonius traktieren.", "tokens": ["Dich", "als", "spruch\u00b7wei\u00b7sen", "Po\u00b7lo\u00b7ni\u00b7us", "trak\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "VVFIN", "NE", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}}}}