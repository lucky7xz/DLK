 "measure": "trochaic.tetra"}, "line.20": {"text": "All betaut von hei\u00dfen Tr\u00e4nen,", "tokens": ["All", "be\u00b7taut", "von", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ihr dem\u00fctig lag zu F\u00fc\u00dfen,", "tokens": ["Ihr", "de\u00b7m\u00fc\u00b7tig", "lag", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ach die ihn nicht von sich stie\u00dfen,", "tokens": ["Ach", "die", "ihn", "nicht", "von", "sich", "stie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PRELS", "PPER", "PTKNEG", "APPR", "PRF", "VVFIN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.23": {"text": "Die ich durfte treu umschlingen", "tokens": ["Die", "ich", "durf\u00b7te", "treu", "um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Stirb Jahr, nichts mehr kannst du bringen,", "tokens": ["Stirb", "Jahr", ",", "nichts", "mehr", "kannst", "du", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PIS", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Selig starb die letzte Rose", "tokens": ["Se\u00b7lig", "starb", "die", "letz\u00b7te", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Still entbl\u0