{"textgrid.poem.38199": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Recht wie ein Leichnam wandle ich umher", "tokens": ["Recht", "wie", "ein", "Leich\u00b7nam", "wand\u00b7le", "ich", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu seiner Th\u00fcre Nachts und seufze schwer,", "tokens": ["Zu", "sei\u00b7ner", "Th\u00fc\u00b7re", "Nachts", "und", "seuf\u00b7ze", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus meiner Brust an Trost und Wohlseyn leer.", "tokens": ["Aus", "mei\u00b7ner", "Brust", "an", "Trost", "und", "Wohl\u00b7seyn", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Athem st\u00f6hnet wie ein Fichtenwald,", "tokens": ["Mein", "A\u00b7them", "st\u00f6h\u00b7net", "wie", "ein", "Fich\u00b7ten\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Ungl\u00fcckszeichen mein Gesang erschallt,", "tokens": ["Ein", "Un\u00b7gl\u00fccks\u00b7zei\u00b7chen", "mein", "Ge\u00b7sang", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df alle Nachbarn sich ergrimmen bald.", "tokens": ["Da\u00df", "al\u00b7le", "Nach\u00b7barn", "sich", "er\u00b7grim\u00b7men", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie l\u00e4rmen, nicht zu h\u00f6ren all mein Weh,", "tokens": ["Sie", "l\u00e4r\u00b7men", ",", "nicht", "zu", "h\u00f6\u00b7ren", "all", "mein", "Weh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKNEG", "PTKZU", "VVINF", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie nehmen Umweg, da\u00df mich keiner seh,", "tokens": ["Sie", "neh\u00b7men", "Um\u00b7weg", ",", "da\u00df", "mich", "kei\u00b7ner", "seh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Jezt f\u00fcrcht ich nichts, war scheu sonst wie ein Reh.", "tokens": ["Jezt", "f\u00fcrcht", "ich", "nichts", ",", "war", "scheu", "sonst", "wie", "ein", "Reh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "VAFIN", "ADJD", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie von dem Ast im Traum ein Vogel f\u00e4llt,", "tokens": ["Wie", "von", "dem", "Ast", "im", "Traum", "ein", "Vo\u00b7gel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So flattre ich des Nachts, so ungesellt;", "tokens": ["So", "flatt\u00b7re", "ich", "des", "Nachts", ",", "so", "un\u00b7ge\u00b7sellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Ungl\u00fccksvogel nimmermehr gef\u00e4llt!", "tokens": ["Ein", "Un\u00b7gl\u00fccks\u00b7vo\u00b7gel", "nim\u00b7mer\u00b7mehr", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was soll draus werden? fraget alle Welt.", "tokens": ["Was", "soll", "draus", "wer\u00b7den", "?", "fra\u00b7get", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PAV", "VAINF", "$.", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was ist die Welt? Wer schuf sie unbestellt?", "tokens": ["Was", "ist", "die", "Welt", "?", "Wer", "schuf", "sie", "un\u00b7be\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die schuf allein, die mich so sehr entstellt.", "tokens": ["Die", "schuf", "al\u00b7lein", ",", "die", "mich", "so", "sehr", "ent\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich freu mich, wie mein Fleisch so schwinden thut,", "tokens": ["Ich", "freu", "mich", ",", "wie", "mein", "Fleisch", "so", "schwin\u00b7den", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOKOM", "PPOSAT", "NN", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein festes Land zerrei\u00dft der Strom vom Blut,", "tokens": ["Mein", "fes\u00b7tes", "Land", "zer\u00b7rei\u00dft", "der", "Strom", "vom", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der aus dem Herzen kommt und niemals ruht.", "tokens": ["Der", "aus", "dem", "Her\u00b7zen", "kommt", "und", "nie\u00b7mals", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O meine Thr\u00e4nen, keiner sch\u00e4tzet euch,", "tokens": ["O", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "kei\u00b7ner", "sch\u00e4t\u00b7zet", "euch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr seyd den Himmelsgaben darin gleich;", "tokens": ["Ihr", "seyd", "den", "Him\u00b7mels\u00b7ga\u00b7ben", "da\u00b7rin", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PAV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An allem bin ich arm, in euch so reich.", "tokens": ["An", "al\u00b7lem", "bin", "ich", "arm", ",", "in", "euch", "so", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADJD", "$,", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}