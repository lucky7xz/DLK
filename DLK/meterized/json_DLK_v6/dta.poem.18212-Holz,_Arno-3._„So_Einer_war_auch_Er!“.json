{"dta.poem.18212": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "3.  \n  \u201eSo Einer war auch Er!\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Liegt ein D\u00f6rflein mitten im Walde,", "tokens": ["Liegt", "ein", "D\u00f6r\u00b7flein", "mit\u00b7ten", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ueberdeckt vom Sonnenschein,", "tokens": ["Ue\u00b7berd\u00b7eckt", "vom", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor dem letzten Haus an der Halde", "tokens": ["Und", "vor", "dem", "letz\u00b7ten", "Haus", "an", "der", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sitzt ein steinalt M\u00fctterlein.", "tokens": ["Sitzt", "ein", "stei\u00b7nalt", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie l\u00e4\u00dft den Faden gleiten", "tokens": ["Sie", "l\u00e4\u00dft", "den", "Fa\u00b7den", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Spinnrad Spinnrad sein", "tokens": ["Und", "Spinn\u00b7rad", "Spinn\u00b7rad", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und denkt an die alten Zeiten", "tokens": ["Und", "denkt", "an", "die", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und nickt und schlummert ein.", "tokens": ["Und", "nickt", "und", "schlum\u00b7mert", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heimlich schleicht sich die Mittagsstille", "tokens": ["Heim\u00b7lich", "schleicht", "sich", "die", "Mit\u00b7tags\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Durch das flimmernde, gr\u00fcne Revier.", "tokens": ["Durch", "das", "flim\u00b7mern\u00b7de", ",", "gr\u00fc\u00b7ne", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Alles schl\u00e4ft; selbst Drossel und Grille", "tokens": ["Al\u00b7les", "schl\u00e4ft", ";", "selbst", "Dros\u00b7sel", "und", "Gril\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "ADV", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und vorm Pflug der m\u00fcde Stier.", "tokens": ["Und", "vorm", "Pflug", "der", "m\u00fc\u00b7de", "Stier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da pl\u00f6tzlich kommt es gezogen", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "kommt", "es", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Blitzend den Wald entlang", "tokens": ["Blit\u00b7zend", "den", "Wald", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "APPO"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und vor ihm hergeflogen", "tokens": ["Und", "vor", "ihm", "her\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Trommel und Pfeifenklang.", "tokens": ["Wie", "Trom\u00b7mel", "und", "Pfei\u00b7fen\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und in das Lied vom alten Bl\u00fccher", "tokens": ["Und", "in", "das", "Lied", "vom", "al\u00b7ten", "Bl\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jauchzen die D\u00f6rfler: \u201eSie sind da!\u201c", "tokens": ["Jauch\u00b7zen", "die", "D\u00f6rf\u00b7ler", ":", "\u201e", "Sie", "sind", "da", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "ADV", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und die M\u00e4dels schwenken die T\u00fccher", "tokens": ["Und", "die", "M\u00e4\u00b7dels", "schwen\u00b7ken", "die", "T\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und die Jungens rufen: \u201eHurrah!\u201c", "tokens": ["Und", "die", "Jun\u00b7gens", "ru\u00b7fen", ":", "\u201e", "Hur\u00b7rah", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$.", "$(", "ITJ", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Gott sch\u00fctze die goldnen Saaten,", "tokens": ["Gott", "sch\u00fct\u00b7ze", "die", "gold\u00b7nen", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dazu die weite Welt;", "tokens": ["Da\u00b7zu", "die", "wei\u00b7te", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Kaisers junge Soldaten", "tokens": ["Des", "Kai\u00b7sers", "jun\u00b7ge", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ziehn wieder ins gr\u00fcne Feld!", "tokens": ["Ziehn", "wie\u00b7der", "ins", "gr\u00fc\u00b7ne", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, schon schwenken sie um die Halde,", "tokens": ["Sieh", ",", "schon", "schwen\u00b7ken", "sie", "um", "die", "Hal\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo das letzte der H\u00e4uschen lacht!", "tokens": ["Wo", "das", "letz\u00b7te", "der", "H\u00e4usc\u00b7hen", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schon verschwinden die ersten im Walde", "tokens": ["Schon", "ver\u00b7schwin\u00b7den", "die", "ers\u00b7ten", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPRART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und das M\u00fctterchen ist erwacht.", "tokens": ["Und", "das", "M\u00fct\u00b7ter\u00b7chen", "ist", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Versunken in tiefes Sinnen,", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "tie\u00b7fes", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wird ihr das Herz so schwer", "tokens": ["Wird", "ihr", "das", "Herz", "so", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ihre Thr\u00e4nen rinnen:", "tokens": ["Und", "ih\u00b7re", "Thr\u00e4\u00b7nen", "rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}}}}