{"textgrid.poem.56289": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die M\u00fcllerin", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00fchle, die dreht ihre Fl\u00fcgel,", "tokens": ["Die", "M\u00fch\u00b7le", ",", "die", "dreht", "ih\u00b7re", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Sturm, der sauset darin;", "tokens": ["Der", "Sturm", ",", "der", "sau\u00b7set", "da\u00b7rin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und unter der Linde am H\u00fcgel,", "tokens": ["Und", "un\u00b7ter", "der", "Lin\u00b7de", "am", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da weinet die M\u00fcllerin:", "tokens": ["Da", "wei\u00b7net", "die", "M\u00fcl\u00b7le\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df sausen den Sturm und brausen,", "tokens": ["La\u00df", "sau\u00b7sen", "den", "Sturm", "und", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich habe gebaut auf den Wind;", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7baut", "auf", "den", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich habe gebaut auf Schw\u00fcre \u2013", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7baut", "auf", "Schw\u00fc\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da war ich ein t\u00f6richtes Kind.", "tokens": ["Da", "war", "ich", "ein", "t\u00f6\u00b7rich\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch hat mich der Wind nicht belogen,", "tokens": ["Noch", "hat", "mich", "der", "Wind", "nicht", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Wind, der blieb mir treu;", "tokens": ["Der", "Wind", ",", "der", "blieb", "mir", "treu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bin ich verarmt und betrogen \u2013", "tokens": ["Und", "bin", "ich", "ver\u00b7armt", "und", "be\u00b7tro\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Schw\u00fcre, die waren nur Spreu.", "tokens": ["Die", "Schw\u00fc\u00b7re", ",", "die", "wa\u00b7ren", "nur", "Spreu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Wo ist, der sie geschworen?", "tokens": ["Wo", "ist", ",", "der", "sie", "ge\u00b7schwo\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wind nimmt die Klagen nur auf;", "tokens": ["Der", "Wind", "nimmt", "die", "Kla\u00b7gen", "nur", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er hat sich aufs Wandern verloren \u2013", "tokens": ["Er", "hat", "sich", "aufs", "Wan\u00b7dern", "ver\u00b7lo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPRART", "NN", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es findet der Wind ihn nicht auf.", "tokens": ["Es", "fin\u00b7det", "der", "Wind", "ihn", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}