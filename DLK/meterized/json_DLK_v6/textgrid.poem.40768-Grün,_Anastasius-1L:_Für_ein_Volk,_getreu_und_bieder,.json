{"textgrid.poem.40768": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcr ein Volk, getreu und bieder,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr ein Volk, getreu und bieder,", "tokens": ["F\u00fcr", "ein", "Volk", ",", "ge\u00b7treu", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr ein sch\u00f6nes, freies Recht", "tokens": ["F\u00fcr", "ein", "sch\u00f6\u00b7nes", ",", "frei\u00b7es", "Recht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00e4mpften hei\u00df einst deine Lieder,", "tokens": ["K\u00e4mpf\u00b7ten", "hei\u00df", "einst", "dei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchn, wie Helden im Gefecht.", "tokens": ["K\u00fchn", ",", "wie", "Hel\u00b7den", "im", "Ge\u00b7fecht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wem der Sieg durch Waffen gl\u00fcckte,", "tokens": ["Wem", "der", "Sieg", "durch", "Waf\u00b7fen", "gl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht allein sei Held genannt!", "tokens": ["Nicht", "al\u00b7lein", "sei", "Held", "ge\u00b7nannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "J\u00fcngst an deinem Herde dr\u00fcckte", "tokens": ["J\u00fcngst", "an", "dei\u00b7nem", "Her\u00b7de", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir wohl auch ein Held die Hand.", "tokens": ["Mir", "wohl", "auch", "ein", "Held", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jeder ficht mit eigner Wehre,", "tokens": ["Je\u00b7der", "ficht", "mit", "eig\u00b7ner", "Weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Priester k\u00e4mpft mit dem Brevier,", "tokens": ["Pries\u00b7ter", "k\u00e4mpft", "mit", "dem", "Bre\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Krieger mit dem Schwert und Speere,", "tokens": ["Krie\u00b7ger", "mit", "dem", "Schwert", "und", "Spee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Gesang und Reimen wir.", "tokens": ["Mit", "Ge\u00b7sang", "und", "Rei\u00b7men", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum sind dir nicht fremd die Lieder,", "tokens": ["Drum", "sind", "dir", "nicht", "fremd", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die ich sang von gr\u00fcnen H\u00f6h'n,", "tokens": ["Die", "ich", "sang", "von", "gr\u00fc\u00b7nen", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr ein Volk, das treu und bieder,", "tokens": ["F\u00fcr", "ein", "Volk", ",", "das", "treu", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr ein Recht, das frei und sch\u00f6n!", "tokens": ["F\u00fcr", "ein", "Recht", ",", "das", "frei", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Berge sind emporgeschwollen,", "tokens": ["Ber\u00b7ge", "sind", "em\u00b7por\u00b7ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend B\u00e4ch' und Str\u00f6me ziehn,", "tokens": ["Tau\u00b7send", "B\u00e4ch'", "und", "Str\u00f6\u00b7me", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Land und Fluren endlos rollen", "tokens": ["Land", "und", "Flu\u00b7ren", "end\u00b7los", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen mir und dir dahin!", "tokens": ["Zwi\u00b7schen", "mir", "und", "dir", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In des Waldes gr\u00fcnen G\u00e4ngen", "tokens": ["In", "des", "Wal\u00b7des", "gr\u00fc\u00b7nen", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Las manch zarten Zweig ich aus,", "tokens": ["Las", "manch", "zar\u00b7ten", "Zweig", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche Ros' auf Alpenh\u00e4ngen,", "tokens": ["Man\u00b7che", "Ros'", "auf", "Al\u00b7pen\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Kr\u00e4nzlein wand ich draus.", "tokens": ["Und", "ein", "Kr\u00e4nz\u00b7lein", "wand", "ich", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gern mit liebevollen H\u00e4nden", "tokens": ["Gern", "mit", "lie\u00b7be\u00b7vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4nd' ich's fest an einen Pfeil,", "tokens": ["B\u00e4nd'", "ich's", "fest", "an", "ei\u00b7nen", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Luft ihn dir zu senden!", "tokens": ["Durch", "die", "Luft", "ihn", "dir", "zu", "sen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch so weit fliegt selbst kein Pfeil.", "tokens": ["Doch", "so", "weit", "fliegt", "selbst", "kein", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Einer Taube wollt' ich's schlingen", "tokens": ["Ei\u00b7ner", "Tau\u00b7be", "wollt'", "ich's", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um das wei\u00dfe H\u00e4lschen gern;", "tokens": ["Um", "das", "wei\u00b7\u00dfe", "H\u00e4l\u00b7schen", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch bald s\u00e4nken ihr die Schwingen,", "tokens": ["Doch", "bald", "s\u00e4n\u00b7ken", "ihr", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn das Ziel ist allzufern!", "tokens": ["Denn", "das", "Ziel", "ist", "all\u00b7zu\u00b7fern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und von Ungeduld ergriffen", "tokens": ["Und", "von", "Un\u00b7ge\u00b7duld", "er\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleudr' ich's selber durch die Luft!", "tokens": ["Schleudr'", "ich's", "sel\u00b7ber", "durch", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leicht zu dir hin seh ich's schiffen", "tokens": ["Leicht", "zu", "dir", "hin", "seh", "ich's", "schif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ADV", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Strom, Gebirg und Kluft! \u2013 \u2013", "tokens": ["Ue\u00b7ber", "Strom", ",", "Ge\u00b7birg", "und", "Kluft", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sieh, es kehrt' ein Sieger wieder", "tokens": ["Sieh", ",", "es", "kehrt'", "ein", "Sie\u00b7ger", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heim bei stiller Abendruh',", "tokens": ["Heim", "bei", "stil\u00b7ler", "A\u00b7bendruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bald die m\u00fcden Augenlider", "tokens": ["Bald", "die", "m\u00fc\u00b7den", "Au\u00b7gen\u00b7li\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlo\u00df ihm s\u00fc\u00dfer Schlummer zu.", "tokens": ["Schlo\u00df", "ihm", "s\u00fc\u00b7\u00dfer", "Schlum\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch des Morgens drauf, erwachend,", "tokens": ["Doch", "des", "Mor\u00b7gens", "drauf", ",", "er\u00b7wa\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADV", "PTKVZ", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Kranz er vor sich fand", "tokens": ["Ei\u00b7nen", "Kranz", "er", "vor", "sich", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcn und duftig, frisch und lachend", "tokens": ["Gr\u00fcn", "und", "duf\u00b7tig", ",", "frisch", "und", "la\u00b7chend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie von unsichtbarer Hand!", "tokens": ["Wie", "von", "un\u00b7sicht\u00b7ba\u00b7rer", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Als er lauscht, sein Haupt erhebend", "tokens": ["Als", "er", "lauscht", ",", "sein", "Haupt", "er\u00b7he\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00f6t' und Saitenspiel begann,", "tokens": ["Fl\u00f6t'", "und", "Sai\u00b7ten\u00b7spiel", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsichtbarem Ort' entschwebend,", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7rem", "Ort'", "ent\u00b7schwe\u00b7bend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00df und lieblich, himmelan!", "tokens": ["S\u00fc\u00df", "und", "lieb\u00b7lich", ",", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wer solch Fest von all den Lieben", "tokens": ["Wer", "solch", "Fest", "von", "all", "den", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "APPR", "PIAT", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm ersann, nicht ahnt er's zwar;", "tokens": ["Ihm", "er\u00b7sann", ",", "nicht", "ahnt", "er's", "zwar", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKNEG", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ins Herz ihm ist's geschrieben:", "tokens": ["Doch", "ins", "Herz", "ihm", "ist's", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es wohl die Liebe war. \u2013", "tokens": ["Da\u00df", "es", "wohl", "die", "Lie\u00b7be", "war", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "So auch h\u00f6rst Gesang du schallen,", "tokens": ["So", "auch", "h\u00f6rst", "Ge\u00b7sang", "du", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Kennst doch nicht den Mund, der singt,", "tokens": ["Kennst", "doch", "nicht", "den", "Mund", ",", "der", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst den Kranz auch niederfallen,", "tokens": ["Siehst", "den", "Kranz", "auch", "nie\u00b7der\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch die Hand nicht, die ihn bringt;", "tokens": ["Doch", "die", "Hand", "nicht", ",", "die", "ihn", "bringt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ahnst aus allen, die dich lieben,", "tokens": ["Ahnst", "aus", "al\u00b7len", ",", "die", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise kaum den Rechten zwar;", "tokens": ["Lei\u00b7se", "kaum", "den", "Rech\u00b7ten", "zwar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ins Herz dir ist's geschrieben:", "tokens": ["Doch", "ins", "Herz", "dir", "ist's", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df gewi\u00df die Lieb' es war!", "tokens": ["Da\u00df", "ge\u00b7wi\u00df", "die", "Lieb'", "es", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}