{"textgrid.poem.43225": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich lausche lange oft in einer Ecke,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lausche lange oft in einer Ecke,", "tokens": ["Ich", "lau\u00b7sche", "lan\u00b7ge", "oft", "in", "ei\u00b7ner", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis ich auf der Terrasse Dich ersp\u00e4he;", "tokens": ["Bis", "ich", "auf", "der", "Ter\u00b7ras\u00b7se", "Dich", "er\u00b7sp\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Du aber ahnest nimmer meine N\u00e4he,", "tokens": ["Du", "a\u00b7ber", "ah\u00b7nest", "nim\u00b7mer", "mei\u00b7ne", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ahn'st nicht die Qualen, die ich mir erwecke.", "tokens": ["Ahn'st", "nicht", "die", "Qua\u00b7len", ",", "die", "ich", "mir", "er\u00b7we\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du h\u00fcllest Dich in Deine weichen T\u00fccher", "tokens": ["Du", "h\u00fcl\u00b7lest", "Dich", "in", "Dei\u00b7ne", "wei\u00b7chen", "T\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und tr\u00e4llerst leichthin Deine weichen Lieder,", "tokens": ["Und", "tr\u00e4l\u00b7lerst", "leich\u00b7thin", "Dei\u00b7ne", "wei\u00b7chen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O komm' zu mir, \u2013 komm' wieder, \u2013 komme wieder! \u2013", "tokens": ["O", "komm'", "zu", "mir", ",", "\u2013", "komm'", "wie\u00b7der", ",", "\u2013", "kom\u00b7me", "wie\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "$(", "VVFIN", "ADV", "$,", "$(", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich schaffe Gold \u2013 ich denke gro\u00dfe B\u00fccher;", "tokens": ["Ich", "schaf\u00b7fe", "Gold", "\u2013", "ich", "den\u00b7ke", "gro\u00b7\u00dfe", "B\u00fc\u00b7cher", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Verla\u00df' dies Haus, ich will ein Neues bauen,", "tokens": ["Ver\u00b7la\u00df'", "dies", "Haus", ",", "ich", "will", "ein", "Neu\u00b7es", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ich will f\u00fcr Deinen Putz mich st\u00fcndlich m\u00fchen,", "tokens": ["Ich", "will", "f\u00fcr", "Dei\u00b7nen", "Putz", "mich", "st\u00fcnd\u00b7lich", "m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von Dir begeistert soll mein Lied ergl\u00fchen,", "tokens": ["Von", "Dir", "be\u00b7geis\u00b7tert", "soll", "mein", "Lied", "er\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ich kann nicht dichten, ohne Dich zu schauen! \u2013", "tokens": ["Ich", "kann", "nicht", "dich\u00b7ten", ",", "oh\u00b7ne", "Dich", "zu", "schau\u00b7en", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJA", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was ruhelos zu Dir mich hingetrieben,", "tokens": ["Was", "ru\u00b7he\u00b7los", "zu", "Dir", "mich", "hin\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Was ich ersticken wollt' mit eitlem Lachen, \u2013", "tokens": ["Was", "ich", "er\u00b7sti\u00b7cken", "wollt'", "mit", "eit\u00b7lem", "La\u00b7chen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nur Deine N\u00e4he konnt' es klar mir machen:", "tokens": ["Nur", "Dei\u00b7ne", "N\u00e4\u00b7he", "konnt'", "es", "klar", "mir", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Es ist mein ", "tokens": ["Es", "ist", "mein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}}}}