{"textgrid.poem.55404": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind Gefilde t\u00fcrkisch worden,", "tokens": ["Sind", "Ge\u00b7fil\u00b7de", "t\u00fcr\u00b7kisch", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst Besitz der Albanesen;", "tokens": ["Sonst", "Be\u00b7sitz", "der", "Al\u00b7ba\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stergios ist noch am Leben,", "tokens": ["Ster\u00b7gios", "ist", "noch", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Keines Paschas achtet er.", "tokens": ["Kei\u00b7nes", "Pa\u00b7schas", "ach\u00b7tet", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und solang es schneit hier oben,", "tokens": ["Und", "so\u00b7lang", "es", "schneit", "hier", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Beugen wir den T\u00fcrken nicht.", "tokens": ["Beu\u00b7gen", "wir", "den", "T\u00fcr\u00b7ken", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Setzet eure Vorhut dahin,", "tokens": ["Set\u00b7zet", "eu\u00b7re", "Vor\u00b7hut", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PAV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wo die W\u00f6lfe nistend hecken!", "tokens": ["Wo", "die", "W\u00f6l\u00b7fe", "nis\u00b7tend", "he\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sei der Sklave Stadtbewohner;", "tokens": ["Sei", "der", "Skla\u00b7ve", "Stadt\u00b7be\u00b7woh\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stadtbezirk ist unsern Braven", "tokens": ["Stadt\u00b7be\u00b7zirk", "ist", "un\u00b7sern", "Bra\u00b7ven"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "W\u00fcster Felsen Klippenspalte.", "tokens": ["W\u00fcs\u00b7ter", "Fel\u00b7sen", "Klip\u00b7pen\u00b7spal\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Eh als mit den T\u00fcrken leben,", "tokens": ["Eh", "als", "mit", "den", "T\u00fcr\u00b7ken", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Lieber mit den wilden Tieren!", "tokens": ["Lie\u00b7ber", "mit", "den", "wil\u00b7den", "Tie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}