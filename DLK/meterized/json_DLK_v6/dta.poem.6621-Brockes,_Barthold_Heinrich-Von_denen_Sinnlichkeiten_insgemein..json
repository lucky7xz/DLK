{"dta.poem.6621": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch ausser den Versammlungen der Welt", "tokens": ["Noch", "aus\u00b7ser", "den", "Ver\u00b7samm\u00b7lun\u00b7gen", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und den Zusammensatz von Himmel, Fluth und Erden,", "tokens": ["Und", "den", "Zu\u00b7sam\u00b7men\u00b7satz", "von", "Him\u00b7mel", ",", "Fluth", "und", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die GOTT der HERR auf einmal hingestellt,", "tokens": ["Die", "GoTT", "der", "HeRR", "auf", "ein\u00b7mal", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da sie durch eintzelne Gesetz erhalten werden;", "tokens": ["Da", "sie", "durch", "eint\u00b7zel\u00b7ne", "Ge\u00b7setz", "er\u00b7hal\u00b7ten", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "VAINF", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So giebt es C\u00f6rper noch die jederzeit", "tokens": ["So", "giebt", "es", "C\u00f6r\u00b7per", "noch", "die", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erf\u00fcllet sind mit Krafft von Fruchtbarkeit:", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "sind", "mit", "Krafft", "von", "Frucht\u00b7bar\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Belebte Wesen, welche sich", "tokens": ["Be\u00b7leb\u00b7te", "We\u00b7sen", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ver\u00e4ndern und verst\u00f6hren", "tokens": ["Ver\u00b7\u00e4n\u00b7dern", "und", "ver\u00b7st\u00f6h\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und durch sich selbst sich wiederum geb\u00e4hren", "tokens": ["Und", "durch", "sich", "selbst", "sich", "wie\u00b7de\u00b7rum", "ge\u00b7b\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "ADV", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}