{"textgrid.poem.60535": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00fcrass", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwollt Ihr nicht einen K\u00fcra\u00df kaufen,", "tokens": ["\u00bb", "wollt", "Ihr", "nicht", "ei\u00b7nen", "K\u00fc\u00b7ra\u00df", "kau\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Herr Husar! mein Herr Husar?", "tokens": ["Herr", "Hu\u00b7sar", "!", "mein", "Herr", "Hu\u00b7sar", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPOSAT", "NN", "NE", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "'s ist doch besser im K\u00fcra\u00df raufen,", "tokens": ["'s", "ist", "doch", "bes\u00b7ser", "im", "K\u00fc\u00b7ra\u00df", "rau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Als im schlei\u00dfigen Tuch, nicht wahr?\u00ab", "tokens": ["Als", "im", "schlei\u00b7\u00dfi\u00b7gen", "Tuch", ",", "nicht", "wahr", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "$,", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Lacht der Husar dem Judengauche:", "tokens": ["Lacht", "der", "Hu\u00b7sar", "dem", "Ju\u00b7den\u00b7gau\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "\u00bbhast du den Hafendeckel gebracht,", "tokens": ["\u00bb", "hast", "du", "den", "Ha\u00b7fen\u00b7de\u00b7ckel", "ge\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Da\u00df die Seele mir nicht ausrauche,", "tokens": ["Da\u00df", "die", "See\u00b7le", "mir", "nicht", "aus\u00b7rau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wenn sie mir kocht im Feuer der Schlacht?\u00ab", "tokens": ["Wenn", "sie", "mir", "kocht", "im", "Feu\u00b7er", "der", "Schlacht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbkauft den K\u00fcra\u00df! wie w\u00e4rs doch schade", "tokens": ["\u00bb", "kauft", "den", "K\u00fc\u00b7ra\u00df", "!", "wie", "w\u00e4rs", "doch", "scha\u00b7de"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ART", "NN", "$.", "PWAV", "VAFIN", "ADV", "VVFIN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Um den sch\u00f6nen gewichsten Bart,", "tokens": ["Um", "den", "sch\u00f6\u00b7nen", "ge\u00b7wichs\u00b7ten", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn er um eine schlechte Parade", "tokens": ["Wenn", "er", "um", "ei\u00b7ne", "schlech\u00b7te", "Pa\u00b7ra\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch so schwarz schon w\u00fcrde verscharrt!\u00ab", "tokens": ["Noch", "so", "schwarz", "schon", "w\u00fcr\u00b7de", "ver\u00b7scharrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbjude, kennst du Husarenhiebe?", "tokens": ["\u00bb", "ju\u00b7de", ",", "kennst", "du", "Hu\u00b7sa\u00b7ren\u00b7hie\u00b7be", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "S\u00e4bel und Schild und K\u00fcra\u00df zugleich", "tokens": ["S\u00e4\u00b7bel", "und", "Schild", "und", "K\u00fc\u00b7ra\u00df", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00fchrt meine Faust; Jud, schiebe dich, schiebe,", "tokens": ["F\u00fchrt", "mei\u00b7ne", "Faust", ";", "Jud", ",", "schie\u00b7be", "dich", ",", "schie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonst verkostest du meinen Streich\u00ab", "tokens": ["Sonst", "ver\u00b7kos\u00b7test", "du", "mei\u00b7nen", "Streich", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Und der Husar den blanken S\u00e4bel", "tokens": ["Und", "der", "Hu\u00b7sar", "den", "blan\u00b7ken", "S\u00e4\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kreuzend und kreisend ums Haupt sich schwingt,", "tokens": ["Kreu\u00b7zend", "und", "krei\u00b7send", "ums", "Haupt", "sich", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Da\u00df es wie ein eiserner Nebel", "tokens": ["Da\u00df", "es", "wie", "ein", "ei\u00b7ser\u00b7ner", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vor den Augen des Juden springt.", "tokens": ["Vor", "den", "Au\u00b7gen", "des", "Ju\u00b7den", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "\u00bbbravo, Husar! Doch besser ist besser;", "tokens": ["\u00bb", "bra\u00b7vo", ",", "Hu\u00b7sar", "!", "Doch", "bes\u00b7ser", "ist", "bes\u00b7ser", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "$.", "KON", "ADJD", "VAFIN", "ADJD", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Kauft den K\u00fcra\u00df, helft Euch und mir.", "tokens": ["Kauft", "den", "K\u00fc\u00b7ra\u00df", ",", "helft", "Euch", "und", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Seht, dort reiten drei Eisenfresser,", "tokens": ["Seht", ",", "dort", "rei\u00b7ten", "drei", "Ei\u00b7sen\u00b7fres\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Weh, drei Feinde! bald sind sie hier.\u00ab", "tokens": ["Weh", ",", "drei", "Fein\u00b7de", "!", "bald", "sind", "sie", "hier", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "CARD", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "\u00bbei, so komm\u00ab, so ruft der Magyare,", "tokens": ["\u00bb", "ei", ",", "so", "komm", "\u00ab", ",", "so", "ruft", "der", "Ma\u00b7gy\u00b7a\u00b7re", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "VVFIN", "$(", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbwill dir helfen, du armer Tropf!\u00ab", "tokens": ["\u00bb", "will", "dir", "hel\u00b7fen", ",", "du", "ar\u00b7mer", "Tropf", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "VVINF", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und er packt ihn an seinem Talare,", "tokens": ["Und", "er", "packt", "ihn", "an", "sei\u00b7nem", "Ta\u00b7la\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Setzt ihn vor sich auf den Sattelknopf.", "tokens": ["Setzt", "ihn", "vor", "sich", "auf", "den", "Sat\u00b7tel\u00b7knopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbo du \u00e4rmster Jude auf Erden!", "tokens": ["\u00bb", "o", "du", "\u00e4rms\u00b7ter", "Ju\u00b7de", "auf", "Er\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ich bin hinten, und du bist vorn;", "tokens": ["Ich", "bin", "hin\u00b7ten", ",", "und", "du", "bist", "vorn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KON", "PPER", "VAFIN", "ADV", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du mu\u00dft selber mein K\u00fcra\u00df werden!\u00ab", "tokens": ["Du", "mu\u00dft", "sel\u00b7ber", "mein", "K\u00fc\u00b7ra\u00df", "wer\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und er gibt dem Rosse die Sporn.", "tokens": ["Und", "er", "gibt", "dem", "Ros\u00b7se", "die", "Sporn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Wild verzweifelnd schreit der Hebr\u00e4er,", "tokens": ["Wild", "ver\u00b7zwei\u00b7felnd", "schreit", "der", "Heb\u00b7r\u00e4\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Doch der Husar h\u00e4lt fest; hu! hu!", "tokens": ["Doch", "der", "Hu\u00b7sar", "h\u00e4lt", "fest", ";", "hu", "!", "hu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$.", "XY", "$.", "XY", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Reitet n\u00e4her und n\u00e4her und n\u00e4her", "tokens": ["Rei\u00b7tet", "n\u00e4\u00b7her", "und", "n\u00e4\u00b7her", "und", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Auf die drei feindlichen Reiter zu.", "tokens": ["Auf", "die", "drei", "feind\u00b7li\u00b7chen", "Rei\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "H\u00e4lt den Juden mit seiner Linken,", "tokens": ["H\u00e4lt", "den", "Ju\u00b7den", "mit", "sei\u00b7ner", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit der Rechten f\u00fchrt er das Schwert,", "tokens": ["Mit", "der", "Rech\u00b7ten", "f\u00fchrt", "er", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und die drei Reiter vom Rosse sinken,", "tokens": ["Und", "die", "drei", "Rei\u00b7ter", "vom", "Ros\u00b7se", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "--+---+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und der Jude blieb unversehrt.", "tokens": ["Und", "der", "Ju\u00b7de", "blieb", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Sanft hinab vom sch\u00e4umenden Hengste", "tokens": ["Sanft", "hin\u00b7ab", "vom", "sch\u00e4u\u00b7men\u00b7den", "Hengs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Setzt den Juden jetzt der Husar,", "tokens": ["Setzt", "den", "Ju\u00b7den", "jetzt", "der", "Hu\u00b7sar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr die Gefahren und Todes\u00e4ngste", "tokens": ["F\u00fcr", "die", "Ge\u00b7fah\u00b7ren", "und", "To\u00b7de\u00b7s\u00e4ngs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Reicht er den Beutel voll Goldes ihm dar.", "tokens": ["Reicht", "er", "den", "Beu\u00b7tel", "voll", "Gol\u00b7des", "ihm", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbkeinen K\u00fcra\u00df mehr dem Husaren!\u00ab", "tokens": ["\u00bb", "kei\u00b7nen", "K\u00fc\u00b7ra\u00df", "mehr", "dem", "Hu\u00b7sa\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ruft der Husar und reitet davon;", "tokens": ["Ruft", "der", "Hu\u00b7sar", "und", "rei\u00b7tet", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PAV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Zitternd noch von den Todesgefahren,", "tokens": ["Zit\u00b7ternd", "noch", "von", "den", "To\u00b7des\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Z\u00e4hlt der Jud die Dukaten schon.", "tokens": ["Z\u00e4hlt", "der", "Jud", "die", "Du\u00b7ka\u00b7ten", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}