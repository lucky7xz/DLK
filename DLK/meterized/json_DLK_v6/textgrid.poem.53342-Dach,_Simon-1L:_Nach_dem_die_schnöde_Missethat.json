{"textgrid.poem.53342": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach dem die schn\u00f6de Missethat", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach dem die schn\u00f6de Missethat", "tokens": ["Nach", "dem", "die", "schn\u00f6\u00b7de", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Weltkreis eingenommen hat,", "tokens": ["Den", "Welt\u00b7kreis", "ein\u00b7ge\u00b7nom\u00b7men", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd uns durch b\u00f6ser Lust Begier", "tokens": ["Vnd", "uns", "durch", "b\u00f6\u00b7ser", "Lust", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebracht umb alle Seelen Zier,", "tokens": ["Ge\u00b7bracht", "umb", "al\u00b7le", "See\u00b7len", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O welch ein armes Volck sind wir!", "tokens": ["O", "welch", "ein", "ar\u00b7mes", "Volck", "sind", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Bosheit Pflantze nimmt allein", "tokens": ["Der", "Bos\u00b7heit", "Pflant\u00b7ze", "nimmt", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Raum der Hertzen bey uns ein,", "tokens": ["Den", "Raum", "der", "Hert\u00b7zen", "bey", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hasset Gott und sein Gebot", "tokens": ["Sie", "has\u00b7set", "Gott", "und", "sein", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd jhre Frucht ist Jammer, Noht,", "tokens": ["Vnd", "jhre", "Frucht", "ist", "Jam\u00b7mer", ",", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Furcht, Schrecken, Kummer, Hell und Todt.", "tokens": ["Furcht", ",", "Schre\u00b7cken", ",", "Kum\u00b7mer", ",", "Hell", "und", "Todt", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Fragt nun, woher Pest, Krieg und Brand", "tokens": ["Fragt", "nun", ",", "wo\u00b7her", "Pest", ",", "Krieg", "und", "Brand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verw\u00fcst' erb\u00e4rmlich Leut' und Land,", "tokens": ["Ver\u00b7w\u00fcst'", "er\u00b7b\u00e4rm\u00b7lich", "Leut'", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woher der Zeiten Tiranney", "tokens": ["Wo\u00b7her", "der", "Zei\u00b7ten", "Ti\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So manches Weh und Angst geschrey", "tokens": ["So", "man\u00b7ches", "Weh", "und", "Angst", "ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd so viel tausend Elend sey?", "tokens": ["Vnd", "so", "viel", "tau\u00b7send", "E\u00b7lend", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So, da\u00df man die erst selig sch\u00e4tzt,", "tokens": ["So", ",", "da\u00df", "man", "die", "erst", "se\u00b7lig", "sch\u00e4tzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit der Welt sich schon geletzt", "tokens": ["Die", "mit", "der", "Welt", "sich", "schon", "ge\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd h\u00f6ren nicht den Gram und Mord", "tokens": ["Vnd", "h\u00f6\u00b7ren", "nicht", "den", "Gram", "und", "Mord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd die Gefahr so fort und fort", "tokens": ["Vnd", "die", "Ge\u00b7fahr", "so", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich blicken l\u00e4sst an allem Ort.", "tokens": ["Sich", "bli\u00b7cken", "l\u00e4sst", "an", "al\u00b7lem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVFIN", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Mensch verschmachtet weit und breit", "tokens": ["Ein", "Mensch", "ver\u00b7schmach\u00b7tet", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Wartung der viel \u00e4rgern Zeit,", "tokens": ["F\u00fcr", "War\u00b7tung", "der", "viel", "\u00e4r\u00b7gern", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00e4ter haben sich beschwehrt", "tokens": ["Die", "V\u00e4\u00b7ter", "ha\u00b7ben", "sich", "be\u00b7schwehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alles sich zur neige kehrt,", "tokens": ["Da\u00df", "al\u00b7les", "sich", "zur", "nei\u00b7ge", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sind wir wol bessern Gl\u00fcckes wehrt?", "tokens": ["Sind", "wir", "wol", "bes\u00b7sern", "Gl\u00fc\u00b7ckes", "wehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O weh uns, wenn der Tod nicht noch", "tokens": ["O", "weh", "uns", ",", "wenn", "der", "Tod", "nicht", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vns spannen solt' aus diesem Joch,", "tokens": ["Vns", "span\u00b7nen", "solt'", "aus", "die\u00b7sem", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir Armen w\u00fcrden \u00fcberein", "tokens": ["Wir", "Ar\u00b7men", "w\u00fcr\u00b7den", "\u00fc\u00b7be\u00b7re\u00b7in"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hie zeitlich stets in Ach und Pein", "tokens": ["Hie", "zeit\u00b7lich", "stets", "in", "Ach", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd ewig dort verlohren seyn.", "tokens": ["Vnd", "e\u00b7wig", "dort", "ver\u00b7loh\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gott aber sey es hoch gedanckt", "tokens": ["Gott", "a\u00b7ber", "sey", "es", "hoch", "ge\u00b7danckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df die\u00dffals unser Trost nicht wanckt,", "tokens": ["Da\u00df", "die\u00df\u00b7fals", "un\u00b7ser", "Trost", "nicht", "wanckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wissen durch der Schrifft bericht,", "tokens": ["Wir", "wis\u00b7sen", "durch", "der", "Schrifft", "be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Christus uns sey Weg und Liecht", "tokens": ["Das", "Chris\u00b7tus", "uns", "sey", "Weg", "und", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd lass' uns in dem Grabe nicht.", "tokens": ["Vnd", "lass'", "uns", "in", "dem", "Gra\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er hat der S\u00fcnden strenge Macht", "tokens": ["Er", "hat", "der", "S\u00fcn\u00b7den", "stren\u00b7ge", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Hell und Todt auch umbgebracht", "tokens": ["Wie", "Hell", "und", "Todt", "auch", "umb\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "KON", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd den erw\u00fcnschten Himmelsstand", "tokens": ["Vnd", "den", "er\u00b7w\u00fcnschten", "Him\u00b7mels\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den noch kein Ohr und Hertz erkandt", "tokens": ["Den", "noch", "kein", "Ohr", "und", "Hertz", "er\u00b7kandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vns durch sein Sterben zugewandt.", "tokens": ["Vns", "durch", "sein", "Ster\u00b7ben", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So lasst uns hie nun unsre Schuld", "tokens": ["So", "lasst", "uns", "hie", "nun", "uns\u00b7re", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd alles tragen mit Gedult", "tokens": ["Vnd", "al\u00b7les", "tra\u00b7gen", "mit", "Ge\u00b7dult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd gl\u00e4uben, da\u00df der Tod allein", "tokens": ["Vnd", "gl\u00e4u\u00b7ben", ",", "da\u00df", "der", "Tod", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werd' uns ein rechter Lebens-Schein", "tokens": ["Werd'", "uns", "ein", "rech\u00b7ter", "Le\u00b7bens\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd alles Tr\u00fcbsals Endschafft seyn!", "tokens": ["Vnd", "al\u00b7les", "Tr\u00fcb\u00b7sals", "End\u00b7schafft", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}