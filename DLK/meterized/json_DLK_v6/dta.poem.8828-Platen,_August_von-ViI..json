{"dta.poem.8828": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Es weht ein Duft vom Ort, wo wir uns fanden, her;", "tokens": ["Es", "weht", "ein", "Duft", "vom", "Ort", ",", "wo", "wir", "uns", "fan\u00b7den", ",", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Winter ist ein Greis, doch schickt der Lenz den", "tokens": ["Der", "Win\u00b7ter", "ist", "ein", "Greis", ",", "doch", "schickt", "der", "Lenz", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Kr\u00e4nze, die wir einst als Kinder wanden, her;", "tokens": ["Der", "Kr\u00e4n\u00b7ze", ",", "die", "wir", "einst", "als", "Kin\u00b7der", "wan\u00b7den", ",", "her", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Angesicht verhei\u00dft des Lenzes Wiederkunft,", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "ver\u00b7hei\u00dft", "des", "Len\u00b7zes", "Wie\u00b7der\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du schickst mir einen Blick, den ich verstanden, her;", "tokens": ["Du", "schickst", "mir", "ei\u00b7nen", "Blick", ",", "den", "ich", "ver\u00b7stan\u00b7den", ",", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "K\u00f6nnt' ich dem Fr\u00fchlingshauch nicht \u00f6ffnen meine Brust,", "tokens": ["K\u00f6nnt'", "ich", "dem", "Fr\u00fch\u00b7lings\u00b7hauch", "nicht", "\u00f6ff\u00b7nen", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo n\u00e4hm' ich solchen Muth in solchen Banden her?", "tokens": ["Wo", "n\u00e4hm'", "ich", "sol\u00b7chen", "Muth", "in", "sol\u00b7chen", "Ban\u00b7den", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "La\u00df tr\u00e4umen uns dahin, wo bald die Rebe bl\u00fcht,", "tokens": ["La\u00df", "tr\u00e4u\u00b7men", "uns", "da\u00b7hin", ",", "wo", "bald", "die", "Re\u00b7be", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "PAV", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und, Knaben, bringt den Wein, der noch vorhanden, her!", "tokens": ["Und", ",", "Kna\u00b7ben", ",", "bringt", "den", "Wein", ",", "der", "noch", "vor\u00b7han\u00b7den", ",", "her", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}