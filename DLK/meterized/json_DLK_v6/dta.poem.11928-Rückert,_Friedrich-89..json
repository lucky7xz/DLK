{"dta.poem.11928": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nichts vom Erdenstaub mehr abzusch\u00fctteln bleibt,", "tokens": ["Wenn", "nichts", "vom", "Er\u00b7dens\u00b7taub", "mehr", "ab\u00b7zu\u00b7sch\u00fct\u00b7teln", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "ADV", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann sich der freie Geist entschwingen lichtgeleibt.", "tokens": ["Kann", "sich", "der", "frei\u00b7e", "Geist", "ent\u00b7schwin\u00b7gen", "licht\u00b7ge\u00b7leibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Solang er sich bestrickt f\u00fchlt vom Unreinen B\u00f6sen,", "tokens": ["So\u00b7lang", "er", "sich", "be\u00b7strickt", "f\u00fchlt", "vom", "Un\u00b7rei\u00b7nen", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mu\u00df er des Lebens Kampf fortk\u00e4mpfen, sich zu l\u00f6sen.", "tokens": ["Mu\u00df", "er", "des", "Le\u00b7bens", "Kampf", "fort\u00b7k\u00e4mp\u00b7fen", ",", "sich", "zu", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVPP", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weh aber ihm, wenn er mu\u00df aus dem Kampfe weichen,", "tokens": ["Weh", "a\u00b7ber", "ihm", ",", "wenn", "er", "mu\u00df", "aus", "dem", "Kamp\u00b7fe", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "KOUS", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Eh er des Lichtes Sieg konnt' an der Nacht erreichen.", "tokens": ["Eh", "er", "des", "Lich\u00b7tes", "Sieg", "konnt'", "an", "der", "Nacht", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Er h\u00fcllt sich ins Gef\u00fchl der Niederlage ein,", "tokens": ["Er", "h\u00fcllt", "sich", "ins", "Ge\u00b7f\u00fchl", "der", "Nie\u00b7der\u00b7la\u00b7ge", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dies wird seine Pein, wo er auch seyn mag, seyn.", "tokens": ["Und", "dies", "wird", "sei\u00b7ne", "Pein", ",", "wo", "er", "auch", "seyn", "mag", ",", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "VAINF", "VMFIN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Darum begl\u00fcckt seid ihr, die ihr hin\u00fcberschwebtet", "tokens": ["Da\u00b7rum", "be\u00b7gl\u00fcckt", "seid", "ihr", ",", "die", "ihr", "hin\u00b7\u00fc\u00b7bersc\u00b7hweb\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVPP", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fr\u00fch, eh ihr tiefer euch hinein ins Leben lebtet.", "tokens": ["Fr\u00fch", ",", "eh", "ihr", "tie\u00b7fer", "euch", "hin\u00b7ein", "ins", "Le\u00b7ben", "leb\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Den Fr\u00fchlingsblumen gleich, im Morgenthau gepfl\u00fcckt,", "tokens": ["Den", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", "gleich", ",", "im", "Mor\u00b7gent\u00b7hau", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit am Festtag man den Tempel Gottes schm\u00fcckt.", "tokens": ["Wo\u00b7mit", "am", "Fest\u00b7tag", "man", "den", "Tem\u00b7pel", "Got\u00b7tes", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PIS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch was am Stengel bleibt und soll zu Fr\u00fcchten reifen,", "tokens": ["Doch", "was", "am", "Sten\u00b7gel", "bleibt", "und", "soll", "zu", "Fr\u00fcch\u00b7ten", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN", "KON", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Schmerzen lass' es sich von Sonn' und Wind ergreifen.", "tokens": ["Mit", "Schmer\u00b7zen", "lass'", "es", "sich", "von", "Sonn'", "und", "Wind", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Auch die unreife Frucht wird abgesch\u00fcttelt werden,", "tokens": ["Auch", "die", "un\u00b7rei\u00b7fe", "Frucht", "wird", "ab\u00b7ge\u00b7sch\u00fct\u00b7telt", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Festmahl kommt sie nicht, sie f\u00e4llt mit Schmach zur Erden.", "tokens": ["Zum", "Fest\u00b7mahl", "kommt", "sie", "nicht", ",", "sie", "f\u00e4llt", "mit", "Schmach", "zur", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}