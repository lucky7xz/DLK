{"textgrid.poem.41634": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sagt mir deines Augs kristallenhelle Zier:", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sagt mir deines Augs kristallenhelle Zier:", "tokens": ["Es", "sagt", "mir", "dei\u00b7nes", "Augs", "kris\u00b7tal\u00b7len\u00b7hel\u00b7le", "Zier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was tat, seltsamer Freund, ich wohl dir zu Gefallen?", "tokens": ["Was", "tat", ",", "selt\u00b7sa\u00b7mer", "Freund", ",", "ich", "wohl", "dir", "zu", "Ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sei anmutvoll und schweig! Mein Herz, das feind ist allen,", "tokens": ["Sei", "an\u00b7mut\u00b7voll", "und", "schweig", "!", "Mein", "Herz", ",", "das", "feind", "ist", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$.", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur nicht der Frau von einst, die einfach wie ein Tier,", "tokens": ["Nur", "nicht", "der", "Frau", "von", "einst", ",", "die", "ein\u00b7fach", "wie", "ein", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "APPR", "ADV", "$,", "PRELS", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zeigt nun und nimmermehr sein schlimm Geheimnis dir,", "tokens": ["Zeigt", "nun", "und", "nim\u00b7mer\u00b7mehr", "sein", "schlimm", "Ge\u00b7heim\u00b7nis", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "PPOSAT", "ADJD", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir, deren Hand mich l\u00e4dt in stumme Traumeshallen,", "tokens": ["Dir", ",", "de\u00b7ren", "Hand", "mich", "l\u00e4dt", "in", "stum\u00b7me", "Trau\u00b7mes\u00b7hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch auch die Glutschrift, wie ich tief dem Gram verfallen,", "tokens": ["Noch", "auch", "die", "Glut\u00b7schrift", ",", "wie", "ich", "tief", "dem", "Gram", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hasse Leidenschaft, und Geist ist Plage mir.", "tokens": ["Ich", "has\u00b7se", "Lei\u00b7den\u00b7schaft", ",", "und", "Geist", "ist", "Pla\u00b7ge", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "NN", "VAFIN", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum la\u00df uns lieben sacht. Aus ihrem Machtgebiete", "tokens": ["Drum", "la\u00df", "uns", "lie\u00b7ben", "sacht", ".", "Aus", "ih\u00b7rem", "Macht\u00b7ge\u00b7bie\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVINF", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt ihren Bogen schon die Liebe stumm gespannt.", "tokens": ["H\u00e4lt", "ih\u00b7ren", "Bo\u00b7gen", "schon", "die", "Lie\u00b7be", "stumm", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr drohend Arsenal ist mir gar wohl bekannt.", "tokens": ["Ihr", "dro\u00b7hend", "Ar\u00b7se\u00b7nal", "ist", "mir", "gar", "wohl", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wahnsinn und Graun \u2013 gleich mir, o Wiesenmarguerite,", "tokens": ["Wahn\u00b7sinn", "und", "Graun", "\u2013", "gleich", "mir", ",", "o", "Wie\u00b7sen\u00b7mar\u00b7gu\u00b7e\u00b7ri\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "ADV", "PPER", "$,", "FM", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Bist eine Sonne du, die herbstlich-bleich entschwand,", "tokens": ["Bist", "ei\u00b7ne", "Son\u00b7ne", "du", ",", "die", "herbst\u00b7lich\u00b7bleich", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O meine wei\u00dfe, meine kalte Marguerite.", "tokens": ["O", "mei\u00b7ne", "wei\u00b7\u00dfe", ",", "mei\u00b7ne", "kal\u00b7te", "Mar\u00b7gu\u00b7e\u00b7ri\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}