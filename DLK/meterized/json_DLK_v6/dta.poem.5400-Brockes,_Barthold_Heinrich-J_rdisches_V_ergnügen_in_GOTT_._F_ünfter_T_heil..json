{"dta.poem.5400": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht mit Lust in lauen L\u00fcften", "tokens": ["Man", "sieht", "mit", "Lust", "in", "lau\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und am bew\u00f6lckten Firmament,", "tokens": ["Und", "am", "be\u00b7w\u00f6lck\u00b7ten", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ein gef\u00e4rbtes Feuer brennt", "tokens": ["Wie", "ein", "ge\u00b7f\u00e4rb\u00b7tes", "Feu\u00b7er", "brennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In hie und dort zerst\u00fcckten D\u00fcften.", "tokens": ["In", "hie", "und", "dort", "zer\u00b7st\u00fcck\u00b7ten", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn man nun, wie in Luft und Erden", "tokens": ["Wenn", "man", "nun", ",", "wie", "in", "Luft", "und", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein sch\u00f6n gef\u00e4rbtes Feuer gl\u00fcht,", "tokens": ["Ein", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7tes", "Feu\u00b7er", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jm Herbst, mit frohen Blicken, sieht;", "tokens": ["Jm", "Herbst", ",", "mit", "fro\u00b7hen", "Bli\u00b7cken", ",", "sieht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So lasset uns doch danckbar werden!", "tokens": ["So", "las\u00b7set", "uns", "doch", "dan\u00b7ck\u00b7bar", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "La\u00dft uns im Herbst, mit froher Seelen,", "tokens": ["La\u00dft", "uns", "im", "Herbst", ",", "mit", "fro\u00b7her", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Sch\u00f6pfer, der die Zeit der Welt", "tokens": ["Den", "Sch\u00f6p\u00b7fer", ",", "der", "die", "Zeit", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In solcher Richtigkeit erh\u00e4lt,", "tokens": ["In", "sol\u00b7cher", "Rich\u00b7tig\u00b7keit", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Besingen und sein Lob erzehlen!", "tokens": ["Be\u00b7sin\u00b7gen", "und", "sein", "Lob", "er\u00b7zeh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}