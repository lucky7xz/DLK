{"dta.poem.16107": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "161.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Je stand in einem Buch dis Gleichnis, lieber Sohn:", "tokens": ["Je", "stand", "in", "ei\u00b7nem", "Buch", "dis", "Gleich\u00b7nis", ",", "lie\u00b7ber", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PDS", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt ist wie ein Wald, dein Thun ist wie ein Ton.", "tokens": ["Die", "Welt", "ist", "wie", "ein", "Wald", ",", "dein", "Thun", "ist", "wie", "ein", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie in den Wald du rufst, so ruft er dir zur\u00fcck,", "tokens": ["Wie", "in", "den", "Wald", "du", "rufst", ",", "so", "ruft", "er", "dir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und also selber schufst du in der Welt dein Gl\u00fcck.", "tokens": ["Und", "al\u00b7so", "sel\u00b7ber", "schufst", "du", "in", "der", "Welt", "dein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn in den Wald du schiltst, wirst du heraus gescholten;", "tokens": ["Wenn", "in", "den", "Wald", "du", "schiltst", ",", "wirst", "du", "he\u00b7raus", "ge\u00b7schol\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie du uns vergiltst, wird wieder dir vergolten.", "tokens": ["Und", "wie", "du", "uns", "ver\u00b7giltst", ",", "wird", "wie\u00b7der", "dir", "ver\u00b7gol\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}