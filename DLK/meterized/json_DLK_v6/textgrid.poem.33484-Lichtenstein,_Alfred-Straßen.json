{"textgrid.poem.33484": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Stra\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Himmel liegt auf allen singenden", "tokens": ["Viel", "Him\u00b7mel", "liegt", "auf", "al\u00b7len", "sin\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Einsamen Stra\u00dfen im Laternenscheine.", "tokens": ["Ein\u00b7sa\u00b7men", "Stra\u00b7\u00dfen", "im", "La\u00b7ter\u00b7nen\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ich schwing im Winde \u00fcber graue Steine,", "tokens": ["Ich", "schwing", "im", "Win\u00b7de", "\u00fc\u00b7ber", "grau\u00b7e", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die spiegeln meinen Schritt, den klingenden.", "tokens": ["Die", "spie\u00b7geln", "mei\u00b7nen", "Schritt", ",", "den", "klin\u00b7gen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sp\u00fcre an der Stirne eiligen", "tokens": ["Ich", "sp\u00fc\u00b7re", "an", "der", "Stir\u00b7ne", "ei\u00b7li\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verhauch von gelben und von dunklen Dingen.", "tokens": ["Ver\u00b7hauch", "von", "gel\u00b7ben", "und", "von", "dunk\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich will die Nacht mit Tr\u00e4umerein verbringen.", "tokens": ["Ich", "will", "die", "Nacht", "mit", "Tr\u00e4u\u00b7me\u00b7rein", "ver\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich f\u00fchl den Mond ... gr\u00fcngoldnen Heiligen.", "tokens": ["Ich", "f\u00fchl", "den", "Mond", "...", "gr\u00fcn\u00b7gold\u00b7nen", "Hei\u00b7li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}