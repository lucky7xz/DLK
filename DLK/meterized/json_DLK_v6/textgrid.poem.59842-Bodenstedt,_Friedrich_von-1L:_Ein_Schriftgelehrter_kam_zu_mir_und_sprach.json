{"textgrid.poem.59842": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Schriftgelehrter kam zu mir und sprach", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schriftgelehrter kam zu mir und sprach", "tokens": ["Ein", "Schrift\u00b7ge\u00b7lehr\u00b7ter", "kam", "zu", "mir", "und", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbmirza-Schaffy, was denkst du von dem Schach?", "tokens": ["\u00bb", "mir\u00b7za\u00b7Schaffy", ",", "was", "denkst", "du", "von", "dem", "Schach", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ist ihm die Weisheit wirklich angeboren,", "tokens": ["Ist", "ihm", "die", "Weis\u00b7heit", "wirk\u00b7lich", "an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ist sein Blick so gro\u00df wie seine Ohren?\u00ab", "tokens": ["Und", "ist", "sein", "Blick", "so", "gro\u00df", "wie", "sei\u00b7ne", "Oh\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u2013 Er ist so weise, wie sie alle sind,", "tokens": ["\u2013", "Er", "ist", "so", "wei\u00b7se", ",", "wie", "sie", "al\u00b7le", "sind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "VVFIN", "$,", "PWAV", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Tr\u00e4gere des Talars und der Kapuze;", "tokens": ["Die", "Tr\u00e4\u00b7ge\u00b7re", "des", "Ta\u00b7lars", "und", "der", "Ka\u00b7pu\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er wei\u00df, wie ehrfurchtsdumm das Volk und blind,", "tokens": ["Er", "wei\u00df", ",", "wie", "ehr\u00b7furchts\u00b7dumm", "das", "Volk", "und", "blind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und diese Dummheit macht er sich zunutze! \u2013", "tokens": ["Und", "die\u00b7se", "Dumm\u00b7heit", "macht", "er", "sich", "zu\u00b7nut\u00b7ze", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}