{"dta.poem.18411": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Printz von Wallis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.85", "tr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das gl\u00fccke hatte mir ein m\u00fchlwerck zugedacht/", "tokens": ["Das", "gl\u00fc\u00b7cke", "hat\u00b7te", "mir", "ein", "m\u00fchl\u00b7werck", "zu\u00b7ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPER", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das dessen eignes rad in gang und trieb gebracht;", "tokens": ["Das", "des\u00b7sen", "eig\u00b7nes", "rad", "in", "gang", "und", "trieb", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELAT", "ADJA", "NN", "APPR", "NN", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch itzo steht das werck und bleibet unvollkommen/", "tokens": ["Doch", "it\u00b7zo", "steht", "das", "werck", "und", "blei\u00b7bet", "un\u00b7voll\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil mir ein guter freund das wasser hat benommen.", "tokens": ["Weil", "mir", "ein", "gu\u00b7ter", "freund", "das", "was\u00b7ser", "hat", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}