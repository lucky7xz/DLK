{"dta.poem.10278": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Verbannt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Es schillert um mich gl&#228;nzend bunt Gefieder,             ", "tokens": ["Es", "schil\u00b7lert", "um", "mich", "gl", "&#228;", "nzend", "bunt", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "NE", "$(", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Palmwald l\u00e4rmt der Affen lustig Heer.", "tokens": ["Im", "Palm\u00b7wald", "l\u00e4rmt", "der", "Af\u00b7fen", "lus\u00b7tig", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Indianer st\u00fctzt die schlanken Glieder", "tokens": ["Der", "In\u00b7di\u00b7a\u00b7ner", "st\u00fctzt", "die", "schlan\u00b7ken", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf\u2019s Rohr, und starrt mit mir hinaus ins Meer.", "tokens": ["Auf's", "Rohr", ",", "und", "starrt", "mit", "mir", "hin\u00b7aus", "ins", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "APPR", "PPER", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und kraftvoll hebt ein Adler seine Schwingen,", "tokens": ["Und", "kraft\u00b7voll", "hebt", "ein", "Ad\u00b7ler", "sei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dreht in blaue Fernen sich empor,", "tokens": ["Und", "dreht", "in", "blau\u00b7e", "Fer\u00b7nen", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als wollt\u2019 er trotzig in den Himmel dringen,", "tokens": ["Als", "wollt'", "er", "trot\u00b7zig", "in", "den", "Him\u00b7mel", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und siegend einziehn durch das Sternenthor.", "tokens": ["Und", "sie\u00b7gend", "ein\u00b7ziehn", "durch", "das", "Ster\u00b7nen\u00b7thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In h\u00f6chsten H\u00f6hen, Adler, mu\u00dft du stehen,", "tokens": ["In", "h\u00f6chs\u00b7ten", "H\u00f6\u00b7hen", ",", "Ad\u00b7ler", ",", "mu\u00dft", "du", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schl\u00e4gt dein Fl\u00fcgel an das Weltendach,", "tokens": ["Es", "schl\u00e4gt", "dein", "Fl\u00fc\u00b7gel", "an", "das", "Wel\u00b7ten\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00dft auf mein Vaterland hinuntersehen,", "tokens": ["Mu\u00dft", "auf", "mein", "Va\u00b7ter\u00b7land", "hin\u00b7un\u00b7ter\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, send\u2019 ihm Gr\u00fc\u00dfe, hei\u00dfe Gr\u00fc\u00dfe nach.", "tokens": ["Ach", ",", "send'", "ihm", "Gr\u00fc\u00b7\u00dfe", ",", "hei\u00b7\u00dfe", "Gr\u00fc\u00b7\u00dfe", "nach", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "NN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Abend liegt es: Strohbedeckte H\u00fctten,", "tokens": ["Im", "A\u00b7bend", "liegt", "es", ":", "Stroh\u00b7be\u00b7deck\u00b7te", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schwalbe ruhelos das Dorf durchzieht.", "tokens": ["Die", "Schwal\u00b7be", "ru\u00b7he\u00b7los", "das", "Dorf", "durch\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Kinder l\u00e4rmen und in Apfelbl\u00fcten", "tokens": ["Die", "Kin\u00b7der", "l\u00e4r\u00b7men", "und", "in", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Singt eine Drossel noch ihr einfach Lied.", "tokens": ["Singt", "ei\u00b7ne", "Dros\u00b7sel", "noch", "ihr", "ein\u00b7fach", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Bauern schl\u00e4frig auf den Pferden h\u00e4ngen,", "tokens": ["Die", "Bau\u00b7ern", "schl\u00e4f\u00b7rig", "auf", "den", "Pfer\u00b7den", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still heimw\u00e4rts kehrend vom gewohnten Pflug.", "tokens": ["Still", "heim\u00b7w\u00e4rts", "keh\u00b7rend", "vom", "ge\u00b7wohn\u00b7ten", "Pflug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Waldestiefen t\u00f6nt es von Ges\u00e4ngen,", "tokens": ["Aus", "Wal\u00b7des\u00b7tie\u00b7fen", "t\u00f6nt", "es", "von", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcber ihnen schwimmt ein Kranichzug.", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7nen", "schwimmt", "ein", "Kra\u00b7nich\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mein Vaterland, k\u00f6nnt\u2019 ich in deinen Feldern", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "k\u00f6nnt'", "ich", "in", "dei\u00b7nen", "Fel\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur einmal h\u00f6ren noch der Sense Schnitt,", "tokens": ["Nur", "ein\u00b7mal", "h\u00f6\u00b7ren", "noch", "der", "Sen\u00b7se", "Schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und durch das welke Laub in deinen W\u00e4ldern", "tokens": ["Und", "durch", "das", "wel\u00b7ke", "Laub", "in", "dei\u00b7nen", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch einmal rauschen h\u00f6ren meinen Schritt.", "tokens": ["Noch", "ein\u00b7mal", "rau\u00b7schen", "h\u00f6\u00b7ren", "mei\u00b7nen", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}