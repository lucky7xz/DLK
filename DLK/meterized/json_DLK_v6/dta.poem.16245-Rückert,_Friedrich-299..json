{"dta.poem.16245": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "299.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wissenschaft verlangt ein heiteres Gem\u00fcte,", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", "ver\u00b7langt", "ein", "hei\u00b7te\u00b7res", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der innern G\u00fcte froh bewu\u00dft und Gottes G\u00fcte.", "tokens": ["Der", "in\u00b7nern", "G\u00fc\u00b7te", "froh", "be\u00b7wu\u00dft", "und", "Got\u00b7tes", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Herz, dem untergieng die Klarheit in der Tr\u00fcbung,", "tokens": ["Ein", "Herz", ",", "dem", "un\u00b7ter\u00b7gieng", "die", "Klar\u00b7heit", "in", "der", "Tr\u00fc\u00b7bung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das heilt nicht Wissenschaft, das heilt allein Bu\u00df\u00fcbung.", "tokens": ["Das", "heilt", "nicht", "Wis\u00b7sen\u00b7schaft", ",", "das", "heilt", "al\u00b7lein", "Bu\u00df\u00b7\u00fc\u00b7bung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "NN", "$,", "PDS", "VVF