{"dta.poem.7808": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Die G\u00fcldene Zeit.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erste ist die G\u00fcldne Zeit auf Erd:", "tokens": ["Die", "Ers\u00b7te", "ist", "die", "G\u00fcld\u00b7ne", "Zeit", "auf", "Erd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da Treu und Recht war ungebotten wehrt.", "tokens": ["Da", "Treu", "und", "Recht", "war", "un\u00b7ge\u00b7bot\u00b7ten", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man wuste noch von Furcht un\u0304 Straffe nicht.", "tokens": ["Man", "wus\u00b7te", "noch", "von", "Furcht", "un\u0304", "Straf\u00b7fe", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Unschuld war kein Kerker zugericht.", "tokens": ["Der", "Un\u00b7schuld", "war", "kein", "Ker\u00b7ker", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein Mensch erschrack vor eines Richters", "tokens": ["Kein", "Mensch", "er\u00b7schrack", "vor", "ei\u00b7nes", "Rich\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ohn Richter/ ward vermieden S\u00fcnd und", "tokens": ["ohn", "Rich\u00b7ter", "/", "ward", "ver\u00b7mie\u00b7den", "S\u00fcnd", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "VAFIN", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es war auch noch die Fichte nicht gef\u00e4llt/", "tokens": ["Es", "war", "auch", "noch", "die", "Fich\u00b7te", "nicht", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zu schiffen hin in eine andre Welt.", "tokens": ["zu", "schif\u00b7fen", "hin", "in", "ei\u00b7ne", "and\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es kennte nur ein jeder seinen Heerd:", "tokens": ["Es", "kenn\u00b7te", "nur", "ein", "je\u00b7der", "sei\u00b7nen", "Heerd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "kein fremder Rauch zu sehen ward begehrt.", "tokens": ["kein", "frem\u00b7der", "Rauch", "zu", "se\u00b7hen", "ward", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder fand/ was er gesucht/ zuhaus.", "tokens": ["Ein", "je\u00b7der", "fand", "/", "was", "er", "ge\u00b7sucht", "/", "zu\u00b7haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$(", "PWS", "PPER", "VVPP", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Man reiste nicht nach fremden Lastern aus.", "tokens": ["Man", "reis\u00b7te", "nicht", "nach", "frem\u00b7den", "Las\u00b7tern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Furcht hatt noch \u00fcmmauret keine Stadt.", "tokens": ["Die", "Furcht", "hatt", "noch", "\u00fcm\u00b7mau\u00b7ret", "kei\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Zorn kein Schwerd und keinen Helm", "tokens": ["Der", "Zorn", "kein", "Schwerd", "und", "kei\u00b7nen", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hole Erz blies auch noch nicht zum", "tokens": ["Das", "ho\u00b7le", "Erz", "blies", "auch", "noch", "nicht", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "PTKNEG", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "(breit.", "tokens": ["(", "breit", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nichts war/ als Ruh und Friede/ weit und", "tokens": ["Nichts", "war", "/", "als", "Ruh", "und", "Frie\u00b7de", "/", "weit", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VAFIN", "$(", "KOUS", "NN", "KON", "NN", "$(", "ADJD", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Erd/ noch nicht verwundet von dem", "tokens": ["Die", "Erd", "/", "noch", "nicht", "ver\u00b7wun\u00b7det", "von", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "PTKNEG", "VVPP", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und unbepflanzt/ die b\u00e4sten Fr\u00fcchte trug.", "tokens": ["und", "un\u00b7be\u00b7pflanzt", "/", "die", "b\u00e4s\u00b7ten", "Fr\u00fcch\u00b7te", "trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man war vergn\u00fcgt mit selbstgewachsner", "tokens": ["Man", "war", "ver\u00b7gn\u00fcgt", "mit", "selbst\u00b7ge\u00b7wachs\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und was gebar ein gr\u00fcnes B\u00e4ume-Reis.", "tokens": ["und", "was", "ge\u00b7bar", "ein", "gr\u00fc\u00b7nes", "B\u00e4u\u00b7me\u00b7Reis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Erdbeer man pflag aufzulesen auch/", "tokens": ["Die", "Erd\u00b7beer", "man", "pflag", "auf\u00b7zu\u00b7le\u00b7sen", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "VVIZU", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und manche Beer sonst mehr von manchem", "tokens": ["und", "man\u00b7che", "Beer", "sonst", "mehr", "von", "man\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADV", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es schmeckten gar die bittren Eicheln s\u00fc\u00df/", "tokens": ["Es", "schmeck\u00b7ten", "gar", "die", "bit\u00b7tren", "Ei\u00b7cheln", "s\u00fc\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die Dodons Hayn von Aesten fallen lie\u00df.", "tokens": ["die", "Do\u00b7dons", "Hayn", "von", "A\u00b7es\u00b7ten", "fal\u00b7len", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Es w\u00e4hrte st\u00e4ts die sch\u00f6ne Fr\u00fclingszeit.", "tokens": ["Es", "w\u00e4hr\u00b7te", "st\u00e4ts", "die", "sch\u00f6\u00b7ne", "Fr\u00fc\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der s\u00fcsse West blies durch die Blumen-heid:", "tokens": ["Der", "s\u00fcs\u00b7se", "West", "blies", "durch", "die", "Blu\u00b7men\u00b7heid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Nordes-grim\u0304/ ein Tod der Kr\u00e4uter war.", "tokens": ["Kein", "Nor\u00b7des\u00b7grim\u0304", "/", "ein", "Tod", "der", "Kr\u00e4u\u00b7ter", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ungebaut/ die Erde Frucht gebar.", "tokens": ["Und", "un\u00b7ge\u00b7baut", "/", "die", "Er\u00b7de", "Frucht", "ge\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Im Feld stund st\u00e4ts der Aehre\u0304 schweres Gold.", "tokens": ["Im", "Feld", "stund", "st\u00e4ts", "der", "A\u00b7ehr\u0113", "schwe\u00b7res", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man fand allzeit/ wz jeder w\u00fcnscht\u2019 un\u0304 wolt.", "tokens": ["Man", "fand", "all\u00b7zeit", "/", "wz", "je\u00b7der", "w\u00fcnscht'", "un\u0304", "wolt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$(", "PWS", "PIS", "VVFIN", "ADJD", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es floss der Strom/ mit Milch und Nectar-", "tokens": ["Es", "floss", "der", "Strom", "/", "mit", "Milch", "und", "Nec\u00b7ta\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es schenkten auch die B\u00e4ume Honig ein.", "tokens": ["Es", "schenk\u00b7ten", "auch", "die", "B\u00e4u\u00b7me", "Ho\u00b7nig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}