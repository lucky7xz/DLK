{"textgrid.poem.42528": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Sphinx in Rosen", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umschattet von des Gartens Riesenb\u00e4umen,", "tokens": ["Um\u00b7schat\u00b7tet", "von", "des", "Gar\u00b7tens", "Rie\u00b7sen\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruht eine Sphinx aus blendend wei\u00dfem Steine,", "tokens": ["Ruht", "ei\u00b7ne", "Sphinx", "aus", "blen\u00b7dend", "wei\u00b7\u00dfem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leicht \u00fcberhaucht vom warmen Widerscheine", "tokens": ["Leicht", "\u00fc\u00b7ber\u00b7haucht", "vom", "war\u00b7men", "Wi\u00b7der\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der tausend Rosen, die sie dicht umz\u00e4unen.", "tokens": ["Der", "tau\u00b7send", "Ro\u00b7sen", ",", "die", "sie", "dicht", "um\u00b7z\u00e4u\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "PRELS", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verdrossen, finster und in dumpfem Tr\u00e4umen,", "tokens": ["Ver\u00b7dros\u00b7sen", ",", "fins\u00b7ter", "und", "in", "dum\u00b7pfem", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So br\u00fctet starr sie \u00fcber das geheime,", "tokens": ["So", "br\u00fc\u00b7tet", "starr", "sie", "\u00fc\u00b7ber", "das", "ge\u00b7hei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ewige R\u00e4tsel. Und der Bl\u00fcten eine,", "tokens": ["Das", "e\u00b7wi\u00b7ge", "R\u00e4t\u00b7sel", ".", "Und", "der", "Bl\u00fc\u00b7ten", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ART", "NN", "ART", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sich schalkhaft wiegend, spricht: \u00bbWas willst du s\u00e4umen?", "tokens": ["Sich", "schalk\u00b7haft", "wie\u00b7gend", ",", "spricht", ":", "\u00bb", "Was", "willst", "du", "s\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$,", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So find und gib uns endlich doch die L\u00f6sung!\u00ab", "tokens": ["So", "find", "und", "gib", "uns", "end\u00b7lich", "doch", "die", "L\u00f6\u00b7sung", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVIMP", "PPER", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Winde schaukelten die andern Rosen.", "tokens": ["Im", "Win\u00b7de", "schau\u00b7kel\u00b7ten", "die", "an\u00b7dern", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da, gr\u00e4\u00dflich, klang das eine Wort: Verwesung.", "tokens": ["Da", ",", "gr\u00e4\u00df\u00b7lich", ",", "klang", "das", "ei\u00b7ne", "Wort", ":", "Ver\u00b7we\u00b7sung", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "VVFIN", "ART", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbnein, Liebe ists!\u00ab erwiderten die losen;", "tokens": ["\u00bb", "nein", ",", "Lie\u00b7be", "ists", "!", "\u00ab", "er\u00b7wi\u00b7der\u00b7ten", "die", "lo\u00b7sen", ";"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "VAFIN", "$.", "$(", "VVFIN", "ART", "VVINF", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "\u00bbla\u00df dirs gesagt sein, greulichste der Katzen.\u00ab", "tokens": ["\u00bb", "la\u00df", "dirs", "ge\u00b7sagt", "sein", ",", "greu\u00b7lichs\u00b7te", "der", "Kat\u00b7zen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PIS", "VVPP", "VAINF", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch schmeichelnd k\u00fc\u00dften sie des Untiers Tatzen.", "tokens": ["Doch", "schmei\u00b7chelnd", "k\u00fc\u00df\u00b7ten", "sie", "des", "Un\u00b7tiers", "Tat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}