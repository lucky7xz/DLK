{"textgrid.poem.48190": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Gast", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Kind ist krank zum Sterben,", "tokens": ["Das", "Kind", "ist", "krank", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lampe gibt tr\u00e4gen Schein,", "tokens": ["Die", "Lam\u00b7pe", "gibt", "tr\u00e4\u00b7gen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Mutter spricht: \u00bbMir ist es,", "tokens": ["Die", "Mut\u00b7ter", "spricht", ":", "\u00bb", "Mir", "ist", "es", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4ren wir nicht allein.\u00ab", "tokens": ["Als", "w\u00e4\u00b7ren", "wir", "nicht", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PTKNEG", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Vater sucht zu l\u00e4cheln,", "tokens": ["Der", "Va\u00b7ter", "sucht", "zu", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch im Herzen pocht's ihm bang,", "tokens": ["Doch", "im", "Her\u00b7zen", "pocht's", "ihm", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiller wird's und stiller \u2013", "tokens": ["Stil\u00b7ler", "wird's", "und", "stil\u00b7ler", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die Nacht ist gar zu lang.", "tokens": ["Die", "Nacht", "ist", "gar", "zu", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun scheint der Tag ins Fenster,", "tokens": ["Nun", "scheint", "der", "Tag", "ins", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6gel singen so klar;", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "so", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die beiden wu\u00dften lange,", "tokens": ["Die", "bei\u00b7den", "wu\u00df\u00b7ten", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer der Gast gewesen war.", "tokens": ["Wer", "der", "Gast", "ge\u00b7we\u00b7sen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}