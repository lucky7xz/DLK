{"textgrid.poem.47055": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Sommer drau\u00dfen, als durch Busch und Hecken", "tokens": ["Im", "Som\u00b7mer", "drau\u00b7\u00dfen", ",", "als", "durch", "Busch", "und", "He\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf deinen Fu\u00dftritt meiner sich erpichte,", "tokens": ["Auf", "dei\u00b7nen", "Fu\u00df\u00b7tritt", "mei\u00b7ner", "sich", "er\u00b7pich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beklagt' ich deine Sch\u00f6nheit, da\u00df zunichte", "tokens": ["Be\u00b7klagt'", "ich", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "da\u00df", "zu\u00b7nich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Daran ein Teilchen ward durch Sommerflecken.", "tokens": ["Da\u00b7ran", "ein", "Teil\u00b7chen", "ward", "durch", "Som\u00b7mer\u00b7fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt wie dich die Erinnerungen wecken", "tokens": ["Jetzt", "wie", "dich", "die", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor meinem Geiste, staun' ich, wie im Lichte", "tokens": ["Vor", "mei\u00b7nem", "Geis\u00b7te", ",", "staun'", "ich", ",", "wie", "im", "Lich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du dastehst mit so reinem Angesichte,", "tokens": ["Du", "das\u00b7tehst", "mit", "so", "rei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich kein einz'ges Fleckchen kann entdecken.", "tokens": ["Da\u00df", "ich", "kein", "einz'\u00b7ges", "Fleck\u00b7chen", "kann", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was ist das? Ist es wohl der keusche Winter,", "tokens": ["Was", "ist", "das", "?", "Ist", "es", "wohl", "der", "keu\u00b7sche", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$.", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mit dem Schneeglanz deine Flecken sauber", "tokens": ["Der", "mit", "dem", "Schnee\u00b7glanz", "dei\u00b7ne", "Fle\u00b7cken", "sau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gemacht hat, da\u00df du strahlst als wie die Lilien?", "tokens": ["Ge\u00b7macht", "hat", ",", "da\u00df", "du", "strahlst", "als", "wie", "die", "Li\u00b7li\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O nein! Ein Quell ist das, aus Himmeln rinnt er,", "tokens": ["O", "nein", "!", "Ein", "Quell", "ist", "das", ",", "aus", "Him\u00b7meln", "rinnt", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "ART", "NN", "VAFIN", "PDS", "$,", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der tr\u00e4gt von Ewigkeit in sich den Zauber,", "tokens": ["Der", "tr\u00e4gt", "von", "E\u00b7wig\u00b7keit", "in", "sich", "den", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er kann ird'scher Sch\u00f6nheit Flecken tilgen.", "tokens": ["Da\u00df", "er", "kann", "ird'\u00b7scher", "Sch\u00f6n\u00b7heit", "Fle\u00b7cken", "til\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}