{"textgrid.poem.53592": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Badetag", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie munter ist das in Berlin!", "tokens": ["Wie", "mun\u00b7ter", "ist", "das", "in", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hauswirt, schwer gepeinigt,", "tokens": ["Der", "Haus\u00b7wirt", ",", "schwer", "ge\u00b7pei\u00b7nigt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "l\u00e4\u00dft freitags warmes Wasser ziehn,", "tokens": ["l\u00e4\u00dft", "frei\u00b7tags", "war\u00b7mes", "Was\u00b7ser", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und jeder wird gereinigt.", "tokens": ["und", "je\u00b7der", "wird", "ge\u00b7rei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es baden sich zu gleicher Zeit", "tokens": ["Es", "ba\u00b7den", "sich", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wohl hunderttausend Beine,", "tokens": ["wohl", "hun\u00b7dert\u00b7tau\u00b7send", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "die B\u00fcrgerschaft, die Obrigkeit", "tokens": ["die", "B\u00fcr\u00b7ger\u00b7schaft", ",", "die", "Ob\u00b7rig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und selbst Herrn Heine seine.", "tokens": ["und", "selbst", "Herrn", "Hei\u00b7ne", "sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "NE", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Fern Andra w\u00e4scht sich. Ebert auch.", "tokens": ["Fern", "And\u00b7ra", "w\u00e4scht", "sich", ".", "E\u00b7bert", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "$.", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er sp\u00fclt sich heiter seinen Bauch", "tokens": ["Er", "sp\u00fclt", "sich", "hei\u00b7ter", "sei\u00b7nen", "Bauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "und denkt: Es kann nichts schaden \u2013", "tokens": ["und", "denkt", ":", "Es", "kann", "nichts", "scha\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "du k\u00f6nntst mal wieder baden . . .", "tokens": ["du", "k\u00f6nntst", "mal", "wie\u00b7der", "ba\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und nun sitzt er in der Wanne und nun w\u00e4scht er sich", "tokens": ["Und", "nun", "sitzt", "er", "in", "der", "Wan\u00b7ne", "und", "nun", "w\u00e4scht", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "und b\u00fcrstet nicht zu knapp.", "tokens": ["und", "b\u00fcrs\u00b7tet", "nicht", "zu", "knapp", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und planscht und manscht und seift sich ein", "tokens": ["Und", "planscht", "und", "manscht", "und", "seift", "sich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "NN", "KON", "VVFIN", "PRF", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "und schwemmt sich wieder ab!", "tokens": ["und", "schwemmt", "sich", "wie\u00b7der", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frau Durieux pl\u00e4tschert. Rauscher braust", "tokens": ["Frau", "Du\u00b7ri\u00b7eux", "pl\u00e4t\u00b7schert", ".", "Rau\u00b7scher", "braust"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "VVFIN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "(viel Strahlen \u2013 wenig Wasser).", "tokens": ["(", "viel", "Strah\u00b7len", "\u2013", "we\u00b7nig", "Was\u00b7ser", ")", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "$(", "PIAT", "NN", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kahl f\u00fcrchtet, da\u00df sein Bart zerzaust \u2013", "tokens": ["Kahl", "f\u00fcrch\u00b7tet", ",", "da\u00df", "sein", "Bart", "zer\u00b7zaust", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er ist ein Badehasser.", "tokens": ["er", "ist", "ein", "Ba\u00b7de\u00b7has\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Orska wird im Bad rasiert.", "tokens": ["Die", "O\u00b7rska", "wird", "im", "Bad", "ra\u00b7siert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPRART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei Veidtens filmt es einer.", "tokens": ["Bei", "Veid\u00b7tens", "filmt", "es", "ei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur M\u00e4xchen Pallenberg markiert \u2013", "tokens": ["Nur", "M\u00e4x\u00b7chen", "Pal\u00b7len\u00b7berg", "mar\u00b7kiert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "es sieht ja schlie\u00dflich keiner!", "tokens": ["es", "sieht", "ja", "schlie\u00df\u00b7lich", "kei\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Auch Noske spricht zum Adjutanten:", "tokens": ["Auch", "Nos\u00b7ke", "spricht", "zum", "Ad\u00b7ju\u00b7tan\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbverpatzen Sie derweil das Land!\u00ab", "tokens": ["\u00bb", "ver\u00b7pat\u00b7zen", "Sie", "der\u00b7weil", "das", "Land", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und denkt: Es kann nichts schaden \u2013", "tokens": ["Und", "denkt", ":", "Es", "kann", "nichts", "scha\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "du k\u00f6nntst mal wieder baden . . .", "tokens": ["du", "k\u00f6nntst", "mal", "wie\u00b7der", "ba\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und nun sitzt er in der Wanne und nun w\u00e4scht er sich", "tokens": ["Und", "nun", "sitzt", "er", "in", "der", "Wan\u00b7ne", "und", "nun", "w\u00e4scht", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "und b\u00fcrstet nicht zu knapp.", "tokens": ["und", "b\u00fcrs\u00b7tet", "nicht", "zu", "knapp", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und planscht und manscht und seift sich ein \u2013", "tokens": ["Und", "planscht", "und", "manscht", "und", "seift", "sich", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "NN", "KON", "VVFIN", "PRF", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "doch die Flecke gehn nicht ab!", "tokens": ["doch", "die", "Fle\u00b7cke", "gehn", "nicht", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es baden Fuhr- und Bassermann,", "tokens": ["Es", "ba\u00b7den", "Fuhr", "und", "Bas\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "TRUNC", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "frottiert wird zart Frau Porten.", "tokens": ["frot\u00b7tiert", "wird", "zart", "Frau", "Por\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADJD", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Fischer trieft als nasser Mann \u2013", "tokens": ["Ein", "Fi\u00b7scher", "trieft", "als", "nas\u00b7ser", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie baden aller Orten.", "tokens": ["sie", "ba\u00b7den", "al\u00b7ler", "Or\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gar manche sehr bekannte Frau", "tokens": ["Gar", "man\u00b7che", "sehr", "be\u00b7kann\u00b7te", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "montiert sich ab die Locken.", "tokens": ["mon\u00b7tiert", "sich", "ab", "die", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auch Kl\u00f6pfer nimmts nicht so genau \u2013", "tokens": ["Auch", "Kl\u00f6p\u00b7fer", "nimmts", "nicht", "so", "ge\u00b7nau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "er sitzt nicht gerne trocken.", "tokens": ["er", "sitzt", "nicht", "ger\u00b7ne", "tro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Selbst Ludendorff steigt tapfer rein;", "tokens": ["Selbst", "Lu\u00b7den\u00b7dorff", "steigt", "tap\u00b7fer", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "weil er das kann, seift er sich ein.", "tokens": ["weil", "er", "das", "kann", ",", "seift", "er", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "$,", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und ganz Berlin denkt: Schaden", "tokens": ["Und", "ganz", "Ber\u00b7lin", "denkt", ":", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "NE", "VVFIN", "$.", "NN"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.12": {"text": "kanns nichts, wir wolln mal baden . . .", "tokens": ["kanns", "nichts", ",", "wir", "wolln", "mal", "ba\u00b7den", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "PIS", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und sie sitzen in der Wanne und sie waschen sich", "tokens": ["Und", "sie", "sit\u00b7zen", "in", "der", "Wan\u00b7ne", "und", "sie", "wa\u00b7schen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "PPER", "VVFIN", "PRF"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "und nehmen Bad an Bad.", "tokens": ["und", "neh\u00b7men", "Bad", "an", "Bad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Die Sintflut t\u00e4t es schlie\u00dflich auch!", "tokens": ["Die", "Sint\u00b7flut", "t\u00e4t", "es", "schlie\u00df\u00b7lich", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Gott segne diese Stadt \u2013!", "tokens": ["Gott", "seg\u00b7ne", "die\u00b7se", "Stadt", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}