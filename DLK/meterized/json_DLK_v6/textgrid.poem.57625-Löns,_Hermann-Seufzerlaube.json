{"textgrid.poem.57625": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Seufzerlaube", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt's, da\u00df heute so zerstreut ich bin?", "tokens": ["Wie", "kommt's", ",", "da\u00df", "heu\u00b7te", "so", "zer\u00b7streut", "ich", "bin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "ADV", "ADV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du gr\u00fcner Buchenwald, du liegst mir im Sinn,", "tokens": ["Du", "gr\u00fc\u00b7ner", "Bu\u00b7chen\u00b7wald", ",", "du", "liegst", "mir", "im", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Du alte, laubverdeckte Seufzerlaube;", "tokens": ["Du", "al\u00b7te", ",", "laub\u00b7ver\u00b7deck\u00b7te", "Seuf\u00b7zer\u00b7lau\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sonnenschimmer \u00fcbermalt den See,", "tokens": ["Der", "Son\u00b7nen\u00b7schim\u00b7mer", "\u00fc\u00b7ber\u00b7malt", "den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lautrufend kreist der Bussard in der H\u00f6h',", "tokens": ["Laut\u00b7ru\u00b7fend", "kreist", "der", "Bus\u00b7sard", "in", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und tief im Klotzow ruckst die Ringeltaube.", "tokens": ["Und", "tief", "im", "Klot\u00b7zow", "ruckst", "die", "Rin\u00b7gel\u00b7tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lang' ist es her, seit ich dort tr\u00e4umend sa\u00df", "tokens": ["Lang'", "ist", "es", "her", ",", "seit", "ich", "dort", "tr\u00e4u\u00b7mend", "sa\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Strafarbeit und Karzert\u00fcr verga\u00df", "tokens": ["Und", "Straf\u00b7ar\u00b7beit", "und", "Kar\u00b7zer\u00b7t\u00fcr", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bei Drosselschlag aus gr\u00fcnbeschwerten \u00c4sten;", "tokens": ["Bei", "Dros\u00b7sel\u00b7schlag", "aus", "gr\u00fcn\u00b7be\u00b7schwer\u00b7ten", "\u00c4s\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein schwarzwei\u00dfrotes Kielboot zog vorbei;", "tokens": ["Ein", "schwarz\u00b7wei\u00df\u00b7ro\u00b7tes", "Kiel\u00b7boot", "zog", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es schallte Heines Lied der Loreley,", "tokens": ["Es", "schall\u00b7te", "Hei\u00b7nes", "Lied", "der", "Lo\u00b7re\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und rote Lichter funkelten im Westen.", "tokens": ["Und", "ro\u00b7te", "Lich\u00b7ter", "fun\u00b7kel\u00b7ten", "im", "Wes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Grad' vor der Laube steht ein Lindenbaum,", "tokens": ["Grad'", "vor", "der", "Lau\u00b7be", "steht", "ein", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Rindenschnitt an ihm erkennt man kaum,", "tokens": ["Den", "Rin\u00b7den\u00b7schnitt", "an", "ihm", "er\u00b7kennt", "man", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sechs Jahresringe zeitigte die Linde;", "tokens": ["Sechs", "Jah\u00b7res\u00b7rin\u00b7ge", "zei\u00b7tig\u00b7te", "die", "Lin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Juni war's, jung war das Buchenlaub,", "tokens": ["Im", "Ju\u00b7ni", "wa\u00b7r's", ",", "jung", "war", "das", "Bu\u00b7chen\u00b7laub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Winter kam mit Frost und Flugschneestaub,", "tokens": ["Der", "Win\u00b7ter", "kam", "mit", "Frost", "und", "Flug\u00b7schnee\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die braunen Bl\u00e4tter tanzten wild im Winde.", "tokens": ["Die", "brau\u00b7nen", "Bl\u00e4t\u00b7ter", "tanz\u00b7ten", "wild", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Leb' wohl! Des Tr\u00e4umens ist schon l\u00e4ngst genug,", "tokens": ["Leb'", "wohl", "!", "Des", "Tr\u00e4u\u00b7mens", "ist", "schon", "l\u00e4ngst", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich lese weiter in dem trocknen Buch \u2013", "tokens": ["Ich", "le\u00b7se", "wei\u00b7ter", "in", "dem", "trock\u00b7nen", "Buch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann bin ich wieder in der Seufzerlaube?", "tokens": ["Wann", "bin", "ich", "wie\u00b7der", "in", "der", "Seuf\u00b7zer\u00b7lau\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht, wenn Bart und Haare lange grau,", "tokens": ["Viel\u00b7leicht", ",", "wenn", "Bart", "und", "Haa\u00b7re", "lan\u00b7ge", "grau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn tot du oder eines andern Frau,", "tokens": ["Wenn", "tot", "du", "o\u00b7der", "ei\u00b7nes", "an\u00b7dern", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch tief im Klotzow ruckst dann noch die Taube.", "tokens": ["Doch", "tief", "im", "Klot\u00b7zow", "ruckst", "dann", "noch", "die", "Tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NE", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}