{"textgrid.poem.39996": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Bruder, lieber Mensch, (verwundere dich nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Bruder, lieber Mensch, (verwundere dich nicht,", "tokens": ["Mein", "Bru\u00b7der", ",", "lie\u00b7ber", "Mensch", ",", "(", "ver\u00b7wun\u00b7de\u00b7re", "dich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "NN", "$,", "$(", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df meine Wenigkeit zu dir: mein Bruder! spricht.", "tokens": ["Da\u00df", "mei\u00b7ne", "We\u00b7nig\u00b7keit", "zu", "dir", ":", "mein", "Bru\u00b7der", "!", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "$.", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe Recht dazu, du wirst es selbst gestehen,", "tokens": ["Ich", "ha\u00b7be", "Recht", "da\u00b7zu", ",", "du", "wirst", "es", "selbst", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PAV", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn du mich angeh\u00f6rt, und mich recht angesehen.)", "tokens": ["Wenn", "du", "mich", "an\u00b7ge\u00b7h\u00f6rt", ",", "und", "mich", "recht", "an\u00b7ge\u00b7se\u00b7hen", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,", "KON", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Bruder, sprech' ich denn noch einmahl, sage mir,", "tokens": ["Mein", "Bru\u00b7der", ",", "sprech'", "ich", "denn", "noch", "ein\u00b7mahl", ",", "sa\u00b7ge", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie kommst du dir so gro\u00df, ich so ver\u00e4chtlich, f\u00fcr?", "tokens": ["Wie", "kommst", "du", "dir", "so", "gro\u00df", ",", "ich", "so", "ver\u00b7\u00e4cht\u00b7lich", ",", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "PPER", "ADV", "ADJD", "$,", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind wir, durch eines Sch\u00f6pfers Macht,", "tokens": ["Sind", "wir", ",", "durch", "ei\u00b7nes", "Sch\u00f6p\u00b7fers", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht alle beyd' hervor gebracht?", "tokens": ["Nicht", "al\u00b7le", "beyd'", "her\u00b7vor", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "PIS", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist deine Mutter nicht die Erde, so wie meine?", "tokens": ["Ist", "dei\u00b7ne", "Mut\u00b7ter", "nicht", "die", "Er\u00b7de", ",", "so", "wie", "mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "$,", "ADV", "KOKOM", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Werd' ich von ihr nicht auch sowohl, als du, gen\u00e4hrt?", "tokens": ["Werd'", "ich", "von", "ihr", "nicht", "auch", "so\u00b7wohl", ",", "als", "du", ",", "ge\u00b7n\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie dein, ist auch mein, Leib mit Adern gantz durchr\u00f6hrt,", "tokens": ["Wie", "dein", ",", "ist", "auch", "mein", ",", "Leib", "mit", "A\u00b7dern", "gantz", "durch\u00b7r\u00f6hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "$,", "VAFIN", "ADV", "PPOSAT", "$,", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und diese sind mit Saft so wohl gef\u00fcllt, als deine.", "tokens": ["Und", "die\u00b7se", "sind", "mit", "Saft", "so", "wohl", "ge\u00b7f\u00fcllt", ",", "als", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "APPR", "NN", "ADV", "ADV", "VVPP", "$,", "KOUS", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich habe zwar nur eins, du aber hast zwey Beine;", "tokens": ["Ich", "ha\u00b7be", "zwar", "nur", "eins", ",", "du", "a\u00b7ber", "hast", "zwey", "Bei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIS", "$,", "PPER", "ADV", "VAFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch \u00fcberhebe dich des Vorzugs halber nicht,", "tokens": ["Doch", "\u00fc\u00b7ber\u00b7he\u00b7be", "dich", "des", "Vor\u00b7zugs", "hal\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPO", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil sonst ein Ochs' zu dir", "tokens": ["Weil", "sonst", "ein", "Ochs'", "zu", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Mit ja so grossem Rechte spricht:", "tokens": ["Mit", "ja", "so", "gros\u00b7sem", "Rech\u00b7te", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wie karg ist gegen dich die g\u00fctige Natur,", "tokens": ["Wie", "karg", "ist", "ge\u00b7gen", "dich", "die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Armselige zweybeinigte Figur!", "tokens": ["Arm\u00b7se\u00b7li\u00b7ge", "zwey\u00b7bei\u00b7nig\u00b7te", "Fi\u00b7gur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Hab' ich nicht ihrer vier?", "tokens": ["Hab'", "ich", "nicht", "ih\u00b7rer", "vier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PPOSAT", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Sprich ferner nicht: ", "tokens": ["Sprich", "fer\u00b7ner", "nicht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Sprich, sag' ich, nicht also: sonst werd' ich V\u00f6gel kriegen,", "tokens": ["Sprich", ",", "sag'", "ich", ",", "nicht", "al\u00b7so", ":", "sonst", "werd'", "ich", "V\u00f6\u00b7gel", "krie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "ADV", "$.", "ADV", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Die sagen: Ist der Mensch nicht plump? er kann nicht fliegen!", "tokens": ["Die", "sa\u00b7gen", ":", "Ist", "der", "Mensch", "nicht", "plump", "?", "er", "kann", "nicht", "flie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$.", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ey, pochst du, gantz vom Eifer roth:", "tokens": ["Ey", ",", "pochst", "du", ",", "gantz", "vom", "Ei\u00b7fer", "roth", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du nicht auch, wie wir, vielleicht schon morgen todt,", "tokens": ["Bist", "du", "nicht", "auch", ",", "wie", "wir", ",", "viel\u00b7leicht", "schon", "mor\u00b7gen", "todt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und must du nicht so wohl zur Erden,", "tokens": ["Und", "must", "du", "nicht", "so", "wohl", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich mit meinen Bl\u00e4ttern, werden?", "tokens": ["Als", "ich", "mit", "mei\u00b7nen", "Bl\u00e4t\u00b7tern", ",", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es richten dich annoch mehr F\u00e4ll', als uns, zu Grunde.", "tokens": ["Es", "rich\u00b7ten", "dich", "an\u00b7noch", "mehr", "F\u00e4ll'", ",", "als", "uns", ",", "zu", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "$,", "APPR", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ach h\u00f6re, lieber Mensch, mit meinem stummen Munde", "tokens": ["Ach", "h\u00f6\u00b7re", ",", "lie\u00b7ber", "Mensch", ",", "mit", "mei\u00b7nem", "stum\u00b7men", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "ADV", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lob' ich den Sch\u00f6pfer mehr, als du.", "tokens": ["Lob'", "ich", "den", "Sch\u00f6p\u00b7fer", "mehr", ",", "als", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will nicht einst von meiner Sch\u00f6nheit sagen,", "tokens": ["Ich", "will", "nicht", "einst", "von", "mei\u00b7ner", "Sch\u00f6n\u00b7heit", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Worin der Vorzug ja unstreitig mir geb\u00fchrt,", "tokens": ["Wo\u00b7rin", "der", "Vor\u00b7zug", "ja", "un\u00b7strei\u00b7tig", "mir", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht von dem lieblichen Geruche, der dich r\u00fchrt;", "tokens": ["Nicht", "von", "dem", "lieb\u00b7li\u00b7chen", "Ge\u00b7ru\u00b7che", ",", "der", "dich", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn, wie mich deucht, so h\u00f6r' ich dich schon fragen,", "tokens": ["Denn", ",", "wie", "mich", "deucht", ",", "so", "h\u00f6r'", "ich", "dich", "schon", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und zwar nicht sonder Heftigkeit:", "tokens": ["Und", "zwar", "nicht", "son\u00b7der", "Hef\u00b7tig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Art, wie ich gedenck', ist anders zwar, als deine,", "tokens": ["Die", "Art", ",", "wie", "ich", "ge\u00b7denck", ",", "ist", "an\u00b7ders", "zwar", ",", "als", "dei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das geb' ich zu;", "tokens": ["Das", "geb'", "ich", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Alleine", "tokens": ["Al\u00b7lei\u00b7ne"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wofern auch du,", "tokens": ["Wo\u00b7fern", "auch", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wenn du mich siehst, nicht gleich dein Dencken lenckest", "tokens": ["Wenn", "du", "mich", "siehst", ",", "nicht", "gleich", "dein", "Den\u00b7cken", "len\u00b7ckest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf Den, Der uns gemacht,", "tokens": ["Auf", "Den", ",", "Der", "uns", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und an den Sch\u00f6pfer nicht gedenckest,", "tokens": ["Und", "an", "den", "Sch\u00f6p\u00b7fer", "nicht", "ge\u00b7den\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der uns so wunderbar hervor gebracht,", "tokens": ["Der", "uns", "so", "wun\u00b7der\u00b7bar", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der dir dein Wesen so, wie meines mir, gegeben;", "tokens": ["Der", "dir", "dein", "We\u00b7sen", "so", ",", "wie", "mei\u00b7nes", "mir", ",", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "ADV", "$,", "PWAV", "PIS", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So hast du, glaub' es mir, in deinem gantzen Leben", "tokens": ["So", "hast", "du", ",", "glaub'", "es", "mir", ",", "in", "dei\u00b7nem", "gant\u00b7zen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nicht weniger, als ich, so gut als nichts, gedacht.", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", ",", "als", "ich", ",", "so", "gut", "als", "nichts", ",", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPER", "$,", "ADV", "ADJD", "KOKOM", "PIS", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}