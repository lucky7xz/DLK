{"dta.poem.14896": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Meeresstille .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' von des Schiffes Rande", "tokens": ["Ich", "seh'", "von", "des", "Schif\u00b7fes", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tief in die Fluth hinein:", "tokens": ["Tief", "in", "die", "Fluth", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gebirge und gr\u00fcne Lande", "tokens": ["Ge\u00b7bir\u00b7ge", "und", "gr\u00fc\u00b7ne", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Tr\u00fcmmer im falben Schein", "tokens": ["Und", "Tr\u00fcm\u00b7mer", "im", "fal\u00b7ben", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und zackige Th\u00fcrme im Grunde,", "tokens": ["Und", "za\u00b7cki\u00b7ge", "Th\u00fcr\u00b7me", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wie ich's oft im Traum mir gedacht,", "tokens": ["Wie", "ich's", "oft", "im", "Traum", "mir", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Das d\u00e4mmert alles da unten", "tokens": ["Das", "d\u00e4m\u00b7mert", "al\u00b7les", "da", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Als wie eine pr\u00e4chtige Nacht.", "tokens": ["Als", "wie", "ei\u00b7ne", "pr\u00e4ch\u00b7ti\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}