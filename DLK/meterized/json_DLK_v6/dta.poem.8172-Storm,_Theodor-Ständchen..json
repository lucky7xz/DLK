{"dta.poem.8172": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "St\u00e4ndchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Auf den feuchten Wiesenplanen;", "tokens": ["Auf", "den", "feuch\u00b7ten", "Wie\u00b7sen\u00b7pla\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du die Guitarre stimmen", "tokens": ["H\u00f6rst", "du", "die", "Gui\u00b7tar\u00b7re", "stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Schatten der Platanen?", "tokens": ["In", "dem", "Schat\u00b7ten", "der", "Pla\u00b7ta\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dreizehn Lieder sollst du h\u00f6ren,", "tokens": ["Drei\u00b7zehn", "Lie\u00b7der", "sollst", "du", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dreizehn Lieder frisch gedichtet;", "tokens": ["Drei\u00b7zehn", "Lie\u00b7der", "frisch", "ge\u00b7dich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle sind, ich kann's beschw\u00f6ren,", "tokens": ["Al\u00b7le", "sind", ",", "ich", "kann's", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle nur an dich gerichtet.", "tokens": ["Al\u00b7le", "nur", "an", "dich", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "An dem zarten schlanken Leibchen", "tokens": ["An", "dem", "zar\u00b7ten", "schlan\u00b7ken", "Leib\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zur Stirne auf und nieder,", "tokens": ["Bis", "zur", "Stir\u00b7ne", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes F\u00fcnkchen, jedes St\u00e4ubchen,", "tokens": ["Je\u00b7des", "F\u00fcnk\u00b7chen", ",", "je\u00b7des", "St\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles preisen meine Lieder.", "tokens": ["Al\u00b7les", "prei\u00b7sen", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahrlich Kind, ich hab' zu Zeiten", "tokens": ["Wahr\u00b7lich", "Kind", ",", "ich", "hab'", "zu", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "PPER", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderk\u00fchnliche Gedanken!", "tokens": ["Wun\u00b7der\u00b7k\u00fchn\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unerm\u00fcdlich sind die Saiten", "tokens": ["Un\u00b7er\u00b7m\u00fcd\u00b7lich", "sind", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Mund ist ohne Schranken.", "tokens": ["Und", "der", "Mund", "ist", "oh\u00b7ne", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vom geheimsten Druck der H\u00e4nde", "tokens": ["Vom", "ge\u00b7heims\u00b7ten", "Druck", "der", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zum nimmersatten K\u00fcssen;", "tokens": ["Bis", "zum", "nim\u00b7mer\u00b7sat\u00b7ten", "K\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, ich selber wei\u00df am Ende", "tokens": ["Ja", ",", "ich", "sel\u00b7ber", "wei\u00df", "am", "En\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht, was du wirst h\u00f6ren m\u00fcssen.", "tokens": ["Nicht", ",", "was", "du", "wirst", "h\u00f6\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWS", "PPER", "VAFIN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df dich warnen, la\u00df mich schweigen,", "tokens": ["La\u00df", "dich", "war\u00b7nen", ",", "la\u00df", "mich", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich Lied um Liebe tauschen;", "tokens": ["La\u00df", "mich", "Lied", "um", "Lie\u00b7be", "tau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Bl\u00e4tter an den Zweigen", "tokens": ["Denn", "die", "Bl\u00e4t\u00b7ter", "an", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wachen auf und wollen lauschen.", "tokens": ["Wa\u00b7chen", "auf", "und", "wol\u00b7len", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wei\u00dfe Mondesnebel schwimmen", "tokens": ["Wei\u00b7\u00dfe", "Mon\u00b7des\u00b7ne\u00b7bel", "schwim\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Auf den feuchten Wiesenplanen;", "tokens": ["Auf", "den", "feuch\u00b7ten", "Wie\u00b7sen\u00b7pla\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst du die Guitarre stimmen", "tokens": ["H\u00f6rst", "du", "die", "Gui\u00b7tar\u00b7re", "stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Schatten der Platanen?", "tokens": ["In", "dem", "Schat\u00b7ten", "der", "Pla\u00b7ta\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}