{"textgrid.poem.33121": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbzu leben und zu sterben am reichsten Gl\u00fccke arm!\u00ab", "tokens": ["\u00bb", "zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben", "am", "reichs\u00b7ten", "Gl\u00fc\u00b7cke", "arm", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es f\u00fcllt Jolanthens Seele dies Wort mit stillem Harm,", "tokens": ["Es", "f\u00fcllt", "Jo\u00b7lan\u00b7thens", "See\u00b7le", "dies", "Wort", "mit", "stil\u00b7lem", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es t\u00f6net in ihr weiter, wie eine Harfe bebet", "tokens": ["Es", "t\u00f6\u00b7net", "in", "ihr", "wei\u00b7ter", ",", "wie", "ei\u00b7ne", "Har\u00b7fe", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Noch lange, wenn schon ferne die Hand, die sie belebet.", "tokens": ["Noch", "lan\u00b7ge", ",", "wenn", "schon", "fer\u00b7ne", "die", "Hand", ",", "die", "sie", "be\u00b7le\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie steht am Bogenfenster, sieht in die stille Nacht:", "tokens": ["Sie", "steht", "am", "Bo\u00b7gen\u00b7fens\u00b7ter", ",", "sieht", "in", "die", "stil\u00b7le", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Mondenstrahl ergl\u00e4nzet der Alpen hohe Wacht,", "tokens": ["Im", "Mon\u00b7dens\u00b7trahl", "er\u00b7gl\u00e4n\u00b7zet", "der", "Al\u00b7pen", "ho\u00b7he", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Mondenstrahl umwebet den See mit goldnen S\u00e4umen,", "tokens": ["Der", "Mon\u00b7dens\u00b7trahl", "um\u00b7we\u00b7bet", "den", "See", "mit", "gold\u00b7nen", "S\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er lockt das arme Herze zum Sehnen und zum Tr\u00e4umen.", "tokens": ["Er", "lockt", "das", "ar\u00b7me", "Her\u00b7ze", "zum", "Seh\u00b7nen", "und", "zum", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und leise, leise ziehet, wie ferner Geisterklang,", "tokens": ["Und", "lei\u00b7se", ",", "lei\u00b7se", "zie\u00b7het", ",", "wie", "fer\u00b7ner", "Geis\u00b7ter\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "$,", "PWAV", "ADV", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jetzt durch des Schlosses Hallen gar lieblicher Gesang:", "tokens": ["Jetzt", "durch", "des", "Schlos\u00b7ses", "Hal\u00b7len", "gar", "lieb\u00b7li\u00b7cher", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Frau Bertha auf den Knieen vor einem Bettlein lieget,", "tokens": ["Frau", "Bert\u00b7ha", "auf", "den", "Kni\u00b7e\u00b7en", "vor", "ei\u00b7nem", "Bet\u00b7tlein", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "In Schlaf und s\u00fc\u00dfe Ruhe ihr holdes Kindchen wieget.", "tokens": ["In", "Schlaf", "und", "s\u00fc\u00b7\u00dfe", "Ru\u00b7he", "ihr", "hol\u00b7des", "Kind\u00b7chen", "wie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Jolanthe, o Jolanthe, sieh' nicht auf dieses Gl\u00fcck,", "tokens": ["Jo\u00b7lan\u00b7the", ",", "o", "Jo\u00b7lan\u00b7the", ",", "sieh'", "nicht", "auf", "die\u00b7ses", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NE", "$,", "VVIMP", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und vor der Welt da drau\u00dfen verschlie\u00dfe deinen Blick!", "tokens": ["Und", "vor", "der", "Welt", "da", "drau\u00b7\u00dfen", "ver\u00b7schlie\u00b7\u00dfe", "dei\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dir kann in Klostermauern nicht die Erinn'rung frommen,", "tokens": ["Dir", "kann", "in", "Klos\u00b7ter\u00b7mau\u00b7ern", "nicht", "die", "Er\u00b7inn'\u00b7rung", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie selig eine Mutter und die Natur vollkommen. \u2013", "tokens": ["Wie", "se\u00b7lig", "ei\u00b7ne", "Mut\u00b7ter", "und", "die", "Na\u00b7tur", "voll\u00b7kom\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "KON", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie hat kein Schlo\u00df und Erbe, allein, verwaist stand sie,", "tokens": ["Sie", "hat", "kein", "Schlo\u00df", "und", "Er\u00b7be", ",", "al\u00b7lein", ",", "ver\u00b7waist", "stand", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "NN", "$,", "ADV", "$,", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kaum kannte sie den Vater und Mutterliebe nie, \u2013", "tokens": ["Kaum", "kann\u00b7te", "sie", "den", "Va\u00b7ter", "und", "Mut\u00b7ter\u00b7lie\u00b7be", "nie", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "ADV", "$,", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die fromme Braut des Himmels, will sie den Schleier nehmen,", "tokens": ["Die", "from\u00b7me", "Braut", "des", "Him\u00b7mels", ",", "will", "sie", "den", "Schlei\u00b7er", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie konnt' es bis zur Stunde noch ohne Schmerz und Gr\u00e4men.", "tokens": ["Sie", "konnt'", "es", "bis", "zur", "Stun\u00b7de", "noch", "oh\u00b7ne", "Schmerz", "und", "Gr\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch ach! mit einem Male scheint ihr die Welt so hold,", "tokens": ["Doch", "ach", "!", "mit", "ei\u00b7nem", "Ma\u00b7le", "scheint", "ihr", "die", "Welt", "so", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Freundin Muttername erw\u00e4rmt wie Sonnengold", "tokens": ["Der", "Freun\u00b7din", "Mut\u00b7ter\u00b7na\u00b7me", "er\u00b7w\u00e4rmt", "wie", "Son\u00b7nen\u00b7gold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "KOKOM", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Jungfrau reines Herze \u2013 soll sie die nie erwerben,", "tokens": ["Der", "Jung\u00b7frau", "rei\u00b7nes", "Her\u00b7ze", "\u2013", "soll", "sie", "die", "nie", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVFIN", "$(", "VMFIN", "PPER", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Soll arm am reichsten Gl\u00fccke sie leben so und sterben?", "tokens": ["Soll", "arm", "am", "reichs\u00b7ten", "Gl\u00fc\u00b7cke", "sie", "le\u00b7ben", "so", "und", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}