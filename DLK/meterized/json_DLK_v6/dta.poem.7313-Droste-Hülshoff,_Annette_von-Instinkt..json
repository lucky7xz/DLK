{"dta.poem.7313": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Instinkt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Bin ich allein, verhallt des Tages Rauschen", "tokens": ["Bin", "ich", "al\u00b7lein", ",", "ver\u00b7hallt", "des", "Ta\u00b7ges", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im frischen Wald, im braunen Haideland,", "tokens": ["Im", "fri\u00b7schen", "Wald", ",", "im", "brau\u00b7nen", "Hai\u00b7de\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um mein Gesicht die Gr\u00e4ser nickend bauschen,", "tokens": ["Um", "mein", "Ge\u00b7sicht", "die", "Gr\u00e4\u00b7ser", "ni\u00b7ckend", "bau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Vogel flattert an des Nestes Rand,", "tokens": ["Ein", "Vo\u00b7gel", "flat\u00b7tert", "an", "des", "Nes\u00b7tes", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mir zu F\u00fc\u00dfen liegt mein treuer Hund,", "tokens": ["Und", "mir", "zu", "F\u00fc\u00b7\u00dfen", "liegt", "mein", "treu\u00b7er", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleich Feuerw\u00fcrmern seine Augen glimmen,", "tokens": ["Gleich", "Feu\u00b7er\u00b7w\u00fcr\u00b7mern", "sei\u00b7ne", "Au\u00b7gen", "glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dann kommen mir Gedanken, ob gesund,", "tokens": ["Dann", "kom\u00b7men", "mir", "Ge\u00b7dan\u00b7ken", ",", "ob", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ob krank, das mag ich selber nicht bestimmen.", "tokens": ["Ob", "krank", ",", "das", "mag", "ich", "sel\u00b7ber", "nicht", "be\u00b7stim\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ergr\u00fcnden m\u00f6cht ich, ob das Blut, das gr\u00fcne,", "tokens": ["Er\u00b7gr\u00fcn\u00b7den", "m\u00f6cht", "ich", ",", "ob", "das", "Blut", ",", "das", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Lebenspuls durch jene Kr\u00e4uter tr\u00e4gt,", "tokens": ["Kein", "Le\u00b7bens\u00b7puls", "durch", "je\u00b7ne", "Kr\u00e4u\u00b7ter", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob ", "tokens": ["Ob"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Bewu\u00dftlos ihre rauhen Netze schl\u00e4gt,", "tokens": ["Be\u00b7wu\u00dft\u00b7los", "ih\u00b7re", "rau\u00b7hen", "Net\u00b7ze", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was in dem wei\u00dfen Sterne ", "tokens": ["Was", "in", "dem", "wei\u00b7\u00dfen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn er, die F\u00e4den streckend, leise schauert,", "tokens": ["Wenn", "er", ",", "die", "F\u00e4\u00b7den", "stre\u00b7ckend", ",", "lei\u00b7se", "schau\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVPP", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und ob, vom Duft der Menschenhand gestreift,", "tokens": ["Und", "ob", ",", "vom", "Duft", "der", "Men\u00b7schen\u00b7hand", "ge\u00b7streift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gef\u00fchllos ganz die Sensitive trauert?", "tokens": ["Ge\u00b7f\u00fchl\u00b7los", "ganz", "die", "Sen\u00b7si\u00b7ti\u00b7ve", "trau\u00b7ert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wieder mu\u00df ich auf den Vogel sehen,", "tokens": ["Und", "wie\u00b7der", "mu\u00df", "ich", "auf", "den", "Vo\u00b7gel", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der dort so z\u00fcrnend seine Federn str\u00e4ubt,", "tokens": ["Der", "dort", "so", "z\u00fcr\u00b7nend", "sei\u00b7ne", "Fe\u00b7dern", "str\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit kriegerischem Schrei mich aus den N\u00e4hen", "tokens": ["Mit", "krie\u00b7ge\u00b7ri\u00b7schem", "Schrei", "mich", "aus", "den", "N\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nackten Brut, nach allen Kr\u00e4ften treibt.", "tokens": ["Der", "nack\u00b7ten", "Brut", ",", "nach", "al\u00b7len", "Kr\u00e4f\u00b7ten", "treibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was ist Instinkt? \u2014 tiefsten Gef\u00fchles Heerd;", "tokens": ["Was", "ist", "Ins\u00b7tinkt", "?", "tiefs\u00b7ten", "Ge\u00b7f\u00fch\u00b7les", "Heerd", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "$(", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Instinkt trieb auch die Mutter zu dem Kinde,", "tokens": ["Ins\u00b7tinkt", "trieb", "auch", "die", "Mut\u00b7ter", "zu", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als jene F\u00fcrstin, von der Glut verzehrt,", "tokens": ["Als", "je\u00b7ne", "F\u00fcrs\u00b7tin", ",", "von", "der", "Glut", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als Heilge ward posaunt in alle Winde.", "tokens": ["Als", "Heil\u00b7ge", "ward", "po\u00b7saunt", "in", "al\u00b7le", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und du, mein zottger Tremm, der schlafestrunken", "tokens": ["Und", "du", ",", "mein", "zott\u00b7ger", "Tremm", ",", "der", "schla\u00b7fe\u00b7strun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch ob der Herrin wacht, und durch das Gr\u00fcn", "tokens": ["Noch", "ob", "der", "Her\u00b7rin", "wacht", ",", "und", "durch", "das", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft blinzelnd streifen seiner Blicke Funken,", "tokens": ["L\u00e4\u00dft", "blin\u00b7zelnd", "strei\u00b7fen", "sei\u00b7ner", "Bli\u00b7cke", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sag an, was deine klugen Augen gl\u00fchn?", "tokens": ["Sag", "an", ",", "was", "dei\u00b7ne", "klu\u00b7gen", "Au\u00b7gen", "gl\u00fchn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin es nicht, die deine Schale f\u00fcllt,", "tokens": ["Ich", "bin", "es", "nicht", ",", "die", "dei\u00b7ne", "Scha\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht gab der Nahrung Trieb dich mir zu eigen,", "tokens": ["Nicht", "gab", "der", "Nah\u00b7rung", "Trieb", "dich", "mir", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und mit der Sklavenpeitsche kann mein Bild", "tokens": ["Und", "mit", "der", "Skla\u00b7ven\u00b7peit\u00b7sche", "kann", "mein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Noch minder dir im dumpfen Hirne steigen.", "tokens": ["Noch", "min\u00b7der", "dir", "im", "dum\u00b7pfen", "Hir\u00b7ne", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer kann mir sagen, ob des Hundes Seele", "tokens": ["Wer", "kann", "mir", "sa\u00b7gen", ",", "ob", "des", "Hun\u00b7des", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinaufw\u00e4rts, oder ob nach unten steigt?", "tokens": ["Hin\u00b7auf\u00b7w\u00e4rts", ",", "o\u00b7der", "ob", "nach", "un\u00b7ten", "steigt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "KOUS", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00fcde, m\u00fcde dr\u00fcck' ich in die Schmehle", "tokens": ["Und", "m\u00fc\u00b7de", ",", "m\u00fc\u00b7de", "dr\u00fcck", "ich", "in", "die", "Schmeh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Haupt, wo siedend der Gedanke steigt.", "tokens": ["Mein", "Haupt", ",", "wo", "sie\u00b7dend", "der", "Ge\u00b7dan\u00b7ke", "steigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was ist es, das ein hungermattes Thier,", "tokens": ["Was", "ist", "es", ",", "das", "ein", "hun\u00b7ger\u00b7mat\u00b7tes", "Thier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit dem gestohlnen Brode f\u00fcr das bleiche", "tokens": ["Mit", "dem", "ge\u00b7stohl\u00b7nen", "Bro\u00b7de", "f\u00fcr", "das", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Blutr\u00fcnst'ge Antlitz, in das Waldrevier", "tokens": ["Blut\u00b7r\u00fcnst'\u00b7ge", "Ant\u00b7litz", ",", "in", "das", "Wald\u00b7re\u00b7vier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "L\u00e4\u00dft fl\u00fcchten und verschmachten bei der Leiche?", "tokens": ["L\u00e4\u00dft", "fl\u00fcch\u00b7ten", "und", "ver\u00b7schmach\u00b7ten", "bei", "der", "Lei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das sind Gedanken, die uns k\u00f6nnten t\u00f6dten,", "tokens": ["Das", "sind", "Ge\u00b7dan\u00b7ken", ",", "die", "uns", "k\u00f6nn\u00b7ten", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Geist bet\u00e4uben, rauben jedes Gl\u00fcck,", "tokens": ["Den", "Geist", "be\u00b7t\u00e4u\u00b7ben", ",", "rau\u00b7ben", "je\u00b7des", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit tausendfachem Mord die H\u00e4nde r\u00f6then,", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7chem", "Mord", "die", "H\u00e4n\u00b7de", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und leise schaudernd wend' ich meinen Blick.", "tokens": ["Und", "lei\u00b7se", "schau\u00b7dernd", "wend'", "ich", "mei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O schlimme Zeit, die solche G\u00e4ste rief", "tokens": ["O", "schlim\u00b7me", "Zeit", ",", "die", "sol\u00b7che", "G\u00e4s\u00b7te", "rief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In meines Sinnens harmlos lichte Bl\u00e4ue!", "tokens": ["In", "mei\u00b7nes", "Sin\u00b7nens", "harm\u00b7los", "lich\u00b7te", "Bl\u00e4u\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O schlechte Welt, die mich so lang' und tief", "tokens": ["O", "schlech\u00b7te", "Welt", ",", "die", "mich", "so", "lang'", "und", "tief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Lie\u00df gr\u00fcbeln \u00fcber eines Pudels Treue!", "tokens": ["Lie\u00df", "gr\u00fc\u00b7beln", "\u00fc\u00b7ber", "ei\u00b7nes", "Pu\u00b7dels", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}