{"dta.poem.12274": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Er .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich geh vor mir auf Weg und Stra\u00dfen,               ", "tokens": ["Wenn", "ich", "geh", "vor", "mir", "auf", "Weg", "und", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sehen mich schon alle Leute an,", "tokens": ["Se\u00b7hen", "mich", "schon", "al\u00b7le", "Leu\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Augen gie\u00dfen helles Wasser,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "gie\u00b7\u00dfen", "hel\u00b7les", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil ich gar nichts anders sprechen kann.", "tokens": ["Weil", "ich", "gar", "nichts", "an\u00b7ders", "spre\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach wie oft sind wir beysamm gesessen", "tokens": ["Ach", "wie", "oft", "sind", "wir", "bey\u00b7samm", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Manche liebe halbe stille Nacht,", "tokens": ["Man\u00b7che", "lie\u00b7be", "hal\u00b7be", "stil\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und den Schlaf den hatten wir vergessen,", "tokens": ["Und", "den", "Schlaf", "den", "hat\u00b7ten", "wir", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur mit Liebe ward sie zugebracht.", "tokens": ["Nur", "mit", "Lie\u00b7be", "ward", "sie", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Spielet auf ihr kleinen Musikanten,", "tokens": ["Spie\u00b7let", "auf", "ihr", "klei\u00b7nen", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spielet auf ein neues neues Lied,", "tokens": ["Spie\u00b7let", "auf", "ein", "neu\u00b7es", "neu\u00b7es", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ihr T\u00f6ne, liebliche Gesandten,", "tokens": ["Und", "ihr", "T\u00f6\u00b7ne", ",", "lieb\u00b7li\u00b7che", "Ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sagt Ade, weil ich auf lange scheid.", "tokens": ["Sagt", "A\u00b7de", ",", "weil", "ich", "auf", "lan\u00b7ge", "scheid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach in Trauren mu\u00df ich schlafen gehn,", "tokens": ["Ach", "in", "Trau\u00b7ren", "mu\u00df", "ich", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ach in Trauren mu\u00df ich fr\u00fch aufstehn,", "tokens": ["Ach", "in", "Trau\u00b7ren", "mu\u00df", "ich", "fr\u00fch", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In Trauren mu\u00df ich leben meine Zeit,", "tokens": ["In", "Trau\u00b7ren", "mu\u00df", "ich", "le\u00b7ben", "mei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dieweil ich nicht kann haben, die mein Herz erfreut.", "tokens": ["Die\u00b7weil", "ich", "nicht", "kann", "ha\u00b7ben", ",", "die", "mein", "Herz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VAINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach ihr Berg und tiefe, tiefe Thal,", "tokens": ["Ach", "ihr", "Berg", "und", "tie\u00b7fe", ",", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Seh ich meinen Schaz zum lezten Mahl?", "tokens": ["Seh", "ich", "mei\u00b7nen", "Schaz", "zum", "lez\u00b7ten", "Mahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die Sonne, der Mond, das ganze Firmament,", "tokens": ["Die", "Son\u00b7ne", ",", "der", "Mond", ",", "das", "gan\u00b7ze", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die sollen mit mir traurig seyn bis an mein End.", "tokens": ["Die", "sol\u00b7len", "mit", "mir", "trau\u00b7rig", "seyn", "bis", "an", "mein", "End", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "ADJD", "VAINF", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ach in Trauren mu\u00df ich schlafen gehn,", "tokens": ["Ach", "in", "Trau\u00b7ren", "mu\u00df", "ich", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ach in Trauren mu\u00df ich fr\u00fch aufstehn,", "tokens": ["Ach", "in", "Trau\u00b7ren", "mu\u00df", "ich", "fr\u00fch", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In Trauren mu\u00df ich leben meine Zeit,", "tokens": ["In", "Trau\u00b7ren", "mu\u00df", "ich", "le\u00b7ben", "mei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dieweil ich nicht kann haben, die mein Herz erfreut.", "tokens": ["Die\u00b7weil", "ich", "nicht", "kann", "ha\u00b7ben", ",", "die", "mein", "Herz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VAINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Geht dirs wohl, so denke du an mich,", "tokens": ["Geht", "dirs", "wohl", ",", "so", "den\u00b7ke", "du", "an", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gehts dir \u00fcbel, ach so kr\u00e4nkt es mich,", "tokens": ["Gehts", "dir", "\u00fc\u00b7bel", ",", "ach", "so", "kr\u00e4nkt", "es", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ITJ", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie froh wollt ich schon seyn, wenns wohl dir geht,", "tokens": ["Wie", "froh", "wollt", "ich", "schon", "seyn", ",", "wenns", "wohl", "dir", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ADV", "VAINF", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn schon mein jung frisch Leben in Trauren steht.", "tokens": ["Wenn", "schon", "mein", "jung", "frisch", "Le\u00b7ben", "in", "Trau\u00b7ren", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJD", "ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ach ihr Berge und tiefe tiefe Thal,", "tokens": ["Ach", "ihr", "Ber\u00b7ge", "und", "tie\u00b7fe", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ach ihr seht mein Lieb noch tausendmal,", "tokens": ["Ach", "ihr", "seht", "mein", "Lieb", "noch", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach tausendmal ihr tiefe tiefe Thal,", "tokens": ["Ach", "tau\u00b7send\u00b7mal", "ihr", "tie\u00b7fe", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr steht doch ewig ferne, ich nur bin ihr nah.", "tokens": ["Ihr", "steht", "doch", "e\u00b7wig", "fer\u00b7ne", ",", "ich", "nur", "bin", "ihr", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADJA", "$,", "PPER", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}