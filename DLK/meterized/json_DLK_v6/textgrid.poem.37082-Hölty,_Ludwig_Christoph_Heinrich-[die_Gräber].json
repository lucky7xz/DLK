{"textgrid.poem.37082": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Gr\u00e4ber]", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcher knie vor ihr, als ich vor dir mein Knie", "tokens": ["Fr\u00fc\u00b7her", "knie", "vor", "ihr", ",", "als", "ich", "vor", "dir", "mein", "Knie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Beuge, falte die Beterhand!", "tokens": ["Beu\u00b7ge", ",", "fal\u00b7te", "die", "Be\u00b7ter\u00b7hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Winke Winke der Held, wie sie ein Vater winkt,", "tokens": ["Win\u00b7ke", "Win\u00b7ke", "der", "Held", ",", "wie", "sie", "ein", "Va\u00b7ter", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Da\u00df die Boten des Himmels uns", "tokens": ["Da\u00df", "die", "Bo\u00b7ten", "des", "Him\u00b7mels", "uns"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "In dein ewiges Reich f\u00fchren, Allg\u00fctiger!", "tokens": ["In", "dein", "e\u00b7wi\u00b7ges", "Reich", "f\u00fch\u00b7ren", ",", "All\u00b7g\u00fc\u00b7ti\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}}}}}