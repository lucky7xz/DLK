{"textgrid.poem.47299": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solang' die Sonne nicht den Nachtflor bricht,", "tokens": ["So\u00b7lang'", "die", "Son\u00b7ne", "nicht", "den", "Nacht\u00b7flor", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind Tagesv\u00f6gel ohne Zuversicht.", "tokens": ["Sind", "Ta\u00b7ges\u00b7v\u00f6\u00b7gel", "oh\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Blick der Sonne ruft die Tulpen auf;", "tokens": ["Der", "Blick", "der", "Son\u00b7ne", "ruft", "die", "Tul\u00b7pen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt ist, o Herz, dir zu erwachen Pflicht.", "tokens": ["Jetzt", "ist", ",", "o", "Herz", ",", "dir", "zu", "er\u00b7wa\u00b7chen", "Pflicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$,", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Sonnenschwert gie\u00dft aus im Morgenrot", "tokens": ["Das", "Son\u00b7nen\u00b7schwert", "gie\u00dft", "aus", "im", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Blut der Nacht, von der es Sieg erficht.", "tokens": ["Das", "Blut", "der", "Nacht", ",", "von", "der", "es", "Sieg", "er\u00b7ficht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Voll Schlafs das Auge, sprach ich: \u00bbEs ist Nacht.\u00ab", "tokens": ["Voll", "Schlafs", "das", "Au\u00b7ge", ",", "sprach", "ich", ":", "\u00bb", "Es", "ist", "Nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er sprach: \u00bbVor meinem Angesichte nicht.\u00ab", "tokens": ["Er", "sprach", ":", "\u00bb", "Vor", "mei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPR", "PPOSAT", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Solang' es graut, ist zweifelhaft der Tag;", "tokens": ["So\u00b7lang'", "es", "graut", ",", "ist", "zwei\u00b7fel\u00b7haft", "der", "Tag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Am hellen Tag, wer zweifelt noch am Licht?", "tokens": ["Am", "hel\u00b7len", "Tag", ",", "wer", "zwei\u00b7felt", "noch", "am", "Licht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Im Osten steht das Licht, ich steh' im West,", "tokens": ["Im", "Os\u00b7ten", "steht", "das", "Licht", ",", "ich", "steh'", "im", "West", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein Berg, an dessen Haupt der Schein sich bricht.", "tokens": ["Ein", "Berg", ",", "an", "des\u00b7sen", "Haupt", "der", "Schein", "sich", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ich bin der Sch\u00f6nheitssonne blasser Mond;", "tokens": ["Ich", "bin", "der", "Sch\u00f6n\u00b7heits\u00b7son\u00b7ne", "blas\u00b7ser", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Schau weg von mir, der Sonn' ins Angesicht!", "tokens": ["Schau", "weg", "von", "mir", ",", "der", "Sonn'", "ins", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Des Wiederschein euch zeiget mein Gedicht.", "tokens": ["Des", "Wie\u00b7der\u00b7schein", "euch", "zei\u00b7get", "mein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}