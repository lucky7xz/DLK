{"textgrid.poem.47795": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Zweifel", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Glaube ist zum Ruhen gut,", "tokens": ["Der", "Glau\u00b7be", "ist", "zum", "Ru\u00b7hen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch bringt er nicht von der Stelle;", "tokens": ["Doch", "bringt", "er", "nicht", "von", "der", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Zweifel in ehrlicher M\u00e4nnerfaust,", "tokens": ["Der", "Zwei\u00b7fel", "in", "ehr\u00b7li\u00b7cher", "M\u00e4n\u00b7ner\u00b7faust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der sprengt die Pforten der H\u00f6lle.", "tokens": ["Der", "sprengt", "die", "Pfor\u00b7ten", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}