{"textgrid.poem.50764": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne f\u00e4hrt durchs Morgentor", "tokens": ["Die", "Son\u00b7ne", "f\u00e4hrt", "durchs", "Mor\u00b7gen\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Goldfunkelnd \u00fcber den Bergen,", "tokens": ["Gold\u00b7fun\u00b7kelnd", "\u00fc\u00b7ber", "den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und, wie zwei Veilchen im fr\u00fchen Mai,", "tokens": ["Und", ",", "wie", "zwei", "Veil\u00b7chen", "im", "fr\u00fc\u00b7hen", "Mai", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOKOM", "CARD", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zwei blaue Augen, klar und frei,", "tokens": ["Zwei", "blau\u00b7e", "Au\u00b7gen", ",", "klar", "und", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die lachen auf ihren Wegen", "tokens": ["Die", "la\u00b7chen", "auf", "ih\u00b7ren", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ge\u00f6ffnet ihr entgegen!", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "ihr", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcck auf! mein Liebchen ist erwacht", "tokens": ["Gl\u00fcck", "auf", "!", "mein", "Lieb\u00b7chen", "ist", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit purpurroten Wangen!", "tokens": ["Mit", "pur\u00b7pur\u00b7ro\u00b7ten", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Fenster glitzert im Morgenstrahl,", "tokens": ["Ihr", "Fens\u00b7ter", "glit\u00b7zert", "im", "Mor\u00b7gen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alle Blumen in Garten und Tal", "tokens": ["Und", "al\u00b7le", "Blu\u00b7men", "in", "Gar\u00b7ten", "und", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Erwarten sie mit Sehnen,", "tokens": ["Er\u00b7war\u00b7ten", "sie", "mit", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die \u00c4uglein voller Tr\u00e4nen.", "tokens": ["Die", "\u00c4ug\u00b7lein", "vol\u00b7ler", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist nichts Sch\u00f6neres in der Welt", "tokens": ["Es", "ist", "nichts", "Sch\u00f6\u00b7ne\u00b7res", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als diese gr\u00fcne Erde:", "tokens": ["Als", "die\u00b7se", "gr\u00fc\u00b7ne", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man darauf ein Sch\u00e4tzlein hat,", "tokens": ["Wenn", "man", "da\u00b7rauf", "ein", "Sch\u00e4tz\u00b7lein", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das still und innig, fr\u00fch und spat", "tokens": ["Das", "still", "und", "in\u00b7nig", ",", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr einen lebt und bl\u00fchet,", "tokens": ["F\u00fcr", "ei\u00b7nen", "lebt", "und", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein heimlich Feuerlein, gl\u00fchet!", "tokens": ["Ein", "heim\u00b7lich", "Feu\u00b7er\u00b7lein", ",", "gl\u00fc\u00b7het", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Hallo, du schl\u00e4friger J\u00e4gersmann,", "tokens": ["Hal\u00b7lo", ",", "du", "schl\u00e4f\u00b7ri\u00b7ger", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie reibst du deine Augen!", "tokens": ["Wie", "reibst", "du", "dei\u00b7ne", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab die ganze Nacht durchschw\u00e4rmt", "tokens": ["Ich", "hab", "die", "gan\u00b7ze", "Nacht", "durch\u00b7schw\u00e4rmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich am Mondenschein gew\u00e4rmt", "tokens": ["Und", "mich", "am", "Mon\u00b7den\u00b7schein", "ge\u00b7w\u00e4rmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und steige frisch und munter", "tokens": ["Und", "stei\u00b7ge", "frisch", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von meinem Berg herunter!", "tokens": ["Von", "mei\u00b7nem", "Berg", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein M\u00e4dchen durch den Garten geht", "tokens": ["Mein", "M\u00e4d\u00b7chen", "durch", "den", "Gar\u00b7ten", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt halblaute Weisen;", "tokens": ["Und", "singt", "hal\u00b7blau\u00b7te", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich d\u00fcnkt, ich kenne der Lieder Ton:", "tokens": ["Mich", "d\u00fcnkt", ",", "ich", "ken\u00b7ne", "der", "Lie\u00b7der", "Ton", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was gilt's, ich habe sie alle schon", "tokens": ["Was", "gilt's", ",", "ich", "ha\u00b7be", "sie", "al\u00b7le", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "PIS", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Heut nacht dort oben gesungen?", "tokens": ["Heut", "nacht", "dort", "o\u00b7ben", "ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie sind her\u00fcbergeklungen!", "tokens": ["Sie", "sind", "her\u00b7\u00fc\u00b7ber\u00b7ge\u00b7klun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}