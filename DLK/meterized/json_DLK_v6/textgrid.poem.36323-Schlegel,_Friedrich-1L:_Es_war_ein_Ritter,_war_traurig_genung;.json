r", "ihn", "ver\u00b7grub", ",", "nie", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Fern singt er am Meere manch heimliches Lied,", "tokens": ["Fern", "singt", "er", "am", "Mee\u00b7re", "manch", "heim\u00b7li\u00b7ches", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Bei Sonn' und bei Mond, wie die Wolke zieht.", "tokens": ["Bei", "Sonn'", "und", "bei", "Mond", ",", "wie", "die", "Wol\u00b7ke", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}