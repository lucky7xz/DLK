{"textgrid.poem.36323": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war ein Ritter, war traurig genung;", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Ritter, war traurig genung;", "tokens": ["Es", "war", "ein", "Rit\u00b7ter", ",", "war", "trau\u00b7rig", "ge\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er sah sie laufen, sich raufen und schnaufen um nichts.", "tokens": ["Er", "sah", "sie", "lau\u00b7fen", ",", "sich", "rau\u00b7fen", "und", "schnau\u00b7fen", "um", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "PRF", "VVFIN", "KON", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Haar wurde grau, doch der Mut blieb ihm jung,", "tokens": ["Sein", "Haar", "wur\u00b7de", "grau", ",", "doch", "der", "Mut", "blieb", "ihm", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und eckelt' ihn manchen Dreihellergesichts.", "tokens": ["Und", "e\u00b7ckelt'", "ihn", "man\u00b7chen", "Drei\u00b7hel\u00b7ler\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es trippelten, trappelten Zwerg' um ihn her,", "tokens": ["Es", "trip\u00b7pel\u00b7ten", ",", "trap\u00b7pel\u00b7ten", "Zwer\u00b7g'", "um", "ihn", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die klipperten, klapperten, rappelten sehr.", "tokens": ["Die", "klip\u00b7per\u00b7ten", ",", "klap\u00b7per\u00b7ten", ",", "rap\u00b7pel\u00b7ten", "sehr", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbade,\u00ab sprach der Ritter, \u00bbdu Vaterland mein;\u00ab", "tokens": ["\u00bb", "a\u00b7de", ",", "\u00ab", "sprach", "der", "Rit\u00b7ter", ",", "\u00bb", "du", "Va\u00b7ter\u00b7land", "mein", ";", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "PPER", "NN", "PPOSAT", "$.", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Es starrete kalt in der Brust ihm das Herz \u2013", "tokens": ["Es", "star\u00b7re\u00b7te", "kalt", "in", "der", "Brust", "ihm", "das", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "PPER", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbade, es mu\u00df nun geschieden sein,", "tokens": ["\u00bb", "a\u00b7de", ",", "es", "mu\u00df", "nun", "ge\u00b7schie\u00b7den", "sein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was wei\u00df diese Brut, was wei\u00df sie von Glut und von Schmerz?\u00ab", "tokens": ["Was", "wei\u00df", "die\u00b7se", "Brut", ",", "was", "wei\u00df", "sie", "von", "Glut", "und", "von", "Schmerz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$,", "PWS", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So zieht er und flieht von dannen fort,", "tokens": ["So", "zieht", "er", "und", "flieht", "von", "dan\u00b7nen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Kleinod doch l\u00e4\u00dft er am heimischen Ort.", "tokens": ["Ein", "Klei\u00b7nod", "doch", "l\u00e4\u00dft", "er", "am", "hei\u00b7mi\u00b7schen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbtu auf deinen Scho\u00df, o Waldestal,", "tokens": ["\u00bb", "tu", "auf", "dei\u00b7nen", "Scho\u00df", ",", "o", "Wal\u00b7des\u00b7tal", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und nimm dieses Kleinod, nimm treulich es auf!", "tokens": ["Und", "nimm", "die\u00b7ses", "Klei\u00b7nod", ",", "nimm", "treu\u00b7lich", "es", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PDAT", "NN", "$,", "VVIMP", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich strebte und lebte der Liebe zumal.\u00ab", "tokens": ["Ich", "streb\u00b7te", "und", "leb\u00b7te", "der", "Lie\u00b7be", "zu\u00b7mal", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann sch\u00fcttet er viele Figuren darauf;", "tokens": ["Dann", "sch\u00fct\u00b7tet", "er", "vie\u00b7le", "Fi\u00b7gu\u00b7ren", "da\u00b7rauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u00bbdie Zeichen sind magisch, die m\u00f6gen da ruhn;", "tokens": ["\u00bb", "die", "Zei\u00b7chen", "sind", "ma\u00b7gisch", ",", "die", "m\u00f6\u00b7gen", "da", "ruhn", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Was wollen die Zwerge den Zeichen tun?\u00ab", "tokens": ["Was", "wol\u00b7len", "die", "Zwer\u00b7ge", "den", "Zei\u00b7chen", "tun", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "So sprach er und ging von dannen im Zorn.", "tokens": ["So", "sprach", "er", "und", "ging", "von", "dan\u00b7nen", "im", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gleich sp\u00fcrten die Zwerge und r\u00fchrten am Platz,", "tokens": ["Gleich", "sp\u00fcr\u00b7ten", "die", "Zwer\u00b7ge", "und", "r\u00fchr\u00b7ten", "am", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie reinliche, kleinliche M\u00e4use im Korn,", "tokens": ["Wie", "rein\u00b7li\u00b7che", ",", "klein\u00b7li\u00b7che", "M\u00e4u\u00b7se", "im", "Korn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So knaupelten, graupelten die in dem Schatz;", "tokens": ["So", "knau\u00b7pel\u00b7ten", ",", "grau\u00b7pel\u00b7ten", "die", "in", "dem", "Schatz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ART", "APPR", "ART", "NN", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie trugen die St\u00fccke zu Markte heraus,", "tokens": ["Sie", "tru\u00b7gen", "die", "St\u00fc\u00b7cke", "zu", "Mark\u00b7te", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und machten sich zierliche M\u00e4ntelchen draus.", "tokens": ["Und", "mach\u00b7ten", "sich", "zier\u00b7li\u00b7che", "M\u00e4n\u00b7tel\u00b7chen", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sprangen auf St\u00fchlen und B\u00e4nken frisch", "tokens": ["Sie", "spran\u00b7gen", "auf", "St\u00fch\u00b7len", "und", "B\u00e4n\u00b7ken", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und gingen auf K\u00f6pfen wunderlich,", "tokens": ["Und", "gin\u00b7gen", "auf", "K\u00f6p\u00b7fen", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bald sa\u00dfen sie ernsthaft am langen Tisch,", "tokens": ["Bald", "sa\u00b7\u00dfen", "sie", "ernst\u00b7haft", "am", "lan\u00b7gen", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bald drehten wie Kr\u00e4usel im Kreise sie sich.", "tokens": ["Bald", "dreh\u00b7ten", "wie", "Kr\u00e4u\u00b7sel", "im", "Krei\u00b7se", "sie", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NN", "APPRART", "NN", "PPER", "PRF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie hatten zu viel genascht und genagt", "tokens": ["Sie", "hat\u00b7ten", "zu", "viel", "ge\u00b7nascht", "und", "ge\u00b7nagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "VVPP", "KON", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Am heimlichen Schatze, von dem wir gesagt.", "tokens": ["Am", "heim\u00b7li\u00b7chen", "Schat\u00b7ze", ",", "von", "dem", "wir", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Sie warfen die Bilder wohl hin und wohl her,", "tokens": ["Sie", "war\u00b7fen", "die", "Bil\u00b7der", "wohl", "hin", "und", "wohl", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und hatten des immer und nimmer Gewinn.", "tokens": ["Und", "hat\u00b7ten", "des", "im\u00b7mer", "und", "nim\u00b7mer", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADV", "KON", "ADV", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie stellten die Zeichen die Kreuz und die Quer", "tokens": ["Sie", "stell\u00b7ten", "die", "Zei\u00b7chen", "die", "Kreuz", "und", "die", "Quer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und fanden jedweder sich selber darin.", "tokens": ["Und", "fan\u00b7den", "jed\u00b7we\u00b7der", "sich", "sel\u00b7ber", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "PRF", "ADV", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der rechte Edelstein fehlt ihnen doch,", "tokens": ["Der", "rech\u00b7te", "E\u00b7del\u00b7stein", "fehlt", "ih\u00b7nen", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der ruhte wohl tief in der Erde noch.", "tokens": ["Der", "ruh\u00b7te", "wohl", "tief", "in", "der", "Er\u00b7de", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Es zwitscherten einige schmachtend und zart,", "tokens": ["Es", "zwit\u00b7scher\u00b7ten", "ei\u00b7ni\u00b7ge", "schmach\u00b7tend", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch andre bellten und schalten darauf;", "tokens": ["Doch", "and\u00b7re", "bell\u00b7ten", "und", "schal\u00b7ten", "da\u00b7rauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es strichen sich andre den kleinen Bart,", "tokens": ["Es", "stri\u00b7chen", "sich", "and\u00b7re", "den", "klei\u00b7nen", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und bauten possierliche H\u00e4userchen auf.", "tokens": ["Und", "bau\u00b7ten", "pos\u00b7sier\u00b7li\u00b7che", "H\u00e4u\u00b7ser\u00b7chen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie schrien und schrieben und trieben es viel,", "tokens": ["Sie", "schri\u00b7en", "und", "schrie\u00b7ben", "und", "trie\u00b7ben", "es", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie rissen, zerbissen sich selber zum Spiel.", "tokens": ["Sie", "ris\u00b7sen", ",", "zer\u00b7bis\u00b7sen", "sich", "sel\u00b7ber", "zum", "Spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Nun fanden die Zwerge in selbiger Gruft", "tokens": ["Nun", "fan\u00b7den", "die", "Zwer\u00b7ge", "in", "sel\u00b7bi\u00b7ger", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein heidnisches Bild von Marmelstein:", "tokens": ["Ein", "heid\u00b7ni\u00b7sches", "Bild", "von", "Mar\u00b7mel\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie zerren und zergen's hervor an die Luft:", "tokens": ["Sie", "zer\u00b7ren", "und", "zer\u00b7gen's", "her\u00b7vor", "an", "die", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbdas,\u00ab sprachen sie, \u00bbsoll unser Abgott sein.\u00ab", "tokens": ["\u00bb", "das", ",", "\u00ab", "spra\u00b7chen", "sie", ",", "\u00bb", "soll", "un\u00b7ser", "Ab\u00b7gott", "sein", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie toben und loben das Bildnis fortan,", "tokens": ["Sie", "to\u00b7ben", "und", "lo\u00b7ben", "das", "Bild\u00b7nis", "for\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Den heidnischen, herrlichen Marmormann!", "tokens": ["Den", "heid\u00b7ni\u00b7schen", ",", "herr\u00b7li\u00b7chen", "Mar\u00b7mor\u00b7mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "\u00bbwohl ist es ein alter erkaltender Block,", "tokens": ["\u00bb", "wohl", "ist", "es", "ein", "al\u00b7ter", "er\u00b7kal\u00b7ten\u00b7der", "Block", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und die ihn erfanden, verstanden's nicht recht;", "tokens": ["Und", "die", "ihn", "er\u00b7fan\u00b7den", ",", "ver\u00b7stan\u00b7den's", "nicht", "recht", ";"], "token_info": ["word", "word", "word", "word", "punct", "wor