{"textgrid.poem.33441": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn in bangen tr\u00fcben Stunden", "tokens": ["Wenn", "in", "ban\u00b7gen", "tr\u00fc\u00b7ben", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Herz beinah verzagt,", "tokens": ["Un\u00b7ser", "Herz", "bei\u00b7nah", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn von Krankheit \u00fcberwunden", "tokens": ["Wenn", "von", "Krank\u00b7heit", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Angst in unserm Innern nagt;", "tokens": ["Angst", "in", "un\u00b7serm", "In\u00b7nern", "nagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir der Treugeliebten denken,", "tokens": ["Wir", "der", "Treu\u00b7ge\u00b7lieb\u00b7ten", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie Gram und Kummer dr\u00fcckt,", "tokens": ["Wie", "sie", "Gram", "und", "Kum\u00b7mer", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wolken unsern Blick beschr\u00e4nken,", "tokens": ["Wol\u00b7ken", "un\u00b7sern", "Blick", "be\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die kein Hoffnungsstrahl durchblickt:", "tokens": ["Die", "kein", "Hoff\u00b7nungs\u00b7strahl", "durch\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O! dann neigt sich Gott her\u00fcber,", "tokens": ["O", "!", "dann", "neigt", "sich", "Gott", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PRF", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Liebe kommt uns nah,", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "kommt", "uns", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehnen wir uns dann hin\u00fcber,", "tokens": ["Seh\u00b7nen", "wir", "uns", "dann", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht sein Engel vor uns da,", "tokens": ["Steht", "sein", "En\u00b7gel", "vor", "uns", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bringt den Kelch des frischen Lebens,", "tokens": ["Bringt", "den", "Kelch", "des", "fri\u00b7schen", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lispelt Muth und Trost uns zu;", "tokens": ["Lis\u00b7pelt", "Muth", "und", "Trost", "uns", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir beten nicht vergebens", "tokens": ["Und", "wir", "be\u00b7ten", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch f\u00fcr die Geliebten Ruh.", "tokens": ["Auch", "f\u00fcr", "die", "Ge\u00b7lieb\u00b7ten", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}