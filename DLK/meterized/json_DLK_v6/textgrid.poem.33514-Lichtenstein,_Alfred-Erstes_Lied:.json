{"textgrid.poem.33514": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Lied:", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So viele Jahre sucht ich dich, Maria \u2013", "tokens": ["So", "vie\u00b7le", "Jah\u00b7re", "sucht", "ich", "dich", ",", "Ma\u00b7ria", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "PRF", "$,", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In G\u00e4rten, Stuben, St\u00e4dten und Gebirgen,", "tokens": ["In", "G\u00e4r\u00b7ten", ",", "Stu\u00b7ben", ",", "St\u00e4d\u00b7ten", "und", "Ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Buden, Dirnen, in Theaterschulen,", "tokens": ["In", "Bu\u00b7den", ",", "Dir\u00b7nen", ",", "in", "The\u00b7a\u00b7ter\u00b7schu\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Krankenbetten und in Irrenzimmern,", "tokens": ["In", "Kran\u00b7ken\u00b7bet\u00b7ten", "und", "in", "Ir\u00b7ren\u00b7zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In K\u00fcchenm\u00e4dchen, Schreien, Fr\u00fchlingsfeiern,", "tokens": ["In", "K\u00fc\u00b7chen\u00b7m\u00e4d\u00b7chen", ",", "Schrei\u00b7en", ",", "Fr\u00fch\u00b7lings\u00b7fei\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In allen Wettern und in allen Tagen,", "tokens": ["In", "al\u00b7len", "Wet\u00b7tern", "und", "in", "al\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Kaffeeh\u00e4usern, M\u00fcttern, T\u00e4nzerinnen \u2013", "tokens": ["In", "Kaf\u00b7fee\u00b7h\u00e4u\u00b7sern", ",", "M\u00fct\u00b7tern", ",", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich fand dich nicht in Kneipen, Kinobildern,", "tokens": ["Ich", "fand", "dich", "nicht", "in", "Knei\u00b7pen", ",", "Ki\u00b7no\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Musiklokalen, Sommerdampferfahrten ...", "tokens": ["Mu\u00b7sik\u00b7lo\u00b7ka\u00b7len", ",", "Som\u00b7mer\u00b7damp\u00b7fer\u00b7fahr\u00b7ten", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wer sagt die Qual, wenn ich in Nacht auf Stra\u00dfen", "tokens": ["Wer", "sagt", "die", "Qual", ",", "wenn", "ich", "in", "Nacht", "auf", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nach dir zum toten Himmel schrie \u2013", "tokens": ["Nach", "dir", "zum", "to\u00b7ten", "Him\u00b7mel", "schrie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}