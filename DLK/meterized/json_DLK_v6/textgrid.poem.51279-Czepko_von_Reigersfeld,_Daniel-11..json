{"textgrid.poem.51279": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ich seh' in mir an das gro\u00dfe Rund der Welt,", "tokens": ["Wann", "ich", "seh'", "in", "mir", "an", "das", "gro\u00b7\u00dfe", "Rund", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erblick ich in der erst ein ungest\u00fcmes Meer:", "tokens": ["Er\u00b7blick", "ich", "in", "der", "erst", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dis hat gantz \u00fcberw\u00f6lckt des Irrthums finstre Nacht,", "tokens": ["Dis", "hat", "gantz", "\u00fc\u00b7berw\u00b7\u00f6lckt", "des", "Irr\u00b7thums", "finst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Denn rei\u00dft mich mit ihm fort der Eitelkeiten Lauff:", "tokens": ["Denn", "rei\u00dft", "mich", "mit", "ihm", "fort", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Lauff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich irr' und tapp' und seh', und finde doch kein Licht;", "tokens": ["Ich", "irr'", "und", "tapp'", "und", "seh'", ",", "und", "fin\u00b7de", "doch", "kein", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein andrer Port ist dar als der versehne Tod.", "tokens": ["Kein", "an\u00b7drer", "Port", "ist", "dar", "als", "der", "ver\u00b7seh\u00b7ne", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NE", "VAFIN", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In seinem K\u00f6cher hat mehr Pfeile dieser Todt,", "tokens": ["In", "sei\u00b7nem", "K\u00f6\u00b7cher", "hat", "mehr", "Pfei\u00b7le", "die\u00b7ser", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Sonnenstaub durchzeucht die Kugel dieser Welt:", "tokens": ["Als", "Son\u00b7nen\u00b7staub", "durch\u00b7zeucht", "die", "Ku\u00b7gel", "die\u00b7ser", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Wellen wirfft und bricht das unbepf\u00e4hlte Meer:", "tokens": ["Als", "Wel\u00b7len", "wirfft", "und", "bricht", "das", "un\u00b7be\u00b7pf\u00e4hl\u00b7te", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Sterne Luna f\u00fchrt, wann Sie bekr\u00f6nt die Nacht:", "tokens": ["Als", "Ster\u00b7ne", "Lu\u00b7na", "f\u00fchrt", ",", "wann", "Sie", "be\u00b7kr\u00f6nt", "die", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als Blick' in ihm beschleust der Zeiten strenger Lauff:", "tokens": ["Als", "Blick'", "in", "ihm", "be\u00b7schleust", "der", "Zei\u00b7ten", "stren\u00b7ger", "Lauff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Strahlen aus ihm giebt des grossen Himmels Licht,", "tokens": ["Als", "Strah\u00b7len", "aus", "ihm", "giebt", "des", "gros\u00b7sen", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich sehe wie im Traum zwar offt der Wahrheit Licht,", "tokens": ["Ich", "se\u00b7he", "wie", "im", "Traum", "zwar", "offt", "der", "Wahr\u00b7heit", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann mein Gem\u00fcth und Hertz entfinstert ie der Tod:", "tokens": ["Wann", "mein", "Ge\u00b7m\u00fcth", "und", "Hertz", "ent\u00b7fins\u00b7tert", "ie", "der", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Offt wird mir sehr verhast die arge Lust der Welt:", "tokens": ["Offt", "wird", "mir", "sehr", "ver\u00b7hast", "die", "ar\u00b7ge", "Lust", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem die Ruh bemahlt und stillt das tobe Meer:", "tokens": ["In", "dem", "die", "Ruh", "be\u00b7mahlt", "und", "stillt", "das", "to\u00b7be", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Offt brech ich durch den Schlaff, und steig aus tieffer Nacht,", "tokens": ["Offt", "brech", "ich", "durch", "den", "Schlaff", ",", "und", "steig", "aus", "tief\u00b7fer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja brenne, Himmel an zu nehmen meinen Lauff:", "tokens": ["Ja", "bren\u00b7ne", ",", "Him\u00b7mel", "an", "zu", "neh\u00b7men", "mei\u00b7nen", "Lauff", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "NN", "PTKVZ", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bald aber wird gehemmt der Tugend strenge Lauff:", "tokens": ["Bald", "a\u00b7ber", "wird", "ge\u00b7hemmt", "der", "Tu\u00b7gend", "stren\u00b7ge", "Lauff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ich bin gantz verf\u00fchrt und seh' ein frembdes Licht,", "tokens": ["Und", "ich", "bin", "gantz", "ver\u00b7f\u00fchrt", "und", "seh'", "ein", "fremb\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann seine Furcht erweckt der langsam schnelle Tod:", "tokens": ["Wann", "sei\u00b7ne", "Furcht", "er\u00b7weckt", "der", "lang\u00b7sam", "schnel\u00b7le", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Hertze geht nach Lust so wieder in die Welt,", "tokens": ["Mein", "Hert\u00b7ze", "geht", "nach", "Lust", "so", "wie\u00b7der", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "APPR", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wird hin und her gejagt durch offnes Sorgen Meer;", "tokens": ["Wird", "hin", "und", "her", "ge\u00b7jagt", "durch", "off\u00b7nes", "Sor\u00b7gen", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Deckt \u00fcber seinen Fall das Segel stiller Nacht.", "tokens": ["Deckt", "\u00fc\u00b7ber", "sei\u00b7nen", "Fall", "das", "Se\u00b7gel", "stil\u00b7ler", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wann du in Zirckeln zehlst die Sterne bey der Nacht,", "tokens": ["Wann", "du", "in", "Zir\u00b7ckeln", "zehlst", "die", "Ster\u00b7ne", "bey", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann du zur\u00fccke bringst der Jahre schnellen Lauff,", "tokens": ["Wann", "du", "zu\u00b7r\u00fc\u00b7cke", "bringst", "der", "Jah\u00b7re", "schnel\u00b7len", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann du vom Gl\u00e4ntzen treibst der Sonnen helles Licht:", "tokens": ["Wann", "du", "vom", "Gl\u00e4nt\u00b7zen", "treibst", "der", "Son\u00b7nen", "hel\u00b7les", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann du im Tode selbst kanst singen weg den Tod,", "tokens": ["Wann", "du", "im", "To\u00b7de", "selbst", "kanst", "sin\u00b7gen", "weg", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann du ohn Wind und Lufft wirst sehn den Ball der Wellt,", "tokens": ["Wann", "du", "ohn", "Wind", "und", "Lufft", "wirst", "sehn", "den", "Ball", "der", "Wellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "VAFIN", "VVINF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wirstu von sturm und fall befreyt sein und dein Meer.", "tokens": ["Wirs\u00b7tu", "von", "sturm", "und", "fall", "be\u00b7freyt", "sein", "und", "dein", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJD", "KON", "NN", "VVPP", "VAINF", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Wann ohne Schiffbruch du wilt segeln durch das Meer,", "tokens": ["Wann", "oh\u00b7ne", "Schiff\u00b7bruch", "du", "wilt", "se\u00b7geln", "durch", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Mensch! so rei\u00dfe dich aus der verstockten Nacht,", "tokens": ["O", "Mensch", "!", "so", "rei\u00b7\u00dfe", "dich", "aus", "der", "ver\u00b7stock\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Inngleichen Winckelschnur erhebe deinen Lauff,", "tokens": ["I\u00b7nnglei\u00b7chen", "Win\u00b7ckel\u00b7schnur", "er\u00b7he\u00b7be", "dei\u00b7nen", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Bi\u00df du erblicken kanst der Gottheit klares Licht,", "tokens": ["Bi\u00df", "du", "er\u00b7bli\u00b7cken", "kanst", "der", "Got\u00b7theit", "kla\u00b7res", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann f\u00fchrst du im Triumph durch einen freyen Tod,", "tokens": ["Dann", "f\u00fchrst", "du", "im", "Tri\u00b7umph", "durch", "ei\u00b7nen", "frey\u00b7en", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Tod, das Licht, den Lauff, die Nacht, das Meer, die Welt.", "tokens": ["Den", "Tod", ",", "das", "Licht", ",", "den", "Lauff", ",", "die", "Nacht", ",", "das", "Meer", ",", "die", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sonst nimmt die Welt dein Lob, dein Haab und Gut das Meer,", "tokens": ["Sonst", "nimmt", "die", "Welt", "dein", "Lob", ",", "dein", "Ha\u00b7ab", "und", "Gut", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dein Leben selbst der Tod, dein Herrlichseyn die Nacht,", "tokens": ["Dein", "Le\u00b7ben", "selbst", "der", "Tod", ",", "dein", "Herr\u00b7lich\u00b7seyn", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein blinder Wahn das Licht, die Eitelkeit den Lauff.", "tokens": ["Dein", "blin\u00b7der", "Wahn", "das", "Licht", ",", "die", "Ei\u00b7tel\u00b7keit", "den", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}