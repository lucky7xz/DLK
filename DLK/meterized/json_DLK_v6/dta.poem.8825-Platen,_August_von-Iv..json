{"dta.poem.8825": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mein Stand und sey mein Posten:", "tokens": ["Sey", "mein", "Stand", "und", "sey", "mein", "Pos\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ich schweifte nur vergebens", "tokens": ["Ach", ",", "ich", "schweif\u00b7te", "nur", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald nach Westen, bald nach Osten!", "tokens": ["Bald", "nach", "Wes\u00b7ten", ",", "bald", "nach", "Os\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es pflegt, wie Viele sagen,", "tokens": ["Doch", "es", "pflegt", ",", "wie", "Vie\u00b7le", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alte Liebe nicht zu rosten.", "tokens": ["Al\u00b7te", "Lie\u00b7be", "nicht", "zu", "ros\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fc\u00dferes, als deine Blicke,", "tokens": ["S\u00fc\u00b7\u00dfe\u00b7res", ",", "als", "dei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gab mir nie die Welt zu kosten:", "tokens": ["Gab", "mir", "nie", "die", "Welt", "zu", "kos\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig sende mir dein schwarzes", "tokens": ["E\u00b7wig", "sen\u00b7de", "mir", "dein", "schwar\u00b7zes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Auge s\u00fc\u00dfe Liebesposten!", "tokens": ["Au\u00b7ge", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be\u00b7spos\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}