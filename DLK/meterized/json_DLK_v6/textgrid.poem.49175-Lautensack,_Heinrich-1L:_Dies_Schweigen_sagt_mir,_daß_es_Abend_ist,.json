{"textgrid.poem.49175": {"metadata": {"author": {"name": "Lautensack, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies Schweigen sagt mir, da\u00df es Abend ist,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Schweigen sagt mir, da\u00df es Abend ist,", "tokens": ["Dies", "Schwei\u00b7gen", "sagt", "mir", ",", "da\u00df", "es", "A\u00b7bend", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und meine Harfe, die von selber t\u00f6nt.", "tokens": ["und", "mei\u00b7ne", "Har\u00b7fe", ",", "die", "von", "sel\u00b7ber", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie viele Lieder wu\u00dft ich unter Tag,", "tokens": ["Wie", "vie\u00b7le", "Lie\u00b7der", "wu\u00dft", "ich", "un\u00b7ter", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wie viele T\u00f6ne meiner Harfe. Jetzt", "tokens": ["wie", "vie\u00b7le", "T\u00f6\u00b7ne", "mei\u00b7ner", "Har\u00b7fe", ".", "Jetzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PIAT", "NN", "PPOSAT", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "will eins anheben, das am Tage schwieg,", "tokens": ["will", "eins", "an\u00b7he\u00b7ben", ",", "das", "am", "Ta\u00b7ge", "schwieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "ein seltsam Lied, das keine H\u00f6rer mag,", "tokens": ["ein", "selt\u00b7sam", "Lied", ",", "das", "kei\u00b7ne", "H\u00f6\u00b7rer", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PIAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "das zu sich selber redet:", "tokens": ["das", "zu", "sich", "sel\u00b7ber", "re\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich ... bin ... blind.", "tokens": ["Ich", "...", "bin", "...", "blind", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$(", "VAFIN", "$(", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Blind f\u00fcr die vielen Wege mancher Fraun,", "tokens": ["Blind", "f\u00fcr", "die", "vie\u00b7len", "We\u00b7ge", "man\u00b7cher", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "wenns Abend ist. Ich find mich nicht zurecht,", "tokens": ["wenns", "A\u00b7bend", "ist", ".", "Ich", "find", "mich", "nicht", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "wo viele Wege gegen Abend tr\u00e4umen", "tokens": ["wo", "vie\u00b7le", "We\u00b7ge", "ge\u00b7gen", "A\u00b7bend", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "hin unter B\u00e4umen, die voll Dunkel sind.", "tokens": ["hin", "un\u00b7ter", "B\u00e4u\u00b7men", ",", "die", "voll", "Dun\u00b7kel", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ich steh, und wage keinen Schritt zu tun,", "tokens": ["Ich", "steh", ",", "und", "wa\u00b7ge", "kei\u00b7nen", "Schritt", "zu", "tun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "wenns Abend ist ...", "tokens": ["wenns", "A\u00b7bend", "ist", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "An solchen Abenden", "tokens": ["An", "sol\u00b7chen", "A\u00b7ben\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.16": {"text": "hungert der Tod nach meinem Fleische! Blut", "tokens": ["hun\u00b7gert", "der", "Tod", "nach", "mei\u00b7nem", "Flei\u00b7sche", "!", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "aus allen meinen Wunden, hei\u00df!", "tokens": ["aus", "al\u00b7len", "mei\u00b7nen", "Wun\u00b7den", ",", "hei\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "und meine Klagen gehn wie durch den Herbst,", "tokens": ["und", "mei\u00b7ne", "Kla\u00b7gen", "gehn", "wie", "durch", "den", "Herbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "in rauhem Wind, wie mit verw\u00f6hnten F\u00fc\u00dfen", "tokens": ["in", "rau\u00b7hem", "Wind", ",", "wie", "mit", "ver\u00b7w\u00f6hn\u00b7ten", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "hin \u00fcber abgefallen, schwarz Ge\u00e4st ...", "tokens": ["hin", "\u00fc\u00b7ber", "ab\u00b7ge\u00b7fal\u00b7len", ",", "schwarz", "Ge\u00b7\u00e4st", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVPP", "$,", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und naht die Nacht, und sind die Fraun", "tokens": ["Und", "naht", "die", "Nacht", ",", "und", "sind", "die", "Fraun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bei ihren M\u00e4nnern ... alle Not,", "tokens": ["bei", "ih\u00b7ren", "M\u00e4n\u00b7nern", "...", "al\u00b7le", "Not", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die ich am Tage sang, flammt doppelt rot!", "tokens": ["die", "ich", "am", "Ta\u00b7ge", "sang", ",", "flammt", "dop\u00b7pelt", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich seh von B\u00e4um und Bergen eine Welt", "tokens": ["Ich", "seh", "von", "B\u00e4um", "und", "Ber\u00b7gen", "ei\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und Wasser viel und Boote, die im Traum", "tokens": ["und", "Was\u00b7ser", "viel", "und", "Boo\u00b7te", ",", "die", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "hingleiten; Wege, eng gesellt;", "tokens": ["hing\u00b7lei\u00b7ten", ";", "We\u00b7ge", ",", "eng", "ge\u00b7sellt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und grelle Lichter \u00fcber Heimlichkeiten.", "tokens": ["und", "grel\u00b7le", "Lich\u00b7ter", "\u00fc\u00b7ber", "Heim\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da zwingt ein Mann ein Weib mit einem Fluch,", "tokens": ["Da", "zwingt", "ein", "Mann", "ein", "Weib", "mit", "ei\u00b7nem", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "da betet eines Mannes Brunst zur Hure,", "tokens": ["da", "be\u00b7tet", "ei\u00b7nes", "Man\u00b7nes", "Brunst", "zur", "Hu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "da reden Zweie wie ein Buch,", "tokens": ["da", "re\u00b7den", "Zwei\u00b7e", "wie", "ein", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "dort heben sich vier H\u00e4nde hoch zum Schw\u00fcre,", "tokens": ["dort", "he\u00b7ben", "sich", "vier", "H\u00e4n\u00b7de", "hoch", "zum", "Schw\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "CARD", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "da nahn sich zwei, von dumpfer Lust entstellt,", "tokens": ["da", "nahn", "sich", "zwei", ",", "von", "dum\u00b7pfer", "Lust", "ent\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "CARD", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "verzerrt, und \u00fcberbieten", "tokens": ["ver\u00b7zerrt", ",", "und", "\u00fc\u00b7berb\u00b7ie\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "sich sinnlos an ererbter Kraft,", "tokens": ["sich", "sinn\u00b7los", "an", "er\u00b7erb\u00b7ter", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "dort rinnet eines Alten Saft", "tokens": ["dort", "rin\u00b7net", "ei\u00b7nes", "Al\u00b7ten", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "gelb und vergelbt hin unter jungen Bl\u00fcten ...", "tokens": ["gelb", "und", "ver\u00b7gelbt", "hin", "un\u00b7ter", "jun\u00b7gen", "Bl\u00fc\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.17": {"text": "da bin ich sehend! \u00fcberhell", "tokens": ["da", "bin", "ich", "se\u00b7hend", "!", "\u00fc\u00b7ber\u00b7hell"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$.", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "str\u00f6mts in mich, in mich, dreimal klar!", "tokens": ["str\u00f6mts", "in", "mich", ",", "in", "mich", ",", "drei\u00b7mal", "klar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "dreimal verflammt mein dunkles Haar", "tokens": ["drei\u00b7mal", "ver\u00b7flammt", "mein", "dunk\u00b7les", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "in eisigen, vereisten Winden!", "tokens": ["in", "ei\u00b7si\u00b7gen", ",", "ver\u00b7eis\u00b7ten", "Win\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "in meinem Leib ver\u00e4chzt die Glut!", "tokens": ["in", "mei\u00b7nem", "Leib", "ver\u00b7\u00e4chzt", "die", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "bin eine Seherin \u00fcber Blinden!", "tokens": ["bin", "ei\u00b7ne", "Se\u00b7he\u00b7rin", "\u00fc\u00b7ber", "Blin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "und wie dies weh, wie Sterben, tut,", "tokens": ["und", "wie", "dies", "weh", ",", "wie", "Ster\u00b7ben", ",", "tut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PDS", "PTKVZ", "$,", "PWAV", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "so weh \u2013 \u2013 \u2013 \u2013", "tokens": ["so", "weh", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "$(", "$(", "$(", "$("], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.26": {"text": "Sp\u00e4t lieg ich bei den gro\u00dfen Steinen", "tokens": ["Sp\u00e4t", "lieg", "ich", "bei", "den", "gro\u00b7\u00dfen", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "wach wie bei M\u00e4nnern, ... o so rauh!", "tokens": ["wach", "wie", "bei", "M\u00e4n\u00b7nern", ",", "...", "o", "so", "rauh", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "$,", "$(", "FM", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nun bin ich aller Steine Frau", "tokens": ["Nun", "bin", "ich", "al\u00b7ler", "Stei\u00b7ne", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "mit einem unerl\u00f6sten Weinen ...", "tokens": ["mit", "ei\u00b7nem", "un\u00b7er\u00b7l\u00f6s\u00b7ten", "Wei\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}