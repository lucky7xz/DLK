{"textgrid.poem.33319": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der Freier aus Religionsgr\u00fcnden", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinweg von mir, ihr Furien,", "tokens": ["Hin\u00b7weg", "von", "mir", ",", "ihr", "Fu\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sieben Katechismus-S\u00fcnden!", "tokens": ["Ihr", "sie\u00b7ben", "Ka\u00b7te\u00b7chis\u00b7mus\u00b7S\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein junges Weibchen, fromm und sch\u00f6n,", "tokens": ["Ein", "jun\u00b7ges", "Weib\u00b7chen", ",", "fromm", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll mir euch helfen \u00fcberwinden.", "tokens": ["Soll", "mir", "euch", "hel\u00b7fen", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du schn\u00f6de Hoffahrt trolle dich,", "tokens": ["Du", "schn\u00f6\u00b7de", "Hof\u00b7fahrt", "trol\u00b7le", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird in Demuth dich verkehren,", "tokens": ["Sie", "wird", "in", "De\u00b7muth", "dich", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ihr kleines M\u00f6pschen mich", "tokens": ["Und", "wie", "ihr", "klei\u00b7nes", "M\u00f6p\u00b7schen", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gehorsamst apportiren lehren.", "tokens": ["Ge\u00b7hor\u00b7samst", "ap\u00b7por\u00b7ti\u00b7ren", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Du hellerkarge Filzigkeit", "tokens": ["Du", "hel\u00b7ler\u00b7kar\u00b7ge", "Fil\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sollst mich nun l\u00e4nger nicht beth\u00f6ren:", "tokens": ["Sollst", "mich", "nun", "l\u00e4n\u00b7ger", "nicht", "be\u00b7th\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Weib wird mich Freigebigkeit", "tokens": ["Mein", "Weib", "wird", "mich", "Frei\u00b7ge\u00b7big\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "F\u00fcr Modekr\u00e4merinen lehren.", "tokens": ["F\u00fcr", "Mo\u00b7de\u00b7kr\u00e4\u00b7me\u00b7ri\u00b7nen", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Trieb des Fleisches, magst dich bl\u00e4h'n;", "tokens": ["Du", "Trieb", "des", "Flei\u00b7sches", ",", "magst", "dich", "bl\u00e4h'n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wei\u00df ein Mittel, dich zu d\u00e4mpfen,", "tokens": ["Sie", "wei\u00df", "ein", "Mit\u00b7tel", ",", "dich", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wird zum Nimmeraufersteh'n", "tokens": ["Sie", "wird", "zum", "Nim\u00b7mer\u00b7au\u00b7fer\u00b7steh'n"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im kurzen dich zu Boden k\u00e4mpfen.", "tokens": ["Im", "kur\u00b7zen", "dich", "zu", "Bo\u00b7den", "k\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du b\u00f6ser Neid, flieh Augenblick's,", "tokens": ["Du", "b\u00f6\u00b7ser", "Neid", ",", "flieh", "Au\u00b7gen\u00b7blick's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sollst mich nimmermehr betr\u00fcben;", "tokens": ["Du", "sollst", "mich", "nim\u00b7mer\u00b7mehr", "be\u00b7tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wird mich lehren fremden Gl\u00fcck's", "tokens": ["Sie", "wird", "mich", "leh\u00b7ren", "frem\u00b7den", "Gl\u00fc\u00b7ck's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich freu'n, und meine Schw\u00e4ger lieben.", "tokens": ["Mich", "freu'n", ",", "und", "mei\u00b7ne", "Schw\u00e4\u00b7ger", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vergebens bist du auch bem\u00fcht,", "tokens": ["Ver\u00b7ge\u00b7bens", "bist", "du", "auch", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O V\u00f6llerei, mich zu verf\u00fchren;", "tokens": ["O", "V\u00f6l\u00b7le\u00b7rei", ",", "mich", "zu", "ver\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wird mir schon den Appetit", "tokens": ["Sie", "wird", "mir", "schon", "den", "Ap\u00b7pe\u00b7tit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Tische wegmoralisiren.", "tokens": ["Bei", "Ti\u00b7sche", "weg\u00b7mo\u00b7ra\u00b7li\u00b7si\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du, Zorn, sollst k\u00fcnftig weder Kinn", "tokens": ["Du", ",", "Zorn", ",", "sollst", "k\u00fcnf\u00b7tig", "we\u00b7der", "Kinn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch Augenbraunen mir verschieben;", "tokens": ["Noch", "Au\u00b7gen\u00b7brau\u00b7nen", "mir", "ver\u00b7schie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Weibchen wird sich schon bem\u00fch'n,", "tokens": ["Mein", "Weib\u00b7chen", "wird", "sich", "schon", "be\u00b7m\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich stets in der Geduld zu \u00fcben.", "tokens": ["Mich", "stets", "in", "der", "Ge\u00b7duld", "zu", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auch dein, o Tr\u00e4gheit, lach' ich dann;", "tokens": ["Auch", "dein", ",", "o", "Tr\u00e4g\u00b7heit", ",", "lach'", "ich", "dann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird f\u00fcr meinen Flei\u00df schon sorgen,", "tokens": ["Sie", "wird", "f\u00fcr", "mei\u00b7nen", "Flei\u00df", "schon", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mehr, als ich verdienen kann,", "tokens": ["Und", "mehr", ",", "als", "ich", "ver\u00b7die\u00b7nen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Spitzen, Hauben, B\u00e4nder borgen.", "tokens": ["F\u00fcr", "Spit\u00b7zen", ",", "Hau\u00b7ben", ",", "B\u00e4n\u00b7der", "bor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So wird sie mich vor S\u00fcnden hier,", "tokens": ["So", "wird", "sie", "mich", "vor", "S\u00fcn\u00b7den", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und vor der H\u00f6lle dort bewahren,", "tokens": ["Und", "vor", "der", "H\u00f6l\u00b7le", "dort", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u2013 leb' ich l\u00e4nger noch mit ihr \u2013", "tokens": ["Und", "\u2013", "leb'", "ich", "l\u00e4n\u00b7ger", "noch", "mit", "ihr", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVIMP", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "$("], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mir auch das Fegfeuer ersparen.", "tokens": ["Mir", "auch", "das", "Feg\u00b7feu\u00b7er", "er\u00b7spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}}}}