{"textgrid.poem.25995": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du bist vornehm wie die grauen Nachtigallen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist vornehm wie die grauen Nachtigallen, die sich im Dunkel gefallen;", "tokens": ["Du", "bist", "vor\u00b7nehm", "wie", "die", "grau\u00b7en", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "die", "sich", "im", "Dun\u00b7kel", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "+-----+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Verborgen wie die Veilchen, die blauen, die im Grase knien und nach innen schauen.", "tokens": ["Ver\u00b7bor\u00b7gen", "wie", "die", "Veil\u00b7chen", ",", "die", "blau\u00b7en", ",", "die", "im", "Gra\u00b7se", "kni\u00b7en", "und", "nach", "in\u00b7nen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "ART", "ADJA", "$,", "PRELS", "APPRART", "NN", "VVFIN", "KON", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Aber ein breiter Strahl stahl sich aus deiner Brust,", "tokens": ["A\u00b7ber", "ein", "brei\u00b7ter", "Strahl", "stahl", "sich", "aus", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Drinnen lebe ich, ganz goldener Staub und Lust.", "tokens": ["Drin\u00b7nen", "le\u00b7be", "ich", ",", "ganz", "gol\u00b7de\u00b7ner", "Staub", "und", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}}}}