{"textgrid.poem.44399": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber die Unterschrift an Webers Portr\u00e4t \u00bbWie Gott will!\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie Gott will!\u00ab so sprach ein Kutscher,", "tokens": ["\u00bb", "wie", "Gott", "will", "!", "\u00ab", "so", "sprach", "ein", "Kut\u00b7scher", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "VMFIN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hing die Z\u00fcgel, fuhr vom Fleck;", "tokens": ["Hing", "die", "Z\u00fc\u00b7gel", ",", "fuhr", "vom", "Fleck", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie Gott will! Der Wagen taumelt,", "tokens": ["Wie", "Gott", "will", "!", "Der", "Wa\u00b7gen", "tau\u00b7melt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VMFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schl\u00e4gt sich \u00fcber, liegt im Dreck.", "tokens": ["Schl\u00e4gt", "sich", "\u00fc\u00b7ber", ",", "liegt", "im", "Dreck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der Herr springt aus dem Wagen,", "tokens": ["Und", "der", "Herr", "springt", "aus", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Schwingt sein Rohr und schl\u00e4gt drauf zu:", "tokens": ["Schwingt", "sein", "Rohr", "und", "schl\u00e4gt", "drauf", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwie Gott will, so f\u00e4hrt ein jeder,", "tokens": ["\u00bb", "wie", "Gott", "will", ",", "so", "f\u00e4hrt", "ein", "je\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "VMFIN", "$,", "ADV", "VVFIN", "ART", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hans und J\u00f6rge, nicht blo\u00df du;", "tokens": ["Hans", "und", "J\u00f6r\u00b7ge", ",", "nicht", "blo\u00df", "du", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "PTKNEG", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber willst du ", "tokens": ["A\u00b7ber", "willst", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Triff dein Ziel nach ", "tokens": ["Triff", "dein", "Ziel", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wollt auch \u2013 Er verzeih die S\u00fcnde! \u2013", "tokens": ["Wollt", "auch", "\u2013", "Er", "ver\u00b7zeih", "die", "S\u00fcn\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "$(", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Herrgott selber nicht.\u00ab", "tokens": ["Un\u00b7ser", "Herr\u00b7gott", "sel\u00b7ber", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}