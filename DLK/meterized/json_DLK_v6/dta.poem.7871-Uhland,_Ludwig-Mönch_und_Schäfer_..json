{"dta.poem.7871": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "M\u00f6nch und Sch\u00e4fer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Du klage nicht! Was ist dein Weh?", "tokens": ["Du", "kla\u00b7ge", "nicht", "!", "Was", "ist", "dein", "Weh", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was, als ein schwerer Traum?", "tokens": ["Was", ",", "als", "ein", "schwe\u00b7rer", "Traum", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald gl\u00e4nzt die Blume aus dem Klee,", "tokens": ["Bald", "gl\u00e4nzt", "die", "Blu\u00b7me", "aus", "dem", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00fcthe von dem Baum.", "tokens": ["Die", "Bl\u00fc\u00b7the", "von", "dem", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann steht das Kreuz, davor ich knie\u2019,", "tokens": ["Dann", "steht", "das", "Kreuz", ",", "da\u00b7vor", "ich", "knie'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im gr\u00fcnen Baumgefild;", "tokens": ["Im", "gr\u00fc\u00b7nen", "Baum\u00b7ge\u00b7fild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ach! es gr\u00fcnt und bl\u00fchet nie,", "tokens": ["Doch", "ach", "!", "es", "gr\u00fcnt", "und", "bl\u00fc\u00b7het", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt stets ein sterbend Bild.", "tokens": ["Tr\u00e4gt", "stets", "ein", "ster\u00b7bend", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}