{"textgrid.poem.42810": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Schokolade keimt,", "tokens": ["Wenn", "die", "Scho\u00b7ko\u00b7la\u00b7de", "keimt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn nach langem Druck bei Dichterlingen", "tokens": ["Wenn", "nach", "lan\u00b7gem", "Druck", "bei", "Dich\u00b7ter\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbglockenklingen\u00ab sich auf \u00bbLenzesschwingen\u00ab", "tokens": ["\u00bb", "glo\u00b7cken\u00b7klin\u00b7gen", "\u00ab", "sich", "auf", "\u00bb", "Len\u00b7zes\u00b7schwin\u00b7gen", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVINF", "$(", "PRF", "APPR", "$(", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Endlich reimt", "tokens": ["End\u00b7lich", "reimt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und der Osterhase hinten auch schon pre\u00dft,", "tokens": ["Und", "der", "Os\u00b7ter\u00b7ha\u00b7se", "hin\u00b7ten", "auch", "schon", "pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Dann kommt bald das Osterfest.", "tokens": ["Dann", "kommt", "bald", "das", "Os\u00b7ter\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wenn wirklich dann mit Glockenklingen", "tokens": ["Und", "wenn", "wirk\u00b7lich", "dann", "mit", "Glo\u00b7cken\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ostern naht auf Lenzesschwingen, \u2013 \u2013 \u2013", "tokens": ["Os\u00b7tern", "naht", "auf", "Len\u00b7zes\u00b7schwin\u00b7gen", ",", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,", "$(", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann mit jenen Dichterlingen", "tokens": ["Dann", "mit", "je\u00b7nen", "Dich\u00b7ter\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit deren jugendlichen Br\u00e4uten", "tokens": ["Und", "mit", "de\u00b7ren", "ju\u00b7gend\u00b7li\u00b7chen", "Br\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Drau\u00dfen schwelgen mit berauschten H\u00e4nden \u2013 \u2013 \u2013", "tokens": ["Drau\u00b7\u00dfen", "schwel\u00b7gen", "mit", "be\u00b7rauschten", "H\u00e4n\u00b7den", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$(", "$(", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ach, das denk ich mir entsetzlich,", "tokens": ["Ach", ",", "das", "denk", "ich", "mir", "ent\u00b7setz\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Au\u00dferdem \u2013 unter Umst\u00e4nden \u2013", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "\u2013", "un\u00b7ter", "Um\u00b7st\u00e4n\u00b7den", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$(", "APPR", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Ungesetzlich.", "tokens": ["Un\u00b7ge\u00b7setz\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Aber morgens auf dem Fr\u00fchst\u00fcckstische", "tokens": ["A\u00b7ber", "mor\u00b7gens", "auf", "dem", "Fr\u00fch\u00b7st\u00fccks\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "F\u00fcnf, sechs, sieben flaumweich gelbe, frische", "tokens": ["F\u00fcnf", ",", "sechs", ",", "sie\u00b7ben", "flaum\u00b7weich", "gel\u00b7be", ",", "fri\u00b7sche"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["CARD", "$,", "CARD", "$,", "CARD", "ADJD", "ADJA", "$,", "ADJA"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Eier. Und dann ganz hineingekniet!", "tokens": ["Ei\u00b7er", ".", "Und", "dann", "ganz", "hin\u00b7ein\u00b7ge\u00b7kniet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ha! Da sp\u00fcrt man, wie die Fr\u00fchlingsw\u00e4rme", "tokens": ["Ha", "!", "Da", "sp\u00fcrt", "man", ",", "wie", "die", "Fr\u00fch\u00b7lings\u00b7w\u00e4r\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Durch geheime G\u00e4nge und Ged\u00e4rme", "tokens": ["Durch", "ge\u00b7hei\u00b7me", "G\u00e4n\u00b7ge", "und", "Ge\u00b7d\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In die Zukunft zieht", "tokens": ["In", "die", "Zu\u00b7kunft", "zieht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und wie dankbar wir f\u00fcr solchen Segen", "tokens": ["Und", "wie", "dank\u00b7bar", "wir", "f\u00fcr", "sol\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "PPER", "APPR", "PIAT", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Sein m\u00fcssen.", "tokens": ["Sein", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "VMINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Ach, ich k\u00f6nnte alle Hennen k\u00fcssen,", "tokens": ["Ach", ",", "ich", "k\u00f6nn\u00b7te", "al\u00b7le", "Hen\u00b7nen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die so langgezogene Kugeln legen.", "tokens": ["Die", "so", "lang\u00b7ge\u00b7zo\u00b7ge\u00b7ne", "Ku\u00b7geln", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}