{"textgrid.poem.49515": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Catulls 13. Sinngedicht", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen sollstu bey mir, wie ein K\u00f6nig,", "tokens": ["Mor\u00b7gen", "soll\u00b7stu", "bey", "mir", ",", "wie", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sind die G\u00f6tter dir gewogen, speisen:", "tokens": ["Sind", "die", "G\u00f6t\u00b7ter", "dir", "ge\u00b7wo\u00b7gen", ",", "spei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wohlverstanden, wenn du deine K\u00fcche,", "tokens": ["Wohl\u00b7ver\u00b7stan\u00b7den", ",", "wenn", "du", "dei\u00b7ne", "K\u00fc\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine leckre wohlgespickte K\u00fcche,", "tokens": ["Dei\u00b7ne", "leck\u00b7re", "wohl\u00b7ge\u00b7spick\u00b7te", "K\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Attisch Salz, und Chierwein, und Scherze,", "tokens": ["At\u00b7tisch", "Salz", ",", "und", "Chier\u00b7wein", ",", "und", "Scher\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und dein blondes M\u00e4dchen mit dir bringest.", "tokens": ["Und", "dein", "blon\u00b7des", "M\u00e4d\u00b7chen", "mit", "dir", "brin\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wie ein K\u00f6nig sollstu bey mir speisen,", "tokens": ["Wie", "ein", "K\u00f6\u00b7nig", "soll\u00b7stu", "bey", "mir", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wenn du, sag ich, alles mit dir bringest.", "tokens": ["Wenn", "du", ",", "sag", "ich", ",", "al\u00b7les", "mit", "dir", "brin\u00b7gest", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "PPER", "$,", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Denn ach leider! deines Fre\u00fcndes B\u00f6rse,", "tokens": ["Denn", "ach", "lei\u00b7der", "!", "dei\u00b7nes", "Fre\u00fcn\u00b7des", "B\u00f6r\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Mein Fabullus, ist voll Spinneweben.", "tokens": ["Mein", "Fa\u00b7bul\u00b7lus", ",", "ist", "voll", "Spin\u00b7ne\u00b7we\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADJD", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Doch statt dessen will ich dich mit Blicken", "tokens": ["Doch", "statt", "des\u00b7sen", "will", "ich", "dich", "mit", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDS", "VMFIN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Voll Empfindungen der treusten Liebe,", "tokens": ["Voll", "Emp\u00b7fin\u00b7dun\u00b7gen", "der", "treus\u00b7ten", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und wenn was noch holder ist, bedienen;", "tokens": ["Und", "wenn", "was", "noch", "hol\u00b7der", "ist", ",", "be\u00b7die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Auch den Balsam dir zu riechen geben,", "tokens": ["Auch", "den", "Bal\u00b7sam", "dir", "zu", "rie\u00b7chen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Den die Grazien und Amuretten", "tokens": ["Den", "die", "Gra\u00b7zi\u00b7en", "und", "A\u00b7mu\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Meinem holden M\u00e4dchen einst verehrten.", "tokens": ["Mei\u00b7nem", "hol\u00b7den", "M\u00e4d\u00b7chen", "einst", "ver\u00b7ehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Welchen, wenn du ihn zu riechen anf\u00e4ngst,", "tokens": ["Wel\u00b7chen", ",", "wenn", "du", "ihn", "zu", "rie\u00b7chen", "an\u00b7f\u00e4ngst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Wirstu, schw\u00f6r ich, alle G\u00f6tter bitten:", "tokens": ["Wirs\u00b7tu", ",", "schw\u00f6r", "ich", ",", "al\u00b7le", "G\u00f6t\u00b7ter", "bit\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Macht mich, macht mich doch zu lauter Nase!", "tokens": ["Macht", "mich", ",", "macht", "mich", "doch", "zu", "lau\u00b7ter", "Na\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}