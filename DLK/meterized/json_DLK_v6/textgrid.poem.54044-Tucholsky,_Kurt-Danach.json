{"textgrid.poem.54044": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Danach", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird nach einem happy end", "tokens": ["Es", "wird", "nach", "ei\u00b7nem", "hap\u00b7py", "end"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Film jew\u00f6hnlich abjeblendt.", "tokens": ["im", "Film", "je\u00b7w\u00f6hn\u00b7lich", "ab\u00b7jeb\u00b7lendt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sieht blo\u00df noch in ihre Lippen", "tokens": ["Man", "sieht", "blo\u00df", "noch", "in", "ih\u00b7re", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den Helden seinen Schnurrbart stippen \u2013", "tokens": ["den", "Hel\u00b7den", "sei\u00b7nen", "Schnurr\u00b7bart", "stip\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da hat sie nu den Schentelmen.", "tokens": ["da", "hat", "sie", "nu", "den", "Schen\u00b7tel\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Na, un denn \u2013?", "tokens": ["Na", ",", "un", "denn", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "FM", "KON", "$(", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Denn jehn die beeden brav ins Bett.", "tokens": ["Denn", "jehn", "die", "bee\u00b7den", "brav", "ins", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Na ja . . . di\u00df is ja auch janz nett.", "tokens": ["Na", "ja", ".", ".", ".", "di\u00df", "is", "ja", "auch", "janz", "nett", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "$.", "$.", "PDS", "FM", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "A manchmal m\u00f6cht man doch jern wissn:", "tokens": ["A", "manch\u00b7mal", "m\u00f6cht", "man", "doch", "jern", "wissn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PIS", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wat tun se, wenn se sich nich kissn?", "tokens": ["Wat", "tun", "se", ",", "wenn", "se", "sich", "nich", "kissn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die k\u00f6nn ja doch nich imma penn . . . !", "tokens": ["Die", "k\u00f6nn", "ja", "doch", "nich", "im\u00b7ma", "penn", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "PTKNEG", "NE", "NE", "$.", "$.", "$.", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Na, un denn \u2013?", "tokens": ["Na", ",", "un", "denn", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "FM", "KON", "$(", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Denn s\u00e4uselt im Kamin der Wind.", "tokens": ["Denn", "s\u00e4u\u00b7selt", "im", "Ka\u00b7min", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn kricht det junge Paar 'n Kind.", "tokens": ["Denn", "kricht", "det", "jun\u00b7ge", "Paar", "'n", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn kocht sie Milch. Die Milch looft \u00fcba.", "tokens": ["Denn", "kocht", "sie", "Milch", ".", "Die", "Milch", "looft", "\u00fc\u00b7ba", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn macht er Krach. Denn weent sie dr\u00fcba.", "tokens": ["Denn", "macht", "er", "Krach", ".", "Denn", "weent", "sie", "dr\u00fc\u00b7ba", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$.", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wolln sich beede j\u00e4nzlich trenn . . .", "tokens": ["Denn", "wolln", "sich", "bee\u00b7de", "j\u00e4nz\u00b7lich", "trenn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVFIN", "ADJD", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Na, un denn \u2013?", "tokens": ["Na", ",", "un", "denn", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "FM", "KON", "$(", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Denn is det Kind nich uffn Damm.", "tokens": ["Denn", "is", "det", "Kind", "nich", "uffn", "Damm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "NN", "PTKNEG", "NE", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Denn bleihm die beeden doch zesamm.", "tokens": ["Denn", "bleihm", "die", "bee\u00b7den", "doch", "ze\u00b7samm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn qu\u00e4ln se sich noch manche Jahre.", "tokens": ["Denn", "qu\u00e4ln", "se", "sich", "noch", "man\u00b7che", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "PRF", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er will noch wat mit blonde Haare:", "tokens": ["Er", "will", "noch", "wat", "mit", "blon\u00b7de", "Haa\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "vorn doof und hinten minorenn . . .", "tokens": ["vorn", "doof", "und", "hin\u00b7ten", "mi\u00b7no\u00b7renn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "NE", "KON", "ADV", "ADV", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Na, un denn \u2013?", "tokens": ["Na", ",", "un", "denn", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "FM", "KON", "$(", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Denn sind se alt.", "tokens": ["Denn", "sind", "se", "alt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Sohn haut ab.", "tokens": ["Der", "Sohn", "haut", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Olle macht nu ooch bald schlapp.", "tokens": ["Der", "Ol\u00b7le", "macht", "nu", "o\u00b7och", "bald", "schlapp", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vajessen Ku\u00df und Schnurrbartzeit \u2013", "tokens": ["Va\u00b7jes\u00b7sen", "Ku\u00df", "und", "Schnurr\u00b7bart\u00b7zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, Menschenskind, wie liecht det weit!", "tokens": ["Ach", ",", "Men\u00b7schen\u00b7skind", ",", "wie", "liecht", "det", "weit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PWAV", "ADJD", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie der noch scharf uff Muttern war,", "tokens": ["Wie", "der", "noch", "scharf", "uff", "Mut\u00b7tern", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "ADJD", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "det is schon beinah nich mehr wahr!", "tokens": ["det", "is", "schon", "bei\u00b7nah", "nich", "mehr", "wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der olle Mann denkt so zur\u00fcck:", "tokens": ["Der", "ol\u00b7le", "Mann", "denkt", "so", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "wat hat er nu von seinen Jl\u00fcck?", "tokens": ["wat", "hat", "er", "nu", "von", "sei\u00b7nen", "Jl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Ehe war zum jr\u00f6\u00dften Teile", "tokens": ["Die", "E\u00b7he", "war", "zum", "jr\u00f6\u00df\u00b7ten", "Tei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "vabr\u00fchte Milch un Langeweile.", "tokens": ["va\u00b7br\u00fch\u00b7te", "Milch", "un", "Lan\u00b7ge\u00b7wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und darum wird beim happy end", "tokens": ["Und", "da\u00b7rum", "wird", "beim", "hap\u00b7py", "end"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFI