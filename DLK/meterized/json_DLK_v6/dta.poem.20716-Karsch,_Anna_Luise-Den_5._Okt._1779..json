{"dta.poem.20716": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Den 5. Okt. 1779.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df Dich bey Leibe nicht vergleichen                   ", "tokens": ["La\u00df", "Dich", "bey", "Lei\u00b7be", "nicht", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meiner Kleinigkeit,", "tokens": ["Mit", "mei\u00b7ner", "Klei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich lief nur unter Haselstr\u00e4uchen", "tokens": ["Ich", "lief", "nur", "un\u00b7ter", "Ha\u00b7sel\u00b7str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In fr\u00fcher Jugendzeit,", "tokens": ["In", "fr\u00fc\u00b7her", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn unter einer Bacchuslaube", "tokens": ["Wenn", "un\u00b7ter", "ei\u00b7ner", "Bac\u00b7chus\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein zartes F\u00fc\u00dfchen ging,", "tokens": ["Dein", "zar\u00b7tes", "F\u00fc\u00df\u00b7chen", "ging", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Dir die sch\u00f6nste Purpurtraube", "tokens": ["Wo", "Dir", "die", "sch\u00f6ns\u00b7te", "Pur\u00b7pur\u00b7trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ins Rosenm\u00e4ulchen hing \u2014", "tokens": ["Ins", "Ro\u00b7sen\u00b7m\u00e4ul\u00b7chen", "hing"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich kannte nur die Nachtigallen;", "tokens": ["Ich", "kann\u00b7te", "nur", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kein buntes Papchen lie\u00df", "tokens": ["Kein", "bun\u00b7tes", "Pap\u00b7chen", "lie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Im Hause meinen Namen schallen,", "tokens": ["Im", "Hau\u00b7se", "mei\u00b7nen", "Na\u00b7men", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "F\u00fcrs Futter fein und s\u00fc\u00df.", "tokens": ["F\u00fcrs", "Fut\u00b7ter", "fein", "und", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Mein Sopha war nur Wiesenerde:", "tokens": ["Mein", "So\u00b7pha", "war", "nur", "Wie\u00b7se\u00b7ner\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "+-+--+---", "measure": "trochaic.tri.relaxed"}, "line.14": {"text": "Da schwatzete mein Mund", "tokens": ["Da", "schwat\u00b7ze\u00b7te", "mein", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Mit Blumen und mit meiner Heerde,", "tokens": ["Mit", "Blu\u00b7men", "und", "mit", "mei\u00b7ner", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die trieb ich ohne Hund.", "tokens": ["Die", "trieb", "ich", "oh\u00b7ne", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Mir horchten auf ein Wort drey Rinder,", "tokens": ["Mir", "horch\u00b7ten", "auf", "ein", "Wort", "drey", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wie Dir Fidelchen Boll,", "tokens": ["Wie", "Dir", "Fi\u00b7del\u00b7chen", "Boll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "NE", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Ich pflegte meiner Mutter Kinder,", "tokens": ["Ich", "pfleg\u00b7te", "mei\u00b7ner", "Mut\u00b7ter", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn Du von Liebe voll", "tokens": ["Wenn", "Du", "von", "Lie\u00b7be", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Auf Deinem Scho\u00dfe Zuckerk\u00fcchlein", "tokens": ["Auf", "Dei\u00b7nem", "Scho\u00b7\u00dfe", "Zu\u00b7cker\u00b7k\u00fcch\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Dem Kl\u00e4ffer gabst, und ihn", "tokens": ["Dem", "Kl\u00e4f\u00b7fer", "gabst", ",", "und", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Das Maul mit einem seidnen T\u00fcchlein", "tokens": ["Das", "Maul", "mit", "ei\u00b7nem", "seid\u00b7nen", "T\u00fcch\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Verstopftest, weil es schien,", "tokens": ["Ver\u00b7stopf\u00b7test", ",", "weil", "es", "schien", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Da\u00df er Mamachen wecken m\u00f6chte \u2014", "tokens": ["Da\u00df", "er", "Ma\u00b7ma\u00b7chen", "we\u00b7cken", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Du warst geboren reich;", "tokens": ["Du", "warst", "ge\u00b7bo\u00b7ren", "reich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Ich bin vom Ackerbaugeschlechte,", "tokens": ["Ich", "bin", "vom", "A\u00b7cker\u00b7bau\u00b7ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Darum ist ein Vergleich", "tokens": ["Da\u00b7rum", "ist", "ein", "Ver\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Nie zwischen Dir und mir zu machen.", "tokens": ["Nie", "zwi\u00b7schen", "Dir", "und", "mir", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Du singst dem Mann allein,", "tokens": ["Du", "singst", "dem", "Mann", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Bist gro\u00df, kannst \u00fcber F\u00fcrsten lachen;", "tokens": ["Bist", "gro\u00df", ",", "kannst", "\u00fc\u00b7ber", "F\u00fcrs\u00b7ten", "la\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Ich darf so stolz nicht seyn!", "tokens": ["Ich", "darf", "so", "stolz", "nicht", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Doch dring ich nicht auf Marmorstufen", "tokens": ["Doch", "dring", "ich", "nicht", "auf", "Mar\u00b7mor\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "An karger F\u00fcrsten Ohr:", "tokens": ["An", "kar\u00b7ger", "F\u00fcrs\u00b7ten", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Der K\u00f6nig selber lie\u00df mich rufen", "tokens": ["Der", "K\u00f6\u00b7nig", "sel\u00b7ber", "lie\u00df", "mich", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Nach Sanssouci empor,", "tokens": ["Nach", "Sans\u00b7sou\u00b7ci", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Ob er gleich nicht das Deutsche liebet;", "tokens": ["Ob", "er", "gleich", "nicht", "das", "Deut\u00b7sche", "lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und was kann ich davor,", "tokens": ["Und", "was", "kann", "ich", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Da\u00df Ferdinand mir Antwort giebet,", "tokens": ["Da\u00df", "Fer\u00b7di\u00b7nand", "mir", "Ant\u00b7wort", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Der gro\u00dfe Ferdinand!", "tokens": ["Der", "gro\u00b7\u00dfe", "Fer\u00b7di\u00b7nand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "So vielmal Ihm mein Herz geschrieben,", "tokens": ["So", "viel\u00b7mal", "Ihm", "mein", "Herz", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Von aller Haabsucht rein:", "tokens": ["Von", "al\u00b7ler", "Ha\u00b7ab\u00b7sucht", "rein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Er mu\u00df bey hohen Heldentrieben", "tokens": ["Er", "mu\u00df", "bey", "ho\u00b7hen", "Hel\u00b7den\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "So stolz wie Du nicht seyn.", "tokens": ["So", "stolz", "wie", "Du", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}