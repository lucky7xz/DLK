{"textgrid.poem.63049": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: An Lichtwehrs Arm gieng im Elysium", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Lichtwehrs Arm gieng im Elysium", "tokens": ["An", "Licht\u00b7wehrs", "Arm", "gieng", "im", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aesop, der f\u00fcr des deutschen Sch\u00fclers Ruhm", "tokens": ["A\u00b7e\u00b7sop", ",", "der", "f\u00fcr", "des", "deut\u00b7schen", "Sch\u00fc\u00b7lers", "Ruhm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "So kalt nicht ist, wie Deutschlands neue Barden,", "tokens": ["So", "kalt", "nicht", "ist", ",", "wie", "Deutschlands", "neu\u00b7e", "Bar\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "VAFIN", "$,", "PWAV", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Einst in verherrlichter Gestalt", "tokens": ["Einst", "in", "ver\u00b7herr\u00b7lich\u00b7ter", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf einem Pfad von Thymian und Narden", "tokens": ["Auf", "ei\u00b7nem", "Pfad", "von", "Thy\u00b7mi\u00b7an", "und", "Nar\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im w\u00f6lbenden Cypressenwald.", "tokens": ["Im", "w\u00f6l\u00b7ben\u00b7den", "Cyp\u00b7res\u00b7sen\u00b7wald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da kam mit einem hehren Schatten,", "tokens": ["Da", "kam", "mit", "ei\u00b7nem", "heh\u00b7ren", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf dessen Antlitz heitre Ruh", "tokens": ["Auf", "des\u00b7sen", "Ant\u00b7litz", "heit\u00b7re", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und Weisheit sich gepaaret hatten,", "tokens": ["Und", "Weis\u00b7heit", "sich", "ge\u00b7paa\u00b7ret", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der alte Theokrit auf beede Freunde zu.", "tokens": ["Der", "al\u00b7te", "Theo\u00b7krit", "auf", "bee\u00b7de", "Freun\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Sein Ge\u00dfner wars, der eben an der K\u00fcste", "tokens": ["Sein", "Ge\u00df\u00b7ner", "wars", ",", "der", "e\u00b7ben", "an", "der", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der bessern Welt gelandet war.", "tokens": ["Der", "bes\u00b7sern", "Welt", "ge\u00b7lan\u00b7det", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er nennt mit frohem Stolz ihn dem vertrauten Paar,", "tokens": ["Er", "nennt", "mit", "fro\u00b7hem", "Stolz", "ihn", "dem", "ver\u00b7trau\u00b7ten", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aesop trat vor ihn hin, und gr\u00fc\u00dfte", "tokens": ["A\u00b7e\u00b7sop", "trat", "vor", "ihn", "hin", ",", "und", "gr\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Mit einem Ku\u00df den S\u00e4nger der Natur:", "tokens": ["Mit", "ei\u00b7nem", "Ku\u00df", "den", "S\u00e4n\u00b7ger", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Willkommen, sprach er, Freund, auf unsrer Flur", "tokens": ["Will\u00b7kom\u00b7men", ",", "sprach", "er", ",", "Freund", ",", "auf", "uns\u00b7rer", "Flur"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und nahm den Kranz, der seine Schl\u00e4fe schm\u00fcckte", "tokens": ["Und", "nahm", "den", "Kranz", ",", "der", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und setzt ihn Ge\u00dfnern auf. Der edle Schweitzer b\u00fcckte", "tokens": ["Und", "setzt", "ihn", "Ge\u00df\u00b7nern", "auf", ".", "Der", "ed\u00b7le", "Schweit\u00b7zer", "b\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Besch\u00e4mt das Haupt zur\u00fcck. Empfange dieses Pfand", "tokens": ["Be\u00b7sch\u00e4mt", "das", "Haupt", "zu\u00b7r\u00fcck", ".", "Emp\u00b7fan\u00b7ge", "die\u00b7ses", "Pfand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$.", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Des Sieges, rief Aesop: es war in deiner Jugend", "tokens": ["Des", "Sie\u00b7ges", ",", "rief", "A\u00b7e\u00b7sop", ":", "es", "war", "in", "dei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "NE", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.21": {"text": "Dir schon bestimmt \u2013ich gab den Thieren nur Verstand", "tokens": ["Dir", "schon", "be\u00b7stimmt", "\u2013", "ich", "gab", "den", "Thie\u00b7ren", "nur", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$(", "PPER", "VVFIN", "ART", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und du gabst deinen Hirten Tugend.", "tokens": ["Und", "du", "gabst", "dei\u00b7nen", "Hir\u00b7ten", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}