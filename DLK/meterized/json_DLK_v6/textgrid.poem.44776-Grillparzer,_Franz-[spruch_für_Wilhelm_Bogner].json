{"textgrid.poem.44776": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[spruch f\u00fcr Wilhelm Bogner]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die, welche Vater, Mutter ehrten,", "tokens": ["Die", ",", "wel\u00b7che", "Va\u00b7ter", ",", "Mut\u00b7ter", "ehr\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PWAT", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie leben lange, hei\u00dfts, auf Erden,", "tokens": ["Sie", "le\u00b7ben", "lan\u00b7ge", ",", "hei\u00dfts", ",", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein, wer denkt denn da an sich?", "tokens": ["Al\u00b7lein", ",", "wer", "denkt", "denn", "da", "an", "sich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "ADV", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ja, k\u00f6nnt ich deine Tage mehren,", "tokens": ["Ja", ",", "k\u00f6nnt", "ich", "dei\u00b7ne", "Ta\u00b7ge", "meh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wollte gern dich doppelt ehren,", "tokens": ["Ich", "woll\u00b7te", "gern", "dich", "dop\u00b7pelt", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das w\u00e4r denn doch ein Lohn f\u00fcr mich.", "tokens": ["Das", "w\u00e4r", "denn", "doch", "ein", "Lohn", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}