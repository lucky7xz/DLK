{"textgrid.poem.65019": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst in bess'rer Zeit vermochte ich", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst in bess'rer Zeit vermochte ich", "tokens": ["Einst", "in", "bess'\u00b7rer", "Zeit", "ver\u00b7moch\u00b7te", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Deines Daseins W\u00fcste zu versch\u00f6nen,", "tokens": ["Dei\u00b7nes", "Da\u00b7seins", "W\u00fcs\u00b7te", "zu", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Einst vermochte meine Liebe dich", "tokens": ["Einst", "ver\u00b7moch\u00b7te", "mei\u00b7ne", "Lie\u00b7be", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit dem dunkeln Gegner zu vers\u00f6hnen.", "tokens": ["Mit", "dem", "dun\u00b7keln", "Geg\u00b7ner", "zu", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O wie machte da in Himmelslust", "tokens": ["O", "wie", "mach\u00b7te", "da", "in", "Him\u00b7mels\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mir das Herz, in Stolz und Wonne, schwellen", "tokens": ["Mir", "das", "Herz", ",", "in", "Stolz", "und", "Won\u00b7ne", ",", "schwel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der Gedanke, einer Menschenbrust", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", ",", "ei\u00b7ner", "Men\u00b7schen\u00b7brust"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Leben, Tod und Jenseits zu erhellen.", "tokens": ["Le\u00b7ben", ",", "Tod", "und", "Jen\u00b7seits", "zu", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ob mein eig'nes Schicksal tr\u00fcb und schwer", "tokens": ["Ob", "mein", "eig'\u00b7nes", "Schick\u00b7sal", "tr\u00fcb", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und gebeugt mein Haupt, das kummermatte \u2013", "tokens": ["Und", "ge\u00b7beugt", "mein", "Haupt", ",", "das", "kum\u00b7mer\u00b7mat\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Arm und elend d\u00fcnkt ich mir nicht mehr,", "tokens": ["Arm", "und", "e\u00b7lend", "d\u00fcnkt", "ich", "mir", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da ich Freuden dir zu spenden hatte.", "tokens": ["Da", "ich", "Freu\u00b7den", "dir", "zu", "spen\u00b7den", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Muthig rang ich mit dem finstern Geiste,", "tokens": ["Mut\u00b7hig", "rang", "ich", "mit", "dem", "fins\u00b7tern", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kampfgeno\u00df war mir dein eig'nes Lieben,", "tokens": ["Kampf\u00b7ge\u00b7no\u00df", "war", "mir", "dein", "eig'\u00b7nes", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch seit deine Seele liebverwais't", "tokens": ["Doch", "seit", "dei\u00b7ne", "See\u00b7le", "lieb\u00b7ver\u00b7wais't"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ist dem D\u00e4mon schnell der Sieg verblieben. \u2013", "tokens": ["Ist", "dem", "D\u00e4\u00b7mon", "schnell", "der", "Sieg", "ver\u00b7blie\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Was ich sonst an mildem Trost dir gab", "tokens": ["Was", "ich", "sonst", "an", "mil\u00b7dem", "Trost", "dir", "gab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Trifft nicht mehr den Weg zu deinem Herzen,", "tokens": ["Trifft", "nicht", "mehr", "den", "Weg", "zu", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Worte gleiten machtlos ab,", "tokens": ["Mei\u00b7ne", "Wor\u00b7te", "glei\u00b7ten", "macht\u00b7los", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Von dem starren Panzer deiner Schmerzen.", "tokens": ["Von", "dem", "star\u00b7ren", "Pan\u00b7zer", "dei\u00b7ner", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Untergraben, nach dem Einsturz, seh'", "tokens": ["Un\u00b7ter\u00b7gra\u00b7ben", ",", "nach", "dem", "Ein\u00b7sturz", ",", "seh'"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich dein Sein, und kann es nicht mehr st\u00fctzen,", "tokens": ["Ich", "dein", "Sein", ",", "und", "kann", "es", "nicht", "mehr", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wilder lodert auf mein eig'nes Weh, \u2013", "tokens": ["Wil\u00b7der", "lo\u00b7dert", "auf", "mein", "eig'\u00b7nes", "Weh", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "O so m\u00f6ge Gott uns Beide sch\u00fctzen!", "tokens": ["O", "so", "m\u00f6\u00b7ge", "Gott", "uns", "Bei\u00b7de", "sch\u00fct\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "NN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}