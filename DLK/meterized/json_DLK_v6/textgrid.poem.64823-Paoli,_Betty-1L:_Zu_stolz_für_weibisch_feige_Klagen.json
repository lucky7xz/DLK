{"textgrid.poem.64823": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu stolz f\u00fcr weibisch feige Klagen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu stolz f\u00fcr weibisch feige Klagen", "tokens": ["Zu", "stolz", "f\u00fcr", "wei\u00b7bisch", "fei\u00b7ge", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch zu innerst tief verletzt,", "tokens": ["Und", "doch", "zu", "in\u00b7nerst", "tief", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um l\u00e4nger noch dies Sein zu tragen,", "tokens": ["Um", "l\u00e4n\u00b7ger", "noch", "dies", "Sein", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADV", "PDS", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du ihm rasch ein Ziel gesetzt.", "tokens": ["Hast", "du", "ihm", "rasch", "ein", "Ziel", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du warst mir werth im Seelengrunde,", "tokens": ["Du", "warst", "mir", "werth", "im", "See\u00b7len\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel theurer noch, als du's gewu\u00dft,", "tokens": ["Viel", "theu\u00b7rer", "noch", ",", "als", "du's", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deines Busens Todeswunde", "tokens": ["Und", "dei\u00b7nes", "Bu\u00b7sens", "To\u00b7des\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcht blutend auch in meiner Brust.", "tokens": ["Gl\u00fcht", "blu\u00b7tend", "auch", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch deine k\u00fchne Flucht betrauern,", "tokens": ["Doch", "dei\u00b7ne", "k\u00fch\u00b7ne", "Flucht", "be\u00b7trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit scheinheiligem Gesicht", "tokens": ["Und", "mit", "schein\u00b7hei\u00b7li\u00b7gem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor deiner That zur\u00fcckeschauern \u2013", "tokens": ["Vor", "dei\u00b7ner", "That", "zu\u00b7r\u00fc\u00b7cke\u00b7schau\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beim h\u00f6chsten Gott! das kann ich nicht.", "tokens": ["Beim", "h\u00f6chs\u00b7ten", "Gott", "!", "das", "kann", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Vogel, der im Waldessitze", "tokens": ["Ein", "Vo\u00b7gel", ",", "der", "im", "Wal\u00b7des\u00b7sit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einen Baum herniederflog", "tokens": ["Auf", "ei\u00b7nen", "Baum", "her\u00b7nie\u00b7der\u00b7flog"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, als der Sturm geknickt die St\u00fctze,", "tokens": ["Und", ",", "als", "der", "Sturm", "ge\u00b7knickt", "die", "St\u00fct\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschwinget wieder aufw\u00e4rts zog \u2013", "tokens": ["Be\u00b7schwin\u00b7get", "wie\u00b7der", "auf\u00b7w\u00e4rts", "zog", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In diesem Bild' seh ich Dich schimmern,", "tokens": ["In", "die\u00b7sem", "Bild'", "seh", "ich", "Dich", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Herz voll Liebe und Gesang,", "tokens": ["Du", "Herz", "voll", "Lie\u00b7be", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das von versunk'nen Gl\u00fcckes Tr\u00fcmmern", "tokens": ["Das", "von", "ver\u00b7sunk'\u00b7nen", "Gl\u00fc\u00b7ckes", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich rasch und k\u00fchn zum Himmel schwang.", "tokens": ["Sich", "rasch", "und", "k\u00fchn", "zum", "Him\u00b7mel", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}