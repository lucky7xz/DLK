{"textgrid.poem.48278": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Thomas Harrison", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.71", "fr:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbharrison, du zitterst?\u00ab", "tokens": ["\u00bb", "har\u00b7ri\u00b7son", ",", "du", "zit\u00b7terst", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbich zittre nicht von verlorenem Mut,", "tokens": ["\u00bb", "ich", "zitt\u00b7re", "nicht", "von", "ver\u00b7lo\u00b7re\u00b7nem", "Mut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich zittre von all dem verlorenen Blut,", "tokens": ["Ich", "zitt\u00b7re", "von", "all", "dem", "ver\u00b7lo\u00b7re\u00b7nen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Von all dem Blute, das ich verlor", "tokens": ["Von", "all", "dem", "Blu\u00b7te", ",", "das", "ich", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei Edgehill, Nasby und Marston-Moor,", "tokens": ["Bei", "Ed\u00b7ge\u00b7hill", ",", "Nas\u00b7by", "und", "Mar\u00b7ston\u00b7Moor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "KON", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Das ich verlor im Kampf wider euch \u2013", "tokens": ["Das", "ich", "ver\u00b7lor", "im", "Kampf", "wi\u00b7der", "euch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "PPER", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich zittre ", "tokens": ["Ich", "zitt\u00b7re"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}