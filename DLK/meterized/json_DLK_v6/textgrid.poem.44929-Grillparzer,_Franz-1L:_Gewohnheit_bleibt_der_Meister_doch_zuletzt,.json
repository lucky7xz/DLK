{"textgrid.poem.44929": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gewohnheit bleibt der Meister doch zuletzt,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewohnheit bleibt der Meister doch zuletzt,", "tokens": ["Ge\u00b7wohn\u00b7heit", "bleibt", "der", "Meis\u00b7ter", "doch", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was einer treibt, lehrt ihn die \u00dcbung sch\u00e4tzen,", "tokens": ["Was", "ei\u00b7ner", "treibt", ",", "lehrt", "ihn", "die", "\u00dc\u00b7bung", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kaum hatte er Lord Byron \u00fcbersetzt,", "tokens": ["Kaum", "hat\u00b7te", "er", "Lord", "By\u00b7ron", "\u00fc\u00b7bers\u00b7etzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kam ihm die Lust, sich selbst zu \u00fcbersetzen.", "tokens": ["Kam", "ihm", "die", "Lust", ",", "sich", "selbst", "zu", "\u00fc\u00b7bers\u00b7et\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}