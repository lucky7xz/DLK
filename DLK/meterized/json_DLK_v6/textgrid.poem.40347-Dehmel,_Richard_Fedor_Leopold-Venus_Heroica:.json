{"textgrid.poem.40347": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Heroica:", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein fremder Schein beirrt dich noch:", "tokens": ["Ein", "frem\u00b7der", "Schein", "be\u00b7irrt", "dich", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was sp\u00e4hst du nach Schiffen im Nebel,", "tokens": ["was", "sp\u00e4hst", "du", "nach", "Schif\u00b7fen", "im", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "von Andern gelenkt?!", "tokens": ["von", "An\u00b7dern", "ge\u00b7lenkt", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Aus deinem Leuchtturm blickst du hinab,", "tokens": ["Aus", "dei\u00b7nem", "Leucht\u00b7turm", "blickst", "du", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "und Str\u00f6me, auf denen der Erdball durchs Weltdunkel rast,", "tokens": ["und", "Str\u00f6\u00b7me", ",", "auf", "de\u00b7nen", "der", "Erd\u00b7ball", "durchs", "Welt\u00b7dun\u00b7kel", "rast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--++-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "rei\u00dfen an dir und reizen zum Sturz", "tokens": ["rei\u00b7\u00dfen", "an", "dir", "und", "rei\u00b7zen", "zum", "Sturz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "KON", "VVFIN", "APPRART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "hinunter ans lauernde Ufer.", "tokens": ["hin\u00b7un\u00b7ter", "ans", "lau\u00b7ern\u00b7de", "U\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Dort standest du schon als J\u00fcngling;", "tokens": ["Dort", "stan\u00b7dest", "du", "schon", "als", "J\u00fcng\u00b7ling", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und w\u00e4hrend Woge auf Woge kam,", "tokens": ["und", "w\u00e4h\u00b7rend", "Wo\u00b7ge", "auf", "Wo\u00b7ge", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "schriebst du, den Kr\u00fcckstock tief einbohrend,", "tokens": ["schriebst", "du", ",", "den", "Kr\u00fcck\u00b7stock", "tief", "ein\u00b7boh\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Namen auf Namen in den feuchten Triebsand,", "tokens": ["Na\u00b7men", "auf", "Na\u00b7men", "in", "den", "feuch\u00b7ten", "Trie\u00b7bsand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "geliebte Namen \u2013 und keiner blieb.", "tokens": ["ge\u00b7lieb\u00b7te", "Na\u00b7men", "\u2013", "und", "kei\u00b7ner", "blieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Manche taten schon so", "tokens": ["Man\u00b7che", "ta\u00b7ten", "schon", "so"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "und wurden stolze Verzweifler.", "tokens": ["und", "wur\u00b7den", "stol\u00b7ze", "Ver\u00b7zweif\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aber m\u00e4chtig macht nur der Glaube;", "tokens": ["A\u00b7ber", "m\u00e4ch\u00b7tig", "macht", "nur", "der", "Glau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "und Niemand lebt, den sein Tiefstes", "tokens": ["und", "Nie\u00b7mand", "lebt", ",", "den", "sein", "Tiefs\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "nicht noch \u00fcber die Sonne hinaufweist,", "tokens": ["nicht", "noch", "\u00fc\u00b7ber", "die", "Son\u00b7ne", "hin\u00b7auf\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "\u00fcber die Sterne, und weiter.", "tokens": ["\u00fc\u00b7ber", "die", "Ster\u00b7ne", ",", "und", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Sahst du nicht gestern die Zimmerleute,", "tokens": ["Sahst", "du", "nicht", "ge\u00b7stern", "die", "Zim\u00b7mer\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wie sie die Leiche auf der Leiter trugen,", "tokens": ["wie", "sie", "die", "Lei\u00b7che", "auf", "der", "Lei\u00b7ter", "tru\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "vom Neubau weg;", "tokens": ["vom", "Neu\u00b7bau", "weg", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "machte nicht jeden ihrer schweren Schritte", "tokens": ["mach\u00b7te", "nicht", "je\u00b7den", "ih\u00b7rer", "schwe\u00b7ren", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "die Kraft des Abgest\u00fcrzten", "tokens": ["die", "Kraft", "des", "Ab\u00b7ge\u00b7st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sichrer als je ihn selber?!", "tokens": ["sich\u00b7rer", "als", "je", "ihn", "sel\u00b7ber", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Warlich, Keiner von Diesen", "tokens": ["War\u00b7lich", ",", "Kei\u00b7ner", "von", "Die\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PIS", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "wird sich zu Tode st\u00fcrzen;", "tokens": ["wird", "sich", "zu", "To\u00b7de", "st\u00fcr\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "und wenn sie einst den Geist aufgeben,", "tokens": ["und", "wenn", "sie", "einst", "den", "Geist", "auf\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wird jede dieser sechs Handwerkerseelen", "tokens": ["wird", "je\u00b7de", "die\u00b7ser", "sechs", "Hand\u00b7wer\u00b7ker\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "PDAT", "CARD", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u2013 wir Alle sind Erben \u2013", "tokens": ["\u2013", "wir", "Al\u00b7le", "sind", "Er\u00b7ben", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PIS", "VAFIN", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "hell triumphierend an den Schauder denken,", "tokens": ["hell", "tri\u00b7um\u00b7phie\u00b7rend", "an", "den", "Schau\u00b7der", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "als sie den Andern auf seinem Werkzeug trugen.", "tokens": ["als", "sie", "den", "An\u00b7dern", "auf", "sei\u00b7nem", "Werk\u00b7zeug", "tru\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Bleibe dir heilig, Geist:", "tokens": ["Blei\u00b7be", "dir", "hei\u00b7lig", ",", "Geist", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.7": {"line.1": {"text": "Auf denn, Seele! reck die Glieder!", "tokens": ["Auf", "denn", ",", "See\u00b7le", "!", "reck", "die", "Glie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "NN", "$.", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "fast besch\u00e4mt mich mein Getr\u00e4ume;", "tokens": ["fast", "be\u00b7sch\u00e4mt", "mich", "mein", "Ge\u00b7tr\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "drau\u00dfen h\u00f6r ich meinen biedern", "tokens": ["drau\u00b7\u00dfen", "h\u00f6r", "ich", "mei\u00b7nen", "bie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuster schon am Werktisch r\u00e4umen.", "tokens": ["Schus\u00b7ter", "schon", "am", "Werk\u00b7tisch", "r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und sein n\u00e4rrischer Altgeselle", "tokens": ["Und", "sein", "n\u00e4r\u00b7ri\u00b7scher", "Alt\u00b7ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wird nun gleich nach Fr\u00fchst\u00fcck br\u00fcllen", "tokens": ["wird", "nun", "gleich", "nach", "Fr\u00fch\u00b7st\u00fcck", "br\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und mich dann mit Bibelstellen", "tokens": ["und", "mich", "dann", "mit", "Bi\u00b7bel\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ganz wie Tolstoi m\u00fcrbe kn\u00fcllen.", "tokens": ["ganz", "wie", "Tol\u00b7stoi", "m\u00fcr\u00b7be", "kn\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Warte nur, verehrter Schutzpatron:", "tokens": ["War\u00b7te", "nur", ",", "ver\u00b7ehr\u00b7ter", "Schutz\u00b7pat\u00b7ron", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "heut kommt's anderst! Mit den Mucken", "tokens": ["heut", "kommt's", "an\u00b7derst", "!", "Mit", "den", "Mu\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "deiner christlichen Passion", "tokens": ["dei\u00b7ner", "christ\u00b7li\u00b7chen", "Pas\u00b7si\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "kannst du dann den Pechdraht jucken.", "tokens": ["kannst", "du", "dann", "den", "Pech\u00b7draht", "ju\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ja, ihr w\u00fcrdigen deutschen Volks-Betbasen,", "tokens": ["Ja", ",", "ihr", "w\u00fcr\u00b7di\u00b7gen", "deut\u00b7schen", "Volks\u00b7Bet\u00b7ba\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+--+-++--", "measure": "anapaest.di.plus"}, "line.2": {"text": "faltet nur entsetzt die H\u00e4nde!", "tokens": ["fal\u00b7tet", "nur", "ent\u00b7setzt", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehre genug f\u00fcr eure j\u00fcdischen Phrasen,", "tokens": ["Eh\u00b7re", "ge\u00b7nug", "f\u00fcr", "eu\u00b7re", "j\u00fc\u00b7di\u00b7schen", "Phra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "da\u00df ich meinen Groll euch spende.", "tokens": ["da\u00df", "ich", "mei\u00b7nen", "Groll", "euch", "spen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie mein gallischer Freund Charles Simon grollt,", "tokens": ["Wie", "mein", "gal\u00b7li\u00b7scher", "Freund", "Char\u00b7les", "Si\u00b7mon", "grollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "dem ich mich verbr\u00fcdert f\u00fchle", "tokens": ["dem", "ich", "mich", "ver\u00b7br\u00fc\u00b7dert", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mehr als jedem Teutobold:", "tokens": ["mehr", "als", "je\u00b7dem", "Teu\u00b7to\u00b7bold", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbla nation c'est la crap\u00fcle.\u00ab", "tokens": ["\u00bb", "la", "na\u00b7ti\u00b7on", "c'\u00b7est", "la", "cra\u00b7p\u00fc\u00b7le", ".", "\u00ab"], "token_info": ["punct", "word"