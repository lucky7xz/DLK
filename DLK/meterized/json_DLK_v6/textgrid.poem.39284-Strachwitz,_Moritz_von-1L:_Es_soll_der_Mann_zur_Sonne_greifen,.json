{"textgrid.poem.39284": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es soll der Mann zur Sonne greifen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es soll der Mann zur Sonne greifen,", "tokens": ["Es", "soll", "der", "Mann", "zur", "Son\u00b7ne", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seiner Ehre Wunderland,", "tokens": ["Zu", "sei\u00b7ner", "Eh\u00b7re", "Wun\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum tiefsten Schacht der Erde streifen", "tokens": ["Zum", "tiefs\u00b7ten", "Schacht", "der", "Er\u00b7de", "strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach seines Ruhmes Diamant.", "tokens": ["Nach", "sei\u00b7nes", "Ruh\u00b7mes", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In eures Busens Minnehulden", "tokens": ["In", "eu\u00b7res", "Bu\u00b7sens", "Min\u00b7ne\u00b7hul\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt eures Ruhmes Sonne tief;", "tokens": ["Liegt", "eu\u00b7res", "Ruh\u00b7mes", "Son\u00b7ne", "tief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn eure Kraft ist stilles Dulden", "tokens": ["Denn", "eu\u00b7re", "Kraft", "ist", "stil\u00b7les", "Dul\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Liebe euer Adelsbrief.", "tokens": ["Und", "Lie\u00b7be", "eu\u00b7er", "A\u00b7dels\u00b7brief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}