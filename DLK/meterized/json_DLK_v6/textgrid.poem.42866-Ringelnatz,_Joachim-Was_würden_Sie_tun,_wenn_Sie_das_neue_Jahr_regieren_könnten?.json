{"textgrid.poem.42866": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Was w\u00fcrden Sie tun, wenn Sie das neue Jahr regieren k\u00f6nnten?", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcrde vor Aufregung wahrscheinlich", "tokens": ["Ich", "w\u00fcr\u00b7de", "vor", "Auf\u00b7re\u00b7gung", "wahr\u00b7schein\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die ersten N\u00e4chte schlaflos verbringen", "tokens": ["Die", "ers\u00b7ten", "N\u00e4ch\u00b7te", "schlaf\u00b7los", "ver\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und darauf tagelang \u00e4ngstlich und kleinlich", "tokens": ["Und", "da\u00b7rauf", "ta\u00b7ge\u00b7lang", "\u00e4ngst\u00b7lich", "und", "klein\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ADJD", "ADJD", "KON", "ADJD"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ganz dumme, selbsts\u00fcchtige Pl\u00e4ne schwingen.", "tokens": ["Ganz", "dum\u00b7me", ",", "selbst\u00b7s\u00fcch\u00b7ti\u00b7ge", "Pl\u00e4\u00b7ne", "schwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Dann \u2013 hoffentlich \u2013 aber laut lachen", "tokens": ["Dann", "\u2013", "hof\u00b7fent\u00b7lich", "\u2013", "a\u00b7ber", "laut", "la\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "ADJD", "$(", "ADV", "ADJD", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und endlich den lieben Gott abends leise", "tokens": ["Und", "end\u00b7lich", "den", "lie\u00b7ben", "Gott", "a\u00b7bends", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bitten, doch wieder nach seiner Weise", "tokens": ["Bit\u00b7ten", ",", "doch", "wie\u00b7der", "nach", "sei\u00b7ner", "Wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das neue Jahr g\u00f6ttlich selber zu machen.", "tokens": ["Das", "neu\u00b7e", "Jahr", "g\u00f6tt\u00b7lich", "sel\u00b7ber", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}