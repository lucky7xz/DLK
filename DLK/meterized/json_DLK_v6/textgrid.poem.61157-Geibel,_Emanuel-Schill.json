{"textgrid.poem.61157": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Schill", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O eine Eiche pflanzt auf diesen H\u00fcgel!", "tokens": ["O", "ei\u00b7ne", "Ei\u00b7che", "pflanzt", "auf", "die\u00b7sen", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gr\u00fcnste sucht, so weit die Amsel ruft!", "tokens": ["Die", "gr\u00fcns\u00b7te", "sucht", ",", "so", "weit", "die", "Am\u00b7sel", "ruft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie streue Schatten auf des Helden Gruft,", "tokens": ["Sie", "streu\u00b7e", "Schat\u00b7ten", "auf", "des", "Hel\u00b7den", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Lieder rausch' in ihr des Windes Fl\u00fcgel.", "tokens": ["Und", "Lie\u00b7der", "rausch'", "in", "ihr", "des", "Win\u00b7des", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn gleich dem Ro\u00df, das knirschet in die Z\u00fcgel", "tokens": ["Denn", "gleich", "dem", "Ro\u00df", ",", "das", "knir\u00b7schet", "in", "die", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und scharrt und stampfet, sp\u00fcrt es Morgenluft,", "tokens": ["Und", "scharrt", "und", "stamp\u00b7fet", ",", "sp\u00fcrt", "es", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wittert' er zuerst der Freiheit Duft,", "tokens": ["So", "wit\u00b7tert'", "er", "zu\u00b7erst", "der", "Frei\u00b7heit", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da alles schwieg, und schwang sich in den B\u00fcgel.", "tokens": ["Da", "al\u00b7les", "schwieg", ",", "und", "schwang", "sich", "in", "den", "B\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fcrwahr, o Schill, du warst ein echter Reiter,", "tokens": ["F\u00fcr\u00b7wahr", ",", "o", "Schill", ",", "du", "warst", "ein", "ech\u00b7ter", "Rei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schneller als die Zeiten rittst du gern,", "tokens": ["Und", "schnel\u00b7ler", "als", "die", "Zei\u00b7ten", "rittst", "du", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit dir wie Blitze deine blanken Streiter.", "tokens": ["Mit", "dir", "wie", "Blit\u00b7ze", "dei\u00b7ne", "blan\u00b7ken", "Strei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein Jagdhorn klang: \u00bbDer Tag ist nicht mehr fern!\u00ab", "tokens": ["Dein", "Jagd\u00b7horn", "klang", ":", "\u00bb", "Der", "Tag", "ist", "nicht", "mehr", "fern", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ging der Morgen auf so rot und heiter;", "tokens": ["Da", "ging", "der", "Mor\u00b7gen", "auf", "so", "rot", "und", "hei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch unter gingst du, sch\u00f6ner Morgenstern.", "tokens": ["Doch", "un\u00b7ter", "gingst", "du", ",", "sch\u00f6\u00b7ner", "Mor\u00b7gens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}