{"textgrid.poem.45820": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[der goldne Tag ist heimgegangen]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der goldne Tag ist heimgegangen;", "tokens": ["Der", "gold\u00b7ne", "Tag", "ist", "heim\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah ihn \u00fcber die Berge ziehn,", "tokens": ["Ich", "sah", "ihn", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und all mein sehnendes Verlangen", "tokens": ["Und", "all", "mein", "seh\u00b7nen\u00b7des", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Floh mit ihm hin.", "tokens": ["Floh", "mit", "ihm", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Bunt ist wohl um des J\u00fcnglings H\u00fcften", "tokens": ["Bunt", "ist", "wohl", "um", "des", "J\u00fcng\u00b7lings", "H\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der schimmernde Mantel hingewallt,", "tokens": ["Der", "schim\u00b7mern\u00b7de", "Man\u00b7tel", "hin\u00b7ge\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und leise in den Himmelsl\u00fcften", "tokens": ["Und", "lei\u00b7se", "in", "den", "Him\u00b7mel\u00b7sl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Lied verhallt.", "tokens": ["Sein", "Lied", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich sah wohl die gl\u00fchenden Locken", "tokens": ["Ich", "sah", "wohl", "die", "gl\u00fc\u00b7hen\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Am Berge wehn,", "tokens": ["Am", "Ber\u00b7ge", "wehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Oben ihn stehn,", "tokens": ["O\u00b7ben", "ihn", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und freundlich goldne Flocken", "tokens": ["Und", "freund\u00b7lich", "gold\u00b7ne", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf die Bahn hins\u00e4en,", "tokens": ["Auf", "die", "Bahn", "hin\u00b7s\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Drauf weiter zu gehen.", "tokens": ["Drauf", "wei\u00b7ter", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Da breitet das Leben", "tokens": ["Da", "brei\u00b7tet", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Schmetterlingsfl\u00fcgel,", "tokens": ["Die", "Schmet\u00b7ter\u00b7lings\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Am duftigen H\u00fcgel", "tokens": ["Am", "duf\u00b7ti\u00b7gen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ihn hoch zu erheben,", "tokens": ["Ihn", "hoch", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Uns nochmals zu geben.", "tokens": ["Uns", "noch\u00b7mals", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "So traurig sa\u00df er oben", "tokens": ["So", "trau\u00b7rig", "sa\u00df", "er", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Purpurzelt,", "tokens": ["Im", "Pur\u00b7pur\u00b7zelt", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und gr\u00fc\u00dft' die Welt:", "tokens": ["Und", "gr\u00fc\u00dft'", "die", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Leb wohl da unten!", "tokens": ["Leb", "wohl", "da", "un\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Da hat ihn der Fl\u00fcgel", "tokens": ["Da", "hat", "ihn", "der", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit Flammen umwunden,", "tokens": ["Mit", "Flam\u00b7men", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Am duftigen H\u00fcgel", "tokens": ["Am", "duf\u00b7ti\u00b7gen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hin\u00fcbergehoben.", "tokens": ["Hin\u00b7\u00fc\u00b7ber\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Sein \u00f6des Reich bleibt still zur\u00fccke,", "tokens": ["Sein", "\u00f6\u00b7des", "Reich", "bleibt", "still", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt verweilt ganz herrenlos.", "tokens": ["Die", "Welt", "ver\u00b7weilt", "ganz", "her\u00b7ren\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Leben forscht mit tr\u00fcbem Blicke", "tokens": ["Das", "Le\u00b7ben", "forscht", "mit", "tr\u00fc\u00b7bem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im eignen Scho\u00df.", "tokens": ["Im", "eig\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ein d\u00fcstrer Mantel rauschet nieder", "tokens": ["Ein", "d\u00fcst\u00b7rer", "Man\u00b7tel", "rau\u00b7schet", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rund um des J\u00fcnglings verla\u00dfnen Thron,", "tokens": ["Rund", "um", "des", "J\u00fcng\u00b7lings", "ver\u00b7la\u00df\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und aus den W\u00e4ldern hallet wider", "tokens": ["Und", "aus", "den", "W\u00e4l\u00b7dern", "hal\u00b7let", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein trunkner Ton.", "tokens": ["Ein", "trunk\u00b7ner", "Ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Es r\u00fchren die n\u00e4chtlichen Stunden", "tokens": ["Es", "r\u00fch\u00b7ren", "die", "n\u00e4cht\u00b7li\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sich tief im Tal,", "tokens": ["Sich", "tief", "im", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bereiten ein Mahl", "tokens": ["Be\u00b7rei\u00b7ten", "ein", "Mahl"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Im d\u00e4mmernden Saal,", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Saal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit dichten Gew\u00e4ndern umwunden.", "tokens": ["Mit", "dich\u00b7ten", "Ge\u00b7w\u00e4n\u00b7dern", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Ein matter Strahl", "tokens": ["Ein", "mat\u00b7ter", "Strahl"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Blinkt am Pokal,", "tokens": ["Blinkt", "am", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und s\u00fc\u00df betrunken,", "tokens": ["Und", "s\u00fc\u00df", "be\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vom goldenen Wein,", "tokens": ["Vom", "gol\u00b7de\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Schlummert die j\u00fcngste", "tokens": ["Schlum\u00b7mert", "die", "j\u00fcngs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Stunden schon ein,", "tokens": ["Der", "Stun\u00b7den", "schon", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die andern lauschen", "tokens": ["Die", "an\u00b7dern", "lau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Von au\u00dfenher zu,", "tokens": ["Von", "au\u00b7\u00dfen\u00b7her", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und st\u00fcrzen herein.", "tokens": ["Und", "st\u00fcr\u00b7zen", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Es sterben die Funken,", "tokens": ["Es", "ster\u00b7ben", "die", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Hinabgesunken", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Ist der letzte Strahl", "tokens": ["Ist", "der", "letz\u00b7te", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Von ihrem Pokal.", "tokens": ["Von", "ih\u00b7rem", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Sie irren und rauschen", "tokens": ["Sie", "ir\u00b7ren", "und", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Ohn' Schimmer und Schein,", "tokens": ["Ohn'", "Schim\u00b7mer", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Ohn' alle Ruh'.", "tokens": ["Ohn'", "al\u00b7le", "Ruh'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Zerst\u00f6rt ist das Mahl", "tokens": ["Zer\u00b7st\u00f6rt", "ist", "das", "Mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Und dunkel der Saal.", "tokens": ["Und", "dun\u00b7kel", "der", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Da schreiten die Stunden so leise", "tokens": ["Da", "schrei\u00b7ten", "die", "Stun\u00b7den", "so", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wohl in die Nacht,", "tokens": ["Wohl", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verh\u00fcllen auf finsterer Reise", "tokens": ["Ver\u00b7h\u00fcl\u00b7len", "auf", "fins\u00b7te\u00b7rer", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit ernstem Bedacht,", "tokens": ["Mit", "erns\u00b7tem", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "In dunkeln Falten", "tokens": ["In", "dun\u00b7keln", "Fal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die regen Gestalten,", "tokens": ["Die", "re\u00b7gen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "An denen sie sinnend vor\u00fcberwallten,", "tokens": ["An", "de\u00b7nen", "sie", "sin\u00b7nend", "vor\u00b7\u00fc\u00b7berw\u00b7all\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und alles umarmt sich rings umher,", "tokens": ["Und", "al\u00b7les", "um\u00b7armt", "sich", "rings", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Es giebt keine einzelne Rechte mehr,", "tokens": ["Es", "giebt", "kei\u00b7ne", "ein\u00b7zel\u00b7ne", "Rech\u00b7te", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Es \u00f6ffnet jed Leben dem andern die Brust,", "tokens": ["Es", "\u00f6ff\u00b7net", "jed", "Le\u00b7ben", "dem", "an\u00b7dern", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Und trinket mit Lust,", "tokens": ["Und", "trin\u00b7ket", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Ganz ohnbewu\u00dft,", "tokens": ["Ganz", "ohn\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Den himmlischen Ku\u00df,", "tokens": ["Den", "himm\u00b7li\u00b7schen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Den Wechselgenu\u00df.", "tokens": ["Den", "Wech\u00b7sel\u00b7ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "So innig umschlungen,", "tokens": ["So", "in\u00b7nig", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "So heilig durchdrungen,", "tokens": ["So", "hei\u00b7lig", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Umh\u00fcllet ein Rausch,", "tokens": ["Um\u00b7h\u00fcl\u00b7let", "ein", "Rausch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Den lieblichen Tausch.", "tokens": ["Den", "lieb\u00b7li\u00b7chen", "Tausch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Und endlich l\u00f6sen die Arme sich auf,", "tokens": ["Und", "end\u00b7lich", "l\u00f6\u00b7sen", "die", "Ar\u00b7me", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mond zieht herauf;", "tokens": ["Der", "Mond", "zieht", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der d\u00e4mmernde Blick", "tokens": ["Der", "d\u00e4m\u00b7mern\u00b7de", "Blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Tr\u00e4umt trunkenen Traum.", "tokens": ["Tr\u00e4umt", "trun\u00b7ke\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Im himmlischen Raum", "tokens": ["Im", "himm\u00b7li\u00b7schen", "Raum"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Erbl\u00fchen die Sterne,", "tokens": ["Er\u00b7bl\u00fc\u00b7hen", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und kehret das Licht", "tokens": ["Und", "keh\u00b7ret", "das", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Bescheiden zur\u00fcck.", "tokens": ["Be\u00b7schei\u00b7den", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Das Leben flicht", "tokens": ["Das", "Le\u00b7ben", "flicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dann in der Ferne", "tokens": ["Dann", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den br\u00e4utlichen Kranz,", "tokens": ["Den", "br\u00e4ut\u00b7li\u00b7chen", "Kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Entz\u00fcndet die Lieder,", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Erleuchtet den Tanz.", "tokens": ["Er\u00b7leuch\u00b7tet", "den", "Tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die reizenden Glieder", "tokens": ["Die", "rei\u00b7zen\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Umh\u00fcllt ein Gewand,", "tokens": ["Um\u00b7h\u00fcllt", "ein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Durchsichtig gewebet.", "tokens": ["Durch\u00b7sich\u00b7tig", "ge\u00b7we\u00b7bet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.9": {"text": "Das Leben erhebet,", "tokens": ["Das", "Le\u00b7ben", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Zum Himmel gewandt,", "tokens": ["Zum", "Him\u00b7mel", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Den Busen, und strebet", "tokens": ["Den", "Bu\u00b7sen", ",", "und", "stre\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Sich wieder zu finden.", "tokens": ["Sich", "wie\u00b7der", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Die Sehnsucht erwacht", "tokens": ["Die", "Sehn\u00b7sucht", "er\u00b7wacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "In schimmernder Nacht.", "tokens": ["In", "schim\u00b7mern\u00b7der", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}