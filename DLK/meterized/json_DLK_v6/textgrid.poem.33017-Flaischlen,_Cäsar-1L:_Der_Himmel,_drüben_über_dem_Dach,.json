{"textgrid.poem.33017": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel, dr\u00fcben \u00fcber dem Dach,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel, dr\u00fcben \u00fcber dem Dach,", "tokens": ["Der", "Him\u00b7mel", ",", "dr\u00fc\u00b7ben", "\u00fc\u00b7ber", "dem", "Dach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "in tiefblauem Schweigen,", "tokens": ["in", "tief\u00b7blau\u00b7em", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "ein Baum, dr\u00fcben \u00fcber dem Dach,", "tokens": ["ein", "Baum", ",", "dr\u00fc\u00b7ben", "\u00fc\u00b7ber", "dem", "Dach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "mit wiegenden Zweigen.", "tokens": ["mit", "wie\u00b7gen\u00b7den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "In dem Himmel, den man sieht,", "tokens": ["In", "dem", "Him\u00b7mel", ",", "den", "man", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "klingts wie von Glocken,", "tokens": ["klingts", "wie", "von", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ein Vogel auf dem Baum, den man sieht,", "tokens": ["ein", "Vo\u00b7gel", "auf", "dem", "Baum", ",", "den", "man", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "singt sein Frohlocken.", "tokens": ["singt", "sein", "Froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Gott, mein Gott, so friedlich und sch\u00f6n ...", "tokens": ["Mein", "Gott", ",", "mein", "Gott", ",", "so", "fried\u00b7lich", "und", "sch\u00f6n", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "das dort ist Leben!", "tokens": ["das", "dort", "ist", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "in der Stadt dr\u00fcben dieses frohe Get\u00f6n", "tokens": ["in", "der", "Stadt", "dr\u00fc\u00b7ben", "die\u00b7ses", "fro\u00b7he", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PDAT", "ADJA", "NN"], "meter": "+-++-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "und Summen und Weben! ...", "tokens": ["und", "Sum\u00b7men", "und", "We\u00b7ben", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und du, der du hier weinst,", "tokens": ["Und", "du", ",", "der", "du", "hier", "weinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "durchs Gitter lugend,", "tokens": ["durchs", "Git\u00b7ter", "lu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "was hast du gemacht, sag, der du hier weinst,", "tokens": ["was", "hast", "du", "ge\u00b7macht", ",", "sag", ",", "der", "du", "hier", "weinst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$,", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mit deiner Jugend!?", "tokens": ["mit", "dei\u00b7ner", "Ju\u00b7gend", "!?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}