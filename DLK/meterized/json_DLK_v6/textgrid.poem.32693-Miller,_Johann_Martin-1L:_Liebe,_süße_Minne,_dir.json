{"textgrid.poem.32693": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe, s\u00fc\u00dfe Minne, dir", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe, s\u00fc\u00dfe Minne, dir", "tokens": ["Lie\u00b7be", ",", "s\u00fc\u00b7\u00dfe", "Min\u00b7ne", ",", "dir"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich dienen f\u00fcr und f\u00fcr!", "tokens": ["Will", "ich", "die\u00b7nen", "f\u00fcr", "und", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles, was mein Herz begehret,", "tokens": ["Al\u00b7les", ",", "was", "mein", "Herz", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Alles hast du mir gew\u00e4hret,", "tokens": ["Al\u00b7les", "hast", "du", "mir", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein Liebchen l\u00e4chelt mir!", "tokens": ["Und", "mein", "Lieb\u00b7chen", "l\u00e4\u00b7chelt", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reinen Engelsinn hat sie;", "tokens": ["Rei\u00b7nen", "En\u00b7gel\u00b7sinn", "hat", "sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wen sie minnt, der trauert nie;", "tokens": ["Wen", "sie", "minnt", ",", "der", "trau\u00b7ert", "nie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer sie morgens nur erblicket,", "tokens": ["Wer", "sie", "mor\u00b7gens", "nur", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist den ganzen Tag begl\u00fccket;", "tokens": ["Ist", "den", "gan\u00b7zen", "Tag", "be\u00b7gl\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich sehe t\u00e4glich sie!", "tokens": ["Und", "ich", "se\u00b7he", "t\u00e4g\u00b7lich", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sittsam ist ihr Aug' und blau,", "tokens": ["Sitt\u00b7sam", "ist", "ihr", "Aug'", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Violen auf der Au;", "tokens": ["Wie", "Vi\u00b7o\u00b7len", "auf", "der", "Au", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dfer als Narzissen bl\u00fchet", "tokens": ["Wei\u00b7\u00dfer", "als", "Nar\u00b7zis\u00b7sen", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOUS", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Stirn; ihr M\u00fcndlein gl\u00fchet,", "tokens": ["Ih\u00b7re", "Stirn", ";", "ihr", "M\u00fcnd\u00b7lein", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Ros' im Morgentau.", "tokens": ["Wie", "die", "Ros'", "im", "Mor\u00b7gen\u00b7tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gleich dem lieben Sonnenschein,", "tokens": ["Gleich", "dem", "lie\u00b7ben", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lacht sie allen, gro\u00df und klein,", "tokens": ["Lacht", "sie", "al\u00b7len", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df sie alle zu entz\u00fccken:", "tokens": ["Wei\u00df", "sie", "al\u00b7le", "zu", "ent\u00b7z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber mit der Minne Blicken", "tokens": ["A\u00b7ber", "mit", "der", "Min\u00b7ne", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4chelt sie nur mir allein!", "tokens": ["L\u00e4\u00b7chelt", "sie", "nur", "mir", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}