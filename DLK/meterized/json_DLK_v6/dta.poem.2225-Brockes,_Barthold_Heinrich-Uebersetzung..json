{"dta.poem.2225": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach taugte doch die Ueberlegung, uns f\u00fcr den giftgen", "tokens": ["Ach", "taug\u00b7te", "doch", "die", "Ue\u00b7ber\u00b7le\u00b7gung", ",", "uns", "f\u00fcr", "den", "gift\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Neid zu sch\u00fctzen!", "tokens": ["Neid", "zu", "sch\u00fct\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ein Unrecht sucht der Zorn zu r\u00e4chen: die Lieb\u2019, ein Gu-", "tokens": ["Ein", "Un\u00b7recht", "sucht", "der", "Zorn", "zu", "r\u00e4\u00b7chen", ":", "die", "Lieb'", ",", "ein", "Gu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$.", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "tes zu besitzen,", "tokens": ["tes", "zu", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Furcht, das B\u00f6se zu vermeiden: nur ganz allein der", "tokens": ["Die", "Furcht", ",", "das", "B\u00f6\u00b7se", "zu", "ver\u00b7mei\u00b7den", ":", "nur", "ganz", "al\u00b7lein", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$.", "ADV", "ADV", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "schiele Neid", "tokens": ["schie\u00b7le", "Neid"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sucht, als sein allerh\u00f6chstes Gut, des N\u00e4chsten Plag", "tokens": ["Sucht", ",", "als", "sein", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "Gut", ",", "des", "N\u00e4chs\u00b7ten", "Plag"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und Herzeleid.", "tokens": ["und", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}