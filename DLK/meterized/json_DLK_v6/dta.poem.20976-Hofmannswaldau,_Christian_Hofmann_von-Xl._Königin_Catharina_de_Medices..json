{"dta.poem.20976": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Xl.  \n K\u00f6nigin  Catharina de Medices.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Gantz Franckreich liebte mich umb meines Nahmens", "tokens": ["Gantz", "Fran\u00b7ck\u00b7reich", "lieb\u00b7te", "mich", "umb", "mei\u00b7nes", "Nah\u00b7mens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "willen/", "tokens": ["wil\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Als solte meine Hand ihr grosses Ubel stillen.", "tokens": ["Als", "sol\u00b7te", "mei\u00b7ne", "Hand", "ihr", "gros\u00b7ses", "U\u00b7bel", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Hoffnung war umbsonst man fing mich an zu", "tokens": ["Die", "Hoff\u00b7nung", "war", "um\u00b7bsonst", "man", "fing", "mich", "an", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIS", "VVFIN", "PRF", "APZR", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "hassen/", "tokens": ["has\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Ich konte sonsten nichts als nur zur Ader lassen.", "tokens": ["Ich", "kon\u00b7te", "sons\u00b7ten", "nichts", "als", "nur", "zur", "A\u00b7der", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "KOKOM", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}