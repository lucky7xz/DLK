{"textgrid.poem.38210": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Liebesaugen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald du hebst die klaren Aeugelein,", "tokens": ["So\u00b7bald", "du", "hebst", "die", "kla\u00b7ren", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Freut sich Gestirn und auch der Sonnenschein,", "tokens": ["Freut", "sich", "Ge\u00b7stirn", "und", "auch", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Also gar sehr, du Liebeszier,", "tokens": ["Al\u00b7so", "gar", "sehr", ",", "du", "Lie\u00b7bes\u00b7zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "PPER", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sind sie geneiget dir.", "tokens": ["Sind", "sie", "ge\u00b7nei\u00b7get", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sobald du auch die Erde blickest an,", "tokens": ["So\u00b7bald", "du", "auch", "die", "Er\u00b7de", "bli\u00b7ckest", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist sie erhizt, schickt Bl\u00fcmelein heran,", "tokens": ["Ist", "sie", "er\u00b7hizt", ",", "schickt", "Bl\u00fc\u00b7me\u00b7lein", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie sollt dann ich nicht herziglich,", "tokens": ["Wie", "sollt", "dann", "ich", "nicht", "her\u00b7zig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jungfrau auch lieben dich.", "tokens": ["Jung\u00b7frau", "auch", "lie\u00b7ben", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und schlie\u00dfest du, o Herz, die Aeugelein,", "tokens": ["Und", "schlie\u00b7\u00dfest", "du", ",", "o", "Herz", ",", "die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da giebt der Stern der Venus gro\u00dfen Schein", "tokens": ["Da", "giebt", "der", "Stern", "der", "Ve\u00b7nus", "gro\u00b7\u00dfen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie ihrem Kind, wenn sie offen sind,", "tokens": ["Wie", "ih\u00b7rem", "Kind", ",", "wenn", "sie", "of\u00b7fen", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Die Fackel heftig brinnt,", "tokens": ["Die", "Fa\u00b7ckel", "hef\u00b7tig", "brinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und h\u00fcllst du ein die hellen Aeugelein,", "tokens": ["Und", "h\u00fcllst", "du", "ein", "die", "hel\u00b7len", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Himmel traurig zieht die Sterne ein,", "tokens": ["Der", "Him\u00b7mel", "trau\u00b7rig", "zieht", "die", "Ster\u00b7ne", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Erd ist kalt, Frau Venus alt,", "tokens": ["Die", "Erd", "ist", "kalt", ",", "Frau", "Ve\u00b7nus", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "NN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ohn Feuer Amor bald.", "tokens": ["Ohn", "Feu\u00b7er", "A\u00b7mor", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}