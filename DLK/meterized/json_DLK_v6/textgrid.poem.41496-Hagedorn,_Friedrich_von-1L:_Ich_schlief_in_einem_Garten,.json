{"textgrid.poem.41496": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schlief in einem Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schlief in einem Garten,", "tokens": ["Ich", "schlief", "in", "ei\u00b7nem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Ros' und Myrthe zierten,", "tokens": ["Den", "Ros'", "und", "Myr\u00b7the", "zier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In dem drei holde Sch\u00f6nen", "tokens": ["In", "dem", "drei", "hol\u00b7de", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den habentbl\u00f6\u00dften Busen", "tokens": ["Den", "ha\u00b7bent\u00b7bl\u00f6\u00df\u00b7ten", "Bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit frischen Blumen kr\u00f6nten,", "tokens": ["Mit", "fri\u00b7schen", "Blu\u00b7men", "kr\u00f6n\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die jede singend pfl\u00fcckte.", "tokens": ["Die", "je\u00b7de", "sin\u00b7gend", "pfl\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bald gaukelten die Spiele", "tokens": ["Bald", "gau\u00b7kel\u00b7ten", "die", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Stifters leichter Tr\u00e4ume", "tokens": ["Des", "Stif\u00b7ters", "leich\u00b7ter", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mir um die Augenlider,", "tokens": ["Mir", "um", "die", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und mich versetzten Morpheus", "tokens": ["Und", "mich", "ver\u00b7setz\u00b7ten", "Mor\u00b7pheus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und Phantasus, sein Bruder,", "tokens": ["Und", "Phan\u00b7ta\u00b7sus", ",", "sein", "Bru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ans Ufer von Cythere.", "tokens": ["Ans", "U\u00b7fer", "von", "Cy\u00b7the\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.13": {"text": "Der bunte Fr\u00fchling f\u00e4rbte", "tokens": ["Der", "bun\u00b7te", "Fr\u00fch\u00b7ling", "f\u00e4rb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Die Blumen dieser Insel;", "tokens": ["Die", "Blu\u00b7men", "die\u00b7ser", "In\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der leichte Zephyr k\u00fc\u00dfte", "tokens": ["Der", "leich\u00b7te", "Ze\u00b7phyr", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die Pflanzen dieser Insel;", "tokens": ["Die", "Pflan\u00b7zen", "die\u00b7ser", "In\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und sein Gefolge wiegte", "tokens": ["Und", "sein", "Ge\u00b7fol\u00b7ge", "wieg\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die Wipfel dieser Insel.", "tokens": ["Die", "Wip\u00b7fel", "die\u00b7ser", "In\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wie manches Feld von Rosen,", "tokens": ["Wie", "man\u00b7ches", "Feld", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wie mancher Busch von Myrthen", "tokens": ["Wie", "man\u00b7cher", "Busch", "von", "Myr\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "War hier der Venus heilig!", "tokens": ["War", "hier", "der", "Ve\u00b7nus", "hei\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Der G\u00f6ttin sanfter Freuden,", "tokens": ["Der", "G\u00f6t\u00b7tin", "sanf\u00b7ter", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Der Freuden voller Liebe,", "tokens": ["Der", "Freu\u00b7den", "vol\u00b7ler", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Der Liebe voller Jugend.", "tokens": ["Der", "Lie\u00b7be", "vol\u00b7ler", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ich sah die Huldg\u00f6ttinnen,", "tokens": ["Ich", "sah", "die", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Gef\u00fchrt vom West und Fr\u00fchling,", "tokens": ["Ge\u00b7f\u00fchrt", "vom", "West", "und", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Gefolgt von Z\u00e4rtlichkeiten,", "tokens": ["Ge\u00b7folgt", "von", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Mit Rosen sich umkr\u00e4nzen,", "tokens": ["Mit", "Ro\u00b7sen", "sich", "um\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Sich Mund und H\u00e4nde reichen", "tokens": ["Sich", "Mund", "und", "H\u00e4n\u00b7de", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Und ohne G\u00fcrtel tanzen", "tokens": ["Und", "oh\u00b7ne", "G\u00fcr\u00b7tel", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Und bei den T\u00e4nzen lachen.", "tokens": ["Und", "bei", "den", "T\u00e4n\u00b7zen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Hier fand ich auch den Amor,", "tokens": ["Hier", "fand", "ich", "auch", "den", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Der seine Fl\u00fcgel sonnte,", "tokens": ["Der", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die ihm vom Thau befeuchtet", "tokens": ["Die", "ihm", "vom", "Thau", "be\u00b7feuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und so betr\u00f6pfelt waren,", "tokens": ["Und", "so", "be\u00b7tr\u00f6p\u00b7felt", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Als da er seinen Dichter", "tokens": ["Als", "da", "er", "sei\u00b7nen", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Anacreon besuchte.", "tokens": ["A\u00b7na\u00b7cre\u00b7on", "be\u00b7such\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.38": {"text": "Er wollte von mir wissen,", "tokens": ["Er", "woll\u00b7te", "von", "mir", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Wer von den holden Dreien", "tokens": ["Wer", "von", "den", "hol\u00b7den", "Drei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Bei mir den Vorzug h\u00e4tte,", "tokens": ["Bei", "mir", "den", "Vor\u00b7zug", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Als mich von jenen Sch\u00f6nen,", "tokens": ["Als", "mich", "von", "je\u00b7nen", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Die sich die Blumen pfl\u00fcckten,", "tokens": ["Die", "sich", "die", "Blu\u00b7men", "pfl\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Die Sch\u00f6nste l\u00e4chelnd weckte.", "tokens": ["Die", "Sch\u00f6ns\u00b7te", "l\u00e4\u00b7chelnd", "weck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}