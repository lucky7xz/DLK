{"textgrid.poem.41080": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sturmlieder hast du k\u00fchn gesungen,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sturmlieder hast du k\u00fchn gesungen,", "tokens": ["Sturm\u00b7lie\u00b7der", "hast", "du", "k\u00fchn", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sangst, wie der alte Gott mit Macht", "tokens": ["Sangst", ",", "wie", "der", "al\u00b7te", "Gott", "mit", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Meeresschiffen wild gerungen,", "tokens": ["Mit", "Mee\u00b7res\u00b7schif\u00b7fen", "wild", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie krachend in den Grund gebracht.", "tokens": ["Sie", "kra\u00b7chend", "in", "den", "Grund", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie er als Sum durch W\u00fcsten streifte,", "tokens": ["Wie", "er", "als", "Sum", "durch", "W\u00fcs\u00b7ten", "streif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wettrennend mit des Arbers Ro\u00df,", "tokens": ["Wett\u00b7ren\u00b7nend", "mit", "des", "Ar\u00b7bers", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den hei\u00dfen Sand zum Berge h\u00e4ufte, \u2013", "tokens": ["Den", "hei\u00b7\u00dfen", "Sand", "zum", "Ber\u00b7ge", "h\u00e4uf\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das \u2013 und noch mehr besangst du gro\u00df", "tokens": ["Das", "\u2013", "und", "noch", "mehr", "be\u00b7sangst", "du", "gro\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "KON", "ADV", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O la\u00df, der du ein stilles Klagen", "tokens": ["O", "la\u00df", ",", "der", "du", "ein", "stil\u00b7les", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl auch belauscht in Wald und Flur,", "tokens": ["Wohl", "auch", "be\u00b7lauscht", "in", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein warmes Herz jetzt singen, sagen", "tokens": ["Dein", "war\u00b7mes", "Herz", "jetzt", "sin\u00b7gen", ",", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von mildern Lauten der Natur.", "tokens": ["Von", "mil\u00b7dern", "Lau\u00b7ten", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sing von des Wests melod'schem Gleiten", "tokens": ["Sing", "von", "des", "Wests", "me\u00b7lo\u00b7d'\u00b7schem", "Glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Durch Zweige, die er leise schwingt,", "tokens": ["Durch", "Zwei\u00b7ge", ",", "die", "er", "lei\u00b7se", "schwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie, wehend durch der Harfe Saiten,", "tokens": ["Wie", ",", "we\u00b7hend", "durch", "der", "Har\u00b7fe", "Sai\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kummer der Natur er singt.", "tokens": ["Den", "Kum\u00b7mer", "der", "Na\u00b7tur", "er", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Lausch' auf in hellen Mondenn\u00e4chten", "tokens": ["Lausch'", "auf", "in", "hel\u00b7len", "Mon\u00b7den\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und singe nach dem Sternenklang,", "tokens": ["Und", "sin\u00b7ge", "nach", "dem", "Ster\u00b7nen\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft ist's, als ob die Winde br\u00e4chten", "tokens": ["Oft", "ist's", ",", "als", "ob", "die", "Win\u00b7de", "br\u00e4ch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von da den wunderbarsten Sang.", "tokens": ["Von", "da", "den", "wun\u00b7der\u00b7bars\u00b7ten", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja! solche Laute sind dir eigen,", "tokens": ["Ja", "!", "sol\u00b7che", "Lau\u00b7te", "sind", "dir", "ei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie der Natur der Laut von Schmerz:", "tokens": ["Wie", "der", "Na\u00b7tur", "der", "Laut", "von", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "APPR", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Denn an dir \u2013 kann es nicht verschweigen \u2013", "tokens": ["Denn", "an", "dir", "\u2013", "kann", "es", "nicht", "ver\u00b7schwei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$(", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist jeder Muskel ja ein Herz.", "tokens": ["Ist", "je\u00b7der", "Mus\u00b7kel", "ja", "ein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}