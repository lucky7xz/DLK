{"textgrid.poem.38620": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Feenland", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Rosen umweben", "tokens": ["Mit", "Ro\u00b7sen", "um\u00b7we\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Sterblichen Leben", "tokens": ["Der", "Sterb\u00b7li\u00b7chen", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die g\u00fctigen Feen;", "tokens": ["Die", "g\u00fc\u00b7ti\u00b7gen", "Feen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sie wandeln und walten", "tokens": ["Sie", "wan\u00b7deln", "und", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In tausend Gestalten,", "tokens": ["In", "tau\u00b7send", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bald h\u00e4\u00dflich, bald sch\u00f6n.", "tokens": ["Bald", "h\u00e4\u00df\u00b7lich", ",", "bald", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Da wo sie gebieten", "tokens": ["Da", "wo", "sie", "ge\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lacht alles, mit Bl\u00fcten", "tokens": ["Lacht", "al\u00b7les", ",", "mit", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und Gr\u00fcn emaillirt;", "tokens": ["Und", "Gr\u00fcn", "e\u00b7mail\u00b7lirt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihr Schlo\u00df von Topasen", "tokens": ["Ihr", "Schlo\u00df", "von", "To\u00b7pa\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Ist herrlich mit Vasen", "tokens": ["Ist", "herr\u00b7lich", "mit", "Va\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von Demant geziert.", "tokens": ["Von", "De\u00b7mant", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Von Zeilons Ged\u00fcfte", "tokens": ["Von", "Zei\u00b7lons", "Ge\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sind ewig die L\u00fcfte", "tokens": ["Sind", "e\u00b7wig", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der G\u00e4rten durchweht;", "tokens": ["Der", "G\u00e4r\u00b7ten", "durch\u00b7weht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die G\u00e4nge, statt Sandes,", "tokens": ["Die", "G\u00e4n\u00b7ge", ",", "statt", "San\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nach Weise des Landes,", "tokens": ["Nach", "Wei\u00b7se", "des", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit Perlen bes\u00e4t.", "tokens": ["Mit", "Per\u00b7len", "be\u00b7s\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ambrosiatische", "tokens": ["A\u00b7mbro\u00b7si\u00b7a\u00b7ti\u00b7sche"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sind hier in der Frische", "tokens": ["Sind", "hier", "in", "der", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Grotten versteckt;", "tokens": ["Der", "Grot\u00b7ten", "ver\u00b7steckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dort blasen im Gr\u00fcnen", "tokens": ["Dort", "bla\u00b7sen", "im", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kristallne Delfinen", "tokens": ["Kris\u00b7tall\u00b7ne", "Del\u00b7fi\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Tokaier und Sekt.", "tokens": ["To\u00b7kai\u00b7er", "und", "Sekt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Den Bl\u00fcten entflimmert,", "tokens": ["Den", "Bl\u00fc\u00b7ten", "ent\u00b7flim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von Fr\u00fcchten umschimmert,", "tokens": ["Von", "Fr\u00fcch\u00b7ten", "um\u00b7schim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Kolibri Schmelz,", "tokens": ["Der", "Ko\u00b7lib\u00b7ri", "Schmelz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und Nachtigallkehlen", "tokens": ["Und", "Nach\u00b7ti\u00b7gall\u00b7keh\u00b7len"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vom Leman beseelen", "tokens": ["Vom", "Le\u00b7man", "be\u00b7see\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Badegeh\u00f6lz.", "tokens": ["Das", "Ba\u00b7de\u00b7ge\u00b7h\u00f6lz", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Da flattert, im Scheine", "tokens": ["Da", "flat\u00b7tert", ",", "im", "Schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Mondes, der kleine", "tokens": ["Des", "Mon\u00b7des", ",", "der", "klei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gefl\u00fcgelte Wicht,", "tokens": ["Ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Wicht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schlau, wie die Annalen", "tokens": ["Schlau", ",", "wie", "die", "An\u00b7na\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Cytherens ihn malen,", "tokens": ["Cy\u00b7the\u00b7rens", "ihn", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mit sanftem Gesicht.", "tokens": ["Mit", "sanf\u00b7tem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Aus d\u00e4mmerndem Grunde", "tokens": ["Aus", "d\u00e4m\u00b7mern\u00b7dem", "Grun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Steigt eine Rotunde", "tokens": ["Steigt", "ei\u00b7ne", "Ro\u00b7tun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von Jaspis empor,", "tokens": ["Von", "Jas\u00b7pis", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die W\u00e4nde wie Spiegel,", "tokens": ["Die", "W\u00e4n\u00b7de", "wie", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Von Golde die Riegel", "tokens": ["Von", "Gol\u00b7de", "die", "Rie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Am ehernen Thor.", "tokens": ["Am", "e\u00b7her\u00b7nen", "Thor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Da sprudelt im Dunkel,", "tokens": ["Da", "spru\u00b7delt", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erhellt von Karfunkel,", "tokens": ["Er\u00b7hellt", "von", "Kar\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So alt wie die Zeit,", "tokens": ["So", "alt", "wie", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Quell, dessen Tugend", "tokens": ["Ein", "Quell", ",", "des\u00b7sen", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Blume der Jugend", "tokens": ["Die", "Blu\u00b7me", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und Sch\u00f6nheit erneut.", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Seit Salomo nahte", "tokens": ["Seit", "Sa\u00b7lo\u00b7mo", "nah\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem luftigen Staate", "tokens": ["Dem", "luf\u00b7ti\u00b7gen", "Staa\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kein Aeronaut.", "tokens": ["Kein", "A\u00b7e\u00b7ro\u00b7naut", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dies hat mir, nach Schriften", "tokens": ["Dies", "hat", "mir", ",", "nach", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In Mumiengr\u00fcften,", "tokens": ["In", "Mu\u00b7mi\u00b7en\u00b7gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein Sylphe vertraut.", "tokens": ["Ein", "Syl\u00b7phe", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Noch kann ich zu wenig", "tokens": ["Noch", "kann", "ich", "zu", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKA", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von dem, was der K\u00f6nig", "tokens": ["Von", "dem", ",", "was", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der Geister gekonnt;", "tokens": ["Der", "Geis\u00b7ter", "ge\u00b7konnt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sonst w\u00e4re zur Stunde,", "tokens": ["Sonst", "w\u00e4\u00b7re", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zusamt der Rotunde,", "tokens": ["Zu\u00b7samt", "der", "Ro\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Quell in Pyrmont.", "tokens": ["Der", "Quell", "in", "Pyr\u00b7mont", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}