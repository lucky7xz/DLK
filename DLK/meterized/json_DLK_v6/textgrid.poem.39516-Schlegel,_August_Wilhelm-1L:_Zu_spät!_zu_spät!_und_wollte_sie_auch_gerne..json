{"textgrid.poem.39516": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu sp\u00e4t! zu sp\u00e4t! und wollte sie auch gerne.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu sp\u00e4t! zu sp\u00e4t! und wollte sie auch gerne.", "tokens": ["Zu", "sp\u00e4t", "!", "zu", "sp\u00e4t", "!", "und", "woll\u00b7te", "sie", "auch", "ger\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$.", "PTKA", "ADJD", "$.", "KON", "VMFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Jugend, die mein Haupt gekr\u00f6net,", "tokens": ["Die", "Ju\u00b7gend", ",", "die", "mein", "Haupt", "ge\u00b7kr\u00f6\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Poesie, die meine Brust durcht\u00f6net,", "tokens": ["Die", "Poe\u00b7sie", ",", "die", "mei\u00b7ne", "Brust", "durch\u00b7t\u00f6\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie sind entfloh'n. Es bla\u00dfen meine Sterne.", "tokens": ["Sie", "sind", "ent\u00b7floh'", "n.", "Es", "bla\u00b7\u00dfen", "mei\u00b7ne", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach! warum blieb ich einsam nicht und ferne?", "tokens": ["Ach", "!", "wa\u00b7rum", "blieb", "ich", "ein\u00b7sam", "nicht", "und", "fer\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADJD", "PTKNEG", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4ngst hatt' ich s\u00fc\u00dfem Trug nicht mehr gefr\u00f6hnet,", "tokens": ["L\u00e4ngst", "hatt'", "ich", "s\u00fc\u00b7\u00dfem", "Trug", "nicht", "mehr", "ge\u00b7fr\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ward des Wahnes Schuld noch nicht vers\u00f6hnet,", "tokens": ["Doch", "ward", "des", "Wah\u00b7nes", "Schuld", "noch", "nicht", "ver\u00b7s\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Zeit ist's, da\u00df ich in mir sterben lerne.", "tokens": ["Und", "Zeit", "ist's", ",", "da\u00df", "ich", "in", "mir", "ster\u00b7ben", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Weib begegnet mir voll Huld und Milde,", "tokens": ["Ein", "Weib", "be\u00b7geg\u00b7net", "mir", "voll", "Huld", "und", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ist ein heil'ger Engel ihr Gef\u00e4hrte;", "tokens": ["Doch", "ist", "ein", "heil'\u00b7ger", "En\u00b7gel", "ihr", "Ge\u00b7f\u00e4hr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich darf nicht bitten und sie darf nicht geben.", "tokens": ["Ich", "darf", "nicht", "bit\u00b7ten", "und", "sie", "darf", "nicht", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich schaue sehnend nach dem zarten Bilde,", "tokens": ["Ich", "schau\u00b7e", "seh\u00b7nend", "nach", "dem", "zar\u00b7ten", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da winkt der Cherub mit dem Flammenschwerte:", "tokens": ["Da", "winkt", "der", "Che\u00b7rub", "mit", "dem", "Flam\u00b7men\u00b7schwer\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbnimm Abschied von der Liebe, von dem Leben!\u00ab", "tokens": ["\u00bb", "nimm", "Ab\u00b7schied", "von", "der", "Lie\u00b7be", ",", "von", "dem", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}