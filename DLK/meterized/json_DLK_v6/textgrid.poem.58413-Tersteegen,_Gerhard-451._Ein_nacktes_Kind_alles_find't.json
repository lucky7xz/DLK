{"textgrid.poem.58413": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "451. Ein nacktes Kind alles find't", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie ein kleines Kind ganz unverstellt und stille", "tokens": ["Gleich\u00b7wie", "ein", "klei\u00b7nes", "Kind", "ganz", "un\u00b7ver\u00b7stellt", "und", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leg deinen nackten Grund vor Gottes Augen hin;", "tokens": ["Leg", "dei\u00b7nen", "nack\u00b7ten", "Grund", "vor", "Got\u00b7tes", "Au\u00b7gen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schweige deine Zung', Vernunft und Lust und Wille \u2013", "tokens": ["Es", "schwei\u00b7ge", "dei\u00b7ne", "Zung'", ",", "Ver\u00b7nunft", "und", "Lust", "und", "Wil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was lehrt und gibt uns nicht ein solcher Kindersinn!", "tokens": ["Was", "lehrt", "und", "gibt", "uns", "nicht", "ein", "sol\u00b7cher", "Kin\u00b7der\u00b7sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}