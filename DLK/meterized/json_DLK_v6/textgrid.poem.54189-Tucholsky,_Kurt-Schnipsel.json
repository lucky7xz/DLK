{"textgrid.poem.54189": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er war sehr eitel darauf, nicht eitel zu sein.", "tokens": ["Er", "war", "sehr", "ei\u00b7tel", "da\u00b7rauf", ",", "nicht", "ei\u00b7tel", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PAV", "$,", "PTKNEG", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Seele jeder Ordnung ist ein gro\u00dfer Papierkorb.", "tokens": ["Die", "See\u00b7le", "je\u00b7der", "Ord\u00b7nung", "ist", "ein", "gro\u00b7\u00dfer", "Pa\u00b7pier\u00b7korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Wann beherrschst du eine fremde Sprache wirklich? Wenn du Kreuzwortr\u00e4tsel in ihr l\u00f6sen kannst.", "tokens": ["Wann", "be\u00b7herrschst", "du", "ei\u00b7ne", "frem\u00b7de", "Spra\u00b7che", "wirk\u00b7lich", "?", "Wenn", "du", "Kreuz\u00b7wort\u00b7r\u00e4t\u00b7sel", "in", "ihr", "l\u00f6\u00b7sen", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$.", "KOUS", "PPER", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}