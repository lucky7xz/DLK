{"textgrid.poem.31565": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Taube / die den Fried' aus Christi Wunden bringet!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Taube / die den Fried' aus Christi Wunden bringet!", "tokens": ["Ach", "Tau\u00b7be", "/", "die", "den", "Fried'", "aus", "Chris\u00b7ti", "Wun\u00b7den", "brin\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "ART", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "f\u00fchr' in mein' Herzens Arch ein kleines Zweiglein hin /", "tokens": ["f\u00fchr'", "in", "mein'", "Her\u00b7zens", "Arch", "ein", "klei\u00b7nes", "Zwei\u00b7glein", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "weil in der S\u00fcnden-Flut der Eitelkeit ich bin /", "tokens": ["weil", "in", "der", "S\u00fcn\u00b7den\u00b7Flut", "der", "Ei\u00b7tel\u00b7keit", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die mich / durch deine Gnad / wie andre nicht verschlinget.", "tokens": ["die", "mich", "/", "durch", "dei\u00b7ne", "Gnad", "/", "wie", "and\u00b7re", "nicht", "ver\u00b7schlin\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "APPR", "PPOSAT", "NN", "$(", "PWAV", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch bin ich auch von ihr ganz Seel-beschwert umringet.", "tokens": ["Doch", "bin", "ich", "auch", "von", "ihr", "ganz", "Seel\u00b7be\u00b7schwert", "um\u00b7rin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erh\u00e4ltst ihn nicht / so sinkt mein schwacher Erden-Sinn.", "tokens": ["Er\u00b7h\u00e4ltst", "ihn", "nicht", "/", "so", "sinkt", "mein", "schwa\u00b7cher", "Er\u00b7den\u00b7Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Gnaden-Allmacht macht / da\u00df ich beschutzt entrinn' /", "tokens": ["Dein", "Gna\u00b7den\u00b7All\u00b7macht", "macht", "/", "da\u00df", "ich", "be\u00b7schutzt", "ent\u00b7rinn'", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ob wol der Wellen Braus mein Schifflein m\u00e4chtig schwinget.", "tokens": ["ob", "wol", "der", "Wel\u00b7len", "Braus", "mein", "Schif\u00b7flein", "m\u00e4ch\u00b7tig", "schwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Strahl der G\u00f6ttlichkeit / du unausdenklichs Weben /", "tokens": ["Du", "Strahl", "der", "G\u00f6tt\u00b7lich\u00b7keit", "/", "du", "un\u00b7aus\u00b7denk\u00b7lichs", "We\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "du unverg\u00e4nglicher allein nur Weiser Geist!", "tokens": ["du", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7cher", "al\u00b7lein", "nur", "Wei\u00b7ser", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "wollst mein gefl\u00fcgelts Herz durch deinen Wind erheben /", "tokens": ["wollst", "mein", "ge\u00b7fl\u00fc\u00b7gelts", "Herz", "durch", "dei\u00b7nen", "Wind", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df in der Schn\u00f6dheit schon beginn was himmlisch heist.", "tokens": ["Da\u00df", "in", "der", "Schn\u00f6d\u00b7heit", "schon", "be\u00b7ginn", "was", "himm\u00b7lisch", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "APPR", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "gib durch dein Einflu\u00df-Krafft / da\u00df ich nach dem m\u00f6g streben /", "tokens": ["gib", "durch", "dein", "Ein\u00b7flu\u00df\u00b7Krafft", "/", "da\u00df", "ich", "nach", "dem", "m\u00f6g", "stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "was ewig ist / und dich / mein h\u00f6chster Herrscher / preist.", "tokens": ["was", "e\u00b7wig", "ist", "/", "und", "dich", "/", "mein", "h\u00f6chs\u00b7ter", "Herr\u00b7scher", "/", "preist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$(", "KON", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}