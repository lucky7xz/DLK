{"textgrid.poem.40532": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hingekauert an der Stra\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hingekauert an der Stra\u00dfen", "tokens": ["Hin\u00b7ge\u00b7kau\u00b7ert", "an", "der", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Aloe sich dehnt,", "tokens": ["Ei\u00b7ne", "A\u00b7loe", "sich", "dehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie ein Kn\u00e4ul von Gliedesma\u00dfen,", "tokens": ["Wie", "ein", "Kn\u00e4ul", "von", "Glie\u00b7des\u00b7ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Breit, gem\u00e4chlich hingelehnt.", "tokens": ["Breit", ",", "ge\u00b7m\u00e4ch\u00b7lich", "hin\u00b7ge\u00b7lehnt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So im fernen China sitzen", "tokens": ["So", "im", "fer\u00b7nen", "Chi\u00b7na", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag ein feister Mandarin,", "tokens": ["Mag", "ein", "feis\u00b7ter", "Man\u00b7da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streckend blanke N\u00e4gelspitzen", "tokens": ["Stre\u00b7ckend", "blan\u00b7ke", "N\u00e4\u00b7gel\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbstbehaglich vor sich hin.", "tokens": ["Selbst\u00b7be\u00b7hag\u00b7lich", "vor", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eine Pinie sprie\u00dft daneben,", "tokens": ["Ei\u00b7ne", "Pi\u00b7nie", "sprie\u00dft", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Neigt auf sie ihr buschig Zelt,", "tokens": ["Neigt", "auf", "sie", "ihr", "bu\u00b7schig", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPER", "ADJD", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie sein Sklav' ob Jenem eben", "tokens": ["Wie", "sein", "Skla\u00b7v'", "ob", "Je\u00b7nem", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "KOUS", "PDS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Baldachin und Schirmdach h\u00e4lt.", "tokens": ["Bal\u00b7da\u00b7ch\u00b7in", "und", "Schirm\u00b7dach", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Hundert Jahre ziehn die Stra\u00dfe!", "tokens": ["Hun\u00b7dert", "Jah\u00b7re", "ziehn", "die", "Stra\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von Sonnenschein welch Meer!", "tokens": ["Und", "von", "Son\u00b7nen\u00b7schein", "welch", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lenzesbl\u00fcthen, welche Masse!", "tokens": ["Len\u00b7zes\u00b7bl\u00fc\u00b7then", ",", "wel\u00b7che", "Mas\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Staub und Wandrer, welch ein Heer!", "tokens": ["Staub", "und", "Wand\u00b7rer", ",", "welch", "ein", "Heer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Endlich sp\u00fcrt so seltsam m\u00e4chtig", "tokens": ["End\u00b7lich", "sp\u00fcrt", "so", "selt\u00b7sam", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aloe ihr Herz bedr\u00e4ngt,", "tokens": ["A\u00b7loe", "ihr", "Herz", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bis ein Schaft, gar schlank und pr\u00e4chtig,", "tokens": ["Bis", "ein", "Schaft", ",", "gar", "schlank", "und", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcthenvoll die H\u00fclle sprengt.", "tokens": ["Bl\u00fc\u00b7then\u00b7voll", "die", "H\u00fcl\u00b7le", "sprengt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Erste Bl\u00fcthe, helle, blanke,", "tokens": ["Ers\u00b7te", "Bl\u00fc\u00b7the", ",", "hel\u00b7le", ",", "blan\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den kahlen Schaft umlaubt!", "tokens": ["Die", "den", "kah\u00b7len", "Schaft", "um\u00b7laubt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erster bl\u00fchender Gedanke", "tokens": ["Ers\u00b7ter", "bl\u00fc\u00b7hen\u00b7der", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um des Mandarinen Haupt!", "tokens": ["Um", "des", "Man\u00b7da\u00b7ri\u00b7nen", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Weh, da\u00df einmal nur in Tagen", "tokens": ["Weh", ",", "da\u00df", "ein\u00b7mal", "nur", "in", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Jahrhunderts bl\u00fcht dein Gru\u00df!", "tokens": ["Des", "Jahr\u00b7hun\u00b7derts", "bl\u00fcht", "dein", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wehe, da\u00df, wer dich getragen,", "tokens": ["We\u00b7he", ",", "da\u00df", ",", "wer", "dich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Auch an dir verscheiden mu\u00df!", "tokens": ["Auch", "an", "dir", "ver\u00b7schei\u00b7den", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}