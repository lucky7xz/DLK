{"textgrid.poem.59757": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Verse zur Begleitung des Schillschen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewig, Mensch, sollst du das loben,", "tokens": ["E\u00b7wig", ",", "Mensch", ",", "sollst", "du", "das", "lo\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Erdennot besiegt", "tokens": ["Was", "die", "Er\u00b7den\u00b7not", "be\u00b7siegt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im stolzen Flug nach oben", "tokens": ["Und", "im", "stol\u00b7zen", "Flug", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit des Geistes Fl\u00fcgeln fliegt,", "tokens": ["Mit", "des", "Geis\u00b7tes", "Fl\u00fc\u00b7geln", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was mit hochgebornen Seelen", "tokens": ["Was", "mit", "hoch\u00b7ge\u00b7bor\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um die stolze Freiheit wirbt", "tokens": ["Um", "die", "stol\u00b7ze", "Frei\u00b7heit", "wirbt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nicht rechnen kann und z\u00e4hlen,", "tokens": ["Und", "nicht", "rech\u00b7nen", "kann", "und", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo sich's ehrlich lebt und stirbt.", "tokens": ["Wo", "sich's", "ehr\u00b7lich", "lebt", "und", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}