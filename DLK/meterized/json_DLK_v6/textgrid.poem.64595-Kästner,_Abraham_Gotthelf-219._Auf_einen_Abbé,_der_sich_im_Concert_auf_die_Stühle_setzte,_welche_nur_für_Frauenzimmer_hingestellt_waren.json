{"textgrid.poem.64595": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "219. Auf einen Abb\u00e9, der sich im Concert auf die St\u00fchle setzte, welche nur f\u00fcr Frauenzimmer hingestellt waren", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die St\u00fchle sind zwar nicht f\u00fcr das Abb\u00e9-Geschlecht,", "tokens": ["Die", "St\u00fch\u00b7le", "sind", "zwar", "nicht", "f\u00fcr", "das", "Ab\u00b7b\u00e9\u00b7Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch als ein Geistlicher hat er der Weiber Recht", "tokens": ["Doch", "als", "ein", "Geist\u00b7li\u00b7cher", "hat", "er", "der", "Wei\u00b7ber", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}