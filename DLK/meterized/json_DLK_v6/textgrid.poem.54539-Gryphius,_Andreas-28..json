{"textgrid.poem.54539": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der h\u00f6ll'sche Trauer-Geist/ erhitzt von Rach und Toben/", "tokens": ["Der", "h\u00f6ll'\u00b7sche", "Trau\u00b7e\u00b7rGeist", "/", "er\u00b7hitzt", "von", "Rach", "und", "To\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ri\u00df mitten in der Nacht aus seines Kerckers Nacht/", "tokens": ["Ri\u00df", "mit\u00b7ten", "in", "der", "Nacht", "aus", "sei\u00b7nes", "Ker\u00b7ckers", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wolt in Asch und Rauch/ durch grauser Flammen Macht/", "tokens": ["Und", "wolt", "in", "Asch", "und", "Rauch", "/", "durch", "grau\u00b7ser", "Flam\u00b7men", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NE", "KON", "NN", "$(", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umst\u00fcrtzen was sich kaum aus seinem Grau\u00df erhoben.", "tokens": ["Um\u00b7st\u00fcrt\u00b7zen", "was", "sich", "kaum", "aus", "sei\u00b7nem", "Grau\u00df", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRELS", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Funcken schwungen sich/ der Wiederglantz von oben", "tokens": ["Die", "Fun\u00b7cken", "schwun\u00b7gen", "sich", "/", "der", "Wie\u00b7der\u00b7glantz", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erschr\u00f6ckte Ga\u00df und Wall/ weil Dach und Pfost erkracht;", "tokens": ["Er\u00b7schr\u00f6ck\u00b7te", "Ga\u00df", "und", "Wall", "/", "weil", "Dach", "und", "Pfost", "er\u00b7kracht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "$(", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch ward sein Grimm ged\u00e4mpfft/ weil du vor uns gewacht/", "tokens": ["Doch", "ward", "sein", "Grimm", "ge\u00b7d\u00e4mpfft", "/", "weil", "du", "vor", "uns", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NE", "VVPP", "$(", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du Freuden-Geist/ den wir als Gottes Flamme loben.", "tokens": ["Du", "Freu\u00b7den\u00b7Geist", "/", "den", "wir", "als", "Got\u00b7tes", "Flam\u00b7me", "lo\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "PPER", "KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Feuer/ das den Pusch entz\u00fcndet nicht verzehrt/", "tokens": ["Du", "Feu\u00b7er", "/", "das", "den", "Pusch", "ent\u00b7z\u00fcn\u00b7det", "nicht", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PDS", "ART", "NN", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hast des Verderbers Gluth allm\u00e4chtig abgewehrt;", "tokens": ["Hast", "des", "Ver\u00b7der\u00b7bers", "Gluth", "all\u00b7m\u00e4ch\u00b7tig", "ab\u00b7ge\u00b7wehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gleich wie der lichte Blitz den schwachen Brand erstecket.", "tokens": ["Gleich", "wie", "der", "lich\u00b7te", "Blitz", "den", "schwa\u00b7chen", "Brand", "er\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Feind trug unrein Feur auf deinen Lob-Altar;", "tokens": ["Der", "Feind", "trug", "un\u00b7rein", "Feur", "auf", "dei\u00b7nen", "Lob\u00b7Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O Quelle reiner Lust! du l\u00f6schest die Gefahr/", "tokens": ["O", "Quel\u00b7le", "rei\u00b7ner", "Lust", "!", "du", "l\u00f6\u00b7schest", "die", "Ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wir schlieffen/ du hast uns best\u00fcrtzt in Freud erwecket.", "tokens": ["Wir", "schlief\u00b7fen", "/", "du", "hast", "uns", "be\u00b7st\u00fcrtzt", "in", "Freud", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}