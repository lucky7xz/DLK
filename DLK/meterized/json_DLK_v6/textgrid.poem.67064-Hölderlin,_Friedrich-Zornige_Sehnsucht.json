{"textgrid.poem.67064": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zornige Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich duld es nimmer! ewig und ewig so", "tokens": ["Ich", "duld", "es", "nim\u00b7mer", "!", "e\u00b7wig", "und", "e\u00b7wig", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "ADJD", "KON", "ADJD", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Knabenschritte, wie ein Gekerkerter", "tokens": ["Die", "Kna\u00b7ben\u00b7schrit\u00b7te", ",", "wie", "ein", "Ge\u00b7ker\u00b7ker\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die kurzen vorgeme\u00dfnen Schritte", "tokens": ["Die", "kur\u00b7zen", "vor\u00b7ge\u00b7me\u00df\u00b7nen", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00e4glich zu wandeln, ich duld es nimmer!", "tokens": ["T\u00e4g\u00b7lich", "zu", "wan\u00b7deln", ",", "ich", "duld", "es", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ists Menschenlos \u2013 ists meines? ich trag es nicht,", "tokens": ["Ists", "Men\u00b7schen\u00b7los", "\u2013", "ists", "mei\u00b7nes", "?", "ich", "trag", "es", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "VAFIN", "PPOSAT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mich reizt der Lorbeer, \u2013 Ruhe begl\u00fcckt mich nicht,", "tokens": ["Mich", "reizt", "der", "Lor\u00b7beer", ",", "\u2013", "Ru\u00b7he", "be\u00b7gl\u00fcckt", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "$(", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gefahren zeugen M\u00e4nnerkr\u00e4fte,", "tokens": ["Ge\u00b7fah\u00b7ren", "zeu\u00b7gen", "M\u00e4n\u00b7ner\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leiden erheben die Brust des J\u00fcnglings.", "tokens": ["Lei\u00b7den", "er\u00b7he\u00b7ben", "die", "Brust", "des", "J\u00fcng\u00b7lings", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Was bin ich dir, was bin ich, mein Vaterland?", "tokens": ["Was", "bin", "ich", "dir", ",", "was", "bin", "ich", ",", "mein", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "$,", "PWS", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein siecher S\u00e4ugling, welchen mit tr\u00e4nendem,", "tokens": ["Ein", "sie\u00b7cher", "S\u00e4ug\u00b7ling", ",", "wel\u00b7chen", "mit", "tr\u00e4\u00b7nen\u00b7dem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAT", "APPR", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit hoffnungslosem Blick die Mutter", "tokens": ["Mit", "hoff\u00b7nungs\u00b7lo\u00b7sem", "Blick", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In den gedultigen Armen schaukelt.", "tokens": ["In", "den", "ge\u00b7dul\u00b7ti\u00b7gen", "Ar\u00b7men", "schau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Mich tr\u00f6stete das blinkende Kelchglas nie,", "tokens": ["Mich", "tr\u00f6s\u00b7te\u00b7te", "das", "blin\u00b7ken\u00b7de", "Kelchg\u00b7las", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mich nie der Blick der l\u00e4chelnden T\u00e4ndlerin,", "tokens": ["Mich", "nie", "der", "Blick", "der", "l\u00e4\u00b7cheln\u00b7den", "T\u00e4nd\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Soll ewig Trauern mich umwolken?", "tokens": ["Soll", "e\u00b7wig", "Trau\u00b7ern", "mich", "um\u00b7wol\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewig mich t\u00f6ten die zornge Sehnsucht?", "tokens": ["E\u00b7wig", "mich", "t\u00f6\u00b7ten", "die", "zorn\u00b7ge", "Sehn\u00b7sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Was soll des Freundes traulicher Handschlag mir,", "tokens": ["Was", "soll", "des", "Freun\u00b7des", "trau\u00b7li\u00b7cher", "Hand\u00b7schlag", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was mir des Fr\u00fchlings freundlicher Morgengru\u00df,", "tokens": ["Was", "mir", "des", "Fr\u00fch\u00b7lings", "freund\u00b7li\u00b7cher", "Mor\u00b7gen\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was mir der Eiche Schatten? was der", "tokens": ["Was", "mir", "der", "Ei\u00b7che", "Schat\u00b7ten", "?", "was", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "$.", "PWS", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bl\u00fchenden Rebe, der Linde D\u00fcfte?", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "Re\u00b7be", ",", "der", "Lin\u00b7de", "D\u00fcf\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NE", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Beim grauen Mana! nimmer genie\u00df ich dein,", "tokens": ["Beim", "grau\u00b7en", "Ma\u00b7na", "!", "nim\u00b7mer", "ge\u00b7nie\u00df", "ich", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du Kelch der Freuden, blinkest du noch so sch\u00f6n,", "tokens": ["Du", "Kelch", "der", "Freu\u00b7den", ",", "blin\u00b7kest", "du", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis mir ein M\u00e4nnerwerk gelinget,", "tokens": ["Bis", "mir", "ein", "M\u00e4n\u00b7ner\u00b7werk", "ge\u00b7lin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ich ihn hasche, den ersten Lorbeer.", "tokens": ["Bis", "ich", "ihn", "ha\u00b7sche", ",", "den", "ers\u00b7ten", "Lor\u00b7beer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Schwur ist gro\u00df. Er zeuget im Auge mir", "tokens": ["Der", "Schwur", "ist", "gro\u00df", ".", "Er", "zeu\u00b7get", "im", "Au\u00b7ge", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Tr\u00e4n, und wohl mir, wenn ihn Vollendung kr\u00f6nt,", "tokens": ["Die", "Tr\u00e4n", ",", "und", "wohl", "mir", ",", "wenn", "ihn", "Vol\u00b7len\u00b7dung", "kr\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dann jauchz auch ich, du Kreis der Frohen,", "tokens": ["Dann", "jauchz", "auch", "ich", ",", "du", "Kreis", "der", "Fro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann, o Natur, ist dein L\u00e4cheln Wonne.", "tokens": ["Dann", ",", "o", "Na\u00b7tur", ",", "ist", "dein", "L\u00e4\u00b7cheln", "Won\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}