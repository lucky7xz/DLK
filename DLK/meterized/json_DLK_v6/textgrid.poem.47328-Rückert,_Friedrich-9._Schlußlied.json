{"textgrid.poem.47328": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9. Schlu\u00dflied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Duft, der meine Seele speiset, verla\u00df mich nicht!", "tokens": ["Du", "Duft", ",", "der", "mei\u00b7ne", "See\u00b7le", "spei\u00b7set", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Traum, der mit mir durchs Leben reiset, verla\u00df mich nicht!", "tokens": ["Traum", ",", "der", "mit", "mir", "durchs", "Le\u00b7ben", "rei\u00b7set", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "+----+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Du Paradiesesvogel, dessen Schwing' ungesehn", "tokens": ["Du", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7vo\u00b7gel", ",", "des\u00b7sen", "Schwing'", "un\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELAT", "NN", "VVINF"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit leisem S\u00e4useln mich umkreiset, verla\u00df mich nicht!", "tokens": ["Mit", "lei\u00b7sem", "S\u00e4u\u00b7seln", "mich", "um\u00b7krei\u00b7set", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du Amme mir und Ammenm\u00e4rchen der Kindheit einst!", "tokens": ["Du", "Am\u00b7me", "mir", "und", "Am\u00b7men\u00b7m\u00e4r\u00b7chen", "der", "Kind\u00b7heit", "einst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "KON", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Du fehlst, und ich bin noch verwaiset verla\u00df mich nicht!", "tokens": ["Du", "fehlst", ",", "und", "ich", "bin", "noch", "ver\u00b7wai\u00b7set", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADV", "VVFIN", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+--++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Du statt der Jugend mir geblieben, da sie mir floh;", "tokens": ["Du", "statt", "der", "Ju\u00b7gend", "mir", "ge\u00b7blie\u00b7ben", ",", "da", "sie", "mir", "floh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPER", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wo du mir fliehst, bin ich ergreiset, verla\u00df mich nicht;", "tokens": ["Wo", "du", "mir", "fliehst", ",", "bin", "ich", "er\u00b7grei\u00b7set", ",", "ver\u00b7la\u00df", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "O du mein Fr\u00fchling! sieh, wie drau\u00dfen der Herbst nun braust;", "tokens": ["O", "du", "mein", "Fr\u00fch\u00b7ling", "!", "sieh", ",", "wie", "drau\u00b7\u00dfen", "der", "Herbst", "nun", "braust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Komm, da\u00df nicht Winter mich umeiset, verla\u00df mich nicht!", "tokens": ["Komm", ",", "da\u00df", "nicht", "Win\u00b7ter", "mich", "um\u00b7ei\u00b7set", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PTKNEG", "NN", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "O Hauch des Friedens! horch, wie drau\u00dfen das Leben tobt;", "tokens": ["O", "Hauch", "des", "Frie\u00b7dens", "!", "horch", ",", "wie", "drau\u00b7\u00dfen", "das", "Le\u00b7ben", "tobt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "VVIMP", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wer ist, der still hindurch mich weiset? Verla\u00df mich nicht!", "tokens": ["Wer", "ist", ",", "der", "still", "hin\u00b7durch", "mich", "wei\u00b7set", "?", "Ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADJD", "PAV", "PPER", "VVFIN", "$.", "NN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "O du mein Rausch! du meine Liebe! o du mein Lied!", "tokens": ["O", "du", "mein", "Rausch", "!", "du", "mei\u00b7ne", "Lie\u00b7be", "!", "o", "du", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$.", "PPER", "PPOSAT", "NN", "$.", "FM", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Das hier durch mich sich selber preiset, verla\u00df mich nicht!", "tokens": ["Das", "hier", "durch", "mich", "sich", "sel\u00b7ber", "prei\u00b7set", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "PRF", "ADV", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}