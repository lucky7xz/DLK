{"textgrid.poem.52977": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du dich denn zu Tode gr\u00e4men,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dich denn zu Tode gr\u00e4men,", "tokens": ["Willst", "du", "dich", "denn", "zu", "To\u00b7de", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich die Menschen deiner sch\u00e4men?", "tokens": ["Wenn", "sich", "die", "Men\u00b7schen", "dei\u00b7ner", "sch\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Tritt ohne Furcht in deiner Kraft hervor;", "tokens": ["Tritt", "oh\u00b7ne", "Furcht", "in", "dei\u00b7ner", "Kraft", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was k\u00fcmmert dich der goldne Thor?", "tokens": ["Was", "k\u00fcm\u00b7mert", "dich", "der", "gold\u00b7ne", "Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ver\u00e4chtlich ist gewi\u00df der Mann,", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "ist", "ge\u00b7wi\u00df", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ohne Grund verachten kann.", "tokens": ["Der", "oh\u00b7ne", "Grund", "ver\u00b7ach\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Weise fragt nicht, ob man ihn auch ehrt;", "tokens": ["Der", "Wei\u00b7se", "fragt", "nicht", ",", "ob", "man", "ihn", "auch", "ehrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur er allein bestimmt sich seinen Werth;", "tokens": ["Nur", "er", "al\u00b7lein", "be\u00b7stimmt", "sich", "sei\u00b7nen", "Werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ganz unbesorgt um Ruhm und Schmach,", "tokens": ["Ganz", "un\u00b7be\u00b7sorgt", "um", "Ruhm", "und", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Geht er dem eignen Lichte nach:", "tokens": ["Geht", "er", "dem", "eig\u00b7nen", "Lich\u00b7te", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und hat er durch Vernunft nur Einen Freund gewonnen,", "tokens": ["Und", "hat", "er", "durch", "Ver\u00b7nunft", "nur", "Ei\u00b7nen", "Freund", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So hat die Parze gut gesponnen.", "tokens": ["So", "hat", "die", "Par\u00b7ze", "gut", "ge\u00b7spon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}