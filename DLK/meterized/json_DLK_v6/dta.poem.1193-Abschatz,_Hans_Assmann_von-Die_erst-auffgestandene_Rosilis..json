{"dta.poem.1193": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die erst-auffgestandene Rosilis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kam den andern Tag zur Rosilis gegangen/", "tokens": ["Ich", "kam", "den", "an\u00b7dern", "Tag", "zur", "Ro\u00b7si\u00b7lis", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sie zum Morgen noch unangeleget war.", "tokens": ["Als", "sie", "zum", "Mor\u00b7gen", "noch", "un\u00b7an\u00b7ge\u00b7le\u00b7get", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie stellte die Auror in eignem Bilde dar/", "tokens": ["Sie", "stell\u00b7te", "die", "Au\u00b7ror", "in", "eig\u00b7nem", "Bil\u00b7de", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+++-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Wenn sie der fr\u00fchen Welt zeigt ihre Rosen-Wangen.", "tokens": ["Wenn", "sie", "der", "fr\u00fc\u00b7hen", "Welt", "zeigt", "ih\u00b7re", "Ro\u00b7sen\u00b7Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Augen/ welche fast der Schlaff noch hielt umfangen/", "tokens": ["Die", "Au\u00b7gen", "/", "wel\u00b7che", "fast", "der", "Schlaff", "noch", "hielt", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "ART", "NN", "ADV", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verglichen sich der erst entwichnen Sternen-Schaar/", "tokens": ["Ver\u00b7gli\u00b7chen", "sich", "der", "erst", "ent\u00b7wich\u00b7nen", "Ster\u00b7nen\u00b7Schaar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr \u00fcber Stirne/ Wang und Hals gestreutes Haar", "tokens": ["Ihr", "\u00fc\u00b7ber", "Stir\u00b7ne", "/", "Wang", "und", "Hals", "ge\u00b7streu\u00b7tes", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "$(", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem Netze/ welches uns die theuren W\u00fcrme langen.", "tokens": ["Dem", "Net\u00b7ze", "/", "wel\u00b7ches", "uns", "die", "theu\u00b7ren", "W\u00fcr\u00b7me", "lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der wei\u00dfen H\u00e4nde Schnee schien heller denn der Tag/", "tokens": ["Der", "wei\u00b7\u00dfen", "H\u00e4n\u00b7de", "Schnee", "schien", "hel\u00b7ler", "denn", "der", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADJD", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der angebohrne Schmuck/ die lieblichen Geberden/", "tokens": ["Der", "an\u00b7ge\u00b7bohr\u00b7ne", "Schmuck", "/", "die", "lieb\u00b7li\u00b7chen", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Besch\u00e4mten was der Flei\u00df/ die kluge Kunst/ vermag.", "tokens": ["Be\u00b7sch\u00e4m\u00b7ten", "was", "der", "Flei\u00df", "/", "die", "klu\u00b7ge", "Kunst", "/", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PWS", "ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Giebt Rosilis/ mein Licht/ zum Morgen solchen Schein/", "tokens": ["Giebt", "Ro\u00b7si\u00b7lis", "/", "mein", "Licht", "/", "zum", "Mor\u00b7gen", "sol\u00b7chen", "Schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "PPOSAT", "NN", "$(", "APPRART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie soll mein Hertze nicht zu lauter Flamme werden", "tokens": ["Wie", "soll", "mein", "Hert\u00b7ze", "nicht", "zu", "lau\u00b7ter", "Flam\u00b7me", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "VVFIN", "PTKNEG", "APPR", "PIAT", "NN", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn sie wird angelegt in vollem Mittag seyn!", "tokens": ["Wenn", "sie", "wird", "an\u00b7ge\u00b7legt", "in", "vol\u00b7lem", "Mit\u00b7tag", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}