": {"text": "Auch du, o Schwester Herz! bist itzo vaterlos,", "tokens": ["Auch", "du", ",", "o", "Schwes\u00b7ter", "Herz", "!", "bist", "it\u00b7zo", "va\u00b7ter\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "NN", "$.", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Nachdem du schon mit uns der Mutter Grab beweinet;", "tokens": ["Nach\u00b7dem", "du", "schon", "mit", "uns", "der", "Mut\u00b7ter", "Grab", "be\u00b7wei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Dein z\u00e4rtliches Geschlecht macht deinen Schmerz so gro\u00df,", "tokens": ["Dein", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7schlecht", "macht", "dei\u00b7nen", "Schmerz", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Da\u00df deiner Br\u00fcder Gram gedoppelt leichter scheinet.", "tokens": ["Da\u00df", "dei\u00b7ner", "Br\u00fc\u00b7der", "Gram", "ge\u00b7dop\u00b7pelt", "leich\u00b7ter", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wer nimmt hinf\u00fchro sich wohl deiner Wohlfahrt an?", "tokens": ["Wer", "nimmt", "hin\u00b7f\u00fch\u00b7ro", "sich", "wohl", "dei\u00b7ner", "Wohl\u00b7fahrt", "an", "?"], "token_info": ["word", "word", "word", "w