{"textgrid.poem.57507": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Vater! ach wie tief, wie sehr betr\u00fcbst du mich!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater! ach wie tief, wie sehr betr\u00fcbst du mich!", "tokens": ["Mein", "Va\u00b7ter", "!", "ach", "wie", "tief", ",", "wie", "sehr", "be\u00b7tr\u00fcbst", "du", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "XY", "KOKOM", "ADJD", "$,", "PWAV", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du stirbst, o Schmerzenswort! im Sommer deiner Jahre;", "tokens": ["Du", "stirbst", ",", "o", "Schmer\u00b7zens\u00b7wort", "!", "im", "Som\u00b7mer", "dei\u00b7ner", "Jah\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Angesicht erbla\u00dft, die Augen brechen sich,", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "er\u00b7bla\u00dft", ",", "die", "Au\u00b7gen", "bre\u00b7chen", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Glieder werden starr und f\u00fcllen Sarg und Baare.", "tokens": ["Die", "Glie\u00b7der", "wer\u00b7den", "starr", "und", "f\u00fcl\u00b7len", "Sarg", "und", "Baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O l\u00e4ngst besorgter Fall! o bitter Schmerzenstag!", "tokens": ["O", "l\u00e4ngst", "be\u00b7sorg\u00b7ter", "Fall", "!", "o", "bit\u00b7ter", "Schmer\u00b7zens\u00b7tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich sah dir ja bisher bek\u00fcmmert gnug entgegen;", "tokens": ["Ich", "sah", "dir", "ja", "bis\u00b7her", "be\u00b7k\u00fcm\u00b7mert", "gnug", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da du endlich k\u00f6mmst, so scheint dein harter Schlag,", "tokens": ["Und", "da", "du", "end\u00b7lich", "k\u00f6mmst", ",", "so", "scheint", "dein", "har\u00b7ter", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich mit dem Vater selbst in Staub und Grab zu legen.", "tokens": ["Mich", "mit", "dem", "Va\u00b7ter", "selbst", "in", "Staub", "und", "Grab", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich weis nicht, wie mir war, als ich das Siegel brach;", "tokens": ["Ich", "weis", "nicht", ",", "wie", "mir", "war", ",", "als", "ich", "das", "Sie\u00b7gel", "brach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich las und wu\u00dfte kaum des Schreibens Sinn zu finden:", "tokens": ["Ich", "las", "und", "wu\u00df\u00b7te", "kaum", "des", "Schrei\u00b7bens", "Sinn", "zu", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn weil fast jedes Wort von Tod und Trauern sprach,", "tokens": ["Denn", "weil", "fast", "je\u00b7des", "Wort", "von", "Tod", "und", "Trau\u00b7ern", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So schien mir unvermerkt Gesicht und Witz zu schwinden.", "tokens": ["So", "schien", "mir", "un\u00b7ver\u00b7merkt", "Ge\u00b7sicht", "und", "Witz", "zu", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kaum ward mir durch ein Wort die Schreckenspost bewu\u00dft,", "tokens": ["Kaum", "ward", "mir", "durch", "ein", "Wort", "die", "Schre\u00b7ckens\u00b7post", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So lief der Thr\u00e4nenstrom die welken Wangen nieder;", "tokens": ["So", "lief", "der", "Thr\u00e4\u00b7nen\u00b7strom", "die", "wel\u00b7ken", "Wan\u00b7gen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es wallte Blut und Herz in der getreusten Brust,", "tokens": ["Es", "wall\u00b7te", "Blut", "und", "Herz", "in", "der", "ge\u00b7treus\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und vor gerechtem Schmerz erstarrten alle Glieder.", "tokens": ["Und", "vor", "ge\u00b7rech\u00b7tem", "Schmerz", "er\u00b7starr\u00b7ten", "al\u00b7le", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ists m\u00f6glich? brach der Mund mit hundert Seufzern aus,", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "brach", "der", "Mund", "mit", "hun\u00b7dert", "Seuf\u00b7zern", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ists m\u00f6glich? armer Sohn! du stehst im Weysenorden.", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "ar\u00b7mer", "Sohn", "!", "du", "stehst", "im", "Wey\u00b7sen\u00b7or\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "O ungl\u00fcckseliges und hochbest\u00fcrztes Haus!", "tokens": ["O", "un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ges", "und", "hoch\u00b7be\u00b7st\u00fcrz\u00b7tes", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "So bist du abermal zum Trauerhause worden?", "tokens": ["So", "bist", "du", "a\u00b7ber\u00b7mal", "zum", "Trau\u00b7er\u00b7hau\u00b7se", "wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Neun Jahre sind es kaum, als meiner Mutter Sarg,", "tokens": ["Neun", "Jah\u00b7re", "sind", "es", "kaum", ",", "als", "mei\u00b7ner", "Mut\u00b7ter", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ach thr\u00e4nenwerthes Wort! vor deiner Th\u00fcr gestanden;", "tokens": ["Ach", "thr\u00e4\u00b7nen\u00b7wert\u00b7hes", "Wort", "!", "vor", "dei\u00b7ner", "Th\u00fcr", "ge\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als sich mein Angesicht vor Traurigkeit verbarg,", "tokens": ["Als", "sich", "mein", "An\u00b7ge\u00b7sicht", "vor", "Trau\u00b7rig\u00b7keit", "ver\u00b7barg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Denn was ich da verlohr, war nirgends mehr vorhanden:", "tokens": ["Denn", "was", "ich", "da", "ver\u00b7lohr", ",", "war", "nir\u00b7gends", "mehr", "vor\u00b7han\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und itzo, leider! sinkt die andre St\u00fctze hin;", "tokens": ["Und", "it\u00b7zo", ",", "lei\u00b7der", "!", "sinkt", "die", "and\u00b7re", "St\u00fct\u00b7ze", "hin", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$.", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der theure Vater folgt der Mutter nach dem Grabe.", "tokens": ["Der", "theu\u00b7re", "Va\u00b7ter", "folgt", "der", "Mut\u00b7ter", "nach", "dem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie k\u00f6mmt es, da\u00df ich nicht sogleich des Todes bin,", "tokens": ["Wie", "k\u00f6mmt", "es", ",", "da\u00df", "ich", "nicht", "sog\u00b7leich", "des", "To\u00b7des", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und vor Bek\u00fcmmerni\u00df noch Geist und Leben habe?", "tokens": ["Und", "vor", "Be\u00b7k\u00fcm\u00b7mer\u00b7ni\u00df", "noch", "Geist", "und", "Le\u00b7ben", "ha\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie, wenn ein Ungest\u00fcm durch dicke W\u00e4lder f\u00e4hrt,", "tokens": ["Wie", ",", "wenn", "ein", "Un\u00b7ge\u00b7st\u00fcm", "durch", "di\u00b7cke", "W\u00e4l\u00b7der", "f\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und den gesetzten Stamm der st\u00e4rksten Eichen f\u00e4llet,", "tokens": ["Und", "den", "ge\u00b7setz\u00b7ten", "Stamm", "der", "st\u00e4rks\u00b7ten", "Ei\u00b7chen", "f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sich nicht nur Strumpf und Stiel aus seinen Wurzeln kehrt,", "tokens": ["Sich", "nicht", "nur", "Strumpf", "und", "Stiel", "aus", "sei\u00b7nen", "Wur\u00b7zeln", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Fast jeder Zweig und Ast zersplittert niederf\u00e4llet:", "tokens": ["Fast", "je\u00b7der", "Zweig", "und", "Ast", "zer\u00b7split\u00b7tert", "nie\u00b7der\u00b7f\u00e4l\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So sollte die Natur, bey treuer Aeltern Grab,", "tokens": ["So", "soll\u00b7te", "die", "Na\u00b7tur", ",", "bey", "treu\u00b7er", "A\u00b7el\u00b7tern", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Auch ihrer Kinder Zahl zum k\u00fchlen Staube schicken,", "tokens": ["Auch", "ih\u00b7rer", "Kin\u00b7der", "Zahl", "zum", "k\u00fch\u00b7len", "Stau\u00b7be", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und die, durch deren Kraft sie uns das Leben gab,", "tokens": ["Und", "die", ",", "durch", "de\u00b7ren", "Kraft", "sie", "uns", "das", "Le\u00b7ben", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPR", "PRELAT", "NN", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Nicht schleuniger, als uns, der Eitelkeit entr\u00fccken.", "tokens": ["Nicht", "schleu\u00b7ni\u00b7ger", ",", "als", "uns", ",", "der", "Ei\u00b7tel\u00b7keit", "ent\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Denn wie des Gipfels Pracht, der seinen Baum geziert,", "tokens": ["Denn", "wie", "des", "Gip\u00b7fels", "Pracht", ",", "der", "sei\u00b7nen", "Baum", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nicht l\u00e4nger als der Stamm, in vollem Wachsthum gr\u00fcnet;", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "als", "der", "Stamm", ",", "in", "vol\u00b7lem", "Wach\u00b7sthum", "gr\u00fc\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KOKOM", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Allein so bald der Blitz den festen Stamm ger\u00fchrt,", "tokens": ["Al\u00b7lein", "so", "bald", "der", "Blitz", "den", "fes\u00b7ten", "Stamm", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ganz pl\u00f6tzlich nierderst\u00fcrzt und kaum zu Reisern dienet:", "tokens": ["Ganz", "pl\u00f6tz\u00b7lich", "nier\u00b7der\u00b7st\u00fcrzt", "und", "kaum", "zu"