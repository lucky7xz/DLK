"}, "line.58": {"text": "Vertritt mit Rath und That der rechten Mutter Stelle.", "tokens": ["Ver\u00b7tritt", "mit", "Rath", "und", "That", "der", "rech\u00b7ten", "Mut\u00b7ter", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Sie hat mit dir und uns es immer wohl gemeynt,", "tokens": ["Sie", "hat", "mit", "dir", "und", "uns", "es", "im\u00b7mer", "wohl", "ge\u00b7meynt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KON", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und lindert uns auch hier die schwersten Trauerf\u00e4lle.", "tokens": ["Und", "lin\u00b7dert", "uns", "auch", "hier", "die", "schwers\u00b7ten", "Trau\u00b7er\u00b7f\u00e4l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Verwandte! die Gebl\u00fct und Freundschaft uns verbindt,", "tokens": ["Ver\u00b7wand\u00b7te", "!", "die", "Ge\u00b7bl\u00fct", "und", "Freund\u00b7schaft", "uns", "ver\u00b7bindt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Erw\u00e4gt nebst ihr den Schmerz zu fr\u00fch beraubter Weysen;", "tokens": ["Er\u00b7w\u00e4gt", "nebst", "ihr", "den", "Schmerz", "zu", "fr\u00fch", "be\u00b7raub\u00b7ter", "Wey\u00b7sen", ";"], "token_info": ["word", "word", "