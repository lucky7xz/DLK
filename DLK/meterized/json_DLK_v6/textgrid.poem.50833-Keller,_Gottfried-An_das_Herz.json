{"textgrid.poem.50833": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An das Herz", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du nicht dich schlie\u00dfen,", "tokens": ["Willst", "du", "nicht", "dich", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Herz! du offnes Haus,", "tokens": ["Herz", "!", "du", "off\u00b7nes", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Worin Freund' und Feinde", "tokens": ["Wo\u00b7rin", "Freund'", "und", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "St\u00fcrmen ein und aus?", "tokens": ["St\u00fcr\u00b7men", "ein", "und", "aus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schau, wie sie verletzen", "tokens": ["Schau", ",", "wie", "sie", "ver\u00b7let\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dir das Hausrecht stets!", "tokens": ["Dir", "das", "Haus\u00b7recht", "stets", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchllos auf und nieder,", "tokens": ["F\u00fchl\u00b7los", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Polternd, l\u00e4rmend geht's!", "tokens": ["Pol\u00b7ternd", ",", "l\u00e4r\u00b7mend", "geht's", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Keiner putzt die Schuhe,", "tokens": ["Kei\u00b7ner", "putzt", "die", "Schu\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Keiner sieht sich um!", "tokens": ["Kei\u00b7ner", "sieht", "sich", "um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Staubig brechen alle", "tokens": ["Stau\u00b7big", "bre\u00b7chen", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dir ins Heiligtum;", "tokens": ["Dir", "ins", "Hei\u00b7lig\u00b7tum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Trinken aus den goldnen", "tokens": ["Trin\u00b7ken", "aus", "den", "gold\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kelchen des Altars,", "tokens": ["Kel\u00b7chen", "des", "Al\u00b7tars", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Stehlen M\u00fch und Segen", "tokens": ["Steh\u00b7len", "M\u00fch", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dir des ganzen Jahrs;", "tokens": ["Dir", "des", "gan\u00b7zen", "Jahrs", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Werfen die Penaten", "tokens": ["Wer\u00b7fen", "die", "Pe\u00b7na\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Wild vom Herde dir,", "tokens": ["Wild", "vom", "Her\u00b7de", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Pflanzen drauf mit Toben", "tokens": ["Pflan\u00b7zen", "drauf", "mit", "To\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PAV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr zerfetzt Panier;", "tokens": ["Ihr", "zer\u00b7fetzt", "Pa\u00b7nier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn zu verw\u00fcsten", "tokens": ["Und", "wenn", "zu", "ver\u00b7w\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie nichts finden mehr,", "tokens": ["Sie", "nichts", "fin\u00b7den", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lassen sie im Scheiden", "tokens": ["Las\u00b7sen", "sie", "im", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dich, mein Herz, so leer!", "tokens": ["Dich", ",", "mein", "Herz", ",", "so", "leer", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Nein! und wenn nun alles", "tokens": ["Nein", "!", "und", "wenn", "nun", "al\u00b7les"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KON", "KOUS", "ADV", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Still und tot in dir:", "tokens": ["Still", "und", "tot", "in", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Oh, noch halt dich offen,", "tokens": ["Oh", ",", "noch", "halt", "dich", "of\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Offen f\u00fcr und f\u00fcr!", "tokens": ["Of\u00b7fen", "f\u00fcr", "und", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "La\u00df die Sonne scheinen", "tokens": ["La\u00df", "die", "Son\u00b7ne", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hei\u00df in dich herein,", "tokens": ["Hei\u00df", "in", "dich", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "St\u00fcrme dich durchfahren", "tokens": ["St\u00fcr\u00b7me", "dich", "durch\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und den Wetterschein!", "tokens": ["Und", "den", "Wet\u00b7ter\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn durch deine Hallen", "tokens": ["Wenn", "durch", "dei\u00b7ne", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "So die Windsbraut zieht,", "tokens": ["So", "die", "Winds\u00b7braut", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "La\u00df aus deinen Glocken", "tokens": ["La\u00df", "aus", "dei\u00b7nen", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schallen Lied um Lied!", "tokens": ["Schal\u00b7len", "Lied", "um", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Denn noch kann's geschehen,", "tokens": ["Denn", "noch", "kann's", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Da\u00df auf irrer Flucht", "tokens": ["Da\u00df", "auf", "ir\u00b7rer", "Flucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eine treue Seele", "tokens": ["Ei\u00b7ne", "treu\u00b7e", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bei dir Obdach sucht.", "tokens": ["Bei", "dir", "Ob\u00b7dach", "sucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Dann ist's Zeit, zu schlie\u00dfen", "tokens": ["Dann", "ist's", "Zeit", ",", "zu", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Endlich T\u00fcr und Tor,", "tokens": ["End\u00b7lich", "T\u00fcr", "und", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dann bl\u00fch dir im Innern", "tokens": ["Dann", "bl\u00fch", "dir", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Neu der Lenz hervor!", "tokens": ["Neu", "der", "Lenz", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}