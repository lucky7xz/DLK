{"textgrid.poem.36140": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier an Deutschlands letzter Felsenspitze,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier an Deutschlands letzter Felsenspitze,", "tokens": ["Hier", "an", "Deutschlands", "letz\u00b7ter", "Fel\u00b7sen\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In der Hertha schauervollem Hain,", "tokens": ["In", "der", "Her\u00b7tha", "schau\u00b7er\u00b7vol\u00b7lem", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Voller Hehrgef\u00fchl, und Wand'rerhitze,", "tokens": ["Vol\u00b7ler", "Hehr\u00b7ge\u00b7f\u00fchl", ",", "und", "Wan\u00b7d'\u00b7rer\u00b7hit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Theures M\u00e4dchen, denk' ich dein.", "tokens": ["Theu\u00b7res", "M\u00e4d\u00b7chen", ",", "denk'", "ich", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Grabe hier in dieser Buchenrinde", "tokens": ["Gra\u00b7be", "hier", "in", "die\u00b7ser", "Bu\u00b7chen\u00b7rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deinen Namen ein \u2013 Bewahr' ihn mir", "tokens": ["Dei\u00b7nen", "Na\u00b7men", "ein", "\u2013", "Be\u00b7wahr'", "ihn", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "$(", "VVIMP", "PPER", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Treulich, heil'ge Buche. Leise Winde", "tokens": ["Treu\u00b7lich", ",", "heil'\u00b7ge", "Bu\u00b7che", ".", "Lei\u00b7se", "Win\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJA", "NN", "$.", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Flistern ewig \u00fcber dir!", "tokens": ["Flis\u00b7tern", "e\u00b7wig", "\u00fc\u00b7ber", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nachtigallen fl\u00f6ten in den Zweigen", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "fl\u00f6\u00b7ten", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wonna's Namen, und der Pilger steh',", "tokens": ["Won\u00b7na's", "Na\u00b7men", ",", "und", "der", "Pil\u00b7ger", "steh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lese dich mit ehrfurchtvollem Schweigen,", "tokens": ["Le\u00b7se", "dich", "mit", "ehr\u00b7furcht\u00b7vol\u00b7lem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fchle sich ger\u00fchrt und geh'.", "tokens": ["F\u00fch\u00b7le", "sich", "ge\u00b7r\u00fchrt", "und", "geh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, da\u00df einst der Edlen, die ich liebe,", "tokens": ["Ach", ",", "da\u00df", "einst", "der", "Ed\u00b7len", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einer, wenn auf Stubnitz Felsenh\u00f6h'", "tokens": ["Ei\u00b7ner", ",", "wenn", "auf", "Stub\u00b7nitz", "Fel\u00b7sen\u00b7h\u00f6h'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Neugier und Naturgef\u00fchl ihn triebe,", "tokens": ["Neu\u00b7gier", "und", "Na\u00b7tur\u00b7ge\u00b7f\u00fchl", "ihn", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Diesen theuren Namen s\u00e4h'.", "tokens": ["Die\u00b7sen", "theu\u00b7ren", "Na\u00b7men", "s\u00e4h'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Trauen w\u00fcrd' er kaum dem Auge, w\u00fcrde", "tokens": ["Trau\u00b7en", "w\u00fcrd'", "er", "kaum", "dem", "Au\u00b7ge", ",", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schnell noch einmal lesen, dann sich freu'n,", "tokens": ["Schnell", "noch", "ein\u00b7mal", "le\u00b7sen", ",", "dann", "sich", "freu'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "VVINF", "$,", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und vergessen seines Schicksals B\u00fcrde,", "tokens": ["Und", "ver\u00b7ges\u00b7sen", "sei\u00b7nes", "Schick\u00b7sals", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und den Freundschaftschwur erneu'n.", "tokens": ["Und", "den", "Freund\u00b7schaftsc\u00b7hwur", "er\u00b7neu'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcrchte aber \u2013 sieh! dem Greif zur Seiten", "tokens": ["F\u00fcrch\u00b7te", "a\u00b7ber", "\u2013", "sieh", "!", "dem", "Greif", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "VVIMP", "$.", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spann' ich Tauben \u2013 f\u00fchrte \u00fcbers Meer,", "tokens": ["Spann'", "ich", "Tau\u00b7ben", "\u2013", "f\u00fchr\u00b7te", "\u00fc\u00b7bers", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$(", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ueberall die freundelosen Weiten,", "tokens": ["Ue\u00b7be\u00b7rall", "die", "freun\u00b7de\u00b7lo\u00b7sen", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wonna selbst, die Liebe, her.", "tokens": ["Won\u00b7na", "selbst", ",", "die", "Lie\u00b7be", ",", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und sie s\u00e4h' auf dieser Felsenspitze", "tokens": ["Und", "sie", "s\u00e4h'", "auf", "die\u00b7ser", "Fel\u00b7sen\u00b7spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Buche ihren Namen bl\u00fch'n,", "tokens": ["In", "der", "Bu\u00b7che", "ih\u00b7ren", "Na\u00b7men", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O, wie w\u00fcrd' in schnell entbrannter Hitze", "tokens": ["O", ",", "wie", "w\u00fcrd'", "in", "schnell", "ent\u00b7brann\u00b7ter", "Hit\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VAFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wang' und Auge ihr ergl\u00fch'n!", "tokens": ["Wang'", "und", "Au\u00b7ge", "ihr", "er\u00b7gl\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lieben w\u00fcrde sie den Vielgetreuen,", "tokens": ["Lie\u00b7ben", "w\u00fcr\u00b7de", "sie", "den", "Viel\u00b7ge\u00b7treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oder, wenn ihr das ein Schicksal wehrt,", "tokens": ["O\u00b7der", ",", "wenn", "ihr", "das", "ein", "Schick\u00b7sal", "wehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird sie Thr\u00e4nen seiner Treue weihen,", "tokens": ["Wird", "sie", "Thr\u00e4\u00b7nen", "sei\u00b7ner", "Treu\u00b7e", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Thr\u00e4nen war er werth!!", "tokens": ["Und", "der", "Thr\u00e4\u00b7nen", "war", "er", "werth", "!!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}