{"textgrid.poem.34372": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wuchs empor, wie Weidenb\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wuchs empor, wie Weidenb\u00e4ume", "tokens": ["Ich", "wuchs", "em\u00b7por", ",", "wie", "Wei\u00b7den\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von manchem Nord geschlenkt", "tokens": ["Von", "man\u00b7chem", "Nord", "ge\u00b7schlenkt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr niedrig Haupt in lichte Wolken heben,", "tokens": ["Ihr", "nied\u00b7rig", "Haupt", "in", "lich\u00b7te", "Wol\u00b7ken", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn nun der Fr\u00fchling lacht.", "tokens": ["Wenn", "nun", "der", "Fr\u00fch\u00b7ling", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kroch empor wie das geschmeide Ephen", "tokens": ["Ich", "kroch", "em\u00b7por", "wie", "das", "ge\u00b7schmei\u00b7de", "E\u00b7phen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Schutt und Mauern Wege findt,", "tokens": ["Durch", "Schutt", "und", "Mau\u00b7ern", "We\u00b7ge", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An d\u00fcrren St\u00e4ben h\u00e4lt und h\u00f6her", "tokens": ["An", "d\u00fcr\u00b7ren", "St\u00e4\u00b7ben", "h\u00e4lt", "und", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Sie, zum Schutt an ihren F\u00fc\u00dfen", "tokens": ["Als", "Sie", ",", "zum", "Schutt", "an", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hinunter sieht.", "tokens": ["Hin\u00b7un\u00b7ter", "sieht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich flog empor, wie die Rakete", "tokens": ["Ich", "flog", "em\u00b7por", ",", "wie", "die", "Ra\u00b7ke\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verschlossen und vermacht, die Bande", "tokens": ["Ver\u00b7schlos\u00b7sen", "und", "ver\u00b7macht", ",", "die", "Ban\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zerrei\u00dft und schnell, sobald der Funken", "tokens": ["Zer\u00b7rei\u00dft", "und", "schnell", ",", "so\u00b7bald", "der", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie anger\u00fchrt, gen Himmel steigt.", "tokens": ["Sie", "an\u00b7ge\u00b7r\u00fchrt", ",", "gen", "Him\u00b7mel", "steigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich kletterte wie junge Gemsen,", "tokens": ["Ich", "klet\u00b7ter\u00b7te", "wie", "jun\u00b7ge", "Gem\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nun zuerst die Federkraft", "tokens": ["Die", "nun", "zu\u00b7erst", "die", "Fe\u00b7der\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Sehn'n und Muskeln f\u00fchlen, wenn sie", "tokens": ["In", "Sehn'n", "und", "Mus\u00b7keln", "f\u00fch\u00b7len", ",", "wenn", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVINF", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die steile H\u00f6h' erblicken, empor.", "tokens": ["Die", "stei\u00b7le", "H\u00f6h'", "er\u00b7bli\u00b7cken", ",", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Hier h\u00e4ng ich itzt aus Dunst und Wolken", "tokens": ["Hier", "h\u00e4ng", "ich", "itzt", "aus", "Dunst", "und", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dir furchtbare Tiefe, nieder \u2013", "tokens": ["Nach", "dir", "furcht\u00b7ba\u00b7re", "Tie\u00b7fe", ",", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,", "PTKVZ", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Giebts Engel hier? O komm ein Engel", "tokens": ["Giebts", "En\u00b7gel", "hier", "?", "O", "komm", "ein", "En\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "$.", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rette mich!", "tokens": ["Und", "ret\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "O wenn ich diesen Felsengang st\u00fcrzte,", "tokens": ["O", "wenn", "ich", "die\u00b7sen", "Fel\u00b7sen\u00b7gang", "st\u00fcrz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo w\u00e4r, ihr Engel Gottes! mein Ende?", "tokens": ["Wo", "w\u00e4r", ",", "ihr", "En\u00b7gel", "Got\u00b7tes", "!", "mein", "En\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PPOSAT", "NN", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo w\u00e4r ein Ende meiner Thr\u00e4nen", "tokens": ["Wo", "w\u00e4r", "ein", "En\u00b7de", "mei\u00b7ner", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um dich, um dich verlorne Demuth?", "tokens": ["Um", "dich", ",", "um", "dich", "ver\u00b7lor\u00b7ne", "De\u00b7muth", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "KOUI", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Dich der Christen und nur der Christen", "tokens": ["Dich", "der", "Chris\u00b7ten", "und", "nur", "der", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "KON", "ADV", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Einziger, allerh\u00f6chster Seegen", "tokens": ["Ein\u00b7zi\u00b7ger", ",", "al\u00b7ler\u00b7h\u00f6chs\u00b7ter", "See\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heiliger Balsam! der die Wunden", "tokens": ["Hei\u00b7li\u00b7ger", "Bal\u00b7sam", "!", "der", "die", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Des schwingeversengenden Stolzes heilt.", "tokens": ["Des", "schwin\u00b7ge\u00b7ver\u00b7sen\u00b7gen\u00b7den", "Stol\u00b7zes", "heilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Einzige Lindrung edler Gem\u00fcther,", "tokens": ["Ein\u00b7zi\u00b7ge", "Lind\u00b7rung", "ed\u00b7ler", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn in der trostlosen, hei\u00dfen, \u00f6den,", "tokens": ["Wenn", "in", "der", "trost\u00b7lo\u00b7sen", ",", "hei\u00b7\u00dfen", ",", "\u00f6\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "$,", "VVINF", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hei\u00dfen, \u00f6den, verzehrenden W\u00fcste", "tokens": ["Hei\u00b7\u00dfen", ",", "\u00f6\u00b7den", ",", "ver\u00b7zeh\u00b7ren\u00b7den", "W\u00fcs\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Eitler Ehre sie sich verirrt.", "tokens": ["Eit\u00b7ler", "Eh\u00b7re", "sie", "sich", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Wann sie schmachteten und nicht fanden", "tokens": ["Wann", "sie", "schmach\u00b7te\u00b7ten", "und", "nicht", "fan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PTKNEG", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo sie den Durst der H\u00f6lle stillten", "tokens": ["Wo", "sie", "den", "Durst", "der", "H\u00f6l\u00b7le", "still\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ihr Gebein verzehrte.", "tokens": ["Der", "ihr", "Ge\u00b7bein", "ver\u00b7zehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wann sie, verzweifelnd um Schatten, w\u00e4hlten", "tokens": ["Wann", "sie", ",", "ver\u00b7zwei\u00b7felnd", "um", "Schat\u00b7ten", ",", "w\u00e4hl\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "$,", "VVPP", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wege nach Morgen, nach Mittag, nach Abend", "tokens": ["We\u00b7ge", "nach", "Mor\u00b7gen", ",", "nach", "Mit\u00b7tag", ",", "nach", "A\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und nicht fanden, nicht fanden, nicht fanden", "tokens": ["Und", "nicht", "fan\u00b7den", ",", "nicht", "fan\u00b7den", ",", "nicht", "fan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wo ein Schatten sie k\u00fchlete.", "tokens": ["Wo", "ein", "Schat\u00b7ten", "sie", "k\u00fch\u00b7le\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Wenn sie auf unmitleidigen Sand hin-", "tokens": ["Wenn", "sie", "auf", "un\u00b7mit\u00b7lei\u00b7di\u00b7gen", "Sand", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ab sich st\u00fcrzten und strekten und weinten.", "tokens": ["ab", "sich", "st\u00fcrz\u00b7ten", "und", "strek\u00b7ten", "und", "wein\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ach die Thr\u00e4nen rolleten auf und nieder", "tokens": ["Ach", "die", "Thr\u00e4\u00b7nen", "rol\u00b7le\u00b7ten", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "So hei\u00df war der Sand.", "tokens": ["So", "hei\u00df", "war", "der", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Komm der Christen Erretter und Vater,", "tokens": ["Komm", "der", "Chris\u00b7ten", "Er\u00b7ret\u00b7ter", "und", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Komm du Gott in verachteter Bildung!", "tokens": ["Komm", "du", "Gott", "in", "ver\u00b7ach\u00b7te\u00b7ter", "Bil\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Komm und zeige der Demuth geheime", "tokens": ["Komm", "und", "zei\u00b7ge", "der", "De\u00b7muth", "ge\u00b7hei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Pfade mir an.", "tokens": ["Pfa\u00b7de", "mir", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "F\u00fchre mich weit und nieder hinunter", "tokens": ["F\u00fch\u00b7re", "mich", "weit", "und", "nie\u00b7der", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In ihre dunkeln Schattenthale", "tokens": ["In", "ih\u00b7re", "dun\u00b7keln", "Schat\u00b7tent\u00b7ha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Voll lebendiger springender Brunnen,", "tokens": ["Voll", "le\u00b7ben\u00b7di\u00b7ger", "sprin\u00b7gen\u00b7der", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wo die Einsamkeit oder die Freude", "tokens": ["Wo", "die", "Ein\u00b7sam\u00b7keit", "o\u00b7der", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Also lispelt:", "tokens": ["Al\u00b7so", "lis\u00b7pelt", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.14": {"line.1": {"text": "\u00bbkomm' ger\u00f6steter Laurentius", "tokens": ["\u00bb", "komm'", "ge\u00b7r\u00f6s\u00b7te\u00b7ter", "Lau\u00b7ren\u00b7ti\u00b7us"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADJA", "NN"], "meter": "+-+--+--+", "meas