{"dta.poem.10270": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Herblt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Astern bl&#252;hen schon im Garten,             ", "tokens": ["As\u00b7tern", "bl", "&#252;", "hen", "schon", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schw\u00e4cher trifft der Sonnenpfeil.", "tokens": ["Schw\u00e4\u00b7cher", "trifft", "der", "Son\u00b7nen\u00b7pfeil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen, die den Tod erwarten", "tokens": ["Blu\u00b7men", ",", "die", "den", "Tod", "er\u00b7war\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch des Frostes Henkerbeil.", "tokens": ["Durch", "des", "Fros\u00b7tes", "Hen\u00b7ker\u00b7beil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Brauner dunkelt l\u00e4ngst die Heide,", "tokens": ["Brau\u00b7ner", "dun\u00b7kelt", "l\u00e4ngst", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00e4tter zittern durch die Luft.", "tokens": ["Bl\u00e4t\u00b7ter", "zit\u00b7tern", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es liegen Wald und Weide", "tokens": ["Und", "es", "lie\u00b7gen", "Wald", "und", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbewegt in blauem Duft.", "tokens": ["Un\u00b7be\u00b7wegt", "in", "blau\u00b7em", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pfir sich an der Gartenmauer,", "tokens": ["Pfir", "sich", "an", "der", "Gar\u00b7ten\u00b7mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kranich auf der Winterflucht.", "tokens": ["Kra\u00b7nich", "auf", "der", "Win\u00b7ter\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herbstes Freuden, Herbstes Trauer,", "tokens": ["Herbs\u00b7tes", "Freu\u00b7den", ",", "Herbs\u00b7tes", "Trau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welke Rosen, reife Frucht.", "tokens": ["Wel\u00b7ke", "Ro\u00b7sen", ",", "rei\u00b7fe", "Frucht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}