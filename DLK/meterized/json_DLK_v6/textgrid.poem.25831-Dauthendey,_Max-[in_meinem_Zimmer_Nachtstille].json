{"textgrid.poem.25831": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[in meinem Zimmer Nachtstille]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Zimmer Nachtstille.", "tokens": ["In", "mei\u00b7nem", "Zim\u00b7mer", "Nachts\u00b7til\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Windegewimmer im Ofen.", "tokens": ["Win\u00b7de\u00b7ge\u00b7wim\u00b7mer", "im", "O\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wie seltene Orchideen", "tokens": ["Wie", "sel\u00b7te\u00b7ne", "Or\u00b7chi\u00b7de\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Stehen im schwarzen Nachtspiegel", "tokens": ["Ste\u00b7hen", "im", "schwar\u00b7zen", "Nacht\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.5": {"text": "Fremde, wei\u00dfe Gedanken,", "tokens": ["Frem\u00b7de", ",", "wei\u00b7\u00dfe", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Schwanken vornehm mit ihren Kronen.", "tokens": ["Schwan\u00b7ken", "vor\u00b7nehm", "mit", "ih\u00b7ren", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Getraue die Stille nicht zu brechen,", "tokens": ["Ge\u00b7trau\u00b7e", "die", "Stil\u00b7le", "nicht", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Engel k\u00f6nnten drinn wohnen.", "tokens": ["En\u00b7gel", "k\u00f6nn\u00b7ten", "drinn", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}