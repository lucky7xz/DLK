{"textgrid.poem.32852": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja Doris, du bist mir zu sch\u00f6n!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja Doris, du bist mir zu sch\u00f6n!", "tokens": ["Ja", "Do\u00b7ris", ",", "du", "bist", "mir", "zu", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "$,", "PPER", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Hier! willst du dich im Spiegel sehn?", "tokens": ["Hier", "!", "willst", "du", "dich", "im", "Spie\u00b7gel", "sehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieh! und sag mir ob ich wohl", "tokens": ["Da", "sieh", "!", "und", "sag", "mir", "ob", "ich", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich sehn, und dich nicht lieben soll?", "tokens": ["Dich", "sehn", ",", "und", "dich", "nicht", "lie\u00b7ben", "soll", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch Doris, nein: du bist zu sch\u00f6n!", "tokens": ["Doch", "Do\u00b7ris", ",", "nein", ":", "du", "bist", "zu", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PTKANT", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Spiegel darfst du selten sehn:", "tokens": ["In", "Spie\u00b7gel", "darfst", "du", "sel\u00b7ten", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht m\u00f6chtst du zu meiner Pein,", "tokens": ["Viel\u00b7leicht", "m\u00f6chtst", "du", "zu", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein weiblicher Narcissus seyn!", "tokens": ["Ein", "weib\u00b7li\u00b7cher", "Nar\u00b7cis\u00b7sus", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}