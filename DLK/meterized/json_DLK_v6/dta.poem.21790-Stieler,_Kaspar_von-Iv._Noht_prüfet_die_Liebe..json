{"dta.poem.21790": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Noht pr\u00fcfet die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Solt' ich den Tod nicht fr\u00f6lich leiden?", "tokens": ["Solt'", "ich", "den", "Tod", "nicht", "fr\u00f6\u00b7lich", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rosille weint ob meinen Scheiden/", "tokens": ["Ro\u00b7sil\u00b7le", "weint", "ob", "mei\u00b7nen", "Schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOUS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und in die fernen Felder kehrt.", "tokens": ["und", "in", "die", "fer\u00b7nen", "Fel\u00b7der", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Noht und Jammer sehen tr\u00fcbe:", "tokens": ["In", "Noht", "und", "Jam\u00b7mer", "se\u00b7hen", "tr\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "hieran erkennt man wahre Liebe.", "tokens": ["hie\u00b7ran", "er\u00b7kennt", "man", "wah\u00b7re", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die mit in Freuden lustig war/", "tokens": ["die", "mit", "in", "Freu\u00b7den", "lus\u00b7tig", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPR", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "traurt nu bey meines Bettes Bahr.", "tokens": ["traurt", "nu", "bey", "mei\u00b7nes", "Bet\u00b7tes", "Bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr Wolken-bruch der Tr\u00e4hnen-g\u00fcsse", "tokens": ["Ihr", "Wol\u00b7ken\u00b7bruch", "der", "Tr\u00e4h\u00b7nen\u00b7g\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "macht \u00fcber meinem K\u00f6rper Fl\u00fcsse/", "tokens": ["macht", "\u00fc\u00b7ber", "mei\u00b7nem", "K\u00f6r\u00b7per", "Fl\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dem K\u00f6rper/ der sein Bluht verl\u00e4\u00dft/", "tokens": ["dem", "K\u00f6r\u00b7per", "/", "der", "sein", "Bluht", "ver\u00b7l\u00e4\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und iezt dem Athem au\u00df sich bl\u00e4st.", "tokens": ["und", "iezt", "dem", "A\u00b7them", "au\u00df", "sich", "bl\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Lieb\u2019 ist schlecht und kaum zu nennen:", "tokens": ["Die", "Lieb'", "ist", "schlecht", "und", "kaum", "zu", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur lieben weil die Augeu brennen/", "tokens": ["Nur", "lie\u00b7ben", "weil", "die", "Au\u00b7geu", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "weil noch die Stirn ermuntert sieht", "tokens": ["weil", "noch", "die", "Stirn", "er\u00b7mun\u00b7tert", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und alles Rosenf\u00e4rbig bl\u00fcht.", "tokens": ["und", "al\u00b7les", "Ro\u00b7sen\u00b7f\u00e4r\u00b7big", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich lieg\u2019 allhier auff so viel Wochen/", "tokens": ["Ich", "lieg'", "all\u00b7hier", "auff", "so", "viel", "Wo\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "PIAT", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "mein Leib ist lauter d\u00fcrre Knochen/", "tokens": ["mein", "Leib", "ist", "lau\u00b7ter", "d\u00fcr\u00b7re", "Kno\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der Lippen Purpur bl\u00e4sset wei\u00df", "tokens": ["der", "Lip\u00b7pen", "Pur\u00b7pur", "bl\u00e4s\u00b7set", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der arme Band ist Todten-ey\u00df.", "tokens": ["der", "ar\u00b7me", "Band", "ist", "Tod\u00b7ten\u00b7ey\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich bin nicht mehr ein Mensch zu nennen", "tokens": ["Ich", "bin", "nicht", "mehr", "ein", "Mensch", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mich meiden alle/ die mich kennen/", "tokens": ["mich", "mei\u00b7den", "al\u00b7le", "/", "die", "mich", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$(", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rosille bleibt bey mir und wacht", "tokens": ["Ro\u00b7sil\u00b7le", "bleibt", "bey", "mir", "und", "wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Treu-Exempel! Gunst-gemerke", "tokens": ["O", "Treu\u00b7Exem\u00b7pel", "!", "Gunst\u00b7ge\u00b7mer\u00b7ke"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O Muster wahrer Liebes-Werke!", "tokens": ["O", "Mus\u00b7ter", "wah\u00b7rer", "Lie\u00b7bes\u00b7Wer\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rosill\u2019 h\u00e4lt bi\u00df zur lezten Noht", "tokens": ["Ro\u00b7sill'", "h\u00e4lt", "bi\u00df", "zur", "lez\u00b7ten", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und w\u00fcnscht vor mich ihr selbst den Todt.", "tokens": ["und", "w\u00fcnscht", "vor", "mich", "ihr", "selbst", "den", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie kan ich Freundin/ di\u00df vergelten/", "tokens": ["Wie", "kan", "ich", "Freun\u00b7din", "/", "di\u00df", "ver\u00b7gel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "$(", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "indehm ich folge den Gew\u00e4lten/", "tokens": ["in\u00b7dehm", "ich", "fol\u00b7ge", "den", "Ge\u00b7w\u00e4l\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die ieder Mensch vom Sternen-flu\u00df\u2019", "tokens": ["die", "ie\u00b7der", "Mensch", "vom", "Ster\u00b7nen\u00b7flu\u00df'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ohn allen Einspruch dulden mu\u00df.", "tokens": ["ohn", "al\u00b7len", "Ein\u00b7spruch", "dul\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich wil in deiner Seele leben/", "tokens": ["Ich", "wil", "in", "dei\u00b7ner", "See\u00b7le", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mein Schatten soll stets um dich schweben", "tokens": ["mein", "Schat\u00b7ten", "soll", "stets", "um", "dich", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "bi\u00df du auch au\u00df dem Leben f\u00e4hrst", "tokens": ["bi\u00df", "du", "auch", "au\u00df", "dem", "Le\u00b7ben", "f\u00e4hrst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und deine Seele mir gew\u00e4hrst.", "tokens": ["und", "dei\u00b7ne", "See\u00b7le", "mir", "ge\u00b7w\u00e4hrst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Indessen sollen diese Zeilen", "tokens": ["In\u00b7des\u00b7sen", "sol\u00b7len", "die\u00b7se", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "es soll di\u00df treue Zeuge-blatt", "tokens": ["es", "soll", "di\u00df", "treu\u00b7e", "Zeu\u00b7ge\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Nachwelt r\u00fchmen deine Taht.", "tokens": ["der", "Nach\u00b7welt", "r\u00fch\u00b7men", "dei\u00b7ne", "Taht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}