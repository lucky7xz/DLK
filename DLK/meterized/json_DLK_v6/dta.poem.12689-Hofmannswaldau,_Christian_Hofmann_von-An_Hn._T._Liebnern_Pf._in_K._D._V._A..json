{"dta.poem.12689": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Hn.   T.  Liebnern Pf. in K.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er liebt, und wird geliebt von seiner keuschen braut;", "tokens": ["Er", "liebt", ",", "und", "wird", "ge\u00b7liebt", "von", "sei\u00b7ner", "keu\u00b7schen", "braut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "VVPP", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er selber macht sie auch zu einer Liebnerinne,", "tokens": ["Er", "sel\u00b7ber", "macht", "sie", "auch", "zu", "ei\u00b7ner", "Lieb\u00b7ne\u00b7rin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So werden sie zugleich der liebe gr\u00fcndlich inne,", "tokens": ["So", "wer\u00b7den", "sie", "zu\u00b7gleich", "der", "lie\u00b7be", "gr\u00fcnd\u00b7lich", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil auf den namen hier die wercke sind gebaut.", "tokens": ["Weil", "auf", "den", "na\u00b7men", "hier", "die", "wer\u00b7cke", "sind", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}