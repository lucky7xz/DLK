{"textgrid.poem.65951": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der Barriere zum Halt gezwungen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Barriere zum Halt gezwungen", "tokens": ["An", "der", "Bar\u00b7rie\u00b7re", "zum", "Halt", "ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVPP"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Lief mein Blick l\u00e4ngs den Eisenschienen.", "tokens": ["Lief", "mein", "Blick", "l\u00e4ngs", "den", "Ei\u00b7sen\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Pustend und schnaubend aus feurigen Lungen,", "tokens": ["Pus\u00b7tend", "und", "schnau\u00b7bend", "aus", "feu\u00b7ri\u00b7gen", "Lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Raste der eiserne Renner heran.", "tokens": ["Ras\u00b7te", "der", "ei\u00b7ser\u00b7ne", "Ren\u00b7ner", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Funken schw\u00e4rmten gleich zornigen Bienen.", "tokens": ["Fun\u00b7ken", "schw\u00e4rm\u00b7ten", "gleich", "zor\u00b7ni\u00b7gen", "Bie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rasselnd folgte der Wagen dann", "tokens": ["Ras\u00b7selnd", "folg\u00b7te", "der", "Wa\u00b7gen", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Endlose Kette nach, wie der lange", "tokens": ["End\u00b7lo\u00b7se", "Ket\u00b7te", "nach", ",", "wie", "der", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$,", "PWAV", "ART", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "W\u00e4lzende Leib einer Riesenschlange.", "tokens": ["W\u00e4l\u00b7zen\u00b7de", "Leib", "ei\u00b7ner", "Rie\u00b7sen\u00b7schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wie der Zug so vor\u00fcbergesaust,", "tokens": ["Wie", "der", "Zug", "so", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7saust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Griff er ans Herz mir mit rascher Faust:", "tokens": ["Griff", "er", "ans", "Herz", "mir", "mit", "ra\u00b7scher", "Faust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Stehst hier und gaffst, komm mit, komm mit!", "tokens": ["Stehst", "hier", "und", "gaffst", ",", "komm", "mit", ",", "komm", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ans Ende der Welt sind nur drei Schritt.", "tokens": ["Bis", "ans", "En\u00b7de", "der", "Welt", "sind", "nur", "drei", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "VAFIN", "ADV", "CARD", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und ich sah ihn verschwinden, weit, weit,", "tokens": ["Und", "ich", "sah", "ihn", "ver\u00b7schwin\u00b7den", ",", "weit", ",", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Sah die Welt in lachender Herrlichkeit,", "tokens": ["Sah", "die", "Welt", "in", "la\u00b7chen\u00b7der", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Der Berge Kronen, der Th\u00e4ler Gr\u00fcn,", "tokens": ["Der", "Ber\u00b7ge", "Kro\u00b7nen", ",", "der", "Th\u00e4\u00b7ler", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Versteckte D\u00f6rfer, die Felder im Bl\u00fchn.", "tokens": ["Ver\u00b7steck\u00b7te", "D\u00f6r\u00b7fer", ",", "die", "Fel\u00b7der", "im", "Bl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Sah St\u00e4dte und Str\u00f6me in sausendem Flug,", "tokens": ["Sah", "St\u00e4d\u00b7te", "und", "Str\u00f6\u00b7me", "in", "sau\u00b7sen\u00b7dem", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Bis des Oceans Atem entgegen mir schlug.", "tokens": ["Bis", "des", "O\u00b7ceans", "A\u00b7tem", "ent\u00b7ge\u00b7gen", "mir", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und das Herz ward mir weit, und das Herz ward mir weit!", "tokens": ["Und", "das", "Herz", "ward", "mir", "weit", ",", "und", "das", "Herz", "ward", "mir", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Auffahrend streckt ich im Sehnsuchtsdrang", "tokens": ["Auf\u00b7fah\u00b7rend", "streckt", "ich", "im", "Sehn\u00b7suchts\u00b7drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die Arme nach dem entrollenden Klang", "tokens": ["Die", "Ar\u00b7me", "nach", "dem", "ent\u00b7rol\u00b7len\u00b7den", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Des L\u00e4nderl\u00e4ufers im Eisenkleid.", "tokens": ["Des", "L\u00e4n\u00b7der\u00b7l\u00e4u\u00b7fers", "im", "Ei\u00b7sen\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Da \u00e4chzt und kr\u00e4chzt die Barriere empor,", "tokens": ["Da", "\u00e4chzt", "und", "kr\u00e4chzt", "die", "Bar\u00b7rie\u00b7re", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und der bis ans Ende der Welt sich verlor,", "tokens": ["Und", "der", "bis", "ans", "En\u00b7de", "der", "Welt", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Findet sofort mit gelindem Schreck", "tokens": ["Fin\u00b7det", "so\u00b7fort", "mit", "ge\u00b7lin\u00b7dem", "Schreck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.18": {"text": "Sich wieder auf dem alten Fleck.", "tokens": ["Sich", "wie\u00b7der", "auf", "dem", "al\u00b7ten", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sieht auf der andern Seite der Schienen", "tokens": ["Sieht", "auf", "der", "an\u00b7dern", "Sei\u00b7te", "der", "Schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Ein blondes Kind mit Unschuldsmienen,", "tokens": ["Ein", "blon\u00b7des", "Kind", "mit", "Un\u00b7schulds\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ein menschgewordenes Sonnenstrahlchen,", "tokens": ["Ein", "menschge\u00b7wor\u00b7de\u00b7nes", "Son\u00b7nen\u00b7strahl\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Irgend ein Mienchen oder ein Malchen.", "tokens": ["Ir\u00b7gend", "ein", "Mien\u00b7chen", "o\u00b7der", "ein", "Mal\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Das lacht mit hellen Augen heraus", "tokens": ["Das", "lacht", "mit", "hel\u00b7len", "Au\u00b7gen", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Aus dem modischen Hut, gro\u00df wie ein Haus.", "tokens": ["Aus", "dem", "mo\u00b7di\u00b7schen", "Hut", ",", "gro\u00df", "wie", "ein", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Trippelt die Kleine \u00fcbers Geleise,", "tokens": ["Trip\u00b7pelt", "die", "Klei\u00b7ne", "\u00fc\u00b7bers", "Ge\u00b7lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Streif' ich das Kleid ihr zufallsweise,", "tokens": ["Streif'", "ich", "das", "Kleid", "ihr", "zu\u00b7falls\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Seh' ihr ins Auge so obenhin,", "tokens": ["Seh'", "ihr", "ins", "Au\u00b7ge", "so", "o\u00b7ben\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Lacht eine ganze Welt darin.", "tokens": ["Lacht", "ei\u00b7ne", "gan\u00b7ze", "Welt", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Lange noch nach dem reizenden Kind", "tokens": ["Lan\u00b7ge", "noch", "nach", "dem", "rei\u00b7zen\u00b7den", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sah ich mir fast die Augen blind,", "tokens": ["Sah", "ich", "mir", "fast", "die", "Au\u00b7gen", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Brach mir vom n\u00e4chsten Busch einen Raub,", "tokens": ["Brach", "mir", "vom", "n\u00e4chs\u00b7ten", "Busch", "ei\u00b7nen", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ein Zweiglein mit erstem Fr\u00fchlingslaub.", "tokens": ["Ein", "Zwei\u00b7glein", "mit", "ers\u00b7tem", "Fr\u00fch\u00b7lings\u00b7laub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sorgsam barg ich's im Taschenbuch. Oft", "tokens": ["Sorg\u00b7sam", "barg", "ich's", "im", "Ta\u00b7schen\u00b7buch", ".", "Oft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PIS", "APPRART", "NN", "$.", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Soll's mich erinnern, wie unverhofft", "tokens": ["Soll's", "mich", "e\u00b7rin\u00b7nern", ",", "wie", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PRF", "VVINF", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sich das Dirnlein ein Herz einfing,", "tokens": ["Sich", "das", "Dirn\u00b7lein", "ein", "Herz", "ein\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das schon auf Reiseschuhen ging.", "tokens": ["Das", "schon", "auf", "Rei\u00b7se\u00b7schu\u00b7hen", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}