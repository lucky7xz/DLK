{"textgrid.poem.27692": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hornutus las: Was Gott Job habe weggenommen,", "tokens": ["Hor\u00b7nu\u00b7tus", "las", ":", "Was", "Gott", "Job", "ha\u00b7be", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWS", "NN", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sey duppelt ihm hernach zu Hause wieder kummen.", "tokens": ["Sey", "dup\u00b7pelt", "ihm", "her\u00b7nach", "zu", "Hau\u00b7se", "wie\u00b7der", "kum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie gut, sprach er, war di\u00df, da\u00df Gott sein Weib nicht nam,", "tokens": ["Wie", "gut", ",", "sprach", "er", ",", "war", "di\u00df", ",", "da\u00df", "Gott", "sein", "Weib", "nicht", "nam", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PDS", "$,", "KOUS", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Auf da\u00df Job ihrer zwey f\u00fcr eine nicht bekam!", "tokens": ["Auf", "da\u00df", "Job", "ih\u00b7rer", "zwey", "f\u00fcr", "ei\u00b7ne", "nicht", "be\u00b7kam", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "NE", "PPO