{"textgrid.poem.28612": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann das Weib ihr einen Mann, wann der Mann ein Weib ihm nimmt,", "tokens": ["Wann", "das", "Weib", "ihr", "ei\u00b7nen", "Mann", ",", "wann", "der", "Mann", "ein", "Weib", "ihm", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Weil sie beyde nemen so, wer dann ist, der was bek\u00fcmmt?", "tokens": ["Weil", "sie", "bey\u00b7de", "ne\u00b7men", "so", ",", "wer", "dann", "ist", ",", "der", "was", "be\u00b7k\u00fcmmt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "ADV", "$,", "PWS", "ADV", "VAFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ey, das Weib! dann die empf\u00e4ngt, tr\u00e4get B\u00fcrden ohne Scheue,", "tokens": ["Ey", ",", "das", "Weib", "!", "dann", "die", "emp\u00b7f\u00e4ngt", ",", "tr\u00e4\u00b7get", "B\u00fcr\u00b7den", "oh\u00b7ne", "Scheu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$.", "ADV", "ART", "VVFIN", "$,", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Leget abe, kummet wieder, holet mehr und tr\u00e4gt auffs neue.", "tokens": ["Le\u00b7get", "a\u00b7be", ",", "kum\u00b7met", "wie\u00b7der", ",", "ho\u00b7let", "mehr", "und", "tr\u00e4gt", "auffs", "neu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "KON", "VVFIN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}