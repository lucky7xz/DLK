{"textgrid.poem.54220": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du weltber\u00fchmter B \u2013 \u2013 \u2013 verachte nicht die Zeilen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du weltber\u00fchmter B \u2013 \u2013 \u2013 verachte nicht die Zeilen,", "tokens": ["Du", "welt\u00b7be\u00b7r\u00fchm\u00b7ter", "B", "\u2013", "\u2013", "\u2013", "ver\u00b7ach\u00b7te", "nicht", "die", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "XY", "$(", "$(", "$(", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aus besondern Trieb zu deinen Schwellen eilen,", "tokens": ["Die", "aus", "be\u00b7son\u00b7dern", "Trieb", "zu", "dei\u00b7nen", "Schwel\u00b7len", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df es allzuwohl, da\u00df meine Dichterey", "tokens": ["Ich", "wei\u00df", "es", "all\u00b7zu\u00b7wohl", ",", "da\u00df", "mei\u00b7ne", "Dich\u00b7te\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur noch ein Kinderspiel mit Recht zu nennen sey,", "tokens": ["Nur", "noch", "ein", "Kin\u00b7der\u00b7spiel", "mit", "Recht", "zu", "nen\u00b7nen", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob gleich dein andrer Theil vom irrdischen Vergn\u00fcgen,", "tokens": ["Ob", "gleich", "dein", "an\u00b7drer", "Theil", "vom", "irr\u00b7di\u00b7schen", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch meinen Nahmen mit will zu den andern f\u00fcgen.", "tokens": ["Auch", "mei\u00b7nen", "Nah\u00b7men", "mit", "will", "zu", "den", "an\u00b7dern", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "VMFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich wei\u00df nicht, ", "tokens": ["Ich", "wei\u00df", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Da\u00df deine Schmeicheley von mir ein Lob-Lied macht,", "tokens": ["Da\u00df", "dei\u00b7ne", "Schmei\u00b7che\u00b7ley", "von", "mir", "ein", "Lob\u00b7Lied", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich kenne dich gar wohl, den Werth, und deine Gaben,", "tokens": ["Ich", "ken\u00b7ne", "dich", "gar", "wohl", ",", "den", "Werth", ",", "und", "dei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Rang, und Vorzug wohl, vor vielen andern haben;", "tokens": ["Die", "Rang", ",", "und", "Vor\u00b7zug", "wohl", ",", "vor", "vie\u00b7len", "an\u00b7dern", "ha\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "ADV", "$,", "APPR", "PIAT", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn gantz Germanien stimmt mit mir \u00fcberein,", "tokens": ["Denn", "gantz", "Ger\u00b7ma\u00b7ni\u00b7en", "stimmt", "mit", "mir", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Da\u00df deines gleichen nicht leicht wird zu finden seyn;", "tokens": ["Da\u00df", "dei\u00b7nes", "glei\u00b7chen", "nicht", "leicht", "wird", "zu", "fin\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PTKNEG", "ADJD", "VAFIN", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kein Mahler kan von dir uns rechten Abri\u00df geben,", "tokens": ["Kein", "Mah\u00b7ler", "kan", "von", "dir", "uns", "rech\u00b7ten", "Ab\u00b7ri\u00df", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPER", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kein Pinsel schiltert dich nach Aehnlichkeit und Leben,", "tokens": ["Kein", "Pin\u00b7sel", "schil\u00b7tert", "dich", "nach", "A\u00b7ehn\u00b7lich\u00b7keit", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Dieweil dein hoher Geist gar nicht zu treffen ist,", "tokens": ["Die\u00b7weil", "dein", "ho\u00b7her", "Geist", "gar", "nicht", "zu", "tref\u00b7fen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und man was herrliches aus deinem Wesen list.", "tokens": ["Und", "man", "was", "herr\u00b7li\u00b7ches", "aus", "dei\u00b7nem", "We\u00b7sen", "list", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein Umgang heist uns schon ein Himmel auf der Erden,", "tokens": ["Dein", "Um\u00b7gang", "heist", "uns", "schon", "ein", "Him\u00b7mel", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich selber wolte gleich ein Eremite werden,", "tokens": ["Ich", "sel\u00b7ber", "wol\u00b7te", "gleich", "ein", "E\u00b7re\u00b7mi\u00b7te", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Tr\u00e4ff ich dich, ", "tokens": ["Tr\u00e4ff", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "So hie\u00df mir selbige durch dich ein Canaan.", "tokens": ["So", "hie\u00df", "mir", "sel\u00b7bi\u00b7ge", "durch", "dich", "ein", "Ca\u00b7naan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "APPR", "PPER", "ART", "NE", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Dein Argwohn wird dich zwar auf die Gedancken treiben,", "tokens": ["Dein", "Arg\u00b7wohn", "wird", "dich", "zwar", "auf", "die", "Ge\u00b7dan\u00b7cken", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Ob sucht ich alles dis aus Danckbarkeit zu schreiben;", "tokens": ["Ob", "sucht", "ich", "al\u00b7les", "dis", "aus", "Dan\u00b7ck\u00b7bar\u00b7keit", "zu", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIS", "PDS", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "O nein, dein Ruff und Ruhm, der bi\u00df zum Sternen steigt,", "tokens": ["O", "nein", ",", "dein", "Ruff", "und", "Ruhm", ",", "der", "bi\u00df", "zum", "Ster\u00b7nen", "steigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "PRELS", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Macht, da\u00df sich hier mein Kiel vor deiner Muse neigt,", "tokens": ["Macht", ",", "da\u00df", "sich", "hier", "mein", "Kiel", "vor", "dei\u00b7ner", "Mu\u00b7se", "neigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PRF", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.25": {"text": "Du bist in meiner Gunst schon l\u00e4ngstens angeschrieben,", "tokens": ["Du", "bist", "in", "mei\u00b7ner", "Gunst", "schon", "l\u00e4ngs\u00b7tens", "an\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn alle Welt mu\u00df dich und deine Feder lieben.", "tokens": ["Denn", "al\u00b7le", "Welt", "mu\u00df", "dich", "und", "dei\u00b7ne", "Fe\u00b7der", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nimm ", "tokens": ["Nimm"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Weil die von Z--- sonst nichts zur Vergeltung hat.", "tokens": ["Weil", "die", "von", "Z\u00b7", "sonst", "nichts", "zur", "Ver\u00b7gel\u00b7tung", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "TRUNC", "ADV", "PIS", "APPRART", "NN", "VAFIN", "$."], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.29": {"text": "La\u00df mich noch fernerweit die Gunst von dir besitzen,", "tokens": ["La\u00df", "mich", "noch", "fer\u00b7ner\u00b7weit", "die", "Gunst", "von", "dir", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und mich, mein Socrates, zu deinen F\u00fcssen sitzen;", "tokens": ["Und", "mich", ",", "mein", "So\u00b7cra\u00b7tes", ",", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "sit\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Ich nehme Lehren an, dieweil du selber weist,", "tokens": ["Ich", "neh\u00b7me", "Leh\u00b7ren", "an", ",", "die\u00b7weil", "du", "sel\u00b7ber", "weist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df alles Frauen-Volck ein schwaches Werckzeug heist.", "tokens": ["Da\u00df", "al\u00b7les", "Frau\u00b7en\u00b7Volck", "ein", "schwa\u00b7ches", "Wer\u00b7ck\u00b7zeug", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich schliesse meinen Brief, doch nicht mein Angedencken,", "tokens": ["Ich", "schlies\u00b7se", "mei\u00b7nen", "Brief", ",", "doch", "nicht", "mein", "An\u00b7ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn dieses werd ich dir, so lang ich lebe, schencken,", "tokens": ["Denn", "die\u00b7ses", "werd", "ich", "dir", ",", "so", "lang", "ich", "le\u00b7be", ",", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Versichre dich von mir, da\u00df ich best\u00e4ndig bin,", "tokens": ["Ver\u00b7sich\u00b7re", "dich", "von", "mir", ",", "da\u00df", "ich", "be\u00b7st\u00e4n\u00b7dig", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die dich bewundernde, verpflichte Z---.", "tokens": ["Die", "dich", "be\u00b7wun\u00b7dern\u00b7de", ",", "ver\u00b7pflich\u00b7te", "Z\u00b7", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJA", "TRUNC", "$."], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}}}}}