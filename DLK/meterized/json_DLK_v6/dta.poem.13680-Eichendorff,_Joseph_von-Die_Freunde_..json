{"dta.poem.13680": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Kein ird'scher Laut mehr reichte durch die L\u00fcfte,", "tokens": ["Kein", "ird'\u00b7scher", "Laut", "mehr", "reich\u00b7te", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPR", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir war's, als st\u00e4nde ich mit Gott alleine,", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "st\u00e4n\u00b7de", "ich", "mit", "Gott", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "So einsam, weit und sternhell war's da oben.", "tokens": ["So", "ein\u00b7sam", ",", "weit", "und", "stern\u00b7hell", "wa\u00b7r's", "da", "o\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}