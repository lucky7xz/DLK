{"textgrid.poem.43145": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freilich sah vorbei ich fluthen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freilich sah vorbei ich fluthen", "tokens": ["Frei\u00b7lich", "sah", "vor\u00b7bei", "ich", "flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All' die jammervollen Stunden,", "tokens": ["All'", "die", "jam\u00b7mer\u00b7vol\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freilich sind die alten Schmerzen", "tokens": ["Frei\u00b7lich", "sind", "die", "al\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durchgek\u00e4mpft und \u00fcberwunden!", "tokens": ["Durch\u00b7ge\u00b7k\u00e4mpft", "und", "\u00fc\u00b7berw\u00b7un\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freilich hab' vor Euren Herzen", "tokens": ["Frei\u00b7lich", "hab'", "vor", "Eu\u00b7ren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich Vergebung nun gefunden \u2013", "tokens": ["Ich", "Ver\u00b7ge\u00b7bung", "nun", "ge\u00b7fun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber ich mu\u00df doch verbluten,", "tokens": ["A\u00b7ber", "ich", "mu\u00df", "doch", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Schmerzlos an den alten Wunden!", "tokens": ["Schmerz\u00b7los", "an", "den", "al\u00b7ten", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}