{"textgrid.poem.53851": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Idyll an der Leine", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle M\u00e4dchen erwarten wen,", "tokens": ["Al\u00b7le", "M\u00e4d\u00b7chen", "er\u00b7war\u00b7ten", "wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "wenn die B\u00e4ume in Bl\u00fcte stehn;", "tokens": ["wenn", "die", "B\u00e4u\u00b7me", "in", "Bl\u00fc\u00b7te", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "wir m\u00fcssen immer n\u00e4hn und n\u00e4hn,", "tokens": ["wir", "m\u00fcs\u00b7sen", "im\u00b7mer", "n\u00e4hn", "und", "n\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bis uns die Augen brennen.", "tokens": ["bis", "uns", "die", "Au\u00b7gen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}