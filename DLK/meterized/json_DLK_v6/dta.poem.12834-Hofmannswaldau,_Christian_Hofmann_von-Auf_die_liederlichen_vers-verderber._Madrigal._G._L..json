{"dta.poem.12834": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die liederlichen vers-verderber.  \n Madrigal.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr ungestimmten fl\u00f6ten", "tokens": ["Ihr", "un\u00b7ge\u00b7stimm\u00b7ten", "fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verhungerter poeten!", "tokens": ["Ver\u00b7hun\u00b7ger\u00b7ter", "po\u00b7e\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pfeifft vor ein maas verdorbnes bier", "tokens": ["Pfeifft", "vor", "ein", "maas", "ver\u00b7dorb\u00b7nes", "bier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der welt verwegne poffen f\u00fcr.", "tokens": ["Der", "welt", "ver\u00b7weg\u00b7ne", "pof\u00b7fen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geht ohngefehr dem dorff ein richter ab,", "tokens": ["Geht", "ohn\u00b7ge\u00b7fehr", "dem", "dorff", "ein", "rich\u00b7ter", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie foltert ihr den kopff durch tieffes sinnen,", "tokens": ["Wie", "fol\u00b7tert", "ihr", "den", "kopff", "durch", "tief\u00b7fes", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und seyd bem\u00fcht bey dessen grab", "tokens": ["Und", "seyd", "be\u00b7m\u00fcht", "bey", "des\u00b7sen", "grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch einen reim ein tag-lohn zu gewinnen.", "tokens": ["Durch", "ei\u00b7nen", "reim", "ein", "tag\u00b7lohn", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Vor kleines geld verkaufft man gro\u00dfe l\u00fcgen,", "tokens": ["Vor", "klei\u00b7nes", "geld", "ver\u00b7kaufft", "man", "gro\u00b7\u00dfe", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die stein und eisen \u00fcberwiegen.", "tokens": ["Die", "stein", "und", "ei\u00b7sen", "\u00fc\u00b7berw\u00b7ie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Schlaf aus, du tr\u00e4umender poet!", "tokens": ["Schlaf", "aus", ",", "du", "tr\u00e4u\u00b7men\u00b7der", "poet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.12": {"text": "Suchst du die todten aufzuwecken,", "tokens": ["Suchst", "du", "die", "tod\u00b7ten", "auf\u00b7zu\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So must du selbst nach geist und leben schmecken.", "tokens": ["So", "must", "du", "selbst", "nach", "geist", "und", "le\u00b7ben", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}