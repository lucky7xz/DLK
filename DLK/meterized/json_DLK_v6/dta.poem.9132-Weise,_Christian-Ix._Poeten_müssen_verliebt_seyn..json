{"dta.poem.9132": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Poeten m\u00fcssen verliebt seyn.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Sprecht mich nicht weiter an", "tokens": ["Sprecht", "mich", "nicht", "wei\u00b7ter", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Um ein verliebtes lied/", "tokens": ["Um", "ein", "ver\u00b7lieb\u00b7tes", "lied", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn ich bin ausgethan", "tokens": ["Denn", "ich", "bin", "aus\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo lust und liebe bl\u00fcht/", "tokens": ["Wo", "lust", "und", "lie\u00b7be", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das gras ist abgemeyht/", "tokens": ["Das", "gras", "ist", "ab\u00b7ge\u00b7meyht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die rosen sind vergangen/", "tokens": ["Die", "ro\u00b7sen", "sind", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der winter f\u00fchrt das leid", "tokens": ["Der", "win\u00b7ter", "f\u00fchrt", "das", "leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und hat sich angefangen.", "tokens": ["Und", "hat", "sich", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Ich f\u00fchle keine lust", "tokens": ["Ich", "f\u00fch\u00b7le", "kei\u00b7ne", "lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die mich zum versen treibt/", "tokens": ["Die", "mich", "zum", "ver\u00b7sen", "treibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil meine kalte brust", "tokens": ["Weil", "mei\u00b7ne", "kal\u00b7te", "brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Unangefochten bleibt:", "tokens": ["Un\u00b7an\u00b7ge\u00b7foch\u00b7ten", "bleibt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das harte silber fleust", "tokens": ["Das", "har\u00b7te", "sil\u00b7ber", "fleust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur bey der grossen hitze/", "tokens": ["Nur", "bey", "der", "gros\u00b7sen", "hit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und der poeten geist", "tokens": ["Und", "der", "po\u00b7e\u00b7ten", "geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wird nur im lieben n\u00fctze.", "tokens": ["Wird", "nur", "im", "lie\u00b7ben", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Wie kan ich itzt betr\u00fcbt", "tokens": ["Wie", "kan", "ich", "itzt", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und wieder fr\u00f6lich seyn/", "tokens": ["Und", "wie\u00b7der", "fr\u00f6\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dem mir nichts beliebt", "tokens": ["In", "dem", "mir", "nichts", "be\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PIS", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von anmuth oder pein/", "tokens": ["Von", "an\u00b7muth", "o\u00b7der", "pein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Soll mein erfrornes hertz", "tokens": ["Soll", "mein", "er\u00b7fror\u00b7nes", "hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von glut und flammen singen.", "tokens": ["Von", "glut", "und", "flam\u00b7men", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und soll der kalte schertz", "tokens": ["Und", "soll", "der", "kal\u00b7te", "schertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die spr\u00f6de feder zwingen.", "tokens": ["Die", "spr\u00f6\u00b7de", "fe\u00b7der", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Ach nein die aloe/", "tokens": ["Ach", "nein", "die", "a\u00b7loe", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "ART", "NE", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der zucker und zibeth/", "tokens": ["Der", "zu\u00b7cker", "und", "zi\u00b7beth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Macht weder wol noch weh.", "tokens": ["Macht", "we\u00b7der", "wol", "noch", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann der geschmack vergeht:", "tokens": ["Wann", "der", "ge\u00b7schmack", "ver\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man mu\u00df die eitelkeit", "tokens": ["Man", "mu\u00df", "die", "ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der liebe noch ertragen/", "tokens": ["Der", "lie\u00b7be", "noch", "er\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Will man von freud und leid", "tokens": ["Will", "man", "von", "freud", "und", "leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "NE", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gereimte reime sagen.", "tokens": ["Ge\u00b7reim\u00b7te", "rei\u00b7me", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Der ist f\u00fcrwar nicht klug/", "tokens": ["Der", "ist", "f\u00fcr\u00b7war", "nicht", "klug", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der ohn ein seitenspiel/", "tokens": ["Der", "ohn", "ein", "sei\u00b7ten\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch einen selbstbetrug/", "tokens": ["Durch", "ei\u00b7nen", "selbst\u00b7be\u00b7trug", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verschwiegen tantzen will/", "tokens": ["Ver\u00b7schwie\u00b7gen", "tant\u00b7zen", "will", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und so wird mein gedicht", "tokens": ["Und", "so", "wird", "mein", "ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein schlechtes urtheil f\u00fchlen/", "tokens": ["Ein", "schlech\u00b7tes", "ur\u00b7theil", "f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo die begierden nicht", "tokens": ["Wo", "die", "be\u00b7gier\u00b7den", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die sarabande spielen.", "tokens": ["Die", "sa\u00b7ra\u00b7ban\u00b7de", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. Geh zarte Poesie/", "tokens": ["Geh", "zar\u00b7te", "Poe\u00b7sie", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du bleibst mir unbewust/", "tokens": ["Du", "bleibst", "mir", "un\u00b7be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh meine s\u00fcsse m\u00fch/", "tokens": ["Geh", "mei\u00b7ne", "s\u00fcs\u00b7se", "m\u00fch", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt meine saure lust/", "tokens": ["Jetzt", "mei\u00b7ne", "sau\u00b7re", "lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schreibe/ was ich kan/", "tokens": ["Ich", "schrei\u00b7be", "/", "was", "ich", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jhr aber meine br\u00fcder/", "tokens": ["Ihr", "a\u00b7ber", "mei\u00b7ne", "br\u00fc\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sprecht mich nicht weiter an", "tokens": ["Sprecht", "mich", "nicht", "wei\u00b7ter", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Um schertz und liebes-lieder.", "tokens": ["Um", "schertz", "und", "lie\u00b7bes\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}