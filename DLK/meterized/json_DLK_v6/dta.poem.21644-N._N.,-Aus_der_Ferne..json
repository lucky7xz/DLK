{"dta.poem.21644": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Ferne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es athmen und \u00e4chzen Millionen Menschen", "tokens": ["Es", "ath\u00b7men", "und", "\u00e4ch\u00b7zen", "Mil\u00b7lion\u00b7en", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "ADJA", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Leben und lieben in bl\u00fchenden Landen,", "tokens": ["Le\u00b7ben", "und", "lie\u00b7ben", "in", "bl\u00fc\u00b7hen\u00b7den", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und es dr\u00e4ngt sich im Weiten w\u00fchlend", "tokens": ["Und", "es", "dr\u00e4ngt", "sich", "im", "Wei\u00b7ten", "w\u00fch\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN", "VVPP"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Strom des Schaffens und strotzender Urkraft,", "tokens": ["Der", "Strom", "des", "Schaf\u00b7fens", "und", "strot\u00b7zen\u00b7der", "Ur\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Endlose Ebenen, W\u00fcsten und W\u00e4lder,", "tokens": ["End\u00b7lo\u00b7se", "E\u00b7be\u00b7nen", ",", "W\u00fcs\u00b7ten", "und", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "St\u00e4dte und Burgen auf st\u00fcrmischen Bergen", "tokens": ["St\u00e4d\u00b7te", "und", "Bur\u00b7gen", "auf", "st\u00fcr\u00b7mi\u00b7schen", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Trennen die treuen,", "tokens": ["Tren\u00b7nen", "die", "treu\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sehnenden Seelen.", "tokens": ["Seh\u00b7nen\u00b7den", "See\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie am Meere verwundert der Wandrer", "tokens": ["Wie", "am", "Mee\u00b7re", "ver\u00b7wun\u00b7dert", "der", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Steht und staunend die Wellen anstarrt,", "tokens": ["Steht", "und", "stau\u00b7nend", "die", "Wel\u00b7len", "an\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Anfang nirgends noch Ende findet,", "tokens": ["An\u00b7fang", "nir\u00b7gends", "noch", "En\u00b7de", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Irrenden Blickes unendlich einsam", "tokens": ["Ir\u00b7ren\u00b7den", "Bli\u00b7ckes", "un\u00b7end\u00b7lich", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "ADJD"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Seine Seele versenkt in Wehmuth:", "tokens": ["Sei\u00b7ne", "See\u00b7le", "ver\u00b7senkt", "in", "Weh\u00b7muth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Also seh ich einsam versinken", "tokens": ["Al\u00b7so", "seh", "ich", "ein\u00b7sam", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Meine Seele im weiten Weltraum,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "im", "wei\u00b7ten", "Welt\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Denk\u2019 ich die Massen m\u00fchseliger Menschen,", "tokens": ["Denk'", "ich", "die", "Mas\u00b7sen", "m\u00fch\u00b7se\u00b7li\u00b7ger", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die sich dr\u00e4ngen in Lust und Drangsal,", "tokens": ["Die", "sich", "dr\u00e4n\u00b7gen", "in", "Lust", "und", "Dran\u00b7gsal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "In Vergessenheit ewig gehen,", "tokens": ["In", "Ver\u00b7ges\u00b7sen\u00b7heit", "e\u00b7wig", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Wenn anathmend der Tod sie umarmet.", "tokens": ["Wenn", "a\u00b7nath\u00b7mend", "der", "Tod", "sie", "um\u00b7ar\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Deine Seele nur singt mein Seufzen,", "tokens": ["Dei\u00b7ne", "See\u00b7le", "nur", "singt", "mein", "Seuf\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deine Seele nur sucht meine Sehnsucht,", "tokens": ["Dei\u00b7ne", "See\u00b7le", "nur", "sucht", "mei\u00b7ne", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zieht zu dir nach der holden Heimath,", "tokens": ["Zieht", "zu", "dir", "nach", "der", "hol\u00b7den", "Hei\u00b7math", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du heiliges Weib, das ich heilig liebe.", "tokens": ["Du", "hei\u00b7li\u00b7ges", "Weib", ",", "das", "ich", "hei\u00b7lig", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wie der einsame Stern erstrahlet", "tokens": ["Wie", "der", "ein\u00b7sa\u00b7me", "Stern", "er\u00b7strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dem Wanderer Weisung winket", "tokens": ["Und", "dem", "Wan\u00b7de\u00b7rer", "Wei\u00b7sung", "win\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Abends, ehe die Nacht sich \u00f6ffnet,", "tokens": ["A\u00b7bends", ",", "e\u00b7he", "die", "Nacht", "sich", "\u00f6ff\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie in Schlacken und gelbem Fluthschlamm", "tokens": ["Wie", "in", "Schla\u00b7cken", "und", "gel\u00b7bem", "Fluth\u00b7schlamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Suchend der Forscher ein seltenes Goldkorn", "tokens": ["Su\u00b7chend", "der", "For\u00b7scher", "ein", "sel\u00b7te\u00b7nes", "Gold\u00b7korn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Endlich findet und jubelnd aufjauchzt,", "tokens": ["End\u00b7lich", "fin\u00b7det", "und", "ju\u00b7belnd", "auf\u00b7jauchzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Also unwandelbar hab\u2019 ich gew\u00e4hlet", "tokens": ["Al\u00b7so", "un\u00b7wan\u00b7del\u00b7bar", "hab'", "ich", "ge\u00b7w\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Dich, du Meine, vor Menschenmillionen.", "tokens": ["Dich", ",", "du", "Mei\u00b7ne", ",", "vor", "Men\u00b7schen\u00b7mil\u00b7li\u00b7o\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "PPOSAT", "$,", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und nun schwindet auch meine Schwermuth", "tokens": ["Und", "nun", "schwin\u00b7det", "auch", "mei\u00b7ne", "Schwer\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und die Angst vor des Weltalls Abgrund,", "tokens": ["Und", "die", "Angst", "vor", "des", "Welt\u00b7alls", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Denn wie nichtig und klein und kl\u00e4glich", "tokens": ["Denn", "wie", "nich\u00b7tig", "und", "klein", "und", "kl\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wir auch wandeln im Weltenwalten:", "tokens": ["Wir", "auch", "wan\u00b7deln", "im", "Wel\u00b7ten\u00b7wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Gro\u00df ist die Welt, in der wir stehen,", "tokens": ["Gro\u00df", "ist", "die", "Welt", ",", "in", "der", "wir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gro\u00dfes greift in unsere Seele,", "tokens": ["Gro\u00b7\u00dfes", "greift", "in", "un\u00b7se\u00b7re", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Denn die Kernkraft der Weltenkr\u00e4fte,", "tokens": ["Denn", "die", "Kern\u00b7kraft", "der", "Wel\u00b7ten\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Denn das Mark des mehrenden M\u00fchens,", "tokens": ["Denn", "das", "Mark", "des", "meh\u00b7ren\u00b7den", "M\u00fc\u00b7hens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Duftende Blume des bl\u00fchenden Daseins", "tokens": ["Duf\u00b7ten\u00b7de", "Blu\u00b7me", "des", "bl\u00fc\u00b7hen\u00b7den", "Da\u00b7seins"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Ist die Kraft der lebendigen Liebe.", "tokens": ["Ist", "die", "Kraft", "der", "le\u00b7ben\u00b7di\u00b7gen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht mehr einsam ist nun mein Ahnen.", "tokens": ["Nicht", "mehr", "ein\u00b7sam", "ist", "nun", "mein", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ueber Str\u00f6me im Mondenstrahle,", "tokens": ["Ue\u00b7ber", "Str\u00f6\u00b7me", "im", "Mon\u00b7dens\u00b7trah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ueber Burgen auf m\u00e4chtigen Bergen,", "tokens": ["Ue\u00b7ber", "Bur\u00b7gen", "auf", "m\u00e4ch\u00b7ti\u00b7gen", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wandelt mein Geist auf goldenen Bahnen,", "tokens": ["Wan\u00b7delt", "mein", "Geist", "auf", "gol\u00b7de\u00b7nen", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und an deine br\u00e4utliche Brust,", "tokens": ["Und", "an", "dei\u00b7ne", "br\u00e4ut\u00b7li\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Bald gebettet an deinen Busen", "tokens": ["Bald", "ge\u00b7bet\u00b7tet", "an", "dei\u00b7nen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "F\u00fchl\u2019 ich selber in Freud\u2019 und Frieden", "tokens": ["F\u00fchl'", "ich", "sel\u00b7ber", "in", "Freud'", "und", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Stark eine Welt in uns erstehen.", "tokens": ["Stark", "ei\u00b7ne", "Welt", "in", "uns", "er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sei gesegnet, du Seelenvolle! \u2014", "tokens": ["Sei", "ge\u00b7seg\u00b7net", ",", "du", "See\u00b7len\u00b7vol\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}