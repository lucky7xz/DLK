{"textgrid.poem.43296": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deine Locken sind es,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Locken sind es,", "tokens": ["Dei\u00b7ne", "Lo\u00b7cken", "sind", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dein Gesicht,", "tokens": ["Dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nur bleich wie Du", "tokens": ["Nur", "bleich", "wie", "Du"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ist das Kindlein nicht.", "tokens": ["Ist", "das", "Kin\u00b7dlein", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Deine Stirne ist es", "tokens": ["Dei\u00b7ne", "Stir\u00b7ne", "ist", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und Dein Mund", "tokens": ["Und", "Dein", "Mund"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und auch Dein Auge", "tokens": ["Und", "auch", "Dein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "So kindlich-rund.", "tokens": ["So", "kind\u00b7lich\u00b7rund", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dein L\u00e4cheln ist es,", "tokens": ["Dein", "L\u00e4\u00b7cheln", "ist", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Dein Zucken gar ...", "tokens": ["Dein", "Zu\u00b7cken", "gar", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Das immer", "tokens": ["Das", "im\u00b7mer"], "token_info": ["word", "word"], "pos": ["PDS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Heimliches Weinen war.", "tokens": ["Heim\u00b7li\u00b7ches", "Wei\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}