{"dta.poem.4192": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Lebens-Reise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Garten ging ich hin und her,", "tokens": ["Jm", "Gar\u00b7ten", "ging", "ich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit sanften Schritten, j\u00fcngst spatzieren,", "tokens": ["Mit", "sanf\u00b7ten", "Schrit\u00b7ten", ",", "j\u00fcngst", "spat\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dacht\u2019 auf unsre Zeit von ungefehr,", "tokens": ["Und", "dacht'", "auf", "uns\u00b7re", "Zeit", "von", "un\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie wir sie unvermerkt verlieren;", "tokens": ["Wie", "wir", "sie", "un\u00b7ver\u00b7merkt", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie schnell die Gegenwart vorbey,", "tokens": ["Wie", "schnell", "die", "Ge\u00b7gen\u00b7wart", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das K\u00fcnft\u2019ge nah, und bald vergangen sey.", "tokens": ["Das", "K\u00fcnft'\u00b7ge", "nah", ",", "und", "bald", "ver\u00b7gan\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich sah, da ich so ging, auf drey, vier Schritte weit,", "tokens": ["Ich", "sah", ",", "da", "ich", "so", "ging", ",", "auf", "drey", ",", "vier", "Schrit\u00b7te", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "APPR", "CARD", "$,", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein buntes Bl\u00e4ttgen, mit Vergn\u00fcgen,", "tokens": ["Ein", "bun\u00b7tes", "Bl\u00e4tt\u00b7gen", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jm Steige, noch entfernet, vor mir liegen,", "tokens": ["Jm", "Stei\u00b7ge", ",", "noch", "ent\u00b7fer\u00b7net", ",", "vor", "mir", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Allein, es blieb nicht so; es war im Augenblick", "tokens": ["Al\u00b7lein", ",", "es", "blieb", "nicht", "so", ";", "es", "war", "im", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$.", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jm Gehn schon unter mir, und bald darauf zur\u00fcck.", "tokens": ["Jm", "Gehn", "schon", "un\u00b7ter", "mir", ",", "und", "bald", "da\u00b7rauf", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPER", "$,", "KON", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die\u00df Bl\u00e4ttgen kam, in dieser Lage, mir,", "tokens": ["Die\u00df", "Bl\u00e4tt\u00b7gen", "kam", ",", "in", "die\u00b7ser", "La\u00b7ge", ",", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "$,", "APPR", "PDAT", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von unsrer schnellen Zeit, als wie ein Sinnbild, f\u00fcr,", "tokens": ["Von", "uns\u00b7rer", "schnel\u00b7len", "Zeit", ",", "als", "wie", "ein", "Sinn\u00b7bild", ",", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und die\u00df um desto mehr, als es geruhig liegt,", "tokens": ["Und", "die\u00df", "um", "des\u00b7to", "mehr", ",", "als", "es", "ge\u00b7ru\u00b7hig", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und, eben wie die Zeit, nur blo\u00df dem Schein nach, fliegt;", "tokens": ["Und", ",", "e\u00b7ben", "wie", "die", "Zeit", ",", "nur", "blo\u00df", "dem", "Schein", "nach", ",", "fliegt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOKOM", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich aber, durch mein Gehn, erst fern, bald in der N\u00e4he,", "tokens": ["Ich", "a\u00b7ber", ",", "durch", "mein", "Gehn", ",", "erst", "fern", ",", "bald", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bald wieder fern vom Blatt, und allen Dingen, gehe,", "tokens": ["Bald", "wie\u00b7der", "fern", "vom", "Blatt", ",", "und", "al\u00b7len", "Din\u00b7gen", ",", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$,", "KON", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die um und bey mir sind. Ja, was noch mehr,", "tokens": ["Die", "um", "und", "bey", "mir", "sind", ".", "Ja", ",", "was", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "KON", "APPR", "PPER", "VAFIN", "$.", "PTKANT", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Hier leidet es die Zeit, da\u00df ich zur\u00fccke kehr,", "tokens": ["Hier", "lei\u00b7det", "es", "die", "Zeit", ",", "da\u00df", "ich", "zu\u00b7r\u00fc\u00b7cke", "kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auch stille stehen kann, wenn mirs gef\u00e4llt.", "tokens": ["Auch", "stil\u00b7le", "ste\u00b7hen", "kann", ",", "wenn", "mirs", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVINF", "VMFIN", "$,", "KOUS", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "In unserm Lebens-Gang, in dieser Welt,", "tokens": ["In", "un\u00b7serm", "Le\u00b7bens\u00b7Gang", ",", "in", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Will die\u00df der Zustand nicht verg\u00f6nnen,", "tokens": ["Will", "die\u00df", "der", "Zu\u00b7stand", "nicht", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Indem wir hier, wir m\u00f6gen stille stehn,", "tokens": ["In\u00b7dem", "wir", "hier", ",", "wir", "m\u00f6\u00b7gen", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wir m\u00f6gen sitzen oder liegen, dennoch best\u00e4ndig vorwerts", "tokens": ["Wir", "m\u00f6\u00b7gen", "sit\u00b7zen", "o\u00b7der", "lie\u00b7gen", ",", "den\u00b7noch", "be\u00b7st\u00e4n\u00b7dig", "vor\u00b7werts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$,", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.25": {"text": "gehn,", "tokens": ["gehn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Und nimmer r\u00fcckwerts kehren k\u00f6nnen.", "tokens": ["Und", "nim\u00b7mer", "r\u00fcck\u00b7werts", "keh\u00b7ren", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ach, la\u00df uns doch, o HErr! so lange", "tokens": ["Ach", ",", "la\u00df", "uns", "doch", ",", "o", "Herr", "!", "so", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADV", "$,", "FM", "NN", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wir hier, in unserm Lebens-Gange,", "tokens": ["Wir", "hier", ",", "in", "un\u00b7serm", "Le\u00b7bens\u00b7Gan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}