{"textgrid.poem.63359": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Bakkarat", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte einst von einer zarten Neun.", "tokens": ["Mir", "tr\u00e4um\u00b7te", "einst", "von", "ei\u00b7ner", "zar\u00b7ten", "Neun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich hielt sie sicher gegen f\u00fcnf und sieben.", "tokens": ["Ich", "hielt", "sie", "si\u00b7cher", "ge\u00b7gen", "f\u00fcnf", "und", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "CARD", "KON", "CARD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Millionen waren in der Bank geblieben,", "tokens": ["Mil\u00b7lion\u00b7en", "wa\u00b7ren", "in", "der", "Bank", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun durft' ich sie in alle Winde streu'n.", "tokens": ["Nun", "durft'", "ich", "sie", "in", "al\u00b7le", "Win\u00b7de", "streu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "PIAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schenkte einem M\u00e4dchen sie beim Heu'n.", "tokens": ["Ich", "schenk\u00b7te", "ei\u00b7nem", "M\u00e4d\u00b7chen", "sie", "beim", "Heu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich lie\u00df das Gold in goldnen Sieben sieben.", "tokens": ["Ich", "lie\u00df", "das", "Gold", "in", "gold\u00b7nen", "Sie\u00b7ben", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wagte tausend Frau'n zugleich zu lieben,", "tokens": ["Ich", "wag\u00b7te", "tau\u00b7send", "Frau'n", "zu\u00b7gleich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und brauchte keinen schlimmen Schutzmann scheu'n.", "tokens": ["Und", "brauch\u00b7te", "kei\u00b7nen", "schlim\u00b7men", "Schutz\u00b7mann", "scheu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich kaufte mir die blanken Feldherrntressen,", "tokens": ["Ich", "kauf\u00b7te", "mir", "die", "blan\u00b7ken", "Feld\u00b7herrnt\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Horizonte, die mein Auge sah,", "tokens": ["Die", "Ho\u00b7ri\u00b7zon\u00b7te", ",", "die", "mein", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lie\u00df meine Verse nur in Silber pressen.", "tokens": ["Lie\u00df", "mei\u00b7ne", "Ver\u00b7se", "nur", "in", "Sil\u00b7ber", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich badete mich in Lawendel \u2013 ah \u2013", "tokens": ["Ich", "ba\u00b7de\u00b7te", "mich", "in", "La\u00b7wen\u00b7del", "\u2013", "ah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NE", "$(", "XY", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kaufte f\u00fcr den Rest mir das Vergessen \u2013", "tokens": ["Und", "kauf\u00b7te", "f\u00fcr", "den", "Rest", "mir", "das", "Ver\u00b7ges\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch dich verga\u00df ich nimmer, Bakkarat!", "tokens": ["Doch", "dich", "ver\u00b7ga\u00df", "ich", "nim\u00b7mer", ",", "Bak\u00b7ka\u00b7rat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}