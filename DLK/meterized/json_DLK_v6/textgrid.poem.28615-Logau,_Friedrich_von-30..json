{"textgrid.poem.28615": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die Complimenten sind, mangelt was gewi\u00df am Willen;", "tokens": ["Wo", "die", "Com\u00b7pli\u00b7men\u00b7ten", "sind", ",", "man\u00b7gelt", "was", "ge\u00b7wi\u00df", "am", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "VVFIN", "PWS", "ADV", "APPRART", "NN", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sonsten d\u00f6rfften Worte nicht, wann nicht Mangel, was erv\u00f6llen.", "tokens": ["Sons\u00b7ten", "d\u00f6rff\u00b7ten", "Wor\u00b7te", "nicht", ",", "wann", "nicht", "Man\u00b7gel", ",", "was", "er\u00b7v\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKNEG", "$,", "PWAV", "PTKNEG", "NN", "$,", "PWS", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}