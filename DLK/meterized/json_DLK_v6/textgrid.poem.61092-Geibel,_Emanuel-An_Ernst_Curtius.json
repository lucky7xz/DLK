{"textgrid.poem.61092": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An Ernst Curtius", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat der Sorge je sein Herz verschlossen?", "tokens": ["Wer", "hat", "der", "Sor\u00b7ge", "je", "sein", "Herz", "ver\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fl\u00f6hn wir zu des Poles eis'gen Strecken,", "tokens": ["Und", "fl\u00f6hn", "wir", "zu", "des", "Po\u00b7les", "eis'\u00b7gen", "Stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie w\u00fcrde dort auch uns vom Lager schrecken,", "tokens": ["Sie", "w\u00fcr\u00b7de", "dort", "auch", "uns", "vom", "La\u00b7ger", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn auf die Wimper kaum sich Schlaf ergossen.", "tokens": ["Wenn", "auf", "die", "Wim\u00b7per", "kaum", "sich", "Schlaf", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PRF", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir sehn von hellem Kerzenglanz umflossen", "tokens": ["Wir", "sehn", "von", "hel\u00b7lem", "Ker\u00b7zen\u00b7glanz", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie flattern an des Prunksaals goldnen Decken;", "tokens": ["Sie", "flat\u00b7tern", "an", "des", "Prunk\u00b7saals", "gold\u00b7nen", "De\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Schiffer folgt sie durch das Meer, dem kecken,", "tokens": ["Dem", "Schif\u00b7fer", "folgt", "sie", "durch", "das", "Meer", ",", "dem", "ke\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Reiter holt sie ein auf fl\u00fccht'gen Rossen.", "tokens": ["Den", "Rei\u00b7ter", "holt", "sie", "ein", "auf", "fl\u00fccht'\u00b7gen", "Ros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum suche nicht, ihr t\u00f6richt zu entfliehen,", "tokens": ["Drum", "su\u00b7che", "nicht", ",", "ihr", "t\u00f6\u00b7richt", "zu", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit L\u00e4cheln wolle das Geschick vers\u00f6hnen,", "tokens": ["Mit", "L\u00e4\u00b7cheln", "wol\u00b7le", "das", "Ge\u00b7schick", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da keinem noch ein reines Gl\u00fcck gediehen.", "tokens": ["Da", "kei\u00b7nem", "noch", "ein", "rei\u00b7nes", "Gl\u00fcck", "ge\u00b7die\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch kannst du dich der Klage nicht entw\u00f6hnen,", "tokens": ["Doch", "kannst", "du", "dich", "der", "Kla\u00b7ge", "nicht", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So reife sie zum Lied, der dir verliehen,", "tokens": ["So", "rei\u00b7fe", "sie", "zum", "Lied", ",", "der", "dir", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der leise Hauch der griechischen Kam\u00f6nen.", "tokens": ["Der", "lei\u00b7se", "Hauch", "der", "grie\u00b7chi\u00b7schen", "Ka\u00b7m\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}