{"textgrid.poem.35019": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Gerechter Tadel", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Natur, du gute Mutter,", "tokens": ["Na\u00b7tur", ",", "du", "gu\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verzeih', ich tadle dich.", "tokens": ["Ver\u00b7zeih'", ",", "ich", "tad\u00b7le", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Anstatt zum Wandervogel,", "tokens": ["An\u00b7statt", "zum", "Wan\u00b7der\u00b7vo\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schufst du zum Menschen mich", "tokens": ["Schufst", "du", "zum", "Men\u00b7schen", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als der ich ja beim Wandern", "tokens": ["Als", "der", "ich", "ja", "beim", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch Gottes sch\u00f6ne Welt", "tokens": ["Durch", "Got\u00b7tes", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Freiheit hab' von N\u00f6then,", "tokens": ["Die", "Frei\u00b7heit", "hab'", "von", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "'nen Pa\u00df und \u2013 kleines Geld.", "tokens": ["'nen", "Pa\u00df", "und", "\u2013", "klei\u00b7nes", "Geld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}