{"textgrid.poem.58460": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "498. Ohne Reflektieren", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft dich selbst nicht sehen,", "tokens": ["Du", "mu\u00dft", "dich", "selbst", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du willst sicher gehen,", "tokens": ["Wenn", "du", "willst", "si\u00b7cher", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Aug' mu\u00df licht und rein", "tokens": ["Dein", "Aug'", "mu\u00df", "licht", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ganz einf\u00e4ltig sein.", "tokens": ["Und", "ganz", "ein\u00b7f\u00e4l\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur kindlich Gott alleine", "tokens": ["Nur", "kind\u00b7lich", "Gott", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In allem schau und meine", "tokens": ["In", "al\u00b7lem", "schau", "und", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PTKVZ", "KON", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und tu nicht einen Blick", "tokens": ["Und", "tu", "nicht", "ei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PTKNEG", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In Eigenheit zur\u00fcck!", "tokens": ["In", "Ei\u00b7gen\u00b7heit", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}