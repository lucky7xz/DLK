{"textgrid.poem.62564": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Christus hat mich sehr geliebt", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Christus hat mich sehr geliebt,", "tokens": ["Chris\u00b7tus", "hat", "mich", "sehr", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und viel Schmach f\u00fcr mich gelitten;", "tokens": ["Und", "viel", "Schmach", "f\u00fcr", "mich", "ge\u00b7lit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Seele war betr\u00fcbt,", "tokens": ["Sei\u00b7ne", "See\u00b7le", "war", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als er mit dem Tod gestritten.", "tokens": ["Als", "er", "mit", "dem", "Tod", "ge\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welches mir das Leben giebt.", "tokens": ["Wel\u00b7ches", "mir", "das", "Le\u00b7ben", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Christus hat mich sehr geliebt.", "tokens": ["Chris\u00b7tus", "hat", "mich", "sehr", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "HeRR mein Heyl! was forderst du?", "tokens": ["HeRR", "mein", "Heyl", "!", "was", "for\u00b7derst", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was wilst du vor diese Liebe?", "tokens": ["Was", "wilst", "du", "vor", "die\u00b7se", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nichts, als da\u00df ich gleiches thu;", "tokens": ["Nichts", ",", "als", "da\u00df", "ich", "glei\u00b7ches", "thu", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOKOM", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mein Hertz dich ewig liebe.", "tokens": ["Da\u00df", "mein", "Hertz", "dich", "e\u00b7wig", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm es, dir geh\u00f6rt es zu,", "tokens": ["Nimm", "es", ",", "dir", "ge\u00b7h\u00f6rt", "es", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herr mein Heyl; das forderst du.", "tokens": ["Herr", "mein", "Heyl", ";", "das", "for\u00b7derst", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reich mir aber deine Gnad,", "tokens": ["Reich", "mir", "a\u00b7ber", "dei\u00b7ne", "Gnad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ichs rein von S\u00fcnden halte,", "tokens": ["Da\u00df", "ichs", "rein", "von", "S\u00fcn\u00b7den", "hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch keine Missethat,", "tokens": ["Und", "durch", "kei\u00b7ne", "Mis\u00b7se\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich von deiner Liebe spalte:", "tokens": ["Mich", "von", "dei\u00b7ner", "Lie\u00b7be", "spal\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich folge deinem Rath;", "tokens": ["Da\u00df", "ich", "fol\u00b7ge", "dei\u00b7nem", "Rath", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reich mir Jesu deine Gnad.", "tokens": ["Reich", "mir", "Je\u00b7su", "dei\u00b7ne", "Gnad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Iesu! Jesu! dir allein", "tokens": ["Ie\u00b7su", "!", "Je\u00b7su", "!", "dir", "al\u00b7lein"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey mein Herze \u00fcbergeben.", "tokens": ["Sey", "mein", "Her\u00b7ze", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh o Welt mit deinem Schein!", "tokens": ["Geh", "o", "Welt", "mit", "dei\u00b7nem", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "FM", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Christus soll nur in mir leben.", "tokens": ["Chris\u00b7tus", "soll", "nur", "in", "mir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O! wer nimmt mein Herze ein!", "tokens": ["O", "!", "wer", "nimmt", "mein", "Her\u00b7ze", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jesu! Jesu, du allein.", "tokens": ["Je\u00b7su", "!", "Je\u00b7su", ",", "du", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$,", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schaffe mit mir, was du wilt;", "tokens": ["Schaf\u00b7fe", "mit", "mir", ",", "was", "du", "wilt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchr mich nur auf deinem Wege.", "tokens": ["F\u00fchr", "mich", "nur", "auf", "dei\u00b7nem", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil dein Glanz mein Aug erf\u00fcllt,", "tokens": ["Weil", "dein", "Glanz", "mein", "Aug", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hasse ich die S\u00fcnden-Stege.", "tokens": ["Has\u00b7se", "ich", "die", "S\u00fcn\u00b7den\u00b7Ste\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob mich Welt und Sattan schilt!", "tokens": ["Ob", "mich", "Welt", "und", "Sat\u00b7tan", "schilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaffe mit mir, was du wilt.", "tokens": ["Schaf\u00b7fe", "mit", "mir", ",", "was", "du", "wilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Treibe mich nach Thabors H\u00f6h,", "tokens": ["Trei\u00b7be", "mich", "nach", "Tha\u00b7bors", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo es gut zu wohnen scheinet;", "tokens": ["Wo", "es", "gut", "zu", "woh\u00b7nen", "schei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilst du, da\u00df ich stille steh", "tokens": ["Wilst", "du", ",", "da\u00df", "ich", "stil\u00b7le", "steh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Oelberg, wo man weinet:", "tokens": ["An", "dem", "O\u00b7el\u00b7berg", ",", "wo", "man", "wei\u00b7net", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh nur, wie ich freudig geh;", "tokens": ["Sieh", "nur", ",", "wie", "ich", "freu\u00b7dig", "geh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "HeRR! ich folg auf jede H\u00f6h.", "tokens": ["HeRR", "!", "ich", "folg", "auf", "je\u00b7de", "H\u00f6h", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "IeSUS f\u00fchrt mich bey der Hand,", "tokens": ["Ie\u00b7SUS", "f\u00fchrt", "mich", "bey", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum werd ich niemahls fallen;", "tokens": ["Da\u00b7rum", "werd", "ich", "nie\u00b7mahls", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ich gleich zum Vaterland,", "tokens": ["Mu\u00df", "ich", "gleich", "zum", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie mein Jesus thr\u00e4nend wallen.", "tokens": ["Wie", "mein", "Je\u00b7sus", "thr\u00e4\u00b7nend", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NE", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trotz sey allem Wiederstand!", "tokens": ["Trotz", "sey", "al\u00b7lem", "Wie\u00b7der\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PIS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jesus f\u00fchrt mich bey der Hand.", "tokens": ["Je\u00b7sus", "f\u00fchrt", "mich", "bey", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber kan es m\u00f6glich seyn,", "tokens": ["A\u00b7ber", "kan", "es", "m\u00f6g\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ey! so f\u00fchre meine Seele", "tokens": ["Ey", "!", "so", "f\u00fch\u00b7re", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald aus ihrer S\u00fcnden-Pein,", "tokens": ["Bald", "aus", "ih\u00b7rer", "S\u00fcn\u00b7den\u00b7Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie sich nicht l\u00e4nger qu\u00e4le;", "tokens": ["Da\u00df", "sie", "sich", "nicht", "l\u00e4n\u00b7ger", "qu\u00e4\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchre sie zum Himmel ein;", "tokens": ["F\u00fch\u00b7re", "sie", "zum", "Him\u00b7mel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nt es doch bald m\u00f6glich seyn.", "tokens": ["K\u00f6nt", "es", "doch", "bald", "m\u00f6g\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun wohlan! mit Freudigkeit", "tokens": ["Nun", "wo\u00b7hlan", "!", "mit", "Freu\u00b7dig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$.", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eil ich zu den Sternen-H\u00fcgeln,", "tokens": ["Eil", "ich", "zu", "den", "Ster\u00b7nen\u00b7H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mir Gott die Krone beut,", "tokens": ["Wo", "mir", "Gott", "die", "Kro\u00b7ne", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Glauben zu besiegeln.", "tokens": ["Mei\u00b7nen", "Glau\u00b7ben", "zu", "be\u00b7sie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O vergn\u00fcgte Ewigkeit!", "tokens": ["O", "ver\u00b7gn\u00fcg\u00b7te", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du schaffst lauter Freudigkeit.", "tokens": ["Du", "schaffst", "lau\u00b7ter", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}