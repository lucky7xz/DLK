{"textgrid.poem.49229": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als neulich ich mein Lieb umfieng mit vielen K\u00fcssen,", "tokens": ["Als", "neu\u00b7lich", "ich", "mein", "Lieb", "um\u00b7fi\u00b7eng", "mit", "vie\u00b7len", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erseufzte sie gar hoch und machte sich betr\u00fcbt;", "tokens": ["Er\u00b7seufz\u00b7te", "sie", "gar", "hoch", "und", "mach\u00b7te", "sich", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr h\u00f6chster Schmerze war, wie die Vermuthung gibt,", "tokens": ["Ihr", "h\u00f6chs\u00b7ter", "Schmer\u00b7ze", "war", ",", "wie", "die", "Ver\u00b7mu\u00b7thung", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df diese Schmerzen sich zu eilends w\u00fcrden schlie\u00dfen.", "tokens": ["Da\u00df", "die\u00b7se", "Schmer\u00b7zen", "sich", "zu", "ei\u00b7lends", "w\u00fcr\u00b7den", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PRF", "APPR", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}