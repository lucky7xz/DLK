{"textgrid.poem.43288": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kannst tanzen?!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kannst tanzen?!", "tokens": ["Du", "kannst", "tan\u00b7zen", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dich zierlich schwingen,", "tokens": ["Dich", "zier\u00b7lich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "An meiner Hand", "tokens": ["An", "mei\u00b7ner", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den Reigen schlingen?", "tokens": ["Den", "Rei\u00b7gen", "schlin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ich dachte nie dran,", "tokens": ["Ich", "dach\u00b7te", "nie", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.6": {"text": "Da\u00df auch ich es kann \u2013", "tokens": ["Da\u00df", "auch", "ich", "es", "kann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPER", "VMFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mit einmal fand", "tokens": ["Mit", "ein\u00b7mal", "fand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dein eitler Mann,", "tokens": ["Dein", "eit\u00b7ler", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da\u00df er tanzen kann!", "tokens": ["Da\u00df", "er", "tan\u00b7zen", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Du kannst tanzen,", "tokens": ["Du", "kannst", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dich fl\u00fcchtig heben", "tokens": ["Dich", "fl\u00fcch\u00b7tig", "he\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "An meiner Brust", "tokens": ["An", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und weiter schweben!", "tokens": ["Und", "wei\u00b7ter", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ich dachte kaum", "tokens": ["Ich", "dach\u00b7te", "kaum"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Im seligsten Traum", "tokens": ["Im", "se\u00b7ligs\u00b7ten", "Traum"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "An solche Lust!", "tokens": ["An", "sol\u00b7che", "Lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Jetzt lacht Dein Mann,", "tokens": ["Jetzt", "lacht", "Dein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Weil er tanzen kann!", "tokens": ["Weil", "er", "tan\u00b7zen", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}