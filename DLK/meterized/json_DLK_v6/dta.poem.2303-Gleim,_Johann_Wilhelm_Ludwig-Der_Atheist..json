{"dta.poem.2303": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Atheist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allerliebster GOtt der Liebe,", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7ter", "Gott", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dich lieben, liebst du wieder.", "tokens": ["Die", "dich", "lie\u00b7ben", ",", "liebst", "du", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! wilst du mich denn nicht lieben?", "tokens": ["Ach", "!", "wilst", "du", "mich", "denn", "nicht", "lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doris ist noch immer spr\u00f6de.", "tokens": ["Do\u00b7ris", "ist", "noch", "im\u00b7mer", "spr\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spanne doch den Bogen strenger,", "tokens": ["Span\u00b7ne", "doch", "den", "Bo\u00b7gen", "stren\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimm den \u00e4rgsten deiner Pfeile,", "tokens": ["Nimm", "den", "\u00e4rgs\u00b7ten", "dei\u00b7ner", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ihr Herz ist hart, wie Marmor.", "tokens": ["Denn", "ihr", "Herz", "ist", "hart", ",", "wie", "Mar\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Mit der Kunst bered\u2019ter Lippen,", "tokens": ["Mit", "der", "Kunst", "be\u00b7re\u00b7d'\u00b7ter", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Mit der Macht vertrauter Schw\u00fcre,", "tokens": ["Mit", "der", "Macht", "ver\u00b7trau\u00b7ter", "Schw\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit der Staatslist deiner Lehrer,", "tokens": ["Mit", "der", "Staats\u00b7list", "dei\u00b7ner", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit der W\u00fcrkung meiner Waffen", "tokens": ["Mit", "der", "W\u00fcr\u00b7kung", "mei\u00b7ner", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Werd\u2019 ich es nicht leicht erobern;", "tokens": ["Werd'", "ich", "es", "nicht", "leicht", "er\u00b7o\u00b7bern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Denn sie ist zu stark bewafnet.", "tokens": ["Denn", "sie", "ist", "zu", "stark", "be\u00b7waf\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sie versteht die Kunst zu siegen,", "tokens": ["Sie", "ver\u00b7steht", "die", "Kunst", "zu", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Trotz dem besten deiner Krieger.", "tokens": ["Trotz", "dem", "bes\u00b7ten", "dei\u00b7ner", "Krie\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wirst du sie denn \u00fcberwinden?", "tokens": ["Wirst", "du", "sie", "denn", "\u00fc\u00b7berw\u00b7in\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Liebesgott! nur drei Minuten", "tokens": ["Lie\u00b7bes\u00b7gott", "!", "nur", "drei", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Glaub\u2019 ich noch an deine Pfeile;", "tokens": ["Glaub'", "ich", "noch", "an", "dei\u00b7ne", "Pfei\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Hast du mir nach drei Minuten", "tokens": ["Hast", "du", "mir", "nach", "drei", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Diese Spr\u00f6de nicht geb\u00e4ndigt:", "tokens": ["Die\u00b7se", "Spr\u00f6\u00b7de", "nicht", "ge\u00b7b\u00e4n\u00b7digt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "O! so will ich in der vierten", "tokens": ["O", "!", "so", "will", "ich", "in", "der", "vier\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dich und deine Mutter l\u00e4ugnen.", "tokens": ["Dich", "und", "dei\u00b7ne", "Mut\u00b7ter", "l\u00e4ug\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}