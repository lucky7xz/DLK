{"textgrid.poem.56765": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein schwarzes V\u00f6glein fliegt \u00fcber die Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schwarzes V\u00f6glein fliegt \u00fcber die Welt,", "tokens": ["Ein", "schwar\u00b7zes", "V\u00f6\u00b7glein", "fliegt", "\u00fc\u00b7ber", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das singt so todestraurig ...", "tokens": ["das", "singt", "so", "to\u00b7dest\u00b7rau\u00b7rig", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer es h\u00f6rt, der h\u00f6rt nichts anderes mehr,", "tokens": ["Wer", "es", "h\u00f6rt", ",", "der", "h\u00f6rt", "nichts", "an\u00b7de\u00b7res", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PIS", "PIS", "ADV", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "wer es h\u00f6rt, der tut sich ein Leides an,", "tokens": ["wer", "es", "h\u00f6rt", ",", "der", "tut", "sich", "ein", "Lei\u00b7des", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "der mag keine Sonne mehr schauen.", "tokens": ["der", "mag", "kei\u00b7ne", "Son\u00b7ne", "mehr", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Allmitternacht, Allmitternacht", "tokens": ["All\u00b7mit\u00b7ter\u00b7nacht", ",", "All\u00b7mit\u00b7ter\u00b7nacht"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ruht es sich aus auf dem Finger des Tods.", "tokens": ["ruht", "es", "sich", "aus", "auf", "dem", "Fin\u00b7ger", "des", "Tods", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der streichelt's leis und spricht ihm zu:", "tokens": ["Der", "strei\u00b7chelt's", "leis", "und", "spricht", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbflieg, mein V\u00f6gelein! flieg, mein V\u00f6gelein!\u00ab", "tokens": ["\u00bb", "flieg", ",", "mein", "V\u00f6\u00b7ge\u00b7lein", "!", "flieg", ",", "mein", "V\u00f6\u00b7ge\u00b7lein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und wieder fliegt's fl\u00f6tend \u00fcber die Welt.", "tokens": ["Und", "wie\u00b7der", "fliegt's", "fl\u00f6\u00b7tend", "\u00fc\u00b7ber", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}