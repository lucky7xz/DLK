_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das mag kein Knecht vermissen:", "tokens": ["Das", "mag", "kein", "Knecht", "ver\u00b7mis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo lie\u00dfest du das ", "tokens": ["Wo", "lie\u00b7\u00dfest", "du", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "\u00bbwo ich das Brod gelassen?\u00ab", "tokens": ["\u00bb", "wo", "ich", "das", "Brod", "ge\u00b7las\u00b7sen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach da der Pf\u00e4lzer Fritz,", "tokens": ["Sprach", "da", "der", "Pf\u00e4l\u00b7zer", "Fritz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er traf, die bei ihm sassen,", "tokens": ["Er", "traf", ",", "die", "bei", "ihm", "sas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seiner Augen Blitz;", "tokens": ["Mit", "sei\u00b7ner", "Au\u00b7gen", "Blitz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er that die Fensterpforten", "tokens": ["Er", "that", "die", "Fens\u00b7ter\u00b7pfor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit auf im hohen Saal,", "tokens": ["Weit", "auf", "im", "ho\u00b7hen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sah man aller Orten", "tokens": ["Da", "sah", "man", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In's offne Neckarthal.", "tokens": ["In's", "off\u00b7ne", "Ne\u00b7ck\u00b7ar\u00b7thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.13": {"line.1": {"text": "Sie sprangen von den St\u00fchlen", "tokens": ["Sie", "spran\u00b7gen", "von", "den", "St\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und blickten in das Land,", "tokens": ["Und", "blick\u00b7ten", "in", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da rauchten alle M\u00fchlen", "tokens": ["Da", "rauch\u00b7ten", "al\u00b7le", "M\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rings von des Krieges Brand;", "tokens": ["Rings", "von", "des", "Krie\u00b7ges", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Hof ist da zu schauen,", "tokens": ["Kein", "Hof", "ist", "da", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo nicht die Scheune dampft,", "tokens": ["Wo", "nicht", "die", "Scheu\u00b7ne", "dampft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von Rosses Huf und Klauen", "tokens": ["Von", "Ros\u00b7ses", "Huf", "und", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist alles Feld zerstampft.", "tokens": ["Ist", "al\u00b7les", "Feld", "zer\u00b7stampft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bbnun sprecht, von wessen Schulden", "tokens": ["\u00bb", "nun", "sprecht", ",", "von", "wes\u00b7sen", "Schul\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist so mein Mahl bestellt?", "tokens": ["Ist", "so", "mein", "Mahl", "be\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr m\u00fc\u00dft euch wohl gedulden,", "tokens": ["Ihr", "m\u00fc\u00dft", "euch", "wohl", "ge\u00b7dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis ihr bes\u00e4t mein Feld,", "tokens": ["Bis", "ihr", "be\u00b7s\u00e4t", "mein", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis in des Sommers Schw\u00fcle", "tokens": ["Bis", "in", "des", "Som\u00b7mers", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir reifet eure Saat,", "tokens": ["Mir", "rei\u00b7fet", "eu\u00b7re", "Saat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bis mir in der M\u00fchle", "tokens": ["Und", "bis", "mir", "in", "der", "M\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich wieder dreht ein Rad.", "tokens": ["Sich", "wie\u00b7der", "dreht", "ein", "Rad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ihr seht, der Westwind f\u00e4chelt", "tokens": ["Ihr", "seht", ",", "der", "West\u00b7wind", "f\u00e4\u00b7chelt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Stoppeln und Gestr\u00e4uch;", "tokens": ["In", "Stop\u00b7peln", "und", "Ge\u00b7str\u00e4uch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seht, die Sonne l\u00e4chelt,", "tokens": ["Ihr", "seht", ",", "die", "Son\u00b7ne", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie wartet nur auf euch!", "tokens": ["Sie", "war\u00b7tet", "nur", "auf", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum sendet flugs die Schl\u00fcssel", "tokens": ["Drum", "sen\u00b7det", "flugs", "die", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und \u00f6ffnet euren Schatz,", "tokens": ["Und", "\u00f6ff\u00b7net", "eu\u00b7ren", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So findet bei der Sch\u00fcssel", "tokens": ["So", "fin\u00b7det", "bei", "der", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}}}}