{"textgrid.poem.39882": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Mahl zu Heidelberg", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Wirtemberg und Baden", "tokens": ["Von", "Wir\u00b7tem\u00b7berg", "und", "Ba\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Die Herren zogen aus,", "tokens": ["Die", "Her\u00b7ren", "zo\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Metz des Bischofs Gnaden", "tokens": ["Von", "Metz", "des", "Bi\u00b7schofs", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verga\u00df das Gotteshaus;", "tokens": ["Ver\u00b7ga\u00df", "das", "Got\u00b7tes\u00b7haus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie zogen aus zu kriegen", "tokens": ["Sie", "zo\u00b7gen", "aus", "zu", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl in die Pfalz am Rhein,", "tokens": ["Wohl", "in", "die", "Pfalz", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie sahen da sie liegen", "tokens": ["Sie", "sa\u00b7hen", "da", "sie", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Sommersonnenschein.", "tokens": ["Im", "Som\u00b7mer\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Umsonst die Rebenbl\u00fcte", "tokens": ["Um\u00b7sonst", "die", "Re\u00b7ben\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie tr\u00e4nkt mit mildem Duft,", "tokens": ["Sie", "tr\u00e4nkt", "mit", "mil\u00b7dem", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umsonst des Himmels G\u00fcte", "tokens": ["Um\u00b7sonst", "des", "Him\u00b7mels", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Aehrenfeldern ruft:", "tokens": ["Aus", "A\u00b7eh\u00b7ren\u00b7fel\u00b7dern", "ruft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie brannten Hof und Scheuer,", "tokens": ["Sie", "brann\u00b7ten", "Hof", "und", "Scheu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df heulte gro\u00df und klein;", "tokens": ["Da\u00df", "heul\u00b7te", "gro\u00df", "und", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da leuchtete vom Feuer", "tokens": ["Da", "leuch\u00b7te\u00b7te", "vom", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Neckar und der Rhein.", "tokens": ["Der", "Ne\u00b7ckar", "und", "der", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit Gram von seinem Schlosse", "tokens": ["Mit", "Gram", "von", "sei\u00b7nem", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sieht es der Pf\u00e4lzer Fritz;", "tokens": ["Sieht", "es", "der", "Pf\u00e4l\u00b7zer", "Fritz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hei\u00dft springen auf die Rosse", "tokens": ["Hei\u00dft", "sprin\u00b7gen", "auf", "die", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwei Mann auf einen Sitz.", "tokens": ["Zwei", "Mann", "auf", "ei\u00b7nen", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit enggedr\u00e4ngtem Volke", "tokens": ["Mit", "eng\u00b7ge\u00b7dr\u00e4ng\u00b7tem", "Vol\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sprengt er durch Feld und Wald,", "tokens": ["Sprengt", "er", "durch", "Feld", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Doch ward die kleine Wolke", "tokens": ["Doch", "ward", "die", "klei\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Wetterhimmel bald.", "tokens": ["Zum", "Wet\u00b7ter\u00b7him\u00b7mel", "bald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie wollen seiner spotten,", "tokens": ["Sie", "wol\u00b7len", "sei\u00b7ner", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sind sie schon umringt,", "tokens": ["Da", "sind", "sie", "schon", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber ihren Rotten", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7ren", "Rot\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Schwert der Sieger schwingt.", "tokens": ["Sein", "Schwert", "der", "Sie\u00b7ger", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom H\u00fcgel sieht man prangen", "tokens": ["Vom", "H\u00fc\u00b7gel", "sieht", "man", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Heidelberger Schlo\u00df,", "tokens": ["Das", "Hei\u00b7del\u00b7ber\u00b7ger", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dorthin f\u00fchrt er gefangen", "tokens": ["Dor\u00b7thin", "f\u00fchrt", "er", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Die F\u00fcrsten samt dem Tro\u00df.", "tokens": ["Die", "F\u00fcrs\u00b7ten", "samt", "dem", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu hinterst an der Mauer,", "tokens": ["Zu", "hin\u00b7terst", "an", "der", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da ragt ein Thurm so fest,", "tokens": ["Da", "ragt", "ein", "Thurm", "so", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein Sitz der Trauer,", "tokens": ["Das", "ist", "ein", "Sitz", "der", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Schlang' und Eule Nest:", "tokens": ["Der", "Schlang'", "und", "Eu\u00b7le", "Nest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort sollen sie ihm b\u00fc\u00dfen", "tokens": ["Dort", "sol\u00b7len", "sie", "ihm", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Kerker tr\u00fcb und kalt,", "tokens": ["Im", "Ker\u00b7ker", "tr\u00fcb", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es g\u00e4hnt zu ihren F\u00fc\u00dfen", "tokens": ["Es", "g\u00e4hnt", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Schlund und finstrer Wald.", "tokens": ["Ein", "Schlund", "und", "finst\u00b7rer", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hier lernt vom Grimme rasten", "tokens": ["Hier", "lernt", "vom", "Grim\u00b7me", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wirtemberger Utz,", "tokens": ["Der", "Wir\u00b7tem\u00b7ber\u00b7ger", "Utz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Der Bischof h\u00e4lt ein Fasten,", "tokens": ["Der", "Bi\u00b7schof", "h\u00e4lt", "ein", "Fas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Markgraf l\u00e4\u00dft vom Trutz.", "tokens": ["Der", "Mark\u00b7graf", "l\u00e4\u00dft", "vom", "Trutz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie mochten schon in Sorgen", "tokens": ["Sie", "moch\u00b7ten", "schon", "in", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um Leib und Leben sein,", "tokens": ["Um", "Leib", "und", "Le\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da trat am andern Morgen", "tokens": ["Da", "trat", "am", "an\u00b7dern", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der stolze Pf\u00e4lzer ein.", "tokens": ["Der", "stol\u00b7ze", "Pf\u00e4l\u00b7zer", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbherauf, ihr Herrn, gestiegen", "tokens": ["\u00bb", "her\u00b7auf", ",", "ihr", "Herrn", ",", "ge\u00b7stie\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meinen hellen Saal!", "tokens": ["In", "mei\u00b7nen", "hel\u00b7len", "Saal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sollt nicht f\u00fcrder liegen", "tokens": ["Ihr", "sollt", "nicht", "f\u00fcr\u00b7der", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Finsterni\u00df und Qual.", "tokens": ["In", "Fins\u00b7ter\u00b7ni\u00df", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Mahl ist euch ger\u00fcstet,", "tokens": ["Ein", "Mahl", "ist", "euch", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Tafel ist gedeckt,", "tokens": ["Die", "Ta\u00b7fel", "ist", "ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum, wenn es euch gel\u00fcstet,", "tokens": ["Drum", ",", "wenn", "es", "euch", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Versucht ob es euch schmeckt!\u00ab", "tokens": ["Ver\u00b7sucht", "ob", "es", "euch", "schmeckt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOUS", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie lauschen mit Gefallen,", "tokens": ["Sie", "lau\u00b7schen", "mit", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie er so l\u00e4chelnd spricht,", "tokens": ["Wie", "er", "so", "l\u00e4\u00b7chelnd", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wandeln durch die Hallen", "tokens": ["Sie", "wan\u00b7deln", "durch", "die", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An's goldne Tageslicht.", "tokens": ["An's", "gold\u00b7ne", "Ta\u00b7ges\u00b7licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und in dem Saale winket", "tokens": ["Und", "in", "dem", "Saa\u00b7le", "win\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein herrliches Gelag,", "tokens": ["Ein", "herr\u00b7li\u00b7ches", "Ge\u00b7lag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es dampfet und es blinket,", "tokens": ["Es", "damp\u00b7fet", "und", "es", "blin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was nur das Land vermag.", "tokens": ["Was", "nur", "das", "Land", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es satzten sich die F\u00fcrsten;", "tokens": ["Es", "satz\u00b7ten", "sich", "die", "F\u00fcrs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da mocht' es seltsam sein!", "tokens": ["Da", "mocht'", "es", "selt\u00b7sam", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hungern und sie d\u00fcrsten", "tokens": ["Sie", "hun\u00b7gern", "und", "sie", "d\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Braten und beim Wein;", "tokens": ["Beim", "Bra\u00b7ten", "und", "beim", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbnun, will's euch nicht behagen?", "tokens": ["\u00bb", "nun", ",", "will's", "euch", "nicht", "be\u00b7ha\u00b7gen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es fehlt doch, deucht mir, nichts?", "tokens": ["Es", "fehlt", "doch", ",", "deucht", "mir", ",", "nichts", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "$,", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wor\u00fcber ist zu klagen?", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "ist", "zu", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An was, ihr Herrn, gebricht's?", "tokens": ["An", "was", ",", "ihr", "Herrn", ",", "ge\u00b7bricht's", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es schickt zu meinem Tische", "tokens": ["Es", "schickt", "zu", "mei\u00b7nem", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Odenwald das Schwein,", "tokens": ["Der", "O\u00b7den\u00b7wald", "das", "Schwein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Neckar seine Fische,", "tokens": ["Der", "Ne\u00b7ckar", "sei\u00b7ne", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den frommen Trank der Rhein!", "tokens": ["Den", "from\u00b7men", "Trank", "der", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr habt ja sonst erfahren,", "tokens": ["Ihr", "habt", "ja", "sonst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was meine Pfalz bescheert!", "tokens": ["Was", "mei\u00b7ne", "Pfalz", "be\u00b7scheert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was wollt ihr heute sparen,", "tokens": ["Was", "wollt", "ihr", "heu\