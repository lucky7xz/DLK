{"textgrid.poem.52141": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pan Tata h\u00f6rt man dich zu deinem Vater schreyn,", "tokens": ["Pan", "Ta\u00b7ta", "h\u00f6rt", "man", "dich", "zu", "dei\u00b7nem", "Va\u00b7ter", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deiner Mutter zu drauff Pani Mami ruffen,", "tokens": ["Und", "dei\u00b7ner", "Mut\u00b7ter", "zu", "drauff", "Pa\u00b7ni", "Ma\u00b7mi", "ruf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie solt du denn, wenn ich die Eltern so getroffen,", "tokens": ["Wie", "solt", "du", "denn", ",", "wenn", "ich", "die", "El\u00b7tern", "so", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht eine feige Memm und bl\u00f6der Tater seyn?", "tokens": ["Nicht", "ei\u00b7ne", "fei\u00b7ge", "Memm", "und", "bl\u00f6\u00b7der", "Ta\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alex