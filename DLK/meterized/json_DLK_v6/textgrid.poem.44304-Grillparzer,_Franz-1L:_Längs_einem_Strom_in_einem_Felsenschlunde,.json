{"textgrid.poem.44304": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4ngs einem Strom in einem Felsenschlunde,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ngs einem Strom in einem Felsenschlunde,", "tokens": ["L\u00e4ngs", "ei\u00b7nem", "Strom", "in", "ei\u00b7nem", "Fel\u00b7sen\u00b7schlun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging einst ein Edelmann,", "tokens": ["Ging", "einst", "ein", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihn umh\u00fcpften seine beiden Hunde:", "tokens": ["Und", "ihn", "um\u00b7h\u00fcpf\u00b7ten", "sei\u00b7ne", "bei\u00b7den", "Hun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Joli und Soliman.", "tokens": ["Jo\u00b7li", "und", "So\u00b7li\u00b7man", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Joli, das Windspiel, sprang mit tausend Possen", "tokens": ["Jo\u00b7li", ",", "das", "Wind\u00b7spiel", ",", "sprang", "mit", "tau\u00b7send", "Pos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "CARD", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hinan an seinen Herrn,", "tokens": ["Hi\u00b7nan", "an", "sei\u00b7nen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wird gek\u00fc\u00dft, indessen steht versto\u00dfen", "tokens": ["Und", "wird", "ge\u00b7k\u00fc\u00dft", ",", "in\u00b7des\u00b7sen", "steht", "ver\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der arme Pudel fern,", "tokens": ["Der", "ar\u00b7me", "Pu\u00b7del", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den armen liebt man nicht, er kann nicht schmeicheln,", "tokens": ["Den", "ar\u00b7men", "liebt", "man", "nicht", ",", "er", "kann", "nicht", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "PTKNEG", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu finster ist sein Blick,", "tokens": ["Zu", "fins\u00b7ter", "ist", "sein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und statt den treuen, wie Joli, zu streicheln,", "tokens": ["Und", "statt", "den", "treu\u00b7en", ",", "wie", "Jo\u00b7li", ",", "zu", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "PWAV", "NE", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "St\u00f6\u00dft man ihn stets zur\u00fcck.", "tokens": ["St\u00f6\u00dft", "man", "ihn", "stets", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Nun aber wankt der Herr am steilen Strande", "tokens": ["Nun", "a\u00b7ber", "wankt", "der", "Herr", "am", "stei\u00b7len", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ungewissem Fu\u00df", "tokens": ["Mit", "un\u00b7ge\u00b7wis\u00b7sem", "Fu\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und st\u00fcrzet pl\u00f6tzlich von dem glatten Rande", "tokens": ["Und", "st\u00fcr\u00b7zet", "pl\u00f6tz\u00b7lich", "von", "dem", "glat\u00b7ten", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Abgrunds in den Flu\u00df.", "tokens": ["Des", "Ab\u00b7grunds", "in", "den", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Indes Joli mit Furcht und bangem Bellen", "tokens": ["In\u00b7des", "Jo\u00b7li", "mit", "Furcht", "und", "ban\u00b7gem", "Bel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Am hohen Ufer steht,", "tokens": ["Am", "ho\u00b7hen", "U\u00b7fer", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich in dem Silberspiegel glatter Wellen", "tokens": ["Sich", "in", "dem", "Sil\u00b7ber\u00b7spie\u00b7gel", "glat\u00b7ter", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begaffet und dann geht,", "tokens": ["Be\u00b7gaf\u00b7fet", "und", "dann", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "St\u00fcrzt sich der brave, stets verschm\u00e4hte Pudel", "tokens": ["St\u00fcrzt", "sich", "der", "bra\u00b7ve", ",", "stets", "ver\u00b7schm\u00e4h\u00b7te", "Pu\u00b7del"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "$,", "ADV", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hinab vom hohen Strand,", "tokens": ["Hin\u00b7ab", "vom", "ho\u00b7hen", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entrei\u00dfet m\u00fchsam seinen Herrn dem Strudel", "tokens": ["Ent\u00b7rei\u00b7\u00dfet", "m\u00fch\u00b7sam", "sei\u00b7nen", "Herrn", "dem", "Stru\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tr\u00e4gt ihn froh ans Land.", "tokens": ["Und", "tr\u00e4gt", "ihn", "froh", "ans", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O m\u00f6ge diese kleine Fabel lehren,", "tokens": ["O", "m\u00f6\u00b7ge", "die\u00b7se", "klei\u00b7ne", "Fa\u00b7bel", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PDAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft der Schein bel\u00fcgt,", "tokens": ["Wie", "oft", "der", "Schein", "be\u00b7l\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur die Gefahr kann einen Freund bew\u00e4hren,", "tokens": ["Nur", "die", "Ge\u00b7fahr", "kann", "ei\u00b7nen", "Freund", "be\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Au\u00dfenseite tr\u00fcgt.", "tokens": ["Die", "Au\u00b7\u00dfen\u00b7sei\u00b7te", "tr\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr Weltenherrscher hasset nicht den Braven,", "tokens": ["Ihr", "Wel\u00b7ten\u00b7herr\u00b7scher", "has\u00b7set", "nicht", "den", "Bra\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil er nicht niedrig kriecht,", "tokens": ["Weil", "er", "nicht", "nied\u00b7rig", "kriecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der erste eurer tiefgeb\u00fcckten Sklaven", "tokens": ["Der", "ers\u00b7te", "eu\u00b7rer", "tief\u00b7ge\u00b7b\u00fcck\u00b7ten", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist oft ein B\u00f6sewicht.", "tokens": ["Ist", "oft", "ein", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}