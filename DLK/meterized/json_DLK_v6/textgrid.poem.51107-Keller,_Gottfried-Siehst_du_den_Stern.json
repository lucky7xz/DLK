{"textgrid.poem.51107": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Siehst du den Stern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du den Stern im fernsten Blau,", "tokens": ["Siehst", "du", "den", "Stern", "im", "ferns\u00b7ten", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der flimmernd fast erbleicht?", "tokens": ["Der", "flim\u00b7mernd", "fast", "er\u00b7bleicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Licht braucht eine Ewigkeit,", "tokens": ["Sein", "Licht", "braucht", "ei\u00b7ne", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis es dein Aug erreicht!", "tokens": ["Bis", "es", "dein", "Aug", "er\u00b7reicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vielleicht vor tausend Jahren schon", "tokens": ["Viel\u00b7leicht", "vor", "tau\u00b7send", "Jah\u00b7ren", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Asche stob der Stern;", "tokens": ["Zu", "A\u00b7sche", "stob", "der", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch steht dort sein milder Schein", "tokens": ["Und", "doch", "steht", "dort", "sein", "mil\u00b7der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer still und fern.", "tokens": ["Noch", "im\u00b7mer", "still", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Wesen solchen Scheines gleicht,", "tokens": ["Dem", "We\u00b7sen", "sol\u00b7chen", "Schei\u00b7nes", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist und doch nicht ist,", "tokens": ["Der", "ist", "und", "doch", "nicht", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KON", "ADV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Lieb, dein anmutvolles Sein,", "tokens": ["O", "Lieb", ",", "dein", "an\u00b7mut\u00b7vol\u00b7les", "Sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du gestorben bist!", "tokens": ["Wenn", "du", "ge\u00b7stor\u00b7ben", "bist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}