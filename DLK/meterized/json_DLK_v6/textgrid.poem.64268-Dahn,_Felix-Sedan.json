{"textgrid.poem.64268": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Sedan", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00e4lzet blut'ge Wellen hin die Maas,", "tokens": ["Es", "w\u00e4l\u00b7zet", "blut'\u00b7ge", "Wel\u00b7len", "hin", "die", "Maas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es krachen tausend Feuerschl\u00fcnde von", "tokens": ["Es", "kra\u00b7chen", "tau\u00b7send", "Feu\u00b7er\u00b7schl\u00fcn\u00b7de", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den H\u00f6hn herab zu Tal! Jetzt auch im Norden!", "tokens": ["Den", "H\u00f6hn", "her\u00b7ab", "zu", "Tal", "!", "Jetzt", "auch", "im", "Nor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$.", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort bei Givonne! \u2013 Geschlossen ist der Ring:", "tokens": ["Dort", "bei", "Gi\u00b7von\u00b7ne", "!", "\u2013", "Ge\u00b7schlos\u00b7sen", "ist", "der", "Ring", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "$(", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hie Preu\u00dfen, Bayern, Sachsen, W\u00fcrttemberger!", "tokens": ["Hie", "Preu\u00b7\u00dfen", ",", "Bay\u00b7ern", ",", "Sach\u00b7sen", ",", "W\u00fcrt\u00b7tem\u00b7ber\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NE", "$,", "NE", "$,", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein F\u00fchrer und Ein Wille und Ein Sieg!", "tokens": ["Ein", "F\u00fch\u00b7rer", "und", "Ein", "Wil\u00b7le", "und", "Ein", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es sinkt der Tag. \u2013 Aus Sonnengold gew\u00f6lbt,", "tokens": ["Es", "sinkt", "der", "Tag", ".", "\u2013", "Aus", "Son\u00b7nen\u00b7gold", "ge\u00b7w\u00f6lbt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Welch' strahlend Bild! 's ist eine Kaiserkrone!", "tokens": ["Welch'", "strah\u00b7lend", "Bild", "!", "'s", "ist", "ei\u00b7ne", "Kai\u00b7ser\u00b7kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}