{"textgrid.poem.43157": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Abendbild", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grau der Himmel, grau die Erde,", "tokens": ["Grau", "der", "Him\u00b7mel", ",", "grau", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grau das weite d\u00fcrre Land,", "tokens": ["Grau", "das", "wei\u00b7te", "d\u00fcr\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonn'verbrannte nied're Str\u00e4ucher,", "tokens": ["Sonn'\u00b7ver\u00b7brann\u00b7te", "nie\u00b7d'\u00b7re", "Str\u00e4u\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schwarzer Sumpf und hei\u00dfer Sand;", "tokens": ["Schwar\u00b7zer", "Sumpf", "und", "hei\u00b7\u00dfer", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch schon weben in der Ferne", "tokens": ["Doch", "schon", "we\u00b7ben", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abendnebel, d\u00fcnn' und leicht,", "tokens": ["A\u00b7bend\u00b7ne\u00b7bel", ",", "d\u00fcnn'", "und", "leicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Ihre grauen feuchten Schleier", "tokens": ["Ih\u00b7re", "grau\u00b7en", "feuch\u00b7ten", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die tr\u00e4ge Stille weicht.", "tokens": ["Und", "die", "tr\u00e4\u00b7ge", "Stil\u00b7le", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ein mildes k\u00fchles L\u00fcftchen,", "tokens": ["Denn", "ein", "mil\u00b7des", "k\u00fch\u00b7les", "L\u00fcft\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der reine Athemzug", "tokens": ["Wie", "der", "rei\u00b7ne", "A\u00b7them\u00b7zug"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines schlafumfang'nen Kindes,", "tokens": ["Ei\u00b7nes", "schlaf\u00b7um\u00b7fang'\u00b7nen", "Kin\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hemmt der V\u00f6gel matten Flug.", "tokens": ["Hemmt", "der", "V\u00f6\u00b7gel", "mat\u00b7ten", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus den B\u00fcschen, still sich regend,", "tokens": ["Aus", "den", "B\u00fc\u00b7schen", ",", "still", "sich", "re\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VMFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein geheimes Fl\u00fcstern bricht,", "tokens": ["Ein", "ge\u00b7hei\u00b7mes", "Fl\u00fcs\u00b7tern", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise klagt's im Sumpf und silbern", "tokens": ["Lei\u00b7se", "klagt's", "im", "Sumpf", "und", "sil\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spiegelt sich das Mondenlicht. \u2013", "tokens": ["Spie\u00b7gelt", "sich", "das", "Mon\u00b7den\u00b7licht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}