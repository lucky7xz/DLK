{"textgrid.poem.35173": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Selbstbetrug", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo gehst du hin! Du bist auf falschen Pfaden", "tokens": ["Wo", "gehst", "du", "hin", "!", "Du", "bist", "auf", "fal\u00b7schen", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4ssest dich von einem Mund berathen,", "tokens": ["Und", "l\u00e4s\u00b7sest", "dich", "von", "ei\u00b7nem", "Mund", "be\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der auf die Frage nach der Wahrheit schweigt.", "tokens": ["Der", "auf", "die", "Fra\u00b7ge", "nach", "der", "Wahr\u00b7heit", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du hast weit mehr als nur dein eignes Leben", "tokens": ["Du", "hast", "weit", "mehr", "als", "nur", "dein", "eig\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PIAT", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vertrauensvoll in eine Hand gegeben,", "tokens": ["Ver\u00b7trau\u00b7ens\u00b7voll", "in", "ei\u00b7ne", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die dir das Ziel in falscher Richtung zeigt.", "tokens": ["Die", "dir", "das", "Ziel", "in", "fal\u00b7scher", "Rich\u00b7tung", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du merkst es nicht, da\u00df dich der Irrthum leitet", "tokens": ["Du", "merkst", "es", "nicht", ",", "da\u00df", "dich", "der", "Irr\u00b7thum", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mit dir nach verborgnen Tiefen schreitet.", "tokens": ["Und", "mit", "dir", "nach", "ver\u00b7borg\u00b7nen", "Tie\u00b7fen", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hast du nicht auch nach Pylos zu gelangen,", "tokens": ["Hast", "du", "nicht", "auch", "nach", "Py\u00b7los", "zu", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Kunde dort vom Vater zu empfangen,", "tokens": ["Um", "Kun\u00b7de", "dort", "vom", "Va\u00b7ter", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie einstens Telemach von Ithaka?", "tokens": ["Wie", "eins\u00b7tens", "Te\u00b7le\u00b7mach", "von", "I\u00b7tha\u00b7ka", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wei\u00dft du nicht mehr, da\u00df ihn die Weisheit f\u00fchrte", "tokens": ["Wei\u00dft", "du", "nicht", "mehr", ",", "da\u00df", "ihn", "die", "Weis\u00b7heit", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und da\u00df er ihre Hand gehorsam sp\u00fcrte,", "tokens": ["Und", "da\u00df", "er", "ih\u00b7re", "Hand", "ge\u00b7hor\u00b7sam", "sp\u00fcr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Obgleich er nicht als Himmlische sie sah?", "tokens": ["Ob\u00b7gleich", "er", "nicht", "als", "Himm\u00b7li\u00b7sche", "sie", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dort mu\u00dfte sie sich \u00e4u\u00dferlich gestalten;", "tokens": ["Dort", "mu\u00df\u00b7te", "sie", "sich", "\u00e4u\u00b7\u00dfer\u00b7lich", "ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr dich darf sie sich in dir selbst entfalten.", "tokens": ["F\u00fcr", "dich", "darf", "sie", "sich", "in", "dir", "selbst", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PRF", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Drum traue nicht dem Au\u00dfen, nicht den Sinnen;", "tokens": ["Drum", "trau\u00b7e", "nicht", "dem", "Au\u00b7\u00dfen", ",", "nicht", "den", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Richt alle Sorgen um dein Heil nach innen,", "tokens": ["Richt", "al\u00b7le", "Sor\u00b7gen", "um", "dein", "Heil", "nach", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "APPR", "ADV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Denn nur das Herz h\u00f6rt, was der Vater spricht.", "tokens": ["Denn", "nur", "das", "Herz", "h\u00f6rt", ",", "was", "der", "Va\u00b7ter", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und will ein fremder Ton dies schmeichelnd r\u00fcgen,", "tokens": ["Und", "will", "ein", "frem\u00b7der", "Ton", "dies", "schmei\u00b7chelnd", "r\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PDS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So trachtet dich ein Glei\u00dfner zu betr\u00fcgen;", "tokens": ["So", "trach\u00b7tet", "dich", "ein", "Glei\u00df\u00b7ner", "zu", "be\u00b7tr\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei klug, und folge dieser Stimme nicht!", "tokens": ["Sei", "klug", ",", "und", "fol\u00b7ge", "die\u00b7ser", "Stim\u00b7me", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "VVFIN", "PDAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur die Verf\u00fchrung kann das Kinn dir streicheln;", "tokens": ["Nur", "die", "Ver\u00b7f\u00fch\u00b7rung", "kann", "das", "Kinn", "dir", "strei\u00b7cheln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Wahrheit aber wird dir niemals schmeicheln.", "tokens": ["Die", "Wahr\u00b7heit", "a\u00b7ber", "wird", "dir", "nie\u00b7mals", "schmei\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}