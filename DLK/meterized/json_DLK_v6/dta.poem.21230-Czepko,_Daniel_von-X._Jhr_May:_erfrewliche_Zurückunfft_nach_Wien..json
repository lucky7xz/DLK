{"dta.poem.21230": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Jhr May:  \n erfrewliche Zur\u00fcckunfft nach Wien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Schawt vmb den rothen Thurn gantz Oesterreich da stehn/", "tokens": ["Schawt", "vmb", "den", "ro\u00b7then", "Thurn", "gantz", "O\u00b7es\u00b7ter\u00b7reich", "da", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "NE", "ADV", "VVINF", "$("], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da\u00df schon bi\u00df an die ", "tokens": ["Da\u00df", "schon", "bi\u00df", "an", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Es schallt der gantze Marckt; merckt auff! ", "tokens": ["Es", "schallt", "der", "gant\u00b7ze", "Marckt", ";", "merckt", "auff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kommen;", "tokens": ["Sie", "kom\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die Stadt wil vor das Thor mit jhren H\u00e4usern gehn.", "tokens": ["Die", "Stadt", "wil", "vor", "das", "Thor", "mit", "jhren", "H\u00e4u\u00b7sern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch was seht Jhr euch vmb? ", "tokens": ["Doch", "was", "seht", "Ihr", "euch", "vmb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Picken d\u00fcrrer Wald/ der staub von Pferde sohlen/", "tokens": ["Der", "Pi\u00b7cken", "d\u00fcr\u00b7rer", "Wald", "/", "der", "staub", "von", "Pfer\u00b7de", "soh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wei\u00df Seine ", "tokens": ["Wei\u00df", "Sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wie sehr vor Gl\u00fcck zu brennt de\u00df Pflasters helle Bahn.", "tokens": ["Wie", "sehr", "vor", "Gl\u00fcck", "zu", "brennt", "de\u00df", "Pflas\u00b7ters", "hel\u00b7le", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "PTKZU", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ob man das Deutsche Reich sieht vnterm Throne kniehn;", "tokens": ["Ob", "man", "das", "Deut\u00b7sche", "Reich", "sieht", "vn\u00b7term", "Thro\u00b7ne", "kniehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob selbst der Wahltag eilt; Ob die Triones weichen:", "tokens": ["Ob", "selbst", "der", "Wahl\u00b7tag", "eilt", ";", "Ob", "die", "Tri\u00b7o\u00b7nes", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$.", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch kommet ", "tokens": ["Doch", "kom\u00b7met"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Vnd also pflegt ein ", "tokens": ["Vnd", "al\u00b7so", "pflegt", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}}}}