{"dta.poem.2996": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob Himmlische das Leid zu deinem Besten senden?", "tokens": ["Ob", "Himm\u00b7li\u00b7sche", "das", "Leid", "zu", "dei\u00b7nem", "Bes\u00b7ten", "sen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu deinem Besten sollst du wenigstens es wenden.", "tokens": ["Zu", "dei\u00b7nem", "Bes\u00b7ten", "sollst", "du", "we\u00b7nigs\u00b7tens", "es", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu deinem Besten hast du aber es gewandt,", "tokens": ["Zu", "dei\u00b7nem", "Bes\u00b7ten", "hast", "du", "a\u00b7ber", "es", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du es dazu glaubst von Himmlischen gesandt.", "tokens": ["Wenn", "du", "es", "da\u00b7zu", "glaubst", "von", "Himm\u00b7li\u00b7schen", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PAV", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}