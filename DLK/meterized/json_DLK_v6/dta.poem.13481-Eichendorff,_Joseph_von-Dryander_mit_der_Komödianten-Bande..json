{"dta.poem.13481": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Dryander mit der Kom\u00f6dianten-Bande.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mich brennt's an meinen Reiseschuh'n,", "tokens": ["Mich", "brennt's", "an", "mei\u00b7nen", "Rei\u00b7se\u00b7schuh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fort mit der Zeit zu schreiten \u2014", "tokens": ["Fort", "mit", "der", "Zeit", "zu", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Was wollen wir agiren nun", "tokens": ["Was", "wol\u00b7len", "wir", "a\u00b7gi\u00b7ren", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor so viel klugen Leuten?", "tokens": ["Vor", "so", "viel", "klu\u00b7gen", "Leu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Es hebt das Dach sich von dem Haus", "tokens": ["Es", "hebt", "das", "Dach", "sich", "von", "dem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die Koulissen r\u00fchren", "tokens": ["Und", "die", "Kou\u00b7lis\u00b7sen", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und strecken sich zum Himmel raus,", "tokens": ["Und", "stre\u00b7cken", "sich", "zum", "Him\u00b7mel", "raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Strom, W\u00e4lder musiziren!", "tokens": ["Strom", ",", "W\u00e4l\u00b7der", "mu\u00b7si\u00b7zi\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und aus den Wolken langt es sacht,", "tokens": ["Und", "aus", "den", "Wol\u00b7ken", "langt", "es", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt alles durcheinander,", "tokens": ["Stellt", "al\u00b7les", "durch\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sich's kein Autor hat gedacht:", "tokens": ["Wie", "sich's", "kein", "Au\u00b7tor", "hat", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Volk, F\u00fcrsten und Dryander.", "tokens": ["Volk", ",", "F\u00fcrs\u00b7ten", "und", "Dry\u00b7an\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da gehn die einen m\u00fcde fort,", "tokens": ["Da", "gehn", "die", "ei\u00b7nen", "m\u00fc\u00b7de", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die andern nah'n behende,", "tokens": ["Die", "an\u00b7dern", "nah'n", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das alte St\u00fcck, man spielt's so fort", "tokens": ["Das", "al\u00b7te", "St\u00fcck", ",", "man", "spielt's", "so", "fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PIS", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kriegt es nie zu Ende.", "tokens": ["Und", "kriegt", "es", "nie", "zu", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und keiner kennt den letzten Akt", "tokens": ["Und", "kei\u00b7ner", "kennt", "den", "letz\u00b7ten", "Akt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen die da spielen,", "tokens": ["Von", "al\u00b7len", "die", "da", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur der da droben schl\u00e4gt den Takt,", "tokens": ["Nur", "der", "da", "dro\u00b7ben", "schl\u00e4gt", "den", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df, wo das hin will zielen.", "tokens": ["Wei\u00df", ",", "wo", "das", "hin", "will", "zie\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}