{"textgrid.poem.47758": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es hat die Flur ein Sturm verheert]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Flur ein Sturm verheert,", "tokens": ["Es", "hat", "die", "Flur", "ein", "Sturm", "ver\u00b7heert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All ihre Bl\u00fcthen abgekehrt \u2013", "tokens": ["All", "ih\u00b7re", "Bl\u00fc\u00b7then", "ab\u00b7ge\u00b7kehrt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein schreckliches Gestiebe!", "tokens": ["Ein", "schreck\u00b7li\u00b7ches", "Ge\u00b7stie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lilien indessen,", "tokens": ["Die", "Li\u00b7li\u00b7en", "in\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Rosen und Cypressen", "tokens": ["Die", "Ro\u00b7sen", "und", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Der Sch\u00f6nheit und der Liebe,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "und", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gottlob, sie blieben unversehrt.", "tokens": ["Gott\u00b7lob", ",", "sie", "blie\u00b7ben", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}