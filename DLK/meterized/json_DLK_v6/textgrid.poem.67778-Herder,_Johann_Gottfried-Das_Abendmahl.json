{"textgrid.poem.67778": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Abendmahl", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er sprach's und wollte scheiden:", "tokens": ["Er", "sprach's", "und", "woll\u00b7te", "schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbwie, Br\u00fcder, lieb' ich Euch!", "tokens": ["\u00bb", "wie", ",", "Br\u00fc\u00b7der", ",", "lieb'", "ich", "Euch", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Bis uns in Gottes Reich", "tokens": ["Bis", "uns", "in", "Got\u00b7tes", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach M\u00fch und Blut und Streite", "tokens": ["Nach", "M\u00fch", "und", "Blut", "und", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Empf\u00e4ngt ein Labemahl,", "tokens": ["Emp\u00b7f\u00e4ngt", "ein", "La\u00b7be\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Genie\u00dft an Freundes Seite", "tokens": ["Ge\u00b7nie\u00dft", "an", "Freun\u00b7des", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das letzte Freundesmahl!\u00ab", "tokens": ["Das", "letz\u00b7te", "Freun\u00b7des\u00b7mahl", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sprach's, und Herz und Liebe", "tokens": ["Er", "sprach's", ",", "und", "Herz", "und", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umgaben All' ihn da!", "tokens": ["Um\u00b7ga\u00b7ben", "All'", "ihn", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verkl\u00e4rt in Gottes Liebe", "tokens": ["Ver\u00b7kl\u00e4rt", "in", "Got\u00b7tes", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie Jesus Christus sah:", "tokens": ["Sie", "Je\u00b7sus", "Chris\u00b7tus", "sah", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbwie ich geliebt Euch habe,", "tokens": ["\u00bb", "wie", "ich", "ge\u00b7liebt", "Euch", "ha\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "VVPP", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Liebt ewig, ewig Euch!", "tokens": ["Liebt", "e\u00b7wig", ",", "e\u00b7wig", "Euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie ich Euch jetzo labe,", "tokens": ["Wie", "ich", "Euch", "jet\u00b7zo", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Labt einst uns Gottes Reich.\u00ab", "tokens": ["Labt", "einst", "uns", "Got\u00b7tes", "Reich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "PPER", "NN", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Er sprach von Blutvergie\u00dfen,", "tokens": ["Er", "sprach", "von", "Blut\u00b7ver\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Lieben bis ins Grab:", "tokens": ["Von", "Lie\u00b7ben", "bis", "ins", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Blut mu\u00df s\u00f6hnend flie\u00dfen,", "tokens": ["\u00bb", "mein", "Blut", "mu\u00df", "s\u00f6h\u00b7nend", "flie\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Leben bl\u00fchen ab!", "tokens": ["Mein", "Le\u00b7ben", "bl\u00fc\u00b7hen", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seht, Euch zu Trost und Muthe,", "tokens": ["Seht", ",", "Euch", "zu", "Trost", "und", "Mu\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Seht hier Euch ewig Mahl,", "tokens": ["Seht", "hier", "Euch", "e\u00b7wig", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Bund mit meinem Blute,", "tokens": ["Den", "Bund", "mit", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Feier meiner Qual!", "tokens": ["Die", "Fei\u00b7er", "mei\u00b7ner", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Leib! mein Blut! genie\u00dfet", "tokens": ["Mein", "Leib", "!", "mein", "Blut", "!", "ge\u00b7nie\u00b7\u00dfet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier ewig meine Kraft!", "tokens": ["Hier", "e\u00b7wig", "mei\u00b7ne", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Freundes Blut, es flie\u00dfet", "tokens": ["Des", "Freun\u00b7des", "Blut", ",", "es", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Eures Lebens Saft.", "tokens": ["Zu", "Eu\u00b7res", "Le\u00b7bens", "Saft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald wird sich Alles wenden;", "tokens": ["Bald", "wird", "sich", "Al\u00b7les", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Getrost! ich bin bei Euch,", "tokens": ["Ge\u00b7trost", "!", "ich", "bin", "bei", "Euch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hin zu der Welten Enden", "tokens": ["Hin", "zu", "der", "Wel\u00b7ten", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Bis hin in Gottes Reich!\u00ab", "tokens": ["Bis", "hin", "in", "Got\u00b7tes", "Reich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er sprach's und ging zum Leiden", "tokens": ["Er", "sprach's", "und", "ging", "zum", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom letzten Liebesmahl;", "tokens": ["Vom", "letz\u00b7ten", "Lie\u00b7bes\u00b7mahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ewig nach dem Scheiden", "tokens": ["Und", "e\u00b7wig", "nach", "dem", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward es ", "tokens": ["Ward", "es"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ein Mahl der Lieb' und Thr\u00e4nen,", "tokens": ["Ein", "Mahl", "der", "Lieb'", "und", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Freundschaft in den Tod,", "tokens": ["Der", "Freund\u00b7schaft", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Voll Wehmuth, Wonn' und Sehnen", "tokens": ["Voll", "Weh\u00b7muth", ",", "Wonn'", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Labsal hin zu Gott.", "tokens": ["Und", "Lab\u00b7sal", "hin", "zu", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auf Paradieses Auen", "tokens": ["Auf", "Pa\u00b7ra\u00b7die\u00b7ses", "Au\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umarmt er fern sie schon!", "tokens": ["Um\u00b7armt", "er", "fern", "sie", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Kelche konnten schauen", "tokens": ["Vom", "Kel\u00b7che", "konn\u00b7ten", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie auf zu Gottes Thron,", "tokens": ["Sie", "auf", "zu", "Got\u00b7tes", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo einst, nach M\u00fch und Streite", "tokens": ["Wo", "einst", ",", "nach", "M\u00fch", "und", "Strei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Blut und Kampf und Qual,", "tokens": ["Und", "Blut", "und", "Kampf", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An ihres Freundes Seite", "tokens": ["An", "ih\u00b7res", "Freun\u00b7des", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Empf\u00e4ngt sie Freudemahl.", "tokens": ["Emp\u00b7f\u00e4ngt", "sie", "Freu\u00b7de\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}