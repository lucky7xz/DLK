{"textgrid.poem.65159": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die hier im dunkeln Grabesscho\u00dfe ruht,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die hier im dunkeln Grabesscho\u00dfe ruht,", "tokens": ["Die", "hier", "im", "dun\u00b7keln", "Gra\u00b7bes\u00b7scho\u00b7\u00dfe", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach langen Kampfes M\u00fchsal und Beschwerde,", "tokens": ["Nach", "lan\u00b7gen", "Kamp\u00b7fes", "M\u00fch\u00b7sal", "und", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie jedes andre arme Kind der Erde", "tokens": ["Wie", "je\u00b7des", "and\u00b7re", "ar\u00b7me", "Kind", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War sie ein Doppellaut von Schlimm und Gut.", "tokens": ["War", "sie", "ein", "Dop\u00b7pel\u00b7laut", "von", "Schlimm", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts unterschied sie von der gro\u00dfen Schar,", "tokens": ["Nichts", "un\u00b7ter\u00b7schied", "sie", "von", "der", "gro\u00b7\u00dfen", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Behaglich atmend in der L\u00fcge Brodem,", "tokens": ["Be\u00b7hag\u00b7lich", "at\u00b7mend", "in", "der", "L\u00fc\u00b7ge", "Bro\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als da\u00df die Wahrheit ihrer Seele Odem,", "tokens": ["Als", "da\u00df", "die", "Wahr\u00b7heit", "ih\u00b7rer", "See\u00b7le", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df getreu bis in den Tod sie war.", "tokens": ["Und", "da\u00df", "ge\u00b7treu", "bis", "in", "den", "Tod", "sie", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "KON", "APPR", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}