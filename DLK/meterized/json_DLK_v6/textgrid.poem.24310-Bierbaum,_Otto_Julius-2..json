{"textgrid.poem.24310": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zypressenholz und Lorbeerzweige", "tokens": ["Zyp\u00b7res\u00b7sen\u00b7holz", "und", "Lor\u00b7beer\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verknistern im Kamine. Duft", "tokens": ["Ver\u00b7knis\u00b7tern", "im", "Ka\u00b7mi\u00b7ne", ".", "Duft"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "APPRART", "NN", "$.", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Von glimmenden Pinien\u00e4pfeln zieht in", "tokens": ["Von", "glim\u00b7men\u00b7den", "Pi\u00b7ni\u00b7en\u00b7\u00e4p\u00b7feln", "zieht", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Hellblauen Streifen durch die Luft.", "tokens": ["Hell\u00b7blau\u00b7en", "Strei\u00b7fen", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die offene T\u00fcre l\u00e4\u00dft vom Garten", "tokens": ["Die", "of\u00b7fe\u00b7ne", "T\u00fc\u00b7re", "l\u00e4\u00dft", "vom", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den k\u00fchlen Hauch der Nacht herein.", "tokens": ["Den", "k\u00fch\u00b7len", "Hauch", "der", "Nacht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es rascheln die Magnolienbl\u00e4tter", "tokens": ["Es", "ra\u00b7scheln", "die", "Mag\u00b7no\u00b7li\u00b7en\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und blinken matt im Mondenschein.", "tokens": ["Und", "blin\u00b7ken", "matt", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Tisch ist gelb vom Licht der Lampe;", "tokens": ["Mein", "Tisch", "ist", "gelb", "vom", "Licht", "der", "Lam\u00b7pe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr gruner Seidenschleier h\u00e4lt", "tokens": ["Ihr", "gru\u00b7ner", "Sei\u00b7den\u00b7schlei\u00b7er", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Zimmer mir im halben Dunkel.", "tokens": ["Das", "Zim\u00b7mer", "mir", "im", "hal\u00b7ben", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oh: wollustvoll ist meine Welt.", "tokens": ["Oh", ":", "wol\u00b7lust\u00b7voll", "ist", "mei\u00b7ne", "Welt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}