{"textgrid.poem.37287": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[w\u00e4rst du ein B\u00e4chlein, ich ein Bach]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rst du ein B\u00e4chlein, ich ein Bach,", "tokens": ["W\u00e4rst", "du", "ein", "B\u00e4ch\u00b7lein", ",", "ich", "ein", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So eilt ich dir geschwinde nach.", "tokens": ["So", "eilt", "ich", "dir", "ge\u00b7schwin\u00b7de", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich dich gefunden h\u00e4tt'", "tokens": ["Und", "wenn", "ich", "dich", "ge\u00b7fun\u00b7den", "h\u00e4tt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem Blumenuferbett:", "tokens": ["In", "dei\u00b7nem", "Blu\u00b7me\u00b7nu\u00b7fer\u00b7bett", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wollt ich mich in dich ergie\u00dfen", "tokens": ["Wie", "wollt", "ich", "mich", "in", "dich", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ganz mit dir zusammenflie\u00dfen,", "tokens": ["Und", "ganz", "mit", "dir", "zu\u00b7sam\u00b7men\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du vielgeliebtes M\u00e4dchen du!", "tokens": ["Du", "viel\u00b7ge\u00b7lieb\u00b7tes", "M\u00e4d\u00b7chen", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann str\u00f6mten wir bei Nacht und Tage", "tokens": ["Dann", "str\u00f6m\u00b7ten", "wir", "bei", "Nacht", "und", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vereint in s\u00fc\u00dfem Wellenschlage", "tokens": ["Ver\u00b7eint", "in", "s\u00fc\u00b7\u00dfem", "Wel\u00b7len\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem Meere zu.", "tokens": ["Dem", "Mee\u00b7re", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}