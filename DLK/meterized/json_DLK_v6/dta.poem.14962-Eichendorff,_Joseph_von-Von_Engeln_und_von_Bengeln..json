{"dta.poem.14962": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die einen konnten nicht fliegen,", "tokens": ["Die", "ei\u00b7nen", "konn\u00b7ten", "nicht", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wohlleibig, tr\u00e4g' und schwer,", "tokens": ["So", "wohl\u00b7lei\u00b7big", ",", "tr\u00e4g'", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mu\u00dft' Er da lassen liegen,", "tokens": ["Die", "mu\u00dft'", "Er", "da", "las\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Das that ihm leid so sehr.", "tokens": ["Das", "that", "ihm", "leid", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}