{"dta.poem.10167": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine Art Aloe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn and're Bluhmen welck, und matt und Krafft-los\nwerden,", "tokens": ["Wenn", "an\u00b7d'\u00b7re", "Bluh\u00b7men", "welck", ",", "und", "matt", "und", "Krafft\u00b7los", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKVZ", "$,", "KON", "ADJD", "KON", "NN", "VAINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So biegen sie das Haupt, und sencken sich zur Erden.", "tokens": ["So", "bie\u00b7gen", "sie", "das", "Haupt", ",", "und", "sen\u00b7cken", "sich", "zur", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch diese hub, so bald sie ihre Krafft verlohr,", "tokens": ["Doch", "die\u00b7se", "hub", ",", "so", "bald", "sie", "ih\u00b7re", "Krafft", "ver\u00b7lohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bald sie welckete, ihr lechzend Haupt empor.", "tokens": ["So", "bald", "sie", "wel\u00b7cke\u00b7te", ",", "ihr", "lech\u00b7zend", "Haupt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kaum sah\u2019 ich die\u00df, so dacht ich: liebste Bluhme,", "tokens": ["Kaum", "sah'", "ich", "die\u00df", ",", "so", "dacht", "ich", ":", "liebs\u00b7te", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "$,", "ADV", "VVFIN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bl\u00fchetest nicht nur zu deines Sch\u00f6pfers Ruhme;", "tokens": ["Du", "bl\u00fc\u00b7he\u00b7test", "nicht", "nur", "zu", "dei\u00b7nes", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bl\u00fchetest auch nicht f\u00fcr mich vergebens.", "tokens": ["Du", "bl\u00fc\u00b7he\u00b7test", "auch", "nicht", "f\u00fcr", "mich", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sollt, am Ende meines Lebens,", "tokens": ["Du", "sollt", ",", "am", "En\u00b7de", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit GOttes H\u00fclffe mir, selbst in der Todes-Pein,", "tokens": ["Mit", "Got\u00b7tes", "H\u00fclf\u00b7fe", "mir", ",", "selbst", "in", "der", "To\u00b7des\u00b7Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Auch ein belehrend Lehr-Bild seyn.", "tokens": ["Auch", "ein", "be\u00b7leh\u00b7rend", "Lehr\u00b7Bild", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}