{"textgrid.poem.33665": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Kleines Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie geht in aller Fr\u00fche,", "tokens": ["Sie", "geht", "in", "al\u00b7ler", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch eh die D\u00e4mmrung schwand,", "tokens": ["Noch", "eh", "die", "D\u00e4mm\u00b7rung", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Weg zur Tagesm\u00fche", "tokens": ["Den", "Weg", "zur", "Ta\u00b7ges\u00b7m\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im \u00e4rmlichen Gewand.", "tokens": ["Im", "\u00e4rm\u00b7li\u00b7chen", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die dunklen Nebel feuchten", "tokens": ["Die", "dunk\u00b7len", "Ne\u00b7bel", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch in der Stra\u00dfe dicht,", "tokens": ["Noch", "in", "der", "Stra\u00b7\u00dfe", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sonst s\u00e4he man beleuchten", "tokens": ["Sonst", "s\u00e4\u00b7he", "man", "be\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein L\u00e4cheln ihr Gesicht.", "tokens": ["Ein", "L\u00e4\u00b7cheln", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die G\u00f6tter m\u00f6gen wissen,", "tokens": ["Die", "G\u00f6t\u00b7ter", "m\u00f6\u00b7gen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Warum sie heimlich lacht \u2013", "tokens": ["Wa\u00b7rum", "sie", "heim\u00b7lich", "lacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Es wei\u00df es nur das Kissen,", "tokens": ["Es", "wei\u00df", "es", "nur", "das", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Was ihr getr\u00e4umt heut Nacht.", "tokens": ["Was", "ihr", "ge\u00b7tr\u00e4umt", "heut", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}