{"textgrid.poem.40135": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Zweifel", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt ist es aus! jetzt bin ich ", "tokens": ["Jetzt", "ist", "es", "aus", "!", "jetzt", "bin", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun brach vom Herzen mir das letzte kleine", "tokens": ["Nun", "brach", "vom", "Her\u00b7zen", "mir", "das", "letz\u00b7te", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "St\u00fcck Hoffnung noch, das letzte eine", "tokens": ["St\u00fcck", "Hoff\u00b7nung", "noch", ",", "das", "letz\u00b7te", "ei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "$,", "ART", "ADJA", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "gerettete aus meinen Bitternissen.", "tokens": ["ge\u00b7ret\u00b7te\u00b7te", "aus", "mei\u00b7nen", "Bit\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wimmernd zerbarst die letzte Saite", "tokens": ["Wim\u00b7mernd", "zer\u00b7barst", "die", "letz\u00b7te", "Sai\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "des Glaubens mir an Gl\u00fcck und Fried:", "tokens": ["des", "Glau\u00b7bens", "mir", "an", "Gl\u00fcck", "und", "Fried", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der Jammer heult in mir sein marternd Lied,", "tokens": ["der", "Jam\u00b7mer", "heult", "in", "mir", "sein", "mar\u00b7ternd", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und gramverw\u00fcstet starr' ich in die Weite.", "tokens": ["und", "gram\u00b7ver\u00b7w\u00fcs\u00b7tet", "starr'", "ich", "in", "die", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hinaus, hinaus, wo keine ", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", ",", "wo", "kei\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PWAV", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O raffte mich empor ein gl\u00fchender Wind,", "tokens": ["O", "raff\u00b7te", "mich", "em\u00b7por", "ein", "gl\u00fc\u00b7hen\u00b7der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "verschl\u00fcg' er mein Gebein verdorrt in Steppen,", "tokens": ["ver\u00b7schl\u00fcg'", "er", "mein", "Ge\u00b7bein", "ver\u00b7dorrt", "in", "Step\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "durch die aashungrig nur der Schakal schleicht!", "tokens": ["durch", "die", "aas\u00b7hung\u00b7rig", "nur", "der", "Scha\u00b7kal", "schleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ich kann die Last nicht weiterschleppen ...", "tokens": ["ich", "kann", "die", "Last", "nicht", "wei\u00b7ter\u00b7schlep\u00b7pen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In des Fr\u00fchlingswaldes dichteste Schatten", "tokens": ["In", "des", "Fr\u00fch\u00b7lings\u00b7wal\u00b7des", "dich\u00b7tes\u00b7te", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "fl\u00fcchte hinaus, mein Leid, mit mir!", "tokens": ["fl\u00fcch\u00b7te", "hin\u00b7aus", ",", "mein", "Leid", ",", "mit", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "in die bangesten Schluchten will ich mich bergen,", "tokens": ["in", "die", "ban\u00b7ge\u00b7sten", "Schluch\u00b7ten", "will", "ich", "mich", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "da\u00df ich allein sei, allein mit ", "tokens": ["da\u00df", "ich", "al\u00b7lein", "sei", ",", "al\u00b7lein", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "bis du endlich magst ermatten,", "tokens": ["bis", "du", "end\u00b7lich", "magst", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "bis ich auch ", "tokens": ["bis", "ich", "auch"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "schichten kann, die im Grabgew\u00f6lbe", "tokens": ["schich\u00b7ten", "kann", ",", "die", "im", "Grab\u00b7ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "VMFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "meines tiefdumpfeinsamen Busens stehn.", "tokens": ["mei\u00b7nes", "tief\u00b7dum\u00b7pfein\u00b7sa\u00b7men", "Bu\u00b7sens", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Oh, ", "tokens": ["Oh", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "k\u00fchnen Wunsches, seligen Mutes voll,", "tokens": ["k\u00fch\u00b7nen", "Wun\u00b7sches", ",", "se\u00b7li\u00b7gen", "Mu\u00b7tes", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "mit Adlersfittigen, \u00fcber steile Pfade,", "tokens": ["mit", "Ad\u00b7lers\u00b7fit\u00b7ti\u00b7gen", ",", "\u00fc\u00b7ber", "stei\u00b7le", "Pfa\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "zu Dir, Dir, Sonne meines Lebens\u00ab \u2013 \u2013", "tokens": ["zu", "Dir", ",", "Dir", ",", "Son\u00b7ne", "mei\u00b7nes", "Le\u00b7bens", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "$,", "NN", "PPOSAT", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O wie tr\u00e4umt' ich s\u00fc\u00df,", "tokens": ["O", "wie", "tr\u00e4umt'", "ich", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "ich Wahnverz\u00fcckter,", "tokens": ["ich", "Wahn\u00b7ver\u00b7z\u00fcck\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "versunken in ihren Strahlenblick,", "tokens": ["ver\u00b7sun\u00b7ken", "in", "ih\u00b7ren", "Strah\u00b7len\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "in das goldene Sonnenauge:", "tokens": ["in", "das", "gol\u00b7de\u00b7ne", "Son\u00b7nen\u00b7au\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u00bbbin ich nicht ", "tokens": ["\u00bb", "bin", "ich", "nicht"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "st\u00e4rker als ich ", "tokens": ["st\u00e4r\u00b7ker", "als", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "PPER"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.7": {"text": "Sonnenauge, Du ", "tokens": ["Son\u00b7nen\u00b7au\u00b7ge", ",", "Du"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Sonnenseele, ", "tokens": ["Son\u00b7nen\u00b7see\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Sonne, dich fassen! Sonne, dich halten!", "tokens": ["Son\u00b7ne", ",", "dich", "fas\u00b7sen", "!", "Son\u00b7ne", ",", "dich", "hal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "VVINF", "$.", "NN", "$,", "PRF", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Sonne, f\u00fcr und f\u00fcr", "tokens": ["Son\u00b7ne", ",", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "KON", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "deine reine Flamme lassen walten", "tokens": ["dei\u00b7ne", "rei\u00b7ne", "Flam\u00b7me", "las\u00b7sen", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "durch die flackernde lodernde Seele mir!", "tokens": ["durch", "die", "fla\u00b7ckern\u00b7de", "lo\u00b7dern\u00b7de", "See\u00b7le", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "PPER", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Ueber Gipfel mich heben! auf Wolken schreiten!", "tokens": ["Ue\u00b7ber", "Gip\u00b7fel", "mich", "he\u00b7ben", "!", "auf", "Wol\u00b7ken", "schrei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$.", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Sicher dich tragend, von ", "tokens": ["Si\u00b7cher", "dich", "tra\u00b7gend", ",", "von"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VVPP", "$,", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "zum Doppelgestirn uns zusammenschlie\u00dfen!", "tokens": ["zum", "Dop\u00b7pel\u00b7ges\u00b7tirn", "uns", "zu\u00b7sam\u00b7men\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Spr\u00fchende Funken nach unten spreiten", "tokens": ["Spr\u00fc\u00b7hen\u00b7de", "Fun\u00b7ken", "nach", "un\u00b7ten", "sprei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ADV", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "den Erdens\u00f6hnen, den Menschenbr\u00fcdern!", "tokens": ["den", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", ",", "den", "Men\u00b7schen\u00b7br\u00fc\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Rauschend auf sausendem Feuerwagen,", "tokens": ["Rau\u00b7schend", "auf", "sau\u00b7sen\u00b7dem", "Feu\u00b7er\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Sonne dich singen, Sonne dich sagen", "tokens": ["Son\u00b7ne", "dich", "sin\u00b7gen", ",", "Son\u00b7ne", "dich", "sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "NN", "PPER", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.20": {"text": "in ewigen Lauten, ewigen Worten, ewigen Liedern!\u00ab", "tokens": ["in", "e\u00b7wi\u00b7gen", "Lau\u00b7ten", ",", "e\u00b7wi\u00b7gen", "Wor\u00b7ten", ",", "e\u00b7wi\u00b7gen", "Lie\u00b7dern", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "\u2013 \u2013 ", "tokens": ["\u2013", "\u2013"], "token_info": ["punct", "punct"], "pos": ["$(", "$("]}}, "stanza.6": {"line.1": {"text": "Nein! nein! sah ich sie l\u00e4cheln", "tokens": ["Nein", "!", "nein", "!", "sah", "ich", "sie", "l\u00e4\u00b7cheln"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "VVFIN", "PPER", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "unger\u00fchrt, ungetr\u00fcbt:", "tokens": ["un\u00b7ge\u00b7r\u00fchrt", ",", "un\u00b7ge\u00b7tr\u00fcbt", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Verzichte, verzichte!", "tokens": ["Ver\u00b7zich\u00b7te", ",", "ver\u00b7zich\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "sonst wirst du zunichte", "tokens": ["sonst", "wirst", "du", "zu\u00b7nich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "an deiner ", "tokens": ["an", "dei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Nein, ich f\u00fchl's: ich bin ", "tokens": ["Nein", ",", "ich", "f\u00fchl's", ":", "ich", "bin"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "konnte sie, ach, nicht an mich rei\u00dfen", "tokens": ["konn\u00b7te", "sie", ",", "ach", ",", "nicht", "an", "mich", "rei\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ITJ", "$,", "PTKNEG", "APPR", "PPER", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "mit meinen Blicken, den schwellend hei\u00dfen!", "tokens": ["mit", "mei\u00b7nen", "Bli\u00b7cken", ",", "den", "schwel\u00b7lend", "hei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sengend nur mich selbst erschlaffen! \u2013", "tokens": ["sen\u00b7gend", "nur", "mich", "selbst", "er\u00b7schlaf\u00b7fen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Winselnder Thor, der nicht vermocht", "tokens": ["Win\u00b7seln\u00b7der", "Thor", ",", "der"