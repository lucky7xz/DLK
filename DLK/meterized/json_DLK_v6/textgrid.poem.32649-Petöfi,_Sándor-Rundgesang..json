{"textgrid.poem.32649": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Rundgesang.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohne Freud' ist schal das Leben,", "tokens": ["Oh\u00b7ne", "Freud'", "ist", "schal", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kann nur der Wein uns geben;", "tokens": ["Die", "kann", "nur", "der", "Wein", "uns", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Darum leeren wir im raschen", "tokens": ["Da\u00b7rum", "lee\u00b7ren", "wir", "im", "ra\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zuge zwanzig, drei\u00dfig Flaschen.", "tokens": ["Zu\u00b7ge", "zwan\u00b7zig", ",", "drei\u00b7\u00dfig", "Fla\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "CARD", "$,", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Haben wir auch Haus und Keller,", "tokens": ["Ha\u00b7ben", "wir", "auch", "Haus", "und", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Schenke schmeckt es heller,", "tokens": ["In", "der", "Schen\u00b7ke", "schmeckt", "es", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum bleiben beim Getr\u00e4nke", "tokens": ["Da\u00b7rum", "blei\u00b7ben", "beim", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tag und nachts wir in der Schenke.", "tokens": ["Tag", "und", "nachts", "wir", "in", "der", "Schen\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier ist's, wo wir uns erlaben,", "tokens": ["Hier", "ist's", ",", "wo", "wir", "uns", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weil daheim wir Weiber haben.", "tokens": ["Weil", "da\u00b7heim", "wir", "Wei\u00b7ber", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scharfe Zungen sind sie alle,", "tokens": ["Schar\u00b7fe", "Zun\u00b7gen", "sind", "sie", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, da gibt's nur Gift und Galle!", "tokens": ["Ei", ",", "da", "gibt's", "nur", "Gift", "und", "Gal\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was wir Erspartes haben,", "tokens": ["Und", "was", "wir", "Er\u00b7spar\u00b7tes", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VAFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Mu\u00df man's \u00e4ngstlich denn vergraben?", "tokens": ["Mu\u00df", "man's", "\u00e4ngst\u00b7lich", "denn", "ver\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Geld wird's auch nicht schlimmer,", "tokens": ["Oh\u00b7ne", "Geld", "wird's", "auch", "nicht", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Tode lebt man immer!", "tokens": ["Bis", "zum", "To\u00b7de", "lebt", "man", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darum wacker fortgetrunken", "tokens": ["Da\u00b7rum", "wa\u00b7cker", "fort\u00b7ge\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis wir in das Grab gesunken!", "tokens": ["Bis", "wir", "in", "das", "Grab", "ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer Wein und nie Gez\u00e4nke,", "tokens": ["Im\u00b7mer", "Wein", "und", "nie", "Ge\u00b7z\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinken mu\u00df man in der Schenke!", "tokens": ["Trin\u00b7ken", "mu\u00df", "man", "in", "der", "Schen\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}