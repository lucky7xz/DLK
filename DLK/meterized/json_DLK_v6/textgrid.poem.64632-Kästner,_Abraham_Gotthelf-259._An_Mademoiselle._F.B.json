{"textgrid.poem.64632": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "259. An Mademoiselle. F.B", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch auf dein Wohl ein fr\u00f6hlich Glas zu leeren,", "tokens": ["Noch", "auf", "dein", "Wohl", "ein", "fr\u00f6h\u00b7lich", "Glas", "zu", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst du von mir, Entfernte, nicht begehren:", "tokens": ["Wirst", "du", "von", "mir", ",", "Ent\u00b7fern\u00b7te", ",", "nicht", "be\u00b7geh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bey der Erinnerung, von dir getrennt zu seyn,", "tokens": ["Bey", "der", "E\u00b7rin\u00b7ne\u00b7rung", ",", "von", "dir", "ge\u00b7trennt", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "W\u00e4r' mir Burgunder Wermuthwein.", "tokens": ["W\u00e4r'", "mir", "Bur\u00b7gun\u00b7der", "Wer\u00b7muth\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}