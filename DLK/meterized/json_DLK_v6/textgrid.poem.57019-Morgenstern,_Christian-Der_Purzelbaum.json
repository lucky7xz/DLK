{"textgrid.poem.57019": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Purzelbaum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Purzelbaum trat vor mich hin", "tokens": ["Ein", "Pur\u00b7zel\u00b7baum", "trat", "vor", "mich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sagt: \u00bbDu nur siehst mich", "tokens": ["und", "sagt", ":", "\u00bb", "Du", "nur", "siehst", "mich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und wei\u00dft, was f\u00fcr ein Baum ich bin:", "tokens": ["und", "wei\u00dft", ",", "was", "f\u00fcr", "ein", "Baum", "ich", "bin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schie\u00dfe nicht, man schie\u00dft mich.", "tokens": ["Ich", "schie\u00b7\u00dfe", "nicht", ",", "man", "schie\u00dft", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und trag ich Frucht? Ich glaube kaum;", "tokens": ["Und", "trag", "ich", "Frucht", "?", "Ich", "glau\u00b7be", "kaum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auch bin ich nicht verwurzelt.", "tokens": ["auch", "bin", "ich", "nicht", "ver\u00b7wur\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nur noch ein Purzeltraum,", "tokens": ["Ich", "bin", "nur", "noch", "ein", "Pur\u00b7zel\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sobald ich hingepurzelt.\u00ab", "tokens": ["so\u00b7bald", "ich", "hin\u00b7ge\u00b7pur\u00b7zelt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbje nun\u00ab, so sprach ich, \u00bbbester Schatz,", "tokens": ["\u00bb", "je", "nun", "\u00ab", ",", "so", "sprach", "ich", ",", "\u00bb", "bes\u00b7ter", "Schatz", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du bist doch klug und siehst uns; \u2013", "tokens": ["du", "bist", "doch", "klug", "und", "siehst", "uns", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "nun, auch f\u00fcr uns besteht der Satz:", "tokens": ["nun", ",", "auch", "f\u00fcr", "uns", "be\u00b7steht", "der", "Satz", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wir schie\u00dfen nicht, es schie\u00dft uns.", "tokens": ["wir", "schie\u00b7\u00dfen", "nicht", ",", "es", "schie\u00dft", "uns", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch Wurzeln treibt man nicht so bald,", "tokens": ["Auch", "Wur\u00b7zeln", "treibt", "man", "nicht", "so", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PIS", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Fr\u00fcchte nun erst recht nicht.", "tokens": ["und", "Fr\u00fcch\u00b7te", "nun", "erst", "recht", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geh heim in deinen Purzelwald,", "tokens": ["Geh", "heim", "in", "dei\u00b7nen", "Pur\u00b7zel\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und l\u00e4stre dein Geschlecht nicht.\u00ab", "tokens": ["und", "l\u00e4st\u00b7re", "dein", "Ge\u00b7schlecht", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}