{"textgrid.poem.44035": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bedeute doch nur dein Gem\u00fcthe", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bedeute doch nur dein Gem\u00fcthe", "tokens": ["Be\u00b7deu\u00b7te", "doch", "nur", "dein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00e4um es der Vergn\u00fcgung ein.", "tokens": ["Und", "r\u00e4um", "es", "der", "Ver\u00b7gn\u00fc\u00b7gung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Himmel kommt ja nichts als G\u00fcte,", "tokens": ["Vom", "Him\u00b7mel", "kommt", "ja", "nichts", "als", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Zorn mu\u00df unser Nuzen seyn.", "tokens": ["Sein", "Zorn", "mu\u00df", "un\u00b7ser", "Nu\u00b7zen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ruthen thun dem Fleische bange,", "tokens": ["Die", "Ru\u00b7then", "thun", "dem", "Flei\u00b7sche", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schmerzen \u2013 aber ach, wie lange?", "tokens": ["Sie", "schmer\u00b7zen", "\u2013", "a\u00b7ber", "ach", ",", "wie", "lan\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADV", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als unser Wahn nicht weiter denckt.", "tokens": ["Als", "un\u00b7ser", "Wahn", "nicht", "wei\u00b7ter", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sobald wir mit Vernunft und Gr\u00fcnden", "tokens": ["So\u00b7bald", "wir", "mit", "Ver\u00b7nunft", "und", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den guten Zweck der Strafe finden,", "tokens": ["Den", "gu\u00b7ten", "Zweck", "der", "Stra\u00b7fe", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sobald ist unsre Lust in Creuzkelch eingeschenckt.", "tokens": ["So\u00b7bald", "ist", "uns\u00b7re", "Lust", "in", "Creuz\u00b7kelch", "ein\u00b7ge\u00b7schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was schadet uns der Menschen Richten?", "tokens": ["Was", "scha\u00b7det", "uns", "der", "Men\u00b7schen", "Rich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Gottes Urtheil kommt es an:", "tokens": ["Auf", "Got\u00b7tes", "Ur\u00b7theil", "kommt", "es", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer mag wohl dem von uns was dichten,", "tokens": ["Wer", "mag", "wohl", "dem", "von", "uns", "was", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "APPR", "PPER", "PIS", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herz und Nieren pr\u00fcfen kan?", "tokens": ["Der", "Herz", "und", "Nie\u00b7ren", "pr\u00fc\u00b7fen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bem\u00fch dich um ein rein Gewi\u00dfen;", "tokens": ["Be\u00b7m\u00fch", "dich", "um", "ein", "rein", "Ge\u00b7wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es wird uns doch gelingen m\u00fc\u00dfen,", "tokens": ["Es", "wird", "uns", "doch", "ge\u00b7lin\u00b7gen", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So starck auch der Verfolger scheint.", "tokens": ["So", "starck", "auch", "der", "Ver\u00b7fol\u00b7ger", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir haben unsern Schaz in H\u00e4nden;", "tokens": ["Wir", "ha\u00b7ben", "un\u00b7sern", "Schaz", "in", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wer will wohl dem das Gl\u00fcck entwenden,", "tokens": ["Wer", "will", "wohl", "dem", "das", "Gl\u00fcck", "ent\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der in sich selber f\u00fchlt, er hab es gut gemeint?", "tokens": ["Der", "in", "sich", "sel\u00b7ber", "f\u00fchlt", ",", "er", "hab", "es", "gut", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gesezt, wir sollen unsre Jahre,", "tokens": ["Ge\u00b7sezt", ",", "wir", "sol\u00b7len", "uns\u00b7re", "Jah\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jezo unter Dornen bl\u00fchn,", "tokens": ["Die", "je\u00b7zo", "un\u00b7ter", "Dor\u00b7nen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von nun an bis zur Todtenbaare", "tokens": ["Von", "nun", "an", "bis", "zur", "Tod\u00b7ten\u00b7baa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In eu\u00dferlicher Noth vollziehn,", "tokens": ["In", "eu\u00b7\u00dfer\u00b7li\u00b7cher", "Noth", "voll\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was w\u00fcrden wir bey viel Beschwerden", "tokens": ["Was", "w\u00fcr\u00b7den", "wir", "bey", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Groll und Gram gebe\u00dfert werden?", "tokens": ["Durch", "Groll", "und", "Gram", "ge\u00b7be\u00b7\u00dfert", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir machten erst die Noth zur Noth.", "tokens": ["Wir", "mach\u00b7ten", "erst", "die", "Noth", "zur", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich schweige von den Wei\u00dfheitssch\u00e4zen,", "tokens": ["Ich", "schwei\u00b7ge", "von", "den", "Wei\u00df\u00b7heits\u00b7sch\u00e4\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die allen Mangel reich ersezen", "tokens": ["Die", "al\u00b7len", "Man\u00b7gel", "reich", "er\u00b7se\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und starcke Wafen sind, wenn Feind und Ungl\u00fcck droht.", "tokens": ["Und", "star\u00b7cke", "Wa\u00b7fen", "sind", ",", "wenn", "Feind", "und", "Un\u00b7gl\u00fcck", "droht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VAFIN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}