{"textgrid.poem.65075": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wann zerrei\u00dft der Nebelschleier,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wann zerrei\u00dft der Nebelschleier,", "tokens": ["O", "wann", "zer\u00b7rei\u00dft", "der", "Ne\u00b7bel\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Licht und Sonne mir entzieht?", "tokens": ["Der", "Licht", "und", "Son\u00b7ne", "mir", "ent\u00b7zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann strahlet Auferstehungsfeier", "tokens": ["Wann", "strah\u00b7let", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7fei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem still in mir versargten Lied?", "tokens": ["Dem", "still", "in", "mir", "ver\u00b7sarg\u00b7ten", "Lied", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann kehrt von dunklen Irrefahrten", "tokens": ["Wann", "kehrt", "von", "dunk\u00b7len", "Ir\u00b7re\u00b7fahr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur\u00fcck die Seele, br\u00e4utlich froh?", "tokens": ["Zu\u00b7r\u00fcck", "die", "See\u00b7le", ",", "br\u00e4ut\u00b7lich", "froh", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann wird die W\u00fcste mir zum Garten?", "tokens": ["Wann", "wird", "die", "W\u00fcs\u00b7te", "mir", "zum", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wann seh' ich dich, Ottavio?", "tokens": ["Wann", "seh'", "ich", "dich", ",", "Ot\u00b7ta\u00b7vio", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In dieser einen Frage schwinden", "tokens": ["In", "die\u00b7ser", "ei\u00b7nen", "Fra\u00b7ge", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die andern alle, alle hin!", "tokens": ["Die", "an\u00b7dern", "al\u00b7le", ",", "al\u00b7le", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIS", "$,", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schw\u00e4nden mir im Wiederfinden", "tokens": ["So", "schw\u00e4n\u00b7den", "mir", "im", "Wie\u00b7der\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens Gram und sein Gewinn,", "tokens": ["Des", "Le\u00b7bens", "Gram", "und", "sein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn meiner Tage Sonnenhelle", "tokens": ["Denn", "mei\u00b7ner", "Ta\u00b7ge", "Son\u00b7nen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein lebenspendend Ostern, wo", "tokens": ["Mein", "le\u00b7ben\u00b7spen\u00b7dend", "Os\u00b7tern", ",", "wo"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich neu ergie\u00dft des Liedes Quelle", "tokens": ["Sich", "neu", "er\u00b7gie\u00dft", "des", "Lie\u00b7des", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du bist es, mein Ottavio!", "tokens": ["Du", "bist", "es", ",", "mein", "Ot\u00b7ta\u00b7vio", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPOSAT", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Vergolten hast du mit Verderben", "tokens": ["Ver\u00b7gol\u00b7ten", "hast", "du", "mit", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe, die du nie erkannt,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "du", "nie", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, wie des Pilgers, ist im Sterben,", "tokens": ["Doch", ",", "wie", "des", "Pil\u00b7gers", ",", "ist", "im", "Ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Antlitz dir noch zugewandt!", "tokens": ["Mein", "Ant\u00b7litz", "dir", "noch", "zu\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie die welke Bl\u00fcth' am Strauche", "tokens": ["Und", "wie", "die", "wel\u00b7ke", "Bl\u00fcth'", "am", "Strau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Lenze spricht, der l\u00e4ngst entfloh,", "tokens": ["Vom", "Len\u00b7ze", "spricht", ",", "der", "l\u00e4ngst", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So t\u00f6nt aus meinem letzten Hauche", "tokens": ["So", "t\u00f6nt", "aus", "mei\u00b7nem", "letz\u00b7ten", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Name noch: Ottavio!", "tokens": ["Dein", "Na\u00b7me", "noch", ":", "Ot\u00b7ta\u00b7vio", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$.", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}