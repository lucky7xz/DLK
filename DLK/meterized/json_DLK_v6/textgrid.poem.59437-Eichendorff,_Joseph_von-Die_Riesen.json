{"textgrid.poem.59437": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Riesen", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch \u00fcber blauen Bergen", "tokens": ["Hoch", "\u00fc\u00b7ber", "blau\u00b7en", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht ein sch\u00f6nes Schlo\u00df,", "tokens": ["Da", "steht", "ein", "sch\u00f6\u00b7nes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das h\u00fctet von Gezwergen", "tokens": ["Das", "h\u00fc\u00b7tet", "von", "Ge\u00b7zwer\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wunderlicher Tro\u00df.", "tokens": ["Ein", "wun\u00b7der\u00b7li\u00b7cher", "Tro\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da ist ein Lautenschlagen", "tokens": ["Da", "ist", "ein", "Lau\u00b7ten\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Singen insgemein,", "tokens": ["Und", "Sin\u00b7gen", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die L\u00fcfte es vertragen", "tokens": ["Die", "L\u00fcf\u00b7te", "es", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weit in das Land hinein.", "tokens": ["Weit", "in", "das", "Land", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn die L\u00e4nder schweigen,", "tokens": ["Und", "wenn", "die", "L\u00e4n\u00b7der", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Funkelnd im Abendtau,", "tokens": ["Fun\u00b7kelnd", "im", "A\u00b7bend\u00b7tau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Soll manchmal dort sich zeigen", "tokens": ["Soll", "manch\u00b7mal", "dort", "sich", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eine wundersch\u00f6ne Frau.", "tokens": ["Ei\u00b7ne", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da schworen alle Riesen,", "tokens": ["Da", "schwo\u00b7ren", "al\u00b7le", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu holen sie als Braut,", "tokens": ["Zu", "ho\u00b7len", "sie", "als", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Leitern da und Spie\u00dfen", "tokens": ["Mit", "Lei\u00b7tern", "da", "und", "Spie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie stapften gleich durchs Kraut.", "tokens": ["Sie", "stapf\u00b7ten", "gleich", "durchs", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da krachte manche Leiter,", "tokens": ["Da", "krach\u00b7te", "man\u00b7che", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie wunderten sich sehr:", "tokens": ["Sie", "wun\u00b7der\u00b7ten", "sich", "sehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Die Wildnis wuchs, je weiter", "tokens": ["Die", "Wild\u00b7nis", "wuchs", ",", "je", "wei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Je h\u00f6her rings umher.", "tokens": ["Je", "h\u00f6\u00b7her", "rings", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie waren recht bei Stimme", "tokens": ["Sie", "wa\u00b7ren", "recht", "bei", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zankten um ihren Schatz,", "tokens": ["Und", "zank\u00b7ten", "um", "ih\u00b7ren", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und fluchten in gro\u00dfem Grimme,", "tokens": ["Und", "fluch\u00b7ten", "in", "gro\u00b7\u00dfem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und fanden nicht den Platz.", "tokens": ["Und", "fan\u00b7den", "nicht", "den", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und bei dem L\u00e4rm sie stunden", "tokens": ["Und", "bei", "dem", "L\u00e4rm", "sie", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Wolken bis an die Knie,", "tokens": ["In", "Wol\u00b7ken", "bis", "an", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Schlo\u00df, das war verschwunden,", "tokens": ["Das", "Schlo\u00df", ",", "das", "war", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wu\u00dften gar nicht wie. \u2013", "tokens": ["Und", "wu\u00df\u00b7ten", "gar", "nicht", "wie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Aber wie ein Regenbogen", "tokens": ["A\u00b7ber", "wie", "ein", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4nzt's droben durch die Luft,", "tokens": ["Gl\u00e4nzt's", "dro\u00b7ben", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Neue G\u00e4rten in den Duft.", "tokens": ["Neu\u00b7e", "G\u00e4r\u00b7ten", "in", "den", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}