{"textgrid.poem.34200": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Den Verstand in Ehren", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selbst die Musik beruht zuletzt auf Zahl und Verh\u00e4ltni\u00df,", "tokens": ["Selbst", "die", "Mu\u00b7sik", "be\u00b7ruht", "zu\u00b7letzt", "auf", "Zahl", "und", "Ver\u00b7h\u00e4lt\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+----+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und du schiltst den Verstand, wenn er im Drama sich zeigt?", "tokens": ["Und", "du", "schiltst", "den", "Ver\u00b7stand", ",", "wenn", "er", "im", "Dra\u00b7ma", "sich", "zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+--++--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Jegliche Frage gestatt' ihm, doch keine einzige Antwort,", "tokens": ["Jeg\u00b7li\u00b7che", "Fra\u00b7ge", "ge\u00b7statt'", "ihm", ",", "doch", "kei\u00b7ne", "ein\u00b7zi\u00b7ge", "Ant\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und du erk\u00e4ltest dein Bild nimmer, du l\u00e4uterst es nur,", "tokens": ["Und", "du", "er\u00b7k\u00e4l\u00b7test", "dein", "Bild", "nim\u00b7mer", ",", "du", "l\u00e4u\u00b7terst", "es", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Denn die Phantasie wird wieder und wieder sich regen,", "tokens": ["Denn", "die", "Phan\u00b7ta\u00b7sie", "wird", "wie\u00b7der", "und", "wie\u00b7der", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "KON", "ADV", "PRF", "ADJA", "$,"], "meter": "---+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wenn er die schlummernde weckt, bis sie ihm v\u00f6llig gen\u00fcgt.", "tokens": ["Wenn", "er", "die", "schlum\u00b7mern\u00b7de", "weckt", ",", "bis", "sie", "ihm", "v\u00f6l\u00b7lig", "ge\u00b7n\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}