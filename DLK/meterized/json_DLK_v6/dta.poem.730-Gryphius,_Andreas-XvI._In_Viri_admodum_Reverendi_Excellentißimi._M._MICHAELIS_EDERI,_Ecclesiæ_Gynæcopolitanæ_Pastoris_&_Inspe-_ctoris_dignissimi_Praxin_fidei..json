{"dta.poem.730": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  In Viri admodum Reverendi Excellenti\u00dfimi.  \n M.  MICHAELIS EDERI,  \n Ecclesi\u00e6 Gyn\u00e6copolitan\u00e6 Pastoris & Inspe-  \n ctoris dignissimi Praxin fidei.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jm ende dieser welt/ nun Trew vnd Lieb erkalt ", "tokens": ["Jm", "en\u00b7de", "die\u00b7ser", "welt", "/", "nun", "Trew", "vnd", "Lieb", "er\u00b7kalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PDAT", "NN", "$(", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun wahre Redligkeit/ vnd Tugend will verschwinden/", "tokens": ["Nun", "wah\u00b7re", "Red\u00b7lig\u00b7keit", "/", "vnd", "Tu\u00b7gend", "will", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun man vom Christenthumb schier nichts kan \u00fcbrig finden/", "tokens": ["Nun", "man", "vom", "Chris\u00b7ten\u00b7thumb", "schier", "nichts", "kan", "\u00fcb\u00b7rig", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPRART", "NN", "ADV", "PIS", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun Heiligkeit/ vnd Recht vnd Gottesfurcht veralt/", "tokens": ["Nun", "Hei\u00b7lig\u00b7keit", "/", "vnd", "Recht", "vnd", "Got\u00b7tes\u00b7furcht", "ver\u00b7alt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "KON", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verdr\u00fcckt jhr wie jhr m\u00f6gt/ die rasende gewalt", "tokens": ["Ver\u00b7dr\u00fcckt", "jhr", "wie", "jhr", "m\u00f6gt", "/", "die", "ra\u00b7sen\u00b7de", "ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "PPER", "VMFIN", "$(", "ART", "ADJA", "NN"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die alles vberfelt/ die starcke macht der S\u00fcnden.", "tokens": ["Die", "al\u00b7les", "vber\u00b7felt", "/", "die", "star\u00b7cke", "macht", "der", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$(", "ART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jhr zeiget/ was mit GOtt die Menschen kan verbinden", "tokens": ["Ihr", "zei\u00b7get", "/", "was", "mit", "Gott", "die", "Men\u00b7schen", "kan", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PWS", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Glaubens eigenschafft/ vnd lebende gestalt. ", "tokens": ["Des", "Glau\u00b7bens", "ei\u00b7gen\u00b7schafft", "/", "vnd", "le\u00b7ben\u00b7de", "ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Jhr lehret das vmbsonst/ nichts thun/ vnd alles wissen;", "tokens": ["Ihr", "leh\u00b7ret", "das", "vmbsonst", "/", "nichts", "thun", "/", "vnd", "al\u00b7les", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PIS", "VVINF", "$(", "KON", "PIS", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df leiden/ da\u00df Gedult den Himmel auff kan s", "tokens": ["Da\u00df", "lei\u00b7den", "/", "da\u00df", "Ge\u00b7dult", "den", "Him\u00b7mel", "auff", "kan", "s"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVINF", "$(", "KOUS", "NN", "ART", "NN", "APPR", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd da\u00df der nicht ein ", "tokens": ["Vnd", "da\u00df", "der", "nicht", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "PTKNEG", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wie selig seid jhr doch weil euch die Ehrenkrone/", "tokens": ["Wie", "se\u00b7lig", "seid", "jhr", "doch", "weil", "euch", "die", "Eh\u00b7ren\u00b7kro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wissen nicht allein er langt f\u00fcr Gottes Throne", "tokens": ["Die", "wis\u00b7sen", "nicht", "al\u00b7lein", "er", "langt", "f\u00fcr", "Got\u00b7tes", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "PPER", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Printz der Ewigkeit schon w\u00fcrcklich vbergibt.", "tokens": ["Der", "Printz", "der", "E\u00b7wig\u00b7keit", "schon", "w\u00fcrck\u00b7lich", "vberg\u00b7ibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}