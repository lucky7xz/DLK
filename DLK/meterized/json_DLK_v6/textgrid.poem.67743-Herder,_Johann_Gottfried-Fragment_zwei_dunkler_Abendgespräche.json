{"textgrid.poem.67743": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Fragment zwei dunkler Abendgespr\u00e4che", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Empfindung sprach sie \u2013 schrieb sie \u2013 soll sie lesen!", "tokens": ["Emp\u00b7fin\u00b7dung", "sprach", "sie", "\u2013", "schrieb", "sie", "\u2013", "soll", "sie", "le\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Vater \u2013 Dein Gespr\u00e4ch \u2013 die Leyer mein! \u2013", "tokens": ["Dein", "Va\u00b7ter", "\u2013", "Dein", "Ge\u00b7spr\u00e4ch", "\u2013", "die", "Le\u00b7yer", "mein", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Freundschaftskreis webt heil'ges Dunkel ein;", "tokens": ["Der", "Freund\u00b7schafts\u00b7kreis", "webt", "heil'\u00b7ges", "Dun\u00b7kel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer braucht's zu sehn, wer ich und Du gewesen!", "tokens": ["Wer", "braucht's", "zu", "sehn", ",", "wer", "ich", "und", "Du", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKZU", "VVINF", "$,", "PWS", "PPER", "KON", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.6": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.2": {"line.1": {"text": "Und wie er starb, da halber S\u00e4clen Schmerzen", "tokens": ["Und", "wie", "er", "starb", ",", "da", "hal\u00b7ber", "S\u00e4\u00b7clen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vereint ihn fa\u00dften, innres Nerven\u00f6l", "tokens": ["Ver\u00b7eint", "ihn", "fa\u00df\u00b7ten", ",", "inn\u00b7res", "Ner\u00b7ve\u00b7n\u00f6l"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Blut auskochten und herauf zum Herzen", "tokens": ["Wie", "Blut", "aus\u00b7koch\u00b7ten", "und", "her\u00b7auf", "zum", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Sieger donnerten, die seine gro\u00dfe Seel'", "tokens": ["Als", "Sie\u00b7ger", "don\u00b7ner\u00b7ten", ",", "die", "sei\u00b7ne", "gro\u00b7\u00dfe", "Seel'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im K\u00f6nigssitz allgegenw\u00e4rtig st\u00fcrmten", "tokens": ["Im", "K\u00f6\u00b7nigs\u00b7sitz", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "st\u00fcrm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sechsfach jedes t\u00f6nten; und er,", "tokens": ["Und", "sechs\u00b7fach", "je\u00b7des", "t\u00f6n\u00b7ten", ";", "und", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$.", "KON", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fels, nach dessen Haupt sich Tiefen th\u00fcrmten", "tokens": ["Ein", "Fels", ",", "nach", "des\u00b7sen", "Haupt", "sich", "Tie\u00b7fen", "th\u00fcrm\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN", "PRF", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Donner nach dem Fu\u00df, sieht sonnengleich umher,", "tokens": ["Und", "Don\u00b7ner", "nach", "dem", "Fu\u00df", ",", "sieht", "son\u00b7nen\u00b7gleich", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erheitert seine Herzen, die im Kreise,", "tokens": ["Er\u00b7hei\u00b7tert", "sei\u00b7ne", "Her\u00b7zen", ",", "die", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie um den Opfersaltar, Thr\u00e4nen gl\u00fchn,", "tokens": ["Wie", "um", "den", "Op\u00b7fer\u00b7sal\u00b7tar", ",", "Thr\u00e4\u00b7nen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den letzten Trost (Gott! Trost!), das letzte weise", "tokens": ["Den", "letz\u00b7ten", "Trost", "(", "Gott", "!", "Trost", "!", ")", ",", "das", "letz\u00b7te", "wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "$.", "NN", "$.", "$(", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Schreckenk\u00f6nig schwarz im Nachtsturm st\u00fcrmte,", "tokens": ["Der", "Schre\u00b7cken\u00b7k\u00f6\u00b7nig", "schwarz", "im", "Nachts\u00b7turm", "st\u00fcrm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Antlitz sah und bebt' \u2013 Freund, Alles schwieg! \u2013", "tokens": ["Sein", "Ant\u00b7litz", "sah", "und", "bebt'", "\u2013", "Freund", ",", "Al\u00b7les", "schwieg", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$(", "NN", "$,", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die schwangre Stille, die Dir Schrecken th\u00fcrmte \u2013", "tokens": ["Die", "schwang\u00b7re", "Stil\u00b7le", ",", "die", "Dir", "Schre\u00b7cken", "th\u00fcrm\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War Cherubs Beben, der mit ihm zum Himmel stieg.", "tokens": ["War", "Che\u00b7rubs", "Be\u00b7ben", ",", "der", "mit", "ihm", "zum", "Him\u00b7mel", "stieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "$,", "PRELS", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Was er empfand, da schon die H\u00fcll' erbla\u00dfte,", "tokens": ["Was", "er", "emp\u00b7fand", ",", "da", "schon", "die", "H\u00fcll'", "er\u00b7bla\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Geist mit Geistern sprach, wie Gott erscheint! \u2013", "tokens": ["Und", "Geist", "mit", "Geis\u00b7tern", "sprach", ",", "wie", "Gott", "er\u00b7scheint", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ihn, den Erdentschwungnen, Dunkel fa\u00dfte,", "tokens": ["Wie", "ihn", ",", "den", "Er\u00b7dent\u00b7schwung\u00b7nen", ",", "Dun\u00b7kel", "fa\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erhob und tief verbarg \u2013 dies Alles f\u00fchlten, Freund,", "tokens": ["Er\u00b7hob", "und", "tief", "ver\u00b7barg", "\u2013", "dies", "Al\u00b7les", "f\u00fchl\u00b7ten", ",", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVFIN", "$(", "PDS", "PIS", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wir, Du und ich, in D\u00e4mmrung, da sein Schatten", "tokens": ["Wir", ",", "Du", "und", "ich", ",", "in", "D\u00e4mm\u00b7rung", ",", "da", "sein", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "KON", "PPER", "$,", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit unserm Schutzgeist sprach \u2013 und schauderten", "tokens": ["Mit", "un\u00b7serm", "Schutz\u00b7geist", "sprach", "\u2013", "und", "schau\u00b7der\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wir, als ins Grab sein Erdklo\u00df sank, sein Schatten", "tokens": ["Wir", ",", "als", "ins", "Grab", "sein", "Erd\u00b7klo\u00df", "sank", ",", "sein", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "KOUS", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit unserm Schutzgeist sprach, wir heilig schauderten \u2013", "tokens": ["Mit", "un\u00b7serm", "Schutz\u00b7geist", "sprach", ",", "wir", "hei\u00b7lig", "schau\u00b7der\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Und meine Laute, N\u00e4nien zu wimmern,", "tokens": ["Und", "mei\u00b7ne", "Lau\u00b7te", ",", "N\u00e4\u00b7ni\u00b7en", "zu", "wim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt wegbebt \u2013 bis sie mit Dir einst", "tokens": ["Jetzt", "weg\u00b7bebt", "\u2013", "bis", "sie", "mit", "Dir", "einst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "APPR", "PPER", "APPR", "PPER", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "An dunkler Urne, wo nur Sterne schimmern,", "tokens": ["An", "dunk\u00b7ler", "Ur\u00b7ne", ",", "wo", "nur", "Ster\u00b7ne", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Geistert\u00f6ne bebt, die Du mit Ach vereinst!", "tokens": ["Die", "Geis\u00b7ter\u00b7t\u00f6\u00b7ne", "bebt", ",", "die", "Du", "mit", "Ach", "ver\u00b7einst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}