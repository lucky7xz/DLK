{"textgrid.poem.32385": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "63. Der reiche Freier", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bettler ging auf Freiersf\u00fc\u00dfen,", "tokens": ["Ein", "Bett\u00b7ler", "ging", "auf", "Frei\u00b7ers\u00b7f\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach zu einer Magd, die er nach Wunsche fand:", "tokens": ["Und", "sprach", "zu", "ei\u00b7ner", "Magd", ",", "die", "er", "nach", "Wun\u00b7sche", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm mich! Sie fragt: worauf? \u00bbAuf diese d\u00fcrre Hand:", "tokens": ["Nimm", "mich", "!", "Sie", "fragt", ":", "wo\u00b7rauf", "?", "\u00bb", "Auf", "die\u00b7se", "d\u00fcr\u00b7re", "Hand", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "PPER", "VVFIN", "$.", "PWAV", "$.", "$(", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die soll uns wohl ern\u00e4hren m\u00fcssen!\u00ab", "tokens": ["Die", "soll", "uns", "wohl", "er\u00b7n\u00e4h\u00b7ren", "m\u00fcs\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "VMINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Magd besann sich kurz, und gab ihm ihre Hand.", "tokens": ["Die", "Magd", "be\u00b7sann", "sich", "kurz", ",", "und", "gab", "ihm", "ih\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}