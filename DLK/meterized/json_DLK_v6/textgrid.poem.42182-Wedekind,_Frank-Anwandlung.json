{"textgrid.poem.42182": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Anwandlung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00fc\u00dftest du, M\u00e4dchen, wie das tut,", "tokens": ["W\u00fc\u00df\u00b7test", "du", ",", "M\u00e4d\u00b7chen", ",", "wie", "das", "tut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "PDS", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn dein Arm in dem seinen ruht,", "tokens": ["Wenn", "dein", "Arm", "in", "dem", "sei\u00b7nen", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn du an seiner Seite hin", "tokens": ["Wenn", "du", "an", "sei\u00b7ner", "Sei\u00b7te", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wandelst in weltbegl\u00fcckendem Sinn!", "tokens": ["Wan\u00b7delst", "in", "welt\u00b7be\u00b7gl\u00fc\u00b7cken\u00b7dem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PWAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "W\u00fc\u00dftest du, wie mich der Anblick foltert,", "tokens": ["W\u00fc\u00df\u00b7test", "du", ",", "wie", "mich", "der", "An\u00b7blick", "fol\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie mir der Wunsch in der Seele brennt:", "tokens": ["Wie", "mir", "der", "Wunsch", "in", "der", "See\u00b7le", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "K\u00e4m doch das himmlische Firmament", "tokens": ["K\u00e4m", "doch", "das", "himm\u00b7li\u00b7sche", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "\u00dcber euch beide heruntergepoltert!", "tokens": ["\u00dc\u00b7ber", "euch", "bei\u00b7de", "her\u00b7un\u00b7ter\u00b7ge\u00b7pol\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Wolken machen sich nichts daraus,", "tokens": ["Wol\u00b7ken", "ma\u00b7chen", "sich", "nichts", "da\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PIS", "PAV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wandern weiter und lachen mich aus,", "tokens": ["Wan\u00b7dern", "wei\u00b7ter", "und", "la\u00b7chen", "mich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ob ich euch, ob ich ihnen fluche,", "tokens": ["Ob", "ich", "euch", ",", "ob", "ich", "ih\u00b7nen", "flu\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich mich selbst zu erdrosseln suche \u2013", "tokens": ["Ob", "ich", "mich", "selbst", "zu", "er\u00b7dros\u00b7seln", "su\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schlie\u00dflich nach langem qualvollem Bangen", "tokens": ["Schlie\u00df\u00b7lich", "nach", "lan\u00b7gem", "qual\u00b7vol\u00b7lem", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Reichst du mir fl\u00fcchtig die zuckende Hand,", "tokens": ["Reichst", "du", "mir", "fl\u00fcch\u00b7tig", "die", "zu\u00b7cken\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Und das verwickelte Rosenband", "tokens": ["Und", "das", "ver\u00b7wi\u00b7ckel\u00b7te", "Ro\u00b7sen\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "H\u00e4lt mich verdoppelt fester umfangen.", "tokens": ["H\u00e4lt", "mich", "ver\u00b7dop\u00b7pelt", "fes\u00b7ter", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJD", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Kennst jene H\u00fctte du tief im Wald,", "tokens": ["Kennst", "je\u00b7ne", "H\u00fct\u00b7te", "du", "tief", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Zweier B\u00fc\u00dfenden Aufenthalt?", "tokens": ["Zwei\u00b7er", "B\u00fc\u00b7\u00dfen\u00b7den", "Auf\u00b7ent\u00b7halt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Rings unter hohen rauschenden B\u00e4umen", "tokens": ["Rings", "un\u00b7ter", "ho\u00b7hen", "rau\u00b7schen\u00b7den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wildes Kasteien und tiefes Tr\u00e4umen ...", "tokens": ["Wil\u00b7des", "Kas\u00b7tei\u00b7en", "und", "tie\u00b7fes", "Tr\u00e4u\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJA", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Nun ich eben mein B\u00fcndel geschn\u00fcrt,", "tokens": ["Nun", "ich", "e\u00b7ben", "mein", "B\u00fcn\u00b7del", "ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Will mich dieser Gedanke nicht lassen;", "tokens": ["Will", "mich", "die\u00b7ser", "Ge\u00b7dan\u00b7ke", "nicht", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ach und mein Hirn mag es gar nicht fassen,", "tokens": ["Ach", "und", "mein", "Hirn", "mag", "es", "gar", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df mich mein Los schon von hinnen f\u00fchrt.", "tokens": ["Da\u00df", "mich", "mein", "Los", "schon", "von", "hin\u00b7nen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}