{"textgrid.poem.53559": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Krieg dem Kriege", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie lagen vier Jahre im Sch\u00fctzengraben.", "tokens": ["Sie", "la\u00b7gen", "vier", "Jah\u00b7re", "im", "Sch\u00fct\u00b7zen\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zeit, gro\u00dfe Zeit!", "tokens": ["Zeit", ",", "gro\u00b7\u00dfe", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie froren und waren verlaust und haben", "tokens": ["Sie", "fro\u00b7ren", "und", "wa\u00b7ren", "ver\u00b7laust", "und", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "KON", "VAFIN", "VVPP", "KON", "VAFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "daheim eine Frau und zwei kleine Knaben,", "tokens": ["da\u00b7heim", "ei\u00b7ne", "Frau", "und", "zwei", "klei\u00b7ne", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "CARD", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "weit, weit \u2013!", "tokens": ["weit", ",", "weit", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "$(", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Und keiner, der ihnen die Wahrheit sagt.", "tokens": ["Und", "kei\u00b7ner", ",", "der", "ih\u00b7nen", "die", "Wahr\u00b7heit", "sagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und keiner, der aufzubegehren wagt.", "tokens": ["Und", "kei\u00b7ner", ",", "der", "auf\u00b7zu\u00b7be\u00b7geh\u00b7ren", "wagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "VVIZU", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Monat um Monat, Jahr um Jahr . . .", "tokens": ["Mo\u00b7nat", "um", "Mo\u00b7nat", ",", "Jahr", "um", "Jahr", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn mal einer auf Urlaub war,", "tokens": ["Und", "wenn", "mal", "ei\u00b7ner", "auf", "Ur\u00b7laub", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sah er zu Haus die dicken B\u00e4uche.", "tokens": ["sah", "er", "zu", "Haus", "die", "di\u00b7cken", "B\u00e4u\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und es fra\u00dfen dort um sich wie eine Seuche", "tokens": ["Und", "es", "fra\u00b7\u00dfen", "dort", "um", "sich", "wie", "ei\u00b7ne", "Seu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PRF", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "der Tanz, die Gier, das Schiebergesch\u00e4ft.", "tokens": ["der", "Tanz", ",", "die", "Gier", ",", "das", "Schie\u00b7ber\u00b7ge\u00b7sch\u00e4ft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und die Horde alldeutscher Skribenten kl\u00e4fft:", "tokens": ["Und", "die", "Hor\u00b7de", "all\u00b7deut\u00b7scher", "Skri\u00b7ben\u00b7ten", "kl\u00e4fft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "\u00bbkrieg! Krieg!", "tokens": ["\u00bb", "krieg", "!", "Krieg", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Gro\u00dfer Sieg!", "tokens": ["Gro\u00b7\u00dfer", "Sieg", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Sieg in Albanien und Sieg in Flandern!\u00ab", "tokens": ["Sieg", "in", "Al\u00b7ba\u00b7ni\u00b7en", "und", "Sieg", "in", "Flan\u00b7dern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und es starben die andern, die andern, die andern . . .", "tokens": ["Und", "es", "star\u00b7ben", "die", "an\u00b7dern", ",", "die", "an\u00b7dern", ",", "die", "an\u00b7dern", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "$,", "PRELS", "PIS", "$,", "PRELS", "PIS", "$.", "$.", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Sie sahen die Kameraden fallen.", "tokens": ["Sie", "sa\u00b7hen", "die", "Ka\u00b7me\u00b7ra\u00b7den", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das war das Schicksal bei fast allen:", "tokens": ["Das", "war", "das", "Schick\u00b7sal", "bei", "fast", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ADV", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verwundung, Qual wie ein Tier, und Tod.", "tokens": ["Ver\u00b7wun\u00b7dung", ",", "Qual", "wie", "ein", "Tier", ",", "und", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KOKOM", "ART", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein kleiner Fleck, schmutzigrot \u2013", "tokens": ["Ein", "klei\u00b7ner", "Fleck", ",", "schmut\u00b7zi\u00b7grot", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "und man trug sie fort und scharrte sie ein.", "tokens": ["und", "man", "trug", "sie", "fort", "und", "scharr\u00b7te", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Wer wird wohl der n\u00e4chste sein?", "tokens": ["Wer", "wird", "wohl", "der", "n\u00e4chs\u00b7te", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und ein Schrei von Millionen stieg auf zu den Sternen.", "tokens": ["Und", "ein", "Schrei", "von", "Mil\u00b7lion\u00b7en", "stieg", "auf", "zu", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "APPR", "APPR", "ART", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Werden die Menschen es niemals lernen?", "tokens": ["Wer\u00b7den", "die", "Men\u00b7schen", "es", "nie\u00b7mals", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gibt es ein Ding, um das es sich lohnt?", "tokens": ["Gibt", "es", "ein", "Ding", ",", "um", "das", "es", "sich", "lohnt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wer ist das, der da oben thront,", "tokens": ["Wer", "ist", "das", ",", "der", "da", "o\u00b7ben", "thront", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "von oben bis unten bespickt mit Orden,", "tokens": ["von", "o\u00b7ben", "bis", "un\u00b7ten", "be\u00b7spickt", "mit", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "und nur immer befiehlt: Morden! Morden! \u2013", "tokens": ["und", "nur", "im\u00b7mer", "be\u00b7fiehlt", ":", "Mor\u00b7den", "!", "Mor\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$.", "NN", "$.", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Blut und zermalmte Knochen und Dreck . . .", "tokens": ["Blut", "und", "zer\u00b7malm\u00b7te", "Kno\u00b7chen", "und", "Dreck", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "KON", "NE", "$.", "$.", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und dann hie\u00df es pl\u00f6tzlich, das Schiff sei leck.", "tokens": ["Und", "dann", "hie\u00df", "es", "pl\u00f6tz\u00b7lich", ",", "das", "Schiff", "sei", "leck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der Kapit\u00e4n hat den Abschied genommen", "tokens": ["Der", "Ka\u00b7pi\u00b7t\u00e4n", "hat", "den", "Ab\u00b7schied", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "und ist etwas pl\u00f6tzlich von dannen geschwommen.", "tokens": ["und", "ist", "et\u00b7was", "pl\u00f6tz\u00b7lich", "von", "dan\u00b7nen", "ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "APPR", "ADV", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Ratlos stehen die Feldgrauen da.", "tokens": ["Rat\u00b7los", "ste\u00b7hen", "die", "Feld\u00b7grau\u00b7en", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "F\u00fcr wen das alles? Pro patria?", "tokens": ["F\u00fcr", "wen", "das", "al\u00b7les", "?", "Pro", "pat\u00b7ria", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWS", "ART", "PIS", "$.", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Br\u00fcder! Br\u00fcder! Schlie\u00dft die Reihn!", "tokens": ["Br\u00fc\u00b7der", "!", "Br\u00fc\u00b7der", "!", "Schlie\u00dft", "die", "Reihn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fcder! das darf nicht wieder sein!", "tokens": ["Br\u00fc\u00b7der", "!", "das", "darf", "nicht", "wie\u00b7der", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VMFIN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Geben sie uns den Vernichtungsfrieden,", "tokens": ["Ge\u00b7ben", "sie", "uns", "den", "Ver\u00b7nich\u00b7tungs\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "ist das gleiche Los beschieden", "tokens": ["ist", "das", "glei\u00b7che", "Los", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "unsern S\u00f6hnen und euern Enkeln.", "tokens": ["un\u00b7sern", "S\u00f6h\u00b7nen", "und", "eu\u00b7ern", "En\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sollen die wieder blutrot besprenkeln", "tokens": ["Sol\u00b7len", "die", "wie\u00b7der", "blut\u00b7rot", "be\u00b7spren\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADV", "ADJD", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "die Ackergr\u00e4ben, das gr\u00fcne Gras?", "tokens": ["die", "A\u00b7cker\u00b7gr\u00e4\u00b7ben", ",", "das", "gr\u00fc\u00b7ne", "Gras", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Br\u00fcder! Pfeift den Burschen was!", "tokens": ["Br\u00fc\u00b7der", "!", "Pfeift", "den", "Bur\u00b7schen", "was", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ART", "NN", "PWS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Es darf und soll so nicht weitergehn.", "tokens": ["Es", "darf", "und", "soll", "so", "nicht", "wei\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wir haben alle, alle gesehn,", "tokens": ["Wir", "ha\u00b7ben", "al\u00b7le", ",", "al\u00b7le", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PIS", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "wohin ein solcher Wahnsinn f\u00fchrt \u2013", "tokens": ["wo\u00b7hin", "ein", "sol\u00b7cher", "Wahn\u00b7sinn", "f\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Feuer brannte, das sie gesch\u00fcrt.", "tokens": ["Das", "Feu\u00b7er", "brann\u00b7te", ",", "das", "sie", "ge\u00b7sch\u00fcrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00f6scht es aus! Die Imperialisten,", "tokens": ["L\u00f6scht", "es", "aus", "!", "Die", "Im\u00b7pe\u00b7ri\u00b7a\u00b7lis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "die da dr\u00fcben bei jenen nisten,", "tokens": ["die", "da", "dr\u00fc\u00b7ben", "bei", "je\u00b7nen", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PDAT", "ADJA", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "schenken uns wieder Nationalisten.", "tokens": ["schen\u00b7ken", "uns", "wie\u00b7der", "Na\u00b7ti\u00b7o\u00b7na\u00b7lis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und nach abermals zwanzig Jahren", "tokens": ["Und", "nach", "a\u00b7ber\u00b7mals", "zwan\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "CARD", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "kommen neue Kanonen gefahren. \u2013", "tokens": ["kom\u00b7men", "neu\u00b7e", "Ka\u00b7no\u00b7nen", "ge\u00b7fah\u00b7ren", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Das w\u00e4re kein Friede.", "tokens": ["Das", "w\u00e4\u00b7re", "kein", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das w\u00e4re Wahn.", "tokens": ["Das", "w\u00e4\u00b7re", "Wahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der alte Tanz auf dem alten Vulkan.", "tokens": ["Der", "al\u00b7te", "Tanz", "auf", "dem", "al\u00b7ten", "Vul\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Du sollst nicht t\u00f6ten! hat einer gesagt.", "tokens": ["Du", "sollst", "nicht", "t\u00f6\u00b7ten", "!", "hat", "ei\u00b7ner", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "VAFIN", "ART", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und die Menschheit h\u00f6rts, und die Menschheit klagt.", "tokens": ["Und", "die", "Menschheit", "h\u00f6rts", ",", "und", "die", "Menschheit", "klagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Will das niemals anders werden?", "tokens": ["Will", "das", "nie\u00b7mals", "an\u00b7ders", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Krieg dem Kriege!", "tokens": ["Krieg", "dem", "Krie\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Und Friede auf Erden.", "tokens": ["Und", "Frie\u00b7de", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}