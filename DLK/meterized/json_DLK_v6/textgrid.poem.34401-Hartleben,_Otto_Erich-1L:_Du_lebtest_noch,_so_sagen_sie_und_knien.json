{"textgrid.poem.34401": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du lebtest noch, so sagen sie und knien", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lebtest noch, so sagen sie und knien", "tokens": ["Du", "leb\u00b7test", "noch", ",", "so", "sa\u00b7gen", "sie", "und", "kni\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vor deinem Kreuzesholz, daran in Qual", "tokens": ["vor", "dei\u00b7nem", "Kreu\u00b7zes\u00b7holz", ",", "da\u00b7ran", "in", "Qual"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PAV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "du h\u00e4ngst, und k\u00fcssen deine F\u00fcsse.", "tokens": ["du", "h\u00e4ngst", ",", "und", "k\u00fcs\u00b7sen", "dei\u00b7ne", "F\u00fcs\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sahn die Hunde mit dem Schweife wedeln,", "tokens": ["Sie", "sahn", "die", "Hun\u00b7de", "mit", "dem", "Schwei\u00b7fe", "we\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich niederducken vor dem Fuss des Herrn \u2013", "tokens": ["sich", "nie\u00b7der\u00b7du\u00b7cken", "vor", "dem", "Fuss", "des", "Herrn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und gingen hin und thaten Gleiches.", "tokens": ["und", "gin\u00b7gen", "hin", "und", "tha\u00b7ten", "Glei\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha! Lebtest du, du rissest von dem Nagel,", "tokens": ["Ha", "!", "Leb\u00b7test", "du", ",", "du", "ris\u00b7sest", "von", "dem", "Na\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dem martervollen, deinen Fuss \u2013 in Staub", "tokens": ["dem", "mar\u00b7ter\u00b7vol\u00b7len", ",", "dei\u00b7nen", "Fuss", "\u2013", "in", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "VVINF", "$,", "PPOSAT", "NN", "$(", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "tr\u00e4test du sie verachtend nieder!", "tokens": ["tr\u00e4\u00b7test", "du", "sie", "ver\u00b7ach\u00b7tend", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}