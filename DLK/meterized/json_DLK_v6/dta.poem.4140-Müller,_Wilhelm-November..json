{"dta.poem.4140": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "November.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du, auf Tanz und Jagd und Becherklingen,", "tokens": ["Da\u00df", "du", ",", "auf", "Tanz", "und", "Jagd", "und", "Be\u00b7cher\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlangen f\u00fchlst nach w\u00fcrdigeren Dingen,", "tokens": ["Ver\u00b7lan\u00b7gen", "f\u00fchlst", "nach", "w\u00fcr\u00b7di\u00b7ge\u00b7ren", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Womit ich gleich dir kann zu Diensten stehen.", "tokens": ["Wo\u00b7mit", "ich", "gleich", "dir", "kann", "zu", "Diens\u00b7ten", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durch Leipzigs volle Laden ging ich sp\u00e4hen,", "tokens": ["Durch", "Leip\u00b7zigs", "vol\u00b7le", "La\u00b7den", "ging", "ich", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was uns die deutschen Pressen Neues bringen:", "tokens": ["Was", "uns", "die", "deut\u00b7schen", "Pres\u00b7sen", "Neu\u00b7es", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Bogen, die noch auf den Seilen hingen,", "tokens": ["Die", "Bo\u00b7gen", ",", "die", "noch", "auf", "den", "Sei\u00b7len", "hin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie mu\u00dften ungetrocknet mit mir gehen.", "tokens": ["Sie", "mu\u00df\u00b7ten", "un\u00b7ge\u00b7trock\u00b7net", "mit", "mir", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Spar\u00f6fen kauft' ich auch und Sorgenst\u00fchle,", "tokens": ["Spa\u00b7r\u00f6\u00b7fen", "kauft'", "ich", "auch", "und", "Sor\u00b7gen\u00b7st\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kaffee und Knaster, von der besten Sorte,", "tokens": ["Kaf\u00b7fee", "und", "Knas\u00b7ter", ",", "von", "der", "bes\u00b7ten", "Sor\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lange runde Bernsteinpfeifenspitzen.", "tokens": ["Und", "lan\u00b7ge", "run\u00b7de", "Bern\u00b7stein\u00b7pfei\u00b7fen\u00b7spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Entrei\u00df dich, Freund, dem eitlen Weltgew\u00fchle:", "tokens": ["Ent\u00b7rei\u00df", "dich", ",", "Freund", ",", "dem", "eit\u00b7len", "Welt\u00b7ge\u00b7w\u00fch\u00b7le", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchre zu der Weisheit heil'gen Pforte", "tokens": ["Ich", "f\u00fch\u00b7re", "zu", "der", "Weis\u00b7heit", "heil'\u00b7gen", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die J\u00fcnger, ohne sehr sie zu erhitzen.", "tokens": ["Die", "J\u00fcn\u00b7ger", ",", "oh\u00b7ne", "sehr", "sie", "zu", "er\u00b7hit\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}