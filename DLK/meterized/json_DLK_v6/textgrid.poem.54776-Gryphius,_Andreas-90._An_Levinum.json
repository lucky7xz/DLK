{"textgrid.poem.54776": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "90. An Levinum", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was acht ich sprichst du des Monden/ scheinet mir nur Sonnen Licht!", "tokens": ["Was", "acht", "ich", "sprichst", "du", "des", "Mon\u00b7den", "/", "schei\u00b7net", "mir", "nur", "Son\u00b7nen", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "PPER", "VVFIN", "PPER", "ART", "NN", "$(", "VVFIN", "PPER", "ADV", "NN", "NN", "$."], "meter": "-+-++-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "In den Mondens Finstern\u00fcssen/ hilfft uns keine Sonne nicht.", "tokens": ["In", "den", "Mon\u00b7dens", "Fins\u00b7ter\u00b7n\u00fcs\u00b7sen", "/", "hilfft", "uns", "kei\u00b7ne", "Son\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "VVFIN", "PPER", "PIAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}