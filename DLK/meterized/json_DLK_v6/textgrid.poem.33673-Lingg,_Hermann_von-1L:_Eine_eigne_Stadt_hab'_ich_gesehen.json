{"textgrid.poem.33673": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine eigne Stadt hab' ich gesehen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine eigne Stadt hab' ich gesehen", "tokens": ["Ei\u00b7ne", "eig\u00b7ne", "Stadt", "hab'", "ich", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcber unsrer stehen,", "tokens": ["\u00dc\u00b7ber", "uns\u00b7rer", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als ich von des Hauses Giebel heut'", "tokens": ["Als", "ich", "von", "des", "Hau\u00b7ses", "Gie\u00b7bel", "heut'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcberschaut die D\u00e4cher, rings zerstreut,", "tokens": ["\u00dc\u00b7bersc\u00b7haut", "die", "D\u00e4\u00b7cher", ",", "rings", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Diese alten Mauern, Zinnen, T\u00fcrme,", "tokens": ["Die\u00b7se", "al\u00b7ten", "Mau\u00b7ern", ",", "Zin\u00b7nen", ",", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wohnungen der St\u00fcrme,", "tokens": ["Woh\u00b7nun\u00b7gen", "der", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Fenster, dickbestaubt und l\u00e4ngst ergraut,", "tokens": ["Fens\u00b7ter", ",", "dick\u00b7bes\u00b7taubt", "und", "l\u00e4ngst", "er\u00b7graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und durch die kein Menschenblick mehr schaut.", "tokens": ["Und", "durch", "die", "kein", "Men\u00b7schen\u00b7blick", "mehr", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Oben bei dem alten Uhrgeh\u00e4use", "tokens": ["O\u00b7ben", "bei", "dem", "al\u00b7ten", "Uhr\u00b7ge\u00b7h\u00e4u\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hausen Flederm\u00e4use,", "tokens": ["Hau\u00b7sen", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Feuersbr\u00fcnste werfen ihren Schein", "tokens": ["Feu\u00b7ers\u00b7br\u00fcns\u00b7te", "wer\u00b7fen", "ih\u00b7ren", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In die braunen Ziegelrinnen ein.", "tokens": ["In", "die", "brau\u00b7nen", "Zie\u00b7gel\u00b7rin\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Plaudernd raunt und rauscht von hier der Regen", "tokens": ["Plau\u00b7dernd", "raunt", "und", "rauscht", "von", "hier", "der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "VVFIN", "APPR", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seinem Sturz entgegen;", "tokens": ["Sei\u00b7nem", "Sturz", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kleine Bl\u00fcmchen, die noch niemand flocht,", "tokens": ["Klei\u00b7ne", "Bl\u00fcm\u00b7chen", ",", "die", "noch", "nie\u00b7mand", "flocht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hat die Neugier hier zu bl\u00fchn vermocht.", "tokens": ["Hat", "die", "Neu\u00b7gier", "hier", "zu", "bl\u00fchn", "ver\u00b7mocht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nur die Schatten L\u00e4ngstverstorbner nicken", "tokens": ["Nur", "die", "Schat\u00b7ten", "L\u00e4ngst\u00b7ver\u00b7storb\u00b7ner", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hie und da und blicken", "tokens": ["Hie", "und", "da", "und", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Uralt und in l\u00e4ngst verschollner Tracht", "tokens": ["Ur\u00b7alt", "und", "in", "l\u00e4ngst", "ver\u00b7scholl\u00b7ner", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus den blinden Fenstern in die Nacht.", "tokens": ["Aus", "den", "blin\u00b7den", "Fens\u00b7tern", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}