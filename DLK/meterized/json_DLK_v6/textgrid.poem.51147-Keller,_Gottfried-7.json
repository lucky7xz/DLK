{"textgrid.poem.51147": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht die d\u00fcrre Spielersippe:", "tokens": ["Seht", "die", "d\u00fcr\u00b7re", "Spie\u00b7ler\u00b7sip\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vier geharnischte Gerippe,", "tokens": ["Vier", "ge\u00b7har\u00b7nischte", "Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Mit der Klapperfaust, der harten,", "tokens": ["Mit", "der", "Klap\u00b7per\u00b7faust", ",", "der", "har\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haun sie auf den Tisch die Karten!", "tokens": ["Haun", "sie", "auf", "den", "Tisch", "die", "Kar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre Sanduhr ist zerbrochen,", "tokens": ["Ih\u00b7re", "San\u00b7duhr", "ist", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fort doch spielen diese Knochen,", "tokens": ["Fort", "doch", "spie\u00b7len", "die\u00b7se", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hohl die Sch\u00e4del, drinnen nisten", "tokens": ["Hohl", "die", "Sch\u00e4\u00b7del", ",", "drin\u00b7nen", "nis\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bettelhafte Spielerlisten.", "tokens": ["Bet\u00b7tel\u00b7haf\u00b7te", "Spie\u00b7ler\u00b7lis\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sau und Bube, Lumpentr\u00fcmpfe,", "tokens": ["Sau", "und", "Bu\u00b7be", ",", "Lum\u00b7pen\u00b7tr\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helfen ihnen auf die Str\u00fcmpfe", "tokens": ["Hel\u00b7fen", "ih\u00b7nen", "auf", "die", "Str\u00fcmp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So im Rat wie bei den Karten \u2013", "tokens": ["So", "im", "Rat", "wie", "bei", "den", "Kar\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur nicht bei den Feldstandarten.", "tokens": ["Nur", "nicht", "bei", "den", "Felds\u00b7tand\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und sie z\u00e4hlen falsch die Stiche,", "tokens": ["Und", "sie", "z\u00e4h\u00b7len", "falsch", "die", "Sti\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie schleichen ihre Schliche:", "tokens": ["Und", "sie", "schlei\u00b7chen", "ih\u00b7re", "Schli\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcbung, \u00dcbung macht den Meister!", "tokens": ["\u00dc\u00b7bung", ",", "\u00dc\u00b7bung", "macht", "den", "Meis\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprechen auch verlorne Geister.", "tokens": ["Spre\u00b7chen", "auch", "ver\u00b7lor\u00b7ne", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}