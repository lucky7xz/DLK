{"dta.poem.7997": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Noch singt den Wiederhallen", "tokens": ["Noch", "singt", "den", "Wie\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Knabe sein Gef\u00fchl;", "tokens": ["Der", "Kna\u00b7be", "sein", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Elfe hat Gefallen", "tokens": ["Die", "El\u00b7fe", "hat", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am jugendlichen Spiel.", "tokens": ["Am", "ju\u00b7gend\u00b7li\u00b7chen", "Spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es gl\u00e4nzen seine Lieder", "tokens": ["Es", "gl\u00e4n\u00b7zen", "sei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Blumen rings um ihn;", "tokens": ["Wie", "Blu\u00b7men", "rings", "um", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie gehn mit ihm wie Br\u00fcder", "tokens": ["Sie", "gehn", "mit", "ihm", "wie", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch stille Haine hin.", "tokens": ["Durch", "stil\u00b7le", "Hai\u00b7ne", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er kommt zum V\u00f6lkerfeste,", "tokens": ["Er", "kommt", "zum", "V\u00f6l\u00b7ker\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er singt im K\u00f6nigssaal,", "tokens": ["Er", "singt", "im", "K\u00f6\u00b7nigs\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm staunen alle G\u00e4ste,", "tokens": ["Ihm", "stau\u00b7nen", "al\u00b7le", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Lied verkl\u00e4rt das Mahl;", "tokens": ["Sein", "Lied", "ver\u00b7kl\u00e4rt", "das", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Frauen sch\u00f6nste kr\u00f6nen", "tokens": ["Der", "Frau\u00b7en", "sch\u00f6ns\u00b7te", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit lichten Blumen ihn;", "tokens": ["Mit", "lich\u00b7ten", "Blu\u00b7men", "ihn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er senkt das Aug\u2019 in Thr\u00e4nen", "tokens": ["Er", "senkt", "das", "Aug'", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und seine Wangen gl\u00fchn.", "tokens": ["Und", "sei\u00b7ne", "Wan\u00b7gen", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}