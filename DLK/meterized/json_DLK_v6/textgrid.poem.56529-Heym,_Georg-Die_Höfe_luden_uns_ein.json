{"textgrid.poem.56529": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die H\u00f6fe luden uns ein", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00f6fe luden uns ein, mit den Armen schm\u00e4chtig,", "tokens": ["Die", "H\u00f6\u00b7fe", "lu\u00b7den", "uns", "ein", ",", "mit", "den", "Ar\u00b7men", "schm\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fa\u00dften unserer Seelchen zipfeliges Kleid.", "tokens": ["Fa\u00df\u00b7ten", "un\u00b7se\u00b7rer", "Seel\u00b7chen", "zip\u00b7fe\u00b7li\u00b7ges", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und wir entglitten durch Tore n\u00e4chtig", "tokens": ["Und", "wir", "ent\u00b7glit\u00b7ten", "durch", "To\u00b7re", "n\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In toter G\u00e4rten verwunschene Zeit.", "tokens": ["In", "to\u00b7ter", "G\u00e4r\u00b7ten", "ver\u00b7wun\u00b7sche\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Von Regenrohren fiel Wasser bleiern,", "tokens": ["Von", "Re\u00b7gen\u00b7roh\u00b7ren", "fiel", "Was\u00b7ser", "blei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ewig, Wolken flogen so tr\u00fcbe.", "tokens": ["E\u00b7wig", ",", "Wol\u00b7ken", "flo\u00b7gen", "so", "tr\u00fc\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "VVFIN", "ADV", "ADJA", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und \u00fcber der Starre der frostigen Weiher", "tokens": ["Und", "\u00fc\u00b7ber", "der", "Star\u00b7re", "der", "fros\u00b7ti\u00b7gen", "Wei\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Rosen hingen in D\u00fcrre vom Triebe.", "tokens": ["Ro\u00b7sen", "hin\u00b7gen", "in", "D\u00fcr\u00b7re", "vom", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wir gingen auf herbstlichen Pfaden, geringern,", "tokens": ["Und", "wir", "gin\u00b7gen", "auf", "herbst\u00b7li\u00b7chen", "Pfa\u00b7den", ",", "ge\u00b7rin\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADJA", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Gl\u00e4serne Kugeln zerrissen unser Gesicht,", "tokens": ["Gl\u00e4\u00b7ser\u00b7ne", "Ku\u00b7geln", "zer\u00b7ris\u00b7sen", "un\u00b7ser", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jemand hielt sie uns vor auf den spitzigen Fingern.", "tokens": ["Je\u00b7mand", "hielt", "sie", "uns", "vor", "auf", "den", "spit\u00b7zi\u00b7gen", "Fin\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Unsere Qualen machten uns Feuerlicht.", "tokens": ["Un\u00b7se\u00b7re", "Qua\u00b7len", "mach\u00b7ten", "uns", "Feu\u00b7er\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Und wir schwanden so schwach in die gl\u00e4sernen R\u00e4ume.", "tokens": ["Und", "wir", "schwan\u00b7den", "so", "schwach", "in", "die", "gl\u00e4\u00b7ser\u00b7nen", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Riefen voll Wehmut, da d\u00fcnne das Glas zerbrach.", "tokens": ["Rie\u00b7fen", "voll", "Weh\u00b7mut", ",", "da", "d\u00fcn\u00b7ne", "das", "Glas", "zer\u00b7brach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Wir sitzen nun ewig, in wei\u00dflichen Wolken, zu tr\u00e4umen", "tokens": ["Wir", "sit\u00b7zen", "nun", "e\u00b7wig", ",", "in", "wei\u00df\u00b7li\u00b7chen", "Wol\u00b7ken", ",", "zu", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "\u203asp\u00e4rlichem\u2039 Fluge der Falter im Abend\u203arot\u2039 nach.", "tokens": ["\u203a", "sp\u00e4r\u00b7li\u00b7chem", "\u2039", "Flu\u00b7ge", "der", "Fal\u00b7ter", "im", "A\u00b7bend", "\u203a", "rot", "\u2039", "nach", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADJA", "$(", "NN", "ART", "NN", "APPRART", "NN", "$(", "ADJD", "$(", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}