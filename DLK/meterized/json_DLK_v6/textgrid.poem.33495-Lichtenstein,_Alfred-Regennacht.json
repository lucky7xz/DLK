{"textgrid.poem.33495": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Regennacht", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist futsch. Der Himmel ist ersoffen.", "tokens": ["Der", "Tag", "ist", "fut\u00b7sch", ".", "Der", "Him\u00b7mel", "ist", "er\u00b7sof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wie falsche Perlen liegen kleine Stumpen", "tokens": ["Wie", "fal\u00b7sche", "Per\u00b7len", "lie\u00b7gen", "klei\u00b7ne", "Stum\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerhackten Lichts umher und machen offen", "tokens": ["Zer\u00b7hack\u00b7ten", "Lichts", "um\u00b7her", "und", "ma\u00b7chen", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "KON", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein wenig Stra\u00dfe, ein paar H\u00e4userklumpen.", "tokens": ["Ein", "we\u00b7nig", "Stra\u00b7\u00dfe", ",", "ein", "paar", "H\u00e4u\u00b7ser\u00b7klum\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "Verfault ist alles sonst und aufgefressen", "tokens": ["Ver\u00b7fault", "ist", "al\u00b7les", "sonst", "und", "auf\u00b7ge\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PIS", "ADV", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von schwarzem Nebel, der wie eine Mauer", "tokens": ["Von", "schwar\u00b7zem", "Ne\u00b7bel", ",", "der", "wie", "ei\u00b7ne", "Mau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herunterf\u00e4llt und morsch ist. Und im Pressen", "tokens": ["Her\u00b7un\u00b7ter\u00b7f\u00e4llt", "und", "morsch", "ist", ".", "Und", "im", "Pres\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "VAFIN", "$.", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Br\u00f6ckelt wie Schutt der Regen \u2013 dichter \u2013 grauer \u2013", "tokens": ["Br\u00f6\u00b7ckelt", "wie", "Schutt", "der", "Re\u00b7gen", "\u2013", "dich\u00b7ter", "\u2013", "grau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "ART", "NN", "$(", "ADJD", "$(", "ADJD", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Als wollte jeden Augenblick die ganze", "tokens": ["Als", "woll\u00b7te", "je\u00b7den", "Au\u00b7gen\u00b7blick", "die", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PIAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verseuchte Finsternis zusammensinken.", "tokens": ["Ver\u00b7seuch\u00b7te", "Fins\u00b7ter\u00b7nis", "zu\u00b7sam\u00b7men\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie eine seltsame, ertrunkne Pflanze", "tokens": ["Wie", "ei\u00b7ne", "selt\u00b7sa\u00b7me", ",", "er\u00b7trunk\u00b7ne", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unten im Sumpf siehst du ein Auto blinken.", "tokens": ["Un\u00b7ten", "im", "Sumpf", "siehst", "du", "ein", "Au\u00b7to", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Die \u00e4ltsten Huren kommen angekrochen", "tokens": ["Die", "\u00e4lts\u00b7ten", "Hu\u00b7ren", "kom\u00b7men", "an\u00b7ge\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus nassen Schatten \u2013 schwinds\u00fcchtige Kr\u00f6ten.", "tokens": ["Aus", "nas\u00b7sen", "Schat\u00b7ten", "\u2013", "schwind\u00b7s\u00fcch\u00b7ti\u00b7ge", "Kr\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort schleicht eins. Dorten wird ein Schein erstochen.", "tokens": ["Dort", "schleicht", "eins", ".", "Dor\u00b7ten", "wird", "ein", "Schein", "er\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$.", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Regensturz will alles \u00fcbert\u00f6ten ...", "tokens": ["Der", "Re\u00b7gen\u00b7sturz", "will", "al\u00b7les", "\u00fc\u00b7bert\u00b7\u00f6\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du aber wanderst durch die W\u00fcsteneien.", "tokens": ["Du", "a\u00b7ber", "wan\u00b7derst", "durch", "die", "W\u00fcs\u00b7te\u00b7nei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Kleid h\u00e4ngt schwer. Durchn\u00e4\u00dft sind deine Schuhe.", "tokens": ["Dein", "Kleid", "h\u00e4ngt", "schwer", ".", "Durc\u00b7hn\u00e4\u00dft", "sind", "dei\u00b7ne", "Schu\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Auge ist verr\u00fcckt von Gier und Schreien.", "tokens": ["Dein", "Au\u00b7ge", "ist", "ver\u00b7r\u00fcckt", "von", "Gier", "und", "Schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dieses treibt dich \u2013 und du hast nicht Ruhe:", "tokens": ["Und", "die\u00b7ses", "treibt", "dich", "\u2013", "und", "du", "hast", "nicht", "Ru\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$(", "KON", "PPER", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vielleicht erscheint inmitten d\u00fcstrer Feuer", "tokens": ["Viel\u00b7leicht", "er\u00b7scheint", "in\u00b7mit\u00b7ten", "d\u00fcst\u00b7rer", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Teufel selbst in der Gestalt des Schweines.", "tokens": ["Der", "Teu\u00b7fel", "selbst", "in", "der", "Ge\u00b7stalt", "des", "Schwei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht geschieht etwas ganz ungeheuer", "tokens": ["Viel\u00b7leicht", "ge\u00b7schieht", "et\u00b7was", "ganz", "un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bl\u00f6dsinniges, Brutales, Hundsgemeines.", "tokens": ["Bl\u00f6d\u00b7sin\u00b7ni\u00b7ges", ",", "Bru\u00b7ta\u00b7les", ",", "Hunds\u00b7ge\u00b7mei\u00b7nes", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}}}}