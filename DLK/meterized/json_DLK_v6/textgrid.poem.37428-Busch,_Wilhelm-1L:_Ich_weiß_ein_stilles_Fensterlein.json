{"textgrid.poem.37428": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df ein stilles Fensterlein", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein stilles Fensterlein", "tokens": ["Ich", "wei\u00df", "ein", "stil\u00b7les", "Fens\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt heimlich und versteckt,", "tokens": ["Liegt", "heim\u00b7lich", "und", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hat mit Laub der gr\u00fcne Wein", "tokens": ["Das", "hat", "mit", "Laub", "der", "gr\u00fc\u00b7ne", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ranken \u00fcberdeckt.", "tokens": ["Und", "Ran\u00b7ken", "\u00fc\u00b7berd\u00b7eckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Laube spielt der Sommerwind,", "tokens": ["Im", "Lau\u00b7be", "spielt", "der", "Som\u00b7mer\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rebe schwankt und nickt,", "tokens": ["Die", "Re\u00b7be", "schwankt", "und", "nickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dahinter sitzt ein h\u00fcbsches Kind", "tokens": ["Da\u00b7hin\u00b7ter", "sitzt", "ein", "h\u00fcb\u00b7sches", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankenvoll und stickt.", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "und", "stickt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im jugendklaren Angesicht", "tokens": ["Im", "ju\u00b7gend\u00b7kla\u00b7ren", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fcht wunders\u00fc\u00df der Mund", "tokens": ["Bl\u00fcht", "wun\u00b7der\u00b7s\u00fc\u00df", "der", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wie ein Rosenkn\u00f6splein licht", "tokens": ["Als", "wie", "ein", "Ro\u00b7sen\u00b7kn\u00f6s\u00b7plein", "licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00fch in der Morgenstund.", "tokens": ["Fr\u00fch", "in", "der", "Mor\u00b7gen\u00b7stund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Im Netzgeflecht das blonde Haar", "tokens": ["Im", "Netz\u00b7ge\u00b7flecht", "das", "blon\u00b7de", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umfa\u00dft ein braunes Band,", "tokens": ["Um\u00b7fa\u00dft", "ein", "brau\u00b7nes", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das liebe blaue Augenpaar", "tokens": ["Das", "lie\u00b7be", "blau\u00b7e", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt sinnend auf die Hand.", "tokens": ["Blickt", "sin\u00b7nend", "auf", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und 's K\u00f6pfchen scheint so still zu sein.", "tokens": ["Und", "'s", "K\u00f6pf\u00b7chen", "scheint", "so", "still", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VVFIN", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ist doch ein Taubenschlag.", "tokens": ["Ist", "doch", "ein", "Tau\u00b7ben\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gedanken fliegen aus und ein", "tokens": ["Ge\u00b7dan\u00b7ken", "flie\u00b7gen", "aus", "und", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den lieben langen Tag.", "tokens": ["Den", "lie\u00b7ben", "lan\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie fliegen \u00fcber Wald und Flur", "tokens": ["Sie", "flie\u00b7gen", "\u00fc\u00b7ber", "Wald", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins weite Land hinaus.", "tokens": ["Ins", "wei\u00b7te", "Land", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, k\u00e4m ein einzig T\u00e4ubchen nur", "tokens": ["Ach", ",", "k\u00e4m", "ein", "ein\u00b7zig", "T\u00e4ub\u00b7chen", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ART", "ADJD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fl\u00f6ge in mein Haus.", "tokens": ["Und", "fl\u00f6\u00b7ge", "in", "mein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}