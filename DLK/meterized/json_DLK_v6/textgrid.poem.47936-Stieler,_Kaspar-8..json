{"textgrid.poem.47936": {"metadata": {"author": {"name": "Stieler, Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1669, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzweiflung/ Sorge/ Furcht und Schrekken/", "tokens": ["Ver\u00b7zwei\u00b7flung", "/", "Sor\u00b7ge", "/", "Furcht", "und", "Schrek\u00b7ken", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schmerz/ Leiden/ Angst und Quaal/", "tokens": ["Schmerz", "/", "Lei\u00b7den", "/", "Angst", "und", "Qua\u00b7al", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ein Regiment von Gekken/", "tokens": ["ein", "Re\u00b7gi\u00b7ment", "von", "Gek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verspottung ohne Zahl/", "tokens": ["Ver\u00b7spot\u00b7tung", "oh\u00b7ne", "Zahl", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "das ist der Liebe Leib-gedinge.", "tokens": ["das", "ist", "der", "Lie\u00b7be", "Leib\u00b7ge\u00b7din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wer das nicht kennt/ der wei\u00df auch nicht/ was Amor ist.", "tokens": ["wer", "das", "nicht", "kennt", "/", "der", "wei\u00df", "auch", "nicht", "/", "was", "A\u00b7mor", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "PTKNEG", "VVFIN", "$(", "ART", "VVFIN", "ADV", "PTKNEG", "$(", "PWS", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sey nu geehrt/ geliebt/ gek\u00fc\u00dft/", "tokens": ["Sey", "nu", "ge\u00b7ehrt", "/", "ge\u00b7liebt", "/", "ge\u00b7k\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$(", "VVPP", "$(", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und sey darbey ein Haubt der Narren.", "tokens": ["und", "sey", "dar\u00b7bey", "ein", "Haubt", "der", "Nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wi\u00dft ihr/ wenn ich das Lieben wolte g\u00f6nnen?", "tokens": ["Wi\u00dft", "ihr", "/", "wenn", "ich", "das", "Lie\u00b7ben", "wol\u00b7te", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "ART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "dem (mein' ich) der mich nie hat lieben k\u00f6nnen.", "tokens": ["dem", "(", "mein'", "ich", ")", "der", "mich", "nie", "hat", "lie\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "VVFIN", "PPER", "$(", "PRELS", "PPER", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}