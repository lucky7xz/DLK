{"dta.poem.7415": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, der wir lang geharrt,", "tokens": ["Die", "Son\u00b7ne", ",", "der", "wir", "lang", "ge\u00b7harrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist endlich aufgegangen;", "tokens": ["Ist", "end\u00b7lich", "auf\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir schauen ihre Himmelfahrt", "tokens": ["Wir", "schau\u00b7en", "ih\u00b7re", "Him\u00b7mel\u00b7fahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Sehnen und Verlangen.", "tokens": ["Voll", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo ist ein Herz, das ruhig schl\u00e4gt,", "tokens": ["Wo", "ist", "ein", "Herz", ",", "das", "ru\u00b7hig", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn solch ein Tag die Schwingen regt?", "tokens": ["Wenn", "solch", "ein", "Tag", "die", "Schwin\u00b7gen", "regt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr V\u00f6lker wachet auf!", "tokens": ["Ihr", "V\u00f6l\u00b7ker", "wa\u00b7chet", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Ketten brach der Lenz entzwei", "tokens": ["Die", "Ket\u00b7ten", "brach", "der", "Lenz", "ent\u00b7zwei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinen Rosend\u00fcften,", "tokens": ["Mit", "sei\u00b7nen", "Ro\u00b7sen\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und unsre Seelen rauschen frei,", "tokens": ["Und", "uns\u00b7re", "See\u00b7len", "rau\u00b7schen", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Adler, in den L\u00fcften.", "tokens": ["Wie", "Ad\u00b7ler", ",", "in", "den", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Toten dr\u00fcckt der Tod heut' nicht;", "tokens": ["Die", "To\u00b7ten", "dr\u00fcckt", "der", "Tod", "heut'", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Horcht! unser Meister lebt und spricht:", "tokens": ["Horcht", "!", "un\u00b7ser", "Meis\u00b7ter", "lebt", "und", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr V\u00f6lker, wachet auf!", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "wa\u00b7chet", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr V\u00f6lker, wachet auf und seht", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "wa\u00b7chet", "auf", "und", "seht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmel selbst in Flammen!", "tokens": ["Den", "Him\u00b7mel", "selbst", "in", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr V\u00f6lker, wachet auf und steht", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "wa\u00b7chet", "auf", "und", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein einig Heer zusammen!", "tokens": ["Ein", "ei\u00b7nig", "Heer", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Voran, voran, im Sturm voran!", "tokens": ["Vo\u00b7ran", ",", "vo\u00b7ran", ",", "im", "Sturm", "vo\u00b7ran", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Gutenberg tr\u00e4gt uns die Fahn'.", "tokens": ["Der", "Gu\u00b7ten\u00b7berg", "tr\u00e4gt", "uns", "die", "Fahn'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ihr V\u00f6lker, wachet auf!", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "wa\u00b7chet", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verheissend schaut sein selig Haubt", "tokens": ["Ver\u00b7heis\u00b7send", "schaut", "sein", "se\u00b7lig", "Haubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Wolken zu der Erden;", "tokens": ["Aus", "Wol\u00b7ken", "zu", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob man die Bl\u00fcten uns geraubt,", "tokens": ["Ob", "man", "die", "Bl\u00fc\u00b7ten", "uns", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frucht soll uns doch werden;", "tokens": ["Die", "Frucht", "soll", "uns", "doch", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was solch ein guter Geist ersann,", "tokens": ["Was", "solch", "ein", "gu\u00b7ter", "Geist", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das thut kein Teufel in den Bann.", "tokens": ["Das", "thut", "kein", "Teu\u00b7fel", "in", "den", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr V\u00f6lker, wachet auf!", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "wa\u00b7chet", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}