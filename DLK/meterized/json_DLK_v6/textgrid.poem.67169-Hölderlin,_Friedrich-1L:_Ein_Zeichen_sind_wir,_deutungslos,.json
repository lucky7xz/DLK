{"textgrid.poem.67169": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Zeichen sind wir, deutungslos,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zeichen sind wir, deutungslos,", "tokens": ["Ein", "Zei\u00b7chen", "sind", "wir", ",", "deu\u00b7tungs\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schmerzlos sind wir und haben fast", "tokens": ["Schmerz\u00b7los", "sind", "wir", "und", "ha\u00b7ben", "fast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sprache in der Fremde verloren.", "tokens": ["Die", "Spra\u00b7che", "in", "der", "Frem\u00b7de", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn n\u00e4mlich \u00fcber Menschen", "tokens": ["Wenn", "n\u00e4m\u00b7lich", "\u00fc\u00b7ber", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Streit ist an dem Himmel und gewaltig", "tokens": ["Ein", "Streit", "ist", "an", "dem", "Him\u00b7mel", "und", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Monde gehn, so redet", "tokens": ["Die", "Mon\u00b7de", "gehn", ",", "so", "re\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Meer auch und Str\u00f6me m\u00fcssen", "tokens": ["Das", "Meer", "auch", "und", "Str\u00f6\u00b7me", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "NN", "VMFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Den Pfad sich suchen. Zweifellos", "tokens": ["Den", "Pfad", "sich", "su\u00b7chen", ".", "Zwei\u00b7fel\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PRF", "VVINF", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist aber ", "tokens": ["Ist", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Kann t\u00e4glich es \u00e4ndern. Kaum bedarf er", "tokens": ["Kann", "t\u00e4g\u00b7lich", "es", "\u00e4n\u00b7dern", ".", "Kaum", "be\u00b7darf", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Gesetz. Und es t\u00f6net das Blatt und Eichb\u00e4ume wehn dann neben", "tokens": ["Ge\u00b7setz", ".", "Und", "es", "t\u00f6\u00b7net", "das", "Blatt", "und", "Eich\u00b7b\u00e4u\u00b7me", "wehn", "dann", "ne\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "KON", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+--+--+-+--++--", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Den Firnen. Denn nicht verm\u00f6gen", "tokens": ["Den", "Fir\u00b7nen", ".", "Denn", "nicht", "ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PTKNEG", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Die Himmlischen alles. N\u00e4mlich es reichen", "tokens": ["Die", "Himm\u00b7li\u00b7schen", "al\u00b7les", ".", "N\u00e4m\u00b7lich", "es", "rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "$.", "ADV", "PPER", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Die Sterblichen eh an den Abgrund. Also wendet es sich, das Echo,", "tokens": ["Die", "Sterb\u00b7li\u00b7chen", "eh", "an", "den", "Ab\u00b7grund", ".", "Al\u00b7so", "wen\u00b7det", "es", "sich", ",", "das", "E\u00b7cho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "$,"], "meter": "-+--++-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.15": {"text": "Mit diesen. Lang ist", "tokens": ["Mit", "die\u00b7sen", ".", "Lang", "ist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "$.", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Die Zeit, es ereignet sich aber", "tokens": ["Die", "Zeit", ",", "es", "er\u00b7eig\u00b7net", "sich", "a\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Das Wahre.", "tokens": ["Das", "Wah\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Wie aber Liebes? Sonnenschein", "tokens": ["Wie", "a\u00b7ber", "Lie\u00b7bes", "?", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Boden sehen wir und trockenen Staub", "tokens": ["Am", "Bo\u00b7den", "se\u00b7hen", "wir", "und", "tro\u00b7cke\u00b7nen", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und tief mit Schatten die W\u00e4lder und es bl\u00fchet", "tokens": ["Und", "tief", "mit", "Schat\u00b7ten", "die", "W\u00e4l\u00b7der", "und", "es", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "NN", "KON", "PPER", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "An D\u00e4chern der Rauch, bei alter Krone", "tokens": ["An", "D\u00e4\u00b7chern", "der", "Rauch", ",", "bei", "al\u00b7ter", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der T\u00fcrme, friedsam; und es girren", "tokens": ["Der", "T\u00fcr\u00b7me", ",", "fried\u00b7sam", ";", "und", "es", "gir\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$.", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verloren in der Luft die Lerchen und unter dem Tage weiden", "tokens": ["Ver\u00b7lo\u00b7ren", "in", "der", "Luft", "die", "Ler\u00b7chen", "und", "un\u00b7ter", "dem", "Ta\u00b7ge", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+--+--+-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Wohlangef\u00fchrt die Schafe des Himmels.", "tokens": ["Woh\u00b7lan\u00b7ge\u00b7f\u00fchrt", "die", "Scha\u00b7fe", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und Schnee, wie Maienblumen", "tokens": ["Und", "Schnee", ",", "wie", "Mai\u00b7en\u00b7blu\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das Edelm\u00fctige, wo", "tokens": ["Das", "E\u00b7del\u00b7m\u00fc\u00b7ti\u00b7ge", ",", "wo"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PWAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Es seie, bedeutend, gl\u00e4nzet mit", "tokens": ["Es", "sei\u00b7e", ",", "be\u00b7deu\u00b7tend", ",", "gl\u00e4n\u00b7zet", "mit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "$,", "VVFIN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Der gr\u00fcnen Wiese", "tokens": ["Der", "gr\u00fc\u00b7nen", "Wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Der Alpen, h\u00e4lftig, da ging", "tokens": ["Der", "Al\u00b7pen", ",", "h\u00e4lf\u00b7tig", ",", "da", "ging"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Vom Kreuze redend, das", "tokens": ["Vom", "Kreu\u00b7ze", "re\u00b7dend", ",", "das"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "PRELS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Gesetzt ist unterwegs einmal", "tokens": ["Ge\u00b7setzt", "ist", "un\u00b7ter\u00b7wegs", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gestorbenen, auf der schroffen Stra\u00df", "tokens": ["Ge\u00b7stor\u00b7be\u00b7nen", ",", "auf", "der", "schrof\u00b7fen", "Stra\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Ein Wandersmann mit", "tokens": ["Ein", "Wan\u00b7ders\u00b7mann", "mit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Dem andern, aber was ist dies?", "tokens": ["Dem", "an\u00b7dern", ",", "a\u00b7ber", "was", "ist", "dies", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PWS", "VAFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Feigenbaum ist mein", "tokens": ["Am", "Fei\u00b7gen\u00b7baum", "ist", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Achilles mir gestorben,", "tokens": ["A\u00b7chil\u00b7les", "mir", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Ajax liegt", "tokens": ["Und", "A\u00b7jax", "liegt"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "An den Grotten, nahe der See,", "tokens": ["An", "den", "Grot\u00b7ten", ",", "na\u00b7he", "der", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "An B\u00e4chen, benachbart dem Skamandros.", "tokens": ["An", "B\u00e4\u00b7chen", ",", "be\u00b7nach\u00b7bart", "dem", "Ska\u00b7man\u00b7dros", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Genius k\u00fchn ist bei Windessausen, nach", "tokens": ["Vom", "Ge\u00b7nius", "k\u00fchn", "ist", "bei", "Win\u00b7des\u00b7sau\u00b7sen", ",", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ADJD", "VAFIN", "APPR", "NN", "$,", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Der heimatlichen Salamis s\u00fc\u00dfer", "tokens": ["Der", "hei\u00b7mat\u00b7li\u00b7chen", "Sa\u00b7la\u00b7mis", "s\u00fc\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gewohnheit, in der Fremd", "tokens": ["Ge\u00b7wohn\u00b7heit", ",", "in", "der", "Fremd"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ajax gestorben,", "tokens": ["A\u00b7jax", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.10": {"text": "Patroklos aber in des K\u00f6niges Harnisch. Und es starben", "tokens": ["Pat\u00b7ro\u00b7klos", "a\u00b7ber", "in", "des", "K\u00f6\u00b7ni\u00b7ges", "Har\u00b7nisch", ".", "Und", "es", "star\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "NE", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Noch andere viel. Mit eigener Hand", "tokens": ["Noch", "an\u00b7de\u00b7re", "viel", ".", "Mit", "ei\u00b7ge\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Viel traurige, wilden Muts, doch g\u00f6ttlich", "tokens": ["Viel", "trau\u00b7ri\u00b7ge", ",", "wil\u00b7den", "Muts", ",", "doch", "g\u00f6tt\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Gezwungen, zuletzt, die anderen aber", "tokens": ["Ge\u00b7zwun\u00b7gen", ",", "zu\u00b7letzt", ",", "die", "an\u00b7de\u00b7ren", "a\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "$,", "PRELS", "PIS", "ADV"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Im Geschicke stehend, im Feld. Unwillig n\u00e4mlich", "tokens": ["Im", "Ge\u00b7schi\u00b7cke", "ste\u00b7hend", ",", "im", "Feld", ".", "Un\u00b7wil\u00b7lig", "n\u00e4m\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "$.", "ADJD", "ADV"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Sind Himmlische, wenn einer nicht die Seele schonend sich", "tokens": ["Sind", "Himm\u00b7li\u00b7sche", ",", "wenn", "ei\u00b7ner", "nicht", "die", "See\u00b7le", "scho\u00b7nend", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PIS", "PTKNEG", "ART", "NN", "ADJD", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Zusammengenommen, aber er mu\u00df doch; dem", "tokens": ["Zu\u00b7sam\u00b7men\u00b7ge\u00b7nom\u00b7men", ",", "a\u00b7ber", "er", "mu\u00df", "doch", ";", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "KON", "PPER", "VMFIN", "ADV", "$.", "ART"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Gleich fehlet die Trauer.", "tokens": ["Gleich", "feh\u00b7let", "die", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}