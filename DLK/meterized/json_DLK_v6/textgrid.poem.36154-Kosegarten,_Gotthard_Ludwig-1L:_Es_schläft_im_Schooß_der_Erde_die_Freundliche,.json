{"textgrid.poem.36154": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schl\u00e4ft im Schoo\u00df der Erde die Freundliche,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schl\u00e4ft im Schoo\u00df der Erde die Freundliche,", "tokens": ["Es", "schl\u00e4ft", "im", "Schoo\u00df", "der", "Er\u00b7de", "die", "Freund\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die mich gebar, und s\u00e4ugte, und auferzog.", "tokens": ["Die", "mich", "ge\u00b7bar", ",", "und", "s\u00e4ug\u00b7te", ",", "und", "auf\u00b7er\u00b7zog", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schon lange schl\u00e4ft sie, und der Knabe", "tokens": ["Schon", "lan\u00b7ge", "schl\u00e4ft", "sie", ",", "und", "der", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat ihr nicht Liebe noch Dank gestammelt.", "tokens": ["Hat", "ihr", "nicht", "Lie\u00b7be", "noch", "Dank", "ge\u00b7stam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Allein im J\u00fcnglingsbusen ergl\u00fcht sie mir,", "tokens": ["Al\u00b7lein", "im", "J\u00fcng\u00b7lings\u00b7bu\u00b7sen", "er\u00b7gl\u00fcht", "sie", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umrauscht mich oft in sinnender Einsamkeit,", "tokens": ["Um\u00b7rauscht", "mich", "oft", "in", "sin\u00b7nen\u00b7der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umweht mich in der Sommermondnacht,", "tokens": ["Um\u00b7weht", "mich", "in", "der", "Som\u00b7mer\u00b7mond\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kr\u00e4ftigt im Traum mich zu That und Tugend.", "tokens": ["Kr\u00e4f\u00b7tigt", "im", "Traum", "mich", "zu", "That", "und", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Sie schl\u00e4ft, mein trautes M\u00e4dchen, im k\u00fchlen Grab'", "tokens": ["Sie", "schl\u00e4ft", ",", "mein", "trau\u00b7tes", "M\u00e4d\u00b7chen", ",", "im", "k\u00fch\u00b7len", "Grab'"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch deine Mutter luftigen leisen Schlaf,", "tokens": ["Auch", "dei\u00b7ne", "Mut\u00b7ter", "luf\u00b7ti\u00b7gen", "lei\u00b7sen", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und lie\u00df dir, als sie schied, ihr Erbtheil,", "tokens": ["Und", "lie\u00df", "dir", ",", "als", "sie", "schied", ",", "ihr", "E\u00b7rbtheil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "F\u00fclle des Herzens und Weibesmilde.", "tokens": ["F\u00fcl\u00b7le", "des", "Her\u00b7zens", "und", "Wei\u00b7bes\u00b7mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Sie schl\u00e4ft. Ihr Schlaf sey friedlich und k\u00fchl und still!", "tokens": ["Sie", "schl\u00e4ft", ".", "Ihr", "Schlaf", "sey", "fried\u00b7lich", "und", "k\u00fchl", "und", "still", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allein ihr Angedenken erw\u00e4rmt dein Herz,", "tokens": ["Al\u00b7lein", "ihr", "An\u00b7ge\u00b7den\u00b7ken", "er\u00b7w\u00e4rmt", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entflammt auch mich \u2013 du hast die Flamme", "tokens": ["Ent\u00b7flammt", "auch", "mich", "\u2013", "du", "hast", "die", "Flam\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "$(", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In mir gez\u00fcndet \u2013 o Ruhm! o Wonne!", "tokens": ["In", "mir", "ge\u00b7z\u00fcn\u00b7det", "\u2013", "o", "Ruhm", "!", "o", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$(", "FM", "NN", "$.", "FM", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Hast mich in ihre letzten durchrungenen", "tokens": ["Hast", "mich", "in", "ih\u00b7re", "letz\u00b7ten", "durch\u00b7run\u00b7ge\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Durchk\u00e4mpften Tage, hast mich an ihre Gruft", "tokens": ["Durch\u00b7k\u00e4mpf\u00b7ten", "Ta\u00b7ge", ",", "hast", "mich", "an", "ih\u00b7re", "Gruft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und in das Heil'ge eures Grames,", "tokens": ["Und", "in", "das", "Heil'\u00b7ge", "eu\u00b7res", "Gra\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Theure, gef\u00fchrt \u2013 o Wonne! o Wehmuth!", "tokens": ["Theu\u00b7re", ",", "ge\u00b7f\u00fchrt", "\u2013", "o", "Won\u00b7ne", "!", "o", "Weh\u00b7muth", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$(", "FM", "NN", "$.", "FM", "NN", "$."], "meter": "+--+-+-+++", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Ich seh', ich seh' es, wie sie so sanft! so still!", "tokens": ["Ich", "seh'", ",", "ich", "seh'", "es", ",", "wie", "sie", "so", "sanft", "!", "so", "still", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADJD", "$.", "ADV", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Grabe welkte, wie sie den gro\u00dfen Schmerz", "tokens": ["Dem", "Gra\u00b7be", "welk\u00b7te", ",", "wie", "sie", "den", "gro\u00b7\u00dfen", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In sich verschlo\u00df, und ihren Kindern", "tokens": ["In", "sich", "ver\u00b7schlo\u00df", ",", "und", "ih\u00b7ren", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlichkeit l\u00e4chelt' aus matten Augen.", "tokens": ["Freund\u00b7lich\u00b7keit", "l\u00e4\u00b7chelt'", "aus", "mat\u00b7ten", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und wie des Todes eisernen Riesenschritt", "tokens": ["Und", "wie", "des", "To\u00b7des", "ei\u00b7ser\u00b7nen", "Rie\u00b7sen\u00b7schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie hallen h\u00f6rte, dumpfig und ferneher!", "tokens": ["Sie", "hal\u00b7len", "h\u00f6r\u00b7te", ",", "dump\u00b7fig", "und", "fer\u00b7ne\u00b7her", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ihr es Br\u00e4ut'gams Stimme d\u00e4uchte,", "tokens": ["Und", "ihr", "es", "Br\u00e4ut'\u00b7gams", "Stim\u00b7me", "d\u00e4uch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welcher die Braut in die Kammer winket.", "tokens": ["Wel\u00b7cher", "die", "Braut", "in", "die", "Kam\u00b7mer", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Und wie, des Herrn erquickenden Tag zu sehn,", "tokens": ["Und", "wie", ",", "des", "Herrn", "er\u00b7qui\u00b7cken\u00b7den", "Tag", "zu", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr sie ans Fenster rolltet, und Vorgef\u00fchl", "tokens": ["Ihr", "sie", "ans", "Fens\u00b7ter", "roll\u00b7tet", ",", "und", "Vor\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die fromme Dulderinn durchzuckte", "tokens": ["Die", "from\u00b7me", "Dul\u00b7de\u00b7rinn", "durch\u00b7zuck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jenes viel herrlichern hellern Tages.", "tokens": ["Je\u00b7nes", "viel", "herr\u00b7li\u00b7chern", "hel\u00b7lern", "Ta\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Und wie sie rang den ringenden letzten Kampf,", "tokens": ["Und", "wie", "sie", "rang", "den", "rin\u00b7gen\u00b7den", "letz\u00b7ten", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mit verh\u00fclltem Angesicht Ida stand,", "tokens": ["Und", "mit", "ver\u00b7h\u00fcll\u00b7tem", "An\u00b7ge\u00b7sicht", "I\u00b7da", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sie die letzte Hand ihr reichte,", "tokens": ["Und", "sie", "die", "letz\u00b7te", "Hand", "ihr", "reich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kraftlos sie dr\u00fcckte, und sank und einschlief!", "tokens": ["Kraft\u00b7los", "sie", "dr\u00fcck\u00b7te", ",", "und", "sank", "und", "ein\u00b7schlief", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Abend graut. Nun l\u00e4uten sie Todeston.", "tokens": ["Der", "A\u00b7bend", "graut", ".", "Nun", "l\u00e4u\u00b7ten", "sie", "To\u00b7des\u00b7ton", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nun tragen sie die Heilige dir hinaus.", "tokens": ["Nun", "tra\u00b7gen", "sie", "die", "Hei\u00b7li\u00b7ge", "dir", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der blinde Vater wankt. Es wanken", "tokens": ["Der", "blin\u00b7de", "Va\u00b7ter", "wankt", ".", "Es", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ida ihr nach, und die stummen S\u00f6hne.", "tokens": ["I\u00b7da", "ihr", "nach", ",", "und", "die", "stum\u00b7men", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Die Seile rasseln. Nieder die enge Gruft", "tokens": ["Die", "Sei\u00b7le", "ras\u00b7seln", ".", "Nie\u00b7der", "die", "en\u00b7ge", "Gruft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sinkt schon der Sarg. Die \u00e4ngstliche Schaufel schurrt.", "tokens": ["Sinkt", "schon", "der", "Sarg", ".", "Die", "\u00e4ngst\u00b7li\u00b7che", "Schau\u00b7fel", "schurrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die lockre Erde rollt hinunter \u2013", "tokens": ["Die", "lock\u00b7re", "Er\u00b7de", "rollt", "hin\u00b7un\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Decke sie leis' und los', o Erde!", "tokens": ["De\u00b7cke", "sie", "leis'", "und", "los'", ",", "o", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Nun zogen Regenschauer im Sturm herauf.", "tokens": ["Nun", "zo\u00b7gen", "Re\u00b7gen\u00b7schau\u00b7er", "im", "Sturm", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und nun ergossen rasselnd die Schauer sich.", "tokens": ["Und", "nun", "er\u00b7gos\u00b7sen", "ras\u00b7selnd", "die", "Schau\u00b7er", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ART", "NN", "PRF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von ihrem stillen Stern' herunter", "tokens": ["Von", "ih\u00b7rem", "stil\u00