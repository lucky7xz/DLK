{"textgrid.poem.61525": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gibt die Not dich wieder frei,", "tokens": ["Gibt", "die", "Not", "dich", "wie\u00b7der", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Pr\u00fcfe dich mit frommem Eifer,", "tokens": ["Pr\u00fc\u00b7fe", "dich", "mit", "from\u00b7mem", "Ei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, und wardst du drin nicht reifer,", "tokens": ["Ach", ",", "und", "wardst", "du", "drin", "nicht", "rei\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprich noch nicht: Sie ist vorbei.", "tokens": ["Sprich", "noch", "nicht", ":", "Sie", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}