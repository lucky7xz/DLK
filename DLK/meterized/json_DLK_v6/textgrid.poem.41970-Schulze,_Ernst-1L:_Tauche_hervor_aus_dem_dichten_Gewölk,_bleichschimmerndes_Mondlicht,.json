{"textgrid.poem.41970": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tauche hervor aus dem dichten Gew\u00f6lk, bleichschimmerndes Mondlicht,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tauche hervor aus dem dichten Gew\u00f6lk, bleichschimmerndes Mondlicht,", "tokens": ["Tau\u00b7che", "her\u00b7vor", "aus", "dem", "dich\u00b7ten", "Ge\u00b7w\u00f6lk", ",", "bleich\u00b7schim\u00b7mern\u00b7des", "Mond\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Leite des Liebenden Schritt durch die chaotische Nacht,", "tokens": ["Lei\u00b7te", "des", "Lie\u00b7ben\u00b7den", "Schritt", "durch", "die", "chao\u00b7ti\u00b7sche", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Und ihr Blumen der himmlischen Flur, h\u00fclfreiche Gestirne,", "tokens": ["Und", "ihr", "Blu\u00b7men", "der", "himm\u00b7li\u00b7schen", "Flur", ",", "h\u00fclf\u00b7rei\u00b7che", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sendet den traulichen Strahl auf den unendlichen Weg.", "tokens": ["Sen\u00b7det", "den", "trau\u00b7li\u00b7chen", "Strahl", "auf", "den", "un\u00b7end\u00b7li\u00b7chen", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Ueber die Weiten enteilt mit gefl\u00fcgeltem Fu\u00dfe die Hoffnung:", "tokens": ["Ue\u00b7ber", "die", "Wei\u00b7ten", "ent\u00b7eilt", "mit", "ge\u00b7fl\u00fc\u00b7gel\u00b7tem", "Fu\u00b7\u00dfe", "die", "Hoff\u00b7nung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber Verlangen erneut stets den gewandelten Pfad.", "tokens": ["A\u00b7ber", "Ver\u00b7lan\u00b7gen", "er\u00b7neut", "stets", "den", "ge\u00b7wan\u00b7del\u00b7ten", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Still durch's dichte Geb\u00fcsch hin dr\u00e4ng' ich mich, suche des Harzwalds", "tokens": ["Still", "durch's", "dich\u00b7te", "Ge\u00b7b\u00fcsch", "hin", "dr\u00e4ng'", "ich", "mich", ",", "su\u00b7che", "des", "Harz\u00b7walds"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Oedesten Steig, und scheu beb' ich vor jeglichem West.", "tokens": ["O\u00b7e\u00b7des\u00b7ten", "Steig", ",", "und", "scheu", "beb'", "ich", "vor", "jeg\u00b7li\u00b7chem", "West", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.9": {"text": "Lieb' ist ein Bl\u00fcmchen der Flur, s\u00fc\u00dfduftend im heiteren Mayglanz:", "tokens": ["Lieb'", "ist", "ein", "Bl\u00fcm\u00b7chen", "der", "Flur", ",", "s\u00fc\u00df\u00b7duf\u00b7tend", "im", "hei\u00b7te\u00b7ren", "May\u00b7glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Aber dem lindesten Hauch weicht es und f\u00fcrchtet Verrath.", "tokens": ["A\u00b7ber", "dem", "lin\u00b7des\u00b7ten", "Hauch", "weicht", "es", "und", "f\u00fcrch\u00b7tet", "Ver\u00b7rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "KON", "VVFIN", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "K\u00e4m' auch ein Freund entgegen mir itzt, er w\u00fcrde zum Feind mir;", "tokens": ["K\u00e4m'", "auch", "ein", "Freund", "ent\u00b7ge\u00b7gen", "mir", "itzt", ",", "er", "w\u00fcr\u00b7de", "zum", "Feind", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "PPER", "ADV", "$,", "PPER", "VAFIN", "APPRART", "NN", "PPER", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Hebt sich die Sonne, so flieht jeglicher andere Schein.", "tokens": ["Hebt", "sich", "die", "Son\u00b7ne", ",", "so", "flieht", "jeg\u00b7li\u00b7cher", "an\u00b7de\u00b7re", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "S\u00fc\u00df ist traulicher Freunde Gespr\u00e4ch und s\u00fc\u00df die Umarmung,", "tokens": ["S\u00fc\u00df", "ist", "trau\u00b7li\u00b7cher", "Freun\u00b7de", "Ge\u00b7spr\u00e4ch", "und", "s\u00fc\u00df", "die", "Um\u00b7ar\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Aber verg\u00fctet sie mir, was mir im Z\u00f6gern entfloh?", "tokens": ["A\u00b7ber", "ver\u00b7g\u00fc\u00b7tet", "sie", "mir", ",", "was", "mir", "im", "Z\u00f6\u00b7gern", "ent\u00b7floh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Kennt sie den Zauber der hoffenden Brust, wenn stets die Erwartung", "tokens": ["Kennt", "sie", "den", "Zau\u00b7ber", "der", "hof\u00b7fen\u00b7den", "Brust", ",", "wenn", "stets", "die", "Er\u00b7war\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "An dem entbl\u00fcheten Gl\u00fcck keimende Knospen noch beut?", "tokens": ["An", "dem", "ent\u00b7bl\u00fc\u00b7he\u00b7ten", "Gl\u00fcck", "kei\u00b7men\u00b7de", "Knos\u00b7pen", "noch", "beut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Ruhiges Gl\u00fcck ist wahrlich ein Gl\u00fcck, doch Schmerz und Entsagung,", "tokens": ["Ru\u00b7hi\u00b7ges", "Gl\u00fcck", "ist", "wahr\u00b7lich", "ein", "Gl\u00fcck", ",", "doch", "Schmerz", "und", "Ent\u00b7sa\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Hoffnung, Sehnen und Lust flechten den bunteren Kranz.", "tokens": ["Hoff\u00b7nung", ",", "Seh\u00b7nen", "und", "Lust", "flech\u00b7ten", "den", "bun\u00b7te\u00b7ren", "Kranz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Ach, wohl harret sie meiner vielleicht; vom umdufteten Altan", "tokens": ["Ach", ",", "wohl", "har\u00b7ret", "sie", "mei\u00b7ner", "viel\u00b7leicht", ";", "vom", "um\u00b7duf\u00b7te\u00b7ten", "Al\u00b7tan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "ADV", "$.", "APPRART", "ADJA", "NN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.20": {"text": "Schaut sie sp\u00e4hend hinaus in die entfaltete Nacht;", "tokens": ["Schaut", "sie", "sp\u00e4\u00b7hend", "hin\u00b7aus", "in", "die", "ent\u00b7fal\u00b7te\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+------+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Sehnsucht ziehet den magischen Kreis um die Reizende, pfeilschnell", "tokens": ["Sehn\u00b7sucht", "zie\u00b7het", "den", "ma\u00b7gi\u00b7schen", "Kreis", "um", "die", "Rei\u00b7zen\u00b7de", ",", "pfeil\u00b7schnell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Flattern die Bilder herbei, welche die Zauberin ruft.", "tokens": ["Flat\u00b7tern", "die", "Bil\u00b7der", "her\u00b7bei", ",", "wel\u00b7che", "die", "Zau\u00b7be\u00b7rin", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Sieh, das erzitternde Blatt und der leicht hinschl\u00fcpfende Vogel,", "tokens": ["Sieh", ",", "das", "er\u00b7zit\u00b7tern\u00b7de", "Blatt", "und", "der", "leicht", "hin\u00b7schl\u00fcp\u00b7fen\u00b7de", "Vo\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "KON", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Was nur s\u00e4uselt und rauscht bringet ihr Kunde von mir.", "tokens": ["Was", "nur", "s\u00e4u\u00b7selt", "und", "rauscht", "brin\u00b7get", "ihr", "Kun\u00b7de", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Jetzt im h\u00fcpfenden Strahl und im Schattengebild des Gestr\u00e4uchs jetzt,", "tokens": ["Jetzt", "im", "h\u00fcp\u00b7fen\u00b7den", "Strahl", "und", "im", "Schat\u00b7ten\u00b7ge\u00b7bild", "des", "Ge\u00b7str\u00e4uchs", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "KON", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Jetzt im alternden Stamm hofft und erkennt sie mich,", "tokens": ["Jetzt", "im", "al\u00b7tern\u00b7den", "Stamm", "hofft", "und", "er\u00b7kennt", "sie", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Und sie enteilt mit klopfender Brust, ein lispelnder Gru\u00df schon", "tokens": ["Und", "sie", "ent\u00b7eilt", "mit", "klop\u00b7fen\u00b7der", "Brust", ",", "ein", "lis\u00b7peln\u00b7der", "Gru\u00df", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Schwellt ihr die Lippe, doch rasch schwindet der eitele Traum.", "tokens": ["Schwellt", "ihr", "die", "Lip\u00b7pe", ",", "doch", "rasch", "schwin\u00b7det", "der", "ei\u00b7te\u00b7le", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "++-+---+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Z\u00fcrnend entsagt sie dem t\u00e4uschenden Wahn und schw\u00f6rt, dem Betrug nicht", "tokens": ["Z\u00fcr\u00b7nend", "ent\u00b7sagt", "sie", "dem", "t\u00e4u\u00b7schen\u00b7den", "Wahn", "und", "schw\u00f6rt", ",", "dem", "Be\u00b7trug", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "VVFIN", "$,", "ART", "NN", "PTKNEG"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Ferner zu trauen, und doch t\u00e4uscht sie der andre Moment.", "tokens": ["Fer\u00b7ner", "zu", "trau\u00b7en", ",", "und", "doch", "t\u00e4uscht", "sie", "der", "and\u00b7re", "Mo\u00b7ment", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Oder es wand um die Harrende wohl nach langer Erwartung", "tokens": ["O\u00b7der", "es", "wand", "um", "die", "Har\u00b7ren\u00b7de", "wohl", "nach", "lan\u00b7ger", "Er\u00b7war\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Leise der sch\u00fcchterne Schlaf seinen ambrosischen Arm;", "tokens": ["Lei\u00b7se", "der", "sch\u00fcch\u00b7ter\u00b7ne", "Schlaf", "sei\u00b7nen", "am\u00b7bro\u00b7si\u00b7schen", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.33": {"text": "Liebend rang er mit ihr, und mit s\u00e4uselnden Fittigen buhlt' er", "tokens": ["Lie\u00b7bend", "rang", "er", "mit", "ihr", ",", "und", "mit", "s\u00e4u\u00b7seln\u00b7den", "Fit\u00b7ti\u00b7gen", "buhlt'", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "S\u00fc\u00df um die Wimpern, und go\u00df lauliche D\u00fcfte herab,", "tokens": ["S\u00fc\u00df", "um", "die", "Wim\u00b7pern", ",", "und", "go\u00df", "lau\u00b7li\u00b7che", "D\u00fcf\u00b7te", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.35": {"text": "Schlang um den Busen der Reizenden sich, und fl\u00fcsterte zart wie", "tokens": ["Schlang", "um", "den", "Bu\u00b7sen", "der", "Rei\u00b7zen\u00b7den", "sich", ",", "und", "fl\u00fcs\u00b7ter\u00b7te", "zart", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PRF", "$,", "KON", "VVFIN", "ADJD", "KOKOM"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Wellengeriesel und West luftige Lieder ihr zu;", "tokens": ["Wel\u00b7len\u00b7ge\u00b7rie\u00b7sel", "und", "West", "luf\u00b7ti\u00b7ge", "Lie\u00b7der", "ihr", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Und sie erlag, wie die Blume den Schoo\u00df in der schweigenden Nacht schlie\u00dft;", "tokens": ["Und", "sie", "er\u00b7lag", ",", "wie", "die", "Blu\u00b7me", "den", "Schoo\u00df", "in", "der", "schwei\u00b7gen\u00b7den", "Nacht", "schlie\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "In sein Feengefild f\u00fchrt er die liebliche Braut.", "tokens": ["In", "sein", "Fe\u00b7en\u00b7ge\u00b7fild", "f\u00fchrt", "er", "die", "lieb\u00b7li\u00b7che", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.39": {"text": "Sieh, er versammelt den gaukelnden Kreis fantastischer Tr\u00e4ume;", "tokens": ["Sieh", ",", "er", "ver\u00b7sam\u00b7melt", "den", "gau\u00b7keln\u00b7den", "Kreis", "fan\u00b7tas\u00b7ti\u00b7scher", "Tr\u00e4u\u00b7me", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.40": {"text": "Um die Gebieterin her reihen die Fr\u00f6hlichen sich;", "tokens": ["Um", "die", "Ge\u00b7bie\u00b7te\u00b7rin", "her", "rei\u00b7hen", "die", "Fr\u00f6h\u00b7li\u00b7chen", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APZR", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.41": {"text": "Diese bekr\u00e4nzen mit Rosen die Brust, und wiegen sich freundlich", "tokens": ["Die\u00b7se", "be\u00b7kr\u00e4n\u00b7zen", "mit", "Ro\u00b7sen", "die", "Brust", ",", "und", "wie\u00b7gen", "sich", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "ADJD"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Im labyrinthischen Kelch, leise, wie Gri