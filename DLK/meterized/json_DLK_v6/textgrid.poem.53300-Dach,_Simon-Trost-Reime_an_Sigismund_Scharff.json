{"textgrid.poem.53300": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Reime an Sigismund Scharff", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So g\u00e4ntzlich ist auff nichts allhie zu bawen,", "tokens": ["So", "g\u00e4ntz\u00b7lich", "ist", "auff", "nichts", "all\u00b7hie", "zu", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kein Gl\u00fcck \u00fcbt stete Trew,", "tokens": ["Kein", "Gl\u00fcck", "\u00fcbt", "ste\u00b7te", "Trew", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob Frewde sich noch eins so gro\u00df l\u00e4st schawen,", "tokens": ["Ob", "Frew\u00b7de", "sich", "noch", "eins", "so", "gro\u00df", "l\u00e4st", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADV", "PIS", "ADV", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Da\u00df sie vollkommen sey;", "tokens": ["Da\u00df", "sie", "voll\u00b7kom\u00b7men", "sey", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wil mag vmb sie schweben,", "tokens": ["Wer", "wil", "mag", "vmb", "sie", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich traw' jhr nimmermehr,", "tokens": ["Ich", "tra\u00b7w'", "jhr", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wil blo\u00df mich Gott ergeben,", "tokens": ["Wil", "blo\u00df", "mich", "Gott", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "NN", "VVPP", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Sie treuget gar zu sehr.", "tokens": ["Sie", "treu\u00b7get", "gar", "zu", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O grosser Gott, wie hab ich \u00fcberschlagen", "tokens": ["O", "gros\u00b7ser", "Gott", ",", "wie", "hab", "ich", "\u00fc\u00b7bersc\u00b7hla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt so offt vnd viel?", "tokens": ["Die", "Welt", "so", "offt", "vnd", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich nehme f\u00fcr die Kron' vnd Scepter tragen", "tokens": ["Ich", "neh\u00b7me", "f\u00fcr", "die", "Kron'", "vnd", "Scep\u00b7ter", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd forsch' ohn' End vnd Ziel,", "tokens": ["Vnd", "for\u00b7sch'", "ohn'", "End", "vnd", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Ob hie nicht sey zu finden", "tokens": ["Ob", "hie", "nicht", "sey", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Worauff ich fussen kan;", "tokens": ["Wo\u00b7rauff", "ich", "fus\u00b7sen", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie vnd jhr Pracht mu\u00df schwinden", "tokens": ["Sie", "vnd", "jhr", "Pracht", "mu\u00df", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie sonst ein BawersMann.", "tokens": ["Wie", "sonst", "ein", "Ba\u00b7wers", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonne, selbst die Fackel dieser Erden,", "tokens": ["Die", "Son\u00b7ne", ",", "selbst", "die", "Fa\u00b7ckel", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheint t\u00e4glich au\u00dfzugehn,", "tokens": ["Scheint", "t\u00e4g\u00b7lich", "au\u00df\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was vormals ward, wird vnd hernach kan werden,", "tokens": ["Was", "vor\u00b7mals", "ward", ",", "wird", "vnd", "her\u00b7nach", "kan", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VAFIN", "KON", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bleibt nicht die L\u00e4nge stehn.", "tokens": ["Bleibt", "nicht", "die", "L\u00e4n\u00b7ge", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Worauff ich noch kan kommen", "tokens": ["Wo\u00b7rauff", "ich", "noch", "kan", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Forschung, Gott, bist du,", "tokens": ["In", "For\u00b7schung", ",", "Gott", ",", "bist", "du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "VAFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du bist das Gut der Frommen", "tokens": ["Du", "bist", "das", "Gut", "der", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd vnsre wahre Ruh.", "tokens": ["Vnd", "vns\u00b7re", "wah\u00b7re", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr mercket wol, Herr Scharff, was ich wil sprechen,", "tokens": ["Ihr", "mer\u00b7cket", "wol", ",", "Herr", "Scharff", ",", "was", "ich", "wil", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "NE", "$,", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin jhr euch gewandt,", "tokens": ["Wo\u00b7hin", "jhr", "euch", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sahet jhr euch nichts an Lust gebrechen,", "tokens": ["Da", "sa\u00b7het", "jhr", "euch", "nichts", "an", "Lust", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil Gott mit reicher Hand", "tokens": ["Weil", "Gott", "mit", "rei\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das alles euch gewehret,", "tokens": ["Das", "al\u00b7les", "euch", "ge\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Warumb ein Mensch sich frisst,", "tokens": ["Wa\u00b7rumb", "ein", "Mensch", "sich", "frisst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd was ein Hertz begehret", "tokens": ["Vnd", "was", "ein", "Hertz", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Das zu vergn\u00fcgen ist.", "tokens": ["Das", "zu", "ver\u00b7gn\u00fc\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Liebsten nur jn etwas zu gedencken", "tokens": ["Der", "Liebs\u00b7ten", "nur", "jn", "et\u00b7was", "zu", "ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPER", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo jhr mich lassen wollt,", "tokens": ["Wo", "jhr", "mich", "las\u00b7sen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kunt' euch mehr als F\u00fcrsten-G\u00fcter schencken,", "tokens": ["Sie", "kunt'", "euch", "mehr", "als", "F\u00fcrs\u00b7ten\u00b7G\u00fc\u00b7ter", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie war euch alles Gold.", "tokens": ["Sie", "war", "euch", "al\u00b7les", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "M\u00f6cht' euch Geschlecht anlachen?", "tokens": ["M\u00f6cht'", "euch", "Ge\u00b7schlecht", "an\u00b7la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wer gieng jhr hie gro\u00df vor?", "tokens": ["Wer", "gieng", "jhr", "hie", "gro\u00df", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des besten Gl\u00fcckes Sachen?", "tokens": ["Des", "bes\u00b7ten", "Gl\u00fc\u00b7ckes", "Sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die huben sie empor.", "tokens": ["Die", "hu\u00b7ben", "sie", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Des Leibes Pracht? wer sah' an jhr wol Tadel?", "tokens": ["Des", "Lei\u00b7bes", "Pracht", "?", "wer", "sah'", "an", "jhr", "wol", "Ta\u00b7del", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PWS", "VVFIN", "APPR", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der g\u00fcldnen Tugend Zier,", "tokens": ["Der", "g\u00fcld\u00b7nen", "Tu\u00b7gend", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Vnschuld Lob, der Menschen bester Adel?", "tokens": ["Der", "Vn\u00b7schuld", "Lob", ",", "der", "Men\u00b7schen", "bes\u00b7ter", "A\u00b7del", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das war vorau\u00df bey jhr,", "tokens": ["Das", "war", "vor\u00b7au\u00df", "bey", "jhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Warumb sie au\u00dferlesen.", "tokens": ["Wa\u00b7rumb", "sie", "au\u00b7\u00dfer\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was aber f\u00fcr Gewinn", "tokens": ["Was", "a\u00b7ber", "f\u00fcr", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist sie euch lang gewesen?", "tokens": ["Ist", "sie", "euch", "lang", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Man tr\u00e4gt sie schon dahin.", "tokens": ["Man", "tr\u00e4gt", "sie", "schon", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr weinet recht: Wen das nicht wolt erbarmen", "tokens": ["Ihr", "wei\u00b7net", "recht", ":", "Wen", "das", "nicht", "wolt", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PWS", "PDS", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der w\u00e4r ein Stock vnd Bley.", "tokens": ["Der", "w\u00e4r", "ein", "Stock", "vnd", "Bley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr werdet sie hinfort nicht mehr vmbarmen", "tokens": ["Ihr", "wer\u00b7det", "sie", "hin\u00b7fort", "nicht", "mehr", "vm\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In h\u00f6chster Lieb' vnd Trew',", "tokens": ["In", "h\u00f6chs\u00b7ter", "Lieb'", "vnd", "Tre\u00b7w'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach was jhr vor erkohren,", "tokens": ["Ach", "was", "jhr", "vor", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "PPER", "APPR", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Kan jetzt nicht tauglich seyn,", "tokens": ["Kan", "jetzt", "nicht", "taug\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dieweil jhr habt verlohren", "tokens": ["Die\u00b7weil", "jhr", "habt", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Sie, ewers Gl\u00fcckes Schein.", "tokens": ["Sie", ",", "e\u00b7wers", "Gl\u00fc\u00b7ckes", "Schein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So gehet es mit diesen eiteln Sachen,", "tokens": ["So", "ge\u00b7het", "es", "mit", "die\u00b7sen", "ei\u00b7teln", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Di\u00df ist der Welt Gemerck.", "tokens": ["Di\u00df", "ist", "der", "Welt", "Ge\u00b7merck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer h\u00f6ret je sie etwas anders machen", "tokens": ["Wer", "h\u00f6\u00b7ret", "je", "sie", "et\u00b7was", "an\u00b7ders", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als di\u00df jhr altes Werck?", "tokens": ["Als", "di\u00df", "jhr", "al\u00b7tes", "Werck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Wesen dieses Lebens", "tokens": ["Das", "We\u00b7sen", "die\u00b7ses", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Findt in sich keine statt,", "tokens": ["Findt", "in", "sich", "kei\u00b7ne", "statt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man sucht darin vergebens", "tokens": ["Man", "sucht", "da\u00b7rin", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PAV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das, was es selbst nicht hat.", "tokens": ["Das", ",", "was", "es", "selbst", "nicht", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "i