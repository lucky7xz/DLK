{"textgrid.poem.64251": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch wahrer als der Blick des Auges k\u00fcndet", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch wahrer als der Blick des Auges k\u00fcndet", "tokens": ["Noch", "wah\u00b7rer", "als", "der", "Blick", "des", "Au\u00b7ges", "k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Menschen Eigenart der Stimme Klang.", "tokens": ["Des", "Men\u00b7schen", "Ei\u00b7gen\u00b7art", "der", "Stim\u00b7me", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es kann das Auge t\u00e4uschen, nicht die Stimme!", "tokens": ["Es", "kann", "das", "Au\u00b7ge", "t\u00e4u\u00b7schen", ",", "nicht", "die", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie dr\u00fcckt den tiefsten Kern des Wesens aus,", "tokens": ["Sie", "dr\u00fcckt", "den", "tiefs\u00b7ten", "Kern", "des", "We\u00b7sens", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Volkes Stimme, \u2013 das ist Volkes Sprache.", "tokens": ["Und", "Vol\u00b7kes", "Stim\u00b7me", ",", "\u2013", "das", "ist", "Vol\u00b7kes", "Spra\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,", "$(", "PDS", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie k\u00fcndet wahrer, unverf\u00e4lschlicher", "tokens": ["Sie", "k\u00fcn\u00b7det", "wah\u00b7rer", ",", "un\u00b7ver\u00b7f\u00e4l\u00b7schli\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Als Aug' und Haar und Antlitz und Gestalt", "tokens": ["Als", "Aug'", "und", "Haar", "und", "Ant\u00b7litz", "und", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des Volkes Seele. \u2013 Weh darum dem Volk,", "tokens": ["Des", "Vol\u00b7kes", "See\u00b7le", ".", "\u2013", "Weh", "da\u00b7rum", "dem", "Volk", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "$(", "NN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das seiner Sprache Heiligtum nicht ehrt", "tokens": ["Das", "sei\u00b7ner", "Spra\u00b7che", "Hei\u00b7lig\u00b7tum", "nicht", "ehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "NE", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und liebt und sch\u00fctzt und pflegt mit frommer Treue.", "tokens": ["Und", "liebt", "und", "sch\u00fctzt", "und", "pflegt", "mit", "from\u00b7mer", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kein Splitter unsers Volkstums ist verloren,", "tokens": ["Kein", "Split\u00b7ter", "un\u00b7sers", "Volks\u00b7tums", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Kein Haus und keine H\u00fctte noch so fern,", "tokens": ["Kein", "Haus", "und", "kei\u00b7ne", "H\u00fct\u00b7te", "noch", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Darin noch deutsch das Vaterunser t\u00f6nt.", "tokens": ["Da\u00b7rin", "noch", "deutsch", "das", "Va\u00b7ter\u00b7un\u00b7ser", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So sch\u00fctzt die deutsche Sprache \u00fcberall,", "tokens": ["So", "sch\u00fctzt", "die", "deut\u00b7sche", "Spra\u00b7che", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ihr sch\u00fctzt damit zugleich die eigne Seele.", "tokens": ["Ihr", "sch\u00fctzt", "da\u00b7mit", "zu\u00b7gleich", "die", "eig\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die Sprache zieht mit uns im Schritt der Heere", "tokens": ["Die", "Spra\u00b7che", "zieht", "mit", "uns", "im", "Schritt", "der", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sie schwebt mit uns im Schiff durchs blaue Meer", "tokens": ["Sie", "schwebt", "mit", "uns", "im", "Schiff", "durchs", "blau\u00b7e", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und baut, wo immer ihre Sch\u00f6ne klingt,", "tokens": ["Und", "baut", ",", "wo", "im\u00b7mer", "ih\u00b7re", "Sch\u00f6\u00b7ne", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Uns sieghaft eine neue deutsche Welt.", "tokens": ["Uns", "sieg\u00b7haft", "ei\u00b7ne", "neu\u00b7e", "deut\u00b7sche", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}