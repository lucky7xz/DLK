{"dta.poem.15638": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Menschenwitze wars vonje die schwerste Plage,", "tokens": ["Dem", "Men\u00b7schen\u00b7wit\u00b7ze", "wars", "von\u00b7je", "die", "schwers\u00b7te", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie seine Freiheit sich mit Gottes Rath vertrage.", "tokens": ["Wie", "sei\u00b7ne", "Frei\u00b7heit", "sich", "mit", "Got\u00b7tes", "Rath", "ver\u00b7tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die zwei vertragen sich durch eine Auskunft blo\u00df:", "tokens": ["Die", "zwei", "ver\u00b7tra\u00b7gen", "sich", "durch", "ei\u00b7ne", "Aus\u00b7kunft", "blo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Spielraum, Mensch, ist klein, der Gottes ist gar gro\u00df.", "tokens": ["Dein", "Spiel\u00b7raum", ",", "Mensch", ",", "ist", "klein", ",", "der", "Got\u00b7tes", "ist", "gar", "gro\u00df", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du magst in deinem Raum mit Freiheit dich geberden,", "tokens": ["Du", "magst", "in", "dei\u00b7nem", "Raum", "mit", "Frei\u00b7heit", "dich", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch dich unselig auch, durch dich auch selig werden.", "tokens": ["Durch", "dich", "un\u00b7se\u00b7lig", "auch", ",", "durch", "dich", "auch", "se\u00b7lig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ADV", "$,", "APPR", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Er aber hat es vorgesehn und vorgedacht,", "tokens": ["Er", "a\u00b7ber", "hat", "es", "vor\u00b7ge\u00b7sehn", "und", "vor\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df all dein Wille nur den seinen wirklich macht.", "tokens": ["Da\u00df", "all", "dein", "Wil\u00b7le", "nur", "den", "sei\u00b7nen", "wirk\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "ADV", "ART", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}