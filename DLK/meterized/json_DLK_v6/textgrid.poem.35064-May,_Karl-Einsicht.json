{"textgrid.poem.35064": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Einsicht", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau nicht, schau nicht so um dich her,", "tokens": ["Schau", "nicht", ",", "schau", "nicht", "so", "um", "dich", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADJD", "PTKNEG", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Als ob da deine Welt sich breite.", "tokens": ["Als", "ob", "da", "dei\u00b7ne", "Welt", "sich", "brei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Erde nicht und nicht das Meer,", "tokens": ["Die", "Er\u00b7de", "nicht", "und", "nicht", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zieh deinen Blick hinaus ins Weite.", "tokens": ["Zieh", "dei\u00b7nen", "Blick", "hin\u00b7aus", "ins", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du wohnst hier nur im Wanderzelt;", "tokens": ["Du", "wohnst", "hier", "nur", "im", "Wan\u00b7der\u00b7zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Heimath fordert all dein Sinnen,", "tokens": ["Die", "Hei\u00b7math", "for\u00b7dert", "all", "dein", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und suchst du ", "tokens": ["Und", "suchst", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "So richte deinen Blick nach innen.", "tokens": ["So", "rich\u00b7te", "dei\u00b7nen", "Blick", "nach", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bau nicht, bau nicht ein festes Haus", "tokens": ["Bau", "nicht", ",", "bau", "nicht", "ein", "fes\u00b7tes", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Als Heim auf irdschem Grund und Boden;", "tokens": ["Als", "Heim", "auf", "ird\u00b7schem", "Grund", "und", "Bo\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man tr\u00e4gt dich doch dereinst hinaus", "tokens": ["Man", "tr\u00e4gt", "dich", "doch", "de\u00b7reinst", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und legt als todt dich zu den Todten.", "tokens": ["Und", "legt", "als", "todt", "dich", "zu", "den", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wohin du lebst und denkst, zu schauen,", "tokens": ["Wo\u00b7hin", "du", "lebst", "und", "denkst", ",", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und jede That und jedes Wort", "tokens": ["Und", "je\u00b7de", "That", "und", "je\u00b7des", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tr\u00e4gst du ihm zu, um es zu bauen.", "tokens": ["Tr\u00e4gst", "du", "ihm", "zu", ",", "um", "es", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Trau nicht, trau nicht dem eb'nen Weg,", "tokens": ["Trau", "nicht", ",", "trau", "nicht", "dem", "eb'\u00b7nen", "Weg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Den Tausende durchs Leben wandern.", "tokens": ["Den", "Tau\u00b7sen\u00b7de", "durchs", "Le\u00b7ben", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weich ab, weich ab zum steilen Steg,", "tokens": ["Weich", "ab", ",", "weich", "ab", "zum", "stei\u00b7len", "Steg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und la\u00df sie l\u00e4cheln, all die Andern.", "tokens": ["Und", "la\u00df", "sie", "l\u00e4\u00b7cheln", ",", "all", "die", "An\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "$,", "PIAT", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh auf die Thoren nicht zur\u00fcck,", "tokens": ["Sieh", "auf", "die", "Tho\u00b7ren", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und achte nicht auf ihre Stimmen;", "tokens": ["Und", "ach\u00b7te", "nicht", "auf", "ih\u00b7re", "Stim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wisse wohl, ", "tokens": ["Denn", "wis\u00b7se", "wohl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Liegt hoch und l\u00e4\u00dft sich nur erklimmen.", "tokens": ["Liegt", "hoch", "und", "l\u00e4\u00dft", "sich", "nur", "er\u00b7klim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}