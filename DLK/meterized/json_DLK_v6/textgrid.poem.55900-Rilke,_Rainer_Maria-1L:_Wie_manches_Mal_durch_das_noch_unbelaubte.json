{"textgrid.poem.55900": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie manches Mal durch das noch unbelaubte", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie manches Mal durch das noch unbelaubte", "tokens": ["Wie", "man\u00b7ches", "Mal", "durch", "das", "noch", "un\u00b7be\u00b7laub\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gezweig ein Morgen durchsieht, der schon ganz", "tokens": ["Ge\u00b7zweig", "ein", "Mor\u00b7gen", "durch\u00b7sieht", ",", "der", "schon", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "im Fr\u00fchling ist: so ist in seinem Haupte", "tokens": ["im", "Fr\u00fch\u00b7ling", "ist", ":", "so", "ist", "in", "sei\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "$.", "ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nichts was verhindern k\u00f6nnte, da\u00df der Glanz", "tokens": ["nichts", "was", "ver\u00b7hin\u00b7dern", "k\u00f6nn\u00b7te", ",", "da\u00df", "der", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "PIS", "VVINF", "VMFIN", "$,", "KOUS", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "aller Gedichte uns fast t\u00f6dlich tr\u00e4fe;", "tokens": ["al\u00b7ler", "Ge\u00b7dich\u00b7te", "uns", "fast", "t\u00f6d\u00b7lich", "tr\u00e4\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "denn noch kein Schatten ist in seinem Schaun,", "tokens": ["denn", "noch", "kein", "Schat\u00b7ten", "ist", "in", "sei\u00b7nem", "Schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu k\u00fchl f\u00fcr Lorbeer sind noch seine Schl\u00e4fe", "tokens": ["zu", "k\u00fchl", "f\u00fcr", "Lor\u00b7beer", "sind", "noch", "sei\u00b7ne", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "APPR", "NE", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und sp\u00e4ter erst wird aus den Augenbraun", "tokens": ["und", "sp\u00e4\u00b7ter", "erst", "wird", "aus", "den", "Au\u00b7gen\u00b7braun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "hochst\u00e4mmig sich der Rosengarten heben,", "tokens": ["hoch\u00b7st\u00e4m\u00b7mig", "sich", "der", "Ro\u00b7sen\u00b7gar\u00b7ten", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus welchem Bl\u00e4tter, einzeln, ausgel\u00f6st", "tokens": ["aus", "wel\u00b7chem", "Bl\u00e4t\u00b7ter", ",", "ein\u00b7zeln", ",", "aus\u00b7ge\u00b7l\u00f6st"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "hintreiben werden auf des Mundes Beben,", "tokens": ["hin\u00b7trei\u00b7ben", "wer\u00b7den", "auf", "des", "Mun\u00b7des", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "der jetzt noch still ist, niegebraucht und blinkend", "tokens": ["der", "jetzt", "noch", "still", "ist", ",", "nie\u00b7ge\u00b7braucht", "und", "blin\u00b7kend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und nur mit seinem L\u00e4cheln etwas trinkend", "tokens": ["und", "nur", "mit", "sei\u00b7nem", "L\u00e4\u00b7cheln", "et\u00b7was", "trin\u00b7kend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als w\u00fcrde ihm sein Singen eingefl\u00f6\u00dft.", "tokens": ["als", "w\u00fcr\u00b7de", "ihm", "sein", "Sin\u00b7gen", "ein\u00b7ge\u00b7fl\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}