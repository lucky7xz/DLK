{"dta.poem.20822": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich dann hin und wieder dachte/", "tokens": ["Weil", "ich", "dann", "hin", "und", "wie\u00b7der", "dach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKVZ", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich an dieser Regung hing?", "tokens": ["Wie", "ich", "an", "die\u00b7ser", "Re\u00b7gung", "hing", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mein Gem\u00fcthe sich betrachte/", "tokens": ["Und", "mein", "Ge\u00b7m\u00fc\u00b7the", "sich", "be\u00b7trach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ihm der Reim von statten gieng/", "tokens": ["Wie", "ihm", "der", "Reim", "von", "stat\u00b7ten", "gieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bliesen die Getichts-G\u00f6ttinnen/", "tokens": ["So", "blie\u00b7sen", "die", "Ge\u00b7tichts\u00b7G\u00f6t\u00b7tin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zuvor erz\u00f6rnt auf mein Beginnen/", "tokens": ["Zu\u00b7vor", "er\u00b7z\u00f6rnt", "auf", "mein", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich itzt mit klugem Athen an.", "tokens": ["Mich", "itzt", "mit", "klu\u00b7gem", "A\u00b7then", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie pflantzten mir in das Gem\u00fcthe/", "tokens": ["Sie", "pflantz\u00b7ten", "mir", "in", "das", "Ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Ein klein Verm\u00f6gen/ dessen G\u00fcte", "tokens": ["Ein", "klein", "Ver\u00b7m\u00f6\u00b7gen", "/", "des\u00b7sen", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "NN", "$(", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich kleiner Torheit gleichen kan.", "tokens": ["Sich", "klei\u00b7ner", "Tor\u00b7heit", "glei\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}