{"textgrid.poem.26139": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wenn die Wolken sich hei\u00df den Liebeshof machen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein lechzend Gewitter durch den Nachmittag strich", "tokens": ["Ein", "lech\u00b7zend", "Ge\u00b7wit\u00b7ter", "durch", "den", "Nach\u00b7mit\u00b7tag", "strich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und krepierend hinter die Berge hinschlich.", "tokens": ["Und", "kre\u00b7pie\u00b7rend", "hin\u00b7ter", "die", "Ber\u00b7ge", "hin\u00b7schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als lagen Drachen im Liebeskampf,", "tokens": ["Als", "la\u00b7gen", "Dra\u00b7chen", "im", "Lie\u00b7bes\u00b7kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umbr\u00fcllten sich Wolken mit dumpfem Gestampf.", "tokens": ["Um\u00b7br\u00fcll\u00b7ten", "sich", "Wol\u00b7ken", "mit", "dum\u00b7pfem", "Ge\u00b7stampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wenn die Wolken sich hei\u00df den Liebeshof machen,", "tokens": ["Wenn", "die", "Wol\u00b7ken", "sich", "hei\u00df", "den", "Lie\u00b7bes\u00b7hof", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Sitzt grell der Tod in ihrem Lachen.", "tokens": ["Sitzt", "grell", "der", "Tod", "in", "ih\u00b7rem", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jetzt atmet das Gras wieder hell und klar;", "tokens": ["Jetzt", "at\u00b7met", "das", "Gras", "wie\u00b7der", "hell", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "K\u00fchl steht die Welt an alter Stell'", "tokens": ["K\u00fchl", "steht", "die", "Welt", "an", "al\u00b7ter", "Stell'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wei\u00df kaum noch, da\u00df sie voll Durstgef\u00fchl war.", "tokens": ["Und", "wei\u00df", "kaum", "noch", ",", "da\u00df", "sie", "voll", "Durst\u00b7ge\u00b7f\u00fchl", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}