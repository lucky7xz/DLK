{"dta.poem.23734": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das Veilchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Warum, geliebtes Veilchen, bl\u00fchst", "tokens": ["Wa\u00b7rum", ",", "ge\u00b7lieb\u00b7tes", "Veil\u00b7chen", ",", "bl\u00fchst"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du so entfernt im Thal?", "tokens": ["Du", "so", "ent\u00b7fernt", "im", "Thal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Versteckst dich unter Bl\u00e4ttern, fliehst", "tokens": ["Ver\u00b7steckst", "dich", "un\u00b7ter", "Bl\u00e4t\u00b7tern", ",", "fliehst"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der stolzern Blumen Zahl?", "tokens": ["Der", "stol\u00b7zern", "Blu\u00b7men", "Zahl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und doch voll Liebreiz duftest du,", "tokens": ["Und", "doch", "voll", "Lieb\u00b7reiz", "duf\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bald man dich nur pfl\u00fcckt,", "tokens": ["So", "bald", "man", "dich", "nur", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns s\u00fc\u00dfre Wohlger\u00fcche zu,", "tokens": ["Uns", "s\u00fc\u00df\u00b7re", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als manche, die sich schm\u00fcckt.", "tokens": ["Als", "man\u00b7che", ",", "die", "sich", "schm\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist der Demuth Ebenbild,", "tokens": ["Du", "bist", "der", "De\u00b7muth", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die in der Stille wohnt,", "tokens": ["Die", "in", "der", "Stil\u00b7le", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und den, der ihr Verdienst enth\u00fcllt,", "tokens": ["Und", "den", ",", "der", "ihr", "Ver\u00b7dienst", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit frommen Dank belohnt.", "tokens": ["Mit", "from\u00b7men", "Dank", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}