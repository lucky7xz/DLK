{"textgrid.poem.41646": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geist, dem einst h\u00f6chste Lust ein kriegrisch-froh Beginnen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geist, dem einst h\u00f6chste Lust ein kriegrisch-froh Beginnen \u2013", "tokens": ["Geist", ",", "dem", "einst", "h\u00f6chs\u00b7te", "Lust", "ein", "krie\u00b7grischfroh", "Be\u00b7gin\u00b7nen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Hoffnung, deren Sporn dem Mut zu Hilfe kam,", "tokens": ["Die", "Hoff\u00b7nung", ",", "de\u00b7ren", "Sporn", "dem", "Mut", "zu", "Hil\u00b7fe", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Treibt dich nicht l\u00e4nger an! Streck hin dich ohne Scham,", "tokens": ["Treibt", "dich", "nicht", "l\u00e4n\u00b7ger", "an", "!", "Streck", "hin", "dich", "oh\u00b7ne", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$.", "NE", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ro\u00df, dessen greiser Huf kein Ziel mehr mag gewinnen.", "tokens": ["Ro\u00df", ",", "des\u00b7sen", "grei\u00b7ser", "Huf", "kein", "Ziel", "mehr", "mag", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "ADJA", "NN", "PIAT", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In stumpfem Schlaf ertr\u00e4nk verzweifelnd Herz und Sinnen.", "tokens": ["In", "stump\u00b7fem", "Schlaf", "er\u00b7tr\u00e4nk", "ver\u00b7zwei\u00b7felnd", "Herz", "und", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Besiegter, m\u00fcder Geist! Du Bettler, alt und zahm,", "tokens": ["Be\u00b7sieg\u00b7ter", ",", "m\u00fc\u00b7der", "Geist", "!", "Du", "Bett\u00b7ler", ",", "alt", "und", "zahm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlst weder Lust zum Streit noch Lieb im Herzen drinnen;", "tokens": ["F\u00fchlst", "we\u00b7der", "Lust", "zum", "Streit", "noch", "Lieb", "im", "Her\u00b7zen", "drin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "ADV", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lebt, Fl\u00f6tenseufzer, wohl, die z\u00e4rtlich mich umspinnen!", "tokens": ["Lebt", ",", "Fl\u00f6\u00b7ten\u00b7seuf\u00b7zer", ",", "wohl", ",", "die", "z\u00e4rt\u00b7lich", "mich", "um\u00b7spin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Versucht, ihr Freuden, nicht ein Herz voll dunklem Gram!", "tokens": ["Ver\u00b7sucht", ",", "ihr", "Freu\u00b7den", ",", "nicht", "ein", "Herz", "voll", "dunk\u00b7lem", "Gram", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Tot ist des Fr\u00fchlings Duft, der s\u00fc\u00df mich \u00fcberkam!", "tokens": ["Tot", "ist", "des", "Fr\u00fch\u00b7lings", "Duft", ",", "der", "s\u00fc\u00df", "mich", "\u00fc\u00b7ber\u00b7kam", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und schleichend zehrt mich auf der Stunden qualvoll Rinnen,", "tokens": ["Und", "schlei\u00b7chend", "zehrt", "mich", "auf", "der", "Stun\u00b7den", "qual\u00b7voll", "Rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Schnee, der einen Leib in eisge F\u00e4nge nahm;", "tokens": ["Wie", "Schnee", ",", "der", "ei\u00b7nen", "Leib", "in", "eis\u00b7ge", "F\u00e4n\u00b7ge", "nahm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Erdball schaut mein Geist, erstarrt und fl\u00fcgellahm,", "tokens": ["Den", "Erd\u00b7ball", "schaut", "mein", "Geist", ",", "er\u00b7starrt", "und", "fl\u00fc\u00b7gel\u00b7lahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sucht nicht einen Schutz, dem Wirbel zu entrinnen!", "tokens": ["Und", "sucht", "nicht", "ei\u00b7nen", "Schutz", ",", "dem", "Wir\u00b7bel", "zu", "ent\u00b7rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Lawine, rei\u00dft du mich in deinem Sturz von hinnen?", "tokens": ["La\u00b7wi\u00b7ne", ",", "rei\u00dft", "du", "mich", "in", "dei\u00b7nem", "Sturz", "von", "hin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}}}}