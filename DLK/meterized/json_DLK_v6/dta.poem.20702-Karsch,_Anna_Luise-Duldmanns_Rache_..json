{"dta.poem.20702": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Duldmanns Rache .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Duldmann sich gerochen,                 ", "tokens": ["Wie", "Duld\u00b7mann", "sich", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ihm in sein Gebiet", "tokens": ["Als", "ihm", "in", "sein", "Ge\u00b7biet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Weiberdieb gebrochen,", "tokens": ["Ein", "Wei\u00b7ber\u00b7dieb", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies sey mein lehrend Lied", "tokens": ["Dies", "sey", "mein", "leh\u00b7rend", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An\u2019s ganze Mannsgeschlechte,", "tokens": ["An's", "gan\u00b7ze", "Manns\u00b7ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wen von ungef\u00e4hr", "tokens": ["Und", "wen", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Schicksal treffen m\u00f6chte,", "tokens": ["Sein", "Schick\u00b7sal", "tref\u00b7fen", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der mach es so wie er.", "tokens": ["Der", "mach", "es", "so", "wie", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sein b\u00f6ser Engel brachte", "tokens": ["Sein", "b\u00f6\u00b7ser", "En\u00b7gel", "brach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur ungelegnen Zeit", "tokens": ["Zur", "un\u00b7ge\u00b7leg\u00b7nen", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn in sein Haus und machte,", "tokens": ["Ihn", "in", "sein", "Haus", "und", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er mit Hastigkeit", "tokens": ["Da\u00df", "er", "mit", "Has\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach seinem Weibchen fragte.", "tokens": ["Nach", "sei\u00b7nem", "Weib\u00b7chen", "frag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stellt euch sein Schrecken vor,", "tokens": ["Stellt", "euch", "sein", "Schre\u00b7cken", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als ihm die K\u00f6chin sagte:", "tokens": ["Als", "ihm", "die", "K\u00f6\u00b7chin", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bei ihr sey Coridor.", "tokens": ["Bei", "ihr", "sey", "Co\u00b7ri\u00b7dor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Puz- und Oberst\u00fcbchem", "tokens": ["Im", "Puz", "und", "O\u00b7bers\u00b7t\u00fcb\u00b7chem"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf weichem Sofa war", "tokens": ["Auf", "wei\u00b7chem", "So\u00b7fa", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein angetrautes Liebchen;", "tokens": ["Sein", "an\u00b7ge\u00b7trau\u00b7tes", "Lieb\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er fand sie offenbar", "tokens": ["Er", "fand", "sie", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihrem Zeitvertreiber,", "tokens": ["Mit", "ih\u00b7rem", "Zeit\u00b7ver\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie einst der Schmiedegott", "tokens": ["Wie", "einst", "der", "Schmie\u00b7de\u00b7gott"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die K\u00f6niginn der Weiber", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ginn", "der", "Wei\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu aller G\u00f6tter Spott \u2014", "tokens": ["Zu", "al\u00b7ler", "G\u00f6t\u00b7ter", "Spott"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was meint ihr? Was begonnte", "tokens": ["Was", "meint", "ihr", "?", "Was", "be\u00b7gonn\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Mannes Grimmgef\u00fchl,", "tokens": ["Des", "Man\u00b7nes", "Grimm\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Stoff uns geben konnte", "tokens": ["Der", "Stoff", "uns", "ge\u00b7ben", "konn\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Mord- und Trauerspiel,", "tokens": ["Zum", "Mord", "und", "Trau\u00b7er\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So schrecklich wie\u2019s den Britten", "tokens": ["So", "schreck\u00b7lich", "wie's", "den", "Brit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der gro\u00dfe Shakspear sang", "tokens": ["Der", "gro\u00b7\u00dfe", "Shak\u00b7spe\u00b7ar", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Vom Mohren, den kein Bitten", "tokens": ["Vom", "Moh\u00b7ren", ",", "den", "kein", "Bit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und keine Thr\u00e4ne zwang?", "tokens": ["Und", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "zwang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr denkt an Dolch und Messer,", "tokens": ["Ihr", "denkt", "an", "Dolch", "und", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Pulver und an Blei,", "tokens": ["An", "Pul\u00b7ver", "und", "an", "Blei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An giftgemischt Gew\u00e4sser,", "tokens": ["An", "gift\u00b7ge\u00b7mischt", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und an die Barbarei,", "tokens": ["Und", "an", "die", "Bar\u00b7ba\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie beide zu durchboren", "tokens": ["Sie", "bei\u00b7de", "zu", "durch\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit einem Degenstich;", "tokens": ["Mit", "ei\u00b7nem", "De\u00b7gen\u00b7stich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr h\u00e4ttet drauf geschworen,", "tokens": ["Ihr", "h\u00e4t\u00b7tet", "drauf", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df Mund an Mund verblich.", "tokens": ["Da\u00df", "Mund", "an", "Mund", "ver\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}