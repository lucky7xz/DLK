{"dta.poem.3808": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Bergm\u00e4nnischer Gl\u00fcck-Wunsch.  \n An einen  Baulustigen Bergm\u00e4nnischen Freund / der  \n ein Hoffnungs-volles Ort getroffen hatte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So will ich denn mit wenig W\u00f6rtern", "tokens": ["So", "will", "ich", "denn", "mit", "we\u00b7nig", "W\u00f6r\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Freund! dein Bergwercks Gl\u00fcck er\u00f6rtern.", "tokens": ["O", "Freund", "!", "dein", "Berg\u00b7wercks", "Gl\u00fcck", "er\u00b7\u00f6r\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So triffst du, wie ich mercken kan,", "tokens": ["So", "triffst", "du", ",", "wie", "ich", "mer\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nunmehro einen Hauptgang an.", "tokens": ["Nun\u00b7meh\u00b7ro", "ei\u00b7nen", "Haupt\u00b7gang", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was schon die Alten unterbrochen,", "tokens": ["Was", "schon", "die", "Al\u00b7ten", "un\u00b7ter\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo schon die G\u00e4nge abgestochen,", "tokens": ["Wo", "schon", "die", "G\u00e4n\u00b7ge", "ab\u00b7ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo schon gezogenes Gestein,", "tokens": ["Wo", "schon", "ge\u00b7zo\u00b7ge\u00b7nes", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das l\u00e4st du unber\u00fchret seyn;", "tokens": ["Das", "l\u00e4st", "du", "un\u00b7be\u00b7r\u00fch\u00b7ret", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn abgekehrte Spinneweben", "tokens": ["Denn", "ab\u00b7ge\u00b7kehr\u00b7te", "Spin\u00b7ne\u00b7we\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die w\u00fcrden dir kein Silber geben.", "tokens": ["Die", "w\u00fcr\u00b7den", "dir", "kein", "Sil\u00b7ber", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Du suchst ins frische einzuhaun,", "tokens": ["Du", "suchst", "ins", "fri\u00b7sche", "ein\u00b7zu\u00b7haun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und einen derben Gang zu baun,", "tokens": ["Und", "ei\u00b7nen", "der\u00b7ben", "Gang", "zu", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wo noch kein Schl\u00e4gel nichts verletzet,", "tokens": ["Wo", "noch", "kein", "Schl\u00e4\u00b7gel", "nichts", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wo noch kein Feustel angesetzet.", "tokens": ["Wo", "noch", "kein", "Feu\u00b7stel", "an\u00b7ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So schl\u00e4gst du, und so mu\u00df es seyn,", "tokens": ["So", "schl\u00e4gst", "du", ",", "und", "so", "mu\u00df", "es", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In unerschrotne Felder ein.", "tokens": ["In", "un\u00b7er\u00b7schrot\u00b7ne", "Fel\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dein scharff gemacht und l\u00fcstern Eisen", "tokens": ["Dein", "scharff", "ge\u00b7macht", "und", "l\u00fcs\u00b7tern", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VVPP", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wird dir ergiebge G\u00e4nge weisen.", "tokens": ["Wird", "dir", "er\u00b7gieb\u00b7ge", "G\u00e4n\u00b7ge", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ich wei\u00df schon, da\u00df dein frischer Schacht", "tokens": ["Ich", "wei\u00df", "schon", ",", "da\u00df", "dein", "fri\u00b7scher", "Schacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dir bald die gr\u00f6ste Hoffnung macht,", "tokens": ["Dir", "bald", "die", "gr\u00f6s\u00b7te", "Hoff\u00b7nung", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zumahl sich in den Nebeng\u00e4ngen", "tokens": ["Zu\u00b7mahl", "sich", "in", "den", "Ne\u00b7ben\u00b7g\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Auch feine Tr\u00fcmmer untermengen,", "tokens": ["Auch", "fei\u00b7ne", "Tr\u00fcm\u00b7mer", "un\u00b7ter\u00b7men\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nun haust du einen Hauptgang an,", "tokens": ["Nun", "haust", "du", "ei\u00b7nen", "Haupt\u00b7gang", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wo man bald Ertze sp\u00fchren kan.", "tokens": ["Wo", "man", "bald", "Ert\u00b7ze", "sp\u00fch\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Du wirst die Lehn alleine tragen,", "tokens": ["Du", "wirst", "die", "Lehn", "al\u00b7lei\u00b7ne", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Du wirst nichts nach Gewercken fragen,", "tokens": ["Du", "wirst", "nichts", "nach", "Ge\u00b7wer\u00b7cken", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und f\u00e4llt der Anbruch reich und fein,", "tokens": ["Und", "f\u00e4llt", "der", "An\u00b7bruch", "reich", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So bleibt er auch alleine dein.", "tokens": ["So", "bleibt", "er", "auch", "al\u00b7lei\u00b7ne", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So wird von dir bald nach Verlangen", "tokens": ["So", "wird", "von", "dir", "bald", "nach", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ein guter Haspel hingehangen.", "tokens": ["Ein", "gu\u00b7ter", "Has\u00b7pel", "hin\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Denn schaffest du, und das mit Recht,", "tokens": ["Denn", "schaf\u00b7fest", "du", ",", "und", "das", "mit", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "PDS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Auch einen feisten Haspelknecht.", "tokens": ["Auch", "ei\u00b7nen", "feis\u00b7ten", "Has\u00b7pel\u00b7knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Denn wird von Steigern, H\u00e4uern, Jungen,", "tokens": ["Denn", "wird", "von", "Stei\u00b7gern", ",", "H\u00e4u\u00b7ern", ",", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ein fr\u00f6liches Gl\u00fcck auf! gesungen;", "tokens": ["Ein", "fr\u00f6\u00b7li\u00b7ches", "Gl\u00fcck", "auf", "!", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Und diesem allen hilfft zumahl", "tokens": ["Und", "die\u00b7sem", "al\u00b7len", "hilfft", "zu\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "PIAT", "NN", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Ein gut und fest geh\u00e4rter Stahl.", "tokens": ["Ein", "gut", "und", "fest", "ge\u00b7h\u00e4r\u00b7ter", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Es wird euch weder Dunst und Schwaden,", "tokens": ["Es", "wird", "euch", "we\u00b7der", "Dunst", "und", "Schwa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Noch auch der b\u00f6se Kobald schaden.", "tokens": ["Noch", "auch", "der", "b\u00f6\u00b7se", "Ko\u00b7bald", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "So hast du gl\u00fccklich eingesch\u00fcrfft,", "tokens": ["So", "hast", "du", "gl\u00fcck\u00b7lich", "ein\u00b7ge\u00b7sch\u00fcrfft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da sich der Gang ins Krumme wirfft,", "tokens": ["Da", "sich", "der", "Gang", "ins", "Krum\u00b7me", "wirfft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Bringst du nun balde sch\u00f6ne Stuffen,", "tokens": ["Bringst", "du", "nun", "bal\u00b7de", "sch\u00f6\u00b7ne", "Stuf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "So will ich auch Gl\u00fcck auf! mit ruffen.", "tokens": ["So", "will", "ich", "auch", "Gl\u00fcck", "auf", "!", "mit", "ruf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "PTKVZ", "$.", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Nie f\u00f6rdere ein falsches Quertzt,", "tokens": ["Nie", "f\u00f6r\u00b7de\u00b7re", "ein", "fal\u00b7sches", "Quertzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Vielmehr ein sch\u00f6n gewachsen Ertzt,", "tokens": ["Viel\u00b7mehr", "ein", "sch\u00f6n", "ge\u00b7wach\u00b7sen", "Ertzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "So kanst du offt in deinem Leben", "tokens": ["So", "kanst", "du", "offt", "in", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Vergn\u00fcgt und gute Ausbeuth geben.", "tokens": ["Ver\u00b7gn\u00fcgt", "und", "gu\u00b7te", "Aus\u00b7beuth", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Ich w\u00fcnsche, da\u00df bey erster Schicht,", "tokens": ["Ich", "w\u00fcn\u00b7sche", ",", "da\u00df", "bey", "ers\u00b7ter", "Schicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Rothg\u00fclden Ertzt und Silber bricht.", "tokens": ["Roth\u00b7g\u00fcl\u00b7den", "Ertzt", "und", "Sil\u00b7ber", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Denn scheue du auch kein Bem\u00fchen,", "tokens": ["Denn", "scheu\u00b7e", "du", "auch", "kein", "Be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "La\u00df F\u00f6rdern, K\u00fcbel, Haspel ziehen,", "tokens": ["La\u00df", "F\u00f6r\u00b7dern", ",", "K\u00fc\u00b7bel", ",", "Has\u00b7pel", "zie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Da\u00df deine Zeche tragen mag.", "tokens": ["Da\u00df", "dei\u00b7ne", "Ze\u00b7che", "tra\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "So Zubu\u00df, Ausbeuth, als Verlag.", "tokens": ["So", "Zu\u00b7bu\u00df", ",", "Aus\u00b7beuth", ",", "als", "Ver\u00b7lag", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "So wirst du bey ergiebgen Fl\u00f6tzen,", "tokens": ["So", "wirst", "du", "bey", "er\u00b7gieb\u00b7gen", "Fl\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Dein Berggesinntes Hertz erg\u00f6tzen.", "