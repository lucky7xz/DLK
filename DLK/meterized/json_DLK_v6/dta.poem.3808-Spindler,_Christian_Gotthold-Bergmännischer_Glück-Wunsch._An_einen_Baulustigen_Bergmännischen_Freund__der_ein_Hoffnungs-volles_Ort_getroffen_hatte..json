i.chol"}, "line.73": {"text": "So must du diesen Fehl zudecken,", "tokens": ["So", "must", "du", "die\u00b7sen", "Fehl", "zu\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.74": {"text": "Und mich nicht auf den Rieb-Baum strecken.", "tokens": ["Und", "mich", "nicht", "auf", "den", "Rie\u00b7bBaum", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.75": {"text": "Gesetzt, ich k\u00e4me auch darauff,", "tokens": ["Ge\u00b7setzt", ",", "ich", "k\u00e4\u00b7me", "auch", "dar\u00b7auff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.76": {"text": "So ruff ich doch: o Freund, Gl\u00fcck auf!", "tokens": ["So", "ruff", "ich", "doch", ":", "o", "Freund", ",", "Gl\u00fcck", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "FM", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}