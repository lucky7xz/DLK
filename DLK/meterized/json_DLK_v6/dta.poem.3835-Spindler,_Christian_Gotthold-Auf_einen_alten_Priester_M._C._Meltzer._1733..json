{"dta.poem.3835": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen alten Priester/  \n  M. C. Meltzer.  1733.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ruhe sanfft, erbla\u00dfter Grei\u00df! in dem k\u00fchlen", "tokens": ["Ru\u00b7he", "sanfft", ",", "er\u00b7bla\u00df\u00b7ter", "Grei\u00df", "!", "in", "dem", "k\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "$.", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schoo\u00df der Erden,", "tokens": ["Schoo\u00df", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Theurer Meltzer, gehe nun in das frohe Salem ein,", "tokens": ["Theu\u00b7rer", "Melt\u00b7zer", ",", "ge\u00b7he", "nun", "in", "das", "fro\u00b7he", "Sa\u00b7lem", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Fliehe Kedars bange Noth, fliehe Bochims Angst-", "tokens": ["Flie\u00b7he", "Ke\u00b7dars", "ban\u00b7ge", "Noth", ",", "flie\u00b7he", "Bo\u00b7chims", "Angst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "ADV", "NN", "$,", "VVFIN", "NE", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Beschwerden,", "tokens": ["Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Jedes Beicht-Kind weyht die Brust dir zu einem", "tokens": ["Je\u00b7des", "Beicht\u00b7Kind", "weyht", "die", "Brust", "dir", "zu", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "PPER", "APPR", "ART"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Leichen-Stein.", "tokens": ["Lei\u00b7chen\u00b7Stein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Buchholtz l\u00e4st auf deine Grufft viel gerechte Z\u00e4h-", "tokens": ["Buch\u00b7holtz", "l\u00e4st", "auf", "dei\u00b7ne", "Grufft", "viel", "ge\u00b7rech\u00b7te", "Z\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "ren fliessen.", "tokens": ["ren", "flies\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Du triebst sechs und viertzig Jahre deine schwere", "tokens": ["Du", "triebst", "sechs", "und", "viert\u00b7zig", "Jah\u00b7re", "dei\u00b7ne", "schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "KON", "CARD", "NN", "PPOSAT", "ADJA"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.11": {"text": "Priester-Pflicht.", "tokens": ["Pries\u00b7ter\u00b7Pflicht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Hat dich gleich der blasse Tod dieser Sterblichkeit", "tokens": ["Hat", "dich", "gleich", "der", "blas\u00b7se", "Tod", "die\u00b7ser", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PDAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "entrissen,", "tokens": ["ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "O! so stirbt dein Ruhm und Nahme doch in unsern", "tokens": ["O", "!", "so", "stirbt", "dein", "Ruhm", "und", "Nah\u00b7me", "doch", "in", "un\u00b7sern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Hertzen nicht.", "tokens": ["Hert\u00b7zen", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}