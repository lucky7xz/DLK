{"dta.poem.7049": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob gleich die GOTTHEJT immerfort", "tokens": ["Ob", "gleich", "die", "GoTTHEJT", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "An allen Orten ist; so ist jedoch ein Ort,", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "ist", ";", "so", "ist", "je\u00b7doch", "ein", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$.", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Woselbst Sie sonderlich,", "tokens": ["Wo\u00b7selbst", "Sie", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu Jhrem ew\u2019gen Lob und Preise,", "tokens": ["Zu", "Ih\u00b7rem", "ew'\u00b7gen", "Lob", "und", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf eine herrlich sicht-und wunderbare Weise,", "tokens": ["Auf", "ei\u00b7ne", "herr\u00b7lich", "sicht\u00b7\u00b7und", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den seeligen Gesch\u00f6pffen Sich", "tokens": ["Den", "see\u00b7li\u00b7gen", "Ge\u00b7sch\u00f6pf\u00b7fen", "Sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Unendlich pr\u00e4chtig zeigt: Der nach der Bibel Lehren,", "tokens": ["Un\u00b7end\u00b7lich", "pr\u00e4ch\u00b7tig", "zeigt", ":", "Der", "nach", "der", "Bi\u00b7bel", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$.", "ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der dritte Himmel, Paradeis,", "tokens": ["Der", "drit\u00b7te", "Him\u00b7mel", ",", "Pa\u00b7ra\u00b7deis", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des Allerh\u00f6chsten Thron, die Wohnung Seiner Ehren,", "tokens": ["Des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Thron", ",", "die", "Woh\u00b7nung", "Sei\u00b7ner", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Mittel-Punct und Krei\u00df", "tokens": ["Der", "Mit\u00b7tel\u00b7Punct", "und", "Krei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der ew'gen Seeligkeit, genannt wird; ist allda,", "tokens": ["Der", "ew'\u00b7gen", "See\u00b7lig\u00b7keit", ",", "ge\u00b7nannt", "wird", ";", "ist", "all\u00b7da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "VAFIN", "$.", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Woselbst mit ungez\u00e4hlt unz\u00e4hlbarn Millionen,", "tokens": ["Wo\u00b7selbst", "mit", "un\u00b7ge\u00b7z\u00e4hlt", "un\u00b7z\u00e4hl\u00b7barn", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "In tausend, tausend, tausend Choren,", "tokens": ["In", "tau\u00b7send", ",", "tau\u00b7send", ",", "tau\u00b7send", "Cho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "$,", "VVPP", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mit ewigem Hallelujah,", "tokens": ["Mit", "e\u00b7wi\u00b7gem", "Hal\u00b7le\u00b7lu\u00b7jah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Die Engel, Jhren Sch\u00f6pffer, ehren.", "tokens": ["Die", "En\u00b7gel", ",", "Ih\u00b7ren", "Sch\u00f6pf\u00b7fer", ",", "eh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Es ist unstreitig wahr, da\u00df GOTT so wesentlich", "tokens": ["Es", "ist", "un\u00b7strei\u00b7tig", "wahr", ",", "da\u00df", "GoTT", "so", "we\u00b7sent\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PTKVZ", "$,", "KOUS", "NE", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "An allen Orten sey, als Er an diesem ist:", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "sey", ",", "als", "Er", "an", "die\u00b7sem", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "APPR", "PDS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Allein, hier wohnt und zeigt Er sich", "tokens": ["Al\u00b7lein", ",", "hier", "wohnt", "und", "zeigt", "Er", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "In einer Majest\u00e4t, die zwar kein Geist ermisst,", "tokens": ["In", "ei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", ",", "die", "zwar", "kein", "Geist", "er\u00b7misst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die doch empfindlich ist. Es wohnt die ew\u2019ge G\u00fcte,", "tokens": ["Die", "doch", "emp\u00b7find\u00b7lich", "ist", ".", "Es", "wohnt", "die", "ew'\u00b7ge", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es wohnt die ew\u2019ge Macht daselbst, in einem Schein", "tokens": ["Es", "wohnt", "die", "ew'\u00b7ge", "Macht", "da\u00b7selbst", ",", "in", "ei\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Von solchem Glantz, als in dem Menschlichen Gem\u00fcthe,", "tokens": ["Von", "sol\u00b7chem", "Glantz", ",", "als", "in", "dem", "Menschli\u00b7chen", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Und keiner Creatur, je mag erschienen seyn.", "tokens": ["Und", "kei\u00b7ner", "Crea\u00b7tur", ",", "je", "mag", "er\u00b7schie\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "In diesem grossen Glantz und Lichte zeigen sich", "tokens": ["In", "die\u00b7sem", "gros\u00b7sen", "Glantz", "und", "Lich\u00b7te", "zei\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "KON", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So gar die Geister sichtbarlich.", "tokens": ["So", "gar", "die", "Geis\u00b7ter", "sicht\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Das Allerheiligste in Salomonis Tempel", "tokens": ["Das", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", "in", "Sa\u00b7lo\u00b7mo\u00b7nis", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Scheint fast, als w\u00e4r\u2019 es ein Exempel,", "tokens": ["Scheint", "fast", ",", "als", "w\u00e4r'", "es", "ein", "Ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Ein Schatten-Ri\u00df, von dieser Herrlichkeit.", "tokens": ["Ein", "Schat\u00b7ten\u00b7Ri\u00df", ",", "von", "die\u00b7ser", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}