{"dta.poem.13424": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling eilt herzu mit schnellem Lauf,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "eilt", "her\u00b7zu", "mit", "schnel\u00b7lem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einem leisen Schritt durchstreift er ganze", "tokens": ["In", "ei\u00b7nem", "lei\u00b7sen", "Schritt", "durch\u00b7streift", "er", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er l\u00f6set die gestrengen Bande", "tokens": ["Er", "l\u00f6\u00b7set", "die", "ge\u00b7stren\u00b7gen", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Winters blo\u00df mit seinem Oden auf,", "tokens": ["Des", "Win\u00b7ters", "blo\u00df", "mit", "sei\u00b7nem", "O\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und unter seinen lauen F\u00fcssen", "tokens": ["Und", "un\u00b7ter", "sei\u00b7nen", "lau\u00b7en", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beginnen Blumen aufzuschiessen.", "tokens": ["Be\u00b7gin\u00b7nen", "Blu\u00b7men", "auf\u00b7zu\u00b7schies\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Welt wird aufger\u00e4umt, und manche Fluhr", "tokens": ["Die", "Welt", "wird", "auf\u00b7ge\u00b7r\u00e4umt", ",", "und", "man\u00b7che", "Fluhr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird wiederum geschickt zu einem Sch\u00e4fertanze,", "tokens": ["Wird", "wie\u00b7de\u00b7rum", "ge\u00b7schickt", "zu", "ei\u00b7nem", "Sch\u00e4\u00b7fer\u00b7tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit einem gr\u00fcnen Myrthenkranze", "tokens": ["Mit", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Myr\u00b7then\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schm\u00fcckt sich auch selbst die Mutter der Natur!", "tokens": ["Schm\u00fcckt", "sich", "auch", "selbst", "die", "Mut\u00b7ter", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ohnerachtet ihrer Jahre", "tokens": ["Und", "oh\u00b7ne\u00b7rach\u00b7tet", "ih\u00b7rer", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bekommt sie wieder blonde Haare.", "tokens": ["Be\u00b7kommt", "sie", "wie\u00b7der", "blon\u00b7de", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Indem der Lenz bey uns f\u00fcr\u00fcber zieht,", "tokens": ["In\u00b7dem", "der", "Lenz", "bey", "uns", "f\u00fc\u00b7r\u00fc\u00b7ber", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So f\u00e4nget alles an, von neuen aufzuleben;", "tokens": ["So", "f\u00e4n\u00b7get", "al\u00b7les", "an", ",", "von", "neu\u00b7en", "auf\u00b7zu\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Vogel scheint inde\u00df zu beben,", "tokens": ["Der", "Vo\u00b7gel", "scheint", "in\u00b7de\u00df", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er wiederholt das halbverlernte Lied,", "tokens": ["Er", "wie\u00b7der\u00b7holt", "das", "halb\u00b7ver\u00b7lern\u00b7te", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und l\u00e4\u00dft sich, selbigem zu Ehren", "tokens": ["Und", "l\u00e4\u00dft", "sich", ",", "sel\u00b7bi\u00b7gem", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "ADJA", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit \u00fcberh\u00fcpften Noten h\u00f6ren.", "tokens": ["Mit", "\u00fc\u00b7ber\u00b7h\u00fcpf\u00b7ten", "No\u00b7ten", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In St\u00e4llen wirkt ein unsichtbarer Trieb,", "tokens": ["In", "St\u00e4l\u00b7len", "wirkt", "ein", "un\u00b7sicht\u00b7ba\u00b7rer", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das tr\u00e4ge Vieh erwacht, u. schreyt mit neuem Muthe,", "tokens": ["Das", "tr\u00e4\u00b7ge", "Vieh", "er\u00b7wacht", ",", "u.", "schreyt", "mit", "neu\u00b7em", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es f\u00fchlt ein Wallen in dem Blute,", "tokens": ["Es", "f\u00fchlt", "ein", "Wal\u00b7len", "in", "dem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat nicht mehr die vollen Krippen lieb,", "tokens": ["Und", "hat", "nicht", "mehr", "die", "vol\u00b7len", "Krip\u00b7pen", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit einer sehnsuchtsvollen Freude", "tokens": ["Mit", "ei\u00b7ner", "sehn\u00b7suchts\u00b7vol\u00b7len", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht es sich um nach neuer Weyde.", "tokens": ["Sieht", "es", "sich", "um", "nach", "neu\u00b7er", "Wey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Ackersmann l\u00e4\u00dft seine H\u00fctte leer,", "tokens": ["Der", "A\u00b7ckers\u00b7mann", "l\u00e4\u00dft", "sei\u00b7ne", "H\u00fct\u00b7te", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Worinnen er bisher dem Feuerherde nahe,", "tokens": ["Wo\u00b7rin\u00b7nen", "er", "bis\u00b7her", "dem", "Feu\u00b7er\u00b7her\u00b7de", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich fast mit Widerwillen sahe,", "tokens": ["Sich", "fast", "mit", "Wi\u00b7der\u00b7wil\u00b7len", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bessert schon sein halbverrost Gewehr,", "tokens": ["Und", "bes\u00b7sert", "schon", "sein", "halb\u00b7ver\u00b7rost", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VAINF", "VAFIN", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Damit, den Fr\u00fchling zu vergn\u00fcgen,", "tokens": ["Da\u00b7mit", ",", "den", "Fr\u00fch\u00b7ling", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Aecker wieder umzupfl\u00fcgen.", "tokens": ["Die", "A\u00b7e\u00b7cker", "wie\u00b7der", "um\u00b7zu\u00b7pfl\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Der Sch\u00e4fer winkt der blonden Sch\u00e4ferin,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "winkt", "der", "blon\u00b7den", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Heerden treiben sie nach abgelegnen Auen,", "tokens": ["Die", "Heer\u00b7den", "trei\u00b7ben", "sie", "nach", "ab\u00b7ge\u00b7leg\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da ohngehindert sich zu schauen,", "tokens": ["Da", "ohn\u00b7ge\u00b7hin\u00b7dert", "sich", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe f\u00fchrt sie beyderseits dahin,", "tokens": ["Die", "Lie\u00b7be", "f\u00fchrt", "sie", "bey\u00b7der\u00b7seits", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als H\u00fcter ihrer keuschen Jugend", "tokens": ["Als", "H\u00fc\u00b7ter", "ih\u00b7rer", "keu\u00b7schen", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Begleitet sie ein Heer der Tugend.", "tokens": ["Be\u00b7glei\u00b7tet", "sie", "ein", "Heer", "der", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und wie? was reizt denn wiederum bey mir", "tokens": ["Und", "wie", "?", "was", "reizt", "denn", "wie\u00b7de\u00b7rum", "bey", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "PWS", "VVFIN", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der holden Poesie fast abgelebte Triebe?", "tokens": ["Der", "hol\u00b7den", "Poe\u00b7sie", "fast", "ab\u00b7ge\u00b7leb\u00b7te", "Trie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bist du es etwa selbst, o Liebe!", "tokens": ["Bist", "du", "es", "et\u00b7wa", "selbst", ",", "o", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt \u00fcberall das junge Jahr mit dir?", "tokens": ["Kommt", "\u00fc\u00b7be\u00b7rall", "das", "jun\u00b7ge", "Jahr", "mit", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und da ich Helden will besingen,", "tokens": ["Und", "da", "ich", "Hel\u00b7den", "will", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6r ich die Leyer z\u00e4rtlich klingen?", "tokens": ["H\u00f6r", "ich", "die", "Le\u00b7yer", "z\u00e4rt\u00b7lich", "klin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach Kind! o Freundin! weckt mich deine Hand?", "tokens": ["Ach", "Kind", "!", "o", "Freun\u00b7din", "!", "weckt", "mich", "dei\u00b7ne", "Hand", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "FM", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ruft mich dein milder Ernst aus einem langen Schlafe", "tokens": ["Ruft", "mich", "dein", "mil\u00b7der", "Ernst", "aus", "ei\u00b7nem", "lan\u00b7gen", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdient mein Herze deine Strafe?", "tokens": ["Ver\u00b7di\u00b7ent", "mein", "Her\u00b7ze", "dei\u00b7ne", "Stra\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Worinnen du dein Feuer angebrannt?", "tokens": ["Wo\u00b7rin\u00b7nen", "du", "dein", "Feu\u00b7er", "an\u00b7ge\u00b7brannt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mu\u00df mich Chloens Reiz entz\u00fccken,", "tokens": ["Und", "mu\u00df", "mich", "Chloens", "Reiz", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "NE", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Die Spr\u00f6de kehrt mir ja den R\u00fccken!", "tokens": ["Die", "Spr\u00f6\u00b7de", "kehrt", "mir", "ja", "den", "R\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach! warum stirbt mit Chloris Z\u00e4rtlichkeit,", "tokens": ["Ach", "!", "wa\u00b7rum", "stirbt", "mit", "Chlo\u00b7ris", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihrer Jugend Pracht nicht alle mein Verlangen!", "tokens": ["Mit", "ih\u00b7rer", "Ju\u00b7gend", "Pracht", "nicht", "al\u00b7le", "mein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKNEG", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "meas