, "measure": "iambic.penta"}, "line.5": {"text": "Verhei\u00df einmahl mit einem Blicke", "tokens": ["Ver\u00b7hei\u00df", "ein\u00b7mahl", "mit", "ei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir mein zuk\u00fcnftig ganzes Gl\u00fccke.", "tokens": ["Mir", "mein", "zu\u00b7k\u00fcnf\u00b7tig", "gan\u00b7zes", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Schatz! Freundin! Engel! sch\u00f6nste Sch\u00e4ferin!", "tokens": ["Schatz", "!", "Freun\u00b7din", "!", "En\u00b7gel", "!", "sch\u00f6ns\u00b7te", "Sch\u00e4\u00b7fe\u00b7rin", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NE", "$.", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "La\u00df meine Liebe sich hier nicht vergebens winden,", "tokens": ["La\u00df", "mei\u00b7ne", "Lie\u00b7be", "sich", "hier", "nicht", "ver\u00b7ge\u00b7bens", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schw\u00f6re bey den Westenwinden!", "tokens": ["Ich", "schw\u00f6\u00b7re", "bey", "den", "Wes\u00b7ten\u00b7win\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schw\u00f6re dir, so wahr ich redlich bin!", "tokens": ["Ich", "schw\u00f6\u00b7re", "dir", ",", "so", "wahr", "ich", "red\u00b7lich", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich schw\u00f6re bey den Fr\u00fchlings-Tagen,", "tokens": ["Ich", "schw\u00f6\u00b7re", "bey", "den", "Fr\u00fch\u00b7lings\u00b7Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will dich auf den H\u00e4nden tragen!", "tokens": ["Ich", "will", "dich", "auf", "den", "H\u00e4n\u00b7den", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Jhr Fluhren, h\u00f6rt ihrs! Nympfen dieser Au!", "tokens": ["Ihr", "Fluh\u00b7ren", ",", "h\u00f6rt", "ihrs", "!", "Nymp\u00b7fen", "die\u00b7ser", "Au", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "NN", "$.", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seyd Zeugen, was ich itzt der Sch\u00e4ferin versprochen,", "tokens": ["Seyd", "Zeu\u00b7gen", ",", "was", "ich", "itzt", "der", "Sch\u00e4\u00b7fe\u00b7rin", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "$,", "PWS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vernehmt ihr, da\u00df mein Schwur gebrochen,", "tokens": ["Ver\u00b7nehmt", "ihr", ",", "da\u00df", "mein", "Schwur", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohlan! so werd ich niemahls alt und grau!", "tokens": ["Wo\u00b7hlan", "!", "so", "werd", "ich", "nie\u00b7mahls", "alt", "und", "grau", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So sey der Inhalt meiner Lieder", "tokens": ["So", "sey", "der", "In\u00b7halt", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Einst der Nachkommenschaft zuwider!", "tokens": ["Einst", "der", "Nach\u00b7kom\u00b7men\u00b7schaft", "zu\u00b7wi\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "So eile denn herzu mit schnellem Lauf!", "tokens": ["So", "ei\u00b7le", "denn", "her\u00b7zu", "mit", "schnel\u00b7lem", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verdopple deinen Schritt zu dem gelobten Lande", "tokens": ["Ver\u00b7dopp\u00b7le", "dei\u00b7nen", "Schritt", "zu", "dem", "ge\u00b7lob\u00b7ten", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und putze mich