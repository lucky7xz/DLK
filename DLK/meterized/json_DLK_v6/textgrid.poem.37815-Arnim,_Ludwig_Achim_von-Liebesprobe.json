{"textgrid.poem.37815": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Liebesprobe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sah eine Linde ins tiefe Thal,", "tokens": ["Es", "sah", "ei\u00b7ne", "Lin\u00b7de", "ins", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "War unten breit und oben schmal,", "tokens": ["War", "un\u00b7ten", "breit", "und", "o\u00b7ben", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worunter zwey Verliebte sa\u00dfen,", "tokens": ["Wo\u00b7run\u00b7ter", "zwey", "Ver\u00b7lieb\u00b7te", "sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Lieb' ihr Leid verga\u00dfen.", "tokens": ["Vor", "Lieb'", "ihr", "Leid", "ver\u00b7ga\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbfeins Liebchen wir m\u00fcssen von einander,", "tokens": ["\u00bb", "feins", "Lieb\u00b7chen", "wir", "m\u00fcs\u00b7sen", "von", "ein\u00b7an\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "PPER", "VMFIN", "APPR", "ADV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich mu\u00df noch sieben Jahre wandern;\u00ab", "tokens": ["Ich", "mu\u00df", "noch", "sie\u00b7ben", "Jah\u00b7re", "wan\u00b7dern", ";", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "CARD", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmu\u00dft du noch sieben Jahr wandern,", "tokens": ["\u00bb", "mu\u00dft", "du", "noch", "sie\u00b7ben", "Jahr", "wan\u00b7dern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "So heurath ich mir keinen andern.\u00ab", "tokens": ["So", "heu\u00b7rath", "ich", "mir", "kei\u00b7nen", "an\u00b7dern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIAT", "ADJA", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als nun die sieben Jahr um waren,", "tokens": ["Und", "als", "nun", "die", "sie\u00b7ben", "Jahr", "um", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "CARD", "NN", "APPR", "VAFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sie meinte ihr Liebchen k\u00e4me bald,", "tokens": ["Sie", "mein\u00b7te", "ihr", "Lieb\u00b7chen", "k\u00e4\u00b7me", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie ging wohl in den Garten,", "tokens": ["Sie", "ging", "wohl", "in", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr feines Liebchen zu erwarten.", "tokens": ["Ihr", "fei\u00b7nes", "Lieb\u00b7chen", "zu", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie ging wohl in das gr\u00fcne Holz,", "tokens": ["Sie", "ging", "wohl", "in", "das", "gr\u00fc\u00b7ne", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam ein Reuter geritten stolz;", "tokens": ["Da", "kam", "ein", "Reu\u00b7ter", "ge\u00b7rit\u00b7ten", "stolz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "VVFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbgott gr\u00fc\u00dfe dich M\u00e4gdlein feine,", "tokens": ["\u00bb", "gott", "gr\u00fc\u00b7\u00dfe", "dich", "M\u00e4gd\u00b7lein", "fei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was machst du hier alleine.", "tokens": ["Was", "machst", "du", "hier", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ist dir dein Vater oder Mutter gram,", "tokens": ["Ist", "dir", "dein", "Va\u00b7ter", "o\u00b7der", "Mut\u00b7ter", "gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Oder hast du heimlich einen Mann?\u00ab", "tokens": ["O\u00b7der", "hast", "du", "heim\u00b7lich", "ei\u00b7nen", "Mann", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbmein Vater und Mutter sind mir nicht gram,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", "und", "Mut\u00b7ter", "sind", "mir", "nicht", "gram", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich hab' auch heimlich keinen Mann.", "tokens": ["Ich", "hab'", "auch", "heim\u00b7lich", "kei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gestern wars drey Wochen \u00fcber sieben Jahr,", "tokens": ["Ge\u00b7stern", "wars", "drey", "Wo\u00b7chen", "\u00fc\u00b7ber", "sie\u00b7ben", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "CARD", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da mein feines Liebchen ausgewandert war.\u00ab", "tokens": ["Da", "mein", "fei\u00b7nes", "Lieb\u00b7chen", "aus\u00b7ge\u00b7wan\u00b7dert", "war", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "\u00bbgestern bin ich geritten durch eine Stadt,", "tokens": ["\u00bb", "ge\u00b7stern", "bin", "ich", "ge\u00b7rit\u00b7ten", "durch", "ei\u00b7ne", "Stadt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da dein feins Liebchen hat Hochzeit gehabt.", "tokens": ["Da", "dein", "feins", "Lieb\u00b7chen", "hat", "Hoch\u00b7zeit", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "VAPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Was thust du ihm denn w\u00fcnschen,", "tokens": ["Was", "thust", "du", "ihm", "denn", "w\u00fcn\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er nicht gehalten seine Treu?\u00ab", "tokens": ["Da\u00df", "er", "nicht", "ge\u00b7hal\u00b7ten", "sei\u00b7ne", "Treu", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbich w\u00fcnsch ihm so viel gute Zeit,", "tokens": ["\u00bb", "ich", "w\u00fcnsch", "ihm", "so", "viel", "gu\u00b7te", "Zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So viel wie Sand am Meere breit.\u00ab", "tokens": ["So", "viel", "wie", "Sand", "am", "Mee\u00b7re", "breit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "NN", "APPRART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was zog er von seinem Finger?", "tokens": ["Was", "zog", "er", "von", "sei\u00b7nem", "Fin\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein'n Ring von reinem Gold gar fein.", "tokens": ["Ein'n", "Ring", "von", "rei\u00b7nem", "Gold", "gar", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er warf den Ring in ihren Schoo\u00df,", "tokens": ["Er", "warf", "den", "Ring", "in", "ih\u00b7ren", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie weinte, da\u00df der Ring gar flo\u00df.", "tokens": ["Sie", "wein\u00b7te", ",", "da\u00df", "der", "Ring", "gar", "flo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was zog er aus seiner Taschen?", "tokens": ["Was", "zog", "er", "aus", "sei\u00b7ner", "Ta\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Tuch sehr wei\u00df gewaschen.", "tokens": ["Ein", "Tuch", "sehr", "wei\u00df", "ge\u00b7wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbtrockne ab, trockne ab dein Aeugelein,", "tokens": ["\u00bb", "trock\u00b7ne", "ab", ",", "trock\u00b7ne", "ab", "dein", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "PTKVZ", "$,", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Du sollst hinfort mein eigen seyn.", "tokens": ["Du", "sollst", "hin\u00b7fort", "mein", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich thu dich nur versuchen,", "tokens": ["Ich", "thu", "dich", "nur", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob du w\u00fcrd'st schw\u00f6ren oder fluchen;", "tokens": ["Ob", "du", "w\u00fcrd'st", "schw\u00f6\u00b7ren", "o\u00b7der", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt'st du einen Fluch oder Schwur gethan,", "tokens": ["H\u00e4tt'st", "du", "ei\u00b7nen", "Fluch", "o\u00b7der", "Schwur", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "VVPP", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So w\u00e4r ich gleich geritten davon.\u00ab", "tokens": ["So", "w\u00e4r", "ich", "gleich", "ge\u00b7rit\u00b7ten", "da\u00b7von", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "PAV", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}