{"textgrid.poem.31373": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch am Neuen Jahre 1800 f\u00fcr Ulrike von Kleist", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amphibion Du, das in zwei Elementen stets lebet,", "tokens": ["Am\u00b7phi\u00b7bi\u00b7on", "Du", ",", "das", "in", "zwei", "E\u00b7le\u00b7men\u00b7ten", "stets", "le\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "APPR", "CARD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Schwanke nicht l\u00e4nger und w\u00e4hle Dir endlich ein sichres Geschlecht.", "tokens": ["Schwan\u00b7ke", "nicht", "l\u00e4n\u00b7ger", "und", "w\u00e4h\u00b7le", "Dir", "end\u00b7lich", "ein", "sich\u00b7res", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "KON", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+", "measure": "dactylic.tetra.plus"}, "line.3": {"text": "Schwimmen und fliegen geht nicht zugleich, drum verlasse das Wasser,", "tokens": ["Schwim\u00b7men", "und", "flie\u00b7gen", "geht", "nicht", "zu\u00b7gleich", ",", "drum", "ver\u00b7las\u00b7se", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VVFIN", "PTKNEG", "ADV", "$,", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Versuch es einmal in der Luft, sch\u00fcttle die Schwingen und fleuch!", "tokens": ["Ver\u00b7such", "es", "ein\u00b7mal", "in", "der", "Luft", ",", "sch\u00fctt\u00b7le", "die", "Schwin\u00b7gen", "und", "fleuch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+---+--+", "measure": "iambic.hexa.chol"}}}}}