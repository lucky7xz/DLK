{"textgrid.poem.34840": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Hand die kleine Lampe,", "tokens": ["In", "der", "Hand", "die", "klei\u00b7ne", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Brust die gro\u00dfe Glut,", "tokens": ["In", "der", "Brust", "die", "gro\u00b7\u00dfe", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleichet Psyche zu dem Lager,", "tokens": ["Schlei\u00b7chet", "Psy\u00b7che", "zu", "dem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der holde Schl\u00e4fer ruht.", "tokens": ["Wo", "der", "hol\u00b7de", "Schl\u00e4\u00b7fer", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie err\u00f6tet und sie zittert,", "tokens": ["Sie", "er\u00b7r\u00f6\u00b7tet", "und", "sie", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie seine Sch\u00f6nheit sieht \u2013", "tokens": ["Wie", "sie", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "sieht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der enth\u00fcllte Gott der Liebe,", "tokens": ["Der", "ent\u00b7h\u00fcll\u00b7te", "Gott", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er erwacht und er entflieht.", "tokens": ["Er", "er\u00b7wacht", "und", "er", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Achtzehnhundertj\u00e4hr'ge Bu\u00dfe!", "tokens": ["Acht\u00b7zehn\u00b7hun\u00b7dert\u00b7j\u00e4hr'\u00b7ge", "Bu\u00b7\u00dfe", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die \u00c4rmste stirbt beinah!", "tokens": ["Und", "die", "\u00c4rms\u00b7te", "stirbt", "bei\u00b7nah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Psyche fastet und kasteit sich,", "tokens": ["Psy\u00b7che", "fas\u00b7tet", "und", "kas\u00b7teit", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "PRF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Weil sie Amorn nackend sah.", "tokens": ["Weil", "sie", "A\u00b7morn", "na\u00b7ckend", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}