{"dta.poem.4166": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wir wissen uns zu finden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, wenn's immer Messe w\u00e4re,", "tokens": ["Ja", ",", "wenn's", "im\u00b7mer", "Mes\u00b7se", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "NN", "VAFIN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und die Mess' auch immer gut,", "tokens": ["Und", "die", "Mess'", "auch", "im\u00b7mer", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4b' ich mein Hotel, auf Ehre,", "tokens": ["G\u00e4b'", "ich", "mein", "Ho\u00b7tel", ",", "auf", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nicht um einen Rathsherrnhut.", "tokens": ["Nicht", "um", "ei\u00b7nen", "Raths\u00b7herrn\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, schon kleiner wird die Sch\u00fcssel,", "tokens": ["Doch", ",", "schon", "klei\u00b7ner", "wird", "die", "Sch\u00fcs\u00b7sel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich seh' die vielen Schl\u00fcssel", "tokens": ["Und", "ich", "seh'", "die", "vie\u00b7len", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieder h\u00e4ngen an den W\u00e4nden.", "tokens": ["Wie\u00b7der", "h\u00e4n\u00b7gen", "an", "den", "W\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drum, wer seine Kunst versteht,", "tokens": ["Drum", ",", "wer", "sei\u00b7ne", "Kunst", "ver\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Denke, wenn er's hat in H\u00e4nden:", "tokens": ["Den\u00b7ke", ",", "wenn", "er's", "hat", "in", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "VAFIN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}