{"textgrid.poem.41644": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann wie ein Deckel sich der Himmel tief gesenkt hat", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann wie ein Deckel sich der Himmel tief gesenkt hat", "tokens": ["Wann", "wie", "ein", "De\u00b7ckel", "sich", "der", "Him\u00b7mel", "tief", "ge\u00b7senkt", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KOKOM", "ART", "NN", "PRF", "ART", "NN", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf unsern Geist, den bang die Leere seufzen macht,", "tokens": ["Auf", "un\u00b7sern", "Geist", ",", "den", "bang", "die", "Lee\u00b7re", "seuf\u00b7zen", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann er den Horizont umschattet und umschr\u00e4nkt hat", "tokens": ["Wann", "er", "den", "Ho\u00b7ri\u00b7zont", "um\u00b7schat\u00b7tet", "und", "um\u00b7schr\u00e4nkt", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "KON", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schwarzen Tag ergie\u00dft, der tr\u00fcber als die Nacht;", "tokens": ["Und", "schwar\u00b7zen", "Tag", "er\u00b7gie\u00dft", ",", "der", "tr\u00fc\u00b7ber", "als", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann wie ein feucht Verlie\u00df das Erdall auf uns lastet,", "tokens": ["Wann", "wie", "ein", "feucht", "Ver\u00b7lie\u00df", "das", "Er\u00b7dall", "auf", "uns", "las\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOKOM", "ART", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin die Hoffnung gleich geschreckter Fledermaus", "tokens": ["Da\u00b7rin", "die", "Hoff\u00b7nung", "gleich", "ge\u00b7schreck\u00b7ter", "Fle\u00b7der\u00b7maus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit angstbeschwingtem Flug l\u00e4ngs dunkler Mauer hastet", "tokens": ["Mit", "angst\u00b7be\u00b7schwing\u00b7tem", "Flug", "l\u00e4ngs", "dunk\u00b7ler", "Mau\u00b7er", "has\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sich den Kopf st\u00f6\u00dft am Gew\u00f6lb des dumpfen Baus;", "tokens": ["Und", "sich", "den", "Kopf", "st\u00f6\u00dft", "am", "Ge\u00b7w\u00f6lb", "des", "dum\u00b7pfen", "Baus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann grau der Regenflut Gie\u00dff\u00e4den niederrinnen", "tokens": ["Wann", "grau", "der", "Re\u00b7gen\u00b7flut", "Gie\u00df\u00b7f\u00e4\u00b7den", "nie\u00b7der\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich eines Kerkerraums gewaltger Gitterwand,", "tokens": ["Gleich", "ei\u00b7nes", "Ker\u00b7ker\u00b7raums", "ge\u00b7walt\u00b7ger", "Git\u00b7ter\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wann ein stummes Volk von unheilvollen Spinnen", "tokens": ["Und", "wann", "ein", "stum\u00b7mes", "Volk", "von", "un\u00b7heil\u00b7vol\u00b7len", "Spin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Grunde unsres Hirns verruchte Netze spannt,", "tokens": ["Im", "Grun\u00b7de", "uns\u00b7res", "Hirns", "ver\u00b7ruch\u00b7te", "Net\u00b7ze", "spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann springen Glocken auf in w\u00fctendem Erbeben", "tokens": ["Dann", "sprin\u00b7gen", "Glo\u00b7cken", "auf", "in", "w\u00fc\u00b7ten\u00b7dem", "Er\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und senden ihr Geheul dem Himmel schrecklich zu,", "tokens": ["Und", "sen\u00b7den", "ihr", "Ge\u00b7heul", "dem", "Him\u00b7mel", "schreck\u00b7lich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie fremde Geister, die ge\u00e4chtet irrend schweben", "tokens": ["Wie", "frem\u00b7de", "Geis\u00b7ter", ",", "die", "ge\u00e4ch\u00b7tet", "ir\u00b7rend", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "VVPP", "ADJD", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und qu\u00e4lend Klagget\u00f6n ausst\u00f6hnen ohne Ruh.", "tokens": ["Und", "qu\u00e4\u00b7lend", "Klag\u00b7ge\u00b7t\u00f6n", "aus\u00b7st\u00f6h\u00b7nen", "oh\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und Leichenz\u00fcge, stumm, kein trauernd Grablied singend,", "tokens": ["Und", "Lei\u00b7chen\u00b7z\u00fc\u00b7ge", ",", "stumm", ",", "kein", "trau\u00b7ernd", "Grab\u00b7lied", "sin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "$,", "PIAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ziehn langsam durch mein Herz; die Hoffnung siegberaubt,", "tokens": ["Ziehn", "lang\u00b7sam", "durch", "mein", "Herz", ";", "die", "Hoff\u00b7nung", "sieg\u00b7be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Flieht weinend, und die Angst, entsetzlich, allbezwingend,", "tokens": ["Flieht", "wei\u00b7nend", ",", "und", "die", "Angst", ",", "ent\u00b7setz\u00b7lich", ",", "all\u00b7be\u00b7zwin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "VVPP", "$,", "KON", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Pflanzt ihre Fahne schwarz auf mein gesenktes Haupt.", "tokens": ["Pflanzt", "ih\u00b7re", "Fah\u00b7ne", "schwarz", "auf", "mein", "ge\u00b7senk\u00b7tes", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}