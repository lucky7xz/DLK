{"textgrid.poem.26649": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Preis dir, die du dorten heraufstrahlst, Tochter des Himmels!", "tokens": ["Preis", "dir", ",", "die", "du", "dor\u00b7ten", "her\u00b7auf\u00b7strahlst", ",", "Toch\u00b7ter", "des", "Him\u00b7mels", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Preis dem lieblichen Glanz", "tokens": ["Preis", "dem", "lieb\u00b7li\u00b7chen", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Deines L\u00e4chelns, der alles begr\u00fc\u00dfet und alles erfreuet!", "tokens": ["Dei\u00b7nes", "L\u00e4\u00b7chelns", ",", "der", "al\u00b7les", "be\u00b7gr\u00fc\u00b7\u00dfet", "und", "al\u00b7les", "er\u00b7freu\u00b7et", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Tr\u00fcb in Schauern und Nacht", "tokens": ["Tr\u00fcb", "in", "Schau\u00b7ern", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Stand begraben die pr\u00e4chtige Sch\u00f6pfung: tot war die Sch\u00f6nheit", "tokens": ["Stand", "be\u00b7gra\u00b7ben", "die", "pr\u00e4ch\u00b7ti\u00b7ge", "Sch\u00f6p\u00b7fung", ":", "tot", "war", "die", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$.", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Lang dem lechzenden Blick;", "tokens": ["Lang", "dem", "lech\u00b7zen\u00b7den", "Blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Aber liebevoll stiegst du fr\u00fch aus dem rosigen Scho\u00dfe", "tokens": ["A\u00b7ber", "lie\u00b7be\u00b7voll", "stiegst", "du", "fr\u00fch", "aus", "dem", "ro\u00b7si\u00b7gen", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Deiner Wolken empor,", "tokens": ["Dei\u00b7ner", "Wol\u00b7ken", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Wecktest uns auf die Morgenr\u00f6te; und freundlich", "tokens": ["Weck\u00b7test", "uns", "auf", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ";", "und", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "KON", "ADJD"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Schimmert' diese herf\u00fcr", "tokens": ["Schim\u00b7mert'", "die\u00b7se", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "\u00dcber die Berg und verk\u00fcndete deine s\u00fc\u00dfe Hervorkunft.", "tokens": ["\u00dc\u00b7ber", "die", "Berg", "und", "ver\u00b7k\u00fcn\u00b7de\u00b7te", "dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Her\u00b7vor\u00b7kunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Schnell begann nun das Graun", "tokens": ["Schnell", "be\u00b7gann", "nun", "das", "Graun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Sich zu w\u00e4lzen dahin in ungeheuern Geb\u00fcrgen.", "tokens": ["Sich", "zu", "w\u00e4l\u00b7zen", "da\u00b7hin", "in", "un\u00b7ge\u00b7heu\u00b7ern", "Ge\u00b7b\u00fcr\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Dann erschienest du selbst,", "tokens": ["Dann", "er\u00b7schie\u00b7nest", "du", "selbst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Herrliche du, und verschwunden waren die neblichte Riesen!", "tokens": ["Herr\u00b7li\u00b7che", "du", ",", "und", "ver\u00b7schwun\u00b7den", "wa\u00b7ren", "die", "neb\u00b7lich\u00b7te", "Rie\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-----+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Ach! wie Liebende nun,", "tokens": ["Ach", "!", "wie", "Lie\u00b7ben\u00b7de", "nun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "NN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Lange getrennt, lieb\u00e4ugelt der Himmel zur Erden, und diese", "tokens": ["Lan\u00b7ge", "ge\u00b7trennt", ",", "lie\u00b7b\u00e4u\u00b7gelt", "der", "Him\u00b7mel", "zur", "Er\u00b7den", ",", "und", "die\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "KON", "PDS"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "L\u00e4chelt zum Liebling empor;", "tokens": ["L\u00e4\u00b7chelt", "zum", "Lieb\u00b7ling", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Und es k\u00fcssen die Wolken am Saume der H\u00f6he die H\u00fcgel;", "tokens": ["Und", "es", "k\u00fcs\u00b7sen", "die", "Wol\u00b7ken", "am", "Sau\u00b7me", "der", "H\u00f6\u00b7he", "die", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.20": {"text": "S\u00fc\u00dfer atmet die Luft;", "tokens": ["S\u00fc\u00b7\u00dfer", "at\u00b7met", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Alle Fluren baden in deines Angesichts Abglanz", "tokens": ["Al\u00b7le", "Flu\u00b7ren", "ba\u00b7den", "in", "dei\u00b7nes", "An\u00b7ge\u00b7sichts", "Ab\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "--+--+-+-+--++", "measure": "anapaest.di.plus"}, "line.22": {"text": "Sich, und es wirbelt der Chor", "tokens": ["Sich", ",", "und", "es", "wir\u00b7belt", "der", "Chor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Des Gev\u00f6gels aus der vergoldeten Gr\u00fcne der W\u00e4lder", "tokens": ["Des", "Ge\u00b7v\u00f6\u00b7gels", "aus", "der", "ver\u00b7gol\u00b7de\u00b7ten", "Gr\u00fc\u00b7ne", "der", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Freudenlieder hinauf;", "tokens": ["Freu\u00b7den\u00b7lie\u00b7der", "hin\u00b7auf", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Alle Wesen taumeln wie am Busen der Wonne:", "tokens": ["Al\u00b7le", "We\u00b7sen", "tau\u00b7meln", "wie", "am", "Bu\u00b7sen", "der", "Won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "KOKOM", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Selig die ganze Natur!", "tokens": ["Se\u00b7lig", "die", "gan\u00b7ze", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Und dies alles, o Sonn! entquoll deiner himmlischen Liebe.", "tokens": ["Und", "dies", "al\u00b7les", ",", "o", "Sonn", "!", "ent\u00b7quoll", "dei\u00b7ner", "himm\u00b7li\u00b7schen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "$,", "FM", "NN", "$.", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Vater der Heilgen, vergib,", "tokens": ["Va\u00b7ter", "der", "Heil\u00b7gen", ",", "ver\u00b7gib", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVIMP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "O vergib mir, da\u00df ich auf mein Angesicht falle", "tokens": ["O", "ver\u00b7gib", "mir", ",", "da\u00df", "ich", "auf", "mein", "An\u00b7ge\u00b7sicht", "fal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Und anbete dein Werk! \u2013", "tokens": ["Und", "an\u00b7be\u00b7te", "dein", "Werk", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Aber nun schwebet sie fort im Zug der Purpurgew\u00f6lke", "tokens": ["A\u00b7ber", "nun", "schwe\u00b7bet", "sie", "fort", "im", "Zug", "der", "Pur\u00b7pur\u00b7ge\u00b7w\u00f6l\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "\u00dcber der K\u00f6nige Reich,", "tokens": ["\u00dc\u00b7ber", "der", "K\u00f6\u00b7ni\u00b7ge", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "\u00dcber die unabsehbarn Wasser, \u00fcber das Weltall:", "tokens": ["\u00dc\u00b7ber", "die", "un\u00b7ab\u00b7seh\u00b7barn", "Was\u00b7ser", ",", "\u00fc\u00b7ber", "das", "Wel\u00b7tall", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.34": {"text": "Unter ihr werden zu Staub", "tokens": ["Un\u00b7ter", "ihr", "wer\u00b7den", "zu", "Staub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "APPR", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "Alle Thronen, Moder die himmelaufschimmernden St\u00e4dte;", "tokens": ["Al\u00b7le", "Thro\u00b7nen", ",", "Mo\u00b7der", "die", "him\u00b7me\u00b7lauf\u00b7schim\u00b7mern\u00b7den", "St\u00e4d\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Ach! die Erde ist selbst", "tokens": ["Ach", "!", "die", "Er\u00b7de", "ist", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Grabesh\u00fcgel geworden. Sie aber bleibt in der H\u00f6he,", "tokens": ["Gra\u00b7bes\u00b7h\u00fc\u00b7gel", "ge\u00b7wor\u00b7den", ".", "Sie", "a\u00b7ber", "bleibt", "in", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAPP", "$.", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "L\u00e4chelt der M\u00f6rderin Zeit", "tokens": ["L\u00e4\u00b7chelt", "der", "M\u00f6r\u00b7de\u00b7rin", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.39": {"text": "Und erf\u00fcllet ihr gro\u00dfes Gesch\u00e4ft, erleuchtet die Sph\u00e4ren.", "tokens": ["Und", "er\u00b7f\u00fcl\u00b7let", "ihr", "gro\u00b7\u00dfes", "Ge\u00b7sch\u00e4ft", ",", "er\u00b7leuch\u00b7tet", "die", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.40": {"text": "O besuche noch lang,", "tokens": ["O", "be\u00b7su\u00b7che", "noch", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Herrlichstes F\u00fcrbild der Edeln! mit mildem, freundlichem Blicke", "tokens": ["Herr\u00b7lichs\u00b7tes", "F\u00fcr\u00b7bild", "der", "E\u00b7deln", "!", "mit", "mil\u00b7dem", ",", "freund\u00b7li\u00b7chem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Unsre Wohnung, bis einst", "tokens": ["Uns\u00b7re", "Woh\u00b7nung", ",", "bis", "einst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.43": {"text": "Vor dem Schelten des Ewigen sinken die Sterne", "tokens": ["Vor", "dem", "Schel\u00b7ten", "des", "E\u00b7wi\u00b7gen", "sin\u00b7ken", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.44": {"text": "Und du selbsten erbleichst.", "tokens": ["Und", "du", "selbs\u00b7ten", "er\u00b7bleichst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}