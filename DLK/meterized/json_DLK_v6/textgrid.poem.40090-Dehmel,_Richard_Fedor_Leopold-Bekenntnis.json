{"textgrid.poem.40090": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Bekenntnis", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will ergr\u00fcnden alle Lust,", "tokens": ["Ich", "will", "er\u00b7gr\u00fcn\u00b7den", "al\u00b7le", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so tief ich d\u00fcrften kann;", "tokens": ["so", "tief", "ich", "d\u00fcrf\u00b7ten", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich will sie schl\u00fcrfen ganz und gar,", "tokens": ["ich", "will", "sie", "schl\u00fcr\u00b7fen", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und st\u00fcrbe ich daran.", "tokens": ["und", "st\u00fcr\u00b7be", "ich", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will entlodern all die Glut,", "tokens": ["Ich", "will", "ent\u00b7lo\u00b7dern", "all", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die mir im Herzen brennt;", "tokens": ["die", "mir", "im", "Her\u00b7zen", "brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "hinrasend Element.", "tokens": ["hin\u00b7ra\u00b7send", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "NN", "$."], "meter": "++---+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Ward ich durch frommer Lippen Macht,", "tokens": ["Ward", "ich", "durch", "from\u00b7mer", "Lip\u00b7pen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch sanfter K\u00fcsse Tausch?", "tokens": ["durch", "sanf\u00b7ter", "K\u00fcs\u00b7se", "Tausch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich ward erzeugt bei wilder Nacht", "tokens": ["Ich", "ward", "er\u00b7zeugt", "bei", "wil\u00b7der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in tollem Wollustrausch!", "tokens": ["in", "tol\u00b7lem", "Wol\u00b7lust\u00b7rausch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun will ich ", "tokens": ["Nun", "will", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "da mich die Lust erschuf; \u2013", "tokens": ["da", "mich", "die", "Lust", "er\u00b7schuf", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "schreit nur den Himmel ", "tokens": ["schreit", "nur", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "ihr Beter von Beruf!", "tokens": ["ihr", "Be\u00b7ter", "von", "Be\u00b7ruf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}