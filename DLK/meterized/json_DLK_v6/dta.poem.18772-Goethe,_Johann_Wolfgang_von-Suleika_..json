{"dta.poem.18772": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Hochbegl\u00fcckt in deiner Liebe", "tokens": ["Hoch\u00b7be\u00b7gl\u00fcckt", "in", "dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schelt ich nicht Gelegenheit,", "tokens": ["Schelt", "ich", "nicht", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward sie auch an dir zum Diebe", "tokens": ["Ward", "sie", "auch", "an", "dir", "zum", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie mich solch ein Raub erfreut!", "tokens": ["Wie", "mich", "solch", "ein", "Raub", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wozu denn auch berauben?", "tokens": ["Und", "wo\u00b7zu", "denn", "auch", "be\u00b7rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gieb dich mir aus freyer Wahl,", "tokens": ["Gieb", "dich", "mir", "aus", "frey\u00b7er", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gar zu gerne m\u00f6cht ich glauben \u2014", "tokens": ["Gar", "zu", "ger\u00b7ne", "m\u00f6cht", "ich", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja! ich bin\u2019s die dich bestahl.", "tokens": ["Ja", "!", "ich", "bin's", "die", "dich", "be\u00b7stahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was so willig du gegeben", "tokens": ["Was", "so", "wil\u00b7lig", "du", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJD", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt dir herrlichen Gewinn,", "tokens": ["Bringt", "dir", "herr\u00b7li\u00b7chen", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Ruh, mein reiches Leben", "tokens": ["Mei\u00b7ne", "Ruh", ",", "mein", "rei\u00b7ches", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geb\u2019 ich freudig, nimm es hin.", "tokens": ["Geb'", "ich", "freu\u00b7dig", ",", "nimm", "es", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Scherze nicht! Nichts von Verarmen!", "tokens": ["Scher\u00b7ze", "nicht", "!", "Nichts", "von", "Ver\u00b7ar\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht uns nicht die Liebe reich?", "tokens": ["Macht", "uns", "nicht", "die", "Lie\u00b7be", "reich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halt ich dich in meinen Armen,", "tokens": ["Halt", "ich", "dich", "in", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Gl\u00fcck ist meines gleich.", "tokens": ["Je\u00b7dem", "Gl\u00fcck", "ist", "mei\u00b7nes", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}