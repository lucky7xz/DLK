{"textgrid.poem.31434": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erleuterung der Angst / des Herzens Ringerung /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erleuterung der Angst / des Herzens Ringerung /", "tokens": ["Er\u00b7leu\u00b7te\u00b7rung", "der", "Angst", "/", "des", "Her\u00b7zens", "Rin\u00b7ge\u00b7rung", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "der schmerzen Wolkenbruch / der Trauer-Augen Regen /", "tokens": ["der", "schmer\u00b7zen", "Wol\u00b7ken\u00b7bruch", "/", "der", "Trau\u00b7e\u00b7rAu\u00b7gen", "Re\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ihr Thr\u00e4nen! die ihr seyd / wann sich die Winde legen /", "tokens": ["ihr", "Thr\u00e4\u00b7nen", "!", "die", "ihr", "seyd", "/", "wann", "sich", "die", "Win\u00b7de", "le\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PRELS", "PPER", "VAFIN", "$(", "PWAV", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die Seufzer / da das Herz vor Aengsten schier zersprung!", "tokens": ["die", "Seuf\u00b7zer", "/", "da", "das", "Herz", "vor", "A\u00b7engs\u00b7ten", "schier", "zer\u00b7sprung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "ART", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ihr seyd recht zwischen Furcht und Trost die d\u00e4mmerung.", "tokens": ["Ihr", "seyd", "recht", "zwi\u00b7schen", "Furcht", "und", "Trost", "die", "d\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mit sorgen seet man euch auf diesen Vngl\u00fccks wegen:", "tokens": ["Mit", "sor\u00b7gen", "seet", "man", "euch", "auf", "die\u00b7sen", "Vn\u00b7gl\u00fccks", "we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PPER", "APPR", "PDAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "doch bringt ihr manches mal de\u00df reichen Gl\u00fcckes Segen /", "tokens": ["doch", "bringt", "ihr", "man\u00b7ches", "mal", "de\u00df", "rei\u00b7chen", "Gl\u00fc\u00b7ckes", "Se\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "aus ursach / weil eur Flu\u00df die Himmels F\u00e4st durchdrung.", "tokens": ["aus", "ur\u00b7sach", "/", "weil", "eur", "Flu\u00df", "die", "Him\u00b7mels", "F\u00e4st", "durch\u00b7drung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "De\u00df Himmel-Regens Zweck / ist fruchtbarkeit der Erden /", "tokens": ["De\u00df", "Him\u00b7mel\u00b7Re\u00b7gens", "Zweck", "/", "ist", "frucht\u00b7bar\u00b7keit", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "VAFIN", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da dann der fr\u00fcchte Frucht Gott wider geben werden /", "tokens": ["da", "dann", "der", "fr\u00fcch\u00b7te", "Frucht", "Gott", "wi\u00b7der", "ge\u00b7ben", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "NN", "APPR", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Lob' / Ehre / Prei\u00df und dank / mit worten und geb\u00e4rden.", "tokens": ["Lob'", "/", "Eh\u00b7re", "/", "Prei\u00df", "und", "dank", "/", "mit", "wor\u00b7ten", "und", "ge\u00b7b\u00e4r\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "APPR", "$(", "APPR", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Thr\u00e4nen / dienen auch / zu wahrer Tugend Zucht /", "tokens": ["Die", "Thr\u00e4\u00b7nen", "/", "die\u00b7nen", "auch", "/", "zu", "wah\u00b7rer", "Tu\u00b7gend", "Zucht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "ADV", "$(", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "erweichen Gottes Herz und bringen freuden Frucht.", "tokens": ["er\u00b7wei\u00b7chen", "Got\u00b7tes", "Herz", "und", "brin\u00b7gen", "freu\u00b7den", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch sie / nimt aus dem Sinn die kummernus die Flucht.", "tokens": ["Durch", "sie", "/", "nimt", "aus", "dem", "Sinn", "die", "kum\u00b7mer\u00b7nus", "die", "Flucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}