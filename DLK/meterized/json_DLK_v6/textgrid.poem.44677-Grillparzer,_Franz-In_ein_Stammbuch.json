{"textgrid.poem.44677": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn nicht Unwert d\u00fcnkelvoll,", "tokens": ["Wenn", "nicht", "Un\u00b7wert", "d\u00fcn\u00b7kel\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wert dagegen w\u00e4r bescheiden,", "tokens": ["Wert", "da\u00b7ge\u00b7gen", "w\u00e4r", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spr\u00e4che leicht man, wo man soll,", "tokens": ["Spr\u00e4\u00b7che", "leicht", "man", ",", "wo", "man", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIS", "$,", "PWAV", "PIS", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spr\u00e4che nicht, wos zu vermeiden:", "tokens": ["Spr\u00e4\u00b7che", "nicht", ",", "wos", "zu", "ver\u00b7mei\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "PTKZU", "VVINF", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Doch, da nun das Ma\u00df verkehrt,", "tokens": ["Doch", ",", "da", "nun", "das", "Ma\u00df", "ver\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Kann sich stumm nur Achtung zeigen,", "tokens": ["Kann", "sich", "stumm", "nur", "Ach\u00b7tung", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, erkennend deinen Wert,", "tokens": ["Und", ",", "er\u00b7ken\u00b7nend", "dei\u00b7nen", "Wert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df ich mein Gef\u00fchl verschweigen.", "tokens": ["Mu\u00df", "ich", "mein", "Ge\u00b7f\u00fchl", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}