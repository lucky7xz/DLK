{"textgrid.poem.58424": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "462. Innige Zeitk\u00fcrzung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Aug', von allem ab-, auf Gott stets eingewandt,", "tokens": ["Ein", "Aug'", ",", "von", "al\u00b7lem", "ab", ",", "auf", "Gott", "stets", "ein\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PIS", "TRUNC", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein abgesp\u00e4ntes Herz, mit Gott im Geist bekannt,", "tokens": ["Ein", "ab\u00b7ge\u00b7sp\u00e4n\u00b7tes", "Herz", ",", "mit", "Gott", "im", "Geist", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ihn stets inniglich umfa\u00dft mit stiller Liebe,", "tokens": ["Das", "ihn", "stets", "in\u00b7nig\u00b7lich", "um\u00b7fa\u00dft", "mit", "stil\u00b7ler", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies ist mein Zeitvertreib, worin ich stets mich \u00fcbe.", "tokens": ["Dies", "ist", "mein", "Zeit\u00b7ver\u00b7treib", ",", "wo\u00b7rin", "ich", "stets", "mich", "\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}