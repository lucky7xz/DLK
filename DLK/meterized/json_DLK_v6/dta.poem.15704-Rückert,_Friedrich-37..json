{"dta.poem.15704": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Glaubenseifer ruft: Gib die Vernunft gefangen!", "tokens": ["Der", "Glau\u00b7ben\u00b7sei\u00b7fer", "ruft", ":", "Gib", "die", "Ver\u00b7nunft", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sie, die freie, will nicht blind an Satzung hangen.", "tokens": ["Doch", "sie", ",", "die", "frei\u00b7e", ",", "will", "nicht", "blind", "an", "Sat\u00b7zung", "han\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "$,", "VMFIN", "PTKNEG", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur wer ihr zeigt, wie Glaub' und Freiheit sich vertr\u00e4gt,", "tokens": ["Nur", "wer", "ihr", "zeigt", ",", "wie", "Glaub'", "und", "Frei\u00b7heit", "sich", "ver\u00b7tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "$,", "PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat der ungl\u00e4ubigen die Fessel angelegt.", "tokens": ["Hat", "der", "un\u00b7gl\u00e4u\u00b7bi\u00b7gen", "die", "Fes\u00b7sel", "an\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN",