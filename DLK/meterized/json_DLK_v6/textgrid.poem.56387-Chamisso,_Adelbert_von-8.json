{"textgrid.poem.56387": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich halte verborgen", "tokens": ["\u00bb", "ich", "hal\u00b7te", "ver\u00b7bor\u00b7gen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Hammer des Thors", "tokens": ["Den", "Ham\u00b7mer", "des", "Thors"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wohl unter der Erde", "tokens": ["Wohl", "un\u00b7ter", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Acht Morgen tief,", "tokens": ["Acht", "Mor\u00b7gen", "tief", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und wieder erwerben,", "tokens": ["Und", "wie\u00b7der", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "F\u00fcrwahr, soll ihn keiner,", "tokens": ["F\u00fcr\u00b7wahr", ",", "soll", "ihn", "kei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Er f\u00fchre denn Fraya", "tokens": ["Er", "f\u00fch\u00b7re", "denn", "Fray\u00b7a"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zur Frau mir heim.\u00ab", "tokens": ["Zur", "Frau", "mir", "heim", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}