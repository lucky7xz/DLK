{"dta.poem.22390": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "193. Der Sieg ist wesentlich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch weil es nicht im wolln un\u0303 eygnen Lauffen ligt/", "tokens": ["Mensch", "weil", "es", "nicht", "im", "wolln", "u\u00f1", "eyg\u00b7nen", "Lauf\u00b7fen", "ligt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PTKNEG", "APPRART", "ADJA", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mustu thun wie GOtt/ der ohne willen Sigt.", "tokens": ["So", "mus\u00b7tu", "thun", "wie", "Gott", "/", "der", "oh\u00b7ne", "wil\u00b7len", "Sigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "KOKOM", "NN", "$(", "ART", "APPR", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}