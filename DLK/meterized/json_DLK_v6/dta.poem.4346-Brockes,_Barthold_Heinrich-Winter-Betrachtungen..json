{"dta.poem.4346": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Betrachtungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein vern\u00fcnftig Menschen-Auge", "tokens": ["Ein", "ver\u00b7n\u00fcnf\u00b7tig", "Men\u00b7schen\u00b7Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Findet, auch zur Winters-Zeit,", "tokens": ["Fin\u00b7det", ",", "auch", "zur", "Win\u00b7ter\u00b7sZeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie verschiedne Zierlichkeit", "tokens": ["Wie", "ver\u00b7schied\u00b7ne", "Zier\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es noch zu vergn\u00fcgen tauge,", "tokens": ["Es", "noch", "zu", "ver\u00b7gn\u00fc\u00b7gen", "tau\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn sichs nur so viel bem\u00fcht,", "tokens": ["Wenn", "sichs", "nur", "so", "viel", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Dinge, die auf Erden", "tokens": ["Und", "die", "Din\u00b7ge", ",", "die", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch geschm\u00fcckt gesehen werden,", "tokens": ["Noch", "ge\u00b7schm\u00fcckt", "ge\u00b7se\u00b7hen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit Bedachtsamkeit besieht.", "tokens": ["Mit", "Be\u00b7dacht\u00b7sam\u00b7keit", "be\u00b7sieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn es itzo schneit und frieret,", "tokens": ["Wenn", "es", "it\u00b7zo", "schneit", "und", "frie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird uns eine weisse Welt,", "tokens": ["Wird", "uns", "ei\u00b7ne", "weis\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Voller Schimmer, vorgestellt,", "tokens": ["Vol\u00b7ler", "Schim\u00b7mer", ",", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ein Glanz, wie Silber, zieret,", "tokens": ["Die", "ein", "Glanz", ",", "wie", "Sil\u00b7ber", ",", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PWAV", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des glatten Eises Schein", "tokens": ["Und", "des", "glat\u00b7ten", "Ei\u00b7ses", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Funkelt, wenn der Sonnen Strahlen", "tokens": ["Fun\u00b7kelt", ",", "wenn", "der", "Son\u00b7nen", "Strah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Es mit tausend Farben mahlen,", "tokens": ["Es", "mit", "tau\u00b7send", "Far\u00b7ben", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein Diamant so rein.", "tokens": ["Wie", "ein", "Di\u00b7a\u00b7mant", "so", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Ist es schlackricht, und es thauet;", "tokens": ["Ist", "es", "schlack\u00b7richt", ",", "und", "es", "thau\u00b7et", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist doch noch die Sch\u00f6nheit gro\u00df,", "tokens": ["Ist", "doch", "noch", "die", "Sch\u00f6n\u00b7heit", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die man in dem gr\u00fcnen Moo\u00df,", "tokens": ["Die", "man", "in", "dem", "gr\u00fc\u00b7nen", "Moo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der den Sammt besch\u00e4met, schauet.", "tokens": ["Der", "den", "Sammt", "be\u00b7sch\u00e4\u00b7met", ",", "schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ist wohl ein Gr\u00fcn so gr\u00fcn,", "tokens": ["Wo", "ist", "wohl", "ein", "Gr\u00fcn", "so", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Als die dicht-vereinte Spitzen,", "tokens": ["Als", "die", "dicht\u00b7ver\u00b7ein\u00b7te", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die itzt auf den D\u00e4chern sitzen,", "tokens": ["Die", "itzt", "auf", "den", "D\u00e4\u00b7chern", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Welche sich mit Moo\u00df beziehn.", "tokens": ["Wel\u00b7che", "sich", "mit", "Moo\u00df", "be\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn fast alles Gr\u00fcn vergangen,", "tokens": ["Wenn", "fast", "al\u00b7les", "Gr\u00fcn", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die\u00df angenehme Kraut,", "tokens": ["Hat", "die\u00df", "an\u00b7ge\u00b7neh\u00b7me", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das man, sonder Lust, nicht schaut,", "tokens": ["Das", "man", ",", "son\u00b7der", "Lust", ",", "nicht", "schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "KON", "NN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst zu gr\u00fcnen angefangen.", "tokens": ["Erst", "zu", "gr\u00fc\u00b7nen", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es wird \u00fcberall erblickt.", "tokens": ["Es", "wird", "\u00fc\u00b7be\u00b7rall", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Aus den St\u00e4mmen von den B\u00e4umen", "tokens": ["Aus", "den", "St\u00e4m\u00b7men", "von", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieht man es am meisten keimen,", "tokens": ["Sieht", "man", "es", "am", "meis\u00b7ten", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PTKA", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die es wunderw\u00fcrdig schm\u00fcckt.", "tokens": ["Die", "es", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da wir an den feuchten Rinden", "tokens": ["Da", "wir", "an", "den", "feuch\u00b7ten", "Rin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Es ganz unbeschreiblich sch\u00f6n", "tokens": ["Es", "ganz", "un\u00b7be\u00b7schreib\u00b7lich", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mannigfach gef\u00e4rbet sehn,", "tokens": ["Man\u00b7nig\u00b7fach", "ge\u00b7f\u00e4r\u00b7bet", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in vielen Formen finden.", "tokens": ["Und", "in", "vie\u00b7len", "For\u00b7men", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer, der es nie bemerkt,", "tokens": ["Ei\u00b7ner", ",", "der", "es", "nie", "be\u00b7merkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Stutzet, wenn er es beachtet;", "tokens": ["Stut\u00b7zet", ",", "wenn", "er", "es", "be\u00b7ach\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einer, der es sonst betrachtet,", "tokens": ["Ei\u00b7ner", ",", "der", "es", "sonst", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird in seiner Lust gest\u00e4rkt.", "tokens": ["Wird", "in", "sei\u00b7ner", "Lust", "ge\u00b7st\u00e4rkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "S\u00fc\u00df und lieblich sieht man spielen", "tokens": ["S\u00fc\u00df", "und", "lieb\u00b7lich", "sieht", "man", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gelblich, R\u00f6htlich, Gr\u00fcn und Wei\u00df.", "tokens": ["Gelb\u00b7lich", ",", "R\u00f6ht\u00b7lich", ",", "Gr\u00fcn", "und", "Wei\u00df", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes scheinet auf den Preis,", "tokens": ["Je\u00b7des", "schei\u00b7net", "auf", "den", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was das sch\u00f6nste sey, zu zielen.", "tokens": ["Was", "das", "sch\u00f6ns\u00b7te", "sey", ",", "zu", "zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche zierliche Figur", "tokens": ["Man\u00b7che", "zier\u00b7li\u00b7che", "Fi\u00b7gur"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht man hier und dort sich mischen,", "tokens": ["Sieht", "man", "hier", "und", "dort", "sich", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "KON", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von kleinen holden B\u00fcschen", "tokens": ["Und", "von", "klei\u00b7nen", "hol\u00b7den", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeigt sich hier und dort die Spur.", "tokens": ["Zeigt", "sich", "hier", "und", "dort", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ferner sehen unsre Blicke,", "tokens": ["Fer\u00b7ner", "se\u00b7hen", "uns\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im Winter ja so wohl", "tokens": ["Wie", "im", "Win\u00b7ter", "ja", "so", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der gef\u00e4rbte braune Kohl", "tokens": ["Der", "ge\u00b7f\u00e4rb\u00b7te", "brau\u00b7ne", "Kohl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den entlaubten Garten schm\u00fccke.", "tokens": ["Den", "ent\u00b7laub\u00b7ten", "Gar\u00b7ten", "schm\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sein mit Gr\u00fcn gemischtes Blatt,", "tokens": ["Sein", "mit", "Gr\u00fcn", "ge\u00b7mischtes", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Das auf Purpur-Stengeln stehet,", "tokens": ["Das", "auf", "Pur\u00b7pur\u00b7S\u00b7ten\u00b7geln", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wird, durch manche Farb\u2019, erh\u00f6het,", "tokens": ["Wird", ",", "durch", "man\u00b7che", "Fa\u00b7rb'", ",", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Deren er verschiedne hat.", "tokens": ["De\u00b7ren", "er", "ver\u00b7schied\u00b7ne", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn, zumahl bey heiterm Wetter,", "tokens": ["Wenn", ",", "zu\u00b7mahl", "bey", "hei\u00b7term", "Wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das entw\u00f6lkte Sonnen-Licht", "tokens": ["Das", "ent\u00b7w\u00f6lk\u00b7te", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch ihr zart Gewebe bricht,", "tokens": ["Durch", "ihr", "zart", "Ge\u00b7we\u00b7be", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Funkeln die durchsicht\u2019ge Bl\u00e4tter.", "tokens": ["Fun\u00b7keln", "die", "durch\u00b7sicht'\u00b7ge", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch vermehret sich ihr Schein,", "tokens": ["Noch", "ver\u00b7meh\u00b7ret", "sich", "ihr", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn, wie Kugeln, von Krystallen", "tokens": ["Wenn", ",", "wie", "Ku\u00b7geln", ",", "von", "Krys\u00b7tal\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kleine Tropfen drauf gefallen,", "tokens": ["Klei\u00b7ne", "Trop\u00b7fen", "drauf", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die dem Demant \u00e4hnlich seyn.", "tokens": ["Die", "dem", "De\u00b7mant", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Seht, wie dort der Beeten Grenzen", "tokens": ["Seht", ",", "wie", "dort", "der", "Bee\u00