{"dta.poem.19320": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht darum soll es sich bei deinem Willen handeln,", "tokens": ["Nicht", "da\u00b7rum", "soll", "es", "sich", "bei", "dei\u00b7nem", "Wil\u00b7len", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn zu verbessern, Mensch, vielmehr ihn zu verwandeln;", "tokens": ["Ihn", "zu", "ver\u00b7bes\u00b7sern", ",", "Mensch", ",", "viel\u00b7mehr", "ihn", "zu", "ver\u00b7wan\u00b7deln", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "NN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn unverbesserlich, unheilbar sei der Schade,", "tokens": ["Denn", "un\u00b7ver\u00b7bes\u00b7ser\u00b7lich", ",", "un\u00b7heil\u00b7bar", "sei", "der", "Scha\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umwandlung m\u00f6glich nur durchs Wunderwerk der Gnade.", "tokens": ["Um\u00b7wand\u00b7lung", "m\u00f6g\u00b7lich", "nur", "durchs", "Wun\u00b7der\u00b7werk", "der", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein der h\u00f6chste Streit, der \u00fcber die Natur", "tokens": ["Al\u00b7lein", "der", "h\u00f6chs\u00b7te", "Streit", ",", "der", "\u00fc\u00b7ber", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Willens wird gef\u00fchrt, scheint mir ein Wortstreit nur.", "tokens": ["Des", "Wil\u00b7lens", "wird", "ge\u00b7f\u00fchrt", ",", "scheint", "mir", "ein", "Wort\u00b7streit", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Umwandeln m\u00f6gt ihr ihn, verwandeln ganz und gar,", "tokens": ["Um\u00b7wan\u00b7deln", "m\u00f6gt", "ihr", "ihn", ",", "ver\u00b7wan\u00b7deln", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einem andern doch nicht machen als er war.", "tokens": ["Zu", "ei\u00b7nem", "an\u00b7dern", "doch", "nicht", "ma\u00b7chen", "als", "er", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "PTKNEG", "VVINF", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Verwandeltet ihr mich, da\u00df ich nicht mehr w\u00e4r' ich,", "tokens": ["Ver\u00b7wan\u00b7del\u00b7tet", "ihr", "mich", ",", "da\u00df", "ich", "nicht", "mehr", "w\u00e4r'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00e4ttet ihr, ich wei\u00df nicht wen, geheilt, nicht mich.", "tokens": ["So", "h\u00e4t\u00b7tet", "ihr", ",", "ich", "wei\u00df", "nicht", "wen", ",", "ge\u00b7heilt", ",", "nicht", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "VVINF", "$,", "VVPP", "$,", "PTKNEG", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch einen guten Kern m\u00fc\u00dft ihr dem Willen g\u00f6nnen;", "tokens": ["Doch", "ei\u00b7nen", "gu\u00b7ten", "Kern", "m\u00fc\u00dft", "ihr", "dem", "Wil\u00b7len", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn schlecht im Kerne, w\u00fcrd' er gut nie werden k\u00f6nnen.", "tokens": ["Denn", "schlecht", "im", "Ker\u00b7ne", ",", "w\u00fcrd'", "er", "gut", "nie", "wer\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "VAFIN", "PPER", "ADJD", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Am kahlen S\u00fcnderkopf m\u00fc\u00dft ihr ein L\u00f6ckchen lassen,", "tokens": ["Am", "kah\u00b7len", "S\u00fcn\u00b7der\u00b7kopf", "m\u00fc\u00dft", "ihr", "ein", "L\u00f6ck\u00b7chen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daran der Finger ihn der Gnadenzucht kann fassen.", "tokens": ["Da\u00b7ran", "der", "Fin\u00b7ger", "ihn", "der", "Gna\u00b7den\u00b7zucht", "kann", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein Aschenf\u00fcnkchen mu\u00df doch seyn im Aschenhaufen,", "tokens": ["Ein", "A\u00b7schen\u00b7f\u00fcnk\u00b7chen", "mu\u00df", "doch", "seyn", "im", "A\u00b7schen\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VAINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst bl\u00e4st das Feuer an kein Schnauben und kein Schnaufen.", "tokens": ["Sonst", "bl\u00e4st", "das", "Feu\u00b7er", "an", "kein", "Schnau\u00b7ben", "und", "kein", "Schnau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ein gleich Bed\u00fcrfnis wird verschiedentlich gef\u00fchlt,", "tokens": ["Ein", "gleich", "Be\u00b7d\u00fcrf\u00b7nis", "wird", "ver\u00b7schie\u00b7dent\u00b7lich", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Da\u00df etwas sei hinweg gewaschen und gesp\u00fclt,", "tokens": ["Da\u00df", "et\u00b7was", "sei", "hin\u00b7weg", "ge\u00b7wa\u00b7schen", "und", "ge\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "ADV", "ADJA", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ein Schmutz hinweggefegt, ein Rost hinweggescheuert,", "tokens": ["Ein", "Schmutz", "hin\u00b7weg\u00b7ge\u00b7fegt", ",", "ein", "Rost", "hin\u00b7weg\u00b7ge\u00b7scheu\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit im eignen Glanz der Spiegel sei erneuert.", "tokens": ["Da\u00b7mit", "im", "eig\u00b7nen", "Glanz", "der", "Spie\u00b7gel", "sei", "er\u00b7neu\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Da\u00df sich der Spiegel selbst nicht kl\u00e4ren kann, ist klar;", "tokens": ["Da\u00df", "sich", "der", "Spie\u00b7gel", "selbst", "nicht", "kl\u00e4\u00b7ren", "kann", ",", "ist", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihm nur Gott den Dienst gew\u00e4hren kann, ist wahr.", "tokens": ["Da\u00df", "ihm", "nur", "Gott", "den", "Dienst", "ge\u00b7w\u00e4h\u00b7ren", "kann", ",", "ist", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ART", "NN", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Da\u00df Gott sich spiegle, mu\u00dft du ihm den Spiegel leihen,", "tokens": ["Da\u00df", "Gott", "sich", "spieg\u00b7le", ",", "mu\u00dft", "du", "ihm", "den", "Spie\u00b7gel", "lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Selbstbespieglung fern und von Vorspiegeleien.", "tokens": ["Von", "Selbst\u00b7be\u00b7spieg\u00b7lung", "fern", "und", "von", "Vor\u00b7spie\u00b7ge\u00b7lei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Die Hauptsach' aber ist, da\u00df rein der Spiegel sei;", "tokens": ["Die", "Haupt\u00b7sach'", "a\u00b7ber", "ist", ",", "da\u00df", "rein", "der", "Spie\u00b7gel", "sei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "KOUS", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Uebrige, mein Sohn, ist Spiegelfechterei.", "tokens": ["Das", "Ueb\u00b7ri\u00b7ge", ",", "mein", "Sohn", ",", "ist", "Spie\u00b7gel\u00b7fech\u00b7te\u00b7rei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}