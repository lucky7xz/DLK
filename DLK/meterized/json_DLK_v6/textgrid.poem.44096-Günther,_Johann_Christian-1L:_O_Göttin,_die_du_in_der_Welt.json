{"textgrid.poem.44096": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: O G\u00f6ttin, die du in der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O G\u00f6ttin, die du in der Welt", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "die", "du", "in", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die meisten W\u00fcntsche kriegst und zeugest,", "tokens": ["Die", "meis\u00b7ten", "W\u00fcnt\u00b7sche", "kriegst", "und", "zeu\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An Nachdruck Warheit, Lieb und Geld,", "tokens": ["An", "Nach\u00b7druck", "War\u00b7heit", ",", "Lieb", "und", "Geld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Lust die Hofnung \u00fcbersteigest,", "tokens": ["An", "Lust", "die", "Hof\u00b7nung", "\u00fc\u00b7bers\u00b7tei\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du, deren Huld der Kl\u00fcgsten List", "tokens": ["Du", ",", "de\u00b7ren", "Huld", "der", "Kl\u00fcgs\u00b7ten", "List"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So angenehm als n\u00f6thig ist,", "tokens": ["So", "an\u00b7ge\u00b7nehm", "als", "n\u00f6\u00b7thig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo soll ich deinen Tempel finden?", "tokens": ["Wo", "soll", "ich", "dei\u00b7nen", "Tem\u00b7pel", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo steht dein Bild, wo raucht dein Herd,", "tokens": ["Wo", "steht", "dein", "Bild", ",", "wo", "raucht", "dein", "Herd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dir, wenn man H\u00fclf und Trost begehrt,", "tokens": ["Dir", ",", "wenn", "man", "H\u00fclf", "und", "Trost", "be\u00b7gehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PIS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein kr\u00e4ftig Opfer anzuz\u00fcnden?", "tokens": ["Ein", "kr\u00e4f\u00b7tig", "Op\u00b7fer", "an\u00b7zu\u00b7z\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kenn und ehre deine Macht,", "tokens": ["Ich", "kenn", "und", "eh\u00b7re", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie trozt Gefahr, Vernunft und St\u00e4rcke,", "tokens": ["Sie", "trozt", "Ge\u00b7fahr", ",", "Ver\u00b7nunft", "und", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie leitet uns durch Fall und Nacht", "tokens": ["Sie", "lei\u00b7tet", "uns", "durch", "Fall", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und thut verliebte Wunderwercke;", "tokens": ["Und", "thut", "ver\u00b7lieb\u00b7te", "Wun\u00b7der\u00b7wer\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie st\u00fcrzt und mehrt der Reiche Staat,", "tokens": ["Sie", "st\u00fcrzt", "und", "mehrt", "der", "Rei\u00b7che", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Wort kommt mit in Kriegesrath", "tokens": ["Ihr", "Wort", "kommt", "mit", "in", "Krie\u00b7ges\u00b7rath"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hat den Rang in allen F\u00e4llen;", "tokens": ["Und", "hat", "den", "Rang", "in", "al\u00b7len", "F\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie macht auch Sclaven endlich frey", "tokens": ["Sie", "macht", "auch", "Scla\u00b7ven", "end\u00b7lich", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und spricht gar oft zur Tyranney:", "tokens": ["Und", "spricht", "gar", "oft", "zur", "Ty\u00b7ran\u00b7ney", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hier lege deine stolze Wellen.", "tokens": ["Hier", "le\u00b7ge", "dei\u00b7ne", "stol\u00b7ze", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was kan wohl Gr\u00f6\u00dfres als Eugen", "tokens": ["Was", "kan", "wohl", "Gr\u00f6\u00df\u00b7res", "als", "Eu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "NN", "KOKOM", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Nachruhm unsrer Zeit erheben?", "tokens": ["Den", "Nach\u00b7ruhm", "uns\u00b7rer", "Zeit", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und gleichwohl wird der Held gestehn,", "tokens": ["Und", "gleich\u00b7wohl", "wird", "der", "Held", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Vorschub hab ihm Glanz gegeben.", "tokens": ["Dein", "Vor\u00b7schub", "hab", "ihm", "Glanz", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit was vor Sorgfalt, M\u00fch und Kunst", "tokens": ["Mit", "was", "vor", "Sorg\u00b7falt", ",", "M\u00fch", "und", "Kunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bedient er sich nicht deiner Gunst", "tokens": ["Be\u00b7dient", "er", "sich", "nicht", "dei\u00b7ner", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Franckreichs Hohn und Stambols Falle!", "tokens": ["Zu", "Fran\u00b7ck\u00b7reichs", "Hohn", "und", "Stam\u00b7bols", "Fal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Er zieht, er schl\u00e4gt, er siegt durch dich;", "tokens": ["Er", "zieht", ",", "er", "schl\u00e4gt", ",", "er", "siegt", "durch", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Cremona sch\u00e4mt sich ewiglich,", "tokens": ["Cre\u00b7mo\u00b7na", "sch\u00e4mt", "sich", "e\u00b7wig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dies seltne Beyspiel steht vor alle.", "tokens": ["Dies", "selt\u00b7ne", "Bey\u00b7spiel", "steht", "vor", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun, m\u00e4chtige Gelegenheit,", "tokens": ["Nun", ",", "m\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun komm und gieb mir holde Mienen;", "tokens": ["Nun", "komm", "und", "gieb", "mir", "hol\u00b7de", "Mie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst dir von der Danckbarkeit", "tokens": ["Du", "wirst", "dir", "von", "der", "Dan\u00b7ck\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Manch hohes Ehrenlied verdienen.", "tokens": ["Manch", "ho\u00b7hes", "Eh\u00b7ren\u00b7lied", "ver\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast dich mich, so lang ich bin,", "tokens": ["Du", "hast", "dich", "mich", ",", "so", "lang", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Eifer oder Eigensinn", "tokens": ["Aus", "Ei\u00b7fer", "o\u00b7der", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Oft sehn, doch niemahls halten la\u00dfen.", "tokens": ["Oft", "sehn", ",", "doch", "nie\u00b7mahls", "hal\u00b7ten", "la\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So nah mir auch dein fl\u00fcchtig Haar", "tokens": ["So", "nah", "mir", "auch", "dein", "fl\u00fcch\u00b7tig", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In Dresden und in B[re\u00dflau] war,", "tokens": ["In", "Dres\u00b7den", "und", "in", "B", "re\u00df\u00b7lau", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "XY", "$(", "ADJD", "$(", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So schamroth kriegt ich Wind zu fa\u00dfen.", "tokens": ["So", "scham\u00b7roth", "kriegt", "ich", "Wind", "zu", "fa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich z\u00fcrne, wenn der P\u00f6bel spricht,", "tokens": ["Ich", "z\u00fcr\u00b7ne", ",", "wenn", "der", "P\u00f6\u00b7bel", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du seyst die Mutter aller Diebe;", "tokens": ["Du", "seyst", "die", "Mut\u00b7ter", "al\u00b7ler", "Die\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur teusche mich vor diesmahl nicht", "tokens": ["Nur", "teu\u00b7sche", "mich", "vor", "dies\u00b7mahl", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "PPER", "APPR", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und thu mir einmahl was zu Liebe.", "tokens": ["Und", "thu", "mir", "ein\u00b7mahl", "was", "zu", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du solt, wo nicht in Gold und Stein,", "tokens": ["Du", "solt", ",", "wo", "nicht", "in", "Gold", "und", "Stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Davor in Wachs mein Hausgott seyn.", "tokens": ["Da\u00b7vor", "in", "Wachs", "mein", "Haus\u00b7gott", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bitte ja nicht hohe Sachen.", "tokens": ["Ich", "bit\u00b7te", "ja", "nicht", "ho\u00b7he", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was denn? Um Mittel, jeden Feind,", "tokens": ["Was", "denn", "?", "Um", "Mit\u00b7tel", ",", "je\u00b7den", "Feind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "APPR", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der jezt mein \u00e4rgstes Ungl\u00fcck meint,", "tokens": ["Der", "jezt", "mein", "\u00e4rgs\u00b7tes", "Un\u00b7gl\u00fcck", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch H\u00fclf und Wohlthun fromm zu machen.", "tokens": ["Durch", "H\u00fclf", "und", "Wohl\u00b7thun", "fromm", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}