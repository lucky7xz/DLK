{"textgrid.poem.37142": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Freundschaft, Chloe, soll ich singen?", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Freundschaft, Chloe, soll ich singen?", "tokens": ["Von", "Freund\u00b7schaft", ",", "Chloe", ",", "soll", "ich", "sin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wann gab ein Dichter der sein Lied?", "tokens": ["Wann", "gab", "ein", "Dich\u00b7ter", "der", "sein", "Lied", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die um ihr ganzes Recht zu bringen,", "tokens": ["Die", "um", "ihr", "gan\u00b7zes", "Recht", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brauchts nur da\u00df man dich l\u00e4cheln sieht.", "tokens": ["Brauchts", "nur", "da\u00df", "man", "dich", "l\u00e4\u00b7cheln", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "PIS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bewundrer hat in ihrem Lenze", "tokens": ["Be\u00b7wund\u00b7rer", "hat", "in", "ih\u00b7rem", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00f6nheit, aber Freunde nicht.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "a\u00b7ber", "Freun\u00b7de", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Scherze flechten Heben Kr\u00e4nze,", "tokens": ["Die", "Scher\u00b7ze", "flech\u00b7ten", "He\u00b7ben", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lacht und h\u00f6rt, was Amor spricht.", "tokens": ["Sie", "lacht", "und", "h\u00f6rt", ",", "was", "A\u00b7mor", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch f\u00fchlen wir der Jugend Freuden,", "tokens": ["Noch", "f\u00fch\u00b7len", "wir", "der", "Ju\u00b7gend", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir lieben und wir wollen noch", "tokens": ["Wir", "lie\u00b7ben", "und", "wir", "wol\u00b7len", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um seine nicht den Herbst beneiden,", "tokens": ["Um", "sei\u00b7ne", "nicht", "den", "Herbst", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6mmt er zu fr\u00fch uns einstens doch!", "tokens": ["K\u00f6mmt", "er", "zu", "fr\u00fch", "uns", "eins\u00b7tens", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}