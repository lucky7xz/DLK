{"textgrid.poem.42709": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als j\u00fcngst in seinem Grimme sich", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst in seinem Grimme sich", "tokens": ["Als", "j\u00fcngst", "in", "sei\u00b7nem", "Grim\u00b7me", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eismond nahte, f\u00fchlt' ich mich", "tokens": ["Der", "Eis\u00b7mond", "nah\u00b7te", ",", "f\u00fchlt'", "ich", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Fieber \u00fcbermannt,", "tokens": ["Vom", "Fie\u00b7ber", "\u00fc\u00b7berm\u00b7annt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ach! bis auf das letzte Reis", "tokens": ["Und", "ach", "!", "bis", "auf", "das", "letz\u00b7te", "Reis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War all mein Holz, das ich durch Schweiss", "tokens": ["War", "all", "mein", "Holz", ",", "das", "ich", "durch", "Schweiss"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Flehn erwarb, verbrannt.", "tokens": ["Und", "Flehn", "er\u00b7warb", ",", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit siechem K\u00f6rper lag ich da", "tokens": ["Mit", "sie\u00b7chem", "K\u00f6r\u00b7per", "lag", "ich", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf halbvermorschtem Stroh, und sah", "tokens": ["Auf", "halb\u00b7ver\u00b7morschtem", "Stroh", ",", "und", "sah"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mit wehmuthsvollem Sinn,", "tokens": ["Mit", "weh\u00b7muths\u00b7vol\u00b7lem", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vergessen, hilflos, ohne Trost,", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "hil\u00b7flos", ",", "oh\u00b7ne", "Trost", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf meine Kinder, die vor Frost", "tokens": ["Auf", "mei\u00b7ne", "Kin\u00b7der", ",", "die", "vor", "Frost"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Hunger heulten, hin.", "tokens": ["Und", "Hun\u00b7ger", "heul\u00b7ten", ",", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So manchen sah mein banger Blick", "tokens": ["So", "man\u00b7chen", "sah", "mein", "ban\u00b7ger", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor\u00fcbereilen, dem das Gl\u00fcck", "tokens": ["Vor\u00b7\u00fc\u00b7be\u00b7rei\u00b7len", ",", "dem", "das", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr, als erbrauchte, gab.", "tokens": ["Mehr", ",", "als", "er\u00b7brauch\u00b7te", ",", "gab", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch niemand, niemand dachte mein:", "tokens": ["Doch", "nie\u00b7mand", ",", "nie\u00b7mand", "dach\u00b7te", "mein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verlassen war mein K\u00e4mmerlein,", "tokens": ["Ver\u00b7las\u00b7sen", "war", "mein", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie eines Fremdlings Grab.", "tokens": ["Wie", "ei\u00b7nes", "Fremd\u00b7lings", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie f\u00fchllos, rief ich \u00e4chzend aus,", "tokens": ["Wie", "f\u00fchl\u00b7los", ",", "rief", "ich", "\u00e4ch\u00b7zend", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4hrt oft der Gl\u00fcckssohn hin zum Schmaus!", "tokens": ["F\u00e4hrt", "oft", "der", "Gl\u00fccks\u00b7sohn", "hin", "zum", "Schmaus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! mit dem halben Werth", "tokens": ["Ach", "!", "mit", "dem", "hal\u00b7ben", "Werth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Des Gastgebotes h\u00e4tten wir,", "tokens": ["Des", "Gast\u00b7ge\u00b7bo\u00b7tes", "h\u00e4t\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich und die armen Kleinen hier,", "tokens": ["Ich", "und", "die", "ar\u00b7men", "Klei\u00b7nen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns mondenlang gen\u00e4hrt.", "tokens": ["Uns", "mon\u00b7den\u00b7lang", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hart ist das Loos der D\u00fcrftigkeit:", "tokens": ["Hart", "ist", "das", "Loos", "der", "D\u00fcrf\u00b7tig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Gl\u00fcck und Ungl\u00fcck, Freud' und Leid", "tokens": ["Doch", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", ",", "Freud'", "und", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind Gottes F\u00fcgung bloss.", "tokens": ["Sind", "Got\u00b7tes", "F\u00fc\u00b7gung", "bloss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Geduldig ehr' ich sein Gebot:", "tokens": ["Ge\u00b7dul\u00b7dig", "ehr'", "ich", "sein", "Ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur die Gespielen meiner Noth ...", "tokens": ["Nur", "die", "Ge\u00b7spie\u00b7len", "mei\u00b7ner", "Noth", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ach! w\u00e4r' ich kinderlos!", "tokens": ["Ach", "!", "w\u00e4r'", "ich", "kin\u00b7der\u00b7los", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So seufzt' ich trostlos, und schon war", "tokens": ["So", "seufzt'", "ich", "trost\u00b7los", ",", "und", "schon", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Blut der kleinen nackten Schaar,", "tokens": ["Das", "Blut", "der", "klei\u00b7nen", "nack\u00b7ten", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die zitternd mich umkroch,", "tokens": ["Die", "zit\u00b7ternd", "mich", "um\u00b7kroch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und W\u00e4rme suchte, halb erstarrt,", "tokens": ["Und", "W\u00e4r\u00b7me", "such\u00b7te", ",", "halb", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, Gott im Himmel! immer ward", "tokens": ["Und", ",", "Gott", "im", "Him\u00b7mel", "!", "im\u00b7mer", "ward"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "APPRART", "NN", "$.", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die K\u00e4lte strenger noch.", "tokens": ["Die", "K\u00e4l\u00b7te", "stren\u00b7ger", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch nun erbarmte meines Flehns", "tokens": ["Doch", "nun", "er\u00b7barm\u00b7te", "mei\u00b7nes", "Flehns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vorsicht Huld sich: unversehns", "tokens": ["Der", "Vor\u00b7sicht", "Huld", "sich", ":", "un\u00b7ver\u00b7sehns"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "PRF", "$.", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschien ein Retter, sprach", "tokens": ["Er\u00b7schien", "ein", "Ret\u00b7ter", ",", "sprach"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir Tr\u00f6stung zu, und sieh! es schwand,", "tokens": ["Mir", "Tr\u00f6s\u00b7tung", "zu", ",", "und", "sieh", "!", "es", "schwand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "KON", "VVIMP", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verscheucht von seiner milden Hand,", "tokens": ["Ver\u00b7scheucht", "von", "sei\u00b7ner", "mil\u00b7den", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Mangels Ungemach.", "tokens": ["Des", "Man\u00b7gels", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dem frommen Tugendfreunde gleich,", "tokens": ["Dem", "from\u00b7men", "Tu\u00b7gend\u00b7freun\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem pl\u00f6tzlich aus dem Geisterreich", "tokens": ["Dem", "pl\u00f6tz\u00b7lich", "aus", "dem", "Geis\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Seliger erscheint,", "tokens": ["Ein", "Se\u00b7li\u00b7ger", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Blickt' ich mit Thr\u00e4nen himmelw\u00e4rts,", "tokens": ["Blickt'", "ich", "mit", "Thr\u00e4\u00b7nen", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und pries mit stummem Dank dein Herz,", "tokens": ["Und", "pries", "mit", "stum\u00b7mem", "Dank", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erhabner Menschenfreund!", "tokens": ["Er\u00b7hab\u00b7ner", "Men\u00b7schen\u00b7freund", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Denn du, o wahrhaft edler Mann,", "tokens": ["Denn", "du", ",", "o", "wahr\u00b7haft", "ed\u00b7ler", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, wenn er Hilfe bieten kann,", "tokens": ["Der", ",", "wenn", "er", "Hil\u00b7fe", "bie\u00b7ten", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich gl\u00fccklich f\u00fchlet, du,", "tokens": ["Sich", "gl\u00fcck\u00b7lich", "f\u00fch\u00b7let", ",", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Rang und Herzensadel ziert,", "tokens": ["Den", "Rang", "und", "Her\u00b7zen\u00b7sa\u00b7del", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sandtest, durch mein Leid ger\u00fchrt,", "tokens": ["Du", "sand\u00b7test", ",", "durch", "mein", "Leid", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir diesen Retter zu.", "tokens": ["Mir", "die\u00b7sen", "Ret\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Herr, der gute Thaten lohnt,", "tokens": ["Der", "Herr", ",", "der", "gu\u00b7te", "Tha\u00b7ten", "lohnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geb' allen Grossen unterm Mond", "tokens": ["Geb'", "al\u00b7len", "Gros\u00b7sen", "un\u00b7term", "Mond"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herz, wie deines ist!", "tokens": ["Ein", "Herz", ",", "wie", "dei\u00b7nes", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PDS", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie manche Z\u00e4hre fl\u00f6sse hier", "tokens": ["Wie", "man\u00b7che", "Z\u00e4h\u00b7re", "fl\u00f6s\u00b7se", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus wonnevoller Dankbegier,", "tokens": ["Aus", "won\u00b7ne\u00b7vol\u00b7ler", "Dank\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nun aus Kummer fliesst!", "tokens": ["Die", "nun", "aus", "Kum\u00b7mer", "fliesst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}