{"textgrid.poem.58860": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "79. Alles mit Jesus!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Jesus klebe stets von innen,", "tokens": ["An", "Je\u00b7sus", "kle\u00b7be", "stets", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ohn' Jesus mu\u00dft du nichts beginnen;", "tokens": ["Ohn'", "Je\u00b7sus", "mu\u00dft", "du", "nichts", "be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es wird nichts Gutes je geschafft,", "tokens": ["Es", "wird", "nichts", "Gu\u00b7tes", "je", "ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn' Jesu Geist und Lebenskraft.", "tokens": ["Ohn'", "Je\u00b7su", "Geist", "und", "Le\u00b7bens\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}