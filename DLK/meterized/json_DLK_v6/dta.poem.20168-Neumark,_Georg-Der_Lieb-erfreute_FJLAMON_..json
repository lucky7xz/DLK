{"dta.poem.20168": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Unter den sch\u00f6nen rohten Apffel-Wangen", "tokens": ["Un\u00b7ter", "den", "sch\u00f6\u00b7nen", "roh\u00b7ten", "Apf\u00b7fel\u00b7Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Offte nicht hoffend wird ein Wurm gefangen/", "tokens": ["Off\u00b7te", "nicht", "hof\u00b7fend", "wird", "ein", "Wurm", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVPP", "VAFIN", "ART", "NN", "PTKVZ", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Also die Menschen/ die man meynt voll Treuen/", "tokens": ["Al\u00b7so", "die", "Men\u00b7schen", "/", "die", "man", "meynt", "voll", "Treu\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "PRELS", "PIS", "VVFIN", "ADJD", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Denn wenn vielleicht der Mund an einem Orte", "tokens": ["Denn", "wenn", "viel\u00b7leicht", "der", "Mund", "an", "ei\u00b7nem", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Fast Zukker-s\u00fcsse stoft heraus die Worte/", "tokens": ["Fast", "Zuk\u00b7ker\u00b7s\u00fcs\u00b7se", "stoft", "he\u00b7raus", "die", "Wor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "L\u00e4sset das Hertze ihm f\u00e4lschlich doch gefallen", "tokens": ["L\u00e4s\u00b7set", "das", "Hert\u00b7ze", "ihm", "f\u00e4lschlich", "doch", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "VVFIN", "PPER", "ADJD", "ADV", "VVPP"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}