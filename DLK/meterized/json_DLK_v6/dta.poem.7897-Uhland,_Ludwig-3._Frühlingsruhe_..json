{"dta.poem.7897": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.  Fr\u00fchlingsruhe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "O legt mich nicht in\u2019s dunkle Grab,", "tokens": ["O", "legt", "mich", "nicht", "in's", "dunk\u00b7le", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht unter die gr\u00fcne Erd\u2019 hinab!", "tokens": ["Nicht", "un\u00b7ter", "die", "gr\u00fc\u00b7ne", "Erd'", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Soll ich begraben seyn,", "tokens": ["Soll", "ich", "be\u00b7gra\u00b7ben", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Lieg\u2019 ich in\u2019s tiefe Gras hinein.", "tokens": ["Lieg'", "ich", "in's", "tie\u00b7fe", "Gras", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Gras und Blumen lieg\u2019 ich gern,", "tokens": ["In", "Gras", "und", "Blu\u00b7men", "lieg'", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn eine Fl\u00f6te t\u00f6nt von fern,", "tokens": ["Wenn", "ei\u00b7ne", "Fl\u00f6\u00b7te", "t\u00f6nt", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn hoch obenhin", "tokens": ["Und", "wenn", "hoch", "o\u00b7ben\u00b7hin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die hellen Fr\u00fchlingswolken ziehn.", "tokens": ["Die", "hel\u00b7len", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}