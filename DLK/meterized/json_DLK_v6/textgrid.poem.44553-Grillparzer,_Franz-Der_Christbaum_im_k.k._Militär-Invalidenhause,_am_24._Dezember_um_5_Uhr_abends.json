{"textgrid.poem.44553": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Christbaum im k.k. Milit\u00e4r-Invalidenhause, am 24. Dezember um 5 Uhr abends", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist ein Jahr denn nun vorbeigegangen,", "tokens": ["So", "ist", "ein", "Jahr", "denn", "nun", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit uns der Christbaum eben hier vereint,", "tokens": ["Seit", "uns", "der", "Christ\u00b7baum", "e\u00b7ben", "hier", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und manches dachte still wohl mit Verlangen,", "tokens": ["Und", "man\u00b7ches", "dach\u00b7te", "still", "wohl", "mit", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob uns ein gleicher Tag auch n\u00e4chstes Jahr erscheint?", "tokens": ["Ob", "uns", "ein", "glei\u00b7cher", "Tag", "auch", "n\u00e4chs\u00b7tes", "Jahr", "er\u00b7scheint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Priester, der uns etwa gern erfreute,", "tokens": ["Der", "Pries\u00b7ter", ",", "der", "uns", "et\u00b7wa", "gern", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er ist nicht reich, das Beste denn gebricht,", "tokens": ["Er", "ist", "nicht", "reich", ",", "das", "Bes\u00b7te", "denn", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ob ihm beistehn wohlgesinnte Leute,", "tokens": ["Und", "ob", "ihm", "bei\u00b7stehn", "wohl\u00b7ge\u00b7sinn\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir hoffens wohl, allein wir wissens nicht.", "tokens": ["Wir", "hof\u00b7fens", "wohl", ",", "al\u00b7lein", "wir", "wis\u00b7sens", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und siehe da, zu gleichgeme\u00dfnen Zeiten", "tokens": ["Und", "sie\u00b7he", "da", ",", "zu", "gleich\u00b7ge\u00b7me\u00df\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er\u00f6ffnet sich von neuem auch der Saal:", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "sich", "von", "neu\u00b7em", "auch", "der", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr seht schon die Geschenke dort von weiten", "tokens": ["Ihr", "seht", "schon", "die", "Ge\u00b7schen\u00b7ke", "dort", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles gl\u00e4nzt in heller Lichter Strahl.", "tokens": ["Und", "al\u00b7les", "gl\u00e4nzt", "in", "hel\u00b7ler", "Lich\u00b7ter", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Kinder, so wie hier ists auch im Leben,", "tokens": ["Ihr", "Kin\u00b7der", ",", "so", "wie", "hier", "ists", "auch", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "KOKOM", "ADV", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das voll von M\u00fch und Sorgen aller Art,", "tokens": ["Das", "voll", "von", "M\u00fch", "und", "Sor\u00b7gen", "al\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "NE", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch jedem ist ein Christbaum auch gegeben,", "tokens": ["Doch", "je\u00b7dem", "ist", "ein", "Christ\u00b7baum", "auch", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er nur ruhig hofft und gl\u00e4ubig harrt.", "tokens": ["Wenn", "er", "nur", "ru\u00b7hig", "hofft", "und", "gl\u00e4u\u00b7big", "harrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vor allem aber z\u00e4hmt den Eigenwillen,", "tokens": ["Vor", "al\u00b7lem", "a\u00b7ber", "z\u00e4hmt", "den", "Ei\u00b7gen\u00b7wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn der die Gaben gibt und uns beschert,", "tokens": ["Denn", "der", "die", "Ga\u00b7ben", "gibt", "und", "uns", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er kennt das Gute und er sorgt im stillen", "tokens": ["Er", "kennt", "das", "Gu\u00b7te", "und", "er", "sorgt", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht nach dem Schein, er gibt nur nach dem Wert.", "tokens": ["Nicht", "nach", "dem", "Schein", ",", "er", "gibt", "nur", "nach", "dem", "Wert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der eine wollte Spielzeug und statt dessen", "tokens": ["Der", "ei\u00b7ne", "woll\u00b7te", "Spiel\u00b7zeug", "und", "statt", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "APPR", "PDS"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wird ihm ein Kleid, das vor der K\u00e4lte sch\u00fctzt;", "tokens": ["Wird", "ihm", "ein", "Kleid", ",", "das", "vor", "der", "K\u00e4l\u00b7te", "sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der andre m\u00f6chte Leckereien essen,", "tokens": ["Der", "and\u00b7re", "m\u00f6ch\u00b7te", "Le\u00b7cke\u00b7rei\u00b7en", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er findet Speise, die nur n\u00e4hrt und n\u00fctzt.", "tokens": ["Er", "fin\u00b7det", "Spei\u00b7se", ",", "die", "nur", "n\u00e4hrt", "und", "n\u00fctzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn er zur\u00fcckweist nun die fromme Gabe,", "tokens": ["Wenn", "er", "zu\u00b7r\u00fcck\u00b7weist", "nun", "die", "from\u00b7me", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df warten er oft l\u00e4nger als ein Jahr,", "tokens": ["Mu\u00df", "war\u00b7ten", "er", "oft", "l\u00e4n\u00b7ger", "als", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mancher ging als Bettler schon zu Grabe,", "tokens": ["Und", "man\u00b7cher", "ging", "als", "Bett\u00b7ler", "schon", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KOKOM", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil er nicht nahm, was ihm beschieden war.", "tokens": ["Weil", "er", "nicht", "nahm", ",", "was", "ihm", "be\u00b7schie\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Drum, Kinder, bleibt auch Kinder. Zwar bei weiten", "tokens": ["Drum", ",", "Kin\u00b7der", ",", "bleibt", "auch", "Kin\u00b7der", ".", "Zwar", "bei", "wei\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "ADV", "NN", "$.", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Einsicht etwa nicht und an Verstand,", "tokens": ["An", "Ein\u00b7sicht", "et\u00b7wa", "nicht", "und", "an", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKNEG", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn ihr sollt lernen, fort in jedem schreiten,", "tokens": ["Denn", "ihr", "sollt", "ler\u00b7nen", ",", "fort", "in", "je\u00b7dem", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,", "PTKVZ", "APPR", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Nutzen euch, den Eltern und dem Land.", "tokens": ["Zum", "Nut\u00b7zen", "euch", ",", "den", "El\u00b7tern", "und", "dem", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Allein im Herzen, in des Innern letzter Mitte,", "tokens": ["Al\u00b7lein", "im", "Her\u00b7zen", ",", "in", "des", "In\u00b7nern", "letz\u00b7ter", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleib euch ein Teil von dem, was jetzt ihr seid,", "tokens": ["Bleib", "euch", "ein", "Teil", "von", "dem", ",", "was", "jetzt", "ihr", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ART", "$,", "PRELS", "ADV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gehorsam, der nun z\u00fcgelt eure Schritte,", "tokens": ["Ge\u00b7hor\u00b7sam", ",", "der", "nun", "z\u00fc\u00b7gelt", "eu\u00b7re", "Schrit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein warmes F\u00fchlen und Gen\u00fcgsamkeit.", "tokens": ["Ein", "war\u00b7mes", "F\u00fch\u00b7len", "und", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Dann kommt der Tag, seid dessen nur nicht bange,", "tokens": ["Dann", "kommt", "der", "Tag", ",", "seid", "des\u00b7sen", "nur", "nicht", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VAFIN", "PDS", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der euch schon hier f\u00fcr alles schadlos h\u00e4lt,", "tokens": ["Der", "euch", "schon", "hier", "f\u00fcr", "al\u00b7les", "schad\u00b7los", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und z\u00f6gerte der Christbaum gar zu lange,", "tokens": ["Und", "z\u00f6\u00b7ger\u00b7te", "der", "Christ\u00b7baum", "gar", "zu", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKA", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Weihnacht dann in einer bessern Welt.", "tokens": ["Die", "Weih\u00b7nacht", "dann", "in", "ei\u00b7ner", "bes\u00b7sern", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}