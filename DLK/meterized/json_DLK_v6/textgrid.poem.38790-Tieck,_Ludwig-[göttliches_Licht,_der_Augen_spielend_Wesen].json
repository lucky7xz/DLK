{"textgrid.poem.38790": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[g\u00f6ttliches Licht, der Augen spielend Wesen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttliches Licht, der Augen spielend Wesen,", "tokens": ["G\u00f6tt\u00b7li\u00b7ches", "Licht", ",", "der", "Au\u00b7gen", "spie\u00b7lend", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nie hab' ich Blick, Gru\u00df, Augenku\u00df verstanden,", "tokens": ["Nie", "hab'", "ich", "Blick", ",", "Gru\u00df", ",", "Au\u00b7gen\u00b7ku\u00df", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drohende Bitten, blitzend Flehn, in Banden", "tokens": ["Dro\u00b7hen\u00b7de", "Bit\u00b7ten", ",", "blit\u00b7zend", "Flehn", ",", "in", "Ban\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVINF", "$,", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Des Lichtes war ich niemals noch gewesen.", "tokens": ["Des", "Lich\u00b7tes", "war", "ich", "nie\u00b7mals", "noch", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich kann von dieser Krankheit nicht genesen,", "tokens": ["Ich", "kann", "von", "die\u00b7ser", "Krank\u00b7heit", "nicht", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und will nicht k\u00f6nnt' ich, seelgen Zauberlanden", "tokens": ["Und", "will", "nicht", "k\u00f6nnt'", "ich", ",", "seel\u00b7gen", "Zau\u00b7berl\u00b7an\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "VMFIN", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Einheimisch nun, wo alle Zweifel schwanden,", "tokens": ["Ein\u00b7hei\u00b7misch", "nun", ",", "wo", "al\u00b7le", "Zwei\u00b7fel", "schwan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gelehrt in Sternenschrift und tiefbelesen.", "tokens": ["Ge\u00b7lehrt", "in", "Ster\u00b7nen\u00b7schrift", "und", "tief\u00b7be\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja Sterne sind sie, sie sind lichte Bronnen,", "tokens": ["Ja", "Ster\u00b7ne", "sind", "sie", ",", "sie", "sind", "lich\u00b7te", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VAFIN", "PPER", "$,", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blumen, ihr Sehn ist wie ein Liebesthauen,", "tokens": ["Blu\u00b7men", ",", "ihr", "Sehn", "ist", "wie", "ein", "Lie\u00b7best\u00b7hau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Bienen sind meine Blicke, die sie saugen,", "tokens": ["Bie\u00b7nen", "sind", "mei\u00b7ne", "Bli\u00b7cke", ",", "die", "sie", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Himmel ihr Antlitz, sie die ewgen Sonnen,", "tokens": ["Him\u00b7mel", "ihr", "Ant\u00b7litz", ",", "sie", "die", "ew\u00b7gen", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mein gl\u00e4nzend Schicksal, dem will ich vertrauen,", "tokens": ["Mein", "gl\u00e4n\u00b7zend", "Schick\u00b7sal", ",", "dem", "will", "ich", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O mehr als Alles, sie sind Alma's Augen.", "tokens": ["O", "mehr", "als", "Al\u00b7les", ",", "sie", "sind", "Al\u00b7ma's", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "KOKOM", "PIS", "$,", "PPER", "VAFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}