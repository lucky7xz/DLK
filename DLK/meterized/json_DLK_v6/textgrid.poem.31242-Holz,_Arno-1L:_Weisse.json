{"textgrid.poem.31242": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weisse ", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weisse ", "tokens": ["Weis\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "seine \u00c4uglein lustig glizzen/", "tokens": ["sei\u00b7ne", "\u00c4ug\u00b7lein", "lus\u00b7tig", "gliz\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "deine Br\u00fcste Flammen sprizzen!", "tokens": ["dei\u00b7ne", "Br\u00fcs\u00b7te", "Flam\u00b7men", "spriz\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Trunckner b\u00fcn ich al\u00df ", "tokens": ["Trunck\u00b7ner", "b\u00fcn", "ich", "al\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "keiner kan f\u00fcr euch bestehn/", "tokens": ["kei\u00b7ner", "kan", "f\u00fcr", "euch", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "J\u00fcden/ Heyden und s\u00e4lbst Christen", "tokens": ["J\u00fc\u00b7den", "/", "Hey\u00b7den", "und", "s\u00e4lbst", "Chris\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "NN", "KON", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wisst ihr durchau\u00df zu belisten!", "tokens": ["wisst", "ihr", "durch\u00b7au\u00df", "zu", "be\u00b7lis\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Heimlich zwikkts mich bi\u00df zum Zeh/", "tokens": ["Heim\u00b7lich", "zwikkts", "mich", "bi\u00df", "zum", "Zeh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "abens/ wenn ich ", "tokens": ["a\u00b7bens", "/", "wenn", "ich"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "doch ich h\u00fcte mich beym Naschen/", "tokens": ["doch", "ich", "h\u00fc\u00b7te", "mich", "beym", "Na\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "denn ich will nicht Windeln waschen.", "tokens": ["denn", "ich", "will", "nicht", "Win\u00b7deln", "wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Philurille/ spey mich an/", "tokens": ["Phi\u00b7lu\u00b7ril\u00b7le", "/", "spey", "mich", "an", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVIMP", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "niemahls werde ich dein Mann.", "tokens": ["nie\u00b7mahls", "wer\u00b7de", "ich", "dein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "For mein angenehmes Wesen", "tokens": ["For", "mein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hat mich Fillis itzt erlesen.", "tokens": ["hat", "mich", "Fil\u00b7lis", "itzt", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Al\u00df ich letzt bey Chloen stund/", "tokens": ["Al\u00df", "ich", "letzt", "bey", "Chloen", "stund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VVFIN", "$("], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "j\u00fckkte gleich-falls mir der Mund;", "tokens": ["j\u00fckk\u00b7te", "gleich\u00b7falls", "mir", "der", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ach/ ihr arg verlihbtes Hertzgen", "tokens": ["ach", "/", "ihr", "arg", "ver\u00b7lihb\u00b7tes", "Hertz\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["XY", "$(", "PPER", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "br\u00e4nnt schon wie ein R\u00e4ucher-Kertzgen!", "tokens": ["br\u00e4nnt", "schon", "wie", "ein", "R\u00e4u\u00b7cher\u00b7Kertz\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Halt mich nicht for keinen Schwan/", "tokens": ["Halt", "mich", "nicht", "for", "kei\u00b7nen", "Schwan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "denn ich b\u00fcn ein Wetter-Han.", "tokens": ["denn", "ich", "b\u00fcn", "ein", "Wet\u00b7ter\u00b7\u00b7Han", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hundret lihbe kleine Dinger", "tokens": ["Hund\u00b7ret", "lih\u00b7be", "klei\u00b7ne", "Din\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "l\u00e4kken sich nach mir die Finger.", "tokens": ["l\u00e4k\u00b7ken", "sich", "nach", "mir", "die", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Bey Moscat und Malvasir", "tokens": ["Bey", "Mos\u00b7cat", "und", "Mal\u00b7va\u00b7sir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "pfeiffe ich auff Bitter-Bier.", "tokens": ["pfeif\u00b7fe", "ich", "auff", "Bit\u00b7ter\u00b7Bier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mandel-D\u00fctgens find ich n\u00fczze/", "tokens": ["Man\u00b7del\u00b7D\u00fct\u00b7gens", "find", "ich", "n\u00fcz\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "doch mir graut f\u00fcr Hafer-Gr\u00fczze!", "tokens": ["doch", "mir", "graut", "f\u00fcr", "Ha\u00b7fer\u00b7Gr\u00fcz\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}