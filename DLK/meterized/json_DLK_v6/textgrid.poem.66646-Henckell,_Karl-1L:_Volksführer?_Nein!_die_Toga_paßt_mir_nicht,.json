{"textgrid.poem.66646": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Volksf\u00fchrer? Nein! die Toga pa\u00dft mir nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Volksf\u00fchrer? Nein! die Toga pa\u00dft mir nicht,", "tokens": ["Volks\u00b7f\u00fch\u00b7rer", "?", "Nein", "!", "die", "To\u00b7ga", "pa\u00dft", "mir", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um auf dem Markte Politik zu treiben.", "tokens": ["Um", "auf", "dem", "Mark\u00b7te", "Po\u00b7li\u00b7tik", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bilde mich und bilde mein Gedicht,", "tokens": ["Ich", "bil\u00b7de", "mich", "und", "bil\u00b7de", "mein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was meinem Wesen fern liegt, lass' ich bleiben.", "tokens": ["Was", "mei\u00b7nem", "We\u00b7sen", "fern", "liegt", ",", "lass'", "ich", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Aus Mitgef\u00fchl sing' ich mein Lied der Not,", "tokens": ["Aus", "Mit\u00b7ge\u00b7f\u00fchl", "sing'", "ich", "mein", "Lied", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Menschheitslied aus H\u00f6hentrieb der Seele,", "tokens": ["Mein", "Menschheits\u00b7lied", "aus", "H\u00f6\u00b7hen\u00b7trieb", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Doch dem Parteigetriebe bin ich tot \u2013", "tokens": ["Doch", "dem", "Par\u00b7tei\u00b7ge\u00b7trie\u00b7be", "bin", "ich", "tot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nun h\u00e4ngt mich auf \u2013 empfehle mich, empfehle!", "tokens": ["Nun", "h\u00e4ngt", "mich", "auf", "\u2013", "emp\u00b7feh\u00b7le", "mich", ",", "emp\u00b7feh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "$(", "VVFIN", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die hei\u00dfen Geister der Gerechtigkeit", "tokens": ["Die", "hei\u00b7\u00dfen", "Geis\u00b7ter", "der", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verlockten mich, mit Kn\u00fctteln dreinzuschlagen,", "tokens": ["Ver\u00b7lock\u00b7ten", "mich", ",", "mit", "Kn\u00fct\u00b7teln", "drein\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch tut es fast mir um das Pathos leid,", "tokens": ["Doch", "tut", "es", "fast", "mir", "um", "das", "Pa\u00b7thos", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wehm\u00fctig lernt' ich solchem Strau\u00df entsagen.", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "lernt'", "ich", "sol\u00b7chem", "Strau\u00df", "ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich mag nicht mehr, aus innerster Natur,", "tokens": ["Ich", "mag", "nicht", "mehr", ",", "aus", "in\u00b7ners\u00b7ter", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und eins mit mir darf ich getrost gestehen:", "tokens": ["Und", "eins", "mit", "mir", "darf", "ich", "ge\u00b7trost", "ge\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VMFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich werde fortan nur auf einer Spur,", "tokens": ["Ich", "wer\u00b7de", "for\u00b7tan", "nur", "auf", "ei\u00b7ner", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf eigner Spur des reinern Daseins gehen.", "tokens": ["Auf", "eig\u00b7ner", "Spur", "des", "rei\u00b7nern", "Da\u00b7seins", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Ehrgeiz jagt mich auf das Podium,", "tokens": ["Kein", "Ehr\u00b7geiz", "jagt", "mich", "auf", "das", "Po\u00b7di\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Agitator geht an mir verloren,", "tokens": ["Kein", "A\u00b7git\u00b7a\u00b7tor", "geht", "an", "mir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Eifersucht des Siegers bin ich stumm,", "tokens": ["Der", "Ei\u00b7fer\u00b7sucht", "des", "Sie\u00b7gers", "bin", "ich", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fcr das Bravo hab' ich schlechte Ohren;", "tokens": ["Und", "f\u00fcr", "das", "Bra\u00b7vo", "hab'", "ich", "schlech\u00b7te", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das heute dem und morgen jenem schallt,", "tokens": ["Das", "heu\u00b7te", "dem", "und", "mor\u00b7gen", "je\u00b7nem", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "KON", "ADV", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ja augenblicklich treulos sich verwandelt,", "tokens": ["Ja", "au\u00b7gen\u00b7blick\u00b7lich", "treu\u00b7los", "sich", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das eben noch Emp\u00f6rerf\u00e4uste ballt", "tokens": ["Das", "e\u00b7ben", "noch", "Em\u00b7p\u00f6\u00b7rer\u00b7f\u00e4us\u00b7te", "ballt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und gleich darauf mit Schw\u00e4tzern schon verhandelt.", "tokens": ["Und", "gleich", "da\u00b7rauf", "mit", "Schw\u00e4t\u00b7zern", "schon", "ver\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hinweg, ihr Stelzen der Verg\u00e4nglichkeit,", "tokens": ["Hin\u00b7weg", ",", "ihr", "Stel\u00b7zen", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der \u00dcberredung aufgeblasne Robe!", "tokens": ["Der", "\u00dc\u00b7ber\u00b7re\u00b7dung", "auf\u00b7ge\u00b7blas\u00b7ne", "Ro\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man wird so klein, wenn man sich t\u00e4glich weiht", "tokens": ["Man", "wird", "so", "klein", ",", "wenn", "man", "sich", "t\u00e4g\u00b7lich", "weiht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PIS", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Massenkult der menschlichen Mikrobe.", "tokens": ["Dem", "Mas\u00b7sen\u00b7kult", "der", "menschli\u00b7chen", "Mik\u00b7ro\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zu eitel d\u00fcnkt mich dieses Priesterkleid,", "tokens": ["Zu", "ei\u00b7tel", "d\u00fcnkt", "mich", "die\u00b7ses", "Pries\u00b7ter\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Weshalb ich mir den Rock des Weltmanns lobe", "tokens": ["We\u00b7shalb", "ich", "mir", "den", "Rock", "des", "Welt\u00b7manns", "lo\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und dem Augurendienst der Menschheit fremd", "tokens": ["Und", "dem", "Au\u00b7gu\u00b7ren\u00b7dienst", "der", "Menschheit", "fremd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADJD"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein L\u00e4cheln spare, das mein Herz beklemmt.", "tokens": ["Ein", "L\u00e4\u00b7cheln", "spa\u00b7re", ",", "das", "mein", "Herz", "be\u00b7klemmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Versteht mich wohl! Der Menschheit gro\u00dfen Zug", "tokens": ["Ver\u00b7steht", "mich", "wohl", "!", "Der", "Menschheit", "gro\u00b7\u00dfen", "Zug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Werd' ich mit Sinnen nachzuziehen suchen,", "tokens": ["Werd'", "ich", "mit", "Sin\u00b7nen", "nach\u00b7zu\u00b7zie\u00b7hen", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ihren sonnenk\u00fchnen Adlerflug", "tokens": ["Denn", "ih\u00b7ren", "son\u00b7nen\u00b7k\u00fch\u00b7nen", "Ad\u00b7ler\u00b7flug"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verleugnen nur \u00e4sthetische Eunuchen.", "tokens": ["Ver\u00b7leug\u00b7nen", "nur", "\u00e4s\u00b7the\u00b7ti\u00b7sche", "Eu\u00b7nu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es steigt empor das menschliche Geschlecht", "tokens": ["Es", "steigt", "em\u00b7por", "das", "menschli\u00b7che", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Bed\u00e4chtigen Schritts die wunderbaren Stufen,", "tokens": ["Be\u00b7d\u00e4ch\u00b7ti\u00b7gen", "Schritts", "die", "wun\u00b7der\u00b7ba\u00b7ren", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und auch der rohe Bruch von Herr und Knecht", "tokens": ["Und", "auch", "der", "ro\u00b7he", "Bruch", "von", "Herr", "und", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wird einem feinern Formverh\u00e4ltnis rufen.", "tokens": ["Wird", "ei\u00b7nem", "fei\u00b7nern", "Form\u00b7ver\u00b7h\u00e4lt\u00b7nis", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo sich des Denkers reiner Eifer m\u00fcht,", "tokens": ["Wo", "sich", "des", "Den\u00b7kers", "rei\u00b7ner", "Ei\u00b7fer", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Forscherlust lebendige Schl\u00fcsse gattet,", "tokens": ["Wo", "For\u00b7scher\u00b7lust", "le\u00b7ben\u00b7di\u00b7ge", "Schl\u00fcs\u00b7se", "gat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo der verborgne Baum der Weisheit bl\u00fcht,", "tokens": ["Wo", "der", "ver\u00b7borg\u00b7ne", "Baum", "der", "Weis\u00b7heit", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dort birg dein Lied, von Einsamkeit beschattet!", "tokens": ["Dort", "birg", "dein", "Lied", ",", "von", "Ein\u00b7sam\u00b7keit", "be\u00b7schat\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Der augenbeizenden Gew\u00f6hnlichkeiten,", "tokens": ["Der", "au\u00b7gen\u00b7bei\u00b7zen\u00b7den", "Ge\u00b7w\u00f6hn\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was ganz dein eigen, tr\u00e4nke dein Gedicht,", "tokens": ["Was", "ganz", "dein", "ei\u00b7gen", ",", "tr\u00e4n\u00b7ke", "dein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du leite dich, la\u00df sich die andern leiten!", "tokens": ["Du", "lei\u00b7te", "dich", ",", "la\u00df", "sich", "die", "an\u00b7dern", "lei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVIMP", "PRF", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nur frei sein, frei, auch von der \u00bbFreiheit\u00ab frei,", "tokens": ["Nur", "frei", "sein", ",", "frei", ",", "auch", "von", "der", "\u00bb", "Frei\u00b7heit", "\u00ab", "frei", ","], "token_info": ["word", 