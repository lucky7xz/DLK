{"dta.poem.9330": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "\u00fcber Wittenberg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist dis f\u00fcr ein Sitz? Was ist dis f\u00fcr ein ort?", "tokens": ["Was", "ist", "dis", "f\u00fcr", "ein", "Sitz", "?", "Was", "ist", "dis", "f\u00fcr", "ein", "ort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$.", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist hier nicht Tullius? nein. Buchner der Beredte/", "tokens": ["Ist", "hier", "nicht", "Tul\u00b7li\u00b7us", "?", "nein", ".", "Buch\u00b7ner", "der", "Be\u00b7red\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "NN", "$.", "PTKANT", "$.", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den mann zu r\u00fchmen weis/ wo Titan geht zu bette/", "tokens": ["den", "mann", "zu", "r\u00fch\u00b7men", "weis", "/", "wo", "Ti\u00b7tan", "geht", "zu", "bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$(", "PWAV", "NE", "VVFIN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und wider auffersteht: Was h\u00f6rt man f\u00fcr ein wort?", "tokens": ["und", "wi\u00b7der", "auf\u00b7fer\u00b7steht", ":", "Was", "h\u00f6rt", "man", "f\u00fcr", "ein", "wort", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "$.", "PWS", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der G\u00f6tter Ausspruch? nein. Hier ist derselbe port/", "tokens": ["Der", "G\u00f6t\u00b7ter", "Aus\u00b7spruch", "?", "nein", ".", "Hier", "ist", "der\u00b7sel\u00b7be", "port", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PTKANT", "$.", "ADV", "VAFIN", "PDAT", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wo Luther erst mahls harr gestritten \u00fcmb die weite", "tokens": ["wo", "Lu\u00b7ther", "erst", "mahls", "harr", "ge\u00b7strit\u00b7ten", "\u00fcmb", "die", "wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "ADV", "ADJD", "VVPP", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und ritterlich zerbrach des Pabstes starcke kette/", "tokens": ["und", "rit\u00b7ter\u00b7lich", "zer\u00b7brach", "des", "Pabs\u00b7tes", "star\u00b7cke", "ket\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der uns gefangen hielt: Der Ablas muste fort;", "tokens": ["der", "uns", "ge\u00b7fan\u00b7gen", "hielt", ":", "Der", "Ab\u00b7las", "mus\u00b7te", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das wort/ das edle wort quoll lauter rein und klar/", "tokens": ["Das", "wort", "/", "das", "ed\u00b7le", "wort", "quoll", "lau\u00b7ter", "rein", "und", "klar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hier hier erwehlte Gott sein Feuer und Altar;", "tokens": ["hier", "hier", "er\u00b7wehl\u00b7te", "Gott", "sein", "Feu\u00b7er", "und", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun quillt noch weiter fort die himmelische Lehre;", "tokens": ["Nun", "quillt", "noch", "wei\u00b7ter", "fort", "die", "him\u00b7me\u00b7li\u00b7sche", "Leh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hier lehrt der Alten F\u00fcrst der gro\u00dfe Mann Martin/", "tokens": ["Hier", "lehrt", "der", "Al\u00b7ten", "F\u00fcrst", "der", "gro\u00b7\u00dfe", "Mann", "Mar\u00b7tin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Herr R\u00f6ber/ H\u00fclseman/ Scharff/ Sperling viel", "tokens": ["Herr", "R\u00f6\u00b7ber", "/", "H\u00fcl\u00b7se\u00b7man", "/", "Scharff", "/", "Sper\u00b7ling", "viel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$(", "NN", "$(", "NE", "$(", "NE", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erzihn/", "tokens": ["er\u00b7zihn", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "So dann auch werden seyn der Wittenberger Ehre.", "tokens": ["So", "dann", "auch", "wer\u00b7den", "seyn", "der", "Wit\u00b7ten\u00b7ber\u00b7ger", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPOSAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}