{"textgrid.poem.24770": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Reiselied", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwasser st\u00fcrzt, uns zu verschlingen,", "tokens": ["\u00bb", "was\u00b7ser", "st\u00fcrzt", ",", "uns", "zu", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rollt der Fels, uns zu erschlagen,", "tokens": ["Rollt", "der", "Fels", ",", "uns", "zu", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommen schon auf starken Schwingen", "tokens": ["Kom\u00b7men", "schon", "auf", "star\u00b7ken", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "V\u00f6gel her, uns fortzutragen.", "tokens": ["V\u00f6\u00b7gel", "her", ",", "uns", "fort\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber unten liegt ein Land,", "tokens": ["A\u00b7ber", "un\u00b7ten", "liegt", "ein", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fcchte spiegelnd ohne Ende", "tokens": ["Fr\u00fcch\u00b7te", "spie\u00b7gelnd", "oh\u00b7ne", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den alterslosen Seen.", "tokens": ["In", "den", "al\u00b7ters\u00b7lo\u00b7sen", "Seen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Marmorstirn und Brunnenrand", "tokens": ["Mar\u00b7mors\u00b7tirn", "und", "Brun\u00b7nen\u00b7rand"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt aus blumigem Gel\u00e4nde,", "tokens": ["Steigt", "aus", "blu\u00b7mi\u00b7gem", "Ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die leichten Winde wehn.", "tokens": ["Und", "die", "leich\u00b7ten", "Win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}