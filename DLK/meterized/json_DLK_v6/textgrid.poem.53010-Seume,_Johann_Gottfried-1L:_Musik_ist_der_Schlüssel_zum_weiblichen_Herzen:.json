{"textgrid.poem.53010": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Musik ist der Schl\u00fcssel zum weiblichen Herzen:", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Musik ist der Schl\u00fcssel zum weiblichen Herzen:", "tokens": ["Mu\u00b7sik", "ist", "der", "Schl\u00fcs\u00b7sel", "zum", "weib\u00b7li\u00b7chen", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schleicht sich melodisch mit Kosen und Scherzen", "tokens": ["Da", "schleicht", "sich", "me\u00b7lo\u00b7disch", "mit", "Ko\u00b7sen", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Freund Amor unmerkbar mit leiser Magie", "tokens": ["Freund", "A\u00b7mor", "un\u00b7merk\u00b7bar", "mit", "lei\u00b7ser", "Ma\u00b7gie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "In Seelen, als w\u00e4r' es zur Urharmonie.", "tokens": ["In", "See\u00b7len", ",", "als", "w\u00e4r'", "es", "zur", "Ur\u00b7har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOKOM", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Weisheit der Weisen, nur kalt und besonnen,", "tokens": ["Die", "Weis\u00b7heit", "der", "Wei\u00b7sen", ",", "nur", "kalt", "und", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ist schnell mit der g\u00f6ttlichsten Lehre zerronnen:", "tokens": ["Ist", "schnell", "mit", "der", "g\u00f6tt\u00b7lichs\u00b7ten", "Leh\u00b7re", "zer\u00b7ron\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Thracier klaget im mystischen Hain,", "tokens": ["Der", "Thra\u00b7cier", "kla\u00b7get", "im", "mys\u00b7ti\u00b7schen", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alles wird Nachhall zum Geisterverein.", "tokens": ["Und", "al\u00b7les", "wird", "Nach\u00b7hall", "zum", "Geis\u00b7ter\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Der Zauberer spielet in Tonlabyrinthen,", "tokens": ["Der", "Zau\u00b7be\u00b7rer", "spie\u00b7let", "in", "Ton\u00b7la\u00b7by\u00b7rin\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie M\u00e4dchen im Lenze mit Blumengewinden,", "tokens": ["Wie", "M\u00e4d\u00b7chen", "im", "Len\u00b7ze", "mit", "Blu\u00b7men\u00b7ge\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit Herzen, und f\u00fchret in lieblichem Lauf", "tokens": ["Mit", "Her\u00b7zen", ",", "und", "f\u00fch\u00b7ret", "in", "lieb\u00b7li\u00b7chem", "Lauf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie unbedingt herrschend hinab und hinauf.", "tokens": ["Sie", "un\u00b7be\u00b7dingt", "herr\u00b7schend", "hin\u00b7ab", "und", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "J\u00fcngst sann und sann Mozart, der sch\u00f6pf'rische Meister,", "tokens": ["J\u00fcngst", "sann", "und", "sann", "Mo\u00b7zart", ",", "der", "sch\u00f6pf'\u00b7ri\u00b7sche", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Orpheus-Amphion der liebenden Geister,", "tokens": ["Der", "Or\u00b7pheus\u00b7Am\u00b7phi\u00b7on", "der", "lie\u00b7ben\u00b7den", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis seine gefl\u00fcgelte magische Hand", "tokens": ["Bis", "sei\u00b7ne", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "ma\u00b7gi\u00b7sche", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Zauber der Doppelsonaten erfand.", "tokens": ["Den", "Zau\u00b7ber", "der", "Dop\u00b7pel\u00b7so\u00b7na\u00b7ten", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Da wandeln in k\u00fcnstlich verschlungnem Gew\u00fchle", "tokens": ["Da", "wan\u00b7deln", "in", "k\u00fcnst\u00b7lich", "ver\u00b7schlung\u00b7nem", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Aus Seelen in Seelen verwandte Gef\u00fchle;", "tokens": ["Aus", "See\u00b7len", "in", "See\u00b7len", "ver\u00b7wand\u00b7te", "Ge\u00b7f\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da tr\u00e4ufeln die T\u00f6ne, gebunden und frey,", "tokens": ["Da", "tr\u00e4u\u00b7feln", "die", "T\u00f6\u00b7ne", ",", "ge\u00b7bun\u00b7den", "und", "frey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Erquickend und lieblich wie Regen im May.", "tokens": ["Er\u00b7qui\u00b7ckend", "und", "lieb\u00b7lich", "wie", "Re\u00b7gen", "im", "May", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Da ruhet und bebet und sinket und steiget", "tokens": ["Da", "ru\u00b7het", "und", "be\u00b7bet", "und", "sin\u00b7ket", "und", "stei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Seele, bis sanft sie dem Rausche sich neiget,", "tokens": ["Die", "See\u00b7le", ",", "bis", "sanft", "sie", "dem", "Rau\u00b7sche", "sich", "nei\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADJD", "PPER", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und erdevergessend das Auge bewegt,", "tokens": ["Und", "er\u00b7de\u00b7ver\u00b7ges\u00b7send", "das", "Au\u00b7ge", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Her\u00fcber hin\u00fcber den Himmel sich tr\u00e4gt.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "hin\u00b7\u00fc\u00b7ber", "den", "Him\u00b7mel", "sich", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Oft schlie\u00dfet nach Paphiens heimlichem Rathe", "tokens": ["Oft", "schlie\u00b7\u00dfet", "nach", "Pa\u00b7phi\u00b7ens", "heim\u00b7li\u00b7chem", "Ra\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann Seelenentz\u00fcckung die Doppelsonate,", "tokens": ["Dann", "See\u00b7le\u00b7nent\u00b7z\u00fc\u00b7ckung", "die", "Dop\u00b7pel\u00b7so\u00b7na\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wo man mit den G\u00f6ttern vermessen sich mi\u00dft,", "tokens": ["Wo", "man", "mit", "den", "G\u00f6t\u00b7tern", "ver\u00b7mes\u00b7sen", "sich", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und Himmel und Erd' und sich selber vergi\u00dft.", "tokens": ["Und", "Him\u00b7mel", "und", "Erd'", "und", "sich", "sel\u00b7ber", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "PRF", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Dankt Mozart, ihr Sch\u00fcler, dem sch\u00f6pf'rischen Meister,", "tokens": ["Dankt", "Mo\u00b7zart", ",", "ihr", "Sch\u00fc\u00b7ler", ",", "dem", "sch\u00f6pf'\u00b7ri\u00b7schen", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Orpheus-Amphion der lieben Geister,", "tokens": ["Dem", "Or\u00b7pheus\u00b7Am\u00b7phi\u00b7on", "der", "lie\u00b7ben", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm, dessen gefl\u00fcgelte Hand", "tokens": ["Ihm", ",", "des\u00b7sen", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Hand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Den un\u00fcberwindlichen Zauber erfand.", "tokens": ["Den", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7li\u00b7chen", "Zau\u00b7ber", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Und h\u00f6ret, ihr unmusikalischen Seelen,", "tokens": ["Und", "h\u00f6\u00b7ret", ",", "ihr", "un\u00b7mu\u00b7si\u00b7ka\u00b7li\u00b7schen", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "H\u00f6rt auf, euch mit Qualen der Liebe zu qu\u00e4len:", "tokens": ["H\u00f6rt", "auf", ",", "euch", "mit", "Qua\u00b7len", "der", "Lie\u00b7be", "zu", "qu\u00e4\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die G\u00f6tterbegl\u00fcckung in uns'rer Natur", "tokens": ["Die", "G\u00f6t\u00b7ter\u00b7be\u00b7gl\u00fc\u00b7ckung", "in", "un\u00b7s'\u00b7rer", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Geh\u00f6ret den g\u00f6ttlichen Lieblingen nur.", "tokens": ["Ge\u00b7h\u00f6\u00b7ret", "den", "g\u00f6tt\u00b7li\u00b7chen", "Lieb\u00b7lin\u00b7gen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}}}}}