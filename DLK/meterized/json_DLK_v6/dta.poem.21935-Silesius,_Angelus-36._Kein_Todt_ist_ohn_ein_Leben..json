{"dta.poem.21935": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "36. Kein Todt ist ohn ein Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sag es stirbet nichts: nur da\u00df ein ander Leben/", "tokens": ["Ich", "sag", "es", "stir\u00b7bet", "nichts", ":", "nur", "da\u00df", "ein", "an\u00b7der", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PIS", "$.", "ADV", "KOUS", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch selbst da\u00df Peinliche/ wird durch den Tod gegebe\u0303.", "tokens": ["Auch", "selbst", "da\u00df", "Pein\u00b7li\u00b7che", "/", "wird", "durch", "den", "Tod", "ge\u00b7ge\u00b7b\u1ebd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "$(", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}