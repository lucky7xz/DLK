{"textgrid.poem.65842": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "116", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich von keinen Hindernissen h\u00f6ren,", "tokens": ["La\u00df", "mich", "von", "kei\u00b7nen", "Hin\u00b7der\u00b7nis\u00b7sen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die treuer Seelen Ehebund bedr\u00e4un!", "tokens": ["Die", "treu\u00b7er", "See\u00b7len", "E\u00b7he\u00b7bund", "be\u00b7dr\u00e4un", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lieb' ist nicht Liebe, wenn sie St\u00f6rer st\u00f6ren,", "tokens": ["Lieb'", "ist", "nicht", "Lie\u00b7be", ",", "wenn", "sie", "St\u00f6\u00b7rer", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Wenn sie Zerstreuung irrend kann zerstreun.", "tokens": ["Wenn", "sie", "Zer\u00b7streu\u00b7ung", "ir\u00b7rend", "kann", "zer\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O nein! sie ist ein ewig sichres Ziel,", "tokens": ["O", "nein", "!", "sie", "ist", "ein", "e\u00b7wig", "sich\u00b7res", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Thront unersch\u00fcttert \u00fcber Sturmeswogen;", "tokens": ["Thront", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "\u00fc\u00b7ber", "Stur\u00b7mes\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Angelstern f\u00fcr jeden irren Kiel;", "tokens": ["Ein", "An\u00b7gels\u00b7tern", "f\u00fcr", "je\u00b7den", "ir\u00b7ren", "Kiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Kein H\u00f6henma\u00df hat seinen Wert erwogen.", "tokens": ["Kein", "H\u00f6\u00b7hen\u00b7ma\u00df", "hat", "sei\u00b7nen", "Wert", "er\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Lieb' ist kein Narr der Zeit, ob Rosenwangen", "tokens": ["Lieb'", "ist", "kein", "Narr", "der", "Zeit", ",", "ob", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "NN", "ART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auch ihrer Sichel krumme Schneid' umspannt:", "tokens": ["Auch", "ih\u00b7rer", "Si\u00b7chel", "krum\u00b7me", "Schneid'", "um\u00b7spannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In enger Stunden Lauf uneingefangen", "tokens": ["In", "en\u00b7ger", "Stun\u00b7den", "Lauf", "un\u00b7ein\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Beharrt sie bis an Weltgerichtes Rand.", "tokens": ["Be\u00b7harrt", "sie", "bis", "an", "Welt\u00b7ge\u00b7rich\u00b7tes", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wenn dies als Wahn, als L\u00fcge sich ergibt,", "tokens": ["Wenn", "dies", "als", "Wahn", ",", "als", "L\u00fc\u00b7ge", "sich", "er\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "KOUS", "NN", "$,", "KOUS", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So schrieb ich nie, so hat kein Mensch geliebt.", "tokens": ["So", "schrieb", "ich", "nie", ",", "so", "hat", "kein", "Mensch", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}