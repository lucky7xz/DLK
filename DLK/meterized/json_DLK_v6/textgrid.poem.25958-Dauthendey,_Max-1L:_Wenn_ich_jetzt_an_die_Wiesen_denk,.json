{"textgrid.poem.25958": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich jetzt an die Wiesen denk,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich jetzt an die Wiesen denk,", "tokens": ["Wenn", "ich", "jetzt", "an", "die", "Wie\u00b7sen", "denk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scheint jede Feldblum ein Geschenk.", "tokens": ["Scheint", "je\u00b7de", "Feld\u00b7blum", "ein", "Ge\u00b7schenk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blumenstr\u00e4u\u00dfe in dem Gras", "tokens": ["Die", "Blu\u00b7men\u00b7str\u00e4u\u00b7\u00dfe", "in", "dem", "Gras"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind nicht blo\u00df f\u00fcr die K\u00fche Fra\u00df,", "tokens": ["Sind", "nicht", "blo\u00df", "f\u00fcr", "die", "K\u00fc\u00b7he", "Fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Sind nicht nur Dung f\u00fcr alte Erde;", "tokens": ["Sind", "nicht", "nur", "Dung", "f\u00fcr", "al\u00b7te", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Blumen lachende Geb\u00e4rde", "tokens": ["Der", "Blu\u00b7men", "la\u00b7chen\u00b7de", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Steht frisch an jedem Morgen da,", "tokens": ["Steht", "frisch", "an", "je\u00b7dem", "Mor\u00b7gen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo fromm-verliebt mein Schatz hinsah.", "tokens": ["Wo", "from\u00b7mver\u00b7liebt", "mein", "Schatz", "hin\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}