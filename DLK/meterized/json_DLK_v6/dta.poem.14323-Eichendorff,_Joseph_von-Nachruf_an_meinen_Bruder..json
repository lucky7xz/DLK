{"dta.poem.14323": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehast doch keine Schwingen,", "tokens": ["\u201e", "hast", "doch", "kei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch Wolken zu dringen!", "tokens": ["Durch", "Wol\u00b7ken", "zu", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mu\u00dft immerfort schauen", "tokens": ["Mu\u00dft", "im\u00b7mer\u00b7fort", "schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Str\u00f6me, die Auen \u2014", "tokens": ["Die", "Str\u00f6\u00b7me", ",", "die", "Au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die werden Dir singen", "tokens": ["Die", "wer\u00b7den", "Dir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von Ihm Tag und Nacht,", "tokens": ["Von", "Ihm", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mit Wahnsinnes-Macht", "tokens": ["Mit", "Wahn\u00b7sin\u00b7nes\u00b7Macht"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Seele umschlingen.\u201c", "tokens": ["Die", "See\u00b7le", "um\u00b7schlin\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}