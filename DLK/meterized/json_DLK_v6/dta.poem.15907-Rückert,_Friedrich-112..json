{"dta.poem.15907": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df er dich r\u00fchrt, gedeiht \u2014 es ist nur eine Probe", "tokens": ["Da\u00df", "er", "dich", "r\u00fchrt", ",", "ge\u00b7deiht", "es", "ist", "nur", "ei\u00b7ne", "Pro\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von deiner R\u00fchrbarkeit \u2014 dem Dichter nicht zum Lobe.", "tokens": ["Von", "dei\u00b7ner", "R\u00fchr\u00b7bar\u00b7keit", "dem", "Dich\u00b7ter", "nicht", "zum", "Lo\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Herz ist wie das Kraut, ist es dir nicht bekannt?", "tokens": ["Dein", "Herz", "ist", "wie", "das", "Kraut", ",", "ist", "es", "dir", "nicht", "be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Fliegenklappe wird gemeinhin zubenannt,", "tokens": ["Das", "Flie\u00b7gen\u00b7klap\u00b7pe", "wird", "ge\u00b7mein\u00b7hin", "zu\u00b7be\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Blatt so reizbar ist, da\u00df keine Flieg' es j\u00fcckt", "tokens": ["Des", "Blatt", "so", "reiz\u00b7bar", "ist", ",", "da\u00df", "kei\u00b7ne", "Flieg'", "es", "j\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Krabbeln, ohne da\u00df sichs also f\u00fchlt entz\u00fcckt,", "tokens": ["Mit", "Krab\u00b7beln", ",", "oh\u00b7ne", "da\u00df", "sichs", "al\u00b7so", "f\u00fchlt", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "KOUS", "PIS", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df es zusammenklappt und das Insekt ertappt;", "tokens": ["Da\u00df", "es", "zu\u00b7sam\u00b7men\u00b7klappt", "und", "das", "In\u00b7sekt", "er\u00b7tappt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh, welch' ein kribblig Ding hast du nun auch erschnappt!", "tokens": ["Sieh", ",", "welch'", "ein", "kribb\u00b7lig", "Ding", "hast", "du", "nun", "auch", "er\u00b7schnappt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}