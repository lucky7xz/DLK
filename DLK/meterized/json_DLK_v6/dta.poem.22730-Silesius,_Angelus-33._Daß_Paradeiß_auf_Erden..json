{"dta.poem.22730": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "33. Da\u00df Paradei\u00df auf Erden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Du suchst da\u00df Paradei\u00df/ und w\u00fcnschest hin zukommen/", "tokens": ["Du", "suchst", "da\u00df", "Pa\u00b7ra\u00b7dei\u00df", "/", "und", "w\u00fcn\u00b7schest", "hin", "zu\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$(", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du von allem Leid und Unfried bist entnommen.", "tokens": ["Wo", "du", "von", "al\u00b7lem", "Leid", "und", "Un\u00b7fried", "bist", "ent\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIS", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Befriedige dein Hertz/ und mach es Rein und wei\u00df:", "tokens": ["Be\u00b7frie\u00b7di\u00b7ge", "dein", "Hertz", "/", "und", "mach", "es", "Rein", "und", "wei\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bistu selbst noch hier dasselbe Paradei\u00df.", "tokens": ["So", "bis\u00b7tu", "selbst", "noch", "hier", "das\u00b7sel\u00b7be", "Pa\u00b7ra\u00b7dei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "wo