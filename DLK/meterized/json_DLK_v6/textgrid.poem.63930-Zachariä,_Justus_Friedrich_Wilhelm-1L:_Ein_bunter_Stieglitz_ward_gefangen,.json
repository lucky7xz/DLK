{"textgrid.poem.63930": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein bunter Stieglitz ward gefangen,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein bunter Stieglitz ward gefangen,", "tokens": ["Ein", "bun\u00b7ter", "Stieg\u00b7litz", "ward", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und einem Knaben auf Verlangen", "tokens": ["Und", "ei\u00b7nem", "Kna\u00b7ben", "auf", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu seinem Eigenthum geschenkt,", "tokens": ["Zu", "sei\u00b7nem", "Ei\u00b7gen\u00b7thum", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ganz entz\u00fcckt auf nichts mehr denkt,", "tokens": ["Der", "ganz", "ent\u00b7z\u00fcckt", "auf", "nichts", "mehr", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPR", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als seines Vogels recht zu pflegen.", "tokens": ["Als", "sei\u00b7nes", "Vo\u00b7gels", "recht", "zu", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er sucht daher ihm allerwegen", "tokens": ["Er", "sucht", "da\u00b7her", "ihm", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein liebstes Futter, f\u00fcllt sein Glas", "tokens": ["Sein", "liebs\u00b7tes", "Fut\u00b7ter", ",", "f\u00fcllt", "sein", "Glas"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Tages oft mit frischem Na\u00df;", "tokens": ["Des", "Ta\u00b7ges", "oft", "mit", "fri\u00b7schem", "Na\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vergoldet ihm sein kleines Haus,", "tokens": ["Ver\u00b7gol\u00b7det", "ihm", "sein", "klei\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und bringt ihm manchen Distelschmaus.", "tokens": ["Und", "bringt", "ihm", "man\u00b7chen", "Dis\u00b7tel\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Stieglitz aber findet doch", "tokens": ["Der", "Stieg\u00b7litz", "a\u00b7ber", "fin\u00b7det", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zuletzt ein unbemerktes Loch,", "tokens": ["Zu\u00b7letzt", "ein", "un\u00b7be\u00b7merk\u00b7tes", "Loch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Aus welchem er gar bald entkam,", "tokens": ["Aus", "wel\u00b7chem", "er", "gar", "bald", "ent\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und fr\u00f6hlich seinen Abschied nahm.", "tokens": ["Und", "fr\u00f6h\u00b7lich", "sei\u00b7nen", "Ab\u00b7schied", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Knabe rief ihm freundlich zu:", "tokens": ["Der", "Kna\u00b7be", "rief", "ihm", "freund\u00b7lich", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wohin, du armer Vogel, du?", "tokens": ["Wo\u00b7hin", ",", "du", "ar\u00b7mer", "Vo\u00b7gel", ",", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "ADJA", "NE", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Was hat dir denn bei mir gefehlt,", "tokens": ["Was", "hat", "dir", "denn", "bei", "mir", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df sich dein Flug das Weite w\u00e4hlt?", "tokens": ["Da\u00df", "sich", "dein", "Flug", "das", "Wei\u00b7te", "w\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Hab' ich nicht Alles dir gegeben,", "tokens": ["Hab'", "ich", "nicht", "Al\u00b7les", "dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wovon die Herr'n Stieglitze leben?", "tokens": ["Wo\u00b7von", "die", "Herr'n", "Stieg\u00b7lit\u00b7ze", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "War nicht dein K\u00e4fich ein Palast,", "tokens": ["War", "nicht", "dein", "K\u00e4\u00b7fich", "ein", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mit gold'nen Dr\u00e4hten eingefa\u00dft?", "tokens": ["Mit", "gold'\u00b7nen", "Dr\u00e4h\u00b7ten", "ein\u00b7ge\u00b7fa\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und ward dir nicht aus meiner Hand", "tokens": ["Und", "ward", "dir", "nicht", "aus", "mei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Manch St\u00fcckchen Zucker zugewandt?", "tokens": ["Manch", "St\u00fcck\u00b7chen", "Zu\u00b7cker", "zu\u00b7ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Komm' wieder, bitt' ich dich, herein!", "tokens": ["Komm'", "wie\u00b7der", ",", "bitt'", "ich", "dich", ",", "her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "PPER", "PRF", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Stieglitz gab zur Antwort: Nein!", "tokens": ["Der", "Stieg\u00b7litz", "gab", "zur", "Ant\u00b7wort", ":", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Weg mit der ", "tokens": ["Weg", "mit", "der"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.28": {"text": "Hier hab' ich mehr, denn ich bin ", "tokens": ["Hier", "hab'", "ich", "mehr", ",", "denn", "ich", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KON", "PPER", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}