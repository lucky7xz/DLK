{"textgrid.poem.66055": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Reiter", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah zur\u00fcck auf lange Strecken,", "tokens": ["Ich", "sah", "zu\u00b7r\u00fcck", "auf", "lan\u00b7ge", "Stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich durch tiefen Sand hinging.", "tokens": ["Die", "ich", "durch", "tie\u00b7fen", "Sand", "hin\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier, da, an kahlen Hecken", "tokens": ["Hier", ",", "da", ",", "an", "kah\u00b7len", "He\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein bunter Fetzen hing.", "tokens": ["Ein", "bun\u00b7ter", "Fet\u00b7zen", "hing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fcck war mir vorausgeritten,", "tokens": ["Das", "Gl\u00fcck", "war", "mir", "vor\u00b7aus\u00b7ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sah seinen roten Mantel wehn, \u2013", "tokens": ["Ich", "sah", "sei\u00b7nen", "ro\u00b7ten", "Man\u00b7tel", "wehn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Konnt doch mit meinen m\u00fcden Schritten", "tokens": ["Konnt", "doch", "mit", "mei\u00b7nen", "m\u00fc\u00b7den", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So schnell nicht gehn.", "tokens": ["So", "schnell", "nicht", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wer h\u00e4lt da vorn im Weg und richtet", "tokens": ["Wer", "h\u00e4lt", "da", "vorn", "im", "Weg", "und", "rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Rabenr\u00f6\u00dflein auf mich her,", "tokens": ["Sein", "Ra\u00b7ben\u00b7r\u00f6\u00df\u00b7lein", "auf", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem fahlen Glanz umlichtet?", "tokens": ["Von", "ei\u00b7nem", "fah\u00b7len", "Glanz", "um\u00b7lich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz bangt sehr.", "tokens": ["Mein", "Herz", "bangt", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u00bbhast du das Gl\u00fcck nicht reiten sehen,", "tokens": ["\u00bb", "hast", "du", "das", "Gl\u00fcck", "nicht", "rei\u00b7ten", "se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du lieber Rittersmann?", "tokens": ["Du", "lie\u00b7ber", "Rit\u00b7ters\u00b7mann", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einen roten Mantel im Winde wehen", "tokens": ["Ei\u00b7nen", "ro\u00b7ten", "Man\u00b7tel", "im", "Win\u00b7de", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Mit goldner Troddel dran?\u00ab", "tokens": ["Mit", "gold\u00b7ner", "Trod\u00b7del", "dran", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sprach der Tod, und ich erbleichte:", "tokens": ["Da", "sprach", "der", "Tod", ",", "und", "ich", "er\u00b7bleich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdein Gl\u00fcck h\u00e4lt hier\u00ab,", "tokens": ["\u00bb", "dein", "Gl\u00fcck", "h\u00e4lt", "hier", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "$(", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und aus dem Sattel reichte", "tokens": ["Und", "aus", "dem", "Sat\u00b7tel", "reich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er seine harten H\u00e4nde mir.", "tokens": ["Er", "sei\u00b7ne", "har\u00b7ten", "H\u00e4n\u00b7de", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}