{"textgrid.poem.55751": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch Dich hab' ich, reinste der Frauen,", "tokens": ["Auch", "Dich", "hab'", "ich", ",", "reins\u00b7te", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit Lasterbegierden entweiht.", "tokens": ["Mit", "Las\u00b7ter\u00b7be\u00b7gier\u00b7den", "ent\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nicht darf ich Dein Antlitz mehr schauen", "tokens": ["Nicht", "darf", "ich", "Dein", "Ant\u00b7litz", "mehr", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In Ewigkeit.", "tokens": ["In", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist im Schlamme versunken,", "tokens": ["Mein", "Herz", "ist", "im", "Schlam\u00b7me", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gespenstig flackert in mir", "tokens": ["Ge\u00b7spens\u00b7tig", "fla\u00b7ckert", "in", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur Sehnsucht, wahnwitztrunken,", "tokens": ["Nur", "Sehn\u00b7sucht", ",", "wahn\u00b7witz\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kranke Gier.", "tokens": ["Und", "kran\u00b7ke", "Gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was soll mein schluchzendes Ringen,", "tokens": ["Was", "soll", "mein", "schluch\u00b7zen\u00b7des", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Seele Verzweiflungsgebet?", "tokens": ["Der", "See\u00b7le", "Ver\u00b7zwei\u00b7flungs\u00b7ge\u00b7bet", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich kann die D\u00e4monen nicht zwingen \u2013 \u2013", "tokens": ["Ich", "kann", "die", "D\u00e4\u00b7mo\u00b7nen", "nicht", "zwin\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es ist zu sp\u00e4t.", "tokens": ["Es", "ist", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}