{"dta.poem.8848": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Manches Entz\u00fccken und manches Ach;", "tokens": ["Man\u00b7ches", "Ent\u00b7z\u00fc\u00b7cken", "und", "man\u00b7ches", "Ach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIS", "ITJ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ohne zu schwindeln ergehn sie sich", "tokens": ["Oh\u00b7ne", "zu", "schwin\u00b7deln", "er\u00b7gehn", "sie", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF", "VVFIN", "PPER", "PRF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mitten im Schlafe von Dach zu Dach.", "tokens": ["Mit\u00b7ten", "im", "Schla\u00b7fe", "von", "Dach", "zu", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wandelt geschwinde des Wunsches Weg,", "tokens": ["Wan\u00b7delt", "ge\u00b7schwin\u00b7de", "des", "Wun\u00b7sches", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Doch in der N\u00e4he des Ziels gemach!", "tokens": ["Doch", "in", "der", "N\u00e4\u00b7he", "des", "Ziels", "ge\u00b7mach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn ihr den Gipfel erklommen w\u00e4hnt,", "tokens": ["Wenn", "ihr", "den", "Gip\u00b7fel", "er\u00b7klom\u00b7men", "w\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Oeffnen sich gr\u00e4\u00dfliche Schl\u00fcnde jach.", "tokens": ["Oeff\u00b7nen", "sich", "gr\u00e4\u00df\u00b7li\u00b7che", "Schl\u00fcn\u00b7de", "jach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Freunde, mir ist die Vernunft zu schwer,", "tokens": ["Freun\u00b7de", ",", "mir", "ist", "die", "Ver\u00b7nunft", "zu", "schwer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Aber die Liebe, das ist mein Fach!", "tokens": ["A\u00b7ber", "die", "Lie\u00b7be", ",", "das", "ist", "mein", "Fach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}