{"textgrid.poem.55012": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hab ich wirklich dich verloren?", "tokens": ["So", "hab", "ich", "wirk\u00b7lich", "dich", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du, o Sch\u00f6ne, mir entflohn?", "tokens": ["Bist", "du", ",", "o", "Sch\u00f6\u00b7ne", ",", "mir", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch klingt in den gewohnten Ohren", "tokens": ["Noch", "klingt", "in", "den", "ge\u00b7wohn\u00b7ten", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jedes Wort, ein jeder Ton.", "tokens": ["Ein", "je\u00b7des", "Wort", ",", "ein", "je\u00b7der", "Ton", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So wie des Wandrers Blick am Morgen", "tokens": ["So", "wie", "des", "Wand\u00b7rers", "Blick", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergebens in die L\u00fcfte dringt,", "tokens": ["Ver\u00b7ge\u00b7bens", "in", "die", "L\u00fcf\u00b7te", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn, in dem blauen Raum verborgen,", "tokens": ["Wenn", ",", "in", "dem", "blau\u00b7en", "Raum", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber ihm die Lerche singt:", "tokens": ["Hoch", "\u00fc\u00b7ber", "ihm", "die", "Ler\u00b7che", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So dringet \u00e4ngstlich hin und wider", "tokens": ["So", "drin\u00b7get", "\u00e4ngst\u00b7lich", "hin", "und", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Feld und Busch und Wald mein Blick.", "tokens": ["Durch", "Feld", "und", "Busch", "und", "Wald", "mein", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich rufen alle meine Lieder;", "tokens": ["Dich", "ru\u00b7fen", "al\u00b7le", "mei\u00b7ne", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O komm, Geliebte, mir zur\u00fcck!", "tokens": ["O", "komm", ",", "Ge\u00b7lieb\u00b7te", ",", "mir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}