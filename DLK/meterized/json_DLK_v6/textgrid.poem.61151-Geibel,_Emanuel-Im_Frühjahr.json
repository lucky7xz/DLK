{"textgrid.poem.61151": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchjahr", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich im Lenz durch Gr\u00fcn und Rosen walle,", "tokens": ["Wenn", "ich", "im", "Lenz", "durch", "Gr\u00fcn", "und", "Ro\u00b7sen", "wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da wird mir oft zu Sinn, als m\u00fc\u00dft' ich klagen,", "tokens": ["Da", "wird", "mir", "oft", "zu", "Sinn", ",", "als", "m\u00fc\u00dft'", "ich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich geboren bin in solchen Tagen,", "tokens": ["Da\u00df", "ich", "ge\u00b7bo\u00b7ren", "bin", "in", "sol\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die rauh erdr\u00f6hnen von der Waffen Schalle.", "tokens": ["Die", "rauh", "er\u00b7dr\u00f6h\u00b7nen", "von", "der", "Waf\u00b7fen", "Schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich h\u00e4tte gern ein freudig Lied f\u00fcr alle", "tokens": ["Ich", "h\u00e4t\u00b7te", "gern", "ein", "freu\u00b7dig", "Lied", "f\u00fcr", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Gottesfrieden in der Brust getragen,", "tokens": ["Voll", "Got\u00b7tes\u00b7frie\u00b7den", "in", "der", "Brust", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich h\u00e4tte gern im Zauberwald der Sagen", "tokens": ["Ich", "h\u00e4t\u00b7te", "gern", "im", "Zau\u00b7ber\u00b7wald", "der", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein wei\u00dfes Edelwild gebracht zu Falle.", "tokens": ["Ein", "wei\u00b7\u00dfes", "E\u00b7del\u00b7wild", "ge\u00b7bracht", "zu", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Umsonst! Es ziemt uns nicht, im Kranz der Reben", "tokens": ["Um\u00b7sonst", "!", "Es", "ziemt", "uns", "nicht", ",", "im", "Kranz", "der", "Re\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit goldnen M\u00e4rchen das Gelag zu w\u00fcrzen;", "tokens": ["Mit", "gold\u00b7nen", "M\u00e4r\u00b7chen", "das", "Ge\u00b7lag", "zu", "w\u00fcr\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn diese Zeit ist wie die Sphinx von Theben.", "tokens": ["Denn", "die\u00b7se", "Zeit", "ist", "wie", "die", "Sphinx", "von", "The\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer's heute wagt, als Dichter sich zu sch\u00fcrzen,", "tokens": ["Wer's", "heu\u00b7te", "wagt", ",", "als", "Dich\u00b7ter", "sich", "zu", "sch\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$,", "KOUS", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr R\u00e4tsel wird sie ihm zu raten geben", "tokens": ["Ihr", "R\u00e4t\u00b7sel", "wird", "sie", "ihm", "zu", "ra\u00b7ten", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, l\u00f6st er's nicht, ihn in den Abgrund st\u00fcrzen.", "tokens": ["Und", ",", "l\u00f6st", "er's", "nicht", ",", "ihn", "in", "den", "Ab\u00b7grund", "st\u00fcr\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PIS", "PTKNEG", "$,", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}