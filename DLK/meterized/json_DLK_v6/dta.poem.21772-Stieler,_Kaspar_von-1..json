{"dta.poem.21772": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Dorinde hat mich erst gelehrt", "tokens": ["Do\u00b7rin\u00b7de", "hat", "mich", "erst", "ge\u00b7lehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der edlen Freyheit abzusagen.", "tokens": ["der", "ed\u00b7len", "Frey\u00b7heit", "ab\u00b7zu\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir war kein Amor je geehrt/", "tokens": ["Mir", "war", "kein", "A\u00b7mor", "je", "ge\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NE", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Spott der Venus g\u00f6ldner Wagen.", "tokens": ["ein", "Spott", "der", "Ve\u00b7nus", "g\u00f6ld\u00b7ner", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hielte vor ein Kinder-spiel", "tokens": ["Ich", "hiel\u00b7te", "vor", "ein", "Kin\u00b7der\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der Liebenden verbuhltes K\u00fcssen/", "tokens": ["der", "Lie\u00b7ben\u00b7den", "ver\u00b7buhl\u00b7tes", "K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "die Tugend/ ein gelehrtes wissen", "tokens": ["die", "Tu\u00b7gend", "/", "ein", "ge\u00b7lehr\u00b7tes", "wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "war meines Lebens einigs Ziel.", "tokens": ["war", "mei\u00b7nes", "Le\u00b7bens", "ei\u00b7nigs", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nachdehm der schwarzen Augen Straal/", "tokens": ["Nach\u00b7dehm", "der", "schwar\u00b7zen", "Au\u00b7gen", "Straal", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "die Tracht und Anmuht der Dorinden", "tokens": ["die", "Tracht", "und", "An\u00b7muht", "der", "Do\u00b7rin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mir meiner Sinnen Ruder stahl/", "tokens": ["mir", "mei\u00b7ner", "Sin\u00b7nen", "Ru\u00b7der", "stahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wei\u00df ich mich nicht in mir zu finden.", "tokens": ["wei\u00df", "ich", "mich", "nicht", "in", "mir", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kunst-Lust/ ein gesunder Raht", "tokens": ["Die", "Kunst\u00b7Lust", "/", "ein", "ge\u00b7sun\u00b7der", "Raht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ist in mir Blinden gantz verschwunden.", "tokens": ["ist", "in", "mir", "Blin\u00b7den", "gantz", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O der unseelig-b\u00f6sen Stunden/", "tokens": ["O", "der", "un\u00b7see\u00b7lig\u00b7b\u00f6\u00b7sen", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "die mich durch Sie verf\u00fchret hat.", "tokens": ["die", "mich", "durch", "Sie", "ver\u00b7f\u00fch\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}