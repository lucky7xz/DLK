{"textgrid.poem.41012": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Kinder Angebinde", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Band wir, Mutter! bringen,", "tokens": ["Ein", "Band", "wir", ",", "Mut\u00b7ter", "!", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "NN", "$.", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das reichet Liebe dar,", "tokens": ["Das", "rei\u00b7chet", "Lie\u00b7be", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das soll dich fest umschlingen", "tokens": ["Das", "soll", "dich", "fest", "um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Tag, der dich gebar.", "tokens": ["Am", "Tag", ",", "der", "dich", "ge\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von Gold ist's keine Kette,", "tokens": ["Von", "Gold", "ist's", "kei\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Stoff aus fremdem Land,", "tokens": ["Kein", "Stoff", "aus", "frem\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist an ihrer St\u00e4tte", "tokens": ["Es", "ist", "an", "ih\u00b7rer", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein festgewobnes Band.", "tokens": ["Ein", "fest\u00b7ge\u00b7wob\u00b7nes", "Band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl r\u00fchrt, befreit vom Harme,", "tokens": ["Wohl", "r\u00fchrt", ",", "be\u00b7freit", "vom", "Har\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Herz darunter sich.", "tokens": ["Dein", "Herz", "da\u00b7run\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieh! deiner Kinder Arme", "tokens": ["Sieh", "!", "dei\u00b7ner", "Kin\u00b7der", "Ar\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umschlingen, Mutter, dich!", "tokens": ["Um\u00b7schlin\u00b7gen", ",", "Mut\u00b7ter", ",", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}