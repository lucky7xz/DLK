{"dta.poem.9364": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .  \n  Ein Muster der Neuen Art/ darinn  \n der Weibliche 9-sylbig/ der M\u00e4nnliche 8-sylbig/  \n so den Abschnitt nach der 3. hatt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljebster Gott/ la\u00df mich gnade finden/", "tokens": ["Ljebs\u00b7ter", "Gott", "/", "la\u00df", "mich", "gna\u00b7de", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "VVIMP", "PPER", "VVFIN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "tilge doch meine schwere s\u00fcnden/", "tokens": ["til\u00b7ge", "doch", "mei\u00b7ne", "schwe\u00b7re", "s\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "ADJA", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "mein gebein ist zerschlagen gar/", "tokens": ["mein", "ge\u00b7bein", "ist", "zer\u00b7schla\u00b7gen", "gar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "ADV", "$("], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "mein gem\u00fcth kr\u00e4nckr sich jmmerdar;", "tokens": ["mein", "ge\u00b7m\u00fcth", "kr\u00e4n\u00b7ckr", "sich", "jm\u00b7mer\u00b7dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schicke zu lieber Gott/ mir armen/", "tokens": ["Schi\u00b7cke", "zu", "lie\u00b7ber", "Gott", "/", "mir", "ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "NN", "$(", "PPER", "ADJA", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "vor das Recht langmuth und Erbarmen/", "tokens": ["vor", "das", "Recht", "lang\u00b7muth", "und", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "mein Gesicht ist von thr\u00e4nen roth/", "tokens": ["mein", "Ge\u00b7sicht", "ist", "von", "thr\u00e4\u00b7nen", "roth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "welche mich \u00e4ndern auf den todt.", "tokens": ["wel\u00b7che", "mich", "\u00e4n\u00b7dern", "auf", "den", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Eyle doch/ hilf mier aus den n\u00f6then/", "tokens": ["Ey\u00b7le", "doch", "/", "hilf", "mier", "aus", "den", "n\u00f6\u00b7then", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "VVIMP", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.10": {"text": "eyle doch/ alle furcht zu t\u00f6dten/", "tokens": ["ey\u00b7le", "doch", "/", "al\u00b7le", "furcht", "zu", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "die mein Hertz g\u00e4ntzlich hat \u00fcmbtingt/", "tokens": ["die", "mein", "Hertz", "g\u00e4ntz\u00b7lich", "hat", "\u00fcmb\u00b7tingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "da\u00df mein Mund Dier ein Dancklied singt.", "tokens": ["da\u00df", "mein", "Mund", "Dier", "ein", "Danck\u00b7lied", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}