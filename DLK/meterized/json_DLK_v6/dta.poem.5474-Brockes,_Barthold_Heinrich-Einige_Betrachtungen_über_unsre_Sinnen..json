{"dta.poem.5474": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einige Betrachtungen \u00fcber unsre  \n Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint, wann wir uns recht betrachten, da\u00df, ob\nzwar junger Kinder Seelen", "tokens": ["Es", "scheint", ",", "wann", "wir", "uns", "recht", "be\u00b7trach\u00b7ten", ",", "da\u00df", ",", "ob", "zwar", "jun\u00b7ger", "Kin\u00b7der", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVINF", "$,", "KOUS", "$,", "KOUS", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die F\u00e4higkeiten, zu vergleichen, zu schliessen, zu verstehn,", "tokens": ["Die", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", ",", "zu", "ver\u00b7glei\u00b7chen", ",", "zu", "schlies\u00b7sen", ",", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Doch die gedachten F\u00e4higkeiten und ihres Wesens rege", "tokens": ["Doch", "die", "ge\u00b7dach\u00b7ten", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", "und", "ih\u00b7res", "We\u00b7sens", "re\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ohn unsrer Sinnen Wunder-Werckzeug, ohn\u2019 unsrer", "tokens": ["Ohn", "uns\u00b7rer", "Sin\u00b7nen", "Wun\u00b7der\u00b7\u00b7Wer\u00b7ck\u00b7zeug", ",", "ohn'", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "KOUI", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich immermehr entwickeln w\u00fcrden. Die Sinnen wir-", "tokens": ["Sich", "im\u00b7mer\u00b7mehr", "ent\u00b7wi\u00b7ckeln", "w\u00fcr\u00b7den", ".", "Die", "Sin\u00b7nen", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "VVINF", "VAFIN", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da\u00df wir der C\u00f6rper gut geniessen, da\u00df sie f\u00fcr uns ge-", "tokens": ["Da\u00df", "wir", "der", "C\u00f6r\u00b7per", "gut", "ge\u00b7nies\u00b7sen", ",", "da\u00df", "sie", "f\u00fcr", "uns", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$,", "KOUS", "PPER", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.7": {"text": "Da\u00df, auf so wunderbare Weise, mit C\u00f6rpern Geister", "tokens": ["Da\u00df", ",", "auf", "so", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se", ",", "mit", "C\u00f6r\u00b7pern", "Geis\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Da\u00df wir, was auf der Welt vorhanden, geniessen, sehen", "tokens": ["Da\u00df", "wir", ",", "was", "auf", "der", "Welt", "vor\u00b7han\u00b7den", ",", "ge\u00b7nies\u00b7sen", ",", "se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "ADJD", "$,", "VVPP", "$,", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Da\u00df wir der Creaturen Sch\u00f6nheit, die, auf so manche", "tokens": ["Da\u00df", "wir", "der", "Crea\u00b7tu\u00b7ren", "Sch\u00f6n\u00b7heit", ",", "die", ",", "auf", "so", "man\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "PRELS", "$,", "APPR", "ADV", "PIAT"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Da\u00df wir so viele Form- und Farben, da\u00df wir des Lichtes", "tokens": ["Da\u00df", "wir", "so", "vie\u00b7le", "For\u00b7m", "und", "Far\u00b7ben", ",", "da\u00df", "wir", "des", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "TRUNC", "KON", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ohn unsre Sinnen, w\u00fcrden C\u00f6rper, die durch die Sin-", "tokens": ["Ohn", "uns\u00b7re", "Sin\u00b7nen", ",", "w\u00fcr\u00b7den", "C\u00f6r\u00b7per", ",", "die", "durch", "die", "Sin"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VAFIN", "NE", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Auf eine mittelbare Weise, wie wirs empfinden, sich ver-", "tokens": ["Auf", "ei\u00b7ne", "mit\u00b7tel\u00b7ba\u00b7re", "Wei\u00b7se", ",", "wie", "wirs", "emp\u00b7fin\u00b7den", ",", "sich", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Sich, recht als w\u00e4ren sie nicht da, den Seelen gantz und", "tokens": ["Sich", ",", "recht", "als", "w\u00e4\u00b7ren", "sie", "nicht", "da", ",", "den", "See\u00b7len", "gantz", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "$,", "ADJD", "KOKOM", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "ART", "NN", "ADV", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}