{"dta.poem.3872": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "2) Als er ihm die K\u00e4lte und Holtz-Man-  \n gel klaget.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Da deine Vater-Huld auf den Brief, so ich", "tokens": ["Da", "dei\u00b7ne", "Va\u00b7ter\u00b7Huld", "auf", "den", "Brief", ",", "so", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "ADV", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "schrieb", "tokens": ["schrieb"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Den letzten Januar, die Antwort schuldig blieb,", "tokens": ["Den", "letz\u00b7ten", "Ja\u00b7nu\u00b7ar", ",", "die", "Ant\u00b7wort", "schul\u00b7dig", "blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So macht ich diesen Schlu\u00df, du m\u00fcstest gl\u00fccklich", "tokens": ["So", "macht", "ich", "die\u00b7sen", "Schlu\u00df", ",", "du", "m\u00fcs\u00b7test", "gl\u00fcck\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "leben,", "tokens": ["le\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und w\u00fcrdest bald hievon erw\u00fcnschte Nachricht gebe\u0303.", "tokens": ["Und", "w\u00fcr\u00b7dest", "bald", "hie\u00b7von", "er\u00b7w\u00fcnschte", "Nach\u00b7richt", "ge\u00b7b\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PAV", "VVFIN", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Inzwischen glaube nur, da\u00df dein Verlust mich qu\u00e4lt,", "tokens": ["I\u00b7nzwi\u00b7schen", "glau\u00b7be", "nur", ",", "da\u00df", "dein", "Ver\u00b7lust", "mich", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja da\u00df dein armer Sohn so Tag als Stunden zehlt,", "tokens": ["Ja", "da\u00df", "dein", "ar\u00b7mer", "Sohn", "so", "Tag", "als", "Stun\u00b7den", "zehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bis ihn des Schicksals Macht der Philur\u00e4 ent-", "tokens": ["Bis", "ihn", "des", "Schick\u00b7sals", "Macht", "der", "Phi\u00b7lu\u00b7r\u00e4", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "reisse,", "tokens": ["reis\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Und ihm nach GOttes Rath ein besser Leben weisse.", "tokens": ["Und", "ihm", "nach", "Got\u00b7tes", "Rath", "ein", "bes\u00b7ser", "Le\u00b7ben", "weis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein Vater! ach der Frost in unserm Plei\u00df-", "tokens": ["Mein", "Va\u00b7ter", "!", "ach", "der", "Frost", "in", "un\u00b7serm", "Plei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "XY", "ART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Athen", "tokens": ["A\u00b7then"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Ist heuer, glaub es nur, fast nimmer auszustehn,", "tokens": ["Ist", "heu\u00b7er", ",", "glaub", "es", "nur", ",", "fast", "nim\u00b7mer", "aus\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man sorgt um Holtz und Geld, di\u00df bringt ein ban-", "tokens": ["Man", "sorgt", "um", "Holtz", "und", "Geld", ",", "di\u00df", "bringt", "ein", "ban"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "PDS", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "ges H\u00e4rmen,", "tokens": ["ges", "H\u00e4r\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Man l\u00e4ufft den gantzen Tag um nur sich zu er-", "tokens": ["Man", "l\u00e4ufft", "den", "gant\u00b7zen", "Tag", "um", "nur", "sich", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADV", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "w\u00e4rmen,", "tokens": ["w\u00e4r\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Denn das zu theure Holtz reist l\u00e4sterlich ins Geld,", "tokens": ["Denn", "das", "zu", "theu\u00b7re", "Holtz", "reist", "l\u00e4s\u00b7ter\u00b7lich", "ins", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKZU", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du weist, wie solches Geld bey Armen harte h\u00e4lt;", "tokens": ["Du", "weist", ",", "wie", "sol\u00b7ches", "Geld", "bey", "Ar\u00b7men", "har\u00b7te", "h\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIAT", "NN", "APPR", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn mir der Him\u0303el doch fein viele Batzen g\u00f6nnte,", "tokens": ["Wenn", "mir", "der", "Him\u0303el", "doch", "fein", "vie\u00b7le", "Bat\u00b7zen", "g\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Da\u00df ich des Winters Macht dadurch bezwingen", "tokens": ["Da\u00df", "ich", "des", "Win\u00b7ters", "Macht", "da\u00b7durch", "be\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "k\u00f6nte,", "tokens": ["k\u00f6n\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Ich wei\u00df du l\u00e4st mich nicht, ich seh den Brief schon da,", "tokens": ["Ich", "wei\u00df", "du", "l\u00e4st", "mich", "nicht", ",", "ich", "seh", "den", "Brief", "schon", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn rufft mein hoffend Hertz ein frohes \u1f15\u03c5\u03c1\u03b9\u03ba\u03b1!", "tokens": ["Denn", "rufft", "mein", "hof\u00b7fend", "Hertz", "ein", "fro\u00b7hes", "\u1f15\u03c5\u03c1\u03b9\u03ba\u03b1", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Die Sehnsucht eilt inde\u00df dir diesen Brief zu schicken,", "tokens": ["Die", "Sehn\u00b7sucht", "eilt", "in\u00b7de\u00df", "dir", "die\u00b7sen", "Brief", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und einen treuen Ku\u00df statt Siegels aufzudr\u00fccken.", "tokens": ["Und", "ei\u00b7nen", "treu\u00b7en", "Ku\u00df", "statt", "Sie\u00b7gels", "auf\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}