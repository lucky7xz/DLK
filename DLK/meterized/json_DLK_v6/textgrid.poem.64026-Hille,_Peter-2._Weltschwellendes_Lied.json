{"textgrid.poem.64026": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "2. Weltschwellendes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber gr\u00fc\u00dfende Kl\u00fcfte und B\u00fcsche zieht", "tokens": ["\u00dc\u00b7ber", "gr\u00fc\u00b7\u00dfen\u00b7de", "Kl\u00fcf\u00b7te", "und", "B\u00fc\u00b7sche", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und junge V\u00f6gel wiegende Wipfel", "tokens": ["Und", "jun\u00b7ge", "V\u00f6\u00b7gel", "wie\u00b7gen\u00b7de", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zwei gelbe Falter ...", "tokens": ["Zwei", "gel\u00b7be", "Fal\u00b7ter", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Haschen, ein F\u00fchlen,", "tokens": ["Ein", "Ha\u00b7schen", ",", "ein", "F\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vor\u00fcber ...", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "..."], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Das w\u00e4hrt, das w\u00e4hrt.", "tokens": ["Das", "w\u00e4hrt", ",", "das", "w\u00e4hrt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Seliger Flug,", "tokens": ["Se\u00b7li\u00b7ger", "Flug", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hier in den Himmel", "tokens": ["Hier", "in", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "Die beiden es trug:", "tokens": ["Die", "bei\u00b7den", "es", "trug", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mit vier Bl\u00e4ttern", "tokens": ["Mit", "vier", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zwei Blumen.", "tokens": ["Zwei", "Blu\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Was so schwer in der Erde,", "tokens": ["Was", "so", "schwer", "in", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "So ganz schwer \u2013", "tokens": ["So", "ganz", "schwer", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Aller Fr\u00fchling schweigt", "tokens": ["Al\u00b7ler", "Fr\u00fch\u00b7ling", "schweigt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und singt sein leuchtend schwellendes Reifen.", "tokens": ["Und", "singt", "sein", "leuch\u00b7tend", "schwel\u00b7len\u00b7des", "Rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Allmenschen.", "tokens": ["All\u00b7men\u00b7schen", "."], "token_info": ["word", "punct"], "pos": ["PIS", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Braunes M\u00fchen,", "tokens": ["Brau\u00b7nes", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Perlen des Flei\u00dfes,", "tokens": ["Per\u00b7len", "des", "Flei\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Rosen auf greifenden Kn\u00e4ufen.", "tokens": ["Ro\u00b7sen", "auf", "grei\u00b7fen\u00b7den", "Kn\u00e4u\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Bilder rohrleichter H\u00fctten.", "tokens": ["Bil\u00b7der", "rohr\u00b7leich\u00b7ter", "H\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Hurtige Schultern des plaudernd", "tokens": ["Hur\u00b7ti\u00b7ge", "Schul\u00b7tern", "des", "plau\u00b7dernd"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Kindlich treibenden Wichtes", "tokens": ["Kind\u00b7lich", "trei\u00b7ben\u00b7den", "Wich\u00b7tes"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Tragen \u00fcber das Tal zu anderem Hofe", "tokens": ["Tra\u00b7gen", "\u00fc\u00b7ber", "das", "Tal", "zu", "an\u00b7de\u00b7rem", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ziegen und Frucht \u2013", "tokens": ["Zie\u00b7gen", "und", "Frucht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Gr\u00fcne Weiten.", "tokens": ["Gr\u00fc\u00b7ne", "Wei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Ziegenerstiegene.", "tokens": ["Zie\u00b7ge\u00b7ners\u00b7tie\u00b7ge\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Schmerzen w\u00fchlen", "tokens": ["Schmer\u00b7zen", "w\u00fch\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schmerzen, seliges Sichlegen ins Grab \u2013", "tokens": ["Schmer\u00b7zen", ",", "se\u00b7li\u00b7ges", "Sich\u00b7le\u00b7gen", "ins", "Grab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "In Erde all:", "tokens": ["In", "Er\u00b7de", "all", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Schwanken der Seele zur H\u00f6he \u2013", "tokens": ["Schwan\u00b7ken", "der", "See\u00b7le", "zur", "H\u00f6\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Die L\u00fcfte sind m\u00fcde", "tokens": ["Die", "L\u00fcf\u00b7te", "sind", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schwer vom Fremden,", "tokens": ["Schwer", "vom", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "V\u00f6gel darin,", "tokens": ["V\u00f6\u00b7gel", "da\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schwarze V\u00f6gel mit harten, bohrenden Seelen", "tokens": ["Schwar\u00b7ze", "V\u00f6\u00b7gel", "mit", "har\u00b7ten", ",", "boh\u00b7ren\u00b7den", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dunkelrunden Augen,", "tokens": ["Dun\u00b7kel\u00b7run\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Blankem bereitem Schnabel.", "tokens": ["Blan\u00b7kem", "be\u00b7rei\u00b7tem", "Schna\u00b7bel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Schwarzer Scharen fliegendes Fragen,", "tokens": ["Schwar\u00b7zer", "Scha\u00b7ren", "flie\u00b7gen\u00b7des", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Zusammenrufen", "tokens": ["Zu\u00b7sam\u00b7men\u00b7ru\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Dunkelbeutefroher Ruf.", "tokens": ["Dun\u00b7kel\u00b7beu\u00b7te\u00b7fro\u00b7her", "Ruf", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}