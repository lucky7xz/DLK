{"textgrid.poem.49183": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "6. Echo oder Wiederschall", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df Ort, mit B\u00e4umen gantz umbgeben,", "tokens": ["Di\u00df", "Ort", ",", "mit", "B\u00e4u\u00b7men", "gantz", "umb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da nichts als Frucht' und Schatten schweben,", "tokens": ["Da", "nichts", "als", "Frucht'", "und", "Schat\u00b7ten", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da Traurigkeit sich hin verf\u00fcgt,", "tokens": ["Da", "Trau\u00b7rig\u00b7keit", "sich", "hin", "ver\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da alles w\u00fcst' und \u00f6de liegt,", "tokens": ["Da", "al\u00b7les", "w\u00fcst'", "und", "\u00f6\u00b7de", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da auch die Sonne nicht hinweichet,", "tokens": ["Da", "auch", "die", "Son\u00b7ne", "nicht", "hin\u00b7wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da gifftig Ungezieffer schleichet,", "tokens": ["Da", "giff\u00b7tig", "Un\u00b7ge\u00b7zief\u00b7fer", "schlei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da gar kein Wasser sich ergeust,", "tokens": ["Da", "gar", "kein", "Was\u00b7ser", "sich", "er\u00b7geust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als das au\u00df meinen Augen fleust,", "tokens": ["Als", "das", "au\u00df", "mei\u00b7nen", "Au\u00b7gen", "fleust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da gar kein Liecht nicht wird erkennet,", "tokens": ["Da", "gar", "kein", "Liecht", "nicht", "wird", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PTKNEG", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als da\u00df au\u00df meinem Herzen brennet,", "tokens": ["Als", "da\u00df", "au\u00df", "mei\u00b7nem", "Her\u00b7zen", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Bed\u00fcncket mich bequeme seyn,", "tokens": ["Be\u00b7d\u00fcn\u00b7cket", "mich", "be\u00b7que\u00b7me", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da ich mich klag' ab meiner Pein,", "tokens": ["Da", "ich", "mich", "klag'", "ab", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ab meiner Pein und tieffstem Leiden,", "tokens": ["Ab", "mei\u00b7ner", "Pein", "und", "tieffs\u00b7tem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df mich jetzund wird von mir scheiden;", "tokens": ["Da\u00df", "mich", "je\u00b7tzund", "wird", "von", "mir", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch ehe der gew\u00fcntschte Tod", "tokens": ["Doch", "e\u00b7he", "der", "ge\u00b7w\u00fcnt\u00b7schte", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mit Freuden abhilfft meiner Noth,", "tokens": ["Mit", "Freu\u00b7den", "ab\u00b7hilfft", "mei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Will ich von meiner Liebe klagen", "tokens": ["Will", "ich", "von", "mei\u00b7ner", "Lie\u00b7be", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und, ob schon gantz vergeblich, fragen,", "tokens": ["Und", ",", "ob", "schon", "gantz", "ver\u00b7geb\u00b7lich", ",", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "ADV", "ADJD", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ist dann niemand, der tr\u00f6ste mich,", "tokens": ["Ist", "dann", "nie\u00b7mand", ",", "der", "tr\u00f6s\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$,", "PRELS", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Weil ich so trauer' inniglich?", "tokens": ["Weil", "ich", "so", "trau\u00b7e\u00b7r'", "in\u00b7nig\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "O Echo, wirst nur du alleine", "tokens": ["O", "E\u00b7cho", ",", "wirst", "nur", "du", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Hinfort mich tr\u00f6sten, und sonst keine?", "tokens": ["Hin\u00b7fort", "mich", "tr\u00f6s\u00b7ten", ",", "und", "sonst", "kei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KON", "ADV", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wie soll sie leschen meinen Brandt,", "tokens": ["Wie", "soll", "sie", "le\u00b7schen", "mei\u00b7nen", "Brandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ist sie mir doch noch unbekandt?", "tokens": ["Ist", "sie", "mir", "doch", "noch", "un\u00b7be\u00b7kandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sie wil es aber nicht verstehen,", "tokens": ["Sie", "wil", "es", "a\u00b7ber", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Lest mich in Angst ohn Abla\u00df gehen.", "tokens": ["Lest", "mich", "in", "Angst", "ohn", "Ab\u00b7la\u00df", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Verleuret sich denn ja mein Leidt,", "tokens": ["Ver\u00b7leu\u00b7ret", "sich", "denn", "ja", "mein", "Leidt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wem soll ichs dancken mit der Zeit?", "tokens": ["Wem", "soll", "ichs", "dan\u00b7cken", "mit", "der", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So ist nun Noth, da\u00df ich verscharre", "tokens": ["So", "ist", "nun", "Noth", ",", "da\u00df", "ich", "ver\u00b7schar\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Das Feuer, und der Stund' erharre?", "tokens": ["Das", "Feu\u00b7er", ",", "und", "der", "Stund'", "er\u00b7har\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wenn ich zu lange harren solt',", "tokens": ["Wenn", "ich", "zu", "lan\u00b7ge", "har\u00b7ren", "solt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "H\u00fclff etwas meiner Ungedult?", "tokens": ["H\u00fclff", "et\u00b7was", "mei\u00b7ner", "Un\u00b7ge\u00b7dult", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Vielleichte m\u00f6cht' ich sterben ehe,", "tokens": ["Viel\u00b7leich\u00b7te", "m\u00f6cht'", "ich", "ster\u00b7ben", "e\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "KOUS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Weil ich im h\u00f6chsten Elend gehe?", "tokens": ["Weil", "ich", "im", "h\u00f6chs\u00b7ten", "E\u00b7lend", "ge\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "So folg' ich deinem Rathe schlecht,", "tokens": ["So", "fol\u00b7g'", "ich", "dei\u00b7nem", "Ra\u00b7the", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.36": {"text": "Hoff', alles werde gut und recht.", "tokens": ["Hoff'", ",", "al\u00b7les", "wer\u00b7de", "gut", "und", "recht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Nun bin ich vieler Noth entbunden", "tokens": ["Nun", "bin", "ich", "vie\u00b7ler", "Noth", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und habe guten Trost empfunden.", "tokens": ["Und", "ha\u00b7be", "gu\u00b7ten", "Trost", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Du unbewohnte Traurigkeit,", "tokens": ["Du", "un\u00b7be\u00b7wohn\u00b7te", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ihr Hecken voll von meinem Leid',", "tokens": ["Ihr", "He\u00b7cken", "voll", "von", "mei\u00b7nem", "Leid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ihr schwartzen H\u00f6len und ihr W\u00fcsten,", "tokens": ["Ihr", "schwart\u00b7zen", "H\u00f6\u00b7len", "und", "ihr", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Da Eulen, Natern, Schlangen nisten,", "tokens": ["Da", "Eu\u00b7len", ",", "Na\u00b7tern", ",", "Schlan\u00b7gen", "nis\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Du \u00f6des Ort, gehabt euch wol;", "tokens": ["Du", "\u00f6\u00b7des", "Ort", ",", "ge\u00b7habt", "euch", "wol", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VAPP", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ich bin f\u00fcr Trauren Freude voll,", "tokens": ["Ich", "bin", "f\u00fcr", "Trau\u00b7ren", "Freu\u00b7de", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "F\u00fcr Finstern\u00fc\u00df such' ich die Sonnen,", "tokens": ["F\u00fcr", "Fins\u00b7ter\u00b7n\u00fc\u00df", "such'", "ich", "die", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "F\u00fcr Threnen ein k\u00fchlen Bronnen:", "tokens": ["F\u00fcr", "Thre\u00b7nen", "ein", "k\u00fch\u00b7len", "Bron\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "Die so Vertr\u00f6stung mir gethan,", "tokens": ["Die", "so", "Ver\u00b7tr\u00f6s\u00b7tung", "mir", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ist so, da\u00df sie nicht l\u00fcgen kan.", "tokens": ["Ist", "so", ",", "da\u00df", "sie", "nicht", "l\u00fc\u00b7gen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}