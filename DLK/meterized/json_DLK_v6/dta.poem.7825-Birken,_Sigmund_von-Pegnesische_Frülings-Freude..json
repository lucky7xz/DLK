{"dta.poem.7825": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Kanst du dann mein Buhl nit werden/", "tokens": ["Kanst", "du", "dann", "mein", "Buhl", "nit", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dafne/ O du harter Sinn!", "tokens": ["Daf\u00b7ne", "/", "O", "du", "har\u00b7ter", "Sinn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ey so solst du doch/ auf Erden/", "tokens": ["ey", "so", "solst", "du", "doch", "/", "auf", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "$(", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "du solst seyn mein Baum forthin.", "tokens": ["du", "solst", "seyn", "mein", "Baum", "for\u00b7thin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Pfeile/ Kron der Sch\u00f6nen!", "tokens": ["Mei\u00b7ne", "Pfei\u00b7le", "/", "Kron", "der", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haar\u2019 und Cyther/ solst du kr\u00f6nen.", "tokens": ["Haa\u00b7r'", "und", "Cy\u00b7ther", "/", "solst", "du", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$(", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wann siegprangend widerkehren", "tokens": ["Wann", "sieg\u00b7pran\u00b7gend", "wi\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine Helden von dem Streit:", "tokens": ["mei\u00b7ne", "Hel\u00b7den", "von", "dem", "Streit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "solst du ihre Scheitel ehren.", "tokens": ["solst", "du", "ih\u00b7re", "Schei\u00b7tel", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du solst meine liebe Leut/", "tokens": ["Du", "solst", "mei\u00b7ne", "lie\u00b7be", "Leut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "meine Dichter/ auch begl\u00e4nzen", "tokens": ["mei\u00b7ne", "Dich\u00b7ter", "/", "auch", "be\u00b7gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und ihr Sinnenhaus bekr\u00e4nzen.", "tokens": ["und", "ihr", "Sin\u00b7nen\u00b7haus", "be\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich soll auch kein Donner r\u00fchren;", "tokens": ["Dich", "soll", "auch", "kein", "Don\u00b7ner", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und ein immergr\u00fcnes Laub", "tokens": ["und", "ein", "im\u00b7mer\u00b7gr\u00fc\u00b7nes", "Laub"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "deine st\u00e4te Jugend zieren/", "tokens": ["dei\u00b7ne", "st\u00e4\u00b7te", "Ju\u00b7gend", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "keiner Winterwinde Raub.", "tokens": ["kei\u00b7ner", "Win\u00b7ter\u00b7win\u00b7de", "Raub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dafne/ liebste Dafne/ gr\u00fcne!", "tokens": ["Daf\u00b7ne", "/", "liebs\u00b7te", "Daf\u00b7ne", "/", "gr\u00fc\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NE", "$(", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "deine Zier/ mein Leid vers\u00fchne.", "tokens": ["dei\u00b7ne", "Zier", "/", "mein", "Leid", "ver\u00b7s\u00fch\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": 