{"textgrid.poem.66858": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ungebeugter Stolz der Haltung,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ungebeugter Stolz der Haltung,", "tokens": ["Un\u00b7ge\u00b7beug\u00b7ter", "Stolz", "der", "Hal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einfachheit in Wort und Kleid,", "tokens": ["Ein\u00b7fach\u00b7heit", "in", "Wort", "und", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Keine Phrasenprachtentfaltung", "tokens": ["Kei\u00b7ne", "Phra\u00b7sen\u00b7prach\u00b7tent\u00b7fal\u00b7tung"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Falscher Leidenschaftlichkeit;", "tokens": ["Fal\u00b7scher", "Lei\u00b7den\u00b7schaft\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kraft des Wissens in den Blicken,", "tokens": ["Kraft", "des", "Wis\u00b7sens", "in", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruhig redend, klaglos, klug \u2013", "tokens": ["Ru\u00b7hig", "re\u00b7dend", ",", "klag\u00b7los", ",", "klug", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "$,", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bbin die Gruben ihn zu schicken,", "tokens": ["\u00bb", "in", "die", "Gru\u00b7ben", "ihn", "zu", "schi\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist's genug?\u00ab der Richter frug.", "tokens": ["Ist's", "ge\u00b7nug", "?", "\u00ab", "der", "Rich\u00b7ter", "frug", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch die Welt des Elends schreiten,", "tokens": ["Durch", "die", "Welt", "des", "E\u00b7lends", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiten durch der Knechte Nacht,", "tokens": ["Schrei\u00b7ten", "durch", "der", "Knech\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wort der Wahrheit zu verbreiten", "tokens": ["Wort", "der", "Wahr\u00b7heit", "zu", "ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wider Macht der Niedertracht;", "tokens": ["Wi\u00b7der", "Macht", "der", "Nie\u00b7der\u00b7tracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Licht in die Baracken werfen,", "tokens": ["Licht", "in", "die", "Ba\u00b7ra\u00b7cken", "wer\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Hand der Freiheit Buch \u2013", "tokens": ["In", "der", "Hand", "der", "Frei\u00b7heit", "Buch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbihn mit Ketten zu versch\u00e4rfen,", "tokens": ["\u00bb", "ihn", "mit", "Ket\u00b7ten", "zu", "ver\u00b7sch\u00e4r\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist's genug?\u00ab der Richter frug.", "tokens": ["Ist's", "ge\u00b7nug", "?", "\u00ab", "der", "Rich\u00b7ter", "frug", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freundschaft, Gl\u00fcck und Liebe lassen,", "tokens": ["Freund\u00b7schaft", ",", "Gl\u00fcck", "und", "Lie\u00b7be", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam wandeln \u00f6de Bahn,", "tokens": ["Ein\u00b7sam", "wan\u00b7deln", "\u00f6\u00b7de", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Br\u00fcder stumpfe Massen", "tokens": ["Sei\u00b7ner", "Br\u00fc\u00b7der", "stump\u00b7fe", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wecken aus dem starren Wahn;", "tokens": ["We\u00b7cken", "aus", "dem", "star\u00b7ren", "Wahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den Eltern scheu gemieden,", "tokens": ["Von", "den", "El\u00b7tern", "scheu", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der doch keinen Bruder schlug \u2013", "tokens": ["Der", "doch", "kei\u00b7nen", "Bru\u00b7der", "schlug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbihn dem M\u00f6rder anzuschmieden,", "tokens": ["\u00bb", "ihn", "dem", "M\u00f6r\u00b7der", "an\u00b7zu\u00b7schmie\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist's genug?\u00ab der Richter frug.", "tokens": ["Ist's", "ge\u00b7nug", "?", "\u00ab", "der", "Rich\u00b7ter", "frug", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}