{"textgrid.poem.28253": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lupula wil keinen lieben,", "tokens": ["Lu\u00b7pu\u00b7la", "wil", "kei\u00b7nen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Vernunfft zu sehr wil \u00fcben,", "tokens": ["Der", "Ver\u00b7nunfft", "zu", "sehr", "wil", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ihr besser der gef\u00e4llt,", "tokens": ["Weil", "ihr", "bes\u00b7ser", "der", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sich etwas thierisch stellt.", "tokens": ["Der", "sich", "et\u00b7was", "thie\u00b7risch", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der da kan wie Tauben hertzen,", "tokens": ["Der", "da", "kan", "wie", "Tau\u00b7ben", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der da kan wie Spatzen schertzen,", "tokens": ["Der", "da", "kan", "wie", "Spat\u00b7zen", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der wie Hanne buhlen kan,", "tokens": ["Der", "wie", "Han\u00b7ne", "buh\u00b7len", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "VMFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Ist f\u00fcr sie der rechte Man.", "tokens": ["Ist", "f\u00fcr", "sie", "der", "rech\u00b7te", "Man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}