{"dta.poem.2155": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Versuch, die Anst\u00f6\u00dfigkeit vielerley  \n Religionen zu heben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6pfer und Erhalter sey: kann unm\u00f6glich von ihm", "tokens": ["Sch\u00f6p\u00b7fer", "und", "Er\u00b7hal\u00b7ter", "sey", ":", "kann", "un\u00b7m\u00f6g\u00b7lich", "von", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "$.", "VMFIN", "ADJD", "APPR", "PPER"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "denken,", "tokens": ["den\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df er in ein irdisch Bild durch uns W\u00fcrmer einzu-", "tokens": ["Da\u00df", "er", "in", "ein", "ir\u00b7disch", "Bild", "durch", "uns", "W\u00fcr\u00b7mer", "ein\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJD", "NN", "APPR", "PPER", "NN", "TRUNC"], "meter": "----+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "schr\u00e4nken.", "tokens": ["schr\u00e4n\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Dieses widerspricht sich selbst. Betet man nun Bil-", "tokens": ["Die\u00b7ses", "wi\u00b7der\u00b7spricht", "sich", "selbst", ".", "Be\u00b7tet", "man", "nun", "Bil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "$.", "VVFIN", "PIS", "ADV", "TRUNC"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "der an;", "tokens": ["der", "an", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Wie es leider! doch geschicht, so wird kl\u00e4rlich darge-", "tokens": ["Wie", "es", "lei\u00b7der", "!", "doch", "ge\u00b7schicht", ",", "so", "wird", "kl\u00e4r\u00b7lich", "dar\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "$.", "ADV", "VVPP", "$,", "ADV", "VAFIN", "ADJD", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "than,", "tokens": ["than", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Da\u00df kein w\u00fcrdiger Begriff k\u00f6nn in einer solchen Seelen", "tokens": ["Da\u00df", "kein", "w\u00fcr\u00b7di\u00b7ger", "Be\u00b7griff", "k\u00f6nn", "in", "ei\u00b7ner", "sol\u00b7chen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Von der wahren Gottheit seyn; folglich mu\u00df dieselbe", "tokens": ["Von", "der", "wah\u00b7ren", "Got\u00b7theit", "seyn", ";", "folg\u00b7lich", "mu\u00df", "die\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAINF", "$.", "ADV", "VMFIN", "PDAT"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "fehlen,", "tokens": ["feh\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Blo\u00df aus ihrem Unverstand. Bosheit hat hier gar", "tokens": ["Blo\u00df", "aus", "ih\u00b7rem", "Un\u00b7ver\u00b7stand", ".", "Bos\u00b7heit", "hat", "hier", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "NN", "VAFIN", "ADV", "ADV"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "nicht statt:", "tokens": ["nicht", "statt", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Denn da die Natur in uns einen Trieb gepr\u00e4get hat,", "tokens": ["Denn", "da", "die", "Na\u00b7tur", "in", "uns", "ei\u00b7nen", "Trieb", "ge\u00b7pr\u00e4\u00b7get", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Unser Wohlseyn zu bef\u00f6rdern; w\u00fcrd\u2019 auf dieser ganzen", "tokens": ["Un\u00b7ser", "Wohl\u00b7seyn", "zu", "be\u00b7f\u00f6r\u00b7dern", ";", "w\u00fcrd'", "auf", "die\u00b7ser", "gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$.", "VAFIN", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Erden", "tokens": ["Er\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Unter allen Menschenkindern nie ein Mensch gefunden", "tokens": ["Un\u00b7ter", "al\u00b7len", "Men\u00b7schen\u00b7kin\u00b7dern", "nie", "ein", "Mensch", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "werden,", "tokens": ["wer\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Der, mit Vorsatz, falsche G\u00f6tter ehren wollte; h\u00e4tt\u2019 er", "tokens": ["Der", ",", "mit", "Vor\u00b7satz", ",", "fal\u00b7sche", "G\u00f6t\u00b7ter", "eh\u00b7ren", "woll\u00b7te", ";", "h\u00e4tt'", "er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "APPR", "NN", "$,", "ADJA", "NN", "VVINF", "VMFIN", "$.", "VAFIN", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "nur", "tokens": ["nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Von der wahren Gottheit Wesen eine fest\u2019 und sichre", "tokens": ["Von", "der", "wah\u00b7ren", "Got\u00b7theit", "We\u00b7sen", "ei\u00b7ne", "fest'", "und", "sich\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "ART", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "Spur:", "tokens": ["Spur", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Denn mit Flei\u00df sich zu verdammen, leiden die zu starke", "tokens": ["Denn", "mit", "Flei\u00df", "sich", "zu", "ver\u00b7dam\u00b7men", ",", "lei\u00b7den", "die", "zu", "star\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "PTKZU", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "Triebe", "tokens": ["Trie\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Unsrer von Natur in uns eingepr\u00e4gten Eigenliebe,", "tokens": ["Uns\u00b7rer", "von", "Na\u00b7tur", "in", "uns", "ein\u00b7ge\u00b7pr\u00e4g\u00b7ten", "Ei\u00b7gen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Wie erweislich, nimmermehr. Also folgt, da\u00df in der", "tokens": ["Wie", "er\u00b7weis\u00b7lich", ",", "nim\u00b7mer\u00b7mehr", ".", "Al\u00b7so", "folgt", ",", "da\u00df", "in", "der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "ADV", "$.", "ADV", "VVFIN", "$,", "KOUS", "APPR", "ART"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "That", "tokens": ["That"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "An dem falsch- und G\u00f6tzendienst blo\u00df die Dummheit An-", "tokens": ["An", "dem", "falsch", "und", "G\u00f6t\u00b7zen\u00b7dienst", "blo\u00df", "die", "Dumm\u00b7heit", "An"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "ADV", "ART", "NN", "TRUNC"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "theil hat.", "tokens": ["theil", "hat", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Sage nicht: es giebt doch Pfaffen, die so falsche Dienst\u2019", "tokens": ["Sa\u00b7ge", "nicht", ":", "es", "giebt", "doch", "Pfaf\u00b7fen", ",", "die", "so", "fal\u00b7sche", "Dienst'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.31": {"text": "erfunden:", "tokens": ["er\u00b7fun\u00b7den", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}