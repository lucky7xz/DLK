{"textgrid.poem.66975": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Individuell", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trat in keinen Orden,", "tokens": ["Ich", "trat", "in", "kei\u00b7nen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "R\u00fchm' mich nicht Heid' noch Christ,", "tokens": ["R\u00fchm'", "mich", "nicht", "Heid'", "noch", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zog doch auf eignen Wegen", "tokens": ["Zog", "doch", "auf", "eig\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Liebesheil entgegen,", "tokens": ["Dem", "Lie\u00b7bes\u00b7heil", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dadurch ein stiller Segen", "tokens": ["Da\u00b7durch", "ein", "stil\u00b7ler", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein s\u00fc\u00dfer Schatz geworden ist.", "tokens": ["Mein", "s\u00fc\u00b7\u00dfer", "Schatz", "ge\u00b7wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}