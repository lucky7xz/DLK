{"textgrid.poem.40984": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die Puppe", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh die Raup' in ihrer Puppe", "tokens": ["Sieh", "die", "Raup'", "in", "ih\u00b7rer", "Pup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stillem, dunklem Schattenreich,", "tokens": ["Stil\u00b7lem", ",", "dunk\u00b7lem", "Schat\u00b7ten\u00b7reich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun getrennt von den Genossen,", "tokens": ["Nun", "ge\u00b7trennt", "von", "den", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einzig in sich selbst verschlossen,", "tokens": ["Ein\u00b7zig", "in", "sich", "selbst", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tot nicht, ob begraben gleich,", "tokens": ["Tot", "nicht", ",", "ob", "be\u00b7gra\u00b7ben", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaut nicht mehr den Tau der Triften,", "tokens": ["Schaut", "nicht", "mehr", "den", "Tau", "der", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Bl\u00fct' und Kr\u00e4uter bar,", "tokens": ["Ist", "der", "Bl\u00fct'", "und", "Kr\u00e4u\u00b7ter", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4nzlich nur sich selbst gegeben,", "tokens": ["G\u00e4nz\u00b7lich", "nur", "sich", "selbst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt sie das vergangne Leben", "tokens": ["Tr\u00e4gt", "sie", "das", "ver\u00b7gang\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In sich als ein P\u00fcnktchen klar.", "tokens": ["In", "sich", "als", "ein", "P\u00fcnkt\u00b7chen", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und in solcher stillen Klause", "tokens": ["Und", "in", "sol\u00b7cher", "stil\u00b7len", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streift sich ab ihr Erdgewand,", "tokens": ["Streift", "sich", "ab", "ihr", "Erd\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reifen ihr die bunten Schwingen,", "tokens": ["Rei\u00b7fen", "ihr", "die", "bun\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sie einst als Psyche bringen", "tokens": ["Die", "sie", "einst", "als", "Psy\u00b7che", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelw\u00e4rts aus d\u00fcstrem Land.", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "aus", "d\u00fcst\u00b7rem", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh die Raup' in ihrer Puppe!", "tokens": ["Sieh", "die", "Raup'", "in", "ih\u00b7rer", "Pup\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glaube: da\u00df auch dich der Tod", "tokens": ["Glau\u00b7be", ":", "da\u00df", "auch", "dich", "der", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "ADV", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst nicht tr\u00e4gt mit Blitzesschnelle \u2013", "tokens": ["Einst", "nicht", "tr\u00e4gt", "mit", "Blit\u00b7zes\u00b7schnel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist dein Innres noch so helle \u2013", "tokens": ["Ist", "dein", "Inn\u00b7res", "noch", "so", "hel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In ein ew'ges Morgenrot.", "tokens": ["In", "ein", "ew'\u00b7ges", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}