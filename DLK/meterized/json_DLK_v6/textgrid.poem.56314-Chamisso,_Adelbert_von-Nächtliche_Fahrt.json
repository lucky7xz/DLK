{"textgrid.poem.56314": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Fahrt", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Purpur pranget der Abend,", "tokens": ["In", "Pur\u00b7pur", "pran\u00b7get", "der", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Landwind hebet schon an;", "tokens": ["Der", "Land\u00b7wind", "he\u00b7bet", "schon", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur Lustfahrt ladet der Fischer", "tokens": ["Zur", "Lust\u00b7fahrt", "la\u00b7det", "der", "Fi\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dich, M\u00e4dchen, in seinen Kahn. \u2013", "tokens": ["Dich", ",", "M\u00e4d\u00b7chen", ",", "in", "sei\u00b7nen", "Kahn", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Noch hei\u00dfer begehr ich selbander", "tokens": ["Noch", "hei\u00b7\u00dfer", "be\u00b7gehr", "ich", "sel\u00b7ban\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit dir zu fahren, als du.", "tokens": ["Mit", "dir", "zu", "fah\u00b7ren", ",", "als", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gib voll das Segel dem Winde,", "tokens": ["Gib", "voll", "das", "Se\u00b7gel", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es kommt zu steuern mir zu. \u2013", "tokens": ["Es", "kommt", "zu", "steu\u00b7ern", "mir", "zu", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Du steuerst zu k\u00fchn, o M\u00e4dchen,", "tokens": ["Du", "steu\u00b7erst", "zu", "k\u00fchn", ",", "o", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinaus in das offene Meer;", "tokens": ["Hin\u00b7aus", "in", "das", "of\u00b7fe\u00b7ne", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du trauest dem leichten Fahrzeug", "tokens": ["Du", "trau\u00b7est", "dem", "leich\u00b7ten", "Fahr\u00b7zeug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Bei hohen Wellen zu sehr. \u2013", "tokens": ["Bei", "ho\u00b7hen", "Wel\u00b7len", "zu", "sehr", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Mi\u00dftrauen sollt ich dem Fahrzeug?", "tokens": ["Mi\u00df\u00b7trau\u00b7en", "sollt", "ich", "dem", "Fahr\u00b7zeug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe dazu nicht Grund,", "tokens": ["Ich", "ha\u00b7be", "da\u00b7zu", "nicht", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PTKNEG", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die einst ich deiner Treue", "tokens": ["Die", "einst", "ich", "dei\u00b7ner", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Getrauet in b\u00f6ser Stund. \u2013", "tokens": ["Ge\u00b7trau\u00b7et", "in", "b\u00f6\u00b7ser", "Stund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Unsinnige, wende das Ruder!", "tokens": ["Un\u00b7sin\u00b7ni\u00b7ge", ",", "wen\u00b7de", "das", "Ru\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du bringest uns beide in Not;", "tokens": ["Du", "brin\u00b7gest", "uns", "bei\u00b7de", "in", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schon treiben der Wind und die Wellen", "tokens": ["Schon", "trei\u00b7ben", "der", "Wind", "und", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ihr Spiel mit dem schwachen Boot. \u2013", "tokens": ["Ihr", "Spiel", "mit", "dem", "schwa\u00b7chen", "Boot", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "La\u00df treiben den Wind und die Wellen", "tokens": ["La\u00df", "trei\u00b7ben", "den", "Wind", "und", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit diesen Brettern ihr Spiel;", "tokens": ["Mit", "die\u00b7sen", "Bret\u00b7tern", "ihr", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hinweg mit Rudern und Segel,", "tokens": ["Hin\u00b7weg", "mit", "Ru\u00b7dern", "und", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hinweg! ich bin am Ziel.", "tokens": ["Hin\u00b7weg", "!", "ich", "bin", "am", "Ziel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie du mich einst, so hab ich", "tokens": ["Wie", "du", "mich", "einst", ",", "so", "hab", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich heut zu verderben ber\u00fcckt;", "tokens": ["Dich", "heut", "zu", "ver\u00b7der\u00b7ben", "be\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVFIN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mach Frieden mit dem Himmel,", "tokens": ["Mach", "Frie\u00b7den", "mit", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn siehe, der Dolch ist gez\u00fcckt.", "tokens": ["Denn", "sie\u00b7he", ",", "der", "Dolch", "ist", "ge\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Du zitterst, verworfner Betr\u00fcger,", "tokens": ["Du", "zit\u00b7terst", ",", "ver\u00b7worf\u00b7ner", "Be\u00b7tr\u00fc\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vor dieses Messers Schein?", "tokens": ["Vor", "die\u00b7ses", "Mes\u00b7sers", "Schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verratene Treue schneidet", "tokens": ["Ver\u00b7ra\u00b7te\u00b7ne", "Treu\u00b7e", "schnei\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch sch\u00e4rfer ins Herz hinein.", "tokens": ["Noch", "sch\u00e4r\u00b7fer", "ins", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Und manche betrogene Buhle", "tokens": ["Und", "man\u00b7che", "be\u00b7tro\u00b7ge\u00b7ne", "Buh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "H\u00e4rmt stille zu Tode sich:", "tokens": ["H\u00e4rmt", "stil\u00b7le", "zu", "To\u00b7de", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "PRF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wei\u00df nur, mich r\u00e4chend, zu sterben,", "tokens": ["Ich", "wei\u00df", "nur", ",", "mich", "r\u00e4\u00b7chend", ",", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Weh \u00fcber dich und mich! \u2013", "tokens": ["Weh", "\u00fc\u00b7ber", "dich", "und", "mich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der J\u00fcngling rang die H\u00e4nde,", "tokens": ["Der", "J\u00fcng\u00b7ling", "rang", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der eigenen Schuld bewu\u00dft;", "tokens": ["Der", "ei\u00b7ge\u00b7nen", "Schuld", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie stie\u00df den Dolch in das Herz ihm,", "tokens": ["Sie", "stie\u00df", "den", "Dolch", "in", "das", "Herz", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und dann in die eigene Brust.", "tokens": ["Und", "dann", "in", "die", "ei\u00b7ge\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Es trieb ein Wrak an das Ufer", "tokens": ["Es", "trieb", "ein", "Wrak", "an", "das", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bei wiederkehrender Flut,", "tokens": ["Bei", "wie\u00b7der\u00b7keh\u00b7ren\u00b7der", "Flut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es lagen darauf zwei Leichen,", "tokens": ["Es", "la\u00b7gen", "da\u00b7rauf", "zwei", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gebadet in ihrem Blut.", "tokens": ["Ge\u00b7ba\u00b7det", "in", "ih\u00b7rem", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}