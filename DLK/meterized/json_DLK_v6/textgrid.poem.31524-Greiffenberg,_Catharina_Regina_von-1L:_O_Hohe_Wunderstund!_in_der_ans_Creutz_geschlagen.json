{"textgrid.poem.31524": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Hohe Wunderstund! in der ans Creutz geschlagen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Hohe Wunderstund! in der ans Creutz geschlagen", "tokens": ["O", "Ho\u00b7he", "Wun\u00b7der\u00b7stund", "!", "in", "der", "ans", "Creutz", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "ART", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Himmel-Herrscher wird. Der wahre Gottes Sohn /", "tokens": ["der", "Him\u00b7mel\u00b7Herr\u00b7scher", "wird", ".", "Der", "wah\u00b7re", "Got\u00b7tes", "Sohn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hie leidet an dem Holz. Der sonst in Gottes Thron", "tokens": ["hie", "lei\u00b7det", "an", "dem", "Holz", ".", "Der", "sonst", "in", "Got\u00b7tes", "Thron"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das Allmacht-Zepter f\u00fchrt / mu\u00df jetzt das Fluch-Holz tragen.", "tokens": ["das", "All\u00b7macht\u00b7Zep\u00b7ter", "f\u00fchrt", "/", "mu\u00df", "jetzt", "das", "Fluch\u00b7Holz", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Gottheit ist in ihm / doch nicht in Leidensplagen.", "tokens": ["Die", "Got\u00b7theit", "ist", "in", "ihm", "/", "doch", "nicht", "in", "Lei\u00b7den\u00b7spla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$(", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sonn wird nicht genetzt / scheint sie im Wasser schon.", "tokens": ["Die", "Sonn", "wird", "nicht", "ge\u00b7netzt", "/", "scheint", "sie", "im", "Was\u00b7ser", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$(", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Gottheit / gibt ihm Krafft / ist des Verdienstes Kron /", "tokens": ["Die", "Got\u00b7theit", "/", "gibt", "ihm", "Krafft", "/", "ist", "des", "Ver\u00b7diens\u00b7tes", "Kron", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "NN", "$(", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "erh\u00e4lt die Mensch-Natur / und l\u00e4st sie nicht verzagen.", "tokens": ["er\u00b7h\u00e4lt", "die", "Men\u00b7schNa\u00b7tur", "/", "und", "l\u00e4st", "sie", "nicht", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Glori-Strahlen sie einziehet / nicht die Krafft.", "tokens": ["Die", "Glo\u00b7ri\u00b7Strah\u00b7len", "sie", "ein\u00b7zie\u00b7het", "/", "nicht", "die", "Krafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$(", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er \u00e4ussert sich allein der Majest\u00e4t / Macht / W\u00fcrden /", "tokens": ["Er", "\u00e4us\u00b7sert", "sich", "al\u00b7lein", "der", "Ma\u00b7jes\u00b7t\u00e4t", "/", "Macht", "/", "W\u00fcr\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$(", "NN", "$(", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und seiner Gottheit nicht: die ihm die Krafft verschafft /", "tokens": ["und", "sei\u00b7ner", "Got\u00b7theit", "nicht", ":", "die", "ihm", "die", "Krafft", "ver\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$.", "ART", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "da\u00df Er vollziehen kan die innern Lieb-Begierden.", "tokens": ["da\u00df", "Er", "voll\u00b7zie\u00b7hen", "kan", "die", "in\u00b7nern", "Lie\u00b7bBe\u00b7gier\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie ist der Gold-Geist / der das schwere Leidens-Bley /", "tokens": ["Sie", "ist", "der", "Gold\u00b7Geist", "/", "der", "das", "schwe\u00b7re", "Lei\u00b7dens\u00b7Bley", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "ART", "ADJA", "NN", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.14": {"text": "vor Gott Goldg\u00fcltig macht / da\u00df es Erl\u00f6sung sey.", "tokens": ["vor", "Gott", "Gold\u00b7g\u00fcl\u00b7tig", "macht", "/", "da\u00df", "es", "Er\u00b7l\u00f6\u00b7sung", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "VVFIN", "$(", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}