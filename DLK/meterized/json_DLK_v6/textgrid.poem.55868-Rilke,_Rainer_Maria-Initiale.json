{"textgrid.poem.55868": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Initiale", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gieb deine Sch\u00f6nheit immer hin", "tokens": ["Gieb", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "im\u00b7mer", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ohne Rechnen und Reden.", "tokens": ["oh\u00b7ne", "Rech\u00b7nen", "und", "Re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Du schweigst. Sie sagt f\u00fcr dich: Ich bin.", "tokens": ["Du", "schweigst", ".", "Sie", "sagt", "f\u00fcr", "dich", ":", "Ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kommt in tausendfachem Sinn,", "tokens": ["Und", "kommt", "in", "tau\u00b7send\u00b7fa\u00b7chem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "kommt endlich \u00fcber jeden.", "tokens": ["kommt", "end\u00b7lich", "\u00fc\u00b7ber", "je\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}