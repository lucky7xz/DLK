{"textgrid.poem.57016": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Fingur", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lacht die Nachtalp-Henne,", "tokens": ["Es", "lacht", "die", "Nachtal\u00b7pHen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "es weint die Windhorn-Gans,", "tokens": ["es", "weint", "die", "Wind\u00b7horn\u00b7Gans", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "es bl\u00e4st der schwarze Senne", "tokens": ["es", "bl\u00e4st", "der", "schwar\u00b7ze", "Sen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "zum Tanz.", "tokens": ["zum", "Tanz", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ein Uhu-Tauber turtelt", "tokens": ["Ein", "U\u00b7hu\u00b7Tau\u00b7ber", "tur\u00b7telt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nach seiner Uhuin.", "tokens": ["nach", "sei\u00b7ner", "U\u00b7hu\u00b7in", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein kleiner Sechs-Elf hurtelt", "tokens": ["Ein", "klei\u00b7ner", "Sechs\u00b7Elf", "hur\u00b7telt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "von Busch zu Busch dahin ...", "tokens": ["von", "Busch", "zu", "Busch", "da\u00b7hin", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und Wiederg\u00e4nger gehen,", "tokens": ["Und", "Wie\u00b7der\u00b7g\u00e4n\u00b7ger", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und Raben rufen kolk,", "tokens": ["und", "Ra\u00b7ben", "ru\u00b7fen", "kolk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und aus den Teichen sehen", "tokens": ["und", "aus", "den", "Tei\u00b7chen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Fingur und ihr Volk ...", "tokens": ["die", "Fin\u00b7gur", "und", "ihr", "Volk", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}