{"textgrid.poem.39433": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie so gern in deinen Hallen", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so gern in deinen Hallen", "tokens": ["Wie", "so", "gern", "in", "dei\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh' ich aus von fernem Wallen,", "tokens": ["Ruh'", "ich", "aus", "von", "fer\u00b7nem", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Altes, ritterliches Bern!", "tokens": ["Al\u00b7tes", ",", "rit\u00b7ter\u00b7li\u00b7ches", "Bern", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werth, den Namen wohl zu tragen", "tokens": ["Werth", ",", "den", "Na\u00b7men", "wohl", "zu", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jener Burg, wo nach den Sagen", "tokens": ["Je\u00b7ner", "Burg", ",", "wo", "nach", "den", "Sa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einst gewohnt der Helden Kern.", "tokens": ["Einst", "ge\u00b7wohnt", "der", "Hel\u00b7den", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber ach! was mu\u00df begegnen?", "tokens": ["A\u00b7ber", "ach", "!", "was", "mu\u00df", "be\u00b7geg\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PWS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dreifach werd' ich nun dich segnen,", "tokens": ["Drei\u00b7fach", "werd'", "ich", "nun", "dich", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich fand ein Kleinod hier.", "tokens": ["Denn", "ich", "fand", "ein", "Klei\u00b7nod", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du umgiebst viel sch\u00f6ne Frauen,", "tokens": ["Du", "um\u00b7giebst", "viel", "sch\u00f6\u00b7ne", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bilder, herrlich anzuschauen:", "tokens": ["Bil\u00b7der", ",", "herr\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine wohnt im Herzen mir.", "tokens": ["Ei\u00b7ne", "wohnt", "im", "Her\u00b7zen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie vernimmt den Dichter sinnig,", "tokens": ["Sie", "ver\u00b7nimmt", "den", "Dich\u00b7ter", "sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie empfindet zart und innig,", "tokens": ["Sie", "emp\u00b7fin\u00b7det", "zart", "und", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die hohe Kunst erschuf.", "tokens": ["Was", "die", "ho\u00b7he", "Kunst", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie erweckt mir neue Lieder,", "tokens": ["Sie", "er\u00b7weckt", "mir", "neu\u00b7e", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des Genius Gefieder", "tokens": ["Und", "des", "Ge\u00b7nius", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Regt sich ihrem sanften Ruf.", "tokens": ["Regt", "sich", "ih\u00b7rem", "sanf\u00b7ten", "Ruf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was die Vorzeit stark gesungen,", "tokens": ["Was", "die", "Vor\u00b7zeit", "stark", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit Jahrhunderten verklungen,", "tokens": ["Seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Las ich ihr, die Wunderm\u00e4r:", "tokens": ["Las", "ich", "ihr", ",", "die", "Wun\u00b7der\u00b7m\u00e4r", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die nordische Brunhilde,", "tokens": ["Wie", "die", "nor\u00b7di\u00b7sche", "Brun\u00b7hil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00e4mpfend unter Speer und Schilde,", "tokens": ["K\u00e4mp\u00b7fend", "un\u00b7ter", "Speer", "und", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bot der Minne Gegenwehr.", "tokens": ["Bot", "der", "Min\u00b7ne", "Ge\u00b7gen\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie indessen, statt der Lanze,", "tokens": ["Sie", "in\u00b7des\u00b7sen", ",", "statt", "der", "Lan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchrt die Nadel, stickt zum Kranze", "tokens": ["F\u00fchrt", "die", "Na\u00b7del", ",", "stickt", "zum", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen in ein leicht Gewand.", "tokens": ["Blu\u00b7men", "in", "ein", "leicht", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht umpanzert tr\u00e4gt die Holde", "tokens": ["Nicht", "um\u00b7pan\u00b7zert", "tr\u00e4gt", "die", "Hol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Ihren Busen, nur mit Golde", "tokens": ["Ih\u00b7ren", "Bu\u00b7sen", ",", "nur", "mit", "Gol\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Finger ihrer Hand.", "tokens": ["Ei\u00b7nen", "Fin\u00b7ger", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dennoch wei\u00df sie zu verwunden,", "tokens": ["Den\u00b7noch", "wei\u00df", "sie", "zu", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich mu\u00df an mir erkunden", "tokens": ["Und", "ich", "mu\u00df", "an", "mir", "er\u00b7kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenes Liedes Leid und Lust.", "tokens": ["Je\u00b7nes", "Lie\u00b7des", "Leid", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die k\u00fchnsten Helden stritten,", "tokens": ["Was", "die", "k\u00fchns\u00b7ten", "Hel\u00b7den", "strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sie freute, was sie litten,", "tokens": ["Was", "sie", "freu\u00b7te", ",", "was", "sie", "lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kam aus edler Frauen Brust.", "tokens": ["Kam", "aus", "ed\u00b7ler", "Frau\u00b7en", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ob auch Kampf die Dichter sangen,", "tokens": ["Ob", "auch", "Kampf", "die", "Dich\u00b7ter", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob die Lieder krieg'risch klangen", "tokens": ["Ob", "die", "Lie\u00b7der", "krieg'\u00b7risch", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie Trompet' und H\u00f6rnerton:", "tokens": ["Wie", "Trom\u00b7pet'", "und", "H\u00f6r\u00b7ner\u00b7ton", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch ein L\u00e4cheln bl\u00fch'nder Wangen,", "tokens": ["Doch", "ein", "L\u00e4\u00b7cheln", "bl\u00fch'n\u00b7der", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frischer Lippen Gru\u00df empfangen", "tokens": ["Fri\u00b7scher", "Lip\u00b7pen", "Gru\u00df", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollten sie zum sch\u00f6nsten Lohn.", "tokens": ["Woll\u00b7ten", "sie", "zum", "sch\u00f6ns\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihrer Augen blaues Leuchten", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "blau\u00b7es", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich oft mit Wehmuth feuchten", "tokens": ["Sah", "ich", "oft", "mit", "Weh\u00b7muth", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Chriemhilds Trauer, Sifrids Tod.", "tokens": ["Chriem\u00b7hilds", "Trau\u00b7er", ",", "Sif\u00b7rids", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! so kann sie Mitleid hegen", "tokens": ["Ach", "!", "so", "kann", "sie", "Mit\u00b7leid", "he\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit des Herzens bangen Schl\u00e4gen,", "tokens": ["Mit", "des", "Her\u00b7zens", "ban\u00b7gen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seiner hoffnungslosen Noth.", "tokens": ["Sei\u00b7ner", "hoff\u00b7nungs\u00b7lo\u00b7sen", "Noth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Zwar verstummt die k\u00fchne Bitte", "tokens": ["Zwar", "ver\u00b7stummt", "die", "k\u00fch\u00b7ne", "Bit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der zarten stolzen Sitte,", "tokens": ["Vor", "der", "zar\u00b7ten", "stol\u00b7zen", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die kaum Huldigung erlaubt:", "tokens": ["Die", "kaum", "Hul\u00b7di\u00b7gung", "er\u00b7laubt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "M\u00f6chte, wenn die W\u00fcnsche schweigen,", "tokens": ["M\u00f6ch\u00b7te", ",", "wenn", "die", "W\u00fcn\u00b7sche", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich nur linde zu mir neigen", "tokens": ["Sich", "nur", "lin\u00b7de", "zu", "mir", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJA", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses reich umlockte Haupt!", "tokens": ["Die\u00b7ses", "reich", "um\u00b7lock\u00b7te", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "D\u00fcrft' ich dieser Blume warten,", "tokens": ["D\u00fcrft'", "ich", "die\u00b7ser", "Blu\u00b7me", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so einsam steht im Garten,", "tokens": ["Die", "so", "ein\u00b7sam", "steht", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schirmen sie vor rauhem Nord,", "tokens": ["Schir\u00b7men", "sie", "vor", "rau\u00b7hem", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie bethau'n mit meinen Thr\u00e4nen,", "tokens": ["Sie", "be\u00b7thau'n", "mit", "mei\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie umweh'n mit leisem Sehnen!", "tokens": ["Sie", "um\u00b7weh'n", "mit", "lei\u00b7sem", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch mein Schicksal rei\u00dft mich fort.", "tokens": ["Doch", "mein", "Schick\u00b7sal", "rei\u00dft", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Goldne Tr\u00e4ume meiner Jugend,", "tokens": ["Gold\u00b7ne", "Tr\u00e4u\u00b7me", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches Reizes Zaubertugend", "tokens": ["Wel\u00b7ches", "Rei\u00b7zes", "Zau\u00b7ber\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rief euch s\u00fc\u00df und schmerzlich auf?", "tokens": ["Rief", "euch", "s\u00fc\u00df", "und", "schmerz\u00b7lich", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebet wohl! Ich mu\u00df nun scheiden,", "tokens": ["Le\u00b7bet", "wohl", "!", "Ich", "mu\u00df", "nun", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df mich in Entsagung kleiden", "tokens": ["Mu\u00df", "mich", "in", "Ent\u00b7sa\u00b7gung", "klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu des Lebens strengerm Lauf.", "tokens": ["Zu", "des", "Le\u00b7bens", "stren\u00b7germ", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}