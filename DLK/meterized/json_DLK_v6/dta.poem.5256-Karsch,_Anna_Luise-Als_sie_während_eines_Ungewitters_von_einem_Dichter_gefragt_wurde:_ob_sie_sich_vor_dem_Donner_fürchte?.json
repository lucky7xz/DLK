{"dta.poem.5256": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als sie w\u00e4hrend eines Ungewitters  \n von einem Dichter gefragt wurde: ob  \n sie sich vor dem Donner f\u00fcrchte?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Zevs schilt im Wolkenhimmel,", "tokens": ["Zevs", "schilt", "im", "Wol\u00b7ken\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein lauter Donner sprichts!", "tokens": ["Sein", "lau\u00b7ter", "Don\u00b7ner", "sprichts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schilt dem Kriegsget\u00fcmmel,", "tokens": ["Er", "schilt", "dem", "Kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Dichtern thut er nichts.", "tokens": ["Den", "Dich\u00b7tern", "thut", "er", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}