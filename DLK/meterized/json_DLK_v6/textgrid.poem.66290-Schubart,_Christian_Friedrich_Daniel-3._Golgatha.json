{"textgrid.poem.66290": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "3. Golgatha", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seele, hast du keine Fl\u00fcgel?", "tokens": ["See\u00b7le", ",", "hast", "du", "kei\u00b7ne", "Fl\u00fc\u00b7gel", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So fliege doch nach Golgatha,", "tokens": ["So", "flie\u00b7ge", "doch", "nach", "Gol\u00b7ga\u00b7tha", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo auf einem Todesh\u00fcgel", "tokens": ["Wo", "auf", "ei\u00b7nem", "To\u00b7des\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Sohn der Vater leiden sah.", "tokens": ["Den", "Sohn", "der", "Va\u00b7ter", "lei\u00b7den", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Erde zittert,", "tokens": ["Die", "Er\u00b7de", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Schaut und ersch\u00fcttert", "tokens": ["Schaut", "und", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Den Tod, den gro\u00dfen Tod!", "tokens": ["Den", "Tod", ",", "den", "gro\u00b7\u00dfen", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der dem Mittler Gottes droht.", "tokens": ["Der", "dem", "Mitt\u00b7ler", "Got\u00b7tes", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geister stehen auf den H\u00f6hen,", "tokens": ["Geis\u00b7ter", "ste\u00b7hen", "auf", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Todte bleich, wie Gr\u00e4ber stumm!", "tokens": ["Wie", "Tod\u00b7te", "bleich", ",", "wie", "Gr\u00e4\u00b7ber", "stumm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,", "PWAV", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die wen'gen Edlen stehen", "tokens": ["Und", "die", "wen'\u00b7gen", "Ed\u00b7len", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohnm\u00e4chtig um den Pfahl herum;", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "um", "den", "Pfahl", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sehn und schauen", "tokens": ["Sie", "sehn", "und", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den Tod voll Grauen,", "tokens": ["Den", "Tod", "voll", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Den Tod, den gro\u00dfen Tod!", "tokens": ["Den", "Tod", ",", "den", "gro\u00b7\u00dfen", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der dem besten Freunde droht.", "tokens": ["Der", "dem", "bes\u00b7ten", "Freun\u00b7de", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nacht und Dunkel h\u00e4ngt herunter,", "tokens": ["Nacht", "und", "Dun\u00b7kel", "h\u00e4ngt", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Moria, wo ist deine Pracht?", "tokens": ["Mo\u00b7ria", ",", "wo", "ist", "dei\u00b7ne", "Pracht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wo ist deines Tempels Wunder?", "tokens": ["Wo", "ist", "dei\u00b7nes", "Tem\u00b7pels", "Wun\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deckt alles Tod und Mitternacht?", "tokens": ["Deckt", "al\u00b7les", "Tod", "und", "Mit\u00b7ter\u00b7nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Berge zittern,", "tokens": ["Die", "Ber\u00b7ge", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Felsen splittern;", "tokens": ["Die", "Fel\u00b7sen", "split\u00b7tern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "O Tod, o gro\u00dfer Tod!", "tokens": ["O", "Tod", ",", "o", "gro\u00b7\u00dfer", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der dem S\u00fcndentilger droht.", "tokens": ["Der", "dem", "S\u00fcn\u00b7den\u00b7til\u00b7ger", "droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus der f\u00fcrchterlichsten Wolke", "tokens": ["Aus", "der", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7ten", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erhebt die Todesstimme sich", "tokens": ["Er\u00b7hebt", "die", "To\u00b7dess\u00b7tim\u00b7me", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dem zitterenden Volke:", "tokens": ["Vor", "dem", "zit\u00b7te\u00b7ren\u00b7den", "Vol\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Gott! warum verl\u00e4\u00df'st du mich?", "tokens": ["Mein", "Gott", "!", "wa\u00b7rum", "ver\u00b7l\u00e4\u00df'st", "du", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom H\u00f6llengrimme", "tokens": ["Vom", "H\u00f6l\u00b7len\u00b7grim\u00b7me"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zeugt diese Stimme;", "tokens": ["Zeugt", "die\u00b7se", "Stim\u00b7me", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Tod! o welch ein Tod!", "tokens": ["O", "Tod", "!", "o", "welch", "ein", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der dem gr\u00f6\u00dften Menschen droht.", "tokens": ["Der", "dem", "gr\u00f6\u00df\u00b7ten", "Men\u00b7schen", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Blutigrothe Strahlen z\u00fccken", "tokens": ["Blu\u00b7ti\u00b7gro\u00b7the", "Strah\u00b7len", "z\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von eines Todesengels Schwert,", "tokens": ["Von", "ei\u00b7nes", "To\u00b7de\u00b7sen\u00b7gels", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geister h\u00f6ren, staunen, blicken,", "tokens": ["Geis\u00b7ter", "h\u00f6\u00b7ren", ",", "stau\u00b7nen", ",", "bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als sie das letzte Wort geh\u00f6rt:", "tokens": ["Als", "sie", "das", "letz\u00b7te", "Wort", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun ich empfehle", "tokens": ["Nun", "ich", "emp\u00b7feh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dir meine Seele!", "tokens": ["Dir", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Gott, es ist vollbracht!", "tokens": ["O", "Gott", ",", "es", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und sein Haupt sinkt in die Nacht.", "tokens": ["Und", "sein", "Haupt", "sinkt", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tief an deinem Kreuze unten,", "tokens": ["Tief", "an", "dei\u00b7nem", "Kreu\u00b7ze", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottmensch! Erl\u00f6ser! lieg' ich hier.", "tokens": ["Gott\u00b7mensch", "!", "Er\u00b7l\u00f6\u00b7ser", "!", "lieg'", "ich", "hier", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich blick' hinauf nach deinen Wunden,", "tokens": ["Ich", "blick'", "hin\u00b7auf", "nach", "dei\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie str\u00f6men Seligkeit auch mir.", "tokens": ["Sie", "str\u00f6\u00b7men", "Se\u00b7lig\u00b7keit", "auch", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will Tod mich t\u00f6dten,", "tokens": ["Will", "Tod", "mich", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "So soll es reden,", "tokens": ["So", "soll", "es", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dein Blut, Gottmensch, dein Blut!", "tokens": ["Dein", "Blut", ",", "Gott\u00b7mensch", ",", "dein", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und ich trotze seiner Wuth.", "tokens": ["Und", "ich", "trot\u00b7ze", "sei\u00b7ner", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O wie freudig kann ich sterben!", "tokens": ["O", "wie", "freu\u00b7dig", "kann", "ich", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich f\u00fcrchte nicht der H\u00f6lle Gluth;", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nicht", "der", "H\u00f6l\u00b7le", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meine Kleider will ich f\u00e4rben", "tokens": ["Mei\u00b7ne", "Klei\u00b7der", "will", "ich", "f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des erw\u00fcrgten Lammes Blut.", "tokens": ["In", "des", "er\u00b7w\u00fcrg\u00b7ten", "Lam\u00b7mes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch ich empfehle", "tokens": ["Auch", "ich", "emp\u00b7feh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dir meine Seele,", "tokens": ["Dir", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Gott! wenn einst der Tod", "tokens": ["O", "Gott", "!", "wenn", "einst", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mir, wie meinem Mittler droht.", "tokens": ["Mir", ",", "wie", "mei\u00b7nem", "Mitt\u00b7ler", "droht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}