{"textgrid.poem.34777": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh'mals glaubt ich, alle K\u00fcsse,", "tokens": ["Eh'\u00b7mals", "glaubt", "ich", ",", "al\u00b7le", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ein Weib uns gibt und nimmt,", "tokens": ["Die", "ein", "Weib", "uns", "gibt", "und", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seien uns, durch Schicksalsschl\u00fcsse,", "tokens": ["Sei\u00b7en", "uns", ",", "durch", "Schick\u00b7sals\u00b7schl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon urzeitlich vorbestimmt.", "tokens": ["Schon", "ur\u00b7zeit\u00b7lich", "vor\u00b7be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00fcsse nahm ich, und ich k\u00fc\u00dfte", "tokens": ["K\u00fcs\u00b7se", "nahm", "ich", ",", "und", "ich", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So mit Ernst in jener Zeit,", "tokens": ["So", "mit", "Ernst", "in", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ob ich erf\u00fcllen m\u00fc\u00dfte", "tokens": ["Als", "ob", "ich", "er\u00b7f\u00fcl\u00b7len", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Taten der Notwendigkeit.", "tokens": ["Ta\u00b7ten", "der", "Not\u00b7wen\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Jetzo wei\u00df ich, \u00fcberfl\u00fcssig,", "tokens": ["Jet\u00b7zo", "wei\u00df", "ich", ",", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so manches, ist der Ku\u00df,", "tokens": ["Wie", "so", "man\u00b7ches", ",", "ist", "der", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit leichtern Sinnen k\u00fc\u00df ich,", "tokens": ["Und", "mit", "leich\u00b7tern", "Sin\u00b7nen", "k\u00fc\u00df", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaubenlos im \u00dcberflu\u00df.", "tokens": ["Glau\u00b7ben\u00b7los", "im", "\u00dc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}