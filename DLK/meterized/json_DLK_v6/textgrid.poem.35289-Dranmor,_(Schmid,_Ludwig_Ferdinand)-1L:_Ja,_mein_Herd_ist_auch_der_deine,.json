{"textgrid.poem.35289": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, mein Herd ist auch der deine,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, mein Herd ist auch der deine,", "tokens": ["Ja", ",", "mein", "Herd", "ist", "auch", "der", "dei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Armes, heimatloses Kind!", "tokens": ["Ar\u00b7mes", ",", "hei\u00b7mat\u00b7lo\u00b7ses", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du liebst mich nicht zum Scheine,", "tokens": ["Denn", "du", "liebst", "mich", "nicht", "zum", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn du liebst mich treu und blind.", "tokens": ["Denn", "du", "liebst", "mich", "treu", "und", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, die Welt war ohne Gnade,", "tokens": ["Ach", ",", "die", "Welt", "war", "oh\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Mitleid und Verstand;", "tokens": ["Oh\u00b7ne", "Mit\u00b7leid", "und", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch durch dornenlose Pfade", "tokens": ["Doch", "durch", "dor\u00b7nen\u00b7lo\u00b7se", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchr' ich dich an meiner Hand.", "tokens": ["F\u00fchr'", "ich", "dich", "an", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was du wolltest, ist geschehen;", "tokens": ["Was", "du", "woll\u00b7test", ",", "ist", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "That ich mehr als Menschenpflicht?", "tokens": ["That", "ich", "mehr", "als", "Men\u00b7schen\u00b7pflicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bitten konnt' ich widerstehen,", "tokens": ["Bit\u00b7ten", "konnt'", "ich", "wi\u00b7der\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber deinen Thr\u00e4nen nicht.", "tokens": ["A\u00b7ber", "dei\u00b7nen", "Thr\u00e4\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bilder aus vergangnen Tagen", "tokens": ["Bil\u00b7der", "aus", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thun mir in der Seele weh,", "tokens": ["Thun", "mir", "in", "der", "See\u00b7le", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nur zitternd kann ich's sagen:", "tokens": ["Und", "nur", "zit\u00b7ternd", "kann", "ich's", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibe hier, mein wildes Reh!", "tokens": ["Blei\u00b7be", "hier", ",", "mein", "wil\u00b7des", "Reh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ruh' dich aus auf gr\u00fcner Weide,", "tokens": ["Ruh'", "dich", "aus", "auf", "gr\u00fc\u00b7ner", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denke, schaue nicht zur\u00fcck;", "tokens": ["Den\u00b7ke", ",", "schau\u00b7e", "nicht", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du geh\u00f6rst zu meinem Leide,", "tokens": ["Du", "ge\u00b7h\u00f6rst", "zu", "mei\u00b7nem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du geh\u00f6rst zu meinem Gl\u00fcck.", "tokens": ["Du", "ge\u00b7h\u00f6rst", "zu", "mei\u00b7nem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df wir gut zusammentaugen,", "tokens": ["Da\u00df", "wir", "gut", "zu\u00b7sam\u00b7men\u00b7tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df das Rechte wir erw\u00e4hlt,", "tokens": ["Da\u00df", "das", "Rech\u00b7te", "wir", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben deiner braunen Augen", "tokens": ["Ha\u00b7ben", "dei\u00b7ner", "brau\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwere Perlen mir erz\u00e4hlt.", "tokens": ["Schwe\u00b7re", "Per\u00b7len", "mir", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O, wie flogst du mir entgegen,", "tokens": ["O", ",", "wie", "flogst", "du", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie kindlich war dein Ruf,", "tokens": ["Und", "wie", "kind\u00b7lich", "war", "dein", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du nachts durch Wind und Regen", "tokens": ["Wenn", "du", "nachts", "durch", "Wind", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rtest meines Rosses Huf!", "tokens": ["H\u00f6r\u00b7test", "mei\u00b7nes", "Ros\u00b7ses", "Huf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wie kann ich's je beschreiben,", "tokens": ["Und", "wie", "kann", "ich's", "je", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mein Herz f\u00fcr dich gef\u00fchlt,", "tokens": ["Was", "mein", "Herz", "f\u00fcr", "dich", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend an den Fensterscheiben", "tokens": ["W\u00e4h\u00b7rend", "an", "den", "Fens\u00b7ter\u00b7schei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du die hei\u00dfe Stirn gek\u00fchlt!", "tokens": ["Du", "die", "hei\u00b7\u00dfe", "Stirn", "ge\u00b7k\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lachen mag die Welt, die schlimme,", "tokens": ["La\u00b7chen", "mag", "die", "Welt", ",", "die", "schlim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber den gez\u00e4hmten Leu;", "tokens": ["Ue\u00b7ber", "den", "ge\u00b7z\u00e4hm\u00b7ten", "Leu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gerne folgt er deiner Stimme,", "tokens": ["Ger\u00b7ne", "folgt", "er", "dei\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn du liebst ihn blind und treu.", "tokens": ["Denn", "du", "liebst", "ihn", "blind", "und", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und bei ihm bist du geborgen;", "tokens": ["Und", "bei", "ihm", "bist", "du", "ge\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gastlich ist sein Haus, und still", "tokens": ["Gast\u00b7lich", "ist", "sein", "Haus", ",", "und", "still"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr sein armes Kind zu sorgen,", "tokens": ["F\u00fcr", "sein", "ar\u00b7mes", "Kind", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist alles, was er will.", "tokens": ["Das", "ist", "al\u00b7les", ",", "was", "er", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}