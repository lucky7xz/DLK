{"textgrid.poem.35907": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl gerne mag ich Veilchen pfl\u00fccken", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl gerne mag ich Veilchen pfl\u00fccken", "tokens": ["Wohl", "ger\u00b7ne", "mag", "ich", "Veil\u00b7chen", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mich an ihrem Duft erfreun,", "tokens": ["Und", "mich", "an", "ih\u00b7rem", "Duft", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl gerne mich mit Rosen schm\u00fccken", "tokens": ["Wohl", "ger\u00b7ne", "mich", "mit", "Ro\u00b7sen", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Nelken und Vergi\u00dfnichtmein.", "tokens": ["Und", "Nel\u00b7ken", "und", "Ver\u00b7gi\u00df\u00b7nicht\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl gerne seh' ich Lilien gl\u00e4nzen,", "tokens": ["Wohl", "ger\u00b7ne", "seh'", "ich", "Li\u00b7li\u00b7en", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie sie so klar und herrlich stehn,", "tokens": ["Wie", "sie", "so", "klar", "und", "herr\u00b7lich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gern mag ich nach duft'gen Kr\u00e4nzen", "tokens": ["Und", "gern", "mag", "ich", "nach", "duft'\u00b7gen", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch blumenreiche Wiesen gehn.", "tokens": ["Durch", "blu\u00b7men\u00b7rei\u00b7che", "Wie\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, alle Blumen sind zu loben,", "tokens": ["Ja", ",", "al\u00b7le", "Blu\u00b7men", "sind", "zu", "lo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns der gute Vater schenkt,", "tokens": ["Die", "uns", "der", "gu\u00b7te", "Va\u00b7ter", "schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sonne giebt und Luft von droben", "tokens": ["Der", "Son\u00b7ne", "giebt", "und", "Luft", "von", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie mit Thau so milde tr\u00e4nkt.", "tokens": ["Und", "sie", "mit", "Thau", "so", "mil\u00b7de", "tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sind seit wenig Tagen Sterne", "tokens": ["Doch", "sind", "seit", "we\u00b7nig", "Ta\u00b7gen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erbl\u00fcht auf unserm Erdenrund,", "tokens": ["Er\u00b7bl\u00fcht", "auf", "un\u00b7serm", "Er\u00b7den\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die seh' ich, pfl\u00fcck' ich gar so gerne,", "tokens": ["Die", "seh'", "ich", ",", "pfl\u00fcck", "ich", "gar", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie stehn so freudig und so bunt.", "tokens": ["Sie", "stehn", "so", "freu\u00b7dig", "und", "so", "bunt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie stehn in Sehnsucht, stehn in Frieden", "tokens": ["Sie", "stehn", "in", "Sehn\u00b7sucht", ",", "stehn", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und blicken nach dem Sonnenschein;", "tokens": ["Und", "bli\u00b7cken", "nach", "dem", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Reich, ihr Schatz ist nicht hienieden,", "tokens": ["Ihr", "Reich", ",", "ihr", "Schatz", "ist", "nicht", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wollen sie die Welt erfreun.", "tokens": ["Doch", "wol\u00b7len", "sie", "die", "Welt", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie stehn in lichten Thauesthr\u00e4nen", "tokens": ["Sie", "stehn", "in", "lich\u00b7ten", "Thau\u00b7est\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Morgenroth, im Abendschein;", "tokens": ["Im", "Mor\u00b7gen\u00b7roth", ",", "im", "A\u00b7bend\u00b7schein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bl\u00fch'n mir Frieden, bl\u00fch'n mir Sehnen,", "tokens": ["Sie", "bl\u00fch'n", "mir", "Frie\u00b7den", ",", "bl\u00fch'n", "mir", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie bl\u00fchen mir in's Herz hinein.", "tokens": ["Sie", "bl\u00fc\u00b7hen", "mir", "in's", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie bl\u00fch'n in Sehnsucht, bl\u00fch'n in Frieden", "tokens": ["Sie", "bl\u00fch'n", "in", "Sehn\u00b7sucht", ",", "bl\u00fch'n", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Dir aus meiner Brust heraus;", "tokens": ["Zu", "Dir", "aus", "mei\u00b7ner", "Brust", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wollen meinen Gru\u00df Dir bieten:", "tokens": ["Sie", "wol\u00b7len", "mei\u00b7nen", "Gru\u00df", "Dir", "bie\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm, Schwester! hin den vollen Strau\u00df.", "tokens": ["Nimm", ",", "Schwes\u00b7ter", "!", "hin", "den", "vol\u00b7len", "Strau\u00df", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "ADV", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und siehst Du diese bunten Sterne,", "tokens": ["Und", "siehst", "Du", "die\u00b7se", "bun\u00b7ten", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So fl\u00fcstern sie Dir stets von mir,", "tokens": ["So", "fl\u00fcs\u00b7tern", "sie", "Dir", "stets", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und winken aus der weiten Ferne", "tokens": ["Und", "win\u00b7ken", "aus", "der", "wei\u00b7ten", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein liebevolles Gr\u00fc\u00dfen Dir.", "tokens": ["Mein", "lie\u00b7be\u00b7vol\u00b7les", "Gr\u00fc\u00b7\u00dfen", "Dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}