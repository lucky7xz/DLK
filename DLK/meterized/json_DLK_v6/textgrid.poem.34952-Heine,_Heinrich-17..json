{"textgrid.poem.34952": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Traume war ich wieder jung und munter \u2013", "tokens": ["Im", "Trau\u00b7me", "war", "ich", "wie\u00b7der", "jung", "und", "mun\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es war das Landhaus hoch am Bergesrand,", "tokens": ["Es", "war", "das", "Land\u00b7haus", "hoch", "am", "Ber\u00b7ges\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wettlaufend lief ich dort den Pfad hinunter,", "tokens": ["Wett\u00b7lau\u00b7fend", "lief", "ich", "dort", "den", "Pfad", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wettlaufend mit Ottilien Hand in Hand.", "tokens": ["Wett\u00b7lau\u00b7fend", "mit", "Ot\u00b7ti\u00b7li\u00b7en", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie das Pers\u00f6nchen fein formiert! Die s\u00fc\u00dfen", "tokens": ["Wie", "das", "Per\u00b7s\u00f6n\u00b7chen", "fein", "for\u00b7miert", "!", "Die", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVPP", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Meergr\u00fcnen Augen zwinkern nixenhaft.", "tokens": ["Meer\u00b7gr\u00fc\u00b7nen", "Au\u00b7gen", "zwin\u00b7kern", "ni\u00b7xen\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie steht so fest auf ihren kleinen F\u00fc\u00dfen,", "tokens": ["Sie", "steht", "so", "fest", "auf", "ih\u00b7ren", "klei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Bild von Zierlichkeit, vereint mit Kraft.", "tokens": ["Ein", "Bild", "von", "Zier\u00b7lich\u00b7keit", ",", "ver\u00b7eint", "mit", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Ton der Stimme ist so treu und innig,", "tokens": ["Der", "Ton", "der", "Stim\u00b7me", "ist", "so", "treu", "und", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man glaubt zu schaun bis in der Seele Grund;", "tokens": ["Man", "glaubt", "zu", "schaun", "bis", "in", "der", "See\u00b7le", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKZU", "VVINF", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alles, was sie spricht, ist klug und sinnig;", "tokens": ["Und", "al\u00b7les", ",", "was", "sie", "spricht", ",", "ist", "klug", "und", "sin\u00b7nig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Rosenknospe ist der Mund.", "tokens": ["Wie", "ei\u00b7ne", "Ro\u00b7sen\u00b7knos\u00b7pe", "ist", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist nicht Liebesweh, was mich beschleichet,", "tokens": ["Es", "ist", "nicht", "Lie\u00b7bes\u00b7weh", ",", "was", "mich", "be\u00b7schlei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schw\u00e4rme nicht, ich bleibe bei Verstand; \u2013", "tokens": ["Ich", "schw\u00e4r\u00b7me", "nicht", ",", "ich", "blei\u00b7be", "bei", "Ver\u00b7stand", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wunderbar ihr Wesen mich erweichet,", "tokens": ["Doch", "wun\u00b7der\u00b7bar", "ihr", "We\u00b7sen", "mich", "er\u00b7wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heimlich bebend k\u00fc\u00df ich ihre Hand.", "tokens": ["Und", "heim\u00b7lich", "be\u00b7bend", "k\u00fc\u00df", "ich", "ih\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich glaub, am Ende brach ich eine Lilie,", "tokens": ["Ich", "glaub", ",", "am", "En\u00b7de", "brach", "ich", "ei\u00b7ne", "Li\u00b7lie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die gab ich ihr und sprach ganz laut dabei:", "tokens": ["Die", "gab", "ich", "ihr", "und", "sprach", "ganz", "laut", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbheirate mich und sei mein Weib, Ottilie,", "tokens": ["\u00bb", "hei\u00b7ra\u00b7te", "mich", "und", "sei", "mein", "Weib", ",", "Ot\u00b7ti\u00b7lie", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "KON", "VAFIN", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Damit ich fromm wie du und gl\u00fccklich sei.\u00ab", "tokens": ["Da\u00b7mit", "ich", "fromm", "wie", "du", "und", "gl\u00fcck\u00b7lich", "sei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KOKOM", "PPER", "KON", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was sie zur Antwort gab, das wei\u00df ich nimmer,", "tokens": ["Was", "sie", "zur", "Ant\u00b7wort", "gab", ",", "das", "wei\u00df", "ich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn ich erwachte j\u00e4hlings \u2013 und ich war", "tokens": ["Denn", "ich", "er\u00b7wach\u00b7te", "j\u00e4h\u00b7lings", "\u2013", "und", "ich", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$(", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wieder ein Kranker, der im Krankenzimmer", "tokens": ["Wie\u00b7der", "ein", "Kran\u00b7ker", ",", "der", "im", "Kran\u00b7ken\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trostlos daniederliegt seit manchem Jahr. \u2013 \u2013", "tokens": ["Trost\u00b7los", "da\u00b7nie\u00b7der\u00b7liegt", "seit", "man\u00b7chem", "Jahr", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}