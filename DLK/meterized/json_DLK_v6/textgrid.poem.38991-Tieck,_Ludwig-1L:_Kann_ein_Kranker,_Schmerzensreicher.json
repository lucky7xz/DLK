{"textgrid.poem.38991": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kann ein Kranker, Schmerzensreicher", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann ein Kranker, Schmerzensreicher", "tokens": ["Kann", "ein", "Kran\u00b7ker", ",", "Schmer\u00b7zens\u00b7rei\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VMFIN", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne inn'ges Mitgef\u00fchl", "tokens": ["Oh\u00b7ne", "inn'\u00b7ges", "Mit\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Sturz der Riesen sehn?", "tokens": ["Die\u00b7sen", "Sturz", "der", "Rie\u00b7sen", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie zerschmettert,", "tokens": ["Wie", "sie", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In Bergen vergraben,", "tokens": ["In", "Ber\u00b7gen", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ohnm\u00e4chtig diese,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "die\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PDS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Jene noch k\u00e4mpfend,", "tokens": ["Je\u00b7ne", "noch", "k\u00e4mp\u00b7fend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sterbend der in stiller Wuth,", "tokens": ["Ster\u00b7bend", "der", "in", "stil\u00b7ler", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Rings die weite Landschaft f\u00fcllen?", "tokens": ["Rings", "die", "wei\u00b7te", "Land\u00b7schaft", "f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So m\u00e4chtig gro\u00df und wild,", "tokens": ["So", "m\u00e4ch\u00b7tig", "gro\u00df", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Als wenn aus ihren Gebeinen", "tokens": ["Als", "wenn", "aus", "ih\u00b7ren", "Ge\u00b7bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Die Felsen der Erde erw\u00fcchsen,", "tokens": ["Die", "Fel\u00b7sen", "der", "Er\u00b7de", "er\u00b7w\u00fcch\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Die dann noch in stummer Geberde", "tokens": ["Die", "dann", "noch", "in", "stum\u00b7mer", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Durch alle Jahrtausende", "tokens": ["Durch", "al\u00b7le", "Jahr\u00b7tau\u00b7sen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.15": {"text": "Dem Himmel dr\u00e4un.", "tokens": ["Dem", "Him\u00b7mel", "dr\u00e4un", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Oben die G\u00f6tter", "tokens": ["O\u00b7ben", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In Sorg' und in Kampf,", "tokens": ["In", "Sor\u00b7g'", "und", "in", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00fclfth\u00e4ltig jeder.", "tokens": ["H\u00fclf\u00b7th\u00e4l\u00b7tig", "je\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PIS", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nur ein schlauer Satyr", "tokens": ["Nur", "ein", "schlau\u00b7er", "Sa\u00b7tyr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Nimmt, in den Greuel der Verw\u00fcstung", "tokens": ["Nimmt", ",", "in", "den", "Greu\u00b7el", "der", "Ver\u00b7w\u00fcs\u00b7tung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Entsetzt hinunterschauend,", "tokens": ["Ent\u00b7setzt", "hin\u00b7un\u00b7ter\u00b7schau\u00b7end", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch die l\u00fcsterne Nymphe", "tokens": ["Noch", "die", "l\u00fcs\u00b7ter\u00b7ne", "Nym\u00b7phe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit ihr entfliehend:", "tokens": ["Mit", "ihr", "ent\u00b7flie\u00b7hend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mag Zeus nun siegen,", "tokens": ["Mag", "Zeus", "nun", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Die Titanen den Himmel st\u00fcrmen,", "tokens": ["Die", "Ti\u00b7ta\u00b7nen", "den", "Him\u00b7mel", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Er hat den Augenblick erobert.", "tokens": ["Er", "hat", "den", "Au\u00b7gen\u00b7blick", "er\u00b7o\u00b7bert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O k\u00fchner, zu k\u00fchner Julius!", "tokens": ["O", "k\u00fch\u00b7ner", ",", "zu", "k\u00fch\u00b7ner", "Ju\u00b7lius", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "APPR", "ADJA", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie verwegen hat deine Zauberhand", "tokens": ["Wie", "ver\u00b7we\u00b7gen", "hat", "dei\u00b7ne", "Zau\u00b7ber\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dies \u00fcbermenschliche Gedicht vollendet!", "tokens": ["Dies", "\u00fc\u00b7ber\u00b7menschli\u00b7che", "Ge\u00b7dicht", "voll\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und welche Anmuth, welche Frische,", "tokens": ["Und", "wel\u00b7che", "An\u00b7muth", ",", "wel\u00b7che", "Fri\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Welcher Liebreiz und stille Wonne,", "tokens": ["Wel\u00b7cher", "Lieb\u00b7reiz", "und", "stil\u00b7le", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Dort auf dem Lager Psyche's und Amor's.", "tokens": ["Dort", "auf", "dem", "La\u00b7ger", "Psy\u00b7che's", "und", "A\u00b7mor'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "KON", "NE", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "S\u00fc\u00df befriedigt", "tokens": ["S\u00fc\u00df", "be\u00b7frie\u00b7digt"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Ruht das beseligte Paar,", "tokens": ["Ruht", "das", "be\u00b7se\u00b7lig\u00b7te", "Paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Und reines Entz\u00fccken", "tokens": ["Und", "rei\u00b7nes", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Strahlt aus den reinen Formen", "tokens": ["Strahlt", "aus", "den", "rei\u00b7nen", "For\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Hell den Beschauer an.", "tokens": ["Hell", "den", "Be\u00b7schau\u00b7er", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und Centauren und wildes Ungethier,", "tokens": ["Und", "Cen\u00b7tau\u00b7ren", "und", "wil\u00b7des", "Un\u00b7ge\u00b7thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leichter Scherz und L\u00fcsternheit", "tokens": ["Und", "leich\u00b7ter", "Scherz", "und", "L\u00fcs\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht wie ein muthwilliges Gedicht", "tokens": ["Zieht", "wie", "ein", "mut\u00b7hwil\u00b7li\u00b7ges", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Durch alle Mauern des Palastes.", "tokens": ["Durch", "al\u00b7le", "Mau\u00b7ern", "des", "Pa\u00b7las\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ja wohl war dein edler Meister todt,", "tokens": ["Ja", "wohl", "war", "dein", "ed\u00b7ler", "Meis\u00b7ter", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und der ungezogne Liebling der Grazien,", "tokens": ["Und", "der", "un\u00b7ge\u00b7zog\u00b7ne", "Lieb\u00b7ling", "der", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Im eignen Uebermuth sich taumelnd,", "tokens": ["Im", "eig\u00b7nen", "Ue\u00b7ber\u00b7muth", "sich", "tau\u00b7melnd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hat Rafaels Genius", "tokens": ["Hat", "Ra\u00b7faels", "Ge\u00b7nius"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NE", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Mit hei\u00dfem Weine trunken gemacht,", "tokens": ["Mit", "hei\u00b7\u00dfem", "Wei\u00b7ne", "trun\u00b7ken", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Und mehr als begeistert", "tokens": ["Und", "mehr", "als", "be\u00b7geis\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "VVFIN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.11": {"text": "Schw\u00e4rmen die bacchantischen Bilder", "tokens": ["Schw\u00e4r\u00b7men", "die", "bac\u00b7chan\u00b7ti\u00b7schen", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.12": {"text": "Tobend, jubelnd umher,", "tokens": ["To\u00b7bend", ",", "ju\u00b7belnd", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Eigenwillig bei Paukenklang,", "tokens": ["Ei\u00b7gen\u00b7wil\u00b7lig", "bei", "Pau\u00b7ken\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Mit Cymbelnget\u00f6n", "tokens": ["Mit", "Cym\u00b7beln\u00b7ge\u00b7t\u00f6n"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Die Gr\u00e4nze des Parnassus \u00fcberschreitend.", "tokens": ["Die", "Gr\u00e4n\u00b7ze", "des", "Par\u00b7nas\u00b7sus", "\u00fc\u00b7bersc\u00b7hrei\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch alle Musen l\u00e4cheln", "tokens": ["Doch", "al\u00b7le", "Mu\u00b7sen", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Von oben herab,", "tokens": ["Von", "o\u00b7ben", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Und die Grazien sinnend", "tokens": ["Und", "die", "Gra\u00b7zi\u00b7en", "sin\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Wenden sich halb,", "tokens": ["Wen\u00b7den", "sich", "halb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PRF", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Doch leuchtet ihr heller Blick,", "tokens": ["Doch", "leuch\u00b7tet", "ihr", "hel\u00b7ler", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Ohne Tadel und Mi\u00dfmuth", "tokens": ["Oh\u00b7ne", "Ta\u00b7del", "und", "Mi\u00df\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Ungetr\u00fcbt auf die frische Lebensdichtung.", "tokens": ["Un\u00b7ge\u00b7tr\u00fcbt", "auf", "die", "fri\u00b7sche", "Le\u00b7bens\u00b7dich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}