{"textgrid.poem.29423": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df wo durch verme\u00dfnen Artzt ist ein Krancker doch genesen,", "tokens": ["Da\u00df", "wo", "durch", "ver\u00b7me\u00df\u00b7nen", "Artzt", "ist", "ein", "Kran\u00b7cker", "doch", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kan wol seyn; doch wird es nicht Kunst und Regel zugelesen.", "tokens": ["Kan", "wol", "seyn", ";", "doch", "wird", "es", "nicht", "Kunst", "und", "Re\u00b7gel", "zu\u00b7ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VAINF", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}}}}}