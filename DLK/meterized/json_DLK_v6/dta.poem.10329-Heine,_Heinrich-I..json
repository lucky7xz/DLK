{"dta.poem.10329": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Morgens steh ich auf und frage:", "tokens": ["Mor\u00b7gens", "steh", "ich", "auf", "und", "fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt feins Liebchen heut?", "tokens": ["Kommt", "feins", "Lieb\u00b7chen", "heut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Abends sink' ich hin und klage:", "tokens": ["A\u00b7bends", "sink'", "ich", "hin", "und", "kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausblieb sie auch heut.", "tokens": ["Aus\u00b7blieb", "sie", "auch", "heut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "In der Nacht mit meinem Kummer", "tokens": ["In", "der", "Nacht", "mit", "mei\u00b7nem", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieg ich schlaflos, wach;", "tokens": ["Lieg", "ich", "schlaf\u00b7los", ",", "wach", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADJD", 