{"textgrid.poem.59268": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wo ein t\u00fcchtig Leben,", "tokens": ["Und", "wo", "ein", "t\u00fcch\u00b7tig", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wo ein Ehrenhaus,", "tokens": ["Und", "wo", "ein", "Eh\u00b7ren\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da geht der S\u00e4nger eben", "tokens": ["Da", "geht", "der", "S\u00e4n\u00b7ger", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gern gastlich ein und aus.", "tokens": ["Gern", "gast\u00b7lich", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der freudige Geselle", "tokens": ["Der", "freu\u00b7di\u00b7ge", "Ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00fc\u00dft Pfaff und Rittersmann,", "tokens": ["Gr\u00fc\u00dft", "Pfaff", "und", "Rit\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und frische Morgenhelle", "tokens": ["Und", "fri\u00b7sche", "Mor\u00b7gen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weht all' im Liede an.", "tokens": ["Weht", "all'", "im", "Lie\u00b7de", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und k\u00fchn im Rossesb\u00fcgel", "tokens": ["Und", "k\u00fchn", "im", "Ros\u00b7ses\u00b7b\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Ritter waldw\u00e4rts zieht,", "tokens": ["Der", "Rit\u00b7ter", "wald\u00b7w\u00e4rts", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und das Gebet nimmt Fl\u00fcgel", "tokens": ["Und", "das", "Ge\u00b7bet", "nimmt", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und \u00fcberfliegt das Lied.", "tokens": ["Und", "\u00fc\u00b7berf\u00b7liegt", "das", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn ob's mit Schwert, mit Liedern", "tokens": ["Denn", "ob's", "mit", "Schwert", ",", "mit", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich Bahn zum Himmel schafft;", "tokens": ["Sich", "Bahn", "zum", "Him\u00b7mel", "schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "'s ist ", "tokens": ["'s", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Wo die vereint, da ranken", "tokens": ["Wo", "die", "ver\u00b7eint", ",", "da", "ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich willig Stein und Erz,", "tokens": ["Sich", "wil\u00b7lig", "Stein", "und", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da pfeilern die Gedanken", "tokens": ["Da", "pfei\u00b7lern", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich freudig himmelw\u00e4rts.", "tokens": ["Sich", "freu\u00b7dig", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Die haben diese Bogen", "tokens": ["Die", "ha\u00b7ben", "die\u00b7se", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "K\u00fchn \u00fcbern wilden Strom", "tokens": ["K\u00fchn", "\u00fc\u00b7bern", "wil\u00b7den", "Strom"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Emp\u00f6rter Zeit gezogen", "tokens": ["Em\u00b7p\u00f6r\u00b7ter", "Zeit", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum wunderbaren Dom.", "tokens": ["Zum", "wun\u00b7der\u00b7ba\u00b7ren", "Dom", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Burgen sahn wir fallen,", "tokens": ["Die", "Bur\u00b7gen", "sahn", "wir", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Adler zogen aus,", "tokens": ["Die", "Ad\u00b7ler", "zo\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wehklagend durch die Hallen", "tokens": ["Weh\u00b7kla\u00b7gend", "durch", "die", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gehn Winde ein und aus.", "tokens": ["Gehn", "Win\u00b7de", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch droben auf der Zinne", "tokens": ["Doch", "dro\u00b7ben", "auf", "der", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht noch der Heldengeist", "tokens": ["Steht", "noch", "der", "Hel\u00b7den\u00b7geist"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der \u2013 was die Zeit beginne \u2013", "tokens": ["Der", "\u2013", "was", "die", "Zeit", "be\u00b7gin\u00b7ne", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Still nach dem Kreuze weist.", "tokens": ["Still", "nach", "dem", "Kreu\u00b7ze", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Es wechseln viel Geschlechter", "tokens": ["Es", "wech\u00b7seln", "viel", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sinken in die Nacht \u2013", "tokens": ["Und", "sin\u00b7ken", "in", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steh fest, du treuer W\u00e4chter,", "tokens": ["Steh", "fest", ",", "du", "treu\u00b7er", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimm dein Land in acht!", "tokens": ["Und", "nimm", "dein", "Land", "in", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Schon hat zum Kreuzeslichte", "tokens": ["Schon", "hat", "zum", "Kreu\u00b7zes\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Volk sich ernst gewandt,", "tokens": ["Dein", "Volk", "sich", "ernst", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Sturm der Weltgerichte", "tokens": ["Im", "Sturm", "der", "Welt\u00b7ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief schauernd dich erkannt.", "tokens": ["Tief", "schau\u00b7ernd", "dich", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nun hebt sich wieder fr\u00f6hlich", "tokens": ["Nun", "hebt", "sich", "wie\u00b7der", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Haus im Morgenschein,", "tokens": ["Dein", "Haus", "im", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfrau minneselig", "tokens": ["Die", "Jung\u00b7frau", "min\u00b7ne\u00b7se\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaut weit ins Land hinein.", "tokens": ["Schaut", "weit", "ins", "Land", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ges\u00e4nge h\u00f6r ich schallen,", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "h\u00f6r", "ich", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchs Gr\u00fcn geschm\u00fcckte G\u00e4st", "tokens": ["Durchs", "Gr\u00fcn", "ge\u00b7schm\u00fcck\u00b7te", "G\u00e4st"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wallfahrten nach den Hallen \u2013", "tokens": ["Wall\u00b7fahr\u00b7ten", "nach", "den", "Hal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wem gilt das frohe Fest?", "tokens": ["Wem", "gilt", "das", "fro\u00b7he", "Fest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der K\u00f6nigssohn, ihr Preu\u00dfen,", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7sohn", ",", "ihr", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weilt auf dem Ritterschlo\u00df,", "tokens": ["Weilt", "auf", "dem", "Rit\u00b7ter\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das ist nach Adlers Weisen,", "tokens": ["Das", "ist", "nach", "Ad\u00b7lers", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er der H\u00f6h Geno\u00df.", "tokens": ["Da\u00df", "er", "der", "H\u00f6h", "Ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Das ist des K\u00f6nigs Walten,", "tokens": ["Das", "ist", "des", "K\u00f6\u00b7nigs", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was herrlich, gro\u00df und recht,", "tokens": ["Was", "herr\u00b7lich", ",", "gro\u00df", "und", "recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Wechsel zu erhalten", "tokens": ["Im", "Wech\u00b7sel", "zu", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem kommenden Geschlecht.", "tokens": ["Dem", "kom\u00b7men\u00b7den", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Er hob die Heldenmale", "tokens": ["Er", "hob", "die", "Hel\u00b7den\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu neuer Herrlichkeit,", "tokens": ["Zu", "neu\u00b7er", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit das Volk im Tale", "tokens": ["Da\u00b7mit", "das", "Volk", "im", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedenk der gro\u00dfen Zeit.", "tokens": ["Ge\u00b7denk", "der", "gro\u00b7\u00dfen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Das ewig Alt und Neue,", "tokens": ["Das", "e\u00b7wig", "Alt", "und", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mit den Zeiten ringt,", "tokens": ["Das", "mit", "den", "Zei\u00b7ten", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, F\u00fcrst, ist's, was das treue", "tokens": ["Das", ",", "F\u00fcrst", ",", "ist's", ",", "was", "das", "treu\u00b7e"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "NN", "$,", "VAFIN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herz deines Volks durchdringt.", "tokens": ["Herz", "dei\u00b7nes", "Volks", "durch\u00b7dringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.17": {"line.1": {"text": "Wo das noch ehrlich waltet,", "tokens": ["Wo", "das", "noch", "ehr\u00b7lich", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ist zu Gottes Ruhm", "tokens": ["Da", "ist", "zu", "Got\u00b7tes", "Ruhm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kreuzesfahn entfaltet,", "tokens": ["Die", "Kreu\u00b7zes\u00b7fahn", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und rechtes Rittertum.", "tokens": ["Und", "rech\u00b7tes", "Rit\u00b7ter\u00b7tum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Oh, reicht dem Liedersprecher,", "tokens": ["Oh", ",", "reicht", "dem", "Lie\u00b7der\u00b7spre\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bevor er scheiden mu\u00df,", "tokens": ["Be\u00b7vor", "er", "schei\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den hochgef\u00fcllten Becher", "tokens": ["Den", "hoch\u00b7ge\u00b7f\u00fcll\u00b7ten", "Be\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu seinem besten Gru\u00df!", "tokens": ["Zu", "sei\u00b7nem", "bes\u00b7ten", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Doch einzeln nicht verhallen", "tokens": ["Doch", "ein\u00b7zeln", "nicht", "ver\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darf, was ich jetzt gedacht.", "tokens": ["Darf", ",", "was", "ich", "jetzt", "ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was jeder meint, von ", "tokens": ["Was", "je\u00b7der", "meint", ",", "von"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "PIS", "VVFIN", "$,", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sei's freudig auch gebracht!", "tokens": ["Sei's", "freu\u00b7dig", "auch", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "All ritterliche Geister", "tokens": ["All", "rit\u00b7ter\u00b7li\u00b7che", "Geis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umringen fest den Thron,", "tokens": ["Um\u00b7rin\u00b7gen", "fest", "den", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf zum h\u00f6chsten Meister", "tokens": ["Und", "auf", "zum", "h\u00f6chs\u00b7ten", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dringt treuer Liebe Ton:", "tokens": ["Dringt", "treu\u00b7er", "Lie\u00b7be", "Ton", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Dem ritterlichen ", "tokens": ["Dem", "rit\u00b7ter\u00b7li\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Heil, und dem ", "tokens": ["Heil", ",", "und", "dem"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "ART"], "meter": "+--", "measure": "dactylic.init"}}}}}