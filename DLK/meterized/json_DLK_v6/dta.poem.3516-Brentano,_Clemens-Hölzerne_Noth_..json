{"dta.poem.3516": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6lzerne Noth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "S h\u00e4tt sich mol ener zu mer welle k\u00fcpple,               ", "tokens": ["S", "h\u00e4tt", "sich", "mol", "e\u00b7ner", "zu", "mer", "wel\u00b7le", "k\u00fcpp\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "H\u00e4ts L\u00e4del n'in welle kr\u00fcpple,", "tokens": ["H\u00e4ts", "L\u00e4\u00b7del", "n'\u00b7in", "wel\u00b7le", "kr\u00fcpp\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Un als er maint er stoht,", "tokens": ["Un", "als", "er", "maint", "er", "stoht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Heb ich de H\u00e4nd bl\u00fcmbe loth,", "tokens": ["Heb", "ich", "de", "H\u00e4nd", "bl\u00fcm\u00b7be", "loth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Dazu uf et Mist,", "tokens": ["Da\u00b7zu", "uf", "et", "Mist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "D\u00fc H\u00f6nd der de bist,", "tokens": ["D\u00fc", "H\u00f6nd", "der", "de", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NE", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Jetz westa was Gosategeh ist.", "tokens": ["Jetz", "wes\u00b7ta", "was", "Go\u00b7sa\u00b7te\u00b7geh", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Er h\u00e4tt mi au mol zum Tanz welle f\u00fchre,", "tokens": ["Er", "h\u00e4tt", "mi", "au", "mol", "zum", "Tanz", "wel\u00b7le", "f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NE", "NE", "APPRART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4tt welle mit mer agire,", "tokens": ["H\u00e4tt", "wel\u00b7le", "mit", "mer", "a\u00b7gi\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.3": {"text": "Er tanzt wie e B\u00e4r so toll,", "tokens": ["Er", "tanzt", "wie", "e", "B\u00e4r", "so", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mer m\u00e4nt er h\u00e4tt sich g'soffe voll,", "tokens": ["Mer", "m\u00e4nt", "er", "h\u00e4tt", "sich", "g'\u00b7sof\u00b7fe", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VAFIN", "PRF", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der w\u00fcst Sapperlot,", "tokens": ["Der", "w\u00fcst", "Sap\u00b7per\u00b7lot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Er tanzt wie er goht,", "tokens": ["Er", "tanzt", "wie", "er", "goht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "'s is glatt e h\u00f6lzerne Noth.", "tokens": ["'s", "is", "glatt", "e", "h\u00f6l\u00b7zer\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "FM", "FM", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Was w\u00e4re min Kamerade sahe", "tokens": ["Was", "w\u00e4\u00b7re", "min", "Ka\u00b7me\u00b7ra\u00b7de", "sa\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn i so e Stumpe nehm?", "tokens": ["Wenn", "i", "so", "e", "Stum\u00b7pe", "nehm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gieng i mit am \u00fcber d'Ga\u00df,", "tokens": ["Gieng", "i", "mit", "am", "\u00fc\u00b7ber", "d'\u00b7Ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "APPRART", "APPR", "PDS", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "D' Lit h\u00e4tte de gr\u00f6\u00dfte Gespa\u00df,", "tokens": ["D'", "Lit", "h\u00e4t\u00b7te", "de", "gr\u00f6\u00df\u00b7te", "Ge\u00b7spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "NE", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Mit er solche Crot,", "tokens": ["Mit", "er", "sol\u00b7che", "Crot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "'s w\u00e4r e Schand un e Gespott.", "tokens": ["'s", "w\u00e4r", "e", "Schand", "un", "e", "Ge\u00b7spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "FM", "FM", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}}}}