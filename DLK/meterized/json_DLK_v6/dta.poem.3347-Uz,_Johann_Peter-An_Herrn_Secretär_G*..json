{"dta.poem.3347": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r G*.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Der ehmals Hymens Heimlichkeiten", "tokens": ["Der", "eh\u00b7mals", "Hy\u00b7mens", "Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ieden losen Streich, den Amor ihm gespielt,", "tokens": ["Und", "ie\u00b7den", "lo\u00b7sen", "Streich", ",", "den", "A\u00b7mor", "ihm", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "ART", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In seine scherzgewohnten Saiten", "tokens": ["In", "sei\u00b7ne", "scherz\u00b7ge\u00b7wohn\u00b7ten", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So reizend sang, da\u00df wer nur menschlich f\u00fchlt,", "tokens": ["So", "rei\u00b7zend", "sang", ",", "da\u00df", "wer", "nur", "menschlich", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KOUS", "PWS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nach Hymens Freuden diebisch schielt;", "tokens": ["Nach", "Hy\u00b7mens", "Freu\u00b7den", "die\u00b7bisch", "schielt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}