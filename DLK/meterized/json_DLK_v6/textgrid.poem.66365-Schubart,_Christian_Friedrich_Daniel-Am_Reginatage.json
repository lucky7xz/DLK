{"textgrid.poem.66365": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Am Reginatage", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Regina, sieh, vom Himmel schaut", "tokens": ["Re\u00b7gi\u00b7na", ",", "sieh", ",", "vom", "Him\u00b7mel", "schaut"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "APPRART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dein Tag, geschm\u00fcckt, wie eine Braut;", "tokens": ["Dein", "Tag", ",", "ge\u00b7schm\u00fcckt", ",", "wie", "ei\u00b7ne", "Braut", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So l\u00e4chelnd, wie der Fr\u00fchling ist,", "tokens": ["So", "l\u00e4\u00b7chelnd", ",", "wie", "der", "Fr\u00fch\u00b7ling", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Flora seine Wangen k\u00fc\u00dft.", "tokens": ["Wenn", "Flo\u00b7ra", "sei\u00b7ne", "Wan\u00b7gen", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich, Regina, flieg' zu dir,", "tokens": ["Und", "ich", ",", "Re\u00b7gi\u00b7na", ",", "flieg'", "zu", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und lisple dir am Goldklavier", "tokens": ["Und", "lisp\u00b7le", "dir", "am", "Gold\u00b7kla\u00b7vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die W\u00fcnsche meines Herzens zu,", "tokens": ["Die", "W\u00fcn\u00b7sche", "mei\u00b7nes", "Her\u00b7zens", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freudeweinend singst sie du.", "tokens": ["Und", "freu\u00b7de\u00b7wei\u00b7nend", "singst", "sie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als dich die Mutter einst gebar,", "tokens": ["Als", "dich", "die", "Mut\u00b7ter", "einst", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trat zu der Wiege unsichtbar", "tokens": ["Trat", "zu", "der", "Wie\u00b7ge", "un\u00b7sicht\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Genius im Lichtgewand", "tokens": ["Dein", "Ge\u00b7nius", "im", "Licht\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dr\u00fcckte dir die kleine Hand.", "tokens": ["Und", "dr\u00fcck\u00b7te", "dir", "die", "klei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann sprach er: Holdes Schwesterlein,", "tokens": ["Dann", "sprach", "er", ":", "Hol\u00b7des", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Regina soll dein Name sein.", "tokens": ["Re\u00b7gi\u00b7na", "soll", "dein", "Na\u00b7me", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Doch siehst du einst im Himmel mich,", "tokens": ["Doch", "siehst", "du", "einst", "im", "Him\u00b7mel", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So nenn' ich Serafina dich.", "tokens": ["So", "nenn'", "ich", "Se\u00b7ra\u00b7fi\u00b7na", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gott gab dir geistiges Gef\u00fchl,", "tokens": ["Gott", "gab", "dir", "geis\u00b7ti\u00b7ges", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Menschensang und Fl\u00fcgelspiel,", "tokens": ["Und", "Men\u00b7schen\u00b7sang", "und", "Fl\u00fc\u00b7gel\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, trautes M\u00e4dchen! mehr, als dies,", "tokens": ["Und", ",", "trau\u00b7tes", "M\u00e4d\u00b7chen", "!", "mehr", ",", "als", "dies", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$.", "ADV", "$,", "KOUS", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Herz, gemacht f\u00fcr's Paradies.", "tokens": ["Ein", "Herz", ",", "ge\u00b7macht", "f\u00fcr's", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O, da\u00df die faule Sinnlichkeit", "tokens": ["O", ",", "da\u00df", "die", "fau\u00b7le", "Sinn\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie diese reine Seel' entweiht,", "tokens": ["Nie", "die\u00b7se", "rei\u00b7ne", "Seel'", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df, gewiegt von Gotteshuld,", "tokens": ["Und", "da\u00df", ",", "ge\u00b7wiegt", "von", "Got\u00b7tes\u00b7huld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Herz beflecke keine Schuld!", "tokens": ["Dein", "Herz", "be\u00b7fle\u00b7cke", "kei\u00b7ne", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df Lieb' und Freundschaft dich erf\u00fcll',", "tokens": ["Da\u00df", "Lieb'", "und", "Freund\u00b7schaft", "dich", "er\u00b7f\u00fcll'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df dich der Unschuld Seide h\u00fcll'!", "tokens": ["Da\u00df", "dich", "der", "Un\u00b7schuld", "Sei\u00b7de", "h\u00fcll'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df alle deine Freude rein,", "tokens": ["Da\u00df", "al\u00b7le", "dei\u00b7ne", "Freu\u00b7de", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst deine W\u00fcnsche heilig sein!", "tokens": ["Selbst", "dei\u00b7ne", "W\u00fcn\u00b7sche", "hei\u00b7lig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gott gab mich dir zum Genius,", "tokens": ["Gott", "gab", "mich", "dir", "zum", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Regina, nimm den Bruderku\u00df", "tokens": ["Re\u00b7gi\u00b7na", ",", "nimm", "den", "Bru\u00b7der\u00b7ku\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und walle stets an meiner Hand!", "tokens": ["Und", "wal\u00b7le", "stets", "an", "mei\u00b7ner", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sprach der Engel und verschwand.", "tokens": ["So", "sprach", "der", "En\u00b7gel", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}