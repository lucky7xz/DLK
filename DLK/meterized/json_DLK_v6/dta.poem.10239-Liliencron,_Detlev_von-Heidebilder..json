{"dta.poem.10239": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Heidebilder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "In Herbstestagen bricht mit starkem Fl\u00fcgel", "tokens": ["In", "Herbs\u00b7tes\u00b7ta\u00b7gen", "bricht", "mit", "star\u00b7kem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Reiher durch den Nebelduft.", "tokens": ["Der", "Rei\u00b7her", "durch", "den", "Ne\u00b7bel\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie still es ist, kaum h\u00f6r\u2019 ich um den H\u00fcgel", "tokens": ["Wie", "still", "es", "ist", ",", "kaum", "h\u00f6r'", "ich", "um", "den", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch einen Laut in weiter Luft.", "tokens": ["Noch", "ei\u00b7nen", "Laut", "in", "wei\u00b7ter", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf eines Birkenst\u00e4mmchens schwanker Krone", "tokens": ["Auf", "ei\u00b7nes", "Bir\u00b7ken\u00b7st\u00e4mm\u00b7chens", "schwan\u00b7ker", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruht sich ein Wanderfalke aus.", "tokens": ["Ruht", "sich", "ein", "Wan\u00b7der\u00b7fal\u00b7ke", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schl\u00e4ft er nicht, von seinem leichten Throne", "tokens": ["Doch", "schl\u00e4ft", "er", "nicht", ",", "von", "sei\u00b7nem", "leich\u00b7ten", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aeugt er durchdringend scharf hinaus.", "tokens": ["A\u00b7e\u00b7ugt", "er", "durch\u00b7drin\u00b7gend", "scharf", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Der alte Bauer mit verhaltnem Schritte", "tokens": ["Der", "al\u00b7te", "Bau\u00b7er", "mit", "ver\u00b7halt\u00b7nem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schleicht neben seinem Wagen Torf.", "tokens": ["Schleicht", "ne\u00b7ben", "sei\u00b7nem", "Wa\u00b7gen", "Torf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und holpernd, stolpernd schleppt mit lahmem Tritte", "tokens": ["Und", "hol\u00b7pernd", ",", "stol\u00b7pernd", "schleppt", "mit", "lah\u00b7mem", "Trit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alte Schimmel ihn in\u2019s Dorf.", "tokens": ["Der", "al\u00b7te", "Schim\u00b7mel", "ihn", "in's", "Dorf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}