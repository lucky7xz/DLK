{"textgrid.poem.44164": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bricht, mein Patron, die Noth sonst Eisen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bricht, mein Patron, die Noth sonst Eisen,", "tokens": ["Bricht", ",", "mein", "Pat\u00b7ron", ",", "die", "Noth", "sonst", "Ei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So siehstu meiner Grobheit nach;", "tokens": ["So", "sieh\u00b7stu", "mei\u00b7ner", "Grob\u00b7heit", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du weist wohl, wenn wir Dichter reisen,", "tokens": ["Du", "weist", "wohl", ",", "wenn", "wir", "Dich\u00b7ter", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So schwindet stets das beste Fach.", "tokens": ["So", "schwin\u00b7det", "stets", "das", "bes\u00b7te", "Fach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich h\u00e4tt es selbst dir klagen k\u00f6nnen;", "tokens": ["Ich", "h\u00e4tt", "es", "selbst", "dir", "kla\u00b7gen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Poeten haben gro\u00dfe Scham,", "tokens": ["Po\u00b7et\u00b7en", "ha\u00b7ben", "gro\u00b7\u00dfe", "Scham", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wenn sie sich das Maul verbrennen,", "tokens": ["Denn", "wenn", "sie", "sich", "das", "Maul", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wird ihr Gaul hernach gleich lahm.", "tokens": ["So", "wird", "ihr", "Gaul", "her\u00b7nach", "gleich", "lahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du kennst der Musen Bettlerorden,", "tokens": ["Du", "kennst", "der", "Mu\u00b7sen", "Bett\u00b7le\u00b7ror\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Worin auch ich, ich weis nicht wie,", "tokens": ["Wo\u00b7rin", "auch", "ich", ",", "ich", "weis", "nicht", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "$,", "PPER", "PTKVZ", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von Jugend auf geschrieben worden;", "tokens": ["Von", "Ju\u00b7gend", "auf", "ge\u00b7schrie\u00b7ben", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kein Reichthumskummer dr\u00fccket sie,", "tokens": ["Kein", "Reicht\u00b7hums\u00b7kum\u00b7mer", "dr\u00fc\u00b7cket", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein jeder mu\u00df wie Petrus sprechen:", "tokens": ["Ein", "je\u00b7der", "mu\u00df", "wie", "Pet\u00b7rus", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "KOKOM", "FM", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Kein Gold und Silber hab ich nicht.", "tokens": ["Kein", "Gold", "und", "Sil\u00b7ber", "hab", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch will's gleich dann und wann gebrechen,", "tokens": ["Doch", "will's", "gleich", "dann", "und", "wann", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "KON", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Kommt unverhoft ein Freudenlicht.", "tokens": ["Kommt", "un\u00b7ver\u00b7hoft", "ein", "Freu\u00b7den\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mein G\u00f6nner weis, man braucht noch immer,", "tokens": ["Mein", "G\u00f6n\u00b7ner", "weis", ",", "man", "braucht", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "PIS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Es sey so wenig als es will.", "tokens": ["Es", "sey", "so", "we\u00b7nig", "als", "es", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Bald legt sich Schmalhans in das Zimmer,", "tokens": ["Bald", "legt", "sich", "Schmal\u00b7hans", "in", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Umsonst macht keine Magd den Pfiehl,", "tokens": ["Um\u00b7sonst", "macht", "kei\u00b7ne", "Magd", "den", "Pfiehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bald springt ein Knopf, bald rei\u00dfen N\u00e4hte,", "tokens": ["Bald", "springt", "ein", "Knopf", ",", "bald", "rei\u00b7\u00dfen", "N\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Bald bleckt die Zeh, bald plazt der Schuh,", "tokens": ["Bald", "bleckt", "die", "Zeh", ",", "bald", "plazt", "der", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Bald spricht die lange W\u00e4scher-K\u00e4the:", "tokens": ["Bald", "spricht", "die", "lan\u00b7ge", "W\u00e4\u00b7scher\u00b7K\u00e4\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Herr, stopft das Loch mit Bleche zu.", "tokens": ["Herr", ",", "stopft", "das", "Loch", "mit", "Ble\u00b7che", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}