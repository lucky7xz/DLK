{"textgrid.poem.59504": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Lebensmut", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tummle dich, mein junges Leben,", "tokens": ["Tumm\u00b7le", "dich", ",", "mein", "jun\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorw\u00e4rts gleich dem schnellen Renner!", "tokens": ["Vor\u00b7w\u00e4rts", "gleich", "dem", "schnel\u00b7len", "Ren\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00dft nicht vor dem Staube beben", "tokens": ["Mu\u00dft", "nicht", "vor", "dem", "Stau\u00b7be", "be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In dem hei\u00dfen Kampf der M\u00e4nner,", "tokens": ["In", "dem", "hei\u00b7\u00dfen", "Kampf", "der", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00dft nicht vor den St\u00fcrmen zittern,", "tokens": ["Mu\u00dft", "nicht", "vor", "den", "St\u00fcr\u00b7men", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Die die Starken niederschmei\u00dfen,", "tokens": ["Die", "die", "Star\u00b7ken", "nie\u00b7der\u00b7schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eichen aus den Felsen splittern,", "tokens": ["Ei\u00b7chen", "aus", "den", "Fel\u00b7sen", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Felsen selbst zerrei\u00dfen.", "tokens": ["Und", "die", "Fel\u00b7sen", "selbst", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Frisch hinein! wo Tausend fallen,", "tokens": ["Frisch", "hin\u00b7ein", "!", "wo", "Tau\u00b7send", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnen Zehnmaltausend stehen,", "tokens": ["K\u00f6n\u00b7nen", "Zehn\u00b7mal\u00b7tau\u00b7send", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siegeslieder lustig schallen,", "tokens": ["Sie\u00b7ges\u00b7lie\u00b7der", "lus\u00b7tig", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sich Gr\u00e4ber schwarz erh\u00f6hen;", "tokens": ["Wo", "sich", "Gr\u00e4\u00b7ber", "schwarz", "er\u00b7h\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die zu Bergen klimmen, brechen", "tokens": ["Die", "zu", "Ber\u00b7gen", "klim\u00b7men", ",", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "NN", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leicht die himmelk\u00fchnen H\u00e4upter,", "tokens": ["Leicht", "die", "him\u00b7mel\u00b7k\u00fch\u00b7nen", "H\u00e4up\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wohnt der Mensch auf gr\u00fcnen Fl\u00e4chen,", "tokens": ["Wohnt", "der", "Mensch", "auf", "gr\u00fc\u00b7nen", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Flut der Str\u00f6me bleibt er.", "tokens": ["In", "der", "Flut", "der", "Str\u00f6\u00b7me", "bleibt", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was die heil'gen Drei, die schwarzen", "tokens": ["Was", "die", "heil'\u00b7gen", "Drei", ",", "die", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "$,", "PRELS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwestern, unvermeidlich weben,", "tokens": ["Schwes\u00b7tern", ",", "un\u00b7ver\u00b7meid\u00b7lich", "we\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Gesetz der strengen Parzen", "tokens": ["Das", "Ge\u00b7setz", "der", "stren\u00b7gen", "Par\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlinget sich um jedes Leben:", "tokens": ["Schlin\u00b7get", "sich", "um", "je\u00b7des", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was wir streben, was wir wollen,", "tokens": ["Was", "wir", "stre\u00b7ben", ",", "was", "wir", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4lt die tiefe Macht gefangen,", "tokens": ["H\u00e4lt", "die", "tie\u00b7fe", "Macht", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heimarmenens Donner rollen,", "tokens": ["Hei\u00b7mar\u00b7me\u00b7nens", "Don\u00b7ner", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zischend spr\u00fchn Erinnys' Schlangen.", "tokens": ["Zi\u00b7schend", "spr\u00fchn", "Er\u00b7in\u00b7nys'", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kr\u00e4nze deiner Jugend Locken", "tokens": ["Kr\u00e4n\u00b7ze", "dei\u00b7ner", "Ju\u00b7gend", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den sch\u00f6nsten Maienbl\u00fcten,", "tokens": ["Mit", "den", "sch\u00f6ns\u00b7ten", "Mai\u00b7en\u00b7bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis des Winters kalte Flocken", "tokens": ["Bis", "des", "Win\u00b7ters", "kal\u00b7te", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die kahlen Scheitel w\u00fcten;", "tokens": ["Um", "die", "kah\u00b7len", "Schei\u00b7tel", "w\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tapfer mu\u00dft du stehn und fallen,", "tokens": ["Tap\u00b7fer", "mu\u00dft", "du", "stehn", "und", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klanglos ist der Tod der Matten,", "tokens": ["Klang\u00b7los", "ist", "der", "Tod", "der", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die Heldenseelen wallen", "tokens": ["Doch", "die", "Hel\u00b7den\u00b7see\u00b7len", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Herrlich in das Reich der Schatten.", "tokens": ["Herr\u00b7lich", "in", "das", "Reich", "der", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}