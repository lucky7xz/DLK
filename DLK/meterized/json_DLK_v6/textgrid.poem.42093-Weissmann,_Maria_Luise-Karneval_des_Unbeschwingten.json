{"textgrid.poem.42093": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Karneval des Unbeschwingten", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ich war mir ganz entlaufen,", "tokens": ["Ach", ",", "ich", "war", "mir", "ganz", "ent\u00b7lau\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzte fremd im fremden Land", "tokens": ["Tanz\u00b7te", "fremd", "im", "frem\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie wollten mich schon taufen:", "tokens": ["Und", "sie", "woll\u00b7ten", "mich", "schon", "tau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer doch den Namen fand,", "tokens": ["Ei\u00b7ner", "doch", "den", "Na\u00b7men", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der mich rief, wie sie mich nannten", "tokens": ["Der", "mich", "rief", ",", "wie", "sie", "mich", "nann\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Damals, einst, vor langer Zeit...", "tokens": ["Da\u00b7mals", ",", "einst", ",", "vor", "lan\u00b7ger", "Zeit", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich wu\u00dfte: sie erkannten", "tokens": ["Und", "ich", "wu\u00df\u00b7te", ":", "sie", "er\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter dem gemalten Kleid", "tokens": ["Un\u00b7ter", "dem", "ge\u00b7mal\u00b7ten", "Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch den Fremdling, der verblieben,", "tokens": ["Doch", "den", "Fremd\u00b7ling", ",", "der", "ver\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun sich selber unbekannt.", "tokens": ["Nun", "sich", "sel\u00b7ber", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein versto\u00dfnes Herz zu lieben", "tokens": ["Sein", "ver\u00b7sto\u00df\u00b7nes", "Herz", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stand er rot an einer Wand.", "tokens": ["Stand", "er", "rot", "an", "ei\u00b7ner", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}