{"textgrid.poem.28442": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat Gott mich ohne mich gebracht in dieses Leben,", "tokens": ["Hat", "Gott", "mich", "oh\u00b7ne", "mich", "ge\u00b7bracht", "in", "die\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "APPR", "PPER", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird Gott mir, was mir fehlt, mir ohne mich auch geben.", "tokens": ["Wird", "Gott", "mir", ",", "was", "mir", "fehlt", ",", "mir", "oh\u00b7ne", "mich", "auch", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Heyde sorgt zu viel; ein Christ traut seinem Gott,", "tokens": ["Ein", "Hey\u00b7de", "sorgt", "zu", "viel", ";", "ein", "Christ", "traut", "sei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIS", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sein Gesch\u00f6pff erh\u00e4lt in Gl\u00fcck und auch in Noth.", "tokens": ["Der", "sein", "Ge\u00b7sch\u00f6pff", "er\u00b7h\u00e4lt", "in", "Gl\u00fcck", "und", "auch", "in", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}