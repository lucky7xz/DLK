{"textgrid.poem.53691": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An Philipp Scheidemann", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben dich hier \u00f6fters angepfiffen", "tokens": ["Wir", "ha\u00b7ben", "dich", "hier", "\u00f6f\u00b7ters", "an\u00b7ge\u00b7pfif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von wegen deiner leicht verdorrten Hand.", "tokens": ["von", "we\u00b7gen", "dei\u00b7ner", "leicht", "ver\u00b7dorr\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nun ein feiger Lump dich angegriffen,", "tokens": ["Doch", "nun", "ein", "fei\u00b7ger", "Lump", "dich", "an\u00b7ge\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "hat sich das Bl\u00e4ttchen j\u00e4h f\u00fcr dich gewandt.", "tokens": ["hat", "sich", "das", "Bl\u00e4tt\u00b7chen", "j\u00e4h", "f\u00fcr", "dich", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn einer Gift spritzt aus dem Schie\u00dfklistier:", "tokens": ["Wenn", "ei\u00b7ner", "Gift", "spritzt", "aus", "dem", "Schie\u00df\u00b7klis\u00b7tier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "dann, Philipp, stehn wir alle hinter dir!", "tokens": ["dann", ",", "Phi\u00b7lipp", ",", "stehn", "wir", "al\u00b7le", "hin\u00b7ter", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "PPER", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was wollte denn der nationale Affe?", "tokens": ["Was", "woll\u00b7te", "denn", "der", "na\u00b7ti\u00b7o\u00b7na\u00b7le", "Af\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Versailles r\u00e4chen? Bist du General?", "tokens": ["Ver\u00b7sail\u00b7les", "r\u00e4\u00b7chen", "?", "Bist", "du", "Ge\u00b7ne\u00b7ral", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr ihn war das Klistier die einzige Waffe", "tokens": ["F\u00fcr", "ihn", "war", "das", "Klis\u00b7tier", "die", "ein\u00b7zi\u00b7ge", "Waf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "aus seinem reichen Geistes-Arsenal.", "tokens": ["aus", "sei\u00b7nem", "rei\u00b7chen", "Geis\u00b7tes\u00b7Ar\u00b7se\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn was ein richtiger, tapfrer, teutscher Mann,", "tokens": ["Denn", "was", "ein", "rich\u00b7ti\u00b7ger", ",", "tapf\u00b7rer", ",", "teut\u00b7scher", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "der f\u00e4ngt ein jedes Ding von hinten an.", "tokens": ["der", "f\u00e4ngt", "ein", "je\u00b7des", "Ding", "von", "hin\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "PIAT", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und hat wer irgend etwas zu riskieren?", "tokens": ["Und", "hat", "wer", "ir\u00b7gend", "et\u00b7was", "zu", "ris\u00b7kie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PWS", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Sipo bis herab zum Staatsanwalt", "tokens": ["Vom", "Si\u00b7po", "bis", "her\u00b7ab", "zum", "Staats\u00b7an\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wird solchen M\u00f6rder keiner arretieren \u2013", "tokens": ["wird", "sol\u00b7chen", "M\u00f6r\u00b7der", "kei\u00b7ner", "ar\u00b7re\u00b7tie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sie h\u00f6ren nichts, wenn es in Griesbach knallt . . .", "tokens": ["sie", "h\u00f6\u00b7ren", "nichts", ",", "wenn", "es", "in", "Gries\u00b7bach", "knallt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch die Provinzen hallt ein einziger Schrei:", "tokens": ["Durch", "die", "Pro\u00b7vin\u00b7zen", "hallt", "ein", "ein\u00b7zi\u00b7ger", "Schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "\u00bbwie schade, schade \u2013 diesmal gings vorbei!\u00ab", "tokens": ["\u00bb", "wie", "scha\u00b7de", ",", "scha\u00b7de", "\u2013", "dies\u00b7mal", "gings", "vor\u00b7bei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "$,", "VVFIN", "$(", "ADV", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von Liebknecht bis zu dir heut.", "tokens": ["Von", "Lieb\u00b7knecht", "bis", "zu", "dir", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So verworfen", "tokens": ["So", "ver\u00b7wor\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "wie solche M\u00f6rder ist nur noch ein Stand.", "tokens": ["wie", "sol\u00b7che", "M\u00f6r\u00b7der", "ist", "nur", "noch", "ein", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nimm an, es schie\u00dft mal wer auf Ludendorffen:", "tokens": ["Nimm", "an", ",", "es", "schie\u00dft", "mal", "wer", "auf", "Lu\u00b7den\u00b7dorf\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PWS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was, meinst du wohl, bl\u00fcht dem in unserm Land \u2013?", "tokens": ["Was", ",", "meinst", "du", "wohl", ",", "bl\u00fcht", "dem", "in", "un\u00b7serm", "Land", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "\u00bbdie Republik f\u00e4ngt an, mir l\u00e4cherlich zu werden!\u00ab", "tokens": ["\u00bb", "die", "Re\u00b7pub\u00b7lik", "f\u00e4ngt", "an", ",", "mir", "l\u00e4\u00b7cher\u00b7lich", "zu", "wer\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "ADJD", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erhol dich, Philipp! Leb noch lang auf Erden!", "tokens": ["Er\u00b7hol", "dich", ",", "Phi\u00b7lipp", "!", "Leb", "noch", "lang", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$.", "VVIMP", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}