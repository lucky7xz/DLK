{"dta.poem.4324": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nasturtium, oder Indianische  \n Kresse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich bey bunten Bluhmen stehe,", "tokens": ["Wenn", "ich", "bey", "bun\u00b7ten", "Bluh\u00b7men", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bey der Wunder-Pracht bemerke,", "tokens": ["Und", "bey", "der", "Wun\u00b7der\u00b7Pracht", "be\u00b7mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da\u00df sie, o Sch\u00f6pfer, Deine Werke;", "tokens": ["Da\u00df", "sie", ",", "o", "Sch\u00f6p\u00b7fer", ",", "Dei\u00b7ne", "Wer\u00b7ke", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So deucht mich, da\u00df ich in der N\u00e4he", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "ich", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich Selber gegenw\u00e4rtig sehe:", "tokens": ["Dich", "Sel\u00b7ber", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "se\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann zieh\u2019 ich billig Geist und Blicke", "tokens": ["Dann", "zieh'", "ich", "bil\u00b7lig", "Geist", "und", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von allem Gegenwurf zur\u00fccke,", "tokens": ["Von", "al\u00b7lem", "Ge\u00b7gen\u00b7wurf", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bet\u2019, in ihrer Wunder-Pracht,", "tokens": ["Und", "bet'", ",", "in", "ih\u00b7rer", "Wun\u00b7der\u00b7Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die man nicht gnug bewundern kann,", "tokens": ["Die", "man", "nicht", "gnug", "be\u00b7wun\u00b7dern", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Herr, Deine Weisheit, Lieb\u2019 und Macht,", "tokens": ["Herr", ",", "Dei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In Ehrfurcht, Freud\u2019 und Andacht, an!", "tokens": ["In", "Ehr\u00b7furcht", ",", "Freud'", "und", "An\u00b7dacht", ",", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich werde  nicht allein bey dir,", "tokens": ["Ich", "wer\u00b7de", "nicht", "al\u00b7lein", "bey", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fast brennendes Nasturtium,", "tokens": ["Fast", "bren\u00b7nen\u00b7des", "Nas\u00b7tur\u00b7ti\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Lust und f\u00fcr Verwundrung stumm;", "tokens": ["F\u00fcr", "Lust", "und", "f\u00fcr", "Ver\u00b7wund\u00b7rung", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es flicht fast Gram und Scham bey mir,", "tokens": ["Es", "flicht", "fast", "Gram", "und", "Scham", "bey", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bey meiner Lust, zugleich sich ein,", "tokens": ["Bey", "mei\u00b7ner", "Lust", ",", "zu\u00b7gleich", "sich", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich, bewundernswehrte Bluhme,", "tokens": ["Da\u00df", "ich", ",", "be\u00b7wun\u00b7derns\u00b7wehr\u00b7te", "Bluh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Bl\u00e4tter gelben Glanz und Schein,", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "gel\u00b7ben", "Glanz", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die fremde Bildung, die so sch\u00f6n,", "tokens": ["Die", "frem\u00b7de", "Bil\u00b7dung", ",", "die", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem, Der dich schuf, zum Preis und Ruhme,", "tokens": ["Dem", ",", "Der", "dich", "schuf", ",", "zum", "Preis", "und", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "PPER", "VVFIN", "$,", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bisher nur obenhin gesehn,", "tokens": ["Bis\u00b7her", "nur", "o\u00b7ben\u00b7hin", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da du vor vielen andern wehrt,", "tokens": ["Da", "du", "vor", "vie\u00b7len", "an\u00b7dern", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df man, in dir, den Sch\u00f6pfer ehrt,", "tokens": ["Da\u00df", "man", ",", "in", "dir", ",", "den", "Sch\u00f6p\u00b7fer", "ehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Indem du den Geruch allein", "tokens": ["In\u00b7dem", "du", "den", "Ge\u00b7ruch", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Und das Gesicht nicht nur vergn\u00fcgest,", "tokens": ["Und", "das", "Ge\u00b7sicht", "nicht", "nur", "ver\u00b7gn\u00fc\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nein, da du, zu der Farben Schein,", "tokens": ["Nein", ",", "da", "du", ",", "zu", "der", "Far\u00b7ben", "Schein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Noch den Geschmack zu jenen f\u00fcgest,", "tokens": ["Noch", "den", "Ge\u00b7schmack", "zu", "je\u00b7nen", "f\u00fc\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja der Gesundheit dienst; man hat in Arzeneyen", "tokens": ["Ja", "der", "Ge\u00b7sund\u00b7heit", "dienst", ";", "man", "hat", "in", "Ar\u00b7ze\u00b7ne\u00b7yen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "ADV", "$.", "PIS", "VAFIN", "APPR", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sich deiner gleichfalls zu erfreuen.", "tokens": ["Sich", "dei\u00b7ner", "gleich\u00b7falls", "zu", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer sieht dich sonder Anmuht bl\u00fchn?", "tokens": ["Wer", "sieht", "dich", "son\u00b7der", "An\u00b7muht", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht sonderlich ist die Figur,", "tokens": ["Recht", "son\u00b7der\u00b7lich", "ist", "die", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "So die formierende Natur,", "tokens": ["So", "die", "for\u00b7mie\u00b7ren\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Geliebte Bluhme, dir verlieh\u2019n.", "tokens": ["Ge\u00b7lieb\u00b7te", "Bluh\u00b7me", ",", "dir", "ver\u00b7lieh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es gleicht ein Theil von dir des Ueberflusses Horn,", "tokens": ["Es", "gleicht", "ein", "Theil", "von", "dir", "des", "Ue\u00b7ber\u00b7flus\u00b7ses", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das sich vom Stengel r\u00fcckwerts bieget,", "tokens": ["Das", "sich", "vom", "Sten\u00b7gel", "r\u00fcck\u00b7werts", "bie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wodurch die ganze Bluhme denn, die queer auf ihrem", "tokens": ["Wo\u00b7durch", "die", "gan\u00b7ze", "Bluh\u00b7me", "denn", ",", "die", "que\u00b7er", "auf", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "$,", "PRELS", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Stengel lieget,", "tokens": ["Sten\u00b7gel", "lie\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Da aus des Hornes Oeffnung vorn,", "tokens": ["Da", "aus", "des", "Hor\u00b7nes", "Oeff\u00b7nung", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Aus f\u00fcnf getheilter Bl\u00e4tter Ecken,", "tokens": ["Aus", "f\u00fcnf", "ge\u00b7theil\u00b7ter", "Bl\u00e4t\u00b7ter", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die hinten breit, und vorn sich spitzen,", "tokens": ["Die", "hin\u00b7ten", "breit", ",", "und", "vorn", "sich", "spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "KON", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sich f\u00fcnf ganz andre Bl\u00e4tter strecken,", "tokens": ["Sich", "f\u00fcnf", "ganz", "and\u00b7re", "Bl\u00e4t\u00b7ter", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "CARD", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die hinten zugespitzt, an jenen veste sitzen,", "tokens": ["Die", "hin\u00b7ten", "zu\u00b7ge\u00b7spitzt", ",", "an", "je\u00b7nen", "ves\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und die sich oberwerts verbreiten und fast r\u00fcnden,", "tokens": ["Und", "die", "sich", "o\u00b7ber\u00b7werts", "ver\u00b7brei\u00b7ten", "und", "fast", "r\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "ADV", "VVINF", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wovon die obern zwey", "tokens": ["Wo\u00b7von", "die", "o\u00b7bern", "zwey"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "An Gr\u00f6\u00df\u2019 und Bildung einerley,", "tokens": ["An", "Gr\u00f6\u00df'", "und", "Bil\u00b7dung", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den Ursprung aus dem obern Theil von der geh\u00f6rnten", "tokens": ["Den", "Ur\u00b7sprung", "aus", "dem", "o\u00b7bern", "Theil", "von", "der", "ge\u00b7h\u00f6rn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "R\u00f6hre finden,", "tokens": ["R\u00f6h\u00b7re", "fin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Da gegentheils die untern drey", "tokens": ["Da", "ge\u00b7gen\u00b7theils", "die", "un\u00b7tern", "drey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sich durch das untre Paar der andern Bl\u00e4tter scheinen", "tokens": ["Sich", "durch", "das", "un\u00b7tre", "Paar", "der", "an\u00b7dern", "Bl\u00e4t\u00b7ter", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mit ihrem Stiel zugleich sich zu vereinen.", "tokens": ["Mit", "ih\u00b7rem", "Stiel", "zu\u00b7gleich", "sich", "zu", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wenn, von den dreyen, jedes Blatt", "tokens": ["Wenn", ",", "von", "den", "drey\u00b7en", ",", "je\u00b7des", "Blatt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ART", "CARD", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da, wo dieselben sich verbreiten,", "tokens": ["Da", ",", "wo", "die\u00b7sel\u00b7ben", "sich", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PDAT", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.