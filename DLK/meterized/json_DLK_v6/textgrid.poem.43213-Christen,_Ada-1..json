{"textgrid.poem.43213": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was fragst Du den Mann", "tokens": ["Was", "fragst", "Du", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nach Heimath und Haus?", "tokens": ["Nach", "Hei\u00b7math", "und", "Haus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er hat sie nicht \u2013", "tokens": ["Er", "hat", "sie", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Du horchest nach Vater", "tokens": ["Du", "hor\u00b7chest", "nach", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und Mutter ihn aus,", "tokens": ["Und", "Mut\u00b7ter", "ihn", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Er kennt sie nicht.", "tokens": ["Er", "kennt", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was fragst Du den Mann", "tokens": ["Was", "fragst", "Du", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Nach Kind und nach Weib?", "tokens": ["Nach", "Kind", "und", "nach", "Weib", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Er klagt doch nicht,", "tokens": ["Er", "klagt", "doch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Da\u00df sie ihn verlie\u00df", "tokens": ["Da\u00df", "sie", "ihn", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Mit Seele und Leib,", "tokens": ["Mit", "See\u00b7le", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Um einen Wicht...", "tokens": ["Um", "ei\u00b7nen", "Wicht", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Was fragst Du den Mann", "tokens": ["Was", "fragst", "Du", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Nach seinem Gott?", "tokens": ["Nach", "sei\u00b7nem", "Gott", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Er suchte Licht! \u2013", "tokens": ["Er", "such\u00b7te", "Licht", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Warum blieb es dunkel", "tokens": ["Wa\u00b7rum", "blieb", "es", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "In Elend und Spott?", "tokens": ["In", "E\u00b7lend", "und", "Spott", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Er wei\u00df es nicht. \u2013 \u2013", "tokens": ["Er", "wei\u00df", "es", "nicht", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}