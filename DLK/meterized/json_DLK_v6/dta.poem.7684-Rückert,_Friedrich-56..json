{"dta.poem.7684": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr H\u00fcgel, unter die ich legte meine Lieben,", "tokens": ["Ihr", "H\u00fc\u00b7gel", ",", "un\u00b7ter", "die", "ich", "leg\u00b7te", "mei\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht ganz verlor ich sie, denn ihr seid mir geblieben.", "tokens": ["Nicht", "ganz", "ver\u00b7lor", "ich", "sie", ",", "denn", "ihr", "seid", "mir", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Was ist des Todes Macht? da Blumen sanfter Pracht", "tokens": ["Was", "ist", "des", "To\u00b7des", "Macht", "?", "da", "Blu\u00b7men", "sanf\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$.", "KOUS", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir nun an Orten bl\u00fchn, wo ich es nie gedacht.", "tokens": ["Mir", "nun", "an", "Or\u00b7ten", "bl\u00fchn", ",", "wo", "ich", "es", "nie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVINF", "$,", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ist das ich verlor? wenn solch ein Liebesflor", "tokens": ["Was", "ist", "das", "ich", "ver\u00b7lor", "?", "wenn", "solch", "ein", "Lie\u00b7bes\u00b7flor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "PPER", "VVFIN", "$.", "KOUS", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun eine Stelle schm\u00fcckt, mir \u00f6d' und leer zuvor.", "tokens": ["Nun", "ei\u00b7ne", "Stel\u00b7le", "schm\u00fcckt", ",", "mir", "\u00f6d'", "und", "leer", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}