{"textgrid.poem.40800": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lag im weichen Gras, gelehnt auf Tr\u00fcmmer,", "tokens": ["Ich", "lag", "im", "wei\u00b7chen", "Gras", ",", "ge\u00b7lehnt", "auf", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Istriens vom Lenz umbl\u00fchten Strande;", "tokens": ["An", "I\u00b7stri\u00b7ens", "vom", "Lenz", "um\u00b7bl\u00fch\u00b7ten", "Stran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel quoll in abendros'gem Schimmer,", "tokens": ["Der", "Him\u00b7mel", "quoll", "in", "a\u00b7bendros'\u00b7gem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Meer erglomm im purpurrothen Brande.", "tokens": ["Das", "Meer", "er\u00b7glomm", "im", "pur\u00b7pur\u00b7ro\u00b7then", "Bran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie wollen flammend Beid' in eines flie\u00dfen,", "tokens": ["Sie", "wol\u00b7len", "flam\u00b7mend", "Beid'", "in", "ei\u00b7nes", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht sieht das Aug', wo Meer und Luft sich trennen,", "tokens": ["Nicht", "sieht", "das", "Aug'", ",", "wo", "Meer", "und", "Luft", "sich", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sich zwei Lippen an einander schlie\u00dfen,", "tokens": ["Wie", "sich", "zwei", "Lip\u00b7pen", "an", "ein\u00b7an\u00b7der", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "CARD", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem ew'gen Liebesku\u00df zu brennen.", "tokens": ["In", "ei\u00b7nem", "ew'\u00b7gen", "Lie\u00b7bes\u00b7ku\u00df", "zu", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von Liebe wollen Flur und Hain erz\u00e4hlen,", "tokens": ["Von", "Lie\u00b7be", "wol\u00b7len", "Flur", "und", "Hain", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist rings ein Err\u00f6then, Fl\u00fcstern, Kosen!", "tokens": ["Das", "ist", "rings", "ein", "Er\u00b7r\u00f6\u00b7then", ",", "Fl\u00fcs\u00b7tern", ",", "Ko\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wellen h\u00fcpfen ans Gestad' und stehlen", "tokens": ["Die", "Wel\u00b7len", "h\u00fcp\u00b7fen", "ans", "Ge\u00b7stad'", "und", "steh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich fl\u00fcchtig K\u00fcsse von des Strandes Rosen.", "tokens": ["Sich", "fl\u00fcch\u00b7tig", "K\u00fcs\u00b7se", "von", "des", "Stran\u00b7des", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie legen Nachts gar heimlich und behende", "tokens": ["Sie", "le\u00b7gen", "Nachts", "gar", "heim\u00b7lich", "und", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ans Land der Muscheln farbenreich Geschmeide,", "tokens": ["Ans", "Land", "der", "Mu\u00b7scheln", "far\u00b7ben\u00b7reich", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Morgens an der Liebe zarter Spende", "tokens": ["Da\u00df", "Mor\u00b7gens", "an", "der", "Lie\u00b7be", "zar\u00b7ter", "Spen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Rosen Aug' sich beim Erwachen weide.", "tokens": ["Der", "Ro\u00b7sen", "Aug'", "sich", "beim", "Er\u00b7wa\u00b7chen", "wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch du dort, alter Thurm, \u00f6d' und zerfallen,", "tokens": ["Doch", "du", "dort", ",", "al\u00b7ter", "Thurm", ",", "\u00f6d'", "und", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Willst du nicht auch von Lieb' ein W\u00f6rtlein sagen?", "tokens": ["Willst", "du", "nicht", "auch", "von", "Lieb'", "ein", "W\u00f6rt\u00b7lein", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich d\u00fcnkt es, deine morschen Quadern lallen", "tokens": ["Mich", "d\u00fcnkt", "es", ",", "dei\u00b7ne", "mor\u00b7schen", "Qua\u00b7dern", "lal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein b\u00f6ses Lied aus alten, b\u00f6sen Tagen!", "tokens": ["Ein", "b\u00f6\u00b7ses", "Lied", "aus", "al\u00b7ten", ",", "b\u00f6\u00b7sen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dein Antlitz blickt so ernst, als ob es z\u00fcrne,", "tokens": ["Dein", "Ant\u00b7litz", "blickt", "so", "ernst", ",", "als", "ob", "es", "z\u00fcr\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und finstres Moos ist d\u00e4mmernd drauf zu schauen,", "tokens": ["Und", "finst\u00b7res", "Moos", "ist", "d\u00e4m\u00b7mernd", "drauf", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADJD", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie auf des Denkers tiefgefurchter Stirne", "tokens": ["Wie", "auf", "des", "Den\u00b7kers", "tief\u00b7ge\u00b7furch\u00b7ter", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dunklen und gedankenschweren Brauen.", "tokens": ["Die", "dunk\u00b7len", "und", "ge\u00b7dan\u00b7ken\u00b7schwe\u00b7ren", "Brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wohl d\u00e4mmert's in dir von Erinnerungen,", "tokens": ["Wohl", "d\u00e4m\u00b7mert's", "in", "dir", "von", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie Schuldbewu\u00dftsein in des S\u00fcnders Herzen,", "tokens": ["Wie", "Schuld\u00b7be\u00b7wu\u00df\u00b7tsein", "in", "des", "S\u00fcn\u00b7ders", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du finsterer Geselle, rings umschlungen", "tokens": ["Du", "fins\u00b7te\u00b7rer", "Ge\u00b7sel\u00b7le", ",", "rings", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von ros'gen Sch\u00e4kern und verliebten Scherzen!", "tokens": ["Von", "ros'\u00b7gen", "Sch\u00e4\u00b7kern", "und", "ver\u00b7lieb\u00b7ten", "Scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ob deinem Thor ein Wappen, moosumwoben,", "tokens": ["Ob", "dei\u00b7nem", "Thor", "ein", "Wap\u00b7pen", ",", "moo\u00b7sum\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein L\u00f6we ist's, das Evangelium haltend!", "tokens": ["Ein", "L\u00f6\u00b7we", "ist's", ",", "das", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "hal\u00b7tend", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Venedig, ha, dein Leu! Wohl mu\u00df ich loben", "tokens": ["Ve\u00b7ne\u00b7dig", ",", "ha", ",", "dein", "Leu", "!", "Wohl", "mu\u00df", "ich", "lo\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ITJ", "$,", "PPOSAT", "NN", "$.", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Des Sinnbilds Wahl, dein ganzes Sein entfaltend.", "tokens": ["Des", "Sinn\u00b7bilds", "Wahl", ",", "dein", "gan\u00b7zes", "Sein", "ent\u00b7fal\u00b7tend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der M\u00e4hne K\u00f6nigsmantel sch\u00fcttelnd, Leue,", "tokens": ["Der", "M\u00e4h\u00b7ne", "K\u00f6\u00b7nigs\u00b7man\u00b7tel", "sch\u00fct\u00b7telnd", ",", "Leu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch nicht verleugnend das Geschlecht der Katze!", "tokens": ["Doch", "nicht", "ver\u00b7leug\u00b7nend", "das", "Ge\u00b7schlecht", "der", "Kat\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das heil'ge Buch des Glaubens und der Treue", "tokens": ["Das", "heil'\u00b7ge", "Buch", "des", "Glau\u00b7bens", "und", "der", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erhoben hoch, \u2013 doch in bekrallter Tatze!", "tokens": ["Er\u00b7ho\u00b7ben", "hoch", ",", "\u2013", "doch", "in", "be\u00b7krall\u00b7ter", "Tat\u00b7ze", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "$(", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Gro\u00dfm\u00fcthig, wenn ges\u00e4ttigt schon vom Morden,", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7hig", ",", "wenn", "ge\u00b7s\u00e4t\u00b7tigt", "schon", "vom", "Mor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sanft, wenn du geb\u00e4ndigt mu\u00dft erliegen,", "tokens": ["Und", "sanft", ",", "wenn", "du", "ge\u00b7b\u00e4n\u00b7digt", "mu\u00dft", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "VVPP", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Thron die Kluft, drin nie es Tag geworden,", "tokens": ["Dein", "Thron", "die", "Kluft", ",", "drin", "nie", "es", "Tag", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "ADV", "ADV", "PPER", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch voll Glanz und Ruhm und Kraft und Siegen!", "tokens": ["Und", "doch", "voll", "Glanz", "und", "Ruhm", "und", "Kraft", "und", "Sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sprich, und was wolltest du am Thurme dorten?", "tokens": ["Sprich", ",", "und", "was", "woll\u00b7test", "du", "am", "Thur\u00b7me", "dor\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KON", "PWS", "VMFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich ahn's, ein Kerker war's! Als Kerkermeister", "tokens": ["Ich", "ahn's", ",", "ein", "Ker\u00b7ker", "wa\u00b7r's", "!", "Als", "Ker\u00b7ker\u00b7meis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "$.", "KOUS", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hat sich der Leu gelegt vor seine Pforten,", "tokens": ["Hat", "sich", "der", "Leu", "ge\u00b7legt", "vor", "sei\u00b7ne", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn gern in Haft hielt Leiber er und Geister!", "tokens": ["Denn", "gern", "in", "Haft", "hielt", "Lei\u00b7ber", "er", "und", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Sieh hin jetzt: du zertreten, er zerschlagen!", "tokens": ["Sieh", "hin", "jetzt", ":", "du", "zer\u00b7tre\u00b7ten", ",", "er", "zer\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$.", "PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieh selbst dein Werkzeug: Ketten, Eisenstangen", "tokens": ["Sieh", "selb