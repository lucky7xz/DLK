{"dta.poem.8005": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Fr\u00e4ulein sahn vom Schlosse", "tokens": ["Drei", "Fr\u00e4u\u00b7lein", "sahn", "vom", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in\u2019s tiefe Thal.", "tokens": ["Hin\u00b7ab", "in's", "tie\u00b7fe", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Vater kam zu Rosse,", "tokens": ["Ihr", "Va\u00b7ter", "kam", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er trug ein Kleid von Stahl.", "tokens": ["Er", "trug", "ein", "Kleid", "von", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ewillkomm, Herr Vater, Gottwillkomm!", "tokens": ["\u201e", "will\u00b7komm", ",", "Herr", "Va\u00b7ter", ",", "Gott\u00b7will\u00b7komm", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was bringst du deinen Kindern?", "tokens": ["Was", "bringst", "du", "dei\u00b7nen", "Kin\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir waren alle fromm.\u201c", "tokens": ["Wir", "wa\u00b7ren", "al\u00b7le", "fromm", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201emein Kind im gelben Kleide!", "tokens": ["\u201e", "mein", "Kind", "im", "gel\u00b7ben", "Klei\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heut hab\u2019 ich dein gedacht.", "tokens": ["Heut", "hab'", "ich", "dein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schmuck ist deine Freude,", "tokens": ["Der", "Schmuck", "ist", "dei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Liebstes ist die Pracht.", "tokens": ["Dein", "Liebs\u00b7tes", "ist", "die", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von rothem Gold die Kette hier", "tokens": ["Von", "ro\u00b7them", "Gold", "die", "Ket\u00b7te", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm ich dem stolzen Ritter,", "tokens": ["Nahm", "ich", "dem", "stol\u00b7zen", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gab ihm den Tod daf\u00fcr.\u201c", "tokens": ["Gab", "ihm", "den", "Tod", "da\u00b7f\u00fcr", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Fr\u00e4ulein schnell die Kette", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "schnell", "die", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um ihren Nacken band.", "tokens": ["Um", "ih\u00b7ren", "Na\u00b7cken", "band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie ging hinab zur St\u00e4tte,", "tokens": ["Sie", "ging", "hin\u00b7ab", "zur", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da sie den Todten fand.", "tokens": ["Da", "sie", "den", "Tod\u00b7ten", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201edu liegst am Wege, wie ein Dieb,", "tokens": ["\u201e", "du", "liegst", "am", "We\u00b7ge", ",", "wie", "ein", "Dieb", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bist ein edler Ritter,", "tokens": ["Und", "bist", "ein", "ed\u00b7ler", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und bist mein seines Lieb.\u201c", "tokens": ["Und", "bist", "mein", "sei\u00b7nes", "Lieb", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie trug ihn auf den Armen", "tokens": ["Sie", "trug", "ihn", "auf", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Gotteshaus hinab;", "tokens": ["Zum", "Got\u00b7tes\u00b7haus", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie legt\u2019 ihn mit Erbarmen", "tokens": ["Sie", "legt'", "ihn", "mit", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seiner V\u00e4ter Grab.", "tokens": ["In", "sei\u00b7ner", "V\u00e4\u00b7ter", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Kett\u2019, die ihr am Halse schien,", "tokens": ["Die", "Kett'", ",", "die", "ihr", "am", "Hal\u00b7se", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die zog sie fest zusammen,", "tokens": ["Die", "zog", "sie", "fest", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sank zum Lieb dahin.", "tokens": ["Und", "sank", "zum", "Lieb", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}