{"textgrid.poem.59658": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An Henriette von Willich, als ich ihr den Thomas a Kempis \u00bbVon der Nachahmung Christi\u00ab \u00fcberreichte", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel ist gered't, gelesen und geschrieben,", "tokens": ["Viel", "ist", "ge\u00b7red't", ",", "ge\u00b7le\u00b7sen", "und", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit dieses B\u00fcchlein in die Welt gegangen,", "tokens": ["Seit", "die\u00b7ses", "B\u00fcch\u00b7lein", "in", "die", "Welt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Mal und Siegel von dem Geist empfangen,", "tokens": ["Das", "Mal", "und", "Sie\u00b7gel", "von", "dem", "Geist", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe sandte, da\u00df sie lehrte lieben.", "tokens": ["Der", "Lie\u00b7be", "sand\u00b7te", ",", "da\u00df", "sie", "lehr\u00b7te", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie vieles ist gewesen und vergangen,", "tokens": ["Wie", "vie\u00b7les", "ist", "ge\u00b7we\u00b7sen", "und", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "VAPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies B\u00fcchlein hat vier S\u00e4keln \u00fcberdauert,", "tokens": ["Dies", "B\u00fcch\u00b7lein", "hat", "vier", "S\u00e4\u00b7keln", "\u00fc\u00b7berd\u00b7au\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in dem Lande, wo's den Seelen schauert,", "tokens": ["Und", "in", "dem", "Lan\u00b7de", ",", "wo's", "den", "See\u00b7len", "schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lehrt's heute noch das ew'ge Heil erlangen.", "tokens": ["Lehrt's", "heu\u00b7te", "noch", "das", "ew'\u00b7ge", "Heil", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geliebtes Kind, kannst du einf\u00e4ltig fragen,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", ",", "kannst", "du", "ein\u00b7f\u00e4l\u00b7tig", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einf\u00e4ltig wirst du darin Antwort finden:", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "wirst", "du", "da\u00b7rin", "Ant\u00b7wort", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}