{"textgrid.poem.46596": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[da\u00df im Krankenzimmer]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df im Krankenzimmer", "tokens": ["Da\u00df", "im", "Kran\u00b7ken\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bei den lieben Kleinen", "tokens": ["Bei", "den", "lie\u00b7ben", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sie nun sieht den Vater", "tokens": ["Sie", "nun", "sieht", "den", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seltener erscheinen,", "tokens": ["Sel\u00b7te\u00b7ner", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Gilt der klugen Mutter", "tokens": ["Gilt", "der", "klu\u00b7gen", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "F\u00fcr ein gutes Zeichen:", "tokens": ["F\u00fcr", "ein", "gu\u00b7tes", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Krankheit und Besorgni\u00df", "tokens": ["Krank\u00b7heit", "und", "Be\u00b7sorg\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ist nun im Entweichen;", "tokens": ["Ist", "nun", "im", "Ent\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Weil vom Pult nicht jeder", "tokens": ["Weil", "vom", "Pult", "nicht", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PTKNEG", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Augenblick ihn treibet,", "tokens": ["Au\u00b7gen\u00b7blick", "ihn", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Da\u00df er nur die Feder", "tokens": ["Da\u00df", "er", "nur", "die", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Eintaucht und nicht schreibet.", "tokens": ["Ein\u00b7taucht", "und", "nicht", "schrei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Mit dem Vater sieht sie,", "tokens": ["Mit", "dem", "Va\u00b7ter", "sieht", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Da\u00df der Arzt auch gleichen", "tokens": ["Da\u00df", "der", "Arzt", "auch", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Schritt h\u00e4lt, und zuletzt wird", "tokens": ["Schritt", "h\u00e4lt", ",", "und", "zu\u00b7letzt", "wird"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ADV", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Mit dem Uebel weichen;", "tokens": ["Mit", "dem", "Ue\u00b7bel", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Weil er immer sp\u00e4ter", "tokens": ["Weil", "er", "im\u00b7mer", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Kommt und k\u00fcrzer bleibet,", "tokens": ["Kommt", "und", "k\u00fcr\u00b7zer", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Weniger besorgt thut,", "tokens": ["We\u00b7ni\u00b7ger", "be\u00b7sorgt", "thut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.20": {"text": "Und nicht mehr verschreibet.", "tokens": ["Und", "nicht", "mehr", "ver\u00b7schrei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Daf\u00fcr wird durchs Fenster", "tokens": ["Da\u00b7f\u00fcr", "wird", "durchs", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.22": {"text": "Nun dem Sonnenscheine", "tokens": ["Nun", "dem", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Nicht versagt der Eintritt,", "tokens": ["Nicht", "ver\u00b7sagt", "der", "Ein\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Noch der Luft, der reinen.", "tokens": ["Noch", "der", "Luft", ",", "der", "rei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Und hinaus vors Fenster", "tokens": ["Und", "hin\u00b7aus", "vors", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Wandern Arzeneien,", "tokens": ["Wan\u00b7dern", "Ar\u00b7ze\u00b7nei\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Und herein vom Speicher", "tokens": ["Und", "her\u00b7ein", "vom", "Spei\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.28": {"text": "Bessre N\u00e4schereien;", "tokens": ["Bess\u00b7re", "N\u00e4\u00b7sche\u00b7rei\u00b7en", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Statt des Queckentrankes", "tokens": ["Statt", "des", "Que\u00b7cken\u00b7tran\u00b7kes"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Und des Quittenschleimes,", "tokens": ["Und", "des", "Quit\u00b7ten\u00b7schlei\u00b7mes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "Frisches Obst des Schrankes", "tokens": ["Fri\u00b7sches", "Obst", "des", "Schran\u00b7kes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "Aufbewahrt-geheimes.", "tokens": ["Auf\u00b7be\u00b7wahr\u00b7tge\u00b7hei\u00b7mes", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Und die B\u00e4ckchen selber,", "tokens": ["Und", "die", "B\u00e4ck\u00b7chen", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.34": {"text": "Die noch waren bleicher", "tokens": ["Die", "noch", "wa\u00b7ren", "blei\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "Als die Quitten, werden", "tokens": ["Als", "die", "Quit\u00b7ten", ",", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "$,", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.36": {"text": "Wieder apfelgleicher.", "tokens": ["Wie\u00b7der", "ap\u00b7fel\u00b7glei\u00b7cher", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.37": {"text": "Rosen sollen ihnen", "tokens": ["Ro\u00b7sen", "sol\u00b7len", "ih\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.38": {"text": "Bl\u00fchn noch vor dem Maie,", "tokens": ["Bl\u00fchn", "noch", "vor", "dem", "Mai\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.39": {"text": "Wenn das kleine Schw\u00e4rmchen", "tokens": ["Wenn", "das", "klei\u00b7ne", "Schw\u00e4rm\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.40": {"text": "Erst nur darf ins Freie.", "tokens": ["Erst", "nur", "darf", "ins", "Frei\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.41": {"text": "Kirschen werden bl\u00fchen", "tokens": ["Kir\u00b7schen", "wer\u00b7den", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.42": {"text": "Und sogleich auch reifen,", "tokens": ["Und", "sog\u00b7leich", "auch", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Und die Kirschenv\u00f6gel", "tokens": ["Und", "die", "Kir\u00b7schen\u00b7v\u00f6\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.44": {"text": "Werden wieder streifen", "tokens": ["Wer\u00b7den", "wie\u00b7der", "strei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.45": {"text": "In den Kirschengarten,", "tokens": ["In", "den", "Kir\u00b7schen\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.46": {"text": "Wohin sie die Steige", "tokens": ["Wo\u00b7hin", "sie", "die", "Stei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Noch vorm Jahr her kennen.", "tokens": ["Noch", "vorm", "Jahr", "her", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APZR", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.48": {"text": "Da\u00df ich's nicht verschweige,", "tokens": ["Da\u00df", "ich's", "nicht", "ver\u00b7schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.49": {"text": "Was beim ersten Ausflug", "tokens": ["Was", "beim", "ers\u00b7ten", "Aus\u00b7flug"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPRART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.50": {"text": "Auf die Schnabelweide", "tokens": ["Auf", "die", "Schna\u00b7bel\u00b7wei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.51": {"text": "Ich am meisten f\u00fcrchte,", "tokens": ["Ich", "am", "meis\u00b7ten", "f\u00fcrch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "PIS", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.52": {"text": "Und im voraus leide!", "tokens": ["Und", "im", "vo\u00b7raus", "lei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADV", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.53": {"text": "Dort die Frau des G\u00e4rtners,", "tokens": ["Dort", "die", "Frau", "des", "G\u00e4rt\u00b7ners", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.54": {"text": "Die kennt all die Meinen;", "tokens": ["Die", "kennt", "all", "die", "Mei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.55": {"text": "Sieht sie mit geschmolznem", "tokens": ["Sieht", "sie", "mit", "ge\u00b7schmolz\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.56": {"text": "H\u00e4ufchen mich erscheinen,", "tokens": ["H\u00e4uf\u00b7chen", "mich", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.57": {"text": "Wenn sie dann wird fragen:", "tokens": ["Wenn", "sie", "dann", "wird", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.58": {"text": "Wo sind die zwei Kleinen?", "tokens": ["Wo", "sind", "die", "zwei", "Klei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "CARD", "NN", "$."], "meter": "----+-", "measure": "unknown.measure.single"}, "line.59": {"text": "Was soll ich d'rauf sagen,", "tokens": ["Was", "soll", "ich", "d'\u00b7rauf", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Wenn ich nicht will weinen?", "tokens": ["Wenn", "ich", "nicht", "will", "wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.61": {"text": "Von den Knaben allen", "tokens": ["Von", "den", "Kna\u00b7ben", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.62": {"text": "G'rade war der kleinste", "tokens": ["G'\u00b7ra\u00b7de", "war", "der", "kleins\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Ihr ins Aug' gefallen", "tokens": ["Ihr", "ins", "Aug'", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.64": {"text": "Als der allerfeinste.", "tokens": ["Als", "der", "al\u00b7ler\u00b7feins\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.65": {"text": "Und das M\u00e4dchen gar", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.66": {"text": "War die einzig feine,", "tokens": ["War", "die", "ein\u00b7zig", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.67": {"text": "D'rum schon, weil sie war", "tokens": ["D'\u00b7rum", "schon", ",", "weil", "sie", "war"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ADV", "$,", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.68": {"text": "Unsre einzig Eine.", "tokens": ["Uns\u00b7re", "ein\u00b7zig", "Ei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ART", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}