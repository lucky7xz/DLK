{"textgrid.poem.66803": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dreitausend Mark, heidi, per Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreitausend Mark, heidi, per Jahr", "tokens": ["Drei\u00b7tau\u00b7send", "Mark", ",", "hei\u00b7di", ",", "per", "Jahr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "$,", "NE", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Seiner Exzellenz \u2013", "tokens": ["Von", "Sei\u00b7ner", "Ex\u00b7zel\u00b7lenz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie schirmt der Himmel wunderbar", "tokens": ["Wie", "schirmt", "der", "Him\u00b7mel", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lockspitzels Existenz!", "tokens": ["Lock\u00b7spit\u00b7zels", "E\u00b7xis\u00b7tenz", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Kein Gentleman, kein Gentleman,", "tokens": ["Kein", "Gent\u00b7le\u00b7man", ",", "kein", "Gent\u00b7le\u00b7man", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00e4r das ein Malheur,", "tokens": ["Als", "w\u00e4r", "das", "ein", "Mal\u00b7heur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PDS", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "So bin ich denn und bleib' ich denn", "tokens": ["So", "bin", "ich", "denn", "und", "bleib'", "ich", "denn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Spitz, spitz: die Ohren aufgekn\u00f6pft!", "tokens": ["Spitz", ",", "spitz", ":", "die", "Oh\u00b7ren", "auf\u00b7ge\u00b7kn\u00f6pft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Horch, horch nach links und rechts!", "tokens": ["Horch", ",", "horch", "nach", "links", "und", "rechts", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "APPR", "ADV", "KON", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bum, bum! \u00bbUnd alles wird gek\u00f6pft", "tokens": ["Bum", ",", "bum", "!", "\u00bb", "Und", "al\u00b7les", "wird", "ge\u00b7k\u00f6pft"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "XY", "$.", "$(", "KON", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tyrannischen Geschlechts!\u00ab", "tokens": ["Ty\u00b7ran\u00b7ni\u00b7schen", "Ge\u00b7schlechts", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00e4llt mir dann so ein T\u00f6lpel 'rein", "tokens": ["F\u00e4llt", "mir", "dann", "so", "ein", "T\u00f6l\u00b7pel", "'rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und br\u00fcllt: \u00bbDen Teufel \u2013 ja!\u00ab", "tokens": ["Und", "br\u00fcllt", ":", "\u00bb", "Den", "Teu\u00b7fel", "\u2013", "ja", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das mu\u00df ein Anarchiste sein,", "tokens": ["Das", "mu\u00df", "ein", "An\u00b7ar\u00b7chis\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Anarchist, hurra!", "tokens": ["Ein", "An\u00b7ar\u00b7chist", ",", "hur\u00b7ra", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin ein Werkzeug der Gewalt", "tokens": ["Ich", "bin", "ein", "Werk\u00b7zeug", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gottes Gnaden nur,", "tokens": ["Von", "Got\u00b7tes", "Gna\u00b7den", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein unentbehrlich R\u00e4dchen halt", "tokens": ["Ein", "un\u00b7ent\u00b7behr\u00b7lich", "R\u00e4d\u00b7chen", "halt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der Regierungsuhr.", "tokens": ["In", "der", "Re\u00b7gie\u00b7run\u00b7gsuhr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Spitz, spitz, horch, horch! Kein Gentleman,", "tokens": ["Spitz", ",", "spitz", ",", "horch", ",", "horch", "!", "Kein", "Gent\u00b7le\u00b7man", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVIMP", "$,", "VVIMP", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00e4r das ein Malheur.", "tokens": ["Als", "w\u00e4r", "das", "ein", "Mal\u00b7heur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "So bin ich denn und bleib' ich denn", "tokens": ["So", "bin", "ich", "denn", "und", "bleib'", "ich", "denn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbgenosse, prost!\u00ab Begleite ihn,", "tokens": ["\u00bb", "ge\u00b7nos\u00b7se", ",", "prost", "!", "\u00ab", "Be\u00b7glei\u00b7te", "ihn", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$.", "$(", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00fcttl' ihm als Freund die Hand", "tokens": ["Sch\u00fcttl'", "ihm", "als", "Freund", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und melde schleunigst nach Berlin:", "tokens": ["Und", "mel\u00b7de", "schleu\u00b7nigst", "nach", "Ber\u00b7lin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbein frischer Fisch \u2013 brillant!\u00ab", "tokens": ["\u00bb", "ein", "fri\u00b7scher", "Fisch", "\u2013", "bril\u00b7lant", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$(", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus deutschem Reichsfonds stell' ich flugs", "tokens": ["Aus", "deut\u00b7schem", "Reichs\u00b7fonds", "stell'", "ich", "flugs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das s\u00fc\u00dfe Mordsblatt her,", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Mords\u00b7blatt", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die \u00bbFreiheit\u00ab \u2013 kolossaler Jux!", "tokens": ["Die", "\u00bb", "Frei\u00b7heit", "\u00ab", "\u2013", "ko\u00b7los\u00b7sa\u00b7ler", "Jux", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "$(", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ex'lenz, was willst du mehr?", "tokens": ["Ex'\u00b7lenz", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, als ich noch kein Spitzel war,", "tokens": ["Ach", ",", "als", "ich", "noch", "kein", "Spit\u00b7zel", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch kreuzerb\u00e4rmlich Los!", "tokens": ["Welch", "kreu\u00b7zer\u00b7b\u00e4rm\u00b7lich", "Los", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dreitausend Reichsmark jedes Jahr,", "tokens": ["Drei\u00b7tau\u00b7send", "Reichs\u00b7mark", "je\u00b7des", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das macht sich ganz famos.", "tokens": ["Das", "macht", "sich", "ganz", "fa\u00b7mos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mitunter puppert's mir von fern:", "tokens": ["Mi\u00b7tun\u00b7ter", "pup\u00b7pert's", "mir", "von", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbdu Schuft!\u00ab Herrje, ich bin", "tokens": ["\u00bb", "du", "Schuft", "!", "\u00ab", "Herr\u00b7je", ",", "ich", "bin"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "NN", "$.", "$(", "NE", "$,", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein treuer Diener meines Herrn \u2013", "tokens": ["Ein", "treu\u00b7er", "Die\u00b7ner", "mei\u00b7nes", "Herrn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schlag' mir's aus dem Sinn.", "tokens": ["Und", "schlag'", "mir's", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}