{"textgrid.poem.63765": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Cives Romani", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neu \u00fcberh\u00e4uft mit Macht und Glanz und Ehren,", "tokens": ["Neu", "\u00fc\u00b7berh\u00b7\u00e4uft", "mit", "Macht", "und", "Glanz", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "K\u00f6nnt ihr euch nicht erneun an Herz und Sinnen?", "tokens": ["K\u00f6nnt", "ihr", "euch", "nicht", "er\u00b7neun", "an", "Herz", "und", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur eure Weiber sind noch R\u00f6merinnen,", "tokens": ["Nur", "eu\u00b7re", "Wei\u00b7ber", "sind", "noch", "R\u00f6\u00b7me\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Obwohl sie keine R\u00f6mer mehr geb\u00e4ren.", "tokens": ["Ob\u00b7wohl", "sie", "kei\u00b7ne", "R\u00f6\u00b7mer", "mehr", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Groll seht ihr die Fremdenflut sich mehren,", "tokens": ["Mit", "Groll", "seht", "ihr", "die", "Frem\u00b7den\u00b7flut", "sich", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Italiens Banner wehn von euren Zinnen.", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "Ban\u00b7ner", "wehn", "von", "eu\u00b7ren", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur da\u00df ihr jetzt am Mietzins m\u00f6gt gewinnen,", "tokens": ["Nur", "da\u00df", "ihr", "jetzt", "am", "Miet\u00b7zins", "m\u00f6gt", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vermag die finstren Stirnen aufzukl\u00e4ren.", "tokens": ["Ver\u00b7mag", "die", "finst\u00b7ren", "Stir\u00b7nen", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch \u2013 statt des Gepl\u00e4rrs der Bettlerorden", "tokens": ["Und", "doch", "\u2013", "statt", "des", "Ge\u00b7pl\u00e4rrs", "der", "Bett\u00b7le\u00b7ror\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie munter klingt der kriegerischen Banden", "tokens": ["Wie", "mun\u00b7ter", "klingt", "der", "krie\u00b7ge\u00b7ri\u00b7schen", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Musik, ein frischer Zukunftshauch aus Norden!", "tokens": ["Mu\u00b7sik", ",", "ein", "fri\u00b7scher", "Zu\u00b7kunfts\u00b7hauch", "aus", "Nor\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn die p\u00e4pstlichen Karossen schwanden", "tokens": ["Und", "wenn", "die", "p\u00e4pst\u00b7li\u00b7chen", "Ka\u00b7ros\u00b7sen", "schwan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Rot- und Violettstrumpf rar geworden,", "tokens": ["Und", "Rot", "und", "Vi\u00b7o\u00b7lett\u00b7strumpf", "rar", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blaustr\u00fcmpfe doch sind reichlich noch vorhanden.", "tokens": ["Blau\u00b7str\u00fcmp\u00b7fe", "doch", "sind", "reich\u00b7lich", "noch", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADJD", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}