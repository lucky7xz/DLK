{"dta.poem.498": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An seine Liebste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Vor deinen Augen mu\u00df die lichte Sonne weichen/", "tokens": ["Vor", "dei\u00b7nen", "Au\u00b7gen", "mu\u00df", "die", "lich\u00b7te", "Son\u00b7ne", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lippen stechen weg Zinnober und Rubin/", "tokens": ["Die", "Lip\u00b7pen", "ste\u00b7chen", "weg", "Zin\u00b7no\u00b7ber", "und", "Ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "KON", "NE", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor deinen Wangen mu\u00df der Rosen Pracht erbleichen/", "tokens": ["Vor", "dei\u00b7nen", "Wan\u00b7gen", "mu\u00df", "der", "Ro\u00b7sen", "Pracht", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sanffte Athen-West f\u00fchrt ", "tokens": ["Der", "sanff\u00b7te", "A\u00b7then\u00b7West", "f\u00fchrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du bist recht wohlgeziert an Sch\u00f6nheit und Geberden/", "tokens": ["Du", "bist", "recht", "wohl\u00b7ge\u00b7ziert", "an", "Sch\u00f6n\u00b7heit", "und", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So da\u00df dich je dermann als \u00fcberirdisch ehrt.", "tokens": ["So", "da\u00df", "dich", "je", "der\u00b7mann", "als", "\u00fc\u00b7be\u00b7rir\u00b7disch", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PIS", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich aber mu\u00df darum auch dein Leibeigner werden/", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "da\u00b7rum", "auch", "dein", "Leib\u00b7eig\u00b7ner", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PAV", "ADV", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil holde Freund lichkeit der Sch\u00f6nheit Pracht vermehrt.", "tokens": ["Weil", "hol\u00b7de", "Freund", "lich\u00b7keit", "der", "Sch\u00f6n\u00b7heit", "Pracht", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}