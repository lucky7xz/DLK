{"textgrid.poem.44608": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr Scheffer", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer im Gro\u00dfen wirkt und Weiten,", "tokens": ["Wer", "im", "Gro\u00b7\u00dfen", "wirkt", "und", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat den Dank f\u00fcr alle Zeiten,", "tokens": ["Hat", "den", "Dank", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Nachwelt r\u00fchmend Wort", "tokens": ["Und", "der", "Nach\u00b7welt", "r\u00fch\u00b7mend", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Setzt das Lob der Mitwelt fort.", "tokens": ["Setzt", "das", "Lob", "der", "Mit\u00b7welt", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer jedoch in engerm Kreise", "tokens": ["Wer", "je\u00b7doch", "in", "en\u00b7germ", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wirkt nach echter Mannesweise,", "tokens": ["Wirkt", "nach", "ech\u00b7ter", "Man\u00b7nes\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, was er als B\u00fcrger tat,", "tokens": ["Der", ",", "was", "er", "als", "B\u00fcr\u00b7ger", "tat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auch als ", "tokens": ["Auch", "als"], "token_info": ["word", "word"], "pos": ["ADV", "KOKOM"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Dem sei Lohn in seinen Tagen,", "tokens": ["Dem", "sei", "Lohn", "in", "sei\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hrend noch die Herzen schlagen,", "tokens": ["W\u00e4h\u00b7rend", "noch", "die", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die des Reichtums sich bewu\u00dft", "tokens": ["Die", "des", "Reich\u00b7tums", "sich", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PRF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner heut geschm\u00fcckten Brust.", "tokens": ["Sei\u00b7ner", "heut", "ge\u00b7schm\u00fcck\u00b7ten", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}