{"textgrid.poem.37687": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwere, harte, scharfe Stunden", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwere, harte, scharfe Stunden", "tokens": ["Schwe\u00b7re", ",", "har\u00b7te", ",", "schar\u00b7fe", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich wie Kiesel an mir runden,", "tokens": ["Sich", "wie", "Kie\u00b7sel", "an", "mir", "run\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "NN", "APPR", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Lebens Wellenschlag,", "tokens": ["In", "des", "Le\u00b7bens", "Wel\u00b7len\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich f\u00fchl', was ich vermag;", "tokens": ["Und", "ich", "f\u00fchl'", ",", "was", "ich", "ver\u00b7mag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fromme Freundin, ich durft' weinen,", "tokens": ["From\u00b7me", "Freun\u00b7din", ",", "ich", "durft'", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durft' auf deinen H\u00e4nden weinen,", "tokens": ["Durft'", "auf", "dei\u00b7nen", "H\u00e4n\u00b7den", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und gedeckt von deinen H\u00e4nden", "tokens": ["Und", "ge\u00b7deckt", "von", "dei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Konnte Schwachheit mich nicht sch\u00e4nden.", "tokens": ["Konn\u00b7te", "Schwach\u00b7heit", "mich", "nicht", "sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Regentropfen h\u00f6hlen Steine,", "tokens": ["Re\u00b7gen\u00b7trop\u00b7fen", "h\u00f6h\u00b7len", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich tief verschlossen meine,", "tokens": ["Was", "ich", "tief", "ver\u00b7schlos\u00b7sen", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6hlet meines Ungl\u00fccks Stein,", "tokens": ["H\u00f6h\u00b7let", "mei\u00b7nes", "Un\u00b7gl\u00fccks", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllt ihn bald mit Freudenwein;", "tokens": ["F\u00fcllt", "ihn", "bald", "mit", "Freu\u00b7den\u00b7wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freundin, nimm vom Freudenweine,", "tokens": ["Freun\u00b7din", ",", "nimm", "vom", "Freu\u00b7den\u00b7wei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Komm zu mir, du heil'ge, reine,", "tokens": ["Komm", "zu", "mir", ",", "du", "heil'\u00b7ge", ",", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPER", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und beselige mein Mahl,", "tokens": ["Und", "be\u00b7se\u00b7li\u00b7ge", "mein", "Mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bin ich frei von aller Qual.", "tokens": ["Bin", "ich", "frei", "von", "al\u00b7ler", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fchlend kannst du an mich glauben,", "tokens": ["F\u00fch\u00b7lend", "kannst", "du", "an", "mich", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir lieb, nicht spottend rauben,", "tokens": ["Was", "mir", "lieb", ",", "nicht", "spot\u00b7tend", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "$,", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich aus der Seele sprach,", "tokens": ["Was", "ich", "aus", "der", "See\u00b7le", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Klingt dir aus der Seele nach.", "tokens": ["Klingt", "dir", "aus", "der", "See\u00b7le", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fromme Freundin aller Reinen,", "tokens": ["From\u00b7me", "Freun\u00b7din", "al\u00b7ler", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du kannst tr\u00f6sten, du kannst weinen,", "tokens": ["Du", "kannst", "tr\u00f6s\u00b7ten", ",", "du", "kannst", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn du mich auch nicht verstehst,", "tokens": ["Wenn", "du", "mich", "auch", "nicht", "ver\u00b7stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles dir im Geist erh\u00f6hst.", "tokens": ["Al\u00b7les", "dir", "im", "Geist", "er\u00b7h\u00f6hst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}