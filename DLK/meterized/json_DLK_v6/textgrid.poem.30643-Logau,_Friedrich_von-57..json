{"textgrid.poem.30643": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann der Kl\u00f6pffel schl\u00e4gt die Glocke, gibt es einen lauten Hall;", "tokens": ["Wann", "der", "Kl\u00f6pf\u00b7fel", "schl\u00e4gt", "die", "Glo\u00b7cke", ",", "gibt", "es", "ei\u00b7nen", "lau\u00b7ten", "Hall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wann der Mann das Weib casteyet, gibt es einen weiten Schall.", "tokens": ["Wann", "der", "Mann", "das", "Weib", "cas\u00b7te\u00b7yet", ",", "gibt", "es", "ei\u00b7nen", "wei\u00b7ten", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Diese Glocke, wann sie klingt, klingt sie meistens zu dem singen;", "tokens": ["Die\u00b7se", "Glo\u00b7cke", ",", "wann", "sie", "klingt", ",", "klingt", "sie", "meis\u00b7tens", "zu", "dem", "sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Selten aber, wann sie klingt, wil sie zum Gebete klingen.", "tokens": ["Sel\u00b7ten", "a\u00b7ber", ",", "wann", "sie", "klingt", ",", "wil", "sie", "zum", "Ge\u00b7be\u00b7te", "klin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}