{"textgrid.poem.49945": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Frau Luise Walther, geb. v. Breitschwert", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie manchen Morgen, frisch und wohlgemut,", "tokens": ["Wie", "man\u00b7chen", "Mor\u00b7gen", ",", "frisch", "und", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im lichten Sommerkleid, Feldblumen auf dem Hut,", "tokens": ["Im", "lich\u00b7ten", "Som\u00b7mer\u00b7kleid", ",", "Feld\u00b7blu\u00b7men", "auf", "dem", "Hut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Trat sie bei uns, die edle Freundin, ein,", "tokens": ["Trat", "sie", "bei", "uns", ",", "die", "ed\u00b7le", "Freun\u00b7din", ",", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie sie kam, da war es Sonnenschein!", "tokens": ["Und", "wie", "sie", "kam", ",", "da", "war", "es", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als ob sie weiter gar nicht wollte oder w\u00fc\u00dfte,", "tokens": ["Als", "ob", "sie", "wei\u00b7ter", "gar", "nicht", "woll\u00b7te", "o\u00b7der", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "PTKNEG", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur da\u00df sie jedermann zur Freude dasein m\u00fc\u00dfte,", "tokens": ["Nur", "da\u00df", "sie", "je\u00b7der\u00b7mann", "zur", "Freu\u00b7de", "da\u00b7sein", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIS", "APPRART", "NN", "PAV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lebte sie in klarer Gegenwart,", "tokens": ["So", "leb\u00b7te", "sie", "in", "kla\u00b7rer", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Neidlos bei andrer Gl\u00fcck, die Lachende, die Feine;", "tokens": ["Neid\u00b7los", "bei", "an\u00b7drer", "Gl\u00fcck", ",", "die", "La\u00b7chen\u00b7de", ",", "die", "Fei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch heimlich sah ich's oft in ahnungsvollem Scheine", "tokens": ["Doch", "heim\u00b7lich", "sah", "ich's", "oft", "in", "ah\u00b7nungs\u00b7vol\u00b7lem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hoch \u00fcber dieses Scheitels Reine", "tokens": ["Hoch", "\u00fc\u00b7ber", "die\u00b7ses", "Schei\u00b7tels", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie einen sel'gen Stern, der seiner Stunde harrt.", "tokens": ["Wie", "ei\u00b7nen", "sel'\u00b7gen", "Stern", ",", "der", "sei\u00b7ner", "Stun\u00b7de", "harrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun ist's geschehn! und mit verkl\u00e4rtem Blicke", "tokens": ["Nun", "ist's", "ge\u00b7schehn", "!", "und", "mit", "ver\u00b7kl\u00e4r\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihres Lebens Gipfel l\u00e4chelt sie;", "tokens": ["Von", "ih\u00b7res", "Le\u00b7bens", "Gip\u00b7fel", "l\u00e4\u00b7chelt", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es war geschehn, kaum wei\u00df sie selber wie,", "tokens": ["Es", "war", "ge\u00b7schehn", ",", "kaum", "wei\u00df", "sie", "sel\u00b7ber", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "KOKOM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn j\u00e4h erf\u00fcllen sich die himmlischen Geschicke.", "tokens": ["Denn", "j\u00e4h", "er\u00b7f\u00fcl\u00b7len", "sich", "die", "himm\u00b7li\u00b7schen", "Ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}