{"textgrid.poem.59930": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann, der liebt, darf nicht zu bl\u00f6de sein,", "tokens": ["Ein", "Mann", ",", "der", "liebt", ",", "darf", "nicht", "zu", "bl\u00f6\u00b7de", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VMFIN", "PTKNEG", "PTKA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Abschreckend stets ist zuviel Bl\u00f6digkeit!", "tokens": ["Ab\u00b7schre\u00b7ckend", "stets", "ist", "zu\u00b7viel", "Bl\u00f6\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein Weib, das liebt, darf nicht zu spr\u00f6de sein,", "tokens": ["Ein", "Weib", ",", "das", "liebt", ",", "darf", "nicht", "zu", "spr\u00f6\u00b7de", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "$,", "VMFIN", "PTKNEG", "PTKZU", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Abschreckend stets ist zuviel Spr\u00f6digkeit!", "tokens": ["Ab\u00b7schre\u00b7ckend", "stets", "ist", "zu\u00b7viel", "Spr\u00f6\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}