{"textgrid.poem.63426": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ick weet nich, wat dat heeten schall \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ick weet nich, wat dat heeten schall \u2013", "tokens": ["Ick", "weet", "nich", ",", "wat", "dat", "hee\u00b7ten", "schall", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hannjochen kickt mi \u00fcmmer nah,", "tokens": ["Hann\u00b7jo\u00b7chen", "kickt", "mi", "\u00fcm\u00b7mer", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in D\u00f6nsk un Hoff un Sch\u00fcn un Stall", "tokens": ["in", "D\u00f6nsk", "un", "Hoff", "un", "Sch\u00fcn", "un", "Stall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "un wu ick gah un wu ick stah;", "tokens": ["un", "wu", "ick", "gah", "un", "wu", "ick", "stah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "denn kickt he \u00fcm de Eck un denn", "tokens": ["denn", "kickt", "he", "\u00fcm", "de", "Eck", "un", "denn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "FM", "FM", "FM", "FM", "FM", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "eens wedder \u0153wer Tun un Rick,", "tokens": ["e\u00b7ens", "wed\u00b7der", "\u0153wer", "Tun", "un", "Rick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "denn ut de Luk gor nah mi hen \u2013", "tokens": ["denn", "ut", "de", "Luk", "gor", "nah", "mi", "hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "dat is un blifft een narrsches St\u00fcck!", "tokens": ["dat", "is", "un", "blifft", "e\u00b7en", "narr\u00b7sches", "St\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Un wenn he buten haken deit", "tokens": ["Un", "wenn", "he", "bu\u00b7ten", "ha\u00b7ken", "deit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "NE", "ADJA", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "un ick kam nah de R\u00e4gel ran,", "tokens": ["un", "ick", "kam", "nah", "de", "R\u00e4\u00b7gel", "ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPER", "VVFIN", "ADJD", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "denn seggt he: prr! De Haken steiht,", "tokens": ["denn", "seggt", "he", ":", "prr", "!", "De", "Ha\u00b7ken", "steiht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$.", "NE", "$.", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "hei dreigt sick \u00fcm un glupt mi an;", "tokens": ["hei", "dreigt", "sick", "\u00fcm", "un", "glupt", "mi", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "un hal ick's Abens Spol un Fla\u00df", "tokens": ["un", "hal", "ick's", "A\u00b7bens", "Spol", "un", "Fla\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un schuw min Spinnrad ranner ick,", "tokens": ["un", "schuw", "min", "Spinn\u00b7rad", "ran\u00b7ner", "ick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "denn schult he nah mi hen f\u00f6rdwas \u2013", "tokens": ["denn", "schult", "he", "nah", "mi", "hen", "f\u00f6rd\u00b7was", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "dat is un blifft een narrsches St\u00fcck!", "tokens": ["dat", "is", "un", "blifft", "e\u00b7en", "narr\u00b7sches", "St\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "He plirt nah mi, he kapt mi an,", "tokens": ["He", "plirt", "nah", "mi", ",", "he", "kapt", "mi", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "as wenn he mi wat fragen m\u00fccht,", "tokens": ["as", "wenn", "he", "mi", "wat", "fra\u00b7gen", "m\u00fccht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "FM", "FM", "FM", "VVINF", "VMFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "as ob dat \u0153werst rut nich kann,", "tokens": ["as", "ob", "dat", "\u0153werst", "rut", "nich", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "VVFIN", "PTKNEG", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "he sick tov\u00e4l bes\u00fcnn, mi d\u00fccht;", "tokens": ["he", "sick", "to\u00b7v\u00e4l", "be\u00b7s\u00fcnn", ",", "mi", "d\u00fccht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dat deit \u00fcm em mi orig leed,", "tokens": ["dat", "deit", "\u00fcm", "em", "mi", "o\u00b7rig", "leed", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "ick s\u00e4d em jo den Ogenblick", "tokens": ["ick", "s\u00e4d", "em", "jo", "den", "O\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "girn up de St\u00e4d, all wat ick weet \u2013", "tokens": ["girn", "up", "de", "St\u00e4d", ",", "all", "wat", "ick", "weet", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PIAT", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "dat is un blifft en narrsches St\u00fcck!", "tokens": ["dat", "is", "un", "blifft", "en", "narr\u00b7sches", "St\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}