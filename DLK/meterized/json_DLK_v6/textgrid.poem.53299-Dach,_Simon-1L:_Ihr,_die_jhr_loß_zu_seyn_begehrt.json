{"textgrid.poem.53299": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr, die jhr lo\u00df zu seyn begehrt", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die jhr lo\u00df zu seyn begehrt", "tokens": ["Ihr", ",", "die", "jhr", "lo\u00df", "zu", "seyn", "be\u00b7gehrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "PTKVZ", "PTKZU", "VAINF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ewren Missethaten,", "tokens": ["Von", "ew\u00b7ren", "Mis\u00b7se\u00b7tha\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heut hat sich Gott zu uns gekehrt", "tokens": ["Heut", "hat", "sich", "Gott", "zu", "uns", "ge\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd wil uns Armen rahten,", "tokens": ["Vnd", "wil", "uns", "Ar\u00b7men", "rah\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er eussert sich der Herrligkeit", "tokens": ["Er", "eus\u00b7sert", "sich", "der", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd wil uns an Geberden", "tokens": ["Vnd", "wil", "uns", "an", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aehnlich werden,", "tokens": ["A\u00b7ehn\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Deswegen dann sich frewt", "tokens": ["Des\u00b7we\u00b7gen", "dann", "sich", "frewt"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der Himmel sampt der Erden.", "tokens": ["Der", "Him\u00b7mel", "sampt", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er ist uns gleich an Fleisch und Blut,", "tokens": ["Er", "ist", "uns", "gleich", "an", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vns also zu vertreten,", "tokens": ["Vns", "al\u00b7so", "zu", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat hiedurch uns von der Glut", "tokens": ["Er", "hat", "hie\u00b7durch", "uns", "von", "der", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der Hellen lo\u00df gebeten", "tokens": ["Der", "Hel\u00b7len", "lo\u00df", "ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd wird der Himmels-B\u00fcrgerschafft", "tokens": ["Vnd", "wird", "der", "Him\u00b7mels\u00b7B\u00fcr\u00b7ger\u00b7schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vns nachmals einverleiben,", "tokens": ["Vns", "nach\u00b7mals", "ein\u00b7ver\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df wir bleiben", "tokens": ["Da\u00df", "wir", "blei\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Da, wo der Frewden Krafft", "tokens": ["Da", ",", "wo", "der", "Frew\u00b7den", "Krafft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wird alles Leid vertreiben.", "tokens": ["Wird", "al\u00b7les", "Leid", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum kommt, lasst uns mit Frewden gehn", "tokens": ["Drum", "kommt", ",", "lasst", "uns", "mit", "Frew\u00b7den", "gehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd unsern Heyland schawen,", "tokens": ["Vnd", "un\u00b7sern", "Hey\u00b7land", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Lasst uns f\u00fcr seiner Krippen stehn", "tokens": ["Lasst", "uns", "f\u00fcr", "sei\u00b7ner", "Krip\u00b7pen", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd Ihm von Hertzen trawen,", "tokens": ["Vnd", "Ihm", "von", "Hert\u00b7zen", "tra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er wird aus seiner Mutter Scho\u00df", "tokens": ["Er", "wird", "aus", "sei\u00b7ner", "Mut\u00b7ter", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Aermlein nach uns strecken", "tokens": ["Die", "A\u00b7erm\u00b7lein", "nach", "uns", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vnd erwecken,", "tokens": ["Vnd", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Was f\u00fcr der S\u00fcnden sto\u00df", "tokens": ["Was", "f\u00fcr", "der", "S\u00fcn\u00b7den", "sto\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Vns ewig wird bedecken.", "tokens": ["Vns", "e\u00b7wig", "wird", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}