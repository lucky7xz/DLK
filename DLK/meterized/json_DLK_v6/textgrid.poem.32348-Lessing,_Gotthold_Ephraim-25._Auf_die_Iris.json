{"textgrid.poem.32348": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "25. Auf die Iris", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Iris bl\u00fchend volle Brust", "tokens": ["Der", "I\u00b7ris", "bl\u00fc\u00b7hend", "vol\u00b7le", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Reizt uns, o D*, zu welcher Lust!", "tokens": ["Reizt", "uns", ",", "o", "D", "*", ",", "zu", "wel\u00b7cher", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "XY", "XY", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ihr erb\u00e4rmliches Gesichte,", "tokens": ["Doch", "ihr", "er\u00b7b\u00e4rm\u00b7li\u00b7ches", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O D*, macht Reiz und Lust zu nichte.", "tokens": ["O", "D", "*", ",", "macht", "Reiz", "und", "Lust", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "XY", "XY", "$,", "VVFIN", "NN", "KON", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh, Freund, so liegen Frost und Flammen,", "tokens": ["Sieh", ",", "Freund", ",", "so", "lie\u00b7gen", "Frost", "und", "Flam\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Gift und Gegengift beisammen.", "tokens": ["Und", "Gift", "und", "Ge\u00b7gen\u00b7gift", "bei\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}