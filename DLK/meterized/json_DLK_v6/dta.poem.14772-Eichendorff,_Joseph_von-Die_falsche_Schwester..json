{"dta.poem.14772": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die falsche Schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die ganze Nacht hat gelogen,", "tokens": ["Die", "gan\u00b7ze", "Nacht", "hat", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie hat mich so falsch gegr\u00fc\u00dft,", "tokens": ["Sie", "hat", "mich", "so", "falsch", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Engel sind fortgeflogen,", "tokens": ["Die", "En\u00b7gel", "sind", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Haus und Garten steh'n w\u00fcst.", "tokens": ["Und", "Haus", "und", "Gar\u00b7ten", "steh'n", "w\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}