{"textgrid.poem.62592": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je mehr der Himmel sich ergeu\u00dft,", "tokens": ["Je", "mehr", "der", "Him\u00b7mel", "sich", "er\u00b7geu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Regen auf die Erde fleu\u00dft;", "tokens": ["Und", "Re\u00b7gen", "auf", "die", "Er\u00b7de", "fleu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Je rein und kl\u00e4rer wird alsdann die Luft.", "tokens": ["Je", "rein", "und", "kl\u00e4\u00b7rer", "wird", "als\u00b7dann", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So auch, je mehr aus deiner Augen Licht", "tokens": ["So", "auch", ",", "je", "mehr", "aus", "dei\u00b7ner", "Au\u00b7gen", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Thr\u00e4nen-Gu\u00df der S\u00fcnde wegen bricht;", "tokens": ["Ein", "Thr\u00e4\u00b7nen\u00b7Gu\u00df", "der", "S\u00fcn\u00b7de", "we\u00b7gen", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Je reiner wird dein Herz auf dieser Erden", "tokens": ["Je", "rei\u00b7ner", "wird", "dein", "Herz", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vom S\u00fcnden-Schlamm gesaubert werden.", "tokens": ["Vom", "S\u00fcn\u00b7den\u00b7Schlamm", "ge\u00b7sau\u00b7bert", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}