{"dta.poem.2708": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erlangte Ruhe in dem Vaterlande  \n  Hn. G. G. den 3. Martii  1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje wird die schwartze Post dein gantzes Hau\u00df erschre-\ncken", "tokens": ["Wie", "wird", "die", "schwart\u00b7ze", "Post", "dein", "gant\u00b7zes", "Hau\u00df", "er\u00b7schre", "cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "TRUNC", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun du/ o Seeliger/ gehst in der Frembde heim!", "tokens": ["Nun", "du", "/", "o", "See\u00b7li\u00b7ger", "/", "gehst", "in", "der", "Fremb\u00b7de", "heim", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "FM", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und solte Thr\u00e4nen nicht dein Todes-Fall erwecken?", "tokens": ["Und", "sol\u00b7te", "Thr\u00e4\u00b7nen", "nicht", "dein", "To\u00b7des\u00b7Fall", "er\u00b7we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun sich in Wermuth kehrt der Hoffnung Honigseim.", "tokens": ["Nun", "sich", "in", "Wer\u00b7muth", "kehrt", "der", "Hoff\u00b7nung", "Ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie warten gantz umbsonst dich freudig zu empfangen", "tokens": ["Sie", "war\u00b7ten", "gantz", "um\u00b7bsonst", "dich", "freu\u00b7dig", "zu", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die GOtt dir zugesellt und die dein Blut erzeugt:", "tokens": ["Die", "Gott", "dir", "zu\u00b7ge\u00b7sellt", "und", "die", "dein", "Blut", "er\u00b7zeugt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "KON", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dieweil du bist den Weg zur langen Ruh gegangen/", "tokens": ["Die\u00b7weil", "du", "bist", "den", "Weg", "zur", "lan\u00b7gen", "Ruh", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du schreibst nicht mehr zur\u00fcck und Mund und Feder", "tokens": ["Du", "schreibst", "nicht", "mehr", "zu\u00b7r\u00fcck", "und", "Mund", "und", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So sah Alcyone durch n\u00e4chtliche Gesichte/", "tokens": ["So", "sah", "Al\u00b7cyo\u00b7ne", "durch", "n\u00e4cht\u00b7li\u00b7che", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "(schweigt.", "tokens": ["(", "schweigt", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Ein Vorbild/ wie ihr Schatz im Scho\u00df der See ertranck.", "tokens": ["Ein", "Vor\u00b7bild", "/", "wie", "ihr", "Schatz", "im", "Scho\u00df", "der", "See", "er\u00b7tranck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOKOM", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer zweiffelt da\u00df der Mund von fliegendem Ger\u00fcchte", "tokens": ["Wer", "zweif\u00b7felt", "da\u00df", "der", "Mund", "von", "flie\u00b7gen\u00b7dem", "Ge\u00b7r\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bereit erzehlet hat/ wie du zu Bre\u00dflau kranck.", "tokens": ["Be\u00b7reit", "er\u00b7zeh\u00b7let", "hat", "/", "wie", "du", "zu", "Bre\u00df\u00b7lau", "kranck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$(", "PWAV", "PPER", "APPR", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wiewol es f\u00fchrt dich GOtt auff seinen Wunder-Wegen", "tokens": ["Wie\u00b7wol", "es", "f\u00fchrt", "dich", "Gott", "auff", "sei\u00b7nen", "Wun\u00b7der\u00b7We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und was sein heilig Schlu\u00df bestimmt/ das mu\u00df geschehn.", "tokens": ["Und", "was", "sein", "hei\u00b7lig", "Schlu\u00df", "be\u00b7stimmt", "/", "das", "mu\u00df", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJD", "NN", "VVPP", "$(", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du solst im Vaterland dich sanfft zur Ruhe legen", "tokens": ["Du", "solst", "im", "Va\u00b7ter\u00b7land", "dich", "sanfft", "zur", "Ru\u00b7he", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PPER", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und dich wird Dantzig nicht mehr seinen B\u00fcrger sehn.", "tokens": ["Und", "dich", "wird", "Dant\u00b7zig", "nicht", "mehr", "sei\u00b7nen", "B\u00fcr\u00b7ger", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NE", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Rei\u00df ist wol verbracht. Jetzt bist du heimgezogen", "tokens": ["Die", "Rei\u00df", "ist", "wol", "ver\u00b7bracht", ".", "Jetzt", "bist", "du", "heim\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vors erst in Schlesien/ denn nach dem Himmel zu/", "tokens": ["Vors", "erst", "in", "Schle\u00b7si\u00b7en", "/", "denn", "nach", "dem", "Him\u00b7mel", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NE", "$(", "KON", "APPR", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da baut die Freyheit dir so einen Sieges-Bogen", "tokens": ["Da", "baut", "die", "Frey\u00b7heit", "dir", "so", "ei\u00b7nen", "Sie\u00b7ges\u00b7Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der deine Pilgramschafft deckt mit gew\u00fcnschter Ruh.", "tokens": ["Der", "dei\u00b7ne", "Pil\u00b7gramsc\u00b7hafft", "deckt", "mit", "ge\u00b7w\u00fcnschter", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Wir sind doch frembd allhier/ und G\u00e4ste dieser Erden/", "tokens": ["Wir", "sind", "doch", "frembd", "all\u00b7hier", "/", "und", "G\u00e4s\u00b7te", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$(", "KON", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Beschiffen ein solch Meer das tausend Syrten hegt.", "tokens": ["Be\u00b7schif\u00b7fen", "ein", "solch", "Meer", "das", "tau\u00b7send", "Syr\u00b7ten", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIAT", "NN", "ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und unser gantzer Lauff ist Kummer und Beschwerden", "tokens": ["Und", "un\u00b7ser", "gant\u00b7zer", "Lauff", "ist", "Kum\u00b7mer", "und", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bi\u00df da\u00df man uns verblast hin zu drn meisten tr\u00e4gt.", "tokens": ["Bi\u00df", "da\u00df", "man", "uns", "ver\u00b7blast", "hin", "zu", "drn", "meis\u00b7ten", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie ist die grosse See nicht voller Tieff\u2019 und Kr\u00fcmmen?", "tokens": ["Wie", "ist", "die", "gros\u00b7se", "See", "nicht", "vol\u00b7ler", "Tief\u00b7f'", "und", "Kr\u00fcm\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Wie lacht uns ihr Crystall in aller Sanfftmuth an?", "tokens": ["Wie", "lacht", "uns", "ihr", "Crys\u00b7tall", "in", "al\u00b7ler", "Sanfft\u00b7muth", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bald wird sie umb das Schiff mit linden Wellen schwimmen/", "tokens": ["Bald", "wird", "sie", "umb", "das", "Schiff", "mit", "lin\u00b7den", "Wel\u00b7len", "schwim\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Als wenn ohn Arg und List die Marmel-glatte Bahn.", "tokens": ["Als", "wenn", "ohn", "Arg", "und", "List", "die", "Mar\u00b7mel\u00b7glat\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "NN", "KON", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "In einem Augenblick wird drauff ihr Zorn sich heben/", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "wird", "drauff", "ihr", "Zorn", "sich", "he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PAV", "PPOSAT", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df sie den grauen Schaum bi\u00df an die Sterne schmeist/", "tokens": ["Da\u00df", "sie", "den", "grau\u00b7en", "Schaum", "bi\u00df", "an", "die", "Ster\u00b7ne", "schmeist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Winde nichts als Sturm/ die L\u00fcffte Donner geben/", "tokens": ["Die", "Win\u00b7de", "nichts", "als", "Sturm", "/", "die", "L\u00fcff\u00b7te", "Don\u00b7ner", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "KOKOM", "NN", "$(", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und das erschrockne Schiff mit Mast und Kiel zerreist.", "tokens": ["Und", "das", "er\u00b7schrock\u00b7ne", "Schiff", "mit", "Mast", "und", "Kiel", "zer\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Von gr\u00f6sserm Unbestand/ von mehr gesaltznen Wellen", "tokens": ["Von", "gr\u00f6s\u00b7serm", "Un\u00b7be\u00b7stand", "/", "von", "mehr", "ge\u00b7saltz\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ist unser Lebens-See/ jetzt zeigt sie Ebb\u2019 jetzt Fluth:", "tokens": ["Ist", "un\u00b7ser", "Le\u00b7bens\u00b7See", "/", "jetzt", "zeigt", "sie", "Ebb'", "jetzt", "Fluth", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wird gleich der Morgen sich in lichter Sonn\u2019 erhellen/", "tokens": ["Wird", "gleich", "der", "Mor\u00b7gen", "sich", "in", "lich\u00b7ter", "Sonn'", "er\u00b7hel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So wei\u00df man/ da\u00df sie doch vor Abends schaden thut.", "tokens": ["So", "wei\u00df", "man", "/", "da\u00df", "sie", "doch", "vor", "A\u00b7bends", "scha\u00b7den", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "KOUS", "PPER", "ADV", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wir streiffen hin und her von Winden der Begierden", "tokens": ["Wir", "streif\u00b7fen", "hin", "und", "her", "von", "Win\u00b7den", "der", "Be\u00b7gier\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Versuchungen des Fleischs und Blutes angef\u00fchrt;", "tokens": ["Ver\u00b7su\u00b7chun\u00b7gen", "des", "Fleischs", "und", "Blu\u00b7tes", "an\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Bald lockt die Wollust uns durch der Sirenen Zierden/", "tokens": ["Bald", "lockt", "die", "Wol\u00b7lust", "uns", "durch", "der", "Si\u00b7re\u00b7nen", "Zier\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Bald sieht man wie der Geitz auff die Charybden r\u00fchrt.", "tokens": ["Bald", "sieht", "man", "wie", "der", "Geitz", "auff", "die", "Cha\u00b7ryb\u00b7den", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "meas