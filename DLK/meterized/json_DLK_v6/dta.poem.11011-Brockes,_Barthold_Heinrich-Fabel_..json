{"dta.poem.11011": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fabel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es stund ein starker Pfahl, auch eine schlanke Weyde", "tokens": ["Es", "stund", "ein", "star\u00b7ker", "Pfahl", ",", "auch", "ei\u00b7ne", "schlan\u00b7ke", "Wey\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An einem breiten Flu\u00df, der sich im Herbst ergossen.", "tokens": ["An", "ei\u00b7nem", "brei\u00b7ten", "Flu\u00df", ",", "der", "sich", "im", "Herbst", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie waren folglich alle beyde", "tokens": ["Sie", "wa\u00b7ren", "folg\u00b7lich", "al\u00b7le", "bey\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom ausgetretnen Strom beflossen.", "tokens": ["Vom", "aus\u00b7ge\u00b7tret\u00b7nen", "Strom", "be\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun fing der strenge Nord ergrimmt an, zu regieren,", "tokens": ["Nun", "fing", "der", "stren\u00b7ge", "Nord", "er\u00b7grimmt", "an", ",", "zu", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und durch den scharfen Hauch das Wasser zuzufrieren.", "tokens": ["Und", "durch", "den", "schar\u00b7fen", "Hauch", "das", "Was\u00b7ser", "zu\u00b7zu\u00b7frie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von scharfen Schollen schwall die Strudel-reiche Fluth,", "tokens": ["Von", "schar\u00b7fen", "Schol\u00b7len", "schwall", "die", "Stru\u00b7del\u00b7rei\u00b7che", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zeigt in Wirbeln, Schaum und Brausen, ihre Wuth,", "tokens": ["Und", "zeigt", "in", "Wir\u00b7beln", ",", "Schaum", "und", "Brau\u00b7sen", ",", "ih\u00b7re", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Treib-Eis h\u00e4ufte sich, und pre\u00dft, im strengen Gange,", "tokens": ["Das", "Treib\u00b7Eis", "h\u00e4uf\u00b7te", "sich", ",", "und", "pre\u00dft", ",", "im", "stren\u00b7gen", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was ihm entgegen stund, die Weyde beugte sich", "tokens": ["Was", "ihm", "ent\u00b7ge\u00b7gen", "stund", ",", "die", "Wey\u00b7de", "beug\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PTKVZ", "VVFIN", "$,", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vor dem f\u00fcr sie zu starken Drange.", "tokens": ["Vor", "dem", "f\u00fcr", "sie", "zu", "star\u00b7ken", "Dran\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Indessen, da\u00df der Pfahl nicht um ein Haar breit wich,", "tokens": ["In\u00b7des\u00b7sen", ",", "da\u00df", "der", "Pfahl", "nicht", "um", "ein", "Haar", "breit", "wich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und durch sich selbst gesteift, noch Kraft noch Muth verlieret.", "tokens": ["Und", "durch", "sich", "selbst", "ge\u00b7steift", ",", "noch", "Kraft", "noch", "Muth", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "VVPP", "$,", "ADV", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Allein des Eises Macht ward gr\u00f6\u00dfer, und der Pfahl", "tokens": ["Al\u00b7lein", "des", "Ei\u00b7ses", "Macht", "ward", "gr\u00f6\u00b7\u00dfer", ",", "und", "der", "Pfahl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wurd auf einmal", "tokens": ["Wurd", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Heraus gerissen, weggef\u00fchret.", "tokens": ["He\u00b7raus", "ge\u00b7ris\u00b7sen", ",", "weg\u00b7ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Weide f\u00fchlte zwar auch an der Rinde Wunden:", "tokens": ["Die", "Wei\u00b7de", "f\u00fchl\u00b7te", "zwar", "auch", "an", "der", "Rin\u00b7de", "Wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Allein, sie hub so gut, als wie zuvor,", "tokens": ["Al\u00b7lein", ",", "sie", "hub", "so", "gut", ",", "als", "wie", "zu\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nachdem das Eis vorbey, das Haupt empor,", "tokens": ["Nach\u00b7dem", "das", "Eis", "vor\u00b7bey", ",", "das", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Vom Pfahl indessen ward die Stelle nicht gefunden.", "tokens": ["Vom", "Pfahl", "in\u00b7des\u00b7sen", "ward", "die", "Stel\u00b7le", "nicht", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du bist, geliebtes Vaterland, wie wir in alten Schriften lesen,", "tokens": ["Du", "bist", ",", "ge\u00b7lieb\u00b7tes", "Va\u00b7ter\u00b7land", ",", "wie", "wir", "in", "al\u00b7ten", "Schrif\u00b7ten", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.22": {"text": "Bey nicht so allgemeinem Sturm gar oft ein Weydenbaum", "tokens": ["Bey", "nicht", "so", "all\u00b7ge\u00b7mei\u00b7nem", "Sturm", "gar", "oft", "ein", "Wey\u00b7den\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PTKNEG", "ADV", "ADJA", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "So sey denn auch vor diesesmal,", "tokens": ["So", "sey", "denn", "auch", "vor", "die\u00b7ses\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Da mehr, als je, die Winde st\u00fcrmen,", "tokens": ["Da", "mehr", ",", "als", "je", ",", "die", "Win\u00b7de", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und keine St\u00fctzen dich beschirmen,", "tokens": ["Und", "kei\u00b7ne", "St\u00fct\u00b7zen", "dich", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wie gro\u00df dein Recht auch, doch kein Pfahl.", "tokens": ["Wie", "gro\u00df", "dein", "Recht", "auch", ",", "doch", "kein", "Pfahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "ADV", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}