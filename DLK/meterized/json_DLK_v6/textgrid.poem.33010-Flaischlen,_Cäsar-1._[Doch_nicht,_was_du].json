{"textgrid.poem.33010": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1. [Doch nicht, was du]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch nicht, was du von au\u00dfen packst,", "tokens": ["Doch", "nicht", ",", "was", "du", "von", "au\u00b7\u00dfen", "packst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ob dich ein Zufall gl\u00fccklich leitet,", "tokens": ["ob", "dich", "ein", "Zu\u00b7fall", "gl\u00fcck\u00b7lich", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und wenn du's noch so scharf umzackst,", "tokens": ["und", "wenn", "du's", "noch", "so", "scharf", "um\u00b7zackst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "kr\u00f6nt dich zum Sieger und entscheidet ...", "tokens": ["kr\u00f6nt", "dich", "zum", "Sie\u00b7ger", "und", "ent\u00b7schei\u00b7det", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein: ob du's mit den Wurzeln greifst", "tokens": ["Nein", ":", "ob", "du's", "mit", "den", "Wur\u00b7zeln", "greifst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und wie du's stimmst und wie du's reifst", "tokens": ["und", "wie", "du's", "stimmst", "und", "wie", "du's", "reifst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "KON", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in stiller Tage stillem Werden,", "tokens": ["in", "stil\u00b7ler", "Ta\u00b7ge", "stil\u00b7lem", "Wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ob du's zur Sonne aufw\u00e4rts hebst,", "tokens": ["ob", "du's", "zur", "Son\u00b7ne", "auf\u00b7w\u00e4rts", "hebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "empor aus seines Unwerts Tr\u00fcbe,", "tokens": ["em\u00b7por", "aus", "sei\u00b7nes", "Un\u00b7werts", "Tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "empor aus seines Werktags Dunst,", "tokens": ["em\u00b7por", "aus", "sei\u00b7nes", "Werk\u00b7tags", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ob du's mit deinem Ich durchlebst", "tokens": ["ob", "du's", "mit", "dei\u00b7nem", "Ich", "durch\u00b7lebst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und mit der Sehnsucht deiner Liebe,", "tokens": ["und", "mit", "der", "Sehn\u00b7sucht", "dei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "dem Gottesatem freier Kunst.", "tokens": ["dem", "Got\u00b7te\u00b7sa\u00b7tem", "frei\u00b7er", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was sollen wir mit fremder Menschen", "tokens": ["Was", "sol\u00b7len", "wir", "mit", "frem\u00b7der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "gleichg\u00fcltiger Lust, gleichg\u00fcltigem Leid?!", "tokens": ["gleich\u00b7g\u00fcl\u00b7ti\u00b7ger", "Lust", ",", "gleich\u00b7g\u00fcl\u00b7ti\u00b7gem", "Leid", "?!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du gib ihm Wort erst, Wert und Weihe", "tokens": ["Du", "gib", "ihm", "Wort", "erst", ",", "Wert", "und", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVIMP", "PPER", "NN", "ADV", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zu dem, dem du dich selbst geweiht!", "tokens": ["zu", "dem", ",", "dem", "du", "dich", "selbst", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen dich, nicht ... uns, nicht andre!", "tokens": ["Wir", "wol\u00b7len", "dich", ",", "nicht", "...", "uns", ",", "nicht", "and\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PTKNEG", "$(", "PPER", "$,", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wir wollen dich, was dich bewegt,", "tokens": ["wir", "wol\u00b7len", "dich", ",", "was", "dich", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "was dich ... auf freigek\u00e4mpften Schwingen,", "tokens": ["was", "dich", "...", "auf", "frei\u00b7ge\u00b7k\u00e4mpf\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "dem Staub enttr\u00e4gt,", "tokens": ["dem", "Staub", "ent\u00b7tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "dem Staub, dem Dunst, in dem wir ringen,", "tokens": ["dem", "Staub", ",", "dem", "Dunst", ",", "in", "dem", "wir", "rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "der M\u00fchsal zwischen Heut und Morgen,", "tokens": ["der", "M\u00fch\u00b7sal", "zwi\u00b7schen", "Heut", "und", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "die uns mit ewigen Pfennig-Sorgen", "tokens": ["die", "uns", "mit", "e\u00b7wi\u00b7gen", "Pfen\u00b7nig\u00b7Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "um unser bestes Teil betr\u00fcgt!", "tokens": ["um", "un\u00b7ser", "bes\u00b7tes", "Teil", "be\u00b7tr\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit deines Wortes m\u00e4chtigem Werde", "tokens": ["Mit", "dei\u00b7nes", "Wor\u00b7tes", "m\u00e4ch\u00b7ti\u00b7gem", "Wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zerrei\u00df die Nebel, schaff uns Licht ...", "tokens": ["zer\u00b7rei\u00df", "die", "Ne\u00b7bel", ",", "schaff", "uns", "Licht", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und \u00fcber unserem kleinen Dasein", "tokens": ["und", "\u00fc\u00b7ber", "un\u00b7se\u00b7rem", "klei\u00b7nen", "Da\u00b7sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mit seinem riesengro\u00dfen Leid", "tokens": ["mit", "sei\u00b7nem", "rie\u00b7sen\u00b7gro\u00b7\u00dfen", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "zeig uns die morgengoldenen Feuer", "tokens": ["zeig", "uns", "die", "mor\u00b7gen\u00b7gol\u00b7de\u00b7nen", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "der Sonne deiner Ewigkeit!", "tokens": ["der", "Son\u00b7ne", "dei\u00b7ner", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}