{"textgrid.poem.24425": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gott/ dieweil du mich an diesem Tag' erschaffen;", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott/ dieweil du mich an diesem Tag' erschaffen;", "tokens": ["Mein", "Gott", "/", "die\u00b7weil", "du", "mich", "an", "die\u00b7sem", "Tag'", "er\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "PRF", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aber meist daran S\u00fcnden vor geschlaffen:", "tokens": ["Ich", "a\u00b7ber", "meist", "da\u00b7ran", "S\u00fcn\u00b7den", "vor", "ge\u00b7schlaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PAV", "NN", "APPR", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So gieb/ da\u00df dieses Licht/ so mich zur Welt gebracht/", "tokens": ["So", "gieb", "/", "da\u00df", "die\u00b7ses", "Licht", "/", "so", "mich", "zur", "Welt", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$(", "KOUS", "PDAT", "NN", "$(", "ADV", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein steter Morgen sey/ wo meine Seel erwacht.", "tokens": ["Ein", "ste\u00b7ter", "Mor\u00b7gen", "sey", "/", "wo", "mei\u00b7ne", "Seel", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "AD