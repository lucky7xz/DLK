{"dta.poem.1533": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Geburts-Feyer/ Ertz-Hertzogs JOSEPH S/  \n numehro R\u00f6mischen und Hungarischen  \n K\u00f6nigs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Verneute K\u00e4yser-Burg/ mit was f\u00fcr hellen Stralen", "tokens": ["Ver\u00b7neu\u00b7te", "K\u00e4y\u00b7ser\u00b7Burg", "/", "mit", "was", "f\u00fcr", "hel\u00b7len", "Stra\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bezierst du unser deutsches Land!", "tokens": ["Be\u00b7zierst", "du", "un\u00b7ser", "deut\u00b7sches", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel mu\u00df sich dir mit neuen Farben mahlen/", "tokens": ["Der", "Him\u00b7mel", "mu\u00df", "sich", "dir", "mit", "neu\u00b7en", "Far\u00b7ben", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PPER", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Purpur nehmen zum Gewand.", "tokens": ["Sein", "Pur\u00b7pur", "neh\u00b7men", "zum", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er zeigt mit Freuden an/ wie bey der K\u00e4yser-Wiege", "tokens": ["Er", "zeigt", "mit", "Freu\u00b7den", "an", "/", "wie", "bey", "der", "K\u00e4y\u00b7ser\u00b7Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$(", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gl\u00fcck/ Segen/ Wonn und Heyl und Cron und Zepter liege.", "tokens": ["Gl\u00fcck", "/", "Se\u00b7gen", "/", "Wonn", "und", "Heyl", "und", "Cron", "und", "Zep\u00b7ter", "lie\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NE", "KON", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}