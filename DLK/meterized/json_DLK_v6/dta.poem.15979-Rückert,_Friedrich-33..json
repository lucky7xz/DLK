{"dta.poem.15979": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dort wo das Wissen mit dem Seyn zusammenf\u00e4llt,", "tokens": ["Dort", "wo", "das", "Wis\u00b7sen", "mit", "dem", "Seyn", "zu\u00b7sam\u00b7men\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem Bewu\u00dftseyn ist der Mittelpunkt der Welt.", "tokens": ["In", "dem", "Be\u00b7wu\u00df\u00b7tseyn", "ist", "der", "Mit\u00b7tel\u00b7punkt", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur im Bewu\u00dftseyn was du findest, ist gefunden,", "tokens": ["Nur", "im", "Be\u00b7wu\u00df\u00b7tseyn", "was", "du", "fin\u00b7dest", ",", "ist", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sich ein Aeu\u00dferes dem Inneren verbunden.", "tokens": ["Wo", "sich", "ein", "A\u00b7e\u00b7u\u00b7\u00dfe\u00b7res", "dem", "In\u00b7ne\u00b7ren", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Nur im Bewu\u00dftseyn wenn dir Gott ist aufgegangen,", "tokens": ["Nur", "im", "Be\u00b7wu\u00df\u00b7tseyn", "wenn", "dir", "Gott", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KOUS", "PPER", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du ihn wirklich, und gestillt ist dein Verlangen.", "tokens": ["Hast", "du", "ihn", "wirk\u00b7lich", ",", "und", "ge\u00b7stillt", "ist", "dein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du hast ihn nicht gedacht, er ward dir nicht gegeben,", "tokens": ["Du", "hast", "ihn", "nicht", "ge\u00b7dacht", ",", "er", "ward", "dir", "nicht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er lebt in dir, und macht dich und die Welt dir leben.", "tokens": ["Er", "lebt", "in", "dir", ",", "und", "macht", "dich", "und", "die", "Welt", "dir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}