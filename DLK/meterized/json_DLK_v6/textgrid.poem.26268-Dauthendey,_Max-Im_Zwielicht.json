{"textgrid.poem.26268": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Im Zwielicht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Zwielicht lagern K\u00e4hne auf der Meeresfl\u00e4che.", "tokens": ["Im", "Zwie\u00b7licht", "la\u00b7gern", "K\u00e4h\u00b7ne", "auf", "der", "Mee\u00b7res\u00b7fl\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von d\u00fcnnen Masten springt ein schmales Licht.", "tokens": ["Von", "d\u00fcn\u00b7nen", "Mas\u00b7ten", "springt", "ein", "schma\u00b7les", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich Widerschein im Wasser, leben mir Gespr\u00e4che,", "tokens": ["Gleich", "Wi\u00b7der\u00b7schein", "im", "Was\u00b7ser", ",", "le\u00b7ben", "mir", "Ge\u00b7spr\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu denen meine Augen ein Gesicht sich weben.", "tokens": ["Zu", "de\u00b7nen", "mei\u00b7ne", "Au\u00b7gen", "ein", "Ge\u00b7sicht", "sich", "we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bleib! Bleib! Du sollst in schweren N\u00e4chten mich umgeben.", "tokens": ["Bleib", "!", "Bleib", "!", "Du", "sollst", "in", "schwe\u00b7ren", "N\u00e4ch\u00b7ten", "mich", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VMFIN", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}