{"textgrid.poem.34656": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schweig/ mein hertz/ und la\u00df es seyn;", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweig/ mein hertz/ und la\u00df es seyn;", "tokens": ["Schweig", "/", "mein", "hertz", "/", "und", "la\u00df", "es", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "$(", "KON", "VVIMP", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es ist dir recht geschehen/", "tokens": ["Denn", "es", "ist", "dir", "recht", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du wirst eher stahl und stein/", "tokens": ["Du", "wirst", "e\u00b7her", "stahl", "und", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "KON", "ADJD", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Als Sylvetten liebend sehen.", "tokens": ["Als", "Syl\u00b7vet\u00b7ten", "lie\u00b7bend", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bring es/ wie du wilst/ so nah/", "tokens": ["Bring", "es", "/", "wie", "du", "wilst", "/", "so", "nah", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWAV", "PPER", "VMFIN", "$(", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So erlangt dein treues flehen", "tokens": ["So", "er\u00b7langt", "dein", "treu\u00b7es", "fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch niemahls ein s\u00fcsses ja.", "tokens": ["Doch", "nie\u00b7mahls", "ein", "s\u00fcs\u00b7ses", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}