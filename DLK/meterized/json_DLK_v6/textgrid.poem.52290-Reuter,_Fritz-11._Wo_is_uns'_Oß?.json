{"textgrid.poem.52290": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "11. Wo is uns' O\u00df?", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In sinen Staul sitt P\u00e4chter Kawelmaker", "tokens": ["In", "si\u00b7nen", "Staul", "sitt", "P\u00e4ch\u00b7ter", "Ka\u00b7wel\u00b7ma\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Un deiht, wat oft hei deiht, un sl\u00f6ppt.", "tokens": ["Un", "deiht", ",", "wat", "oft", "hei", "deiht", ",", "un", "sl\u00f6ppt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,", "FM", "FM", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunn fohrt tau em herinner Danjel Haker,", "tokens": ["Dunn", "fohrt", "tau", "em", "he\u00b7rin\u00b7ner", "Dan\u00b7jel", "Ha\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sin olle Ossenknecht, un r\u00f6ppt:", "tokens": ["Sin", "ol\u00b7le", "Os\u00b7sen\u00b7knecht", ",", "un", "r\u00f6ppt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "\u00bbherr, dor is wat passiert, is wat passiert,", "tokens": ["\u00bb", "herr", ",", "dor", "is", "wat", "pas\u00b7siert", ",", "is", "wat", "pas\u00b7siert", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wat ganz Gef\u00e4hrlich's is passiert!", "tokens": ["Wat", "ganz", "Ge\u00b7f\u00e4hr\u00b7lich's", "is", "pas\u00b7siert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ick gl\u00f6w just nich an't Hexen un an't Sp\u00e4uken", "tokens": ["Ick", "gl\u00f6w", "just", "nich", "an't", "He\u00b7xen", "un", "an't", "Sp\u00e4u\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Un an den D\u00fcwel un an so'n Maf\u00e4uken,", "tokens": ["Un", "an", "den", "D\u00fc\u00b7wel", "un", "an", "so'n", "Ma\u00b7f\u00e4u\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "FM", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Doch hir, hir hett de D\u00fcwel drin sin Spill!\u00ab", "tokens": ["Doch", "hir", ",", "hir", "hett", "de", "D\u00fc\u00b7wel", "drin", "sin", "Spill", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbholl't Mul, du Klas! \u2013 Wenn ick mal slapen will\u00ab,", "tokens": ["\u00bb", "holl't", "Mul", ",", "du", "Klas", "!", "\u2013", "Wenn", "ick", "mal", "sla\u00b7pen", "will", "\u00ab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "PPER", "NN", "$.", "$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Seggt de oll Herr, \u00bbun will en beten rauhn,", "tokens": ["Seggt", "de", "oll", "Herr", ",", "\u00bb", "un", "will", "en", "be\u00b7ten", "rauhn", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "NN", "$,", "$(", "FM", "FM", "FM", "VVINF", "VVINF", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Denn m\u00f6t ein jeder Nahr mi st\u00fcren;", "tokens": ["Denn", "m\u00f6t", "ein", "je\u00b7der", "Nahr", "mi", "st\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "PIAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "As hadd ick wider nicks tau dauhn,", "tokens": ["As", "hadd", "ick", "wi\u00b7der", "nicks", "tau", "dauhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "As jeden Dr\u00e4hnsnack antauh\u00fcren.\u00ab", "tokens": ["As", "je\u00b7den", "Dr\u00e4hns\u00b7nack", "an\u00b7tau\u00b7h\u00fc\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PIAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "\u00bbne, wat tau dull is, is tau dull!", "tokens": ["\u00bb", "ne", ",", "wat", "tau", "dull", "is", ",", "is", "tau", "dull", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "FM", "FM", "FM", "FM", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ick stek em irst de R\u00f6p noch vull", "tokens": ["Ick", "stek", "em", "irst", "de", "R\u00f6p", "noch", "vull"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Von't beste Klewerheu,", "tokens": ["Von't", "bes\u00b7te", "Kle\u00b7wer\u00b7heu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ick gaww em frische Streu \u2013", "tokens": ["Ick", "gaww", "em", "fri\u00b7sche", "Streu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Nu is hei doch uns fleuten gahn!\u00ab", "tokens": ["Nu", "is", "hei", "doch", "uns", "fleu\u00b7ten", "gahn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "FM", "FM", "KON", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "\u00bbwer is denn weg, du Dummerjahn?", "tokens": ["\u00bb", "wer", "is", "denn", "weg", ",", "du", "Dum\u00b7mer\u00b7jahn", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "FM", "KON", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ick kann jo nich en Wurd verstahn.\u00ab", "tokens": ["Ick", "kann", "jo", "nich", "en", "Wurd", "ver\u00b7stahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "NE", "PTKNEG", "VVINF", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "\u00bbje Herr, weit ick't?", "tokens": ["\u00bb", "je", "Herr", ",", "weit", "ick't", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Ick ward noch heil un deil verr\u00fcckt.", "tokens": ["Ick", "ward", "noch", "heil", "un", "deil", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "FM", "ADV", "ADJD", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.24": {"text": "Ick sw\u00f6r' Sei't tau bi Gott in'n Himmel:", "tokens": ["Ick", "sw\u00f6r'", "Sei't", "tau", "bi", "Gott", "in'n", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Uns' beste O\u00df, uns' sch\u00f6ne St\u00fcmmel!\u00ab", "tokens": ["Un\u00b7s'", "bes\u00b7te", "O\u00df", ",", "un\u00b7s'", "sch\u00f6\u00b7ne", "St\u00fcm\u00b7mel", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.26": {"text": "\u00bbwo, du b\u00fcst woll nich recht bi Sinnen?", "tokens": ["\u00bb", "wo", ",", "du", "b\u00fcst", "woll", "nich", "recht", "bi", "Sin\u00b7nen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "ADJD", "NN", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.27": {"text": "Wo s\u00fcll de O\u00df denn sin? Hei ward sick finnen.\u00ab", "tokens": ["Wo", "s\u00fcll", "de", "O\u00df", "denn", "sin", "?", "Hei", "ward", "sick", "fin\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NE", "NE", "NE", "KON", "PTKVZ", "$.", "NN", "VAFIN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "\u00bbne, Herr, ne, Herr! Dor helpt kein Reden,", "tokens": ["\u00bb", "ne", ",", "Herr", ",", "ne", ",", "Herr", "!", "Dor", "helpt", "kein", "Re\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NN", "$,", "NE", "$,", "NN", "$.", "NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Dor helpt kein Singen un kein Beden,", "tokens": ["Dor", "helpt", "kein", "Sin\u00b7gen", "un", "kein", "Be\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "FM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Uns' O\u00df is weg, uns' O\u00df is weg!", "tokens": ["Un\u00b7s'", "O\u00df", "is", "weg", ",", "un\u00b7s'", "O\u00df", "is", "weg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "PTKVZ", "$,", "FM", "FM", "FM", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Un wenn ick segg: ick segg!,", "tokens": ["Un", "wenn", "ick", "segg", ":", "ick", "segg", "!", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.32": {"text": "Denn k\u00e4nen Sei gl\u00f6wen, weit'ck Bescheid.", "tokens": ["Denn", "k\u00e4\u00b7nen", "Sei", "gl\u00f6\u00b7wen", ",", "weit'ck", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,", "KOUS", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "De Sak hett nich ehr Richtigkeit.\u00ab", "tokens": ["De", "Sak", "hett", "nich", "ehr", "Rich\u00b7tig\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VAFIN", "PTKNEG", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "\u00bbna, meinst du denn, sei heww'n em stahlen?\u00ab", "tokens": ["\u00bb", "na", ",", "meinst", "du", "denn", ",", "sei", "hew\u00b7w'n", "em", "stah\u00b7len", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "XY", "XY", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "\u00bbih, Herr, dat s\u00fcll mi ok noch fehlen!", "tokens": ["\u00bb", "ih", ",", "Herr", ",", "dat", "s\u00fcll", "mi", "ok", "noch", "feh\u00b7len", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NN", "$,", "FM", "FM", "FM", "FM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Wer ward des Middags Ossen stehlen?", "tokens": ["Wer", "ward", "des", "Mid\u00b7dags", "Os\u00b7sen", "steh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Den'n m\u00fc\u00dft doch glik de D\u00fcwel halen.", "tokens": ["Den'n", "m\u00fc\u00dft", "doch", "glik", "de", "D\u00fc\u00b7wel", "ha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "NN", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Oh, ne! Dit's wedder so as dunn mit minen Hund;", "tokens": ["Oh", ",", "ne", "!", "Dit's", "wed\u00b7der", "so", "as", "dunn", "mit", "mi\u00b7nen", "Hund", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "NE", "KON", "ADV", "NE", "NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+---+----+", "measure": "unknown.measure.tetra"}, "line.39": {"text": "De Sak, de hett en annern Grund.\u00ab", "tokens": ["De", "Sak", ",", "de", "hett", "en", "an\u00b7nern", "Grund", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "\u00bbei wat! So holl din Mul un kumm!", "tokens": ["\u00bb", "ei", "wat", "!", "So", "holl", "din", "Mul", "un", "kumm", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$.", "ADV", "ADJD", "PPOSAT", "NN", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Makst mi am En'n noch s\u00fclwst noch dumm.\u00ab", "tokens": ["Makst", "mi", "am", "En'n", "noch", "s\u00fclwst", "noch", "dumm", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "ADV", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.42": {"text": "Sei gungen beid' nu nah den Stall herun.", "tokens": ["Sei", "gun\u00b7gen", "beid'", "nu", "nah", "den", "Stall", "he\u00b7run", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Je, ja! je, ja! Wo s\u00fcs de St\u00fcmmel stunn,", "tokens": ["Je", ",", "ja", "!", "je", ",", "ja", "!", "Wo", "s\u00fcs", "de", "St\u00fcm\u00b7mel", "stunn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "ADV", "$,", "ADV", "$.", "PWAV", "NE", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Dor was en leddig Flag,", "tokens": ["Dor", "was", "en", "led\u00b7dig", "Flag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Dor was dat hellig Dag.", "tokens": ["Dor", "was", "dat", "hel\u00b7lig", "Dag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.46": {"text": "De Oll, de kikt nah Danjel Hakern,", "tokens": ["De", "Oll", ",", "de", "kikt", "nah", "Dan\u00b7jel", "Ha\u00b7kern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Un Danjel kikt nah Kawelmakern;", "tokens": ["Un", "Dan\u00b7jel", "kikt", "nah", "Ka\u00b7wel\u00b7ma\u00b7kern", ";"], "token_info": ["word", "word", "word", "word",