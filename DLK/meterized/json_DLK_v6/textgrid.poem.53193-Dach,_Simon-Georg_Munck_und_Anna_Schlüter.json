{"textgrid.poem.53193": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Georg Munck und Anna Schl\u00fcter", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was thut Herr Munck", "tokens": ["Was", "thut", "Herr", "Munck"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "F\u00fcr einen Sprung?", "tokens": ["F\u00fcr", "ei\u00b7nen", "Sprung", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nicht von des Thurmes Spitzen,", "tokens": ["Nicht", "von", "des", "Thur\u00b7mes", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer also springt,", "tokens": ["Wer", "al\u00b7so", "springt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nach Vngl\u00fck ringt,", "tokens": ["Nach", "Vn\u00b7gl\u00fck", "ringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "K\u00f6mpt ziemlich schlecht zu sitzen.", "tokens": ["K\u00f6mpt", "ziem\u00b7lich", "schlecht", "zu", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der F\u00fcrwitz hat", "tokens": ["Der", "F\u00fcr\u00b7witz", "hat"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bey jhm nicht stat,", "tokens": ["Bey", "jhm", "nicht", "stat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er l\u00e4sst bisher ja mercken", "tokens": ["Er", "l\u00e4sst", "bis\u00b7her", "ja", "mer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur allerhand", "tokens": ["Nur", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word"], "pos": ["ADV", "PIAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Glimpff und Verstand", "tokens": ["Glimpff", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "In allen seinen Wercken.", "tokens": ["In", "al\u00b7len", "sei\u00b7nen", "Wer\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer Ehlo\u00df lebt,", "tokens": ["Wer", "Eh\u00b7lo\u00df", "lebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Derselbe schwebt", "tokens": ["Der\u00b7sel\u00b7be", "schwebt"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "F\u00fcrwar auff hohen Zinnen,", "tokens": ["F\u00fcr\u00b7war", "auff", "ho\u00b7hen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dannen man", "tokens": ["Von", "dan\u00b7nen", "man"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gar leichtlich kan", "tokens": ["Gar", "leicht\u00b7lich", "kan"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Gefahr und Fall gewinnen.", "tokens": ["Ge\u00b7fahr", "und", "Fall", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie mancher ist,", "tokens": ["Wie", "man\u00b7cher", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der sich die List", "tokens": ["Der", "sich", "die", "List"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hie kl\u00e4glich l\u00e4sset f\u00e4llen,", "tokens": ["Hie", "kl\u00e4g\u00b7lich", "l\u00e4s\u00b7set", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd einen Sprung", "tokens": ["Vnd", "ei\u00b7nen", "Sprung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch Venus Trunck", "tokens": ["Durch", "Ve\u00b7nus", "Trunck"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Thut gar bis in die Hellen!", "tokens": ["Thut", "gar", "bis", "in", "die", "Hel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, der Gefahr", "tokens": ["Nein", ",", "der", "Ge\u00b7fahr"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wird er gewahr", "tokens": ["Wird", "er", "ge\u00b7wahr"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vnd wil so steil nicht stehen,", "tokens": ["Vnd", "wil", "so", "steil", "nicht", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er schawt umbher", "tokens": ["Er", "schawt", "um\u00b7bher"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die l\u00e4ng' und quer", "tokens": ["Die", "l\u00e4ng'", "und", "quer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Von dieses Gipffels H\u00f6hen.", "tokens": ["Von", "die\u00b7ses", "Gipf\u00b7fels", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nur schwerer Fall", "tokens": ["Nur", "schwe\u00b7rer", "Fall"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wohnt \u00fcberall,", "tokens": ["Wohnt", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In solchem Vngehewer", "tokens": ["In", "sol\u00b7chem", "Vn\u00b7ge\u00b7he\u00b7wer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Schwingt er den Sinn", "tokens": ["Schwingt", "er", "den", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Gen Himmel hin,", "tokens": ["Gen", "Him\u00b7mel", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der k\u00f6mpt jhm auch zu stewer.", "tokens": ["Der", "k\u00f6mpt", "jhm", "auch", "zu", "ste\u00b7wer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Legt Fl\u00fcgel an", "tokens": ["Legt", "Fl\u00fc\u00b7gel", "an"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dem guten Mann,", "tokens": ["Dem", "gu\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Rath und Vorsicht f\u00fchren,", "tokens": ["Die", "Rath", "und", "Vor\u00b7sicht", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerweht jhm auch", "tokens": ["Zer\u00b7weht", "jhm", "auch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Augen Rauch", "tokens": ["Der", "Au\u00b7gen", "Rauch"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vnd l\u00e4sst jhn heller sp\u00fcren.", "tokens": ["Vnd", "l\u00e4sst", "jhn", "hel\u00b7ler", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es steht ein Saal", "tokens": ["Es", "steht", "ein", "Saal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im Heyraht-Thal", "tokens": ["Im", "Hey\u00b7raht\u00b7Thal"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Myrten gantz umbfangen,", "tokens": ["Mit", "Myr\u00b7ten", "gantz", "umb\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da Lieb und Trew", "tokens": ["Da", "Lieb", "und", "Trew"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vnd Rhu dabey", "tokens": ["Vnd", "Rhu", "da\u00b7bey"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "An allen B\u00e4umen hangen.", "tokens": ["An", "al\u00b7len", "B\u00e4u\u00b7men", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da schawt er hin,", "tokens": ["Da", "schawt", "er", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Schl\u00fcterinn,", "tokens": ["Die", "Schl\u00fc\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gekr\u00f6nt mit Ehr und Tugend,", "tokens": ["Ge\u00b7kr\u00f6nt", "mit", "Ehr", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00f6pfft ohngefehr", "tokens": ["Sch\u00f6pfft", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "---+", "measure": "unknown.measure.single"}, "line.5": {"text": "Lust da umbher", "tokens": ["Lust", "da", "um\u00b7bher"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Nach Art der keuschen Jugend.", "tokens": ["Nach", "Art", "der", "keu\u00b7schen", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da bald Herr Munck", "tokens": ["Da", "bald", "Herr", "Munck"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Fasst einen Schwung,", "tokens": ["Fasst", "ei\u00b7nen", "Schwung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie sieht jhn abwerts fahren", "tokens": ["Sie", "sieht", "jhn", "ab\u00b7werts", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd l\u00e4ufft jhm ein,", "tokens": ["Vnd", "l\u00e4ufft", "jhm", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "(was kan nicht seyn,", "tokens": ["(", "was", "kan", "nicht", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wenn etwas sich sol paaren?)", "tokens": ["Wenn", "et\u00b7was", "sich", "sol", "paa\u00b7ren", "?", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PRF", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Vnd in dem Lauff", "tokens": ["Vnd", "in", "dem", "Lauff"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Fasst sie jhn auff,", "tokens": ["Fasst", "sie", "jhn", "auff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er bleibet gar beliegen", "tokens": ["Er", "blei\u00b7bet", "gar", "be\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Kummers lo\u00df", "tokens": ["Des", "Kum\u00b7mers", "lo\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In jhrem Scho\u00df.", "tokens": ["In", "jhrem", "Scho\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wol dem, der so kan fliegen.", "tokens": ["Wol", "dem", ",", "der", "so", "kan", "flie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.12": {"line.1": {"text": "Der Sprung geb\u00e4hr", "tokens": ["Der", "Sprung", "ge\u00b7b\u00e4hr"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Jhm kein Beschwer,", "tokens": ["Jhm", "kein", "Be\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wir w\u00fcnschen jhm zusammen,", "tokens": ["Wir", "w\u00fcn\u00b7schen", "jhm", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df manche Funck", "tokens": ["Da\u00df", "man\u00b7che", "Funck"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Was? mancher Munck", "tokens": ["Was", "?", "man\u00b7cher", "Munck"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWS", "$.", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Entspring' aus jhren Flammen.", "tokens": ["Ent\u00b7spring'", "aus", "jhren", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}