{"textgrid.poem.45849": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbich habe das gar nicht verlangt\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaut kalt vom hellen Himmel Mondschein nieder", "tokens": ["Schaut", "kalt", "vom", "hel\u00b7len", "Him\u00b7mel", "Mond\u00b7schein", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So st\u00f6ret keine Farbe meinen Blick", "tokens": ["So", "st\u00f6\u00b7ret", "kei\u00b7ne", "Far\u00b7be", "mei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder Ton klingt in mich selbst zur\u00fcck", "tokens": ["Ein", "je\u00b7der", "Ton", "klingt", "in", "mich", "selbst", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der reine Spiegel giebt mein Bild mir wieder.", "tokens": ["Der", "rei\u00b7ne", "Spie\u00b7gel", "giebt", "mein", "Bild", "mir", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Echo wacht, es kehren alle Lieder", "tokens": ["Das", "E\u00b7cho", "wacht", ",", "es", "keh\u00b7ren", "al\u00b7le", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verklungen in dem wechselnden Geschick,", "tokens": ["Ver\u00b7klun\u00b7gen", "in", "dem", "wech\u00b7seln\u00b7den", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das fr\u00fch verschwundne Bild vom innern Gl\u00fcck", "tokens": ["Das", "fr\u00fch", "ver\u00b7schwund\u00b7ne", "Bild", "vom", "in\u00b7nern", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Enth\u00fcllt sich froh, bewegt die leichten Glieder.", "tokens": ["Ent\u00b7h\u00fcllt", "sich", "froh", ",", "be\u00b7wegt", "die", "leich\u00b7ten", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Herzen f\u00fchle ich ein freudig Bangen,", "tokens": ["Im", "Her\u00b7zen", "f\u00fch\u00b7le", "ich", "ein", "freu\u00b7dig", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein k\u00fchles Meer von Glanz schwillt um die Brust,", "tokens": ["Ein", "k\u00fch\u00b7les", "Meer", "von", "Glanz", "schwillt", "um", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin so sch\u00f6n von Dir in mir gefangen,", "tokens": ["Ich", "bin", "so", "sch\u00f6n", "von", "Dir", "in", "mir", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch alles dies bleibt Dir stets ohnbewu\u00dft", "tokens": ["Doch", "al\u00b7les", "dies", "bleibt", "Dir", "stets", "ohn\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PDS", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du hast mir ", "tokens": ["Du", "hast", "mir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und nichts ", "tokens": ["Und", "nichts"], "token_info": ["word", "word"], "pos": ["KON", "PIS"], "meter": "-+", "measure": "iambic.single"}}}}}