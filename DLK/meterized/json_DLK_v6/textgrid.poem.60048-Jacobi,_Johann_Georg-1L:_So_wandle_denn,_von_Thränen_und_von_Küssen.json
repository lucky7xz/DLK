{"textgrid.poem.60048": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wandle denn, von Thr\u00e4nen und von K\u00fcssen", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wandle denn, von Thr\u00e4nen und von K\u00fcssen", "tokens": ["So", "wand\u00b7le", "denn", ",", "von", "Thr\u00e4\u00b7nen", "und", "von", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begleitet, deine Bahn!", "tokens": ["Be\u00b7glei\u00b7tet", ",", "dei\u00b7ne", "Bahn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein kleiner Engel wird voran", "tokens": ["Ein", "klei\u00b7ner", "En\u00b7gel", "wird", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir gehn, und leuchten dir in deinen Finsternissen.", "tokens": ["Dir", "gehn", ",", "und", "leuch\u00b7ten", "dir", "in", "dei\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Engels Haupt ist sanftes Abendroth;", "tokens": ["Des", "En\u00b7gels", "Haupt", "ist", "sanf\u00b7tes", "A\u00b7ben\u00b7droth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus seinen H\u00e4nden nimmt der Tod", "tokens": ["Aus", "sei\u00b7nen", "H\u00e4n\u00b7den", "nimmt", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Becher, den er dir zum letzten Schlummer beut;", "tokens": ["Den", "Be\u00b7cher", ",", "den", "er", "dir", "zum", "letz\u00b7ten", "Schlum\u00b7mer", "beut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und tief im Becher ist des Himmels S\u00fc\u00dfigkeit.", "tokens": ["Und", "tief", "im", "Be\u00b7cher", "ist", "des", "Him\u00b7mels", "S\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schon warten dein mit rosenfarbnen Fl\u00fcgeln,", "tokens": ["Schon", "war\u00b7ten", "dein", "mit", "ro\u00b7sen\u00b7farb\u00b7nen", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf ewig gr\u00fcnen H\u00fcgeln,", "tokens": ["Auf", "e\u00b7wig", "gr\u00fc\u00b7nen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Kinder-Seelen dort, im bessern Sonnenglanz,", "tokens": ["Die", "Kin\u00b7der\u00b7See\u00b7len", "dort", ",", "im", "bes\u00b7sern", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und zeigen sich einander deinen Kranz.", "tokens": ["Und", "zei\u00b7gen", "sich", "ein\u00b7an\u00b7der", "dei\u00b7nen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "O wie so br\u00fcderlich, mit seligem Vertrauen,", "tokens": ["O", "wie", "so", "br\u00fc\u00b7der\u00b7lich", ",", "mit", "se\u00b7li\u00b7gem", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du neuer Engel! wirst du nun", "tokens": ["Du", "neu\u00b7er", "En\u00b7gel", "!", "wirst", "du", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "An ihrer Brust, als ihr Gespiele, ruhn;", "tokens": ["An", "ih\u00b7rer", "Brust", ",", "als", "ihr", "Ge\u00b7spie\u00b7le", ",", "ruhn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mit ihnen Palmen-H\u00fctten bauen,", "tokens": ["Mit", "ih\u00b7nen", "Pal\u00b7men\u00b7H\u00fct\u00b7ten", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und zwischen Lilien den Gott der Wonne schauen,", "tokens": ["Und", "zwi\u00b7schen", "Li\u00b7li\u00b7en", "den", "Gott", "der", "Won\u00b7ne", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den du, vom Winde leicht gek\u00fchlt,", "tokens": ["Den", "du", ",", "vom", "Win\u00b7de", "leicht", "ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Hienieden schon gef\u00fchlt,", "tokens": ["Hien\u00b7ie\u00b7den", "schon", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Als wir in deinen Schoo\u00df die ersten Blumen warfen.", "tokens": ["Als", "wir", "in", "dei\u00b7nen", "Schoo\u00df", "die", "ers\u00b7ten", "Blu\u00b7men", "war\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So wandle denn zum Klang der Silberharfen;", "tokens": ["So", "wand\u00b7le", "denn", "zum", "Klang", "der", "Sil\u00b7ber\u00b7har\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und wenn dein Blick herab von hohen Sternen f\u00e4llt,", "tokens": ["Und", "wenn", "dein", "Blick", "her\u00b7ab", "von", "ho\u00b7hen", "Ster\u00b7nen", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "O dann gedenk' an diese Schatten-Welt,", "tokens": ["O", "dann", "ge\u00b7denk'", "an", "die\u00b7se", "Schat\u00b7ten\u00b7Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "An diesen Erden-Tag,", "tokens": ["An", "die\u00b7sen", "Er\u00b7den\u00b7Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "An diesen Labetrunk, in liebevollen Armen,", "tokens": ["An", "die\u00b7sen", "La\u00b7be\u00b7trunk", ",", "in", "lie\u00b7be\u00b7vol\u00b7len", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das einzige, was irdisches Erbarmen", "tokens": ["Das", "ein\u00b7zi\u00b7ge", ",", "was", "ir\u00b7di\u00b7sches", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Dem Sterbenden zu reichen noch vermag.", "tokens": ["Dem", "Ster\u00b7ben\u00b7den", "zu", "rei\u00b7chen", "noch", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Gedenk' an uns, in deinem Siege!", "tokens": ["Ge\u00b7denk'", "an", "uns", ",", "in", "dei\u00b7nem", "Sie\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Mir aber segnen oft die kleinen, holden Z\u00fcge,", "tokens": ["Mir", "a\u00b7ber", "seg\u00b7nen", "oft", "die", "klei\u00b7nen", ",", "hol\u00b7den", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "In denen uns das Paradies", "tokens": ["In", "de\u00b7nen", "uns", "das", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ein Bild von seiner Unschuld wies.", "tokens": ["Ein", "Bild", "von", "sei\u00b7ner", "Un\u00b7schuld", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}