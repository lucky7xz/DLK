{"textgrid.poem.66252": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin, wohin, du Himmelssohn, o Friede?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, wohin, du Himmelssohn, o Friede?", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "du", "Him\u00b7mels\u00b7sohn", ",", "o", "Frie\u00b7de", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "PPER", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was soll der sonngewandte Blick?", "tokens": ["Was", "soll", "der", "sonn\u00b7ge\u00b7wand\u00b7te", "Blick", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du, des Menschenumgangs m\u00fcde,", "tokens": ["Willst", "du", ",", "des", "Men\u00b7sche\u00b7num\u00b7gangs", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deine Heimath schon zur\u00fcck?", "tokens": ["In", "dei\u00b7ne", "Hei\u00b7math", "schon", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbich will zur\u00fcck. Zwar rinnt die Abschiedsz\u00e4hre;", "tokens": ["\u00bb", "ich", "will", "zu\u00b7r\u00fcck", ".", "Zwar", "rinnt", "die", "Ab\u00b7schieds\u00b7z\u00e4h\u00b7re", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach! ich mu\u00df; denn schau umher!", "tokens": ["Doch", "ach", "!", "ich", "mu\u00df", ";", "denn", "schau", "um\u00b7her", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VMFIN", "$.", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir errichteten Alt\u00e4re", "tokens": ["Die", "mir", "er\u00b7rich\u00b7te\u00b7ten", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind leer, von Opferflammen leer.", "tokens": ["Sind", "leer", ",", "von", "Op\u00b7fer\u00b7flam\u00b7men", "leer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erhoben haben sich zum Menschenw\u00fcrgen", "tokens": ["Er\u00b7ho\u00b7ben", "ha\u00b7ben", "sich", "zum", "Men\u00b7schen\u00b7w\u00fcr\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erdenwaller ohne Zahl.", "tokens": ["Die", "Er\u00b7den\u00b7wal\u00b7ler", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Krieg! donnert's schrecklich von Gebirgen,", "tokens": ["Krieg", "!", "don\u00b7nert's", "schreck\u00b7lich", "von", "Ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "ADJD", "APPR", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Krieg! hallt's entsetzlich nach im Thal.", "tokens": ["Krieg", "!", "hallt's", "ent\u00b7setz\u00b7lich", "nach", "im", "Thal", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Der Mordgeist kommt in dumpfen Schwefeld\u00fcften;", "tokens": ["Der", "Mord\u00b7geist", "kommt", "in", "dum\u00b7pfen", "Schwe\u00b7fel\u00b7d\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Auge rollt in rother Gluth,", "tokens": ["Sein", "Au\u00b7ge", "rollt", "in", "ro\u00b7ther", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Wetter br\u00fcllt um seine H\u00fcften,", "tokens": ["Ein", "Wet\u00b7ter", "br\u00fcllt", "um", "sei\u00b7ne", "H\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Sohle steht in Blut.", "tokens": ["Und", "sei\u00b7ne", "Soh\u00b7le", "steht", "in", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Siehst du das Schattenungeheuer?", "tokens": ["Siehst", "du", "das", "Schat\u00b7ten\u00b7un\u00b7ge\u00b7heu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es kommt, es kommt der Welt zum Fluch;", "tokens": ["Es", "kommt", ",", "es", "kommt", "der", "Welt", "zum", "Fluch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lockt den Adler, Weih' und Geier", "tokens": ["Und", "lockt", "den", "Ad\u00b7ler", ",", "Weih'", "und", "Gei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit der Gemordeten Geruch.", "tokens": ["Mit", "der", "Ge\u00b7mor\u00b7de\u00b7ten", "Ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Ozean erschrickt ob Menschenleichen,", "tokens": ["Der", "O\u00b7ze\u00b7an", "er\u00b7schrickt", "ob", "Men\u00b7schen\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die seine Woge w\u00e4lzen soll.", "tokens": ["Die", "sei\u00b7ne", "Wo\u00b7ge", "w\u00e4l\u00b7zen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die hei\u00dfe Sonne br\u00fctet Seuchen,", "tokens": ["Die", "hei\u00b7\u00dfe", "Son\u00b7ne", "br\u00fc\u00b7tet", "Seu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Jammer ist die Erde voll.", "tokens": ["Von", "Jam\u00b7mer", "ist", "die", "Er\u00b7de", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Von Stambuls Pforte bis nach Peters Th\u00fcrmen", "tokens": ["Von", "Stam\u00b7buls", "Pfor\u00b7te", "bis", "nach", "Pe\u00b7ters", "Th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "APPR", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herrscht Zwist, geboren aus der Nacht.", "tokens": ["Herrscht", "Zwist", ",", "ge\u00b7bo\u00b7ren", "aus", "der", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind, den Olympos zu best\u00fcrmen,", "tokens": ["Sind", ",", "den", "O\u00b7lym\u00b7pos", "zu", "be\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Giganten wieder aufgewacht?", "tokens": ["Gi\u00b7gan\u00b7ten", "wie\u00b7der", "auf\u00b7ge\u00b7wacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich kann nicht sehn ergrimmter Krieger Haufen,", "tokens": ["Ich", "kann", "nicht", "sehn", "er\u00b7grimm\u00b7ter", "Krie\u00b7ger", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann nicht die blasse Mutter sehn,", "tokens": ["Kann", "nicht", "die", "blas\u00b7se", "Mut\u00b7ter", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Wittwen sich die Haare raufen,", "tokens": ["Nicht", "Witt\u00b7wen", "sich", "die", "Haa\u00b7re", "rau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Waisen ohne H\u00fclfe flehn;", "tokens": ["Und", "Wai\u00b7sen", "oh\u00b7ne", "H\u00fcl\u00b7fe", "flehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nicht sehn den Br\u00e4utigam mit hohlen Augen,", "tokens": ["Nicht", "sehn", "den", "Br\u00e4u\u00b7ti\u00b7gam", "mit", "hoh\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und neben ihm die junge Braut,", "tokens": ["Und", "ne\u00b7ben", "ihm", "die", "jun\u00b7ge", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Blut mit blassen Lippen saugen,", "tokens": ["Das", "Blut", "mit", "blas\u00b7sen", "Lip\u00b7pen", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das aus der Todeswunde thaut.", "tokens": ["Das", "aus", "der", "To\u00b7des\u00b7wun\u00b7de", "thaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Kann nicht die Tempel Gottes rauchen sehen,", "tokens": ["Kann", "nicht", "die", "Tem\u00b7pel", "Got\u00b7tes", "rau\u00b7chen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ach den armen Landmann nicht", "tokens": ["Und", "ach", "den", "ar\u00b7men", "Land\u00b7mann", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor der zerst\u00f6rten H\u00fctte stehen", "tokens": ["Vor", "der", "zer\u00b7st\u00f6r\u00b7ten", "H\u00fct\u00b7te", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit gramzerrissenem Gesicht.", "tokens": ["Mit", "gram\u00b7zer\u00b7ris\u00b7se\u00b7nem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Drum flieg' ich auf im Schimmer ew'ger Jugend", "tokens": ["Drum", "flieg'", "ich", "auf", "im", "Schim\u00b7mer", "ew'\u00b7ger", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Gott, der segnend auf mich blickt,", "tokens": ["Zu", "Gott", ",", "der", "seg\u00b7nend", "auf", "mich", "blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis Er, ger\u00fchrt durch eure Tugend,", "tokens": ["Bis", "Er", ",", "ge\u00b7r\u00fchrt", "durch", "eu\u00b7re", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich wieder auf die Erde schickt.\u00ab", "tokens": ["Mich", "wie\u00b7der", "auf", "die", "Er\u00b7de", "schickt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}