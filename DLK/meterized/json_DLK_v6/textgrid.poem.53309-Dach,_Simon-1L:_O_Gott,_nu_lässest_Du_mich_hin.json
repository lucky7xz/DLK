{"textgrid.poem.53309": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Gott, nu l\u00e4ssest Du mich hin", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott, nu l\u00e4ssest Du mich hin", "tokens": ["O", "Gott", ",", "nu", "l\u00e4s\u00b7sest", "Du", "mich", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus diesem Leben fahren,", "tokens": ["Aus", "die\u00b7sem", "Le\u00b7ben", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil ich durchaus nicht besser bin", "tokens": ["Weil", "ich", "durc\u00b7haus", "nicht", "bes\u00b7ser", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als meine V\u00e4ter waren,", "tokens": ["Als", "mei\u00b7ne", "V\u00e4\u00b7ter", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du reissest mir den Faden ab,", "tokens": ["Du", "reis\u00b7sest", "mir", "den", "Fa\u00b7den", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich wie ein Weber pfleget;", "tokens": ["Gleich", "wie", "ein", "We\u00b7ber", "pfle\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich werd' hienunter in das Grab", "tokens": ["Ich", "werd'", "hien\u00b7un\u00b7ter", "in", "das", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn Wiederkunft geleget.", "tokens": ["Ohn", "Wie\u00b7der\u00b7kunft", "ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So bald des Leibes schwacher Sinn", "tokens": ["So", "bald", "des", "Lei\u00b7bes", "schwa\u00b7cher", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur von mir ist gewichen,", "tokens": ["Nur", "von", "mir", "ist", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So bin ich stracks gleich denen hin,", "tokens": ["So", "bin", "ich", "stracks", "gleich", "de\u00b7nen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die l\u00e4ngst zuvor verblichen,", "tokens": ["Die", "l\u00e4ngst", "zu\u00b7vor", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd nun ein tausent Jahr vnd mehr", "tokens": ["Vnd", "nun", "ein", "tau\u00b7sent", "Jahr", "vnd", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "CARD", "NN", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wol au\u00dfgeschlaffen haben,", "tokens": ["Wol", "au\u00df\u00b7ge\u00b7schlaf\u00b7fen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Weg ist mein Thun, weg Stand vnd Ehr,", "tokens": ["Weg", "ist", "mein", "Thun", ",", "weg", "Stand", "vnd", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Weg alle meine Gaben.", "tokens": ["Weg", "al\u00b7le", "mei\u00b7ne", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald werd' ich von Verwesenheit", "tokens": ["Bald", "werd'", "ich", "von", "Ver\u00b7we\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Haut vnd Haar gefressen,", "tokens": ["Mit", "Haut", "vnd", "Haar", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt hat mein in kurtzer Zeit", "tokens": ["Die", "Welt", "hat", "mein", "in", "kurt\u00b7zer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gantz, wie ich jhr, vergessen.", "tokens": ["Gantz", ",", "wie", "ich", "jhr", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich werde nichts, vnd w\u00e4hr' ich auch", "tokens": ["Ich", "wer\u00b7de", "nichts", ",", "vnd", "w\u00e4hr'", "ich", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "$,", "KON", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hie noch so ausserlesen,", "tokens": ["Hie", "noch", "so", "aus\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gleich einem Schatten, Traum vnd Rauch,", "tokens": ["Gleich", "ei\u00b7nem", "Schat\u00b7ten", ",", "Traum", "vnd", "Rauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd dem, der nie gewesen.", "tokens": ["Vnd", "dem", ",", "der", "nie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADV", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sol ich de\u00dfwegen, HERR bey, Dir", "tokens": ["Sol", "ich", "de\u00df\u00b7we\u00b7gen", ",", "HeRR", "bey", ",", "Dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "PAV", "$,", "NN", "PTKVZ", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun eben so vergehen?", "tokens": ["Nun", "e\u00b7ben", "so", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird kein Ged\u00e4chtn\u00fcs mehr von mir", "tokens": ["Wird", "kein", "Ge\u00b7d\u00e4cht\u00b7n\u00fcs", "mehr", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem Hertzen stehen?", "tokens": ["In", "dei\u00b7nem", "Hert\u00b7zen", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sol denn mein Fleisch, der W\u00fcrmer spott,", "tokens": ["Sol", "denn", "mein", "Fleisch", ",", "der", "W\u00fcr\u00b7mer", "spott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn Lebens-Trost verst\u00e4uben?", "tokens": ["Ohn", "Le\u00b7bens\u00b7Trost", "ver\u00b7st\u00e4u\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bist Du im Todt' auch nicht mein Gott,", "tokens": ["Bist", "Du", "im", "Todt'", "auch", "nicht", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo sol mein' Hoffnung bleiben?", "tokens": ["Wo", "sol", "mein'", "Hoff\u00b7nung", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das sey von Dir, O H\u00f6chster, fern,", "tokens": ["Das", "sey", "von", "Dir", ",", "O", "H\u00f6chs\u00b7ter", ",", "fern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "$,", "NE", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin nicht so verdorben,", "tokens": ["Ich", "bin", "nicht", "so", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich lebe Dir nur, meinem HERRN,", "tokens": ["Ich", "le\u00b7be", "Dir", "nur", ",", "mei\u00b7nem", "HeRRN", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir werd' ich seyn gestorben.", "tokens": ["Dir", "werd'", "ich", "seyn", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd weil wir, Vater, Dich allein", "tokens": ["Vnd", "weil", "wir", ",", "Va\u00b7ter", ",", "Dich", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "$,", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lebens GOTT erheben,", "tokens": ["Des", "Le\u00b7bens", "GoTT", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Werd' ich im Todte todt nicht seyn,", "tokens": ["Werd'", "ich", "im", "Tod\u00b7te", "todt", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADJD", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich sterb' vnd werde leben.", "tokens": ["Ich", "sterb'", "vnd", "wer\u00b7de", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn Christus, (wie ich bin gelehrt)", "tokens": ["Denn", "Chris\u00b7tus", ",", "(", "wie", "ich", "bin", "ge\u00b7lehrt", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "$(", "PWAV", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stirbt vnd ersteht im gleichen,", "tokens": ["Stirbt", "vnd", "er\u00b7steht", "im", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auff da\u00df er werd' ein Herr geehrt", "tokens": ["Auff", "da\u00df", "er", "werd'", "ein", "Herr", "ge\u00b7ehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Lebenden vnd Leichen,", "tokens": ["Der", "Le\u00b7ben\u00b7den", "vnd", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er l\u00e4sst hierumb aus trewer Pflicht", "tokens": ["Er", "l\u00e4sst", "hie\u00b7rumb", "aus", "tre\u00b7wer", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwahren in die Erde", "tokens": ["Ver\u00b7wah\u00b7ren", "in", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "All mein Gebein, auff da\u00df ja nicht", "tokens": ["All", "mein", "Ge\u00b7bein", ",", "auff", "da\u00df", "ja", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "APPR", "KOUS", "ADV", "PTKNEG"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Nur eins verlohren werde.", "tokens": ["Nur", "eins", "ver\u00b7loh\u00b7ren", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich wil von M\u00fcntze, Timian", "tokens": ["Ich", "wil", "von", "M\u00fcnt\u00b7ze", ",", "Ti\u00b7mi\u00b7an"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd Nelcken Saamen holen,", "tokens": ["Vnd", "Nel\u00b7cken", "Saa\u00b7men", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vermeng es, thue zu Majoran,", "tokens": ["Ver\u00b7meng", "es", ",", "thue", "zu", "Ma\u00b7jo\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Saate von Violen", "tokens": ["Die", "Saa\u00b7te", "von", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd allen Blumen in gemein,", "tokens": ["Vnd", "al\u00b7len", "Blu\u00b7men", "in", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein G\u00e4rtner wird es kennen,", "tokens": ["Ein", "G\u00e4rt\u00b7ner", "wird", "es", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd auch ein jedes K\u00f6rnelein", "tokens": ["Vnd", "auch", "ein", "je\u00b7des", "K\u00f6r\u00b7nel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach seinem Nahmen nennen.", "tokens": ["Nach", "sei\u00b7nem", "Nah\u00b7men", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "Vielmehr kennst Du mich, werd' ich gleich", "tokens": ["Viel\u00b7mehr", "kennst", "Du", "mich", ",", "werd'", "ich", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Staub vnd Kl\u00f6h\u00dflein Erden,", "tokens": ["Zu", "Staub", "vnd", "Kl\u00f6h\u00df\u00b7lein", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch sol dies Fleisch zu deinem Reich", "tokens": ["Auch", "sol", "dies", "Fleisch", "zu", "dei\u00b7nem", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch aufferwecket werden.", "tokens": ["Noch", "auf\u00b7fer\u00b7we\u00b7cket", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dies mein Ge\u00e4der, Haut vnd Blut", "tokens": ["Dies", "mein", "Ge\u00b7\u00e4\u00b7der", ",", "Haut", "vnd", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird dennoch mir gewehret,", "tokens": ["Wird", "den\u00b7noch", "mir", "ge\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "H\u00e4tt' Erde, Wasser oder Glut", "tokens": ["H\u00e4tt'", "Er\u00b7de", ",", "Was\u00b7ser", "o\u00b7der", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich tausentmahl verzehret.", "tokens": ["Mich", "tau\u00b7sent\u00b7mahl", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit dieser Hoffnung leg' ich mich", "tokens": ["Mit", "die\u00b7ser", "Hoff\u00b7nung", "leg'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PRF"], "m