{"textgrid.poem.44123": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du fromm- und redliches Gebl\u00fcte,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fromm- und redliches Gebl\u00fcte,", "tokens": ["Du", "from\u00b7m", "und", "red\u00b7li\u00b7ches", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es halten dich nur Kluge werth,", "tokens": ["Es", "hal\u00b7ten", "dich", "nur", "Klu\u00b7ge", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt erkenne dein Gem\u00fcthe,", "tokens": ["Die", "Welt", "er\u00b7ken\u00b7ne", "dein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das aller Menschen Heil begehrt,", "tokens": ["Das", "al\u00b7ler", "Men\u00b7schen", "Heil", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Alter geh mit jungen Kr\u00e4ften", "tokens": ["Dein", "Al\u00b7ter", "geh", "mit", "jun\u00b7gen", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch h\u00f6her als der V\u00e4ter Ziel.", "tokens": ["Noch", "h\u00f6\u00b7her", "als", "der", "V\u00e4\u00b7ter", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der blinden G\u00f6ttin Gauckelspiel", "tokens": ["Der", "blin\u00b7den", "G\u00f6t\u00b7tin", "Gau\u00b7ckel\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verschone dich mit Amtsgesch\u00e4ften,", "tokens": ["Ver\u00b7scho\u00b7ne", "dich", "mit", "Amts\u00b7ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Ehrfurcht schaue dir und deiner stillen Ruh", "tokens": ["Die", "Ehr\u00b7furcht", "schau\u00b7e", "dir", "und", "dei\u00b7ner", "stil\u00b7len", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von ihren ungewi\u00dfen Spizen,", "tokens": ["Von", "ih\u00b7ren", "un\u00b7ge\u00b7wi\u00b7\u00dfen", "Spi\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Worauf Gefahr und Unruh sizen,", "tokens": ["Wo\u00b7rauf", "Ge\u00b7fahr", "und", "Un\u00b7ruh", "si\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit niedertr\u00e4chtgem Stolz und scheelen Augen zu.", "tokens": ["Mit", "nie\u00b7der\u00b7tr\u00e4cht\u00b7gem", "Stolz", "und", "schee\u00b7len", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Feind erwerbe Gold und Tittel", "tokens": ["Dein", "Feind", "er\u00b7wer\u00b7be", "Gold", "und", "Tit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lache dich in Thorheit aus,", "tokens": ["Und", "la\u00b7che", "dich", "in", "Thor\u00b7heit", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nothdurft nehme sichre Mittel", "tokens": ["Die", "Noth\u00b7durft", "neh\u00b7me", "sich\u00b7re", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fcnde dir ein eignes Haus.", "tokens": ["Und", "gr\u00fcn\u00b7de", "dir", "ein", "eig\u00b7nes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Friede n\u00e4hre deinetwegen", "tokens": ["Der", "Frie\u00b7de", "n\u00e4h\u00b7re", "dei\u00b7net\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das angenehme Sachsenland,", "tokens": ["Das", "an\u00b7ge\u00b7neh\u00b7me", "Sach\u00b7sen\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es h\u00f6r einmahl der Muldenstrand", "tokens": ["Es", "h\u00f6r", "ein\u00b7mahl", "der", "Mul\u00b7den\u00b7strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Klang von deinen Lautenschl\u00e4gen.", "tokens": ["Den", "Klang", "von", "dei\u00b7nen", "Lau\u00b7ten\u00b7schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dein Herze brennt vor Lust, das gro\u00dfe Rom zu sehn.", "tokens": ["Dein", "Her\u00b7ze", "brennt", "vor", "Lust", ",", "das", "gro\u00b7\u00dfe", "Rom", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "ADJA", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Versprich dir w\u00fcrcklich dies Vergn\u00fcgen,", "tokens": ["Ver\u00b7sprich", "dir", "w\u00fcrck\u00b7lich", "dies", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Allmacht weis es schon zu f\u00fcgen", "tokens": ["Die", "All\u00b7macht", "weis", "es", "schon", "zu", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und l\u00e4st den Ihrigen oft mehr als das geschehn.", "tokens": ["Und", "l\u00e4st", "den", "Ih\u00b7ri\u00b7gen", "oft", "mehr", "als", "das", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PPOSS", "ADV", "PIAT", "KOKOM", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und kehrstu endlich nach dem Reisen", "tokens": ["Und", "kehrs\u00b7tu", "end\u00b7lich", "nach", "dem", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Bienen voller Honigseim", "tokens": ["Wie", "Bie\u00b7nen", "vol\u00b7ler", "Ho\u00b7ni\u00b7gseim"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit eingenommnen Anmuthsspeisen", "tokens": ["Mit", "ein\u00b7ge\u00b7nomm\u00b7nen", "An\u00b7muths\u00b7spei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Deutschlands s\u00fc\u00dfe Gr\u00e4nzen heim,", "tokens": ["In", "Deutschlands", "s\u00fc\u00b7\u00dfe", "Gr\u00e4n\u00b7zen", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So w\u00fcrdige die edlen Linden", "tokens": ["So", "w\u00fcr\u00b7di\u00b7ge", "die", "ed\u00b7len", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zeuch in ihrem Schatten ein,", "tokens": ["Und", "zeuch", "in", "ih\u00b7rem", "Schat\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du wirst allhier bey Brodt und Wein", "tokens": ["Du", "wirst", "all\u00b7hier", "bey", "Brodt", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Paradies voll Engel finden.", "tokens": ["Dein", "Pa\u00b7ra\u00b7dies", "voll", "En\u00b7gel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wer w\u00fcntscht wohl Leipzig nicht? Wer nimmt wohl nicht [in Acht,", "tokens": ["Wer", "w\u00fcnt\u00b7scht", "wohl", "Leip\u00b7zig", "nicht", "?", "Wer", "nimmt", "wohl", "nicht", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "NE", "PTKNEG", "$.", "PWS", "VVFIN", "ADV", "PTKNEG", "$(", "APPR", "CARD", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Da\u00df hier nur die zur Ruhe kommen,", "tokens": ["Da\u00df", "hier", "nur", "die", "zur", "Ru\u00b7he", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Gott, der Vater rechter Frommen,", "tokens": ["Die", "Gott", ",", "der", "Va\u00b7ter", "rech\u00b7ter", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Aus sonderbarer Gunst schon zeitlich seelig macht.", "tokens": ["Aus", "son\u00b7der\u00b7ba\u00b7rer", "Gunst", "schon", "zeit\u00b7lich", "see\u00b7lig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Eh ist Himmel oder H\u00f6lle,", "tokens": ["Die", "Eh", "ist", "Him\u00b7mel", "o\u00b7der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem uns nehmlich diese f\u00fchrt,", "tokens": ["Nach\u00b7dem", "uns", "nehm\u00b7lich", "die\u00b7se", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit der man vor der heilgen Stelle", "tokens": ["Mit", "der", "man", "vor", "der", "heil\u00b7gen", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00e4nde wie das Herz verschn\u00fcrt.", "tokens": ["Die", "H\u00e4n\u00b7de", "wie", "das", "Herz", "ver\u00b7schn\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ger\u00e4th der Bund, so hat ein Weiser", "tokens": ["Ge\u00b7r\u00e4\u00b7th", "der", "Bund", ",", "so", "hat", "ein", "Wei\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Wollust, so nur m\u00f6glich ist,", "tokens": ["Die", "Wol\u00b7lust", ",", "so", "nur", "m\u00f6g\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wer so Lieb als Tugend k\u00fcst,", "tokens": ["Denn", "wer", "so", "Lieb", "als", "Tu\u00b7gend", "k\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der tauscht nicht mit dem gr\u00f6sten Kayser.", "tokens": ["Der", "tauscht", "nicht", "mit", "dem", "gr\u00f6s\u00b7ten", "Kay\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Drum wehl und werbe dir der Himmel so ein Weib,", "tokens": ["Drum", "wehl", "und", "wer\u00b7be", "dir", "der", "Him\u00b7mel", "so", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "KON", "VVFIN", "PPER", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Wiz und Redligkeit verbinde", "tokens": ["Das", "Wiz", "und", "Red\u00b7lig\u00b7keit", "ver\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und keinen Gram im Spiegel finde;", "tokens": ["Und", "kei\u00b7nen", "Gram", "im", "Spie\u00b7gel", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denn jenes st\u00e4rckt das Herz, und dies erg\u00f6zt den Leib.", "tokens": ["Denn", "je\u00b7nes", "st\u00e4rckt", "das", "Herz", ",", "und", "dies", "er\u00b7g\u00f6zt", "den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$,", "KON", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Noch etwas bitt ich von dem Wesen", "tokens": ["Noch", "et\u00b7was", "bitt", "ich", "von", "dem", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ewigen Unendligkeit,", "tokens": ["Der", "e\u00b7wi\u00b7gen", "Un\u00b7end\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das schon vor aller Zeiten Zeit", "tokens": ["Das", "schon", "vor", "al\u00b7ler", "Zei\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um unser Wohl besorgt gewesen:", "tokens": ["Um", "un\u00b7ser", "Wohl", "be\u00b7sorgt", "ge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es gebe de\u00dfen m\u00e4chtger Wille,", "tokens": ["Es", "ge\u00b7be", "de\u00b7\u00dfen", "m\u00e4cht\u00b7ger", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Wofern es sein Verstand erlaubt,", "tokens": ["Wo\u00b7fern", "es", "sein", "Ver\u00b7stand", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df deines wackern Vaters Haupt", "tokens": ["Da\u00df", "dei\u00b7nes", "wa\u00b7ckern", "Va\u00b7ters", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Sarg mit hundert Jahren f\u00fclle.", "tokens": ["Den", "Sarg", "mit", "hun\u00b7dert", "Jah\u00b7ren", "f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So wird der theure Mann, der solche Lust verdient,", "tokens": ["So", "wird", "der", "theu\u00b7re", "Mann", ",", "der", "sol\u00b7che", "Lust", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Simeons beschneiten Haaren", "tokens": ["In", "Si\u00b7me\u00b7ons", "be\u00b7schnei\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Freud und Frieden weiterfahren,", "tokens": ["Mit", "Freud", "und", "Frie\u00b7den", "wei\u00b7ter\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,