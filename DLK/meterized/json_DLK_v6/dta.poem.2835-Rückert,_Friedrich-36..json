{"dta.poem.2835": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich wundert, da\u00df gesinnt ein jeder anders ist?", "tokens": ["Dich", "wun\u00b7dert", ",", "da\u00df", "ge\u00b7sinnt", "ein", "je\u00b7der", "an\u00b7ders", "ist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "ART", "PIS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da du, der eine, selbst gesinnt stets anders bist.", "tokens": ["Da", "du", ",", "der", "ei\u00b7ne", ",", "selbst", "ge\u00b7sinnt", "stets", "an\u00b7ders", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ART", "$,", "ADV", "ADJD", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht so viel Sinne nur, als K\u00f6pfe, sind gefunden,", "tokens": ["Nicht", "so", "viel", "Sin\u00b7ne", "nur", ",", "als", "K\u00f6p\u00b7fe", ",", "sind", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PIAT", "NN", "ADV", "$,", "KOUS", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da jedes Kopfes Sinn sich \u00e4ndert alle Stunden.", "tokens": ["Da", "je\u00b7des", "Kop\u00b7fes", "Sinn", "sich", "\u00e4n\u00b7dert", "al\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["