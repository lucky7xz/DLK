{"textgrid.poem.47212": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Rosen auf das Grab einer edlen Frau", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh\u00f6ret hab' ich, und ich kann es schauen,", "tokens": ["Ge\u00b7h\u00f6\u00b7ret", "hab'", "ich", ",", "und", "ich", "kann", "es", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "KON", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An dieser Thr\u00e4nenst\u00fcrme lautem Tosen,", "tokens": ["An", "die\u00b7ser", "Thr\u00e4\u00b7nen\u00b7st\u00fcr\u00b7me", "lau\u00b7tem", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wohl die Vater- und die Mutterlosen,", "tokens": ["Da\u00df", "wohl", "die", "Va\u00b7ter", "und", "die", "Mut\u00b7ter\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "TRUNC", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die Gebrechlichen und Altergrauen", "tokens": ["Und", "die", "Ge\u00b7brech\u00b7li\u00b7chen", "und", "Al\u00b7ter\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sonst hatten eine Mutter an der Frauen,", "tokens": ["Sonst", "hat\u00b7ten", "ei\u00b7ne", "Mut\u00b7ter", "an", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sie zu Grab jetzt folgen \u2013 f\u00fcr Almosen,", "tokens": ["Der", "sie", "zu", "Grab", "jetzt", "fol\u00b7gen", "\u2013", "f\u00fcr", "Al\u00b7mo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVINF", "$(", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sie von ihr empfingen jetzund Rosen", "tokens": ["Die", "sie", "von", "ihr", "emp\u00b7fin\u00b7gen", "je\u00b7tzund", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aufs Grab ihr streun und Thr\u00e4nen dr\u00fcber tauen.", "tokens": ["Aufs", "Grab", "ihr", "streun", "und", "Thr\u00e4\u00b7nen", "dr\u00fc\u00b7ber", "tau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "KON", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr sollt euch tr\u00f6sten! Auf dem Sterbebette", "tokens": ["Ihr", "sollt", "euch", "tr\u00f6s\u00b7ten", "!", "Auf", "dem", "Ster\u00b7be\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat sie noch ihrer Armen nicht vergessen. \u2013", "tokens": ["Hat", "sie", "noch", "ih\u00b7rer", "Ar\u00b7men", "nicht", "ver\u00b7ges\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir alle, die wir ihren Heimgang ehren,", "tokens": ["Wir", "al\u00b7le", ",", "die", "wir", "ih\u00b7ren", "Heim\u00b7gang", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sind Arme, die empfahn an dieser St\u00e4tte", "tokens": ["Sind", "Ar\u00b7me", ",", "die", "em\u00b7pfahn", "an", "die\u00b7ser", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Almosen, deren Wert nicht zu ermessen,", "tokens": ["Al\u00b7mo\u00b7sen", ",", "de\u00b7ren", "Wert", "nicht", "zu", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Davon die Herzen lang' noch k\u00f6nnen zehren.", "tokens": ["Da\u00b7von", "die", "Her\u00b7zen", "lang'", "noch", "k\u00f6n\u00b7nen", "zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Anblick einer Seele, die in Frieden", "tokens": ["Der", "An\u00b7blick", "ei\u00b7ner", "See\u00b7le", ",", "die", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Gott, der Welt und sich des Amtes pflegte", "tokens": ["Mit", "Gott", ",", "der", "Welt", "und", "sich", "des", "Am\u00b7tes", "pfleg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ART", "NN", "KON", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit treuer Hand, das Gott in ihre legte,", "tokens": ["Mit", "treu\u00b7er", "Hand", ",", "das", "Gott", "in", "ih\u00b7re", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und als der Herr sie abrief von hienieden,", "tokens": ["Und", "als", "der", "Herr", "sie", "ab\u00b7rief", "von", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ihm willig folgte, ruhig, ernst, entschieden;", "tokens": ["Ihm", "wil\u00b7lig", "folg\u00b7te", ",", "ru\u00b7hig", ",", "ernst", ",", "ent\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Selbst noch, als sich um sie der Jammer regte", "tokens": ["Selbst", "noch", ",", "als", "sich", "um", "sie", "der", "Jam\u00b7mer", "reg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PRF", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Ihren, mit dem Troste, den sie hegte", "tokens": ["Der", "Ih\u00b7ren", ",", "mit", "dem", "Tros\u00b7te", ",", "den", "sie", "heg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihrer Brust, sie alle sprach zufrieden;", "tokens": ["In", "ih\u00b7rer", "Brust", ",", "sie", "al\u00b7le", "sprach", "zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Anblick, der uns herrlicher und reiner", "tokens": ["Der", "An\u00b7blick", ",", "der", "uns", "herr\u00b7li\u00b7cher", "und", "rei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJA", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erkennen l\u00e4\u00dft, da\u00df \u00fcber seiner H\u00fclle", "tokens": ["Er\u00b7ken\u00b7nen", "l\u00e4\u00dft", ",", "da\u00df", "\u00fc\u00b7ber", "sei\u00b7ner", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der freie Geist besteht, der wandellose;", "tokens": ["Der", "frei\u00b7e", "Geist", "be\u00b7steht", ",", "der", "wan\u00b7del\u00b7lo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das ist die gro\u00dfe, hier wie sonst an keiner", "tokens": ["Das", "ist", "die", "gro\u00b7\u00dfe", ",", "hier", "wie", "sonst", "an", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "ADV", "KOKOM", "ADV", "APPR", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Grabst\u00e4tte je in also reicher F\u00fclle,", "tokens": ["Grab\u00b7st\u00e4t\u00b7te", "je", "in", "al\u00b7so", "rei\u00b7cher", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An dieser uns gespendete Almose.", "tokens": ["An", "die\u00b7ser", "uns", "ge\u00b7spen\u00b7de\u00b7te", "Al\u00b7mo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}