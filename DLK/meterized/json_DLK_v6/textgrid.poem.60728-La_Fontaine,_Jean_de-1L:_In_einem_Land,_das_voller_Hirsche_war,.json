{"textgrid.poem.60728": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einem Land, das voller Hirsche war,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Land, das voller Hirsche war,", "tokens": ["In", "ei\u00b7nem", "Land", ",", "das", "vol\u00b7ler", "Hir\u00b7sche", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ward eines dieser Tiere krank.", "tokens": ["Ward", "ei\u00b7nes", "die\u00b7ser", "Tie\u00b7re", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man eilt zur Stelle, wo der Edle niedersank,", "tokens": ["Man", "eilt", "zur", "Stel\u00b7le", ",", "wo", "der", "Ed\u00b7le", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihn sehn, ihn tr\u00f6sten. Welche l\u00e4stige Schar!", "tokens": ["Ihn", "sehn", ",", "ihn", "tr\u00f6s\u00b7ten", ".", "Wel\u00b7che", "l\u00e4s\u00b7ti\u00b7ge", "Schar", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF", "$.", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "\u00bbihr Herren, la\u00dft mich sterben!", "tokens": ["\u00bb", "ihr", "Her\u00b7ren", ",", "la\u00dft", "mich", "ster\u00b7ben", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erlaubt, da\u00df auf gewohnte Art", "tokens": ["Er\u00b7laubt", ",", "da\u00df", "auf", "ge\u00b7wohn\u00b7te", "Art"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Parze ende meine Lebensfahrt,", "tokens": ["Die", "Par\u00b7ze", "en\u00b7de", "mei\u00b7ne", "Le\u00b7bens\u00b7fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So werdet ihr euch meinen Dank erwerben;", "tokens": ["So", "wer\u00b7det", "ihr", "euch", "mei\u00b7nen", "Dank", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich bitte, la\u00dft das Weinen sein!\u00ab", "tokens": ["Ich", "bit\u00b7te", ",", "la\u00dft", "das", "Wei\u00b7nen", "sein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "ART", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im Gegenteil, untr\u00f6stlich war ihr Schrein,", "tokens": ["Im", "Ge\u00b7gen\u00b7teil", ",", "un\u00b7tr\u00f6st\u00b7lich", "war", "ihr", "Schrein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Denn atemlose Klage schien hier Pflicht.", "tokens": ["Denn", "a\u00b7tem\u00b7lo\u00b7se", "Kla\u00b7ge", "schien", "hier", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und als sie gingen, so geschah es nicht", "tokens": ["Und", "als", "sie", "gin\u00b7gen", ",", "so", "ge\u00b7schah", "es", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ganz ohne einen Schluck zu trinken,", "tokens": ["Ganz", "oh\u00b7ne", "ei\u00b7nen", "Schluck", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Nicht ohne von dem Recht Gebrauch zu machen,", "tokens": ["Nicht", "oh\u00b7ne", "von", "dem", "Recht", "Ge\u00b7brauch", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Von gr\u00fcnen \u00c4sten, die da winken,", "tokens": ["Von", "gr\u00fc\u00b7nen", "\u00c4s\u00b7ten", ",", "die", "da", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die zarten Zweiglein zu zerkrachen,", "tokens": ["Die", "zar\u00b7ten", "Zwei\u00b7glein", "zu", "zer\u00b7kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die gr\u00fcnen Bl\u00e4ttlein zu verzehren", "tokens": ["Die", "gr\u00fc\u00b7nen", "Bl\u00e4tt\u00b7lein", "zu", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und so das Buschwerk in der Runde", "tokens": ["Und", "so", "das", "Busc\u00b7hwerk", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Von all dem sch\u00f6nen Laub zu leeren.", "tokens": ["Von", "all", "dem", "sch\u00f6\u00b7nen", "Laub", "zu", "lee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der Hirsch in der Genesungsstunde", "tokens": ["Der", "Hirsch", "in", "der", "Ge\u00b7ne\u00b7sungs\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Fand nichts mehr, was ihm Nahrung bot;", "tokens": ["Fand", "nichts", "mehr", ",", "was", "ihm", "Nah\u00b7rung", "bot", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So kam er in noch gr\u00f6\u00dfre Not", "tokens": ["So", "kam", "er", "in", "noch", "gr\u00f6\u00df\u00b7re", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und starb sogar den Hungertod.", "tokens": ["Und", "starb", "so\u00b7gar", "den", "Hun\u00b7ger\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ihr bed\u00fcrft, es kostet stets etwas,", "tokens": ["Was", "ihr", "be\u00b7d\u00fcrft", ",", "es", "kos\u00b7tet", "stets", "et\u00b7was", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sei's nun der Arzt des Leibes, sei es der der Seele.", "tokens": ["Sei's", "nun", "der", "Arzt", "des", "Lei\u00b7bes", ",", "sei", "es", "der", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ART", "NN", "$,", "VAFIN", "PPER", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbo welche Zeit!\u00ab Freund, schone deine Kehle,", "tokens": ["\u00bb", "o", "wel\u00b7che", "Zeit", "!", "\u00ab", "Freund", ",", "scho\u00b7ne", "dei\u00b7ne", "Keh\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PWAT", "NN", "$.", "$(", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du d\u00fcnkst nur jedem gut zum Aderla\u00df!", "tokens": ["Du", "d\u00fcnkst", "nur", "je\u00b7dem", "gut", "zum", "A\u00b7der\u00b7la\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}