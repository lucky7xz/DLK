{"textgrid.poem.44484": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die du vom Himmel stiegest, Ewig-Sch\u00f6ne]", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du vom Himmel stiegest, Ewig-Sch\u00f6ne,", "tokens": ["Die", "du", "vom", "Him\u00b7mel", "stie\u00b7gest", ",", "E\u00b7wig\u00b7Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O kehr zum Himmel wiederum zur\u00fcck,", "tokens": ["O", "kehr", "zum", "Him\u00b7mel", "wie\u00b7de\u00b7rum", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geboren wardst du f\u00fcr des Lichtes S\u00f6hne,", "tokens": ["Ge\u00b7bo\u00b7ren", "wardst", "du", "f\u00fcr", "des", "Lich\u00b7tes", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Staubes S\u00f6hne meiden deinen Blick.", "tokens": ["Des", "Stau\u00b7bes", "S\u00f6h\u00b7ne", "mei\u00b7den", "dei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar glauben sie wohl manchmal dich zu fassen", "tokens": ["Zwar", "glau\u00b7ben", "sie", "wohl", "manch\u00b7mal", "dich", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wohl auch dich zu lieben meinen sie,", "tokens": ["Und", "wohl", "auch", "dich", "zu", "lie\u00b7ben", "mei\u00b7nen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch l\u00e4\u00dft die Armut vom Bed\u00fcrfnis nie,", "tokens": ["Doch", "l\u00e4\u00dft", "die", "Ar\u00b7mut", "vom", "Be\u00b7d\u00fcrf\u00b7nis", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch wird Gemeines vom Gemeinen lassen.", "tokens": ["Noch", "wird", "Ge\u00b7mei\u00b7nes", "vom", "Ge\u00b7mei\u00b7nen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}