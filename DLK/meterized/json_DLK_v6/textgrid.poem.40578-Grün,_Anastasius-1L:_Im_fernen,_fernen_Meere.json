{"textgrid.poem.40578": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im fernen, fernen Meere", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im fernen, fernen Meere", "tokens": ["Im", "fer\u00b7nen", ",", "fer\u00b7nen", "Mee\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da segelt' ein Schiff bei Nacht,", "tokens": ["Da", "se\u00b7gelt'", "ein", "Schiff", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Schiffsherr in der Kaj\u00fcte", "tokens": ["Der", "Schiffs\u00b7herr", "in", "der", "Ka\u00b7j\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entschlief auf der Matte sacht.", "tokens": ["Ent\u00b7schlief", "auf", "der", "Mat\u00b7te", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Kiel schnitt still und ruhig", "tokens": ["Der", "Kiel", "schnitt", "still", "und", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den weiten stillen Raum;", "tokens": ["Den", "wei\u00b7ten", "stil\u00b7len", "Raum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jedoch so still und ruhig", "tokens": ["Je\u00b7doch", "so", "still", "und", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War nicht des Schiffsherrn Traum:", "tokens": ["War", "nicht", "des", "Schiffs\u00b7herrn", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihm tr\u00e4umt', ein Blitzstrahl habe", "tokens": ["Ihm", "tr\u00e4umt'", ",", "ein", "Blitz\u00b7strahl", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den stolzen Mast zerspellt,", "tokens": ["Den", "stol\u00b7zen", "Mast", "zer\u00b7spellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sei an einem Felsen", "tokens": ["Es", "sei", "an", "ei\u00b7nem", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Sturm das Schiff zerschellt,", "tokens": ["Im", "Sturm", "das", "Schiff", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und \u00fcber Bord geschleudert", "tokens": ["Und", "\u00fc\u00b7ber", "Bord", "ge\u00b7schleu\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwimm' er im tosenden Meer,", "tokens": ["Schwimm'", "er", "im", "to\u00b7sen\u00b7den", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und Wogenkolosse und Blitze", "tokens": ["Und", "Wo\u00b7gen\u00b7ko\u00b7los\u00b7se", "und", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die sausen um ihn her.", "tokens": ["Die", "sau\u00b7sen", "um", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er rudert mit brechenden Armen,", "tokens": ["Er", "ru\u00b7dert", "mit", "bre\u00b7chen\u00b7den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Schon sieht er die K\u00fcste nahn,", "tokens": ["Schon", "sieht", "er", "die", "K\u00fcs\u00b7te", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch brausend an ihre Felsen", "tokens": ["Doch", "brau\u00b7send", "an", "ih\u00b7re", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schl\u00e4gt hoch die Brandung hinan.", "tokens": ["Schl\u00e4gt", "hoch", "die", "Bran\u00b7dung", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auf einem der grauen Felsen", "tokens": ["Auf", "ei\u00b7nem", "der", "grau\u00b7en", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sieht er eine Jungfrau stehn;", "tokens": ["Sieht", "er", "ei\u00b7ne", "Jung\u00b7frau", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie winkt und l\u00e4\u00dft hernieder", "tokens": ["Sie", "winkt", "und", "l\u00e4\u00dft", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihm eine Rose wehn.", "tokens": ["Zu", "ihm", "ei\u00b7ne", "Ro\u00b7se", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch dort schwimmt nun ein Balken", "tokens": ["Doch", "dort", "schwimmt", "nun", "ein", "Bal\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Rettung ihm heran;", "tokens": ["Zur", "Ret\u00b7tung", "ihm", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll er zuerst die Rose,", "tokens": ["Soll", "er", "zu\u00b7erst", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zuerst den Balken umfahn?", "tokens": ["Zu\u00b7erst", "den", "Bal\u00b7ken", "um\u00b7fahn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Schon brechen die Arme, schon sinkt er", "tokens": ["Schon", "bre\u00b7chen", "die", "Ar\u00b7me", ",", "schon", "sinkt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ins fluthende Grab hinein;", "tokens": ["Ins", "flut\u00b7hen\u00b7de", "Grab", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da fa\u00dft ihn die Brandung und schleudert", "tokens": ["Da", "fa\u00dft", "ihn", "die", "Bran\u00b7dung", "und", "schleu\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ihn an das Felsgestein.", "tokens": ["Ihn", "an", "das", "Fels\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Schiffsherr erwacht und st\u00fcrzet", "tokens": ["Der", "Schiffs\u00b7herr", "er\u00b7wacht", "und", "st\u00fcr\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Rasch aufs Verdeck hinan;", "tokens": ["Rasch", "aufs", "Ver\u00b7deck", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ruhig und sicher gleitet", "tokens": ["Doch", "ru\u00b7hig", "und", "si\u00b7cher", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Schiff durch die stille Bahn.", "tokens": ["Das", "Schiff", "durch", "die", "stil\u00b7le", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Die fl\u00fcsternden Wellen baden", "tokens": ["Die", "fl\u00fcs\u00b7tern\u00b7den", "Wel\u00b7len", "ba\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Haupt im Morgenlicht; \u2013", "tokens": ["Das", "Haupt", "im", "Mor\u00b7gen\u00b7licht", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl sah er keine Tr\u00fcmmer,", "tokens": ["Wohl", "sah", "er", "kei\u00b7ne", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch auch die Rose nicht.", "tokens": ["Doch", "auch", "die", "Ro\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}