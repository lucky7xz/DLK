{"textgrid.poem.58325": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "363. Bleibe ja dem Hirten nah!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Innig, innig mu\u00dft du dich", "tokens": ["In\u00b7nig", ",", "in\u00b7nig", "mu\u00dft", "du", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "VMFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nah bei Jesus halten,", "tokens": ["Nah", "bei", "Je\u00b7sus", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihn ansehen stetiglich", "tokens": ["Ihn", "an\u00b7se\u00b7hen", "ste\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ihn lassen walten;", "tokens": ["Und", "ihn", "las\u00b7sen", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Licht und Leben, Kraft und Gnad',", "tokens": ["Licht", "und", "Le\u00b7ben", ",", "Kraft", "und", "Gnad'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Man allein in Jesus hat.", "tokens": ["Man", "al\u00b7lein", "in", "Je\u00b7sus", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NE", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}