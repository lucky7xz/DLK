{"dta.poem.21020": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIV.  \n Einer Hebammen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Durch meine kluge Faust lebt eine junge Welt/", "tokens": ["Durch", "mei\u00b7ne", "klu\u00b7ge", "Faust", "lebt", "ei\u00b7ne", "jun\u00b7ge", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel was mich itzt betrit/ das hab ich aufgestellt.", "tokens": ["Viel", "was", "mich", "itzt", "be\u00b7trit", "/", "das", "hab", "ich", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "ADV", "ADJD", "$(", "PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "R\u00fchmt r\u00fchmt euch Heldinnen/ doch sagt was sich ge-", "tokens": ["R\u00fchmt", "r\u00fchmt", "euch", "Hel\u00b7din\u00b7nen", "/", "doch", "sagt", "was", "sich", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "NN", "$(", "ADV", "VVFIN", "PWS", "PRF", "TRUNC"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "b\u00fchret/", "tokens": ["b\u00fch\u00b7ret", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Jhr habt viel abgef\u00fchrt/ und ich viel aufgef\u00fchrt.", "tokens": ["Ihr", "habt", "viel", "ab\u00b7ge\u00b7f\u00fchrt", "/", "und", "ich", "viel", "auf\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}