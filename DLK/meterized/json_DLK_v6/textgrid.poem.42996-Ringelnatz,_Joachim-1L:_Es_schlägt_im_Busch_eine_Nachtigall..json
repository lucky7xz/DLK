{"textgrid.poem.42996": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schl\u00e4gt im Busch eine Nachtigall.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schl\u00e4gt im Busch eine Nachtigall.", "tokens": ["Es", "schl\u00e4gt", "im", "Busch", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schl\u00e4gt ein Knecht auf dem Sommerball", "tokens": ["Es", "schl\u00e4gt", "ein", "Knecht", "auf", "dem", "Som\u00b7mer\u00b7ball"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Einem andern den Sch\u00e4del entzwei.", "tokens": ["Ei\u00b7nem", "an\u00b7dern", "den", "Sch\u00e4\u00b7del", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Es schl\u00e4gt eine Turmuhr drei.", "tokens": ["Es", "schl\u00e4gt", "ei\u00b7ne", "Tur\u00b7muhr", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "CARD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es sagt die Nacht, wenn sie vorbei", "tokens": ["Es", "sagt", "die", "Nacht", ",", "wenn", "sie", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist: \u00bbGuten Tag!\u00ab", "tokens": ["Ist", ":", "\u00bb", "Gu\u00b7ten", "Tag", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es schl\u00e4gt ein frischer Trommelschlag", "tokens": ["Es", "schl\u00e4gt", "ein", "fri\u00b7scher", "Trom\u00b7mel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schl\u00e4frigkeit zu Brei.", "tokens": ["Die", "Schl\u00e4f\u00b7rig\u00b7keit", "zu", "Brei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es sagt der Tag, wenn er vergeht:", "tokens": ["Es", "sagt", "der", "Tag", ",", "wenn", "er", "ver\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbgut Nacht!\u00ab Will nichts besagen.", "tokens": ["\u00bb", "gut", "Nacht", "!", "\u00ab", "Will", "nichts", "be\u00b7sa\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$.", "$(", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schl\u00e4gt alles \u2013 auch letzte Stunde \u2013 vorbei.", "tokens": ["Schl\u00e4gt", "al\u00b7les", "\u2013", "auch", "letz\u00b7te", "Stun\u00b7de", "\u2013", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "ADV", "ADJA", "NN", "$(", "PTKVZ", "$."], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch wer sich drauf und dran versteht,", "tokens": ["Doch", "wer", "sich", "drauf", "und", "dran", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PAV", "KON", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der h\u00f6rt in jeder Schl\u00e4gerei", "tokens": ["Der", "h\u00f6rt", "in", "je\u00b7der", "Schl\u00e4\u00b7ge\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herzen schlagen.", "tokens": ["Her\u00b7zen", "schla\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}