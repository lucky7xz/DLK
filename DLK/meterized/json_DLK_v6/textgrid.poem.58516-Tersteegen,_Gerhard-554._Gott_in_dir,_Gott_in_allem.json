{"textgrid.poem.58516": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "554. Gott in dir, Gott in allem", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist im Innern nah,", "tokens": ["Gott", "ist", "im", "In\u00b7nern", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott ist an allen Enden", "tokens": ["Gott", "ist", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In allem klar zu sehn,", "tokens": ["In", "al\u00b7lem", "klar", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In allem s\u00fc\u00df und sch\u00f6n,", "tokens": ["In", "al\u00b7lem", "s\u00fc\u00df", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In allem liebenswert,", "tokens": ["In", "al\u00b7lem", "lie\u00b7bens\u00b7wert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ganz, was ein Herz begehrt;", "tokens": ["Ganz", ",", "was", "ein", "Herz", "be\u00b7gehrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In allem bet' ich an und la\u00df mich seinen H\u00e4nden.", "tokens": ["In", "al\u00b7lem", "bet'", "ich", "an", "und", "la\u00df", "mich", "sei\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PTKVZ", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Seligsein, mein Vorwurf nur", "tokens": ["Mein", "Se\u00b7lig\u00b7sein", ",", "mein", "Vor\u00b7wurf", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist Gott, nicht ich, nicht Kreatur.", "tokens": ["Ist", "Gott", ",", "nicht", "ich", ",", "nicht", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PTKNEG", "PPER", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}