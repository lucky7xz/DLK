{"dta.poem.6687": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um uns noch mehr zu \u00fcberf\u00fchren,", "tokens": ["Um", "uns", "noch", "mehr", "zu", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es k\u00f6nne nicht das Werckzeug seyn,", "tokens": ["Es", "k\u00f6n\u00b7ne", "nicht", "das", "Wer\u00b7ck\u00b7zeug", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und es sey blo\u00df der Geist allein", "tokens": ["Und", "es", "sey", "blo\u00df", "der", "Geist", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Geschickt und m\u00e4chtig uns zu r\u00fchrrn.", "tokens": ["Ge\u00b7schickt", "und", "m\u00e4ch\u00b7tig", "uns", "zu", "r\u00fchrrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}