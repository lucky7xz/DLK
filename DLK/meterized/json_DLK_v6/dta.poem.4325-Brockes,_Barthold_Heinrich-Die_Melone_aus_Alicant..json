{"dta.poem.4325": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Melone aus Alicant.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wirst du selbst aus Alicant,", "tokens": ["So", "wirst", "du", "selbst", "aus", "A\u00b7li\u00b7cant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O angenehme Frucht, gesandt,", "tokens": ["O", "an\u00b7ge\u00b7neh\u00b7me", "Frucht", ",", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wir, durch dein vergn\u00fcgend K\u00fchlen,", "tokens": ["Da\u00df", "wir", ",", "durch", "dein", "ver\u00b7gn\u00fc\u00b7gend", "K\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Herz-erquickend Labsal f\u00fchlen!", "tokens": ["Ein", "Her\u00b7zer\u00b7qui\u00b7ckend", "Lab\u00b7sal", "f\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was schliessen nicht f\u00fcr S\u00fc\u00dfigkeiten", "tokens": ["Was", "schlies\u00b7sen", "nicht", "f\u00fcr", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dunkel-gr\u00fcne Schaalen ein!", "tokens": ["Die", "dun\u00b7kel\u00b7gr\u00fc\u00b7ne", "Schaa\u00b7len", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was f\u00fcr ein Silber-weisser Schein", "tokens": ["Was", "f\u00fcr", "ein", "Sil\u00b7ber\u00b7weis\u00b7ser", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verschr\u00e4nkt die Frucht von allen Seiten!", "tokens": ["Ver\u00b7schr\u00e4nkt", "die", "Frucht", "von", "al\u00b7len", "Sei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dein Fleisch ist g\u00fcldner als Citronen,", "tokens": ["Dein", "Fleisch", "ist", "g\u00fcld\u00b7ner", "als", "Cit\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du scheinst die Krone der Melonen.", "tokens": ["Du", "scheinst", "die", "Kro\u00b7ne", "der", "Me\u00b7lo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie lieblich ist dein s\u00fcsser Saft,", "tokens": ["Wie", "lieb\u00b7lich", "ist", "dein", "s\u00fcs\u00b7ser", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Gaum und Zunge k\u00fchlt und netzet!", "tokens": ["Der", "Gaum", "und", "Zun\u00b7ge", "k\u00fchlt", "und", "net\u00b7zet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durch eine lind\u2019 und scharfe Kraft", "tokens": ["Durch", "ei\u00b7ne", "lind'", "und", "schar\u00b7fe", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wird fast der ganze Mensch ergetzet.", "tokens": ["Wird", "fast", "der", "gan\u00b7ze", "Mensch", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Man f\u00fchlt, so bald man dich geniesset,", "tokens": ["Man", "f\u00fchlt", ",", "so", "bald", "man", "dich", "ge\u00b7nies\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADV", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und man dein fleischigt Wesen pre\u00dft,", "tokens": ["Und", "man", "dein", "flei\u00b7schigt", "We\u00b7sen", "pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So lauter Honig rinnen l\u00e4\u00dft,", "tokens": ["So", "lau\u00b7ter", "Ho\u00b7nig", "rin\u00b7nen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der wie aus einer Quelle fliesset,", "tokens": ["Der", "wie", "aus", "ei\u00b7ner", "Quel\u00b7le", "flies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wie uns der Saft, der aus dir quillt,", "tokens": ["Wie", "uns", "der", "Saft", ",", "der", "aus", "dir", "quillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Den ganzen Mund mit Nectar f\u00fcllt.", "tokens": ["Den", "gan\u00b7zen", "Mund", "mit", "Nec\u00b7tar", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ich f\u00fcgt\u2019 ein \u00fcberlegend Denken", "tokens": ["Ich", "f\u00fcgt'", "ein", "\u00fc\u00b7berl\u00b7e\u00b7gend", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "VVPP", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Dem angenehmen Schmecken bey,", "tokens": ["Dem", "an\u00b7ge\u00b7neh\u00b7men", "Schme\u00b7cken", "bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und dacht\u2019: Wie der so liebreich sey,", "tokens": ["Und", "dacht'", ":", "Wie", "der", "so", "lieb\u00b7reich", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "ART", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der, im Geschmack, so vielerley", "tokens": ["Der", ",", "im", "Ge\u00b7schmack", ",", "so", "vie\u00b7ler\u00b7ley"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "APPRART", "NN", "$,", "ADV", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Uns Menschen w\u00fcrdiget zu schenken!", "tokens": ["Uns", "Men\u00b7schen", "w\u00fcr\u00b7di\u00b7get", "zu", "schen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ich dachte ferner, Jhm zum Preise,", "tokens": ["Ich", "dach\u00b7te", "fer\u00b7ner", ",", "Jhm", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wie, auf so mannigfache Weise,", "tokens": ["Wie", ",", "auf", "so", "man\u00b7nig\u00b7fa\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die Schiffahrt uns so sehr vergn\u00fcgt,", "tokens": ["Die", "Schif\u00b7fahrt", "uns", "so", "sehr", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sie, durch rege Wasser-Schl\u00f6sser,", "tokens": ["Da", "sie", ",", "durch", "re\u00b7ge", "Was\u00b7ser\u00b7Sch\u00b7l\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Obgleich getrennet durchs Gew\u00e4sser,", "tokens": ["Ob\u00b7gleich", "ge\u00b7tren\u00b7net", "durchs", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Land doch fast ans andre f\u00fcgt.", "tokens": ["Ein", "Land", "doch", "fast", "ans", "and\u00b7re", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Frucht, die so viel Lieblichkeiten", "tokens": ["Die", "Frucht", ",", "die", "so", "viel", "Lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ihren glatten Schaalen h\u00e4uft,", "tokens": ["In", "ih\u00b7ren", "glat\u00b7ten", "Schaa\u00b7len", "h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat eine fremde Luft gereift,", "tokens": ["Hat", "ei\u00b7ne", "frem\u00b7de", "Luft", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df ein entferntes Land bereiten,", "tokens": ["Mu\u00df", "ein", "ent\u00b7fern\u00b7tes", "Land", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mu\u00df, aus so entlegner Erden,", "tokens": ["Und", "mu\u00df", ",", "aus", "so", "ent\u00b7leg\u00b7ner", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Zur Lust uns zugef\u00fchret werden.", "tokens": ["Zur", "Lust", "uns", "zu\u00b7ge\u00b7f\u00fch\u00b7ret", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erfordert es denn meine Pflicht,", "tokens": ["Er\u00b7for\u00b7dert", "es", "denn", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und die ger\u00fchrte Zunge nicht,", "tokens": ["Und", "die", "ge\u00b7r\u00fchr\u00b7te", "Zun\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df ich der Urquell\u2019 aller Dinge", "tokens": ["Da\u00df", "ich", "der", "Ur\u00b7quell'", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Erkenntlichkeit zum Opfer bringe?", "tokens": ["Er\u00b7kennt\u00b7lich\u00b7keit", "zum", "Op\u00b7fer", "brin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die durch den Gaum ger\u00fchrte Seele", "tokens": ["Die", "durch", "den", "Gaum", "ge\u00b7r\u00fchr\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Begreifet, da\u00df sie schuldig sey,", "tokens": ["Be\u00b7grei\u00b7fet", ",", "da\u00df", "sie", "schul\u00b7dig", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df sie des Gebers Lob erzehle,", "tokens": ["Da\u00df", "sie", "des", "Ge\u00b7bers", "Lob", "er\u00b7zeh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und Jhm ihr Lob zum Opfer weih.", "tokens": ["Und", "Jhm", "ihr", "Lob", "zum", "Op\u00b7fer", "weih", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wenn ich mich an der Frucht ergetze,", "tokens": ["Wenn", "ich", "mich", "an", "der", "Frucht", "er\u00b7get\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Denk ich nicht nur an sie allein;", "tokens": ["Denk", "ich", "nicht", "nur", "an", "sie", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ich denk an unsrer Sinnen Sch\u00e4tze,", "tokens": ["Ich", "denk", "an", "uns\u00b7rer", "Sin\u00b7nen", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wie sie so wunderw\u00fcrdig seyn.", "tokens": ["Wie", "sie", "so", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Was w\u00e4r uns die Melone n\u00fctze,", "tokens": ["Was", "w\u00e4r", "uns", "die", "Me\u00b7lo\u00b7ne", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und ihre s\u00fcsse Lieblichkeit,", "tokens": ["Und", "ih\u00b7re", "s\u00fcs\u00b7se", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Jhr holdes K\u00fchlen in der Hitze,", "tokens": ["Ihr", "hol\u00b7des", "K\u00fch\u00b7len", "in", "der", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wofern nicht die Beschaffenheit", "tokens": ["Wo\u00b7fern", "nicht", "die", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Sinnen in den Geist uns dr\u00fcckt", "tokens": ["Der", "Sin\u00b7nen", "in", "den", "Geist", "uns", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Was uns durch ihren Saft erquickt,", "tokens": ["Was", "uns", "durch", "ih\u00b7ren", "Saft", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wenn, was wir lieblichs in ihr finden,", "tokens": ["Wenn", ",", "was", "wir", "lieb\u00b7lichs", "in", "ihr", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So Gaum als Zungen nicht empf\u00fcnden!", "tokens": ["So", "Gaum", "als", "Zun\u00b7gen", "nicht", "emp\u00b7f\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Da mich dein saftig Fleisch ern\u00e4hret,", "tokens": ["Da", "mich", "dein", "saf\u00b7tig", "Fleisch", "er\u00b7n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Hab ich an Den, Der dich gemacht,", "tokens": ["Hab", "ich", "an", "Den", ",", "Der", "dich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NE", "$,", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "An Den, durch Den du mir beschehret,", "tokens": ["An", "Den", ",", "durch", "Den", "du", "mir", "be\u00b7scheh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "w