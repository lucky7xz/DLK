{"textgrid.poem.31108": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "222.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe kurtze Sinn-Getichte, damit die B\u00f6sen minder b\u00f6se,", "tokens": ["Ich", "schrei\u00b7be", "kurt\u00b7ze", "Sinn\u00b7Ge\u00b7tich\u00b7te", ",", "da\u00b7mit", "die", "B\u00f6\u00b7sen", "min\u00b7der", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Auch da\u00df zu wichtigern Beginnen ich desto eh mich abel\u00f6se.", "tokens": ["Auch", "da\u00df", "zu", "wich\u00b7ti\u00b7gern", "Be\u00b7gin\u00b7nen", "ich", "des\u00b7to", "eh", "mich", "a\u00b7be\u00b7l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN", "PPER", "ADV", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}