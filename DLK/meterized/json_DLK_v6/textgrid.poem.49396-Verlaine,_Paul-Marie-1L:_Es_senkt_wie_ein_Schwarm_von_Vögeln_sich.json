{"textgrid.poem.49396": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es senkt wie ein Schwarm von V\u00f6geln sich", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es senkt wie ein Schwarm von V\u00f6geln sich", "tokens": ["Es", "senkt", "wie", "ein", "Schwarm", "von", "V\u00f6\u00b7geln", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "NN", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "All mein Erinnern hernieder auf mich,", "tokens": ["All", "mein", "E\u00b7rin\u00b7nern", "her\u00b7nie\u00b7der", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PTKVZ", "APPR", "PPER", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Hernieder durchs gelbe Laub von den Zweigen,", "tokens": ["Her\u00b7nie\u00b7der", "durchs", "gel\u00b7be", "Laub", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und gebeugt ist mein Herz, wie Erlen sich neigen,", "tokens": ["Und", "ge\u00b7beugt", "ist", "mein", "Herz", ",", "wie", "Er\u00b7len", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die sich spiegeln, wo das Wasser der Reue", "tokens": ["Die", "sich", "spie\u00b7geln", ",", "wo", "das", "Was\u00b7ser", "der", "Reu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "VVINF", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schwerm\u00fctig gleitet in tiefer Bl\u00e4ue.", "tokens": ["Schwer\u00b7m\u00fc\u00b7tig", "glei\u00b7tet", "in", "tie\u00b7fer", "Bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Sie senken sich, bis im wachsenden Wehen", "tokens": ["Sie", "sen\u00b7ken", "sich", ",", "bis", "im", "wach\u00b7sen\u00b7den", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Des Winds ihre b\u00f6sen Stimmen vergehen,", "tokens": ["Des", "Winds", "ih\u00b7re", "b\u00f6\u00b7sen", "Stim\u00b7men", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Im Baume verklingen mit sterbendem Laut,", "tokens": ["Im", "Bau\u00b7me", "ver\u00b7klin\u00b7gen", "mit", "ster\u00b7ben\u00b7dem", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Dass Stille rings von den Zweigen taut.", "tokens": ["Dass", "Stil\u00b7le", "rings", "von", "den", "Zwei\u00b7gen", "taut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Nur die Stimme, die sie, die fern ist, in Tr\u00e4nen", "tokens": ["Nur", "die", "Stim\u00b7me", ",", "die", "sie", ",", "die", "fern", "ist", ",", "in", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Verherrlicht, t\u00f6nt, nur die Stimme voll Sehnen", "tokens": ["Ver\u00b7herr\u00b7licht", ",", "t\u00f6nt", ",", "nur", "die", "Stim\u00b7me", "voll", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "ART", "NN", "ADJD", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Des V\u00f6gleins, das erste Liebe mir war,", "tokens": ["Des", "V\u00f6\u00b7gleins", ",", "das", "ers\u00b7te", "Lie\u00b7be", "mir", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "PPER", "VAFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Das heute noch singt, wie vor manchem Jahr.", "tokens": ["Das", "heu\u00b7te", "noch", "singt", ",", "wie", "vor", "man\u00b7chem", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "$,", "PWAV", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Und in dem trauernden Mondenscheine,", "tokens": ["Und", "in", "dem", "trau\u00b7ern\u00b7den", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Der bleich und feierlich strahlt, wiegt eine", "tokens": ["Der", "bleich", "und", "fei\u00b7er\u00b7lich", "strahlt", ",", "wiegt", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "$,", "VVFIN", "ART"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Schwerm\u00fctige Nacht der Sommerszeit", "tokens": ["Schwer\u00b7m\u00fc\u00b7ti\u00b7ge", "Nacht", "der", "Som\u00b7mers\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Voll tiefer Sehnsucht und Dunkelheit", "tokens": ["Voll", "tie\u00b7fer", "Sehn\u00b7sucht", "und", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Im Himmel in fl\u00fcsternden Windesschauern", "tokens": ["Im", "Him\u00b7mel", "in", "fl\u00fcs\u00b7tern\u00b7den", "Win\u00b7des\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Das Zittern des Baums und des Vogels Trauern.", "tokens": ["Das", "Zit\u00b7tern", "des", "Baums", "und", "des", "Vo\u00b7gels", "Trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}