{"textgrid.poem.64141": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbder ist allein ein K\u00f6nig, wen bindet keine Pflicht,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder ist allein ein K\u00f6nig, wen bindet keine Pflicht,", "tokens": ["\u00bb", "der", "ist", "al\u00b7lein", "ein", "K\u00f6\u00b7nig", ",", "wen", "bin\u00b7det", "kei\u00b7ne", "Pflicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VAFIN", "ADV", "ART", "NN", "$,", "PWS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer andrer Recht soll achten, der ist ein K\u00f6nig nicht.\u00ab", "tokens": ["Wer", "an\u00b7drer", "Recht", "soll", "ach\u00b7ten", ",", "der", "ist", "ein", "K\u00f6\u00b7nig", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADJA", "NN", "VMFIN", "VVINF", "$,", "PRELS", "VAFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So sprach der K\u00f6nig Olaf, frisch kam er von Byzanz,", "tokens": ["So", "sprach", "der", "K\u00f6\u00b7nig", "O\u00b7laf", ",", "frisch", "kam", "er", "von", "By\u00b7zanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$,", "ADJD", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hat dort als Gast bewundert des Imperators Glanz.", "tokens": ["Hat", "dort", "als", "Gast", "be\u00b7wun\u00b7dert", "des", "Im\u00b7pe\u00b7ra\u00b7tors", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOUS", "NN", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "\u00bbich bin der trotz'gen Bauern von Svearike satt,", "tokens": ["\u00bb", "ich", "bin", "der", "trotz'\u00b7gen", "Bau\u00b7ern", "von", "Svea\u00b7ri\u00b7ke", "satt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wie Leo will ich herrschen in seiner goldnen Stadt.\u00ab", "tokens": ["Wie", "Leo", "will", "ich", "herr\u00b7schen", "in", "sei\u00b7ner", "gold\u00b7nen", "Stadt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NE", "VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Er sandte seine Boten und Schatzung schrieb er aus:", "tokens": ["Er", "sand\u00b7te", "sei\u00b7ne", "Bo\u00b7ten", "und", "Schat\u00b7zung", "schrieb", "er", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Von jedem Kopf ein Schilling und zw\u00f6lf von jedem Haus.", "tokens": ["Von", "je\u00b7dem", "Kopf", "ein", "Schil\u00b7ling", "und", "zw\u00f6lf", "von", "je\u00b7dem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "KON", "CARD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und der Bote kam nach Dunsadal und bot das Volk zu Hauf", "tokens": ["Und", "der", "Bo\u00b7te", "kam", "nach", "Dun\u00b7sa\u00b7dal", "und", "bot", "das", "Volk", "zu", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NE", "KON", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Zur Hofburg nach Upsala, zu Ting und Schatzung auf.", "tokens": ["Zur", "Hof\u00b7burg", "nach", "Up\u00b7sa\u00b7la", ",", "zu", "Ting", "und", "Schat\u00b7zung", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,", "APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Da sprach ein Bauer \u2013 man kennt ihn nicht \u2013 sein Bart war wei\u00df wie Schnee:", "tokens": ["Da", "sprach", "ein", "Bau\u00b7er", "\u2013", "man", "kennt", "ihn", "nicht", "\u2013", "sein", "Bart", "war", "wei\u00df", "wie", "Schnee", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "PIS", "VVFIN", "PPER", "PTKNEG", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "\u00bbwer etwas will, der geht zu dem, von dem er's will, von je.", "tokens": ["\u00bb", "wer", "et\u00b7was", "will", ",", "der", "geht", "zu", "dem", ",", "von", "dem", "er's", "will", ",", "von", "je", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "PIS", "VMFIN", "$,", "PRELS", "VVFIN", "APPR", "ART", "$,", "APPR", "ART", "PIS", "VMFIN", "$,", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Wir woll'n von K\u00f6nig Olaf nichts: \u2013 und will er was von uns,", "tokens": ["Wir", "woll'n", "von", "K\u00f6\u00b7nig", "O\u00b7laf", "nichts", ":", "\u2013", "und", "will", "er", "was", "von", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "NE", "PIS", "$.", "$(", "KON", "VMFIN", "PPER", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "So komm' er, wo wir tagen stets, an den K\u00f6nigsbronn von Duns.", "tokens": ["So", "komm'", "er", ",", "wo", "wir", "ta\u00b7gen", "stets", ",", "an", "den", "K\u00f6\u00b7nigs\u00b7bronn", "von", "Duns", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NN", "ADV", "$,", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Da harr'n wir sein zur Sonnenwend', wann die Linden in Bl\u00fcten stehn.\u00ab", "tokens": ["Da", "harr'n", "wir", "sein", "zur", "Son\u00b7nen\u00b7wend'", ",", "wann", "die", "Lin\u00b7den", "in", "Bl\u00fc\u00b7ten", "stehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "APPRART", "NN", "$,", "PWAV", "ART", "NE", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Der Bote ging und der K\u00f6nig schwur: \u00bbDer Trotz soll euch vergehn.\u00ab", "tokens": ["Der", "Bo\u00b7te", "ging", "und", "der", "K\u00f6\u00b7nig", "schwur", ":", "\u00bb", "Der", "Trotz", "soll", "euch", "ver\u00b7gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$.", "$(", "ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Und als die Lind' in Bl\u00fcten stand, entbot er Ro\u00df und Mann", "tokens": ["Und", "als", "die", "Lind'", "in", "Bl\u00fc\u00b7ten", "stand", ",", "ent\u00b7bot", "er", "Ro\u00df", "und", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "Und zog, dreitausend Reiter stark, nach Dunsadal hin dann.", "tokens": ["Und", "zog", ",", "drei\u00b7tau\u00b7send", "Rei\u00b7ter", "stark", ",", "nach", "Dun\u00b7sa\u00b7dal", "hin", "dann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "CARD", "NN", "ADJD", "$,", "APPR", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "Und als er kam zum K\u00f6nigsbronn mit den Seinen von Mittag her,", "tokens": ["Und", "als", "er", "kam", "zum", "K\u00f6\u00b7nigs\u00b7bronn", "mit", "den", "Sei\u00b7nen", "von", "Mit\u00b7tag", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "PPOSS", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Zw\u00f6lf alte M\u00e4nner sa\u00dfen dort, sonst war die Tingstatt leer.", "tokens": ["Zw\u00f6lf", "al\u00b7te", "M\u00e4n\u00b7ner", "sa\u00b7\u00dfen", "dort", ",", "sonst", "war", "die", "Ting\u00b7statt", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Ein dichter Eichwald lag im Nord: hehr lag er, stolz und still,", "tokens": ["Ein", "dich\u00b7ter", "Eich\u00b7wald", "lag", "im", "Nord", ":", "hehr", "lag", "er", ",", "stolz", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "Nur wenn der Wind in den Wipfeln ging, scholl's, wie wenn's wettern will.", "tokens": ["Nur", "wenn", "der", "Wind", "in", "den", "Wip\u00b7feln", "ging", ",", "scholl's", ",", "wie", "wenn's", "wet\u00b7tern", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "Und der K\u00f6nig ritt an des Brunnens Rand: \u2013 der Brunnen war schwarz und tief: \u2013", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "ritt", "an", "des", "Brun\u00b7nens", "Rand", ":", "\u2013", "der", "Brun\u00b7nen", "war", "schwarz", "und", "tief", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "--+--+-+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Die Zw\u00f6lfe sa\u00dfen im Kreise still, der K\u00f6nig aber rief:", "tokens": ["Die", "Zw\u00f6l\u00b7fe", "sa\u00b7\u00dfen", "im", "Krei\u00b7se", "still", ",", "der", "K\u00f6\u00b7nig", "a\u00b7ber", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.25": {"text": "\u00bbich bin gekommen, ihr habt's gewollt: doch mit dreitausend Mann:", "tokens": ["\u00bb", "ich", "bin", "ge\u00b7kom\u00b7men", ",", "ihr", "habt's", "ge\u00b7wollt", ":", "doch", "mit", "drei\u00b7tau\u00b7send", "Mann", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VMPP", "$.", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Wollt ihr jetzt tun, wie ich gebot, und gehorchen meinem Bann?\u00ab \u2013", "tokens": ["Wollt", "ihr", "jetzt", "tun", ",", "wie", "ich", "ge\u00b7bot", ",", "und", "ge\u00b7hor\u00b7chen", "mei\u00b7nem", "Bann", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Da sprach ein Bauer \u2013 man kennt ihn nicht \u2013 sein Haar war silberhell,", "tokens": ["Da", "sprach", "ein", "Bau\u00b7er", "\u2013", "man", "kennt", "ihn", "nicht", "\u2013", "sein", "Haar", "war", "sil\u00b7ber\u00b7hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "PIS", "VVFIN", "PPER", "PTKNEG", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Er trug ein gro\u00dfes B\u00fcffelhorn und sein Mantel war B\u00e4renfell.", "tokens": ["Er", "trug", "ein", "gro\u00b7\u00dfes", "B\u00fcf\u00b7fel\u00b7horn", "und", "sein", "Man\u00b7tel", "war", "B\u00e4\u00b7ren\u00b7fell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.29": {"text": "\u00bbdu hast gefragt:\u00ab \u2013 sprach der alte Mann \u2013 \u00bbals Antwort frag' ich dich:", "tokens": ["\u00bb", "du", "hast", "ge\u00b7fragt", ":", "\u00ab", "\u2013", "sprach", "der", "al\u00b7te", "Mann", "\u2013", "\u00bb", "als", "Ant\u00b7wort", "frag'", "ich", "dich", ":"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$.", "$(", "$(", "VVFIN", "ART", "ADJA", "NN", "$(", "$(", "KOUS", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-++-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.30": {"text": "Woher hei\u00dft der Brunnen K\u00f6nigsbronn, wei\u00dft du das, K\u00f6nig, sprich?\u00ab", "tokens": ["Wo\u00b7her", "hei\u00dft", "der", "Brun\u00b7nen", "K\u00f6\u00b7nigs\u00b7bronn", ",", "wei\u00dft", "du", "das", ",", "K\u00f6\u00b7nig", ",", "sprich", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NE", "$,", "VVFIN", "PPER", "PDS", "$,", "NN", "$,", "ADJD", "$.", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "\u00bbwas soll der Bronn? ich wei\u00df es nicht!\u00ab \u2013 \u00bbSo will ich dir's tun kund:", "tokens": ["\u00bb", "was", "soll", "der", "Bronn", "?", "ich", "wei\u00df", "es", "nicht", "!", "\u00ab", "\u2013", "\u00bb", "So", "will", "ich", "dir's", "tun", "kund", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "$(", "$(", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.32": {"text": "Drei alte Sveak\u00f6n'ge liegen in des Brunnens Grund.", "tokens": ["Drei", "al\u00b7te", "Svea\u00b7k\u00f6n'\u00b7ge", "lie\u00b7gen", "in", "des", "Brun\u00b7nens", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "K\u00f6nig Knut war hart wie Eisen, er war von deinem Geschlecht.", "tokens": ["K\u00f6\u00b7nig", "Knut", "war", "hart", "wie", "Ei\u00b7sen", ",", "er", "war", "von", "dei\u00b7nem", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "KOKOM", "NN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+