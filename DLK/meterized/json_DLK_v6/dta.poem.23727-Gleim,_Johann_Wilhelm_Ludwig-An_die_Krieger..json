{"dta.poem.23727": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Krieger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt doch, allerliebste Krieger,", "tokens": ["H\u00f6rt", "doch", ",", "al\u00b7ler\u00b7liebs\u00b7te", "Krie\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt doch, seid doch nicht so grimmig.", "tokens": ["H\u00f6rt", "doch", ",", "seid", "doch", "nicht", "so", "grim\u00b7mig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ihr mit den Feinden fechtet,", "tokens": ["Wenn", "ihr", "mit", "den", "Fein\u00b7den", "fech\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stechen euch die Feinde Wunden,", "tokens": ["Ste\u00b7chen", "euch", "die", "Fein\u00b7de", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dann m\u00fc\u00dft ihr euch verbluten.", "tokens": ["Und", "dann", "m\u00fc\u00dft", "ihr", "euch", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warum wollt ihr euch verbluten?", "tokens": ["Wa\u00b7rum", "wollt", "ihr", "euch", "ver\u00b7blu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollt ihr etwa, an den Wunden", "tokens": ["Wollt", "ihr", "et\u00b7wa", ",", "an", "den", "Wun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eines sanften Todes sterben?", "tokens": ["Ei\u00b7nes", "sanf\u00b7ten", "To\u00b7des", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Warum wollt ihr denn schon sterben?", "tokens": ["Wa\u00b7rum", "wollt", "ihr", "denn", "schon", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seht ihr nicht, auf diesen Bergen", "tokens": ["Seht", "ihr", "nicht", ",", "auf", "die\u00b7sen", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Reifen schon die vollen Trauben!", "tokens": ["Rei\u00b7fen", "schon", "die", "vol\u00b7len", "Trau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Stiftet Frieden mit den Feinden,", "tokens": ["Stif\u00b7tet", "Frie\u00b7den", "mit", "den", "Fein\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Helft die vollen Trauben keltern,", "tokens": ["Helft", "die", "vol\u00b7len", "Trau\u00b7ben", "kel\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Trinkt den Most,, und werdet Br\u00fcder,", "tokens": ["Trinkt", "den", "Most", ",", ",", "und", "wer\u00b7det", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$,", "KON", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und la\u00dft euch durch Wein und Freundschaft,", "tokens": ["Und", "la\u00dft", "euch", "durch", "Wein", "und", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Alle Lust zum Sterben rauben!", "tokens": ["Al\u00b7le", "Lust", "zum", "Ster\u00b7ben", "rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}