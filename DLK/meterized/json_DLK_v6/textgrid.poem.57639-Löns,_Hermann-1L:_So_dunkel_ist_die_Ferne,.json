{"textgrid.poem.57639": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: So dunkel ist die Ferne,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So dunkel ist die Ferne,", "tokens": ["So", "dun\u00b7kel", "ist", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Heide ist so leer,", "tokens": ["Die", "Hei\u00b7de", "ist", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die schwarzen Kr\u00e4hen kommen", "tokens": ["Die", "schwar\u00b7zen", "Kr\u00e4\u00b7hen", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hastig \u00fcber sie her.", "tokens": ["Has\u00b7tig", "\u00fc\u00b7ber", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Auf weiter dunkler Heide", "tokens": ["Auf", "wei\u00b7ter", "dunk\u00b7ler", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich ganz alleine bin,", "tokens": ["Ich", "ganz", "al\u00b7lei\u00b7ne", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die schwarzen Kr\u00e4hen fliegen", "tokens": ["Die", "schwar\u00b7zen", "Kr\u00e4\u00b7hen", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Immer \u00fcber mich hin.", "tokens": ["Im\u00b7mer", "\u00fc\u00b7ber", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}