{"dta.poem.3047": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob gut ob b\u00f6se sei ein Geist, von dem du dich", "tokens": ["Ob", "gut", "ob", "b\u00f6\u00b7se", "sei", "ein", "Geist", ",", "von", "dem", "du", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KOUS", "ADJD", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Getrieben f\u00fchlest, wei\u00dft du nie so eigentlich.", "tokens": ["Ge\u00b7trie\u00b7ben", "f\u00fch\u00b7lest", ",", "wei\u00dft", "du", "nie", "so", "ei\u00b7gent\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df Gro\u00dfes, Sch\u00f6nes er, ja Gutes thun dich hei\u00dft,", "tokens": ["Da\u00df", "Gro\u00b7\u00dfes", ",", "Sch\u00f6\u00b7nes", "er", ",", "ja", "Gu\u00b7tes", "thun", "dich", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADJA", "PPER", "$,", "ADV", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit ists nicht gethan, das thut auch b\u00f6ser Geist;", "tokens": ["Da\u00b7mit", "ists", "nicht", "ge\u00b7than", ",", "das", "thut", "auch", "b\u00f6\u00b7ser", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKNEG", "VVPP", "$,", "PDS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Hochmuths b\u00f6ser Geist, des Scheins, der Heuchelei,", "tokens": ["Des", "Hoch\u00b7muths", "b\u00f6\u00b7ser", "Geist", ",", "des", "Scheins", ",", "der", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der selbst sich bildet ein, da\u00df er ein guter sei.", "tokens": ["Der", "selbst", "sich", "bil\u00b7det", "ein", ",", "da\u00df", "er", "ein", "gu\u00b7ter", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur wo der Geist dich treibt zu dulden und zu lieben,", "tokens": ["Nur", "wo", "der", "Geist", "dich", "treibt", "zu", "dul\u00b7den", "und", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PPER", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da hat dich ganz gewi\u00df ein guter Geist getrieben.", "tokens": ["Da", "hat", "dich", "ganz", "ge\u00b7wi\u00df", "ein", "gu\u00b7ter", "Geist", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}