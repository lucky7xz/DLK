{"textgrid.poem.62753": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit stillen br\u00fcnstigen Gebeten,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit stillen br\u00fcnstigen Gebeten,", "tokens": ["Mit", "stil\u00b7len", "br\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam t\u00e4glich vor Jehovens Thron,", "tokens": ["Kam", "t\u00e4g\u00b7lich", "vor", "Je\u00b7ho\u00b7vens", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arist, ein frommer Greis, getreten,", "tokens": ["A\u00b7rist", ",", "ein", "from\u00b7mer", "Greis", ",", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bat f\u00fcr seinen frommen Sohn.", "tokens": ["Und", "bat", "f\u00fcr", "sei\u00b7nen", "from\u00b7men", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist, o Gott, mein Trost auf Erden,", "tokens": ["Er", "ist", ",", "o", "Gott", ",", "mein", "Trost", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df ihn daf\u00fcr so gl\u00fccklich werden,", "tokens": ["La\u00df", "ihn", "da\u00b7f\u00fcr", "so", "gl\u00fcck\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als dein Gesch\u00f6pf es werden kann.", "tokens": ["Als", "dein", "Ge\u00b7sch\u00f6pf", "es", "wer\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So betete der heilge Mann.", "tokens": ["So", "be\u00b7te\u00b7te", "der", "heil\u00b7ge", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst sank er zu des Altars Fu\u00dfe", "tokens": ["Einst", "sank", "er", "zu", "des", "Al\u00b7tars", "Fu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In himmlische Begeistrung hin,", "tokens": ["In", "himm\u00b7li\u00b7sche", "Be\u00b7geis\u00b7trung", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da trat mit einem holden Gru\u00dfe", "tokens": ["Da", "trat", "mit", "ei\u00b7nem", "hol\u00b7den", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein lichter Seraph neben ihn.", "tokens": ["Ein", "lich\u00b7ter", "Se\u00b7raph", "ne\u00b7ben", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Der Herr, so sprach er, der dich h\u00f6ret,", "tokens": ["Der", "Herr", ",", "so", "sprach", "er", ",", "der", "dich", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Freund, hat dir deinen Wunsch gew\u00e4hret,", "tokens": ["Freund", ",", "hat", "dir", "dei\u00b7nen", "Wunsch", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und morgen kr\u00f6net hier der Lohn", "tokens": ["Und", "mor\u00b7gen", "kr\u00f6\u00b7net", "hier", "der", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Tugend dich und deinen Sohn.", "tokens": ["Der", "Tu\u00b7gend", "dich", "und", "dei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Alte wacht in seiner Zelle", "tokens": ["Der", "Al\u00b7te", "wacht", "in", "sei\u00b7ner", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und betet, bis es morgen war:", "tokens": ["Und", "be\u00b7tet", ",", "bis", "es", "mor\u00b7gen", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Itzt trat sein Fu\u00df in die Kapelle.", "tokens": ["Itzt", "trat", "sein", "Fu\u00df", "in", "die", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Leichnam lag vor dem Altar.", "tokens": ["Ein", "Leich\u00b7nam", "lag", "vor", "dem", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Es war sein Liebling. Keine Z\u00e4hre", "tokens": ["Es", "war", "sein", "Lieb\u00b7ling", ".", "Kei\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Entweiht sein Auge; Gott sey Ehre!", "tokens": ["Ent\u00b7weiht", "sein", "Au\u00b7ge", ";", "Gott", "sey", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So ruft er, k\u00fc\u00dft mit Himmelslust", "tokens": ["So", "ruft", "er", ",", "k\u00fc\u00dft", "mit", "Him\u00b7mels\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Sohn und \u2013 stirbt auf seiner Brust.", "tokens": ["Den", "Sohn", "und", "\u2013", "stirbt", "auf", "sei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Selma, der ich in der Jugend", "tokens": ["O", "Sel\u00b7ma", ",", "der", "ich", "in", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die\u00df Lied zum Pfand der Freundschaft gab,", "tokens": ["Die\u00df", "Lied", "zum", "Pfand", "der", "Freund\u00b7schaft", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun leg ichs deiner Engeltugend", "tokens": ["Nun", "leg", "ichs", "dei\u00b7ner", "En\u00b7gel\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Denkmal auf dein frisches Grab.", "tokens": ["Zum", "Denk\u00b7mal", "auf", "dein", "fri\u00b7sches", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr, die es leset, fromme Sch\u00f6nen,", "tokens": ["Ihr", ",", "die", "es", "le\u00b7set", ",", "from\u00b7me", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Benetzet es mit euren Thr\u00e4nen", "tokens": ["Be\u00b7net\u00b7zet", "es", "mit", "eu\u00b7ren", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr Selma. Mehr als Elegie", "tokens": ["F\u00fcr", "Sel\u00b7ma", ".", "Mehr", "als", "E\u00b7le\u00b7gie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$.", "PIAT", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Marmor ehren Thr\u00e4nen sie.", "tokens": ["Und", "Mar\u00b7mor", "eh\u00b7ren", "Thr\u00e4\u00b7nen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}