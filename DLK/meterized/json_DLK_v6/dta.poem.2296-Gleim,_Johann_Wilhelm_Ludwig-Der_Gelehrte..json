{"dta.poem.2296": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Gelehrte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich von den Zeitungsschreibern", "tokens": ["Soll", "ich", "von", "den", "Zei\u00b7tungs\u00b7schrei\u00b7bern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Namen schreiben lernen?", "tokens": ["Mei\u00b7nen", "Na\u00b7men", "schrei\u00b7ben", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll ich in dem Sterngew\u00f6lbe", "tokens": ["Soll", "ich", "in", "dem", "Stern\u00b7ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neue Welten sichtbar machen?", "tokens": ["Neu\u00b7e", "Wel\u00b7ten", "sicht\u00b7bar", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich Wolfen oder Knutzen", "tokens": ["Soll", "ich", "Wol\u00b7fen", "o\u00b7der", "Knut\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zweifelsknoten l\u00f6sen helfen?", "tokens": ["Zwei\u00b7fels\u00b7kno\u00b7ten", "l\u00f6\u00b7sen", "hel\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ich Stoff und Sittenlehren", "tokens": ["Soll", "ich", "Stoff", "und", "Sit\u00b7ten\u00b7leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr die Bl\u00e4tterschreiber stehlen?", "tokens": ["F\u00fcr", "die", "Bl\u00e4t\u00b7ter\u00b7schrei\u00b7ber", "steh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll ich von den B\u00fccherrichtern", "tokens": ["Soll", "ich", "von", "den", "B\u00fc\u00b7cher\u00b7rich\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schimpfen oder tadeln lernen?", "tokens": ["Schimp\u00b7fen", "o\u00b7der", "ta\u00b7deln", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Soll ich in der Weltgeschichte", "tokens": ["Soll", "ich", "in", "der", "Welt\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Proben tapfrer Narren suchen?", "tokens": ["Pro\u00b7ben", "tapf\u00b7rer", "Nar\u00b7ren", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Soll ich meinen Geist befragen:", "tokens": ["Soll", "ich", "mei\u00b7nen", "Geist", "be\u00b7fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Was er sei, und wo er wohne?", "tokens": ["Was", "er", "sei", ",", "und", "wo", "er", "woh\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Soll ich mit den Oberpriestern", "tokens": ["Soll", "ich", "mit", "den", "O\u00b7ber\u00b7pries\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Heucheln, oder Ketzer machen?", "tokens": ["Heu\u00b7cheln", ",", "o\u00b7der", "Ket\u00b7zer", "ma\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Soll ich f\u00fcr den Kupferstecher", "tokens": ["Soll", "ich", "f\u00fcr", "den", "Kup\u00b7fer\u00b7ste\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Mein gelehrtes Bildni\u00df mahlen?", "tokens": ["Mein", "ge\u00b7lehr\u00b7tes", "Bild\u00b7ni\u00df", "mah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Soll ich Blei zu Golde schmelzen?", "tokens": ["Soll", "ich", "Blei", "zu", "Gol\u00b7de", "schmel\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Soll ich R\u00e4the rathen lehren?", "tokens": ["Soll", "ich", "R\u00e4\u00b7the", "ra\u00b7then", "leh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Soll ich Miltons Teufel schelten?", "tokens": ["Soll", "ich", "Mil\u00b7tons", "Teu\u00b7fel", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Soll ich Wunderwerke dichten?", "tokens": ["Soll", "ich", "Wun\u00b7der\u00b7wer\u00b7ke", "dich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Oder soll ich sie erkl\u00e4ren?", "tokens": ["O\u00b7der", "soll", "ich", "sie", "er\u00b7kl\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Nein, dis soll mein Anverwandter.", "tokens": ["Nein", ",", "dis", "soll", "mein", "An\u00b7ver\u00b7wand\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Er, der Prinz ber\u00fchmter Narren,", "tokens": ["Er", ",", "der", "Prinz", "be\u00b7r\u00fchm\u00b7ter", "Nar\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Er, der grundgelehrte Wisser,", "tokens": ["Er", ",", "der", "grund\u00b7ge\u00b7lehr\u00b7te", "Wis\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Er, der Pr\u00fcfer der Beweise.", "tokens": ["Er", ",", "der", "Pr\u00fc\u00b7fer", "der", "Be\u00b7wei\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Soll sich noch zu Tode gr\u00fcbeln;", "tokens": ["Soll", "sich", "noch", "zu", "To\u00b7de", "gr\u00fc\u00b7beln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Er, der Erbfeind meiner Freude", "tokens": ["Er", ",", "der", "Erb\u00b7feind", "mei\u00b7ner", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Soll sich bla\u00df und elend lesen.", "tokens": ["Soll", "sich", "bla\u00df", "und", "e\u00b7lend", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und dann will ich ihn befragen:", "tokens": ["Und", "dann", "will", "ich", "ihn", "be\u00b7fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Macht mich auch mein M\u00e4dchen elend?", "tokens": ["Macht", "mich", "auch", "mein", "M\u00e4d\u00b7chen", "e\u00b7lend", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}