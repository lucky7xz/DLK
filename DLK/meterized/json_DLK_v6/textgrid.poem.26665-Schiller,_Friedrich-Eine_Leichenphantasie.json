{"textgrid.poem.26665": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine Leichenphantasie", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit erstorbnem Scheinen", "tokens": ["Mit", "ers\u00b7torb\u00b7nem", "Schei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Steht der Mond auf totenstillen Hainen,", "tokens": ["Steht", "der", "Mond", "auf", "to\u00b7tens\u00b7til\u00b7len", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seufzend streicht der Nachtgeist durch die Luft \u2013", "tokens": ["Seuf\u00b7zend", "streicht", "der", "Nacht\u00b7geist", "durch", "die", "Luft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nebelwolken schauern,", "tokens": ["Ne\u00b7bel\u00b7wol\u00b7ken", "schau\u00b7ern", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sterne trauern", "tokens": ["Ster\u00b7ne", "trau\u00b7ern"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Bleich herab, wie Lampen in der Gruft.", "tokens": ["Bleich", "her\u00b7ab", ",", "wie", "Lam\u00b7pen", "in", "der", "Gruft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Gleich Gespenstern, stumm und hohl und hager,", "tokens": ["Gleich", "Ge\u00b7spens\u00b7tern", ",", "stumm", "und", "hohl", "und", "ha\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zieht in schwarzem Totenpompe dort", "tokens": ["Zieht", "in", "schwar\u00b7zem", "To\u00b7ten\u00b7pom\u00b7pe", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ein Gewimmel nach dem Leichenlager", "tokens": ["Ein", "Ge\u00b7wim\u00b7mel", "nach", "dem", "Lei\u00b7chen\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Unterm Schauerflor der Grabnacht fort.", "tokens": ["Un\u00b7term", "Schau\u00b7er\u00b7flor", "der", "Grab\u00b7nacht", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Zitternd an der Kr\u00fccke,", "tokens": ["Zit\u00b7ternd", "an", "der", "Kr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wer mit d\u00fcsterm, r\u00fcckgesunknem Blicke,", "tokens": ["Wer", "mit", "d\u00fcs\u00b7term", ",", "r\u00fcck\u00b7ge\u00b7sun\u00b7knem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ausgegossen in ein heulend Ach,", "tokens": ["Aus\u00b7ge\u00b7gos\u00b7sen", "in", "ein", "heu\u00b7lend", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "VVPP", "ITJ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schwer geneckt vom eisernen Geschicke,", "tokens": ["Schwer", "ge\u00b7neckt", "vom", "ei\u00b7ser\u00b7nen", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Schwankt dem stumm getragnen Sarge nach?", "tokens": ["Schwankt", "dem", "stumm", "ge\u00b7trag\u00b7nen", "Sar\u00b7ge", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Flo\u00df es ", "tokens": ["Flo\u00df", "es"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Nasse Schauer schauern f\u00fcrchterlich", "tokens": ["Nas\u00b7se", "Schau\u00b7er", "schau\u00b7ern", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Durch sein gramgeschmolzenes Gerippe,", "tokens": ["Durch", "sein", "gram\u00b7ge\u00b7schmol\u00b7ze\u00b7nes", "Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Seine Silberhaare b\u00e4umen sich. \u2013", "tokens": ["Sei\u00b7ne", "Sil\u00b7ber\u00b7haa\u00b7re", "b\u00e4u\u00b7men", "sich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aufgerissen seine Feuerwunde!", "tokens": ["Auf\u00b7ge\u00b7ris\u00b7sen", "sei\u00b7ne", "Feu\u00b7er\u00b7wun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch die Seele H\u00f6llenschmerz!", "tokens": ["Durch", "die", "See\u00b7le", "H\u00f6l\u00b7len\u00b7schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eiskalt, eiskalt liegt er hier im Tuche,", "tokens": ["Eis\u00b7kalt", ",", "eis\u00b7kalt", "liegt", "er", "hier", "im", "Tu\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dein Traum, so golden einst, so s\u00fc\u00df!", "tokens": ["Und", "dein", "Traum", ",", "so", "gol\u00b7den", "einst", ",", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "ADJD", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "S\u00fc\u00df und golden, Vater, dir zum Fluche!", "tokens": ["S\u00fc\u00df", "und", "gol\u00b7den", ",", "Va\u00b7ter", ",", "dir", "zum", "Flu\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$,", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eiskalt, eiskalt liegt er hier im Tuche!", "tokens": ["Eis\u00b7kalt", ",", "eis\u00b7kalt", "liegt", "er", "hier", "im", "Tu\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Deine Wonne und dein Paradies. \u2013", "tokens": ["Dei\u00b7ne", "Won\u00b7ne", "und", "dein", "Pa\u00b7ra\u00b7dies", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mild, wie umweht von Elysiumsl\u00fcften,", "tokens": ["Mild", ",", "wie", "um\u00b7weht", "von", "E\u00b7ly\u00b7si\u00b7ums\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wie aus Auroras Umarmung geschl\u00fcpft,", "tokens": ["Wie", "aus", "Au\u00b7ro\u00b7ras", "Um\u00b7ar\u00b7mung", "ge\u00b7schl\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Himmlisch umg\u00fcrtet mit rosigten D\u00fcften,", "tokens": ["Himm\u00b7lisch", "um\u00b7g\u00fcr\u00b7tet", "mit", "ro\u00b7sig\u00b7ten", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Florens Sohn \u00fcber das Blumenfeld h\u00fcpft,", "tokens": ["Flo\u00b7rens", "Sohn", "\u00fc\u00b7ber", "das", "Blu\u00b7men\u00b7feld", "h\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Flog er einher auf den lachenden Wiesen,", "tokens": ["Flog", "er", "ein\u00b7her", "auf", "den", "la\u00b7chen\u00b7den", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Nachgespiegelt von silberner Flut,", "tokens": ["Nach\u00b7ge\u00b7spie\u00b7gelt", "von", "sil\u00b7ber\u00b7ner", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wollustflammen entspr\u00fchten den K\u00fcssen,", "tokens": ["Wol\u00b7lust\u00b7flam\u00b7men", "ent\u00b7spr\u00fch\u00b7ten", "den", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Jagten die M\u00e4dchen in liebende Glut.", "tokens": ["Jag\u00b7ten", "die", "M\u00e4d\u00b7chen", "in", "lie\u00b7ben\u00b7de", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Mutig sprang er im Gew\u00fchle der Menschen,", "tokens": ["Mu\u00b7tig", "sprang", "er", "im", "Ge\u00b7w\u00fch\u00b7le", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie auf Gebirgen ein jugendlich Reh,", "tokens": ["Wie", "auf", "Ge\u00b7bir\u00b7gen", "ein", "ju\u00b7gend\u00b7lich", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Himmelum flog er in schweifenden W\u00fcnschen,", "tokens": ["Him\u00b7me\u00b7lum", "flog", "er", "in", "schwei\u00b7fen\u00b7den", "W\u00fcn\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Hoch wie der Adler in wolkigter H\u00f6h,", "tokens": ["Hoch", "wie", "der", "Ad\u00b7ler", "in", "wol\u00b7kig\u00b7ter", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Stolz wie die Rosse sich str\u00e4uben und sch\u00e4umen,", "tokens": ["Stolz", "wie", "die", "Ros\u00b7se", "sich", "str\u00e4u\u00b7ben", "und", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Werfen im Sturme die M\u00e4hnen umher,", "tokens": ["Wer\u00b7fen", "im", "Stur\u00b7me", "die", "M\u00e4h\u00b7nen", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "K\u00f6niglich wider den Z\u00fcgel sich b\u00e4umen,", "tokens": ["K\u00f6\u00b7nig\u00b7lich", "wi\u00b7der", "den", "Z\u00fc\u00b7gel", "sich", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Trat er vor Sklaven und F\u00fcrsten daher.", "tokens": ["Trat", "er", "vor", "Skla\u00b7ven", "und", "F\u00fcrs\u00b7ten", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PAV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Heiter wie Fr\u00fchlingstag schwand ihm das Leben,", "tokens": ["Hei\u00b7ter", "wie", "Fr\u00fch\u00b7lings\u00b7tag", "schwand", "ihm", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Floh ihm vor\u00fcber in Hesperus' Glanz,", "tokens": ["Floh", "ihm", "vor\u00b7\u00fc\u00b7ber", "in", "Hes\u00b7pe\u00b7rus'", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Klagen ertr\u00e4nkt' er im Golde der Reben,", "tokens": ["Kla\u00b7gen", "er\u00b7tr\u00e4nkt'", "er", "im", "Gol\u00b7de", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Schmerzen verh\u00fcpft' er im wirbelnden Tanz.", "tokens": ["Schmer\u00b7zen", "ver\u00b7h\u00fcpft'", "er", "im", "wir\u00b7beln\u00b7den", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Welten schliefen im herrlichen Jungen,", "tokens": ["Wel\u00b7ten", "schlie\u00b7fen", "im", "herr\u00b7li\u00b7chen", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ha! wenn er einsten zum Manne gereift \u2013", "tokens": ["Ha", "!", "wenn", "er", "eins\u00b7ten", "zum", "Man\u00b7ne", "ge\u00b7reift", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Freue dich, Vater! \u2013 im herrlichen Jungen", "tokens": ["Freu\u00b7e", "dich", ",", "Va\u00b7ter", "!", "\u2013", "im", "herr\u00b7li\u00b7chen", "Jun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "$(", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Wenn einst die schlafenden Keime gereift.", "tokens": ["Wenn", "einst", "die", "schla\u00b7fen\u00b7den", "Kei\u00b7me", "ge\u00b7reift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Nein doch, Vater \u2013 Horch! die Kirchhoft\u00fcre brauset,", "tokens": ["Nein", "doch", ",", "Va\u00b7ter", "\u2013", "Horch", "!", "die", "Kirch\u00b7hof\u00b7t\u00fc\u00b7re", "brau\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "NN", "$(", "NE", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und die ehrnen Angel klirren auf \u2013", "tokens": ["U