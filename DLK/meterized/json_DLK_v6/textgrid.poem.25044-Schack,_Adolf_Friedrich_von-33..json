{"textgrid.poem.25044": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wozu noch, M\u00e4dchen, soll es frommen,", "tokens": ["Wo\u00b7zu", "noch", ",", "M\u00e4d\u00b7chen", ",", "soll", "es", "from\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du vor mir Verstellung \u00fcbst?", "tokens": ["Da\u00df", "du", "vor", "mir", "Ver\u00b7stel\u00b7lung", "\u00fcbst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hei\u00df froh das neue Gl\u00fcck willkommen", "tokens": ["Hei\u00df", "froh", "das", "neu\u00b7e", "Gl\u00fcck", "will\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sag es offen, da\u00df du liebst!", "tokens": ["Und", "sag", "es", "of\u00b7fen", ",", "da\u00df", "du", "liebst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "An deines Busens h\u00f6herm Schwellen,", "tokens": ["An", "dei\u00b7nes", "Bu\u00b7sens", "h\u00f6\u00b7herm", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Wangenrot, das kommt und geht,", "tokens": ["Dem", "Wan\u00b7gen\u00b7rot", ",", "das", "kommt", "und", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward dein Geheimnis von den Quellen,", "tokens": ["Ward", "dein", "Ge\u00b7heim\u00b7nis", "von", "den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Blumengeistern l\u00e4ngst ersp\u00e4ht.", "tokens": ["Den", "Blu\u00b7men\u00b7geis\u00b7tern", "l\u00e4ngst", "er\u00b7sp\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Wogen murmeln's in den Grotten,", "tokens": ["Die", "Wo\u00b7gen", "mur\u00b7meln's", "in", "den", "Grot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es fl\u00fcstert's leis der Abendwind;", "tokens": ["Es", "fl\u00fcs\u00b7tert's", "leis", "der", "Ab\u00b7end\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo du vorbeigehst, h\u00f6rst du's spotten:", "tokens": ["Wo", "du", "vor\u00b7bei\u00b7gehst", ",", "h\u00f6rst", "du's", "spot\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "VVFIN", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wir wissen es seit lange, Kind!", "tokens": ["Wir", "wis\u00b7sen", "es", "seit", "lan\u00b7ge", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}