{"textgrid.poem.28546": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man wei\u00df, wann Menschen-Blut ein Artzt wil distilliren,", "tokens": ["Man", "wei\u00df", ",", "wann", "Men\u00b7schen\u00b7Blut", "ein", "Artzt", "wil", "dis\u00b7til\u00b7li\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was f\u00fcr Geruch dabey wird seine Nase r\u00fchren.", "tokens": ["Was", "f\u00fcr", "Ge\u00b7ruch", "da\u00b7bey", "wird", "sei\u00b7ne", "Na\u00b7se", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergossen Menschen-Blut nicht r\u00e4chen, sondern decken,", "tokens": ["Ver\u00b7gos\u00b7sen", "Men\u00b7schen\u00b7Blut", "nicht", "r\u00e4\u00b7chen", ",", "son\u00b7dern", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVINF", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was wird f\u00fcr Gottes Thron di\u00df f\u00fcr Geruch erwecken", "tokens": ["Was", "wird", "f\u00fcr", "Got\u00b7tes", "Thron", "di\u00df", "f\u00fcr", "Ge\u00b7ruch", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "NN", "NN", "PDS", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}