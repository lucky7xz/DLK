{"textgrid.poem.66732": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nehmen's die Kr\u00e4mer und M\u00e4kler dir krumm \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmen's die Kr\u00e4mer und M\u00e4kler dir krumm \u2013", "tokens": ["Neh\u00b7men's", "die", "Kr\u00e4\u00b7mer", "und", "M\u00e4k\u00b7ler", "dir", "krumm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "NN", "PPER", "ADJD", "$("], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geh in die Sonne, scher dich nicht drum!", "tokens": ["Geh", "in", "die", "Son\u00b7ne", ",", "scher", "dich", "nicht", "drum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "ADJD", "PPER", "PTKNEG", "PAV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sonne mit goldig erw\u00e4rmendem Licht", "tokens": ["Son\u00b7ne", "mit", "gol\u00b7dig", "er\u00b7w\u00e4r\u00b7men\u00b7dem", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Gibt dir vom Leben und marktet nicht.", "tokens": ["Gibt", "dir", "vom", "Le\u00b7ben", "und", "mark\u00b7tet", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "G\u00fctig und milde voll herrlicher Macht,", "tokens": ["G\u00fc\u00b7tig", "und", "mil\u00b7de", "voll", "herr\u00b7li\u00b7cher", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Sch\u00fcttet sie \u00fcber dich Schimmer und Pracht.", "tokens": ["Sch\u00fct\u00b7tet", "sie", "\u00fc\u00b7ber", "dich", "Schim\u00b7mer", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "W\u00fcrdest du Kr\u00f6sus und Kaiser zumal,", "tokens": ["W\u00fcr\u00b7dest", "du", "Kr\u00f6\u00b7sus", "und", "Kai\u00b7ser", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "K\u00fc\u00dfte dich m\u00fctterlich segnend ihr Strahl.", "tokens": ["K\u00fc\u00df\u00b7te", "dich", "m\u00fct\u00b7ter\u00b7lich", "seg\u00b7nend", "ihr", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Die dich mit lachenden Lippen ber\u00fchrt,", "tokens": ["Die", "dich", "mit", "la\u00b7chen\u00b7den", "Lip\u00b7pen", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat sie dein liebendes Leben versp\u00fcrt?", "tokens": ["Hat", "sie", "dein", "lie\u00b7ben\u00b7des", "Le\u00b7ben", "ver\u00b7sp\u00fcrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00dcber die Ma\u00dfen begl\u00fcckt ihre Huld,", "tokens": ["\u00dc\u00b7ber", "die", "Ma\u00b7\u00dfen", "be\u00b7gl\u00fcckt", "ih\u00b7re", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Jeglichem Schenkenden schenkt sie die Schuld.", "tokens": ["Jeg\u00b7li\u00b7chem", "Schen\u00b7ken\u00b7den", "schenkt", "sie", "die", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Pfiffige Sch\u00e4cher, mit Gro\u00dfmut verbr\u00e4mt,", "tokens": ["Pfif\u00b7fi\u00b7ge", "Sch\u00e4\u00b7cher", ",", "mit", "Gro\u00df\u00b7mut", "ver\u00b7br\u00e4mt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Geht aus der Sonne, wenn ihr euch sch\u00e4mt!", "tokens": ["Geht", "aus", "der", "Son\u00b7ne", ",", "wenn", "ihr", "euch", "sch\u00e4mt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}