{"textgrid.poem.62355": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An eine Orangenbl\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was willst du in den kalten Zonen,", "tokens": ["Was", "willst", "du", "in", "den", "kal\u00b7ten", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Blume, die aus S\u00fcden kam?", "tokens": ["O", "Blu\u00b7me", ",", "die", "aus", "S\u00fc\u00b7den", "kam", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch ich mu\u00df in der Fremde wohnen", "tokens": ["Auch", "ich", "mu\u00df", "in", "der", "Frem\u00b7de", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Sehnsucht und voll Gram.", "tokens": ["Voll", "Sehn\u00b7sucht", "und", "voll", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und beide nur ein kl\u00e4glich Leben,", "tokens": ["Und", "bei\u00b7de", "nur", "ein", "kl\u00e4g\u00b7lich", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Krankenhause, leben wir;", "tokens": ["Im", "Kran\u00b7ken\u00b7hau\u00b7se", ",", "le\u00b7ben", "wir", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was uns der Heimat G\u00f6tter geben,", "tokens": ["Was", "uns", "der", "Hei\u00b7mat", "G\u00f6t\u00b7ter", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer n\u00fctzt und liebt es hier?", "tokens": ["Wer", "n\u00fctzt", "und", "liebt", "es", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verschlie\u00dfe deine zarten D\u00fcfte,", "tokens": ["Ver\u00b7schlie\u00b7\u00dfe", "dei\u00b7ne", "zar\u00b7ten", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kelch von Wohlger\u00fcchen schwer,", "tokens": ["Den", "Kelch", "von", "Wohl\u00b7ge\u00b7r\u00fc\u00b7chen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und str\u00f6me nicht in Todtengr\u00fcfte", "tokens": ["Und", "str\u00f6\u00b7me", "nicht", "in", "Tod\u00b7ten\u00b7gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des h\u00f6chsten Lebens Meer.", "tokens": ["Des", "h\u00f6chs\u00b7ten", "Le\u00b7bens", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch sie, der unter milderm Himmel", "tokens": ["Auch", "sie", ",", "der", "un\u00b7ter", "mil\u00b7derm", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wol manches kleine Lied entquoll,", "tokens": ["Wol", "man\u00b7ches", "klei\u00b7ne", "Lied", "ent\u00b7quoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Harfe schweigt im Kriegsget\u00fcmmel,", "tokens": ["Die", "Har\u00b7fe", "schweigt", "im", "Kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie klang so minnevoll.", "tokens": ["Sie", "klang", "so", "min\u00b7ne\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dort magst du wieder dich entfalten,", "tokens": ["Dort", "magst", "du", "wie\u00b7der", "dich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo deine warme Heimat bl\u00fcht;", "tokens": ["Wo", "dei\u00b7ne", "war\u00b7me", "Hei\u00b7mat", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort, wo die stillen Zauber walten,", "tokens": ["Dort", ",", "wo", "die", "stil\u00b7len", "Zau\u00b7ber", "wal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sing' ich ein neues Lied.", "tokens": ["Sing'", "ich", "ein", "neu\u00b7es", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und k\u00f6nnen wir es nicht erwerben,", "tokens": ["Und", "k\u00f6n\u00b7nen", "wir", "es", "nicht", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6chsten Sehnsucht h\u00f6chstes Ziel,", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Sehn\u00b7sucht", "h\u00f6chs\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df uns welken, la\u00df uns sterben", "tokens": ["So", "la\u00df", "uns", "wel\u00b7ken", ",", "la\u00df", "uns", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In schmerzlichem Gef\u00fchl.", "tokens": ["In", "schmerz\u00b7li\u00b7chem", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}