{"dta.poem.10694": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df die Poeterey vnsterblich sey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Was wirffstu/ schn\u00f6der Neid/ mir f\u00fcr die Lust zu schreiben", "tokens": ["Was", "wirffs\u00b7tu", "/", "schn\u00f6\u00b7der", "Neid", "/", "mir", "f\u00fcr", "die", "Lust", "zu", "schrei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$(", "ADJA", "NN", "$(", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Venus/ vnd mit jhr die Jugendt zu vertreiben", "tokens": ["Von", "Ve\u00b7nus", "/", "vnd", "mit", "jhr", "die", "Ju\u00b7gendt", "zu", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "KON", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich achte deiner nicht/ du liebest Eytelkeit/", "tokens": ["Ich", "ach\u00b7te", "dei\u00b7ner", "nicht", "/", "du", "lie\u00b7best", "Ey\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "PTKNEG", "$(", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Lob vnd Name wird erklingen weit vnd breit.", "tokens": ["Mein", "Lob", "vnd", "Na\u00b7me", "wird", "er\u00b7klin\u00b7gen", "weit", "vnd", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Cupido f\u00fchret mich in eine gr\u00fcne W\u00fcsten/", "tokens": ["Cu\u00b7pi\u00b7do", "f\u00fch\u00b7ret", "mich", "in", "ei\u00b7ne", "gr\u00fc\u00b7ne", "W\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da der Poeten Volck weit von Begierd vnd L\u00fcsten/", "tokens": ["Da", "der", "Po\u00b7et\u00b7en", "Volck", "weit", "von", "Be\u00b7gierd", "vnd", "L\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vor Zeiten hat gewohnt/ wie noch die erste Welt", "tokens": ["Vor", "Zei\u00b7ten", "hat", "ge\u00b7wohnt", "/", "wie", "noch", "die", "ers\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$(", "KOKOM", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nichts von den St\u00e4tten wust/ vnd wohnet in dem Feldt/", "tokens": ["Nichts", "von", "den", "St\u00e4t\u00b7ten", "wust", "/", "vnd", "woh\u00b7net", "in", "dem", "Feldt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$(", "KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Die Nymphen werden mir den Lorbeerkrantz auffsetzen/", "tokens": ["Die", "Nym\u00b7phen", "wer\u00b7den", "mir", "den", "Lor\u00b7beer\u00b7krantz", "auff\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit meinen Versen wird sich Erato ergetzen/", "tokens": ["Mit", "mei\u00b7nen", "Ver\u00b7sen", "wird", "sich", "E\u00b7ra\u00b7to", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PRF", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So weit die gr\u00fcne Lust/ die gr\u00fcne W\u00e4lder gehn/", "tokens": ["So", "weit", "die", "gr\u00fc\u00b7ne", "Lust", "/", "die", "gr\u00fc\u00b7ne", "W\u00e4l\u00b7der", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird gleichsam mein Gedicht an allen B\u00e4wmen stehn/", "tokens": ["Wird", "gleich\u00b7sam", "mein", "Ge\u00b7dicht", "an", "al\u00b7len", "B\u00e4w\u00b7men", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr Oerter voller Frewd/ jhr auffenthalt der Hirten/", "tokens": ["Ihr", "O\u00b7er\u00b7ter", "vol\u00b7ler", "Frewd", "/", "jhr", "auf\u00b7fent\u00b7halt", "der", "Hir\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Ihr B\u00e4ch/ jhr Ahornb\u00e4wm/ jhr Quell/ jhr zarten Myrten/", "tokens": ["Ihr", "B\u00e4ch", "/", "jhr", "A\u00b7horn\u00b7b\u00e4wm", "/", "jhr", "Quell", "/", "jhr", "zar\u00b7ten", "Myr\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ihr Th\u00e4ler/ jhr Gebirg/ jhr sch\u00f6nen Bl\u00fcmelein/", "tokens": ["Ihr", "Th\u00e4\u00b7ler", "/", "jhr", "Ge\u00b7birg", "/", "jhr", "sch\u00f6\u00b7nen", "Bl\u00fc\u00b7me\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ihr Wohnhau\u00df aller Ruh/ bey euch w\u00fcnsch\u2019 ich zu sein/", "tokens": ["Ihr", "Wohn\u00b7hau\u00df", "al\u00b7ler", "Ruh", "/", "bey", "euch", "w\u00fcn\u00b7sch'", "ich", "zu", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "$(", "APPR", "PPER", "VVFIN", "PPER", "PTKZU", "VAINF", "$("], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Bey euch w\u00fcnsch ich zu sein; von ewrer Lust besessen/", "tokens": ["Bey", "euch", "w\u00fcnsch", "ich", "zu", "sein", ";", "von", "ew\u00b7rer", "Lust", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PTKZU", "VAINF", "$.", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Will ich de\u00df jrrdischen/ vnd meiner selbst/ vergessen.", "tokens": ["Will", "ich", "de\u00df", "jrr\u00b7di\u00b7schen", "/", "vnd", "mei\u00b7ner", "selbst", "/", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "$(", "KON", "PPOSAT", "ADV", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie Perseus/ als er erst Andromeden erblickt/", "tokens": ["Wie", "Per\u00b7seus", "/", "als", "er", "erst", "And\u00b7ro\u00b7me\u00b7den", "er\u00b7blickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$(", "KOUS", "PPER", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ward mitten in der Lufft durch jhre Zierd verz\u00fcckt/", "tokens": ["Ward", "mit\u00b7ten", "in", "der", "Lufft", "durch", "jhre", "Zierd", "ver\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "So da\u00df er kaum das Ross vermochte zu regieren/", "tokens": ["So", "da\u00df", "er", "kaum", "das", "Ross", "ver\u00b7moch\u00b7te", "zu", "re\u00b7gie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So soll mich auch von euch kein ander Liebe f\u00fchren/", "tokens": ["So", "soll", "mich", "auch", "von", "euch", "kein", "an\u00b7der", "Lie\u00b7be", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PIAT", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bi\u00df mich der letzte Todt gantz vnversehens kriegt/", "tokens": ["Bi\u00df", "mich", "der", "letz\u00b7te", "Todt", "gantz", "vn\u00b7ver\u00b7se\u00b7hens", "kriegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vnd Venus mich begr\u00e4bt/ wo jhr Adonis ligt.", "tokens": ["Vnd", "Ve\u00b7nus", "mich", "be\u00b7gr\u00e4bt", "/", "wo", "jhr", "A\u00b7do\u00b7nis", "ligt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}