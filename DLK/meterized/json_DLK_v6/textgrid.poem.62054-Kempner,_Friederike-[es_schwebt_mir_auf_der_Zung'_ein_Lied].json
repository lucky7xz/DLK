{"textgrid.poem.62054": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es schwebt mir auf der Zung' ein Lied]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schwebt mir auf der Zung' ein Lied,", "tokens": ["Es", "schwebt", "mir", "auf", "der", "Zung'", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein frohes, sinnig Lied,", "tokens": ["Ein", "fro\u00b7hes", ",", "sin\u00b7nig", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es w\u00e4chst so rasch, es gr\u00fcnt, es bl\u00fcht,", "tokens": ["Es", "w\u00e4chst", "so", "rasch", ",", "es", "gr\u00fcnt", ",", "es", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ging ich, so ging's, schied ich, es schied.", "tokens": ["Ging", "ich", ",", "so", "ging's", ",", "schied", "ich", ",", "es", "schied", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O goldnes Lied, geboren kaum,", "tokens": ["O", "gold\u00b7nes", "Lied", ",", "ge\u00b7bo\u00b7ren", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedankenschwerer junger Traum,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7schwe\u00b7rer", "jun\u00b7ger", "Traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrwahr Du bist von Gott gesandt,", "tokens": ["F\u00fcr\u00b7wahr", "Du", "bist", "von", "Gott", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Himmels s\u00fc\u00dfes Unterpfand!", "tokens": ["Des", "Him\u00b7mels", "s\u00fc\u00b7\u00dfes", "Un\u00b7ter\u00b7pfand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}