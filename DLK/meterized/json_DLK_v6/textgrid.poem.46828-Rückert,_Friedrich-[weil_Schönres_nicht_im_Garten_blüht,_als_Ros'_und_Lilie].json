{"textgrid.poem.46828": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[weil Sch\u00f6nres nicht im Garten bl\u00fcht, als Ros' und Lilie]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil Sch\u00f6nres nicht im Garten bl\u00fcht, als Ros' und Lilie,", "tokens": ["Weil", "Sch\u00f6n\u00b7res", "nicht", "im", "Gar\u00b7ten", "bl\u00fcht", ",", "als", "Ros'", "und", "Li\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$,", "KOUS", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bl\u00fcht ihr stets mir im Gem\u00fcth als Ros' und Lilie.", "tokens": ["So", "bl\u00fcht", "ihr", "stets", "mir", "im", "Ge\u00b7m\u00fcth", "als", "Ros'", "und", "Li\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "APPRART", "NN", "KOKOM", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Garten meines Herzens ist als G\u00e4rtner fr\u00fch und spat", "tokens": ["Im", "Gar\u00b7ten", "mei\u00b7nes", "Her\u00b7zens", "ist", "als", "G\u00e4rt\u00b7ner", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Mein Liebesleid um euch bem\u00fcht, als Ros' und Lilie.", "tokens": ["Mein", "Lie\u00b7be\u00b7sleid", "um", "euch", "be\u00b7m\u00fcht", ",", "als", "Ros'", "und", "Li\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,", "KOUS", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An jedem Morgen seid ihr frisch, an jedem Abend neu,", "tokens": ["An", "je\u00b7dem", "Mor\u00b7gen", "seid", "ihr", "frisch", ",", "an", "je\u00b7dem", "A\u00b7bend", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "$,", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Vom Na\u00df der Augen angespr\u00fcht, als Ros' und Lilie.", "tokens": ["Vom", "Na\u00df", "der", "Au\u00b7gen", "an\u00b7ge\u00b7spr\u00fcht", ",", "als", "Ros'", "und", "Li\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,", "KOUS", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ihr bl\u00fcht als Ros' und Lilie, wenn kalt der Winter st\u00fcrmt,", "tokens": ["Ihr", "bl\u00fcht", "als", "Ros'", "und", "Li\u00b7lie", ",", "wenn", "kalt", "der", "Win\u00b7ter", "st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "KON", "NN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und wenn der hei\u00dfe Sommer gl\u00fcht, als Ros' und Lilie.", "tokens": ["Und", "wenn", "der", "hei\u00b7\u00dfe", "Som\u00b7mer", "gl\u00fcht", ",", "als", "Ros'", "und", "Li\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr bl\u00fcht in der Erinnerung sch\u00f6n wie im Leben einst,", "tokens": ["Ihr", "bl\u00fcht", "in", "der", "E\u00b7rin\u00b7ne\u00b7rung", "sch\u00f6n", "wie", "im", "Le\u00b7ben", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "KOKOM", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Als Bild der Anmuth und der G\u00fct', als Ros' und Lilie.", "tokens": ["Als", "Bild", "der", "An\u00b7muth", "und", "der", "G\u00fct'", ",", "als", "Ros'", "und", "Li\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "KON", "ART", "NN", "$,", "KOUS", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von Liebe sei die Seele klar, und ganz von Flecken rein,", "tokens": ["Von", "Lie\u00b7be", "sei", "die", "See\u00b7le", "klar", ",", "und", "ganz", "von", "Fle\u00b7cken", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADJD", "$,", "KON", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Da\u00df sie in sich euch ewig h\u00fct' als Ros' und Lilie;", "tokens": ["Da\u00df", "sie", "in", "sich", "euch", "e\u00b7wig", "h\u00fct'", "als", "Ros'", "und", "Li\u00b7lie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "PPER", "ADJD", "VVFIN", "KOKOM", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O ihr, so hell als Lilien und Rosen einst erbl\u00fcht,", "tokens": ["O", "ihr", ",", "so", "hell", "als", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", "einst", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$,", "ADV", "ADJD", "KOKOM", "NE", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "So schnell und schneller nun verbl\u00fcht, als Ros' und Lilie!", "tokens": ["So", "schnell", "und", "schnel\u00b7ler", "nun", "ver\u00b7bl\u00fcht", ",", "als", "Ros'", "und", "Li\u00b7lie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "VVPP", "$,", "KOUS", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}