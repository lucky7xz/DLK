{"textgrid.poem.44808": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wollt ihr die deutsche Knechtschaft kennen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr die deutsche Knechtschaft kennen,", "tokens": ["Wollt", "ihr", "die", "deut\u00b7sche", "Knecht\u00b7schaft", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So studiert die deutsche Geschichte.", "tokens": ["So", "stu\u00b7diert", "die", "deut\u00b7sche", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die aber f\u00fcr deutsche Freiheit brennen,", "tokens": ["Die", "a\u00b7ber", "f\u00fcr", "deut\u00b7sche", "Frei\u00b7heit", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fchrt Livius mit besserer Richte.", "tokens": ["F\u00fchrt", "Li\u00b7vius", "mit", "bes\u00b7se\u00b7rer", "Rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}