{"dta.poem.5217": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Don Goldofon: oder der sterbende  \n Geizige,  \n  eine Erz\u00e4hlung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Als Lissabon noch ganz in k\u00f6niglicher Pracht", "tokens": ["Als", "Lis\u00b7sa\u00b7bon", "noch", "ganz", "in", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem alten Tyrus glich; durch Schiffarth stolz gemacht,", "tokens": ["Dem", "al\u00b7ten", "Ty\u00b7rus", "glich", ";", "durch", "Schiff\u00b7arth", "stolz", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$.", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch sein erhabnes Haupt bis an die Wolken th\u00fcrmte,", "tokens": ["Noch", "sein", "er\u00b7hab\u00b7nes", "Haupt", "bis", "an", "die", "Wol\u00b7ken", "th\u00fcrm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh\u2019 aus der Erd ein Gott die starke Hand erhub,", "tokens": ["Eh'", "aus", "der", "Erd", "ein", "Gott", "die", "star\u00b7ke", "Hand", "er\u00b7hub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Herauf an die Pall\u00e4ste st\u00fcrmte,", "tokens": ["Her\u00b7auf", "an", "die", "Pal\u00b7l\u00e4s\u00b7te", "st\u00fcrm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und tief im Abgrund sie begrub:", "tokens": ["Und", "tief", "im", "Ab\u00b7grund", "sie", "be\u00b7grub", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da war ein reicher Mann, ein alter Portugiese.", "tokens": ["Da", "war", "ein", "rei\u00b7cher", "Mann", ",", "ein", "al\u00b7ter", "Por\u00b7tu\u00b7gie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Reichthum fiel ihm zu. So h\u00e4ufen auf der Wiese", "tokens": ["Der", "Reicht\u00b7hum", "fiel", "ihm", "zu", ".", "So", "h\u00e4u\u00b7fen", "auf", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die M\u00e4her trocknes Heu; und an des Meeres Strand", "tokens": ["Die", "M\u00e4\u00b7her", "trock\u00b7nes", "Heu", ";", "und", "an", "des", "Mee\u00b7res", "Strand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hat kleine Steinechen so ungez\u00e4hlt der Sand,", "tokens": ["Hat", "klei\u00b7ne", "Stei\u00b7ne\u00b7chen", "so", "un\u00b7ge\u00b7z\u00e4hlt", "der", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als dieser Goldofon, im raumichten Gew\u00f6lbe", "tokens": ["Als", "die\u00b7ser", "Gol\u00b7do\u00b7fon", ",", "im", "rau\u00b7mich\u00b7ten", "Ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.12": {"text": "Dublonen aufgeh\u00e4ust. Ihm gleichen an der Elbe", "tokens": ["Du\u00b7blo\u00b7nen", "auf\u00b7ge\u00b7h\u00e4ust", ".", "Ihm", "glei\u00b7chen", "an", "der", "El\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vielleicht auch reiche M\u00e4nner? Nein!", "tokens": ["Viel\u00b7leicht", "auch", "rei\u00b7che", "M\u00e4n\u00b7ner", "?", "Nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zu Magdeburg kann nicht der Geiz gebohren seyn!", "tokens": ["Zu", "Mag\u00b7de\u00b7burg", "kann", "nicht", "der", "Geiz", "ge\u00b7boh\u00b7ren", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PTKNEG", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In Lissabon war er nur ganz in seinem Knechte.", "tokens": ["In", "Lis\u00b7sa\u00b7bon", "war", "er", "nur", "ganz", "in", "sei\u00b7nem", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In Lissabon, da sind vom j\u00fcdischen Geschlechte", "tokens": ["In", "Lis\u00b7sa\u00b7bon", ",", "da", "sind", "vom", "j\u00fc\u00b7di\u00b7schen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Viel tausende verkapt, die fromm im Tempel gehn,", "tokens": ["Viel", "tau\u00b7sen\u00b7de", "ver\u00b7kapt", ",", "die", "fromm", "im", "Tem\u00b7pel", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "An einem Rosenkranz die K\u00f6rner z\u00e4hlen stehn,", "tokens": ["An", "ei\u00b7nem", "Ro\u00b7sen\u00b7kranz", "die", "K\u00f6r\u00b7ner", "z\u00e4h\u00b7len", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vor einem Bilde knien, und doch Hebr\u00e4er bleiben.", "tokens": ["Vor", "ei\u00b7nem", "Bil\u00b7de", "kni\u00b7en", ",", "und", "doch", "Heb\u00b7r\u00e4\u00b7er", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "In Lissabon allein, wie mir ein Buch gesagt,", "tokens": ["In", "Lis\u00b7sa\u00b7bon", "al\u00b7lein", ",", "wie", "mir", "ein", "Buch", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "War nur ein Mann, von dem ich weiter nichts beschreiben", "tokens": ["War", "nur", "ein", "Mann", ",", "von", "dem", "ich", "wei\u00b7ter", "nichts", "be\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als nur die Stunde will, in der ein Reicher fragt:", "tokens": ["Als", "nur", "die", "Stun\u00b7de", "will", ",", "in", "der", "ein", "Rei\u00b7cher", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ob Millionen ihm nur einen Tag erkaufen?", "tokens": ["Ob", "Mil\u00b7lion\u00b7en", "ihm", "nur", "ei\u00b7nen", "Tag", "er\u00b7kau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Krank ward der reiche Grei\u00df. Krank dacht er seinen", "tokens": ["Krank", "ward", "der", "rei\u00b7che", "Grei\u00df", ".", "Krank", "dacht", "er", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "NN", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Haufen,", "tokens": ["Hau\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Und seufzte blickend in das Grab:", "tokens": ["Und", "seufz\u00b7te", "bli\u00b7ckend", "in", "das", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "\u201dwas halfs, da\u00df ich Allmosen gab?", "tokens": ["\"", "was", "halfs", ",", "da\u00df", "ich", "All\u00b7mo\u00b7sen", "gab", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "\u201ddie Armen trotzten mir f\u00fcr das, was ich gegeben,", "tokens": ["\"", "die", "Ar\u00b7men", "trotz\u00b7ten", "mir", "f\u00fcr", "das", ",", "was", "ich", "ge\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "APPR", "PDS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201ddoch kein methusalemisch Leben", "tokens": ["\"", "doch", "kein", "me\u00b7thu\u00b7sa\u00b7le\u00b7misch", "Le\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "\u201ddurch ihr Gebet vom harten Himmel ab!", "tokens": ["\"", "durch", "ihr", "Ge\u00b7bet", "vom", "har\u00b7ten", "Him\u00b7mel", "ab", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "So sprach Don Goldofon, mit Zittern und mit Beben,", "tokens": ["So", "sprach", "Don", "Gol\u00b7do\u00b7fon", ",", "mit", "Zit\u00b7tern", "und", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und hie\u00df aufs Sterbebett sich einen Beutel heben,", "tokens": ["Und", "hie\u00df", "aufs", "Ster\u00b7be\u00b7bett", "sich", "ei\u00b7nen", "Beu\u00b7tel", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mit alten goldnen M\u00fcnzen voll.", "tokens": ["Mit", "al\u00b7ten", "gold\u00b7nen", "M\u00fcn\u00b7zen", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Er z\u00e4hlte seinen Trost, und klebte mit den Blicken", "tokens": ["Er", "z\u00e4hl\u00b7te", "sei\u00b7nen", "Trost", ",", "und", "kleb\u00b7te", "mit", "den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie mit der Hand daran. Doch f\u00fcr sein ewig Wohl", "tokens": ["Wie", "mit", "der", "Hand", "da\u00b7ran", ".", "Doch", "f\u00fcr", "sein", "e\u00b7wig", "Wohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PAV", "$.", "KON", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Befand ein Nachbar gut, den Priester hinzuschicken.", "tokens": ["Be\u00b7fand", "ein", "Nach\u00b7bar", "gut", ",", "den", "Pries\u00b7ter", "hin\u00b7zu\u00b7schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Pater kam, und sprach: \u201dDon Goldofon, ich soll", "tokens": ["Der", "Pa\u00b7ter", "kam", ",", "und", "sprach", ":", "\"", "Don", "Gol\u00b7do\u00b7fon", ",", "ich", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "$.", "$(", "NE", "NE", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "\u201dverm\u00f6ge meiner Pflicht, euch fragen,", "tokens": ["\"", "ver\u00b7m\u00f6\u00b7ge", "mei\u00b7ner", "Pflicht", ",", "euch", "fra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "\u201dob ihr bereitet seyd zum Schritt in jene Welt?", "tokens": ["\"", "ob", "ihr", "be\u00b7rei\u00b7tet", "seyd", "zum", "Schritt", "in", "je\u00b7ne", "Welt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VVPP", "VAFIN", "APPRART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "\u201ddort einem Gotte Dank zu sagen,", "tokens": ["\"", "dort", "ei\u00b7nem", "Got\u00b7te", "Dank", "zu", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "\u201dder euch viel G\u00fcter hier, als Darlehn zugestellt!", "tokens": ["\"", "der", "euch", "viel", "G\u00fc\u00b7ter", "hier", ",", "als", "Dar\u00b7lehn", "zu\u00b7ge\u00b7stellt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "PIAT", "NN", "ADV", "$,", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "\u201dihr wurdet alt, bey Gl\u00fcck und guten Tagen;", "tokens": ["\"", "ihr", "wur\u00b7det", "alt", ",", "bey", "Gl\u00fcck", "und", "gu\u00b7ten", "Ta\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$,", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "\u201djetzt hoff ich doch von euch, da\u00df ihr als guter", "tokens": ["\"", "jetzt", "hoff", "ich", "doch", "von", "euch", ",", "da\u00df", "ihr", "als", "gu\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "KOUS", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Christ,", "tokens": ["Christ", ","], "token_info":