{"textgrid.poem.67115": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Mu\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sorglos schlummert die Brust und es ruhn die strengen Gedanken.", "tokens": ["Sorg\u00b7los", "schlum\u00b7mert", "die", "Brust", "und", "es", "ruhn", "die", "stren\u00b7gen", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf die Wiese geh ich hinaus, wo das Gras aus der Wurzel", "tokens": ["Auf", "die", "Wie\u00b7se", "geh", "ich", "hin\u00b7aus", ",", "wo", "das", "Gras", "aus", "der", "Wur\u00b7zel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Frisch, wie die Quelle, mir keimt, wo die liebliche Lippe der Blume", "tokens": ["Frisch", ",", "wie", "die", "Quel\u00b7le", ",", "mir", "keimt", ",", "wo", "die", "lieb\u00b7li\u00b7che", "Lip\u00b7pe", "der", "Blu\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Mir sich \u00f6ffnet und stumm mit s\u00fc\u00dfem Othem mich anhaucht,", "tokens": ["Mir", "sich", "\u00f6ff\u00b7net", "und", "stumm", "mit", "s\u00fc\u00b7\u00dfem", "O\u00b7them", "mich", "an\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "KON", "ADJD", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und an tausend Zweigen des Hains, wie an brennenden Kerzen", "tokens": ["Und", "an", "tau\u00b7send", "Zwei\u00b7gen", "des", "Hains", ",", "wie", "an", "bren\u00b7nen\u00b7den", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "ART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mir das Fl\u00e4mmchen des Lebens gl\u00e4nzt, die r\u00f6tliche Bl\u00fcte,", "tokens": ["Mir", "das", "Fl\u00e4mm\u00b7chen", "des", "Le\u00b7bens", "gl\u00e4nzt", ",", "die", "r\u00f6t\u00b7li\u00b7che", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Wo im sonnigen Quell die zufriednen Fische sich regen,", "tokens": ["Wo", "im", "son\u00b7ni\u00b7gen", "Quell", "die", "zu\u00b7fried\u00b7nen", "Fi\u00b7sche", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Wo die Schwalbe das Nest mit den t\u00f6rigen Jungen umflattert,", "tokens": ["Wo", "die", "Schwal\u00b7be", "das", "Nest", "mit", "den", "t\u00f6\u00b7ri\u00b7gen", "Jun\u00b7gen", "um\u00b7flat\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Und die Schmetterlinge sich freun und die Bienen, da wandl ich", "tokens": ["Und", "die", "Schmet\u00b7ter\u00b7lin\u00b7ge", "sich", "freun", "und", "die", "Bie\u00b7nen", ",", "da", "wandl", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "KON", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Mitten in ihrer Lust; ich steh im friedlichen Felde", "tokens": ["Mit\u00b7ten", "in", "ih\u00b7rer", "Lust", ";", "ich", "steh", "im", "fried\u00b7li\u00b7chen", "Fel\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Wie ein liebender Ulmbaum da, und wie Reben und Trauben", "tokens": ["Wie", "ein", "lie\u00b7ben\u00b7der", "Ulm\u00b7baum", "da", ",", "und", "wie", "Re\u00b7ben", "und", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "PWAV", "NN", "KON", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Schlingen sich rund um mich die s\u00fc\u00dfen Spiele des Lebens.", "tokens": ["Schlin\u00b7gen", "sich", "rund", "um", "mich", "die", "s\u00fc\u00b7\u00dfen", "Spie\u00b7le", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PRF", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Oder schau ich hinauf zum Berge, der mit Gew\u00f6lken", "tokens": ["O\u00b7der", "schau", "ich", "hin\u00b7auf", "zum", "Ber\u00b7ge", ",", "der", "mit", "Ge\u00b7w\u00f6l\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Sich die Scheitel umkr\u00e4nzt und die d\u00fcstern Locken im Winde", "tokens": ["Sich", "die", "Schei\u00b7tel", "um\u00b7kr\u00e4nzt", "und", "die", "d\u00fcs\u00b7tern", "Lo\u00b7cken", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVPP", "KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sch\u00fcttelt, und wenn er mich tr\u00e4gt auf seiner kr\u00e4ftigen Schulter,", "tokens": ["Sch\u00fct\u00b7telt", ",", "und", "wenn", "er", "mich", "tr\u00e4gt", "auf", "sei\u00b7ner", "kr\u00e4f\u00b7ti\u00b7gen", "Schul\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wenn die leichtere Luft mir alle Sinne bezaubert", "tokens": ["Wenn", "die", "leich\u00b7te\u00b7re", "Luft", "mir", "al\u00b7le", "Sin\u00b7ne", "be\u00b7zau\u00b7bert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PIAT", "NN", "VVPP"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und das unendliche Tal, wie eine farbige Wolke,", "tokens": ["Und", "das", "un\u00b7end\u00b7li\u00b7che", "Tal", ",", "wie", "ei\u00b7ne", "far\u00b7bi\u00b7ge", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Unter mir liegt, da werd ich zum Adler, und ledig des Bodens", "tokens": ["Un\u00b7ter", "mir", "liegt", ",", "da", "werd", "ich", "zum", "Ad\u00b7ler", ",", "und", "le\u00b7dig", "des", "Bo\u00b7dens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$,", "KON", "ADJD", "ART", "NN"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.7": {"text": "Wechselt mein Leben im All der Natur wie Nomaden den Wohnort.", "tokens": ["Wech\u00b7selt", "mein", "Le\u00b7ben", "im", "All", "der", "Na\u00b7tur", "wie", "No\u00b7ma\u00b7den", "den", "Wohn\u00b7ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "KOKOM", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Und nun f\u00fchrt mich der Pfad zur\u00fcck ins Leben der Menschen,", "tokens": ["Und", "nun", "f\u00fchrt", "mich", "der", "Pfad", "zu\u00b7r\u00fcck", "ins", "Le\u00b7ben", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Fernher d\u00e4mmert die Stadt, wie eine eherne R\u00fcstung", "tokens": ["Fern\u00b7her", "d\u00e4m\u00b7mert", "die", "Stadt", ",", "wie", "ei\u00b7ne", "e\u00b7her\u00b7ne", "R\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Gegen die Macht des Gewittergotts und der Menschen geschmiedet,", "tokens": ["Ge\u00b7gen", "die", "Macht", "des", "Ge\u00b7wit\u00b7ter\u00b7gotts", "und", "der", "Men\u00b7schen", "ge\u00b7schmie\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.11": {"text": "Majest\u00e4tisch herauf, und ringsum ruhen die D\u00f6rfchen;", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "her\u00b7auf", ",", "und", "ring\u00b7sum", "ru\u00b7hen", "die", "D\u00f6rf\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und die D\u00e4cher umh\u00fcllt, vom Abendlichte ger\u00f6tet,", "tokens": ["Und", "die", "D\u00e4\u00b7cher", "um\u00b7h\u00fcllt", ",", "vom", "A\u00b7bend\u00b7lich\u00b7te", "ge\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Freundlich der h\u00e4usliche Rauch; es ruhn die sorglich umz\u00e4unten", "tokens": ["Freund\u00b7lich", "der", "h\u00e4us\u00b7li\u00b7che", "Rauch", ";", "es", "ruhn", "die", "sorg\u00b7lich", "um\u00b7z\u00e4un\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJD", "VVIZU"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "G\u00e4rten, es schlummert der Pflug auf den gesonderten Feldern.", "tokens": ["G\u00e4r\u00b7ten", ",", "es", "schlum\u00b7mert", "der", "Pflug", "auf", "den", "ge\u00b7son\u00b7der\u00b7ten", "Fel\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Aber ins Mondlicht steigen herauf die zerbrochenen S\u00e4ulen", "tokens": ["A\u00b7ber", "ins", "Mond\u00b7licht", "stei\u00b7gen", "her\u00b7auf", "die", "zer\u00b7bro\u00b7che\u00b7nen", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und die Tempeltore, die einst der Furchtbare traf, der geheime", "tokens": ["Und", "die", "Tem\u00b7pel\u00b7to\u00b7re", ",", "die", "einst", "der", "Furcht\u00b7ba\u00b7re", "traf", ",", "der", "ge\u00b7hei\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "--+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Geist der Unruh, der in der Brust der Erd und der Menschen", "tokens": ["Geist", "der", "Un\u00b7ruh", ",", "der", "in", "der", "Brust", "der", "Erd", "und", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Z\u00fcrnet und g\u00e4rt, der Unbezwungne, der alte Erobrer,", "tokens": ["Z\u00fcr\u00b7net", "und", "g\u00e4rt", ",", "der", "Un\u00b7be\u00b7zwung\u00b7ne", ",", "der", "al\u00b7te", "E\u00b7rob\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Der die St\u00e4dte, wie L\u00e4mmer, zerrei\u00dft, der einst den Olympus", "tokens": ["Der", "die", "St\u00e4d\u00b7te", ",", "wie", "L\u00e4m\u00b7mer", ",", "zer\u00b7rei\u00dft", ",", "der", "einst", "den", "O\u00b7lym\u00b7pus"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "$,", "PWAV", "NN", "$,", "VVFIN", "$,", "PRELS", "ADV", "ART", "NE"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "St\u00fcrmte, der in den Bergen sich regt, und Flammen herauswirft,", "tokens": ["St\u00fcrm\u00b7te", ",", "der", "in", "den", "Ber\u00b7gen", "sich", "regt", ",", "und", "Flam\u00b7men", "her\u00b7aus\u00b7wirft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+---+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Der die W\u00e4lder entwurzelt und durch den Ozean hinf\u00e4hrt", "tokens": ["Der", "die", "W\u00e4l\u00b7der", "ent\u00b7wur\u00b7zelt", "und", "durch", "den", "O\u00b7ze\u00b7an", "hin\u00b7f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVPP", "KON", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+++-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.8": {"text": "Und die Schiffe zerschl\u00e4gt und doch in der ewigen Ordnung", "tokens": ["Und", "die", "Schif\u00b7fe", "zer\u00b7schl\u00e4gt", "und", "doch", "in", "der", "e\u00b7wi\u00b7gen", "Ord\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Niemals irre dich macht, auf der Tafel deiner Gesetze", "tokens": ["Nie\u00b7mals", "ir\u00b7re", "dich", "macht", ",", "auf", "der", "Ta\u00b7fel", "dei\u00b7ner", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Keine Silbe verwischt, der auch dein Sohn, o Natur, ist,", "tokens": ["Kei\u00b7ne", "Sil\u00b7be", "ver\u00b7wischt", ",", "der", "auch", "dein", "Sohn", ",", "o", "Na\u00b7tur", ",", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PRELS", "ADV", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VAFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Mit dem Geiste der Ruh aus ", "tokens": ["Mit", "dem", "Geis\u00b7te", "der", "Ruh", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ"], "meter": "+-+-+++", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Hab ich zu Hause dann, wo die B\u00e4ume das Fenster ums\u00e4useln", "tokens": ["Hab", "ich", "zu", "Hau\u00b7se", "dann", ",", "wo", "die", "