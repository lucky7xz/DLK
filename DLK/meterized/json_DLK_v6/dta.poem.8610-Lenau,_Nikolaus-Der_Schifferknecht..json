{"dta.poem.8610": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Schifferknecht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Am Boden auf dem Rohrgeflecht,", "tokens": ["Am", "Bo\u00b7den", "auf", "dem", "Rohr\u00b7ge\u00b7flecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom harten Gl\u00fcck verstossen,", "tokens": ["Vom", "har\u00b7ten", "Gl\u00fcck", "ver\u00b7stos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ruht der arme Schifferknecht", "tokens": ["Da", "ruht", "der", "ar\u00b7me", "Schif\u00b7fer\u00b7knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinen m\u00fcden Rossen.", "tokens": ["Mit", "sei\u00b7nen", "m\u00fc\u00b7den", "Ros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er haust bei Tag und Nacht am Strand,", "tokens": ["Er", "haust", "bei", "Tag", "und", "Nacht", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herd- und H\u00fcttenlose,", "tokens": ["Der", "Herd", "und", "H\u00fct\u00b7ten\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihm gedeiht im Ufersand", "tokens": ["Und", "ihm", "ge\u00b7deiht", "im", "U\u00b7fer\u00b7sand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl keine Freudenrose.", "tokens": ["Wohl", "kei\u00b7ne", "Freu\u00b7den\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Nacht ist k\u00fchl, es braust der Wind,", "tokens": ["Die", "Nacht", "ist", "k\u00fchl", ",", "es", "braust", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Still blickt der Mond hernieder;", "tokens": ["Still", "blickt", "der", "Mond", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Donau murmelt ihrem Kind", "tokens": ["Die", "Do\u00b7nau", "mur\u00b7melt", "ih\u00b7rem", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewohnte Schlummerlieder.", "tokens": ["Ge\u00b7wohn\u00b7te", "Schlum\u00b7mer\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sein Schlaf ist s\u00fc\u00df, er schl\u00fcrft ihn ein", "tokens": ["Sein", "Schlaf", "ist", "s\u00fc\u00df", ",", "er", "schl\u00fcrft", "ihn", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In starken, tiefen Z\u00fcgen,", "tokens": ["In", "star\u00b7ken", ",", "tie\u00b7fen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Berauschet ihn, ihr Fantasei'n,", "tokens": ["Be\u00b7rau\u00b7schet", "ihn", ",", "ihr", "Fan\u00b7ta\u00b7sei'n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus euren Zauberkr\u00fcgen.", "tokens": ["Aus", "eu\u00b7ren", "Zau\u00b7ber\u00b7kr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00dft wandeln ihn am Wiesenhang", "tokens": ["La\u00dft", "wan\u00b7deln", "ihn", "am", "Wie\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im goldnen Morgenscheine,", "tokens": ["Im", "gold\u00b7nen", "Mor\u00b7gen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihm ert\u00f6ne Vogelsang", "tokens": ["Und", "ihm", "er\u00b7t\u00f6\u00b7ne", "Vo\u00b7gel\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im aufgebl\u00fchten Haine.", "tokens": ["Im", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gebt ihm ein H\u00e4uschen still und traut,", "tokens": ["Gebt", "ihm", "ein", "H\u00e4usc\u00b7hen", "still", "und", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umrankt von gr\u00fcnen B\u00e4umen,", "tokens": ["Um\u00b7rankt", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und eine sch\u00f6ne junge Braut,", "tokens": ["Und", "ei\u00b7ne", "sch\u00f6\u00b7ne", "jun\u00b7ge", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebt ihm in seinen Tr\u00e4umen!", "tokens": ["Gebt", "ihm", "in", "sei\u00b7nen", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Beim H\u00fcttchen auf der Abendbank", "tokens": ["Beim", "H\u00fctt\u00b7chen", "auf", "der", "A\u00b7bend\u00b7bank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sitzen selig beide,", "tokens": ["Da", "sit\u00b7zen", "se\u00b7lig", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heimkehrt mit frohem Glockenklang", "tokens": ["Heim\u00b7kehrt", "mit", "fro\u00b7hem", "Glo\u00b7cken\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Heerde von der Weide.", "tokens": ["Die", "Heer\u00b7de", "von", "der", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun h\u00f6rt er nicht der Pferde Huf,", "tokens": ["Nun", "h\u00f6rt", "er", "nicht", "der", "Pfer\u00b7de", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht die Gei\u00dfel knallen,", "tokens": ["Und", "nicht", "die", "Gei\u00b7\u00dfel", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt nicht der Schiffer langen Ruf", "tokens": ["H\u00f6rt", "nicht", "der", "Schif\u00b7fer", "lan\u00b7gen", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im fernen Wald verhallen.", "tokens": ["Im", "fer\u00b7nen", "Wald", "ver\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er sieht nicht, wie vom Strand hinab", "tokens": ["Er", "sieht", "nicht", ",", "wie", "vom", "Strand", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den armen Kameraden", "tokens": ["Den", "ar\u00b7men", "Ka\u00b7me\u00b7ra\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sammt seinem Ro\u00df in's Wellengrab", "tokens": ["Sammt", "sei\u00b7nem", "Ro\u00df", "in's", "Wel\u00b7len\u00b7grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fortrei\u00dft der arge Faden. ", "tokens": ["For\u00b7trei\u00dft", "der", "ar\u00b7ge", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}