{"dta.poem.8927": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Lx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.85", "af:0.14"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Ruf' ich zu mir die kaum beseelten Dinge:", "tokens": ["Ruf'", "ich", "zu", "mir", "die", "kaum", "be\u00b7seel\u00b7ten", "Din\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr V\u00f6gel kommt, o kommt ihr Schmetterlinge,", "tokens": ["Ihr", "V\u00f6\u00b7gel", "kommt", ",", "o", "kommt", "ihr", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "FM", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bef\u00fcrchtet nichts, und glaubt an meine Treue!", "tokens": ["Be\u00b7f\u00fcrch\u00b7tet", "nichts", ",", "und", "glaubt", "an", "mei\u00b7ne", "Treu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich verr\u00e4therische Kost euch streue,", "tokens": ["Da\u00df", "ich", "ver\u00b7r\u00e4\u00b7the\u00b7ri\u00b7sche", "Kost", "euch", "streu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O w\u00e4hnt es nicht! Ich lege keine Schlinge,", "tokens": ["O", "w\u00e4hnt", "es", "nicht", "!", "Ich", "le\u00b7ge", "kei\u00b7ne", "Schlin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ich die Zeit, den Menschen fern, verbringe,", "tokens": ["Der", "ich", "die", "Zeit", ",", "den", "Men\u00b7schen", "fern", ",", "ver\u00b7brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ich, noch mehr als ihr, die Menschen scheue!", "tokens": ["Der", "ich", ",", "noch", "mehr", "als", "ihr", ",", "die", "Men\u00b7schen", "scheu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "ADV", "KOUS", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O z\u00e4hlt mich nicht zu jenen rohen Horden,", "tokens": ["O", "z\u00e4hlt", "mich", "nicht", "zu", "je\u00b7nen", "ro\u00b7hen", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich, der ich Andern nie gesucht zu schaden,", "tokens": ["Mich", ",", "der", "ich", "An\u00b7dern", "nie", "ge\u00b7sucht", "zu", "scha\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJA", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von den Menschen stets vermieden worden!", "tokens": ["Und", "von", "den", "Men\u00b7schen", "stets", "ver\u00b7mie\u00b7den", "wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00dft d'rum uns fliehn vor allen ihren Pfaden:", "tokens": ["La\u00dft", "d'\u00b7rum", "uns", "fliehn", "vor", "al\u00b7len", "ih\u00b7ren", "Pfa\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Euch streben sie zu haschen und zu morden,", "tokens": ["Euch", "stre\u00b7ben", "sie", "zu", "ha\u00b7schen", "und", "zu", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich haben sie mit ihrem Gram beladen.", "tokens": ["Mich", "ha\u00b7ben", "sie", "mit", "ih\u00b7rem", "Gram", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}