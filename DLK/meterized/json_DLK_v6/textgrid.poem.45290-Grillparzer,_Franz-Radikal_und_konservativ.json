{"textgrid.poem.45290": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Radikal und konservativ", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Unterschied beider Parteigebilde", "tokens": ["Der", "Un\u00b7ter\u00b7schied", "bei\u00b7der", "Par\u00b7tei\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist wert nicht, da\u00df man ein Wort verliere;", "tokens": ["Ist", "wert", "nicht", ",", "da\u00df", "man", "ein", "Wort", "ver\u00b7lie\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Es sind nun eben: die einen wilde,", "tokens": ["Es", "sind", "nun", "e\u00b7ben", ":", "die", "ei\u00b7nen", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$.", "ART", "ART", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die andern dagegen zahme Tiere.", "tokens": ["Die", "an\u00b7dern", "da\u00b7ge\u00b7gen", "zah\u00b7me", "Tie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PAV", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}