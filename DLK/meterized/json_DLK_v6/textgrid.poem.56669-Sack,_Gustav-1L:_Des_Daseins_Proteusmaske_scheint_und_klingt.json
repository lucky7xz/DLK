{"textgrid.poem.56669": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Daseins Proteusmaske scheint und klingt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Daseins Proteusmaske scheint und klingt", "tokens": ["Des", "Da\u00b7seins", "Pro\u00b7teus\u00b7mas\u00b7ke", "scheint", "und", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und mag dem Kind als Wirklichkeit gen\u00fcgen,", "tokens": ["und", "mag", "dem", "Kind", "als", "Wirk\u00b7lich\u00b7keit", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "es wird zu Lust und Tr\u00e4nen blind sich f\u00fcgen,", "tokens": ["es", "wird", "zu", "Lust", "und", "Tr\u00e4\u00b7nen", "blind", "sich", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "je wie der Popanz ihm entgegen springt.", "tokens": ["je", "wie", "der", "Po\u00b7panz", "ihm", "ent\u00b7ge\u00b7gen", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du m\u00f6chtest ihn enth\u00fcllen \u2013 ach! es dringt", "tokens": ["Du", "m\u00f6ch\u00b7test", "ihn", "ent\u00b7h\u00fcl\u00b7len", "\u2013", "ach", "!", "es", "dringt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$(", "ITJ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "kein Blick durch diese schillernd bunten L\u00fcgen", "tokens": ["kein", "Blick", "durch", "die\u00b7se", "schil\u00b7lernd", "bun\u00b7ten", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PDAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zu dem, der mit geheimnisvollen Pfl\u00fcgen", "tokens": ["zu", "dem", ",", "der", "mit", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "Pfl\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Chaos in die Kosmosmaske zwingt.", "tokens": ["das", "Chaos", "in", "die", "Kos\u00b7mos\u00b7mas\u00b7ke", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nenn ihn das Furchtbare und deine Welt", "tokens": ["Nenn", "ihn", "das", "Furcht\u00b7ba\u00b7re", "und", "dei\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sein Maskenkleid und bleibe dir bewu\u00dft,", "tokens": ["sein", "Mas\u00b7ken\u00b7kleid", "und", "blei\u00b7be", "dir", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df jede Maske k\u00e4uflich ist f\u00fcr Geld \u2013", "tokens": ["da\u00df", "je\u00b7de", "Mas\u00b7ke", "k\u00e4uf\u00b7lich", "ist", "f\u00fcr", "Geld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "und diesen Glauben, h\u00f6rst du Glauben, mu\u00dft", "tokens": ["und", "die\u00b7sen", "Glau\u00b7ben", ",", "h\u00f6rst", "du", "Glau\u00b7ben", ",", "mu\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PDAT", "NN", "$,", "VVFIN", "PPER", "NN", "$,", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als Sprungbrett du betrachten, das dich schnellt", "tokens": ["als", "Sprung\u00b7brett", "du", "be\u00b7trach\u00b7ten", ",", "das", "dich", "schnellt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVINF", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu aller Deutung grenzenloser Lust.", "tokens": ["zu", "al\u00b7ler", "Deu\u00b7tung", "gren\u00b7zen\u00b7lo\u00b7ser", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}