{"textgrid.poem.60184": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer sich mit w\u00e4chsernen Fl\u00fcgeln", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich mit w\u00e4chsernen Fl\u00fcgeln", "tokens": ["Wer", "sich", "mit", "w\u00e4ch\u00b7ser\u00b7nen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wagt zu pindarischem Flug,", "tokens": ["Wagt", "zu", "pin\u00b7da\u00b7ri\u00b7schem", "Flug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der bringt unsterblichen Nahmen", "tokens": ["Der", "bringt", "uns\u00b7terb\u00b7li\u00b7chen", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Meer, in welches er st\u00fcrzt.", "tokens": ["Dem", "Meer", ",", "in", "wel\u00b7ches", "er", "st\u00fcrzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "So wie vom Brocken herabrauscht", "tokens": ["So", "wie", "vom", "Bro\u00b7cken", "her\u00b7a\u00b7brauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der aufgeschwollene Flu\u00df,", "tokens": ["Der", "auf\u00b7ge\u00b7schwol\u00b7le\u00b7ne", "Flu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn Wassertragende Wolken", "tokens": ["Wenn", "Was\u00b7ser\u00b7tra\u00b7gen\u00b7de", "Wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herunter st\u00fcrzen auf ihn.", "tokens": ["Her\u00b7un\u00b7ter", "st\u00fcr\u00b7zen", "auf", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "So rauscht vom Munde des Pindars", "tokens": ["So", "rauscht", "vom", "Mun\u00b7de", "des", "Pin\u00b7dars"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unwiederstehlich herab", "tokens": ["Un\u00b7wie\u00b7ders\u00b7teh\u00b7lich", "her\u00b7ab"], "token_info": ["word", "word"], "pos": ["ADJD", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gesang des Dichters, der immer", "tokens": ["Ge\u00b7sang", "des", "Dich\u00b7ters", ",", "der", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verdient apollischen Cranz.", "tokens": ["Ver\u00b7di\u00b7ent", "a\u00b7pol\u00b7li\u00b7schen", "Cranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ADJA", "NN", "$."], "meter": "++--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Er mag in fliegender Ode,", "tokens": ["Er", "mag", "in", "flie\u00b7gen\u00b7der", "O\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit neuen Worten erf\u00fcllt,", "tokens": ["Mit", "neu\u00b7en", "Wor\u00b7ten", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Stark th\u00f6nen oder sanft fliessen", "tokens": ["Stark", "th\u00f6\u00b7nen", "o\u00b7der", "sanft", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "ADJD", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In ungezwungenem Lied;", "tokens": ["In", "un\u00b7ge\u00b7zwun\u00b7ge\u00b7nem", "Lied", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Er singe von dem Olympus", "tokens": ["Er", "sin\u00b7ge", "von", "dem", "O\u00b7lym\u00b7pus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ewig herrschender Macht", "tokens": ["Von", "e\u00b7wig", "herr\u00b7schen\u00b7der", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der G\u00f6tter, oder er preise", "tokens": ["Der", "G\u00f6t\u00b7ter", ",", "o\u00b7der", "er", "prei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPER", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Thronensitzer der Welt;", "tokens": ["Die", "Thro\u00b7nen\u00b7sit\u00b7zer", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Den Held, die siegende R\u00fcckkunft", "tokens": ["Den", "Held", ",", "die", "sie\u00b7gen\u00b7de", "R\u00fcck\u00b7kunft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Von dem olympischen Spiel;", "tokens": ["Von", "dem", "o\u00b7lym\u00b7pi\u00b7schen", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den wagenlenkenden J\u00fcngling", "tokens": ["Den", "wa\u00b7gen\u00b7len\u00b7ken\u00b7den", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und das wettlaufende Ro\u00df;", "tokens": ["Und", "das", "wett\u00b7lau\u00b7fen\u00b7de", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Er sing in klagender Stimme", "tokens": ["Er", "sing", "in", "kla\u00b7gen\u00b7der", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Schmerz der \u00e4chzenden Braut,", "tokens": ["Den", "Schmerz", "der", "\u00e4ch\u00b7zen\u00b7den", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der ihr Verlobter entrissen", "tokens": ["Der", "ihr", "Ver\u00b7lob\u00b7ter", "ent\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ward, in erschr\u00f6cklicher Schlacht;", "tokens": ["Ward", ",", "in", "er\u00b7schr\u00f6ck\u00b7li\u00b7cher", "Schlacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Er reisse goldene Sitten", "tokens": ["Er", "reis\u00b7se", "gol\u00b7de\u00b7ne", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus der Vergessenheit Nacht,", "tokens": ["Aus", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und f\u00fchr zu gl\u00e4nzenden Sternen", "tokens": ["Und", "f\u00fchr", "zu", "gl\u00e4n\u00b7zen\u00b7den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den L\u00f6wengleichenden Muth;", "tokens": ["Den", "L\u00f6\u00b7wen\u00b7glei\u00b7chen\u00b7den", "Muth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "So bleibt er immer der hohe", "tokens": ["So", "bleibt", "er", "im\u00b7mer", "der", "ho\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ganz unnachahmliche Schwan,", "tokens": ["Ganz", "un\u00b7nach\u00b7ahm\u00b7li\u00b7che", "Schwan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den zu den Z\u00fcgen der Wolken", "tokens": ["Den", "zu", "den", "Z\u00fc\u00b7gen", "der", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hebt, eine st\u00e4rkere Luft.", "tokens": ["Hebt", ",", "ei\u00b7ne", "st\u00e4r\u00b7ke\u00b7re", "Luft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Ich gleich der summenden Biene,", "tokens": ["Ich", "gleich", "der", "sum\u00b7men\u00b7den", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die saugt an bl\u00fchendem Klee,", "tokens": ["Die", "saugt", "an", "bl\u00fc\u00b7hen\u00b7dem", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich sinn' am Ufer der Elbe,", "tokens": ["Ich", "sinn'", "am", "U\u00b7fer", "der", "El\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf mein zu niedriges Lied.", "tokens": ["Auf", "mein", "zu", "nied\u00b7ri\u00b7ges", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Ich r\u00fchre Saphische Sayten", "tokens": ["Ich", "r\u00fch\u00b7re", "Sa\u00b7phi\u00b7sche", "Say\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit ungeregeltem Griff;", "tokens": ["Mit", "un\u00b7ge\u00b7re\u00b7gel\u00b7tem", "Griff", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mir fehlt zum Heldengesange", "tokens": ["Mir", "fehlt", "zum", "Hel\u00b7den\u00b7ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gluth und ein m\u00e4nnlicher Schwung.", "tokens": ["Gluth", "und", "ein", "m\u00e4nn\u00b7li\u00b7cher", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Dir aber, welcher dem Flaccus", "tokens": ["Dir", "a\u00b7ber", ",", "wel\u00b7cher", "dem", "Flac\u00b7cus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nachfolgt, dir fodert Gesang", "tokens": ["Nach\u00b7folgt", ",", "dir", "fo\u00b7dert", "Ge\u00b7sang"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Der Sieger, wann er geschm\u00fccket,", "tokens": ["Der", "Sie\u00b7ger", ",", "wann", "er", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mit wohlverdienetem Cranz", "tokens": ["Mit", "wohl\u00b7ver\u00b7die\u00b7ne\u00b7tem", "Cranz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "F\u00e4hrt durch das Menschengedr\u00e4nge", "tokens": ["F\u00e4hrt", "durch", "das", "Men\u00b7schen\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der gro\u00dfpall\u00e4stigen Stadt,", "tokens": ["Der", "gro\u00df\u00b7pal\u00b7l\u00e4s\u00b7ti\u00b7gen", "Stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und an dem Wagen geheftet", "tokens": ["Und", "an", "dem", "Wa\u00b7gen", "ge\u00b7hef\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fchrt den geb\u00e4ndigten Stolz", "tokens": ["F\u00fchrt", "den", "ge\u00b7b\u00e4n\u00b7dig\u00b7ten", "Stolz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Des Feindes, welcher den K\u00f6nig", "tokens": ["Des", "Fein\u00b7des", ",", "wel\u00b7cher", "den", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns lange Jahre hindurch", "tokens": ["Uns", "lan\u00b7ge", "Jah\u00b7re", "hin\u00b7durch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "PAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Entzog, und Tage voll Schrecken", "tokens": ["Ent\u00b7zog", ",", "und", "Ta\u00b7ge", "voll", "Schre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "NN", "ADJD", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Dem Vaterlande gebracht!", "tokens": ["Dem", "Va\u00b7ter\u00b7lan\u00b7de", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Dann sing uns festliche Spiele", "tokens": ["Dann", "sing", "uns", "fest\u00b7li\u00b7che", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der hoch aufh\u00fcpfenden Stadt,", "tokens": ["Der", "hoch", "auf\u00b7h\u00fcp\u00b7fen\u00b7den", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und der Gerechtigkeit S\u00e4\u00e4le", "tokens": ["Und", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "S\u00e4\u00b7\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht voll vom Kl\u00e4gertumult,", "tokens": ["Nicht", "voll", "vom", "Kl\u00e4\u00b7ger\u00b7tu\u00b7mult", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Und wiederlebende Freuden", "tokens": ["Und", "wie\u00b7der\u00b7le\u00b7ben\u00b7de", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ins neugesittete Volk", "tokens": ["Ins", "neu\u00b7ge\u00b7sit\u00b7te\u00b7te", "Volk"], "token_info"