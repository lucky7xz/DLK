{"textgrid.poem.51120": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Er\u00f6ffnungslied am eidgen\u00f6ssischen S\u00e4ngerfest", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben hoch im Bergrevier", "tokens": ["Wir", "ha\u00b7ben", "hoch", "im", "Berg\u00b7re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tannenwald gehauen,", "tokens": ["Den", "Tan\u00b7nen\u00b7wald", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Draus euch in rot und wei\u00dfer Zier", "tokens": ["Draus", "euch", "in", "rot", "und", "wei\u00b7\u00dfer", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wanderzelt zu bauen.", "tokens": ["Das", "Wan\u00b7der\u00b7zelt", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herein, was nun die Halle fa\u00dft,", "tokens": ["Her\u00b7ein", ",", "was", "nun", "die", "Hal\u00b7le", "fa\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Schweizerkind! du deutscher Gast!", "tokens": ["O", "Schwei\u00b7zer\u00b7kind", "!", "du", "deut\u00b7scher", "Gast", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie's im Bergwald k\u00fchn erklang,", "tokens": ["Und", "wie's", "im", "Berg\u00b7wald", "k\u00fchn", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00dft rauschen hier den M\u00e4nnersang!", "tokens": ["La\u00dft", "rau\u00b7schen", "hier", "den", "M\u00e4n\u00b7ner\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die grauen Adler schrieen wild,", "tokens": ["Die", "grau\u00b7en", "Ad\u00b7ler", "schri\u00b7een", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit wir zuletzt gesungen;", "tokens": ["Seit", "wir", "zu\u00b7letzt", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist der Freiheit Silberschild", "tokens": ["Da", "ist", "der", "Frei\u00b7heit", "Sil\u00b7ber\u00b7schild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar hell und rein erklungen!", "tokens": ["Gar", "hell", "und", "rein", "er\u00b7klun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir kehrten ein ins eigne Herz,", "tokens": ["Wir", "kehr\u00b7ten", "ein", "ins", "eig\u00b7ne", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da l\u00f6st' sich jeder Groll und Schmerz,", "tokens": ["Da", "l\u00f6st'", "sich", "je\u00b7der", "Groll", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da hatte sich die Treu gelohnt,", "tokens": ["Da", "hat\u00b7te", "sich", "die", "Treu", "ge\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Rat, der stet im Manne wohnt.", "tokens": ["Der", "Rat", ",", "der", "stet", "im", "Man\u00b7ne", "wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ward geraten, ward gebraut", "tokens": ["Es", "ward", "ge\u00b7ra\u00b7ten", ",", "ward", "ge\u00b7braut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf aller Herren Gassen;", "tokens": ["Auf", "al\u00b7ler", "Her\u00b7ren", "Gas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch jeder tat da, still wie laut,", "tokens": ["Doch", "je\u00b7der", "tat", "da", ",", "still", "wie", "laut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was er nicht konnte lassen!", "tokens": ["Was", "er", "nicht", "konn\u00b7te", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Mehrer seines Reichs zu sein,", "tokens": ["Ein", "Meh\u00b7rer", "sei\u00b7nes", "Reichs", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "D\u00fcnkt sich der F\u00fcrst im blut'gen Schein;", "tokens": ["D\u00fcnkt", "sich", "der", "F\u00fcrst", "im", "blut'\u00b7gen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir mehrten nur im Heimatland", "tokens": ["Wir", "mehr\u00b7ten", "nur", "im", "Hei\u00b7mat\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Menschenwert mit reiner Hand!", "tokens": ["Den", "Men\u00b7schen\u00b7wert", "mit", "rei\u00b7ner", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Erhebt die Stimmen froh und hell!", "tokens": ["Er\u00b7hebt", "die", "Stim\u00b7men", "froh", "und", "hell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ringt um des Preises Schale!", "tokens": ["Ringt", "um", "des", "Prei\u00b7ses", "Scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann setzt euch an den Purpurquell,", "tokens": ["Dann", "setzt", "euch", "an", "den", "Pur\u00b7pur\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Singt abermals beim Mahle!", "tokens": ["Singt", "a\u00b7ber\u00b7mals", "beim", "Mah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und singt: Das Land ist eben recht,", "tokens": ["Und", "singt", ":", "Das", "Land", "ist", "e\u00b7ben", "recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist nicht zu gut und nicht zu schlecht,", "tokens": ["Ist", "nicht", "zu", "gut", "und", "nicht", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist nicht zu gro\u00df und nicht zu klein,", "tokens": ["Ist", "nicht", "zu", "gro\u00df", "und", "nicht", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um drin ein freier Mann zu sein!", "tokens": ["Um", "drin", "ein", "frei\u00b7er", "Mann", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie gr\u00fcne Aun im Firnenschnee", "tokens": ["Wie", "gr\u00fc\u00b7ne", "Aun", "im", "Fir\u00b7nen\u00b7schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In alter Zeit verschwunden,", "tokens": ["In", "al\u00b7ter", "Zeit", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So hat noch jedes Volk das Weh", "tokens": ["So", "hat", "noch", "je\u00b7des", "Volk", "das", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Endes auch empfunden;", "tokens": ["Des", "En\u00b7des", "auch", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch trotzen wir dem Untergang", "tokens": ["Doch", "trot\u00b7zen", "wir", "dem", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch langehin mit Sang und Klang!", "tokens": ["Noch", "lan\u00b7ge\u00b7hin", "mit", "Sang", "und", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch halten wir aus eigner Hand", "tokens": ["Noch", "hal\u00b7ten", "wir", "aus", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich hoch empor, o Vaterland!", "tokens": ["Dich", "hoch", "em\u00b7por", ",", "o", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}