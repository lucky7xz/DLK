{"textgrid.poem.57892": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hatte die Fenster ge\u00f6ffnet,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte die Fenster ge\u00f6ffnet,", "tokens": ["Ich", "hat\u00b7te", "die", "Fens\u00b7ter", "ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es war dieser Tage zu hei\u00df,", "tokens": ["Es", "war", "die\u00b7ser", "Ta\u00b7ge", "zu", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da bimmelt es auf der Stra\u00dfe \u2013", "tokens": ["Da", "bim\u00b7melt", "es", "auf", "der", "Stra\u00b7\u00dfe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das ist der Mann mit dem Eis.", "tokens": ["Das", "ist", "der", "Mann", "mit", "dem", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der Mann, der den Kehricht abf\u00e4hrt,", "tokens": ["Der", "Mann", ",", "der", "den", "Keh\u00b7richt", "ab\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der bimmelt, was er kann,", "tokens": ["Der", "bim\u00b7melt", ",", "was", "er", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von morgens fr\u00fch bis abends,", "tokens": ["Von", "mor\u00b7gens", "fr\u00fch", "bis", "a\u00b7bends", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da bimmelt die Stra\u00dfenbahn.", "tokens": ["Da", "bim\u00b7melt", "die", "Stra\u00b7\u00dfen\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Molkereiwagen bimmeln,", "tokens": ["Die", "Mol\u00b7ke\u00b7rei\u00b7wa\u00b7gen", "bim\u00b7meln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es bimmelt die Feuerwehr,", "tokens": ["Es", "bim\u00b7melt", "die", "Feu\u00b7er\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Radfahrer kommen bimmelnd", "tokens": ["Rad\u00b7fah\u00b7rer", "kom\u00b7men", "bim\u00b7melnd"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In hellen Haufen daher.", "tokens": ["In", "hel\u00b7len", "Hau\u00b7fen", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Dies Bimmeln, ach, dies Bimmeln,", "tokens": ["Dies", "Bim\u00b7meln", ",", "ach", ",", "dies", "Bim\u00b7meln", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ITJ", "$,", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu sch\u00f6n ist doch dieser Brauch \u2013", "tokens": ["Zu", "sch\u00f6n", "ist", "doch", "die\u00b7ser", "Brauch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ADV", "PDAT", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ich glaube, in meinem Kopfe,", "tokens": ["Ich", "glau\u00b7be", ",", "in", "mei\u00b7nem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da bimmelt es n\u00e4chstens auch.", "tokens": ["Da", "bim\u00b7melt", "es", "n\u00e4chs\u00b7tens", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}