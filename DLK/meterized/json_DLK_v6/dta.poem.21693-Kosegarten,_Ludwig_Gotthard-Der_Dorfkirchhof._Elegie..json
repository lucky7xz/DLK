{"dta.poem.21693": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Dorfkirchhof.  \n Elegie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Grabe sinkt der abgeschiedne Tag,", "tokens": ["Zu", "Gra\u00b7be", "sinkt", "der", "ab\u00b7ge\u00b7schied\u00b7ne", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Heerden wanken bl\u00f6kend \u00fcbers Feld.", "tokens": ["Die", "Heer\u00b7den", "wan\u00b7ken", "bl\u00f6\u00b7kend", "\u00fc\u00b7bers", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der m\u00fcde Pfl\u00fcger sucht sein friedlich Dach,", "tokens": ["Der", "m\u00fc\u00b7de", "Pfl\u00fc\u00b7ger", "sucht", "sein", "fried\u00b7lich", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und r\u00e4umt der Dunkelheit und mir die Welt.", "tokens": ["Und", "r\u00e4umt", "der", "Dun\u00b7kel\u00b7heit", "und", "mir", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Landschaft d\u00e4mmert in dem Abendduft,", "tokens": ["Die", "Land\u00b7schaft", "d\u00e4m\u00b7mert", "in", "dem", "A\u00b7bend\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rings waltet Stille, die die Brust beklemmt.", "tokens": ["Rings", "wal\u00b7tet", "Stil\u00b7le", ",", "die", "die", "Brust", "be\u00b7klemmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur summst der K\u00e4fer brausend durch die Luft.", "tokens": ["Nur", "summst", "der", "K\u00e4\u00b7fer", "brau\u00b7send", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur pfeift der Junge, der die Pferde schwemmt.", "tokens": ["Nur", "pfeift", "der", "Jun\u00b7ge", ",", "der", "die", "Pfer\u00b7de", "schwemmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur heult die Eul' im alternden Gestein", "tokens": ["Nur", "heult", "die", "Eul'", "im", "al\u00b7tern\u00b7den", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und klagt ihr Leid dem mitbewussten Mond,", "tokens": ["Und", "klagt", "ihr", "Leid", "dem", "mit\u00b7be\u00b7wuss\u00b7ten", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn etwa Frevler den Bezirk entweihn,", "tokens": ["Wenn", "et\u00b7wa", "Frev\u00b7ler", "den", "Be\u00b7zirk", "ent\u00b7weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Den seit Jahrhunderten ihr Volk bewohnt.", "tokens": ["Den", "seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "ihr", "Volk", "be\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Hier, wo die Ulme mit dem Tax sich paart,", "tokens": ["Hier", ",", "wo", "die", "Ul\u00b7me", "mit", "dem", "Tax", "sich", "paart", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo gr\u00fcnbewachsen Sod' an Sode ragt,", "tokens": ["Wo", "gr\u00fcn\u00b7be\u00b7wach\u00b7sen", "Sod'", "an", "So\u00b7de", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier ruhn des Dorfes Ahnen wohlverwahrt,", "tokens": ["Hier", "ruhn", "des", "Dor\u00b7fes", "Ah\u00b7nen", "wohl\u00b7ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und keinem ward sein enges Haus versagt.", "tokens": ["Und", "kei\u00b7nem", "ward", "sein", "en\u00b7ges", "Haus", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der w\u00fcrzereichen Fr\u00fche frisches K\u00fchl,", "tokens": ["Der", "w\u00fcr\u00b7ze\u00b7rei\u00b7chen", "Fr\u00fc\u00b7he", "fri\u00b7sches", "K\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schwalbe Zwitschern, die den Gatten weckt,", "tokens": ["Der", "Schwal\u00b7be", "Zwit\u00b7schern", ",", "die", "den", "Gat\u00b7ten", "weckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Hiefhorns Schall, des Tages froh Gew\u00fchl", "tokens": ["Des", "Hief\u00b7horns", "Schall", ",", "des", "Ta\u00b7ges", "froh", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weckt nicht die Schl\u00e4fer, die der Rasen deckt.", "tokens": ["Weckt", "nicht", "die", "Schl\u00e4\u00b7fer", ",", "die", "der", "Ra\u00b7sen", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es flammt nicht mehr der traute Heerd f\u00fcr sie.", "tokens": ["Es", "flammt", "nicht", "mehr", "der", "trau\u00b7te", "Heerd", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein kosend Weib scheucht ihren \u00dcberdruss,", "tokens": ["Kein", "ko\u00b7send", "Weib", "scheucht", "ih\u00b7ren", "\u00dc\u00b7berd\u00b7russ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein schmeichelnd Kind erklimmt ihr schaukelnd Knie,", "tokens": ["Kein", "schmei\u00b7chelnd", "Kind", "er\u00b7klimmt", "ihr", "schau\u00b7kelnd", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bettelt um ein M\u00e4hrchen, einen Kuss.", "tokens": ["Und", "bet\u00b7telt", "um", "ein", "M\u00e4hr\u00b7chen", ",", "ei\u00b7nen", "Kuss", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}