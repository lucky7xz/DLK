{"textgrid.poem.50644": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man sagt, da\u00df Spurius an seinen Gott nicht denket;", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, da\u00df Spurius an seinen Gott nicht denket;", "tokens": ["Man", "sagt", ",", "da\u00df", "Spu\u00b7rius", "an", "sei\u00b7nen", "Gott", "nicht", "den\u00b7ket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NE", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch irrt man, weil er ihm sein ganzes Herz geschenket,", "tokens": ["Doch", "irrt", "man", ",", "weil", "er", "ihm", "sein", "gan\u00b7zes", "Herz", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil er ihn brunstig liebt und stets vor Augen h\u00e4lt. \u2013", "tokens": ["Weil", "er", "ihn", "bruns\u00b7tig", "liebt", "und", "stets", "vor", "Au\u00b7gen", "h\u00e4lt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "KON", "ADV", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Versteht mich aber recht: ", "tokens": ["Ver\u00b7steht", "mich", "a\u00b7ber", "recht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}