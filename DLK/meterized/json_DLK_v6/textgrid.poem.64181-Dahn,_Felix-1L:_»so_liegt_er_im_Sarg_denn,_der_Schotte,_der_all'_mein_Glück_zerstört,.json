{"textgrid.poem.64181": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbso liegt er im Sarg denn, der Schotte, der all' mein Gl\u00fcck zerst\u00f6rt,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbso liegt er im Sarg denn, der Schotte, der all' mein Gl\u00fcck zerst\u00f6rt,", "tokens": ["\u00bb", "so", "liegt", "er", "im", "Sarg", "denn", ",", "der", "Schot\u00b7te", ",", "der", "all'", "mein", "Gl\u00fcck", "zer\u00b7st\u00f6rt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "ART", "NN", "$,", "PRELS", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Dem sie Lady Maud gegeben, der einst mein Herz geh\u00f6rt!", "tokens": ["Dem", "sie", "La\u00b7dy", "Maud", "ge\u00b7ge\u00b7ben", ",", "der", "einst", "mein", "Herz", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "NE", "VVPP", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Das sind nun zwanzig Jahre! \u2013 Ich glaub', es geh\u00f6rt ihr noch \u2013:", "tokens": ["Das", "sind", "nun", "zwan\u00b7zig", "Jah\u00b7re", "!", "\u2013", "Ich", "glaub'", ",", "es", "ge\u00b7h\u00f6rt", "ihr", "noch", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "CARD", "NN", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$(", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denk' ich nur ihren Namen, erhebt's ein wild' Gepoch'.", "tokens": ["Denk'", "ich", "nur", "ih\u00b7ren", "Na\u00b7men", ",", "er\u00b7hebt's", "ein", "wild'", "Ge\u00b7poch'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer soll die Witwe sch\u00fctzen nun gegen meinen Zorn?", "tokens": ["Wer", "soll", "die", "Wit\u00b7we", "sch\u00fct\u00b7zen", "nun", "ge\u00b7gen", "mei\u00b7nen", "Zorn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihr Warwicks, auf, wir reiten! Sto\u00dft laut ins Fehdehorn!", "tokens": ["Ihr", "War\u00b7wicks", ",", "auf", ",", "wir", "rei\u00b7ten", "!", "Sto\u00dft", "laut", "ins", "Feh\u00b7de\u00b7horn", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$,", "PPER", "VVFIN", "$.", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr Marwoods und ihr Mordreds, ihr Mallets, sto\u00dft ins Horn!", "tokens": ["Ihr", "Mar\u00b7woods", "und", "ihr", "Mor\u00b7dreds", ",", "ihr", "Mal\u00b7lets", ",", "sto\u00dft", "ins", "Horn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Will wieder einmal traben durch die Buchen von Douglas-Borne!\u00ab", "tokens": ["Will", "wie\u00b7der", "ein\u00b7mal", "tra\u00b7ben", "durch", "die", "Bu\u00b7chen", "von", "Dou\u00b7glas\u00b7Bor\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "So rief der grimme Warwick, sein Graubart flog im Wind.", "tokens": ["So", "rief", "der", "grim\u00b7me", "War\u00b7wick", ",", "sein", "Grau\u00b7bart", "flog", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da sattelten seine Vettern, so viel' an dem Teviot sind.", "tokens": ["Da", "sat\u00b7tel\u00b7ten", "sei\u00b7ne", "Vet\u00b7tern", ",", "so", "viel'", "an", "dem", "Te\u00b7vi\u00b7ot", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PIS", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Marwoods wollten den Wildbann, die Mallets wollten den Zoll:", "tokens": ["Die", "Mar\u00b7woods", "woll\u00b7ten", "den", "Wild\u00b7bann", ",", "die", "Mal\u00b7lets", "woll\u00b7ten", "den", "Zoll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "ART", "NN", "$,", "ART", "NN", "VMFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der alte Warwick aber wollte k\u00fchlen seinen Groll.", "tokens": ["Der", "al\u00b7te", "War\u00b7wick", "a\u00b7ber", "woll\u00b7te", "k\u00fch\u00b7len", "sei\u00b7nen", "Groll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Weit zog voraus er allen; fort trug sein Grimm ihn stark.", "tokens": ["Weit", "zog", "vo\u00b7raus", "er", "al\u00b7len", ";", "fort", "trug", "sein", "Grimm", "ihn", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "PPER", "PIAT", "$.", "PTKVZ", "VVFIN", "PPOSAT", "NE", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Allein, bei Morgengrauen, ritt er \u00fcber die Schottenmark. \u2013", "tokens": ["Al\u00b7lein", ",", "bei", "Mor\u00b7gen\u00b7grau\u00b7en", ",", "ritt", "er", "\u00fc\u00b7ber", "die", "Schot\u00b7ten\u00b7mark", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Das war im fr\u00fchen Maien. \u2013 Aufstieg der junge Tag,", "tokens": ["Das", "war", "im", "fr\u00fc\u00b7hen", "Mai\u00b7en", ".", "\u2013", "Auf\u00b7stieg", "der", "jun\u00b7ge", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$.", "$(", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da sprengt' er in des toten Douglas Buchenhag. \u2013", "tokens": ["Da", "sprengt'", "er", "in", "des", "to\u00b7ten", "Doug\u00b7las", "Bu\u00b7chen\u00b7hag", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und als den Ungest\u00fcmen der stille Wald umfing,", "tokens": ["Und", "als", "den", "Un\u00b7ge\u00b7st\u00fc\u00b7men", "der", "stil\u00b7le", "Wald", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zog er gemach den Z\u00fcgel: \u2013 im Schritt der Rappe ging.", "tokens": ["Zog", "er", "ge\u00b7mach", "den", "Z\u00fc\u00b7gel", ":", "\u2013", "im", "Schritt", "der", "Rap\u00b7pe", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$(", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Und \u00fcber sich und um sich wie staunend sah der Mann", "tokens": ["Und", "\u00fc\u00b7ber", "sich", "und", "um", "sich", "wie", "stau\u00b7nend", "sah", "der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "KON", "APPR", "PRF", "KOKOM", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und strich sich unterm Helmdach die Brauen und hob an:", "tokens": ["Und", "strich", "sich", "un\u00b7term", "Helm\u00b7dach", "die", "Brau\u00b7en", "und", "hob", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbwie gl\u00e4nzt das junge Buchlaub lichtgr\u00fcn im Morgenstrahl!", "tokens": ["\u00bb", "wie", "gl\u00e4nzt", "das", "jun\u00b7ge", "Buch\u00b7laub", "licht\u00b7gr\u00fcn", "im", "Mor\u00b7gen\u00b7strahl", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Tau funkelt auf den B\u00fcschen und Goldduft f\u00fcllt das Tal.", "tokens": ["Tau", "fun\u00b7kelt", "auf", "den", "B\u00fc\u00b7schen", "und", "Gold\u00b7duft", "f\u00fcllt", "das", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Dort \u00e4ugt das Reh, das falbe! Da warnt des H\u00e4hers Schrei!", "tokens": ["Dort", "\u00e4ugt", "das", "Reh", ",", "das", "fal\u00b7be", "!", "Da", "warnt", "des", "H\u00e4\u00b7hers", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$.", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wildtaube huscht, die scheue, pfeilschnellen Flugs vorbei.", "tokens": ["Wild\u00b7tau\u00b7be", "huscht", ",", "die", "scheu\u00b7e", ",", "pfeil\u00b7schnel\u00b7len", "Flugs", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.13": {"line.1": {"text": "Zur Linken rauscht der Waldbach: \u2013 er zieht so silberhell:", "tokens": ["Zur", "Lin\u00b7ken", "rauscht", "der", "Wald\u00b7bach", ":", "\u2013", "er", "zieht", "so", "sil\u00b7ber\u00b7hell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da springen nach tanzenden M\u00fccken die Asch' und die Forell'.", "tokens": ["Da", "sprin\u00b7gen", "nach", "tan\u00b7zen\u00b7den", "M\u00fc\u00b7cken", "die", "Asch'", "und", "die", "Fo\u00b7rell'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.14": {"line.1": {"text": "Wie duftet s\u00fc\u00df der Wei\u00dfdorn, umsummt von Bienen zu Hauf'! \u2013 \u2013", "tokens": ["Wie", "duf\u00b7tet", "s\u00fc\u00df", "der", "Wei\u00df\u00b7dorn", ",", "um\u00b7summt", "von", "Bie\u00b7nen", "zu", "Hauf'", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mir steigt wie Traum und Zauber ein ", "tokens": ["Mir", "steigt", "wie", "Traum", "und", "Zau\u00b7ber", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Da hatten sie den Vetter noch nicht ihr aufgedr\u00e4ngt!", "tokens": ["Da", "hat\u00b7ten", "sie", "den", "Vet\u00b7ter", "noch", "nicht", "ihr", "auf\u00b7ge\u00b7dr\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gar oft an Maienmorgen kam ich hierher gesprengt. \u2013", "tokens": ["Gar", "oft", "an", "Mai\u00b7en\u00b7mor\u00b7gen", "kam", "ich", "hier\u00b7her", "ge\u00b7sprengt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "PPER", "PAV", "VVPP", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Wie feierlich da fl\u00f6tet es hoch vom Buchenbaum!", "tokens": ["Wie", "fei\u00b7er\u00b7lich", "da", "fl\u00f6\u00b7tet", "es", "hoch", "vom", "Bu\u00b7chen\u00b7baum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schwarzamsel! Ja, du freilich geh\u00f6rst in diesen Traum.", "tokens": ["Schwarz\u00b7am\u00b7sel", "!", "Ja", ",", "du", "frei\u00b7lich", "ge\u00b7h\u00f6rst", "in", "die\u00b7sen", "Traum", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "$,", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Dort, an der Br\u00fccke, war es \u2013 am dichten Schlehdornstrauch,", "tokens": ["Dort", ",", "an", "der", "Br\u00fc\u00b7cke", ",", "war", "es", "\u2013", "am", "dich\u00b7ten", "Schleh\u00b7dorn\u00b7strauch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,", "VAFIN", "PPER", "$(", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dort trafen wir uns so gerne: \u2013 dann sangst du, Amsel, auch. \u2013", "tokens": ["Dort", "tra\u00b7fen", "wir", "uns", "so", "ger\u00b7ne", ":", "\u2013", "dann", "sangst", "du", ",", "Am\u00b7sel", ",", "auch", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "$,", "NE", "$,", "ADV", "$.", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "Da wuchsen blaue Glocken so sch\u00f6n wie nirgend im Gau:", "tokens": ["Da", "wuch\u00b7sen", "blau\u00b7e", "Glo\u00b7cken", "so", "sch\u00f6n", "wie", "nir\u00b7gend", "im", "Gau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "ADJD", "KOKOM", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wei\u00df Gott: \u2013 da stehn sie wieder und gr\u00fc\u00dfen und nicken im Tau.", "tokens": ["Wei\u00df", "Gott", ":", "\u2013", "da", "stehn", "sie", "wie\u00b7der", "und", "gr\u00fc\u00b7\u00dfen", "und", "ni\u00b7cken", "im", "Tau", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "ADV", "KON", "VVINF", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "Sie brach mit lichten H\u00e4nden die Blumen sich zum Kranz", "tokens": ["Sie", "brach", "mit", "lich\u00b7ten", "H\u00e4n\u00b7den", "die", "Blu\u00b7men", "sich", "zum", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und schlang ihn um ihr Goldhaar \u2013 Ha! Wie? Bei Gottes Glanz!", "tokens": ["Und", "schlang", "ihn", "um", "ihr", "Gold\u00b7haar", "\u2013", "Ha", "!", "Wie", "?", "Bei", "Got\u00b7tes", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "ITJ", "$.", "PWAV", "$.", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.20": {"line.1": {"text": "Was seh' ich an der Br\u00fccke, dort, unter den Glocken, knien?", "tokens": ["Was", "seh'", "ich", "an", "der", "Br\u00fc\u00b7cke", ",", "dort", ",", "un\u00b7ter", "den", "Glo\u00b7cken", ",", "kni\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verblenden mich die Elfen? Soll ich dem Spuk entfliehn?", "tokens": ["Ver\u00b7blen\u00b7den", "mich", "die", "El\u00b7fen", "?", "Soll", "ich", "dem", "Spuk", "ent\u00b7fliehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$.", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}}, "stanza.21": {"line.1": {"text": "Nein, nein! Sie ist's! \u2013 Wie damals: \u2013 's ist alles, wie es war!", "tokens": ["Nein", ",", "nein", "!", "Sie", "ist's", "!", "\u2013", "Wie", "da\u00b7mals", ":", "\u2013", "'s", "ist", "al\u00b7les", ",", "wie", "es", "war", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "PPER", "VAFIN", "$.", "$(", "PWAV", "ADV", "$.", "$(", "PPER", "VAFIN", "PIS", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Das wei\u00dfe Gewand und die blauen Glocken im goldnen Haar!\u00ab", "tokens": ["Das", "wei\u00b7\u00dfe", "Ge\u00b7wand", "und", "die", "blau\u00b7en", "Glo\u00b7cken", "im", "gold\u00b7nen", "Haar", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.22": {"line.1": {"text": "Und er springt vom Ro\u00df: \u00bbNun sage, du Kleine, du bist doch Maud?", "tokens": ["Und", "er", "springt", "vom", "Ro\u00df", ":", "\u00bb", "Nun", "sa\u00b7ge", ",", "du", "Klei\u00b7ne", ",", "du", "bist", "doch", "Maud", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "ADV", "VVFIN", "$,", "PPER", "ADJA", "$,", "PPER", "VAFIN", "ADV", "NE", "$."], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O sprich, bist du gestorben und erscheinst mir nach dem Tod?\u00ab", "tokens": ["O", "sprich", ",", "bist", "du", "ge\u00b7stor\u00b7ben", "und", "er\u00b7scheinst", "mir", "nach", "dem", "Tod", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.23": {"line.1": {"text": "Da sah ihn hell die Kleine mit lachenden Augen an:", "tokens": ["Da", "sah", "ihn", "hell", "die", "Klei\u00b7ne", "mit", "la\u00b7chen\u00b7den", "Au\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbmaud bin ich freilich! Gestorben? Ein Geist? Ei, r\u00fchr' mich an.\u00ab", "tokens": ["\u00bb", "maud", "bin", "ich", "frei\u00b7lich", "!", "Ge\u00b7stor\u00b7ben", "?", "Ein", "Geist", "?", "Ei", ",", "r\u00fchr'", "mich", "an", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "VAFIN", "PPER", "ADV", "$.", "NN", "$.", "ART", "NN", "$.", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.24": {"line.1": {"text": "Und langsam, sinnend, streicht er mit der erzgepanzerten Hand", "tokens": ["Und", "lang\u00b7sam", ",", "sin\u00b7nend", ",", "streicht", "er", "mit", "der", "erz\u00b7ge\u00b7pan\u00b7zer\u00b7ten", "Hand"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Ihr \u00fcber Haupt und Locken, die l\u00e4chelnd vor ihm stand.", "tokens": ["Ihr", "\u00fc\u00b7ber", "Haupt", "und", "Lo\u00b7cken", ",", "die", "l\u00e4\u00b7chelnd", "vor", "ihm", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.25": {"line.1": {"text": "\u00bbsie ist's \u2013 sie selbst! \u2013 Ach nein doch! \u2013 Ihre", "tokens": ["\u00bb", "sie", "ist's", "\u2013", "sie", "selbst", "!", "\u2013", "Ach", "nein", "doch", "!", "\u2013", "Ih\u00b7re"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "PPER", "VAFIN", "$(", "PPER", "ADV", "$.", "$(", "NN", "PTKANT", "ADV", "$.", "$(", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbach, ", "tokens": ["\u00bb", "ach", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ITJ", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.26": {"line.1": {"text": "Kind, sprich, wie kannst du's wagen? Wie l\u00e4\u00dft dich Lady Maud", "tokens": ["Kind", ",", "sprich", ",", "wie", "kannst", "du's", "wa\u00b7gen", "?", "Wie", "l\u00e4\u00dft", "dich", "La\u00b7dy", "Maud"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "PWAV", "VMFIN", "PIS", "VVINF", "$.", "PWAV", "VVFIN", "PRF", "NE", "NE"], "meter": "++-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Allein in dem Grenzwald wandeln? \u2013 Lord Douglas, der liegt tot:", "tokens": ["Al\u00b7lein", "in", "dem", "Grenz\u00b7wald", "wan\u00b7deln", "?", "\u2013", "Lord", "Doug\u00b7las", ",", "der", "liegt", "tot", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$(", "NN", "NE", "$,", "PRELS", "VVFIN", "ADJD", "$."], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.27": {"line.1": {"text": "Er hatte viele Feinde \u2013 von Fehde seid ihr bedroht.\u00ab", "tokens": ["Er", "hat\u00b7te", "vie\u00b7le", "Fein\u00b7de", "\u2013", "von", "Feh\u00b7de", "seid", "ihr", "be\u00b7droht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$(", "APPR", "NN", "VAFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbdas wei\u00df ich,\u00ab lachte die Kleine. \u00bbDoch hat es keine Not.\u00ab", "tokens": ["\u00bb", "das", "wei\u00df", "ich", ",", "\u00ab", "lach\u00b7te", "die", "Klei\u00b7ne", ".", "\u00bb", "Doch", "hat", "es", "kei\u00b7ne", "Not", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ART", "ADJA", "$.", "$(", "KON", "VAFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.28": {"line.1": {"text": "\u00bbdu k\u00f6nntest irren, Kecke.\u00ab \u2013 \u00bbNein, die Mutter hat's gesagt! \u2013", "tokens": ["\u00bb", "du", "k\u00f6nn\u00b7test", "ir\u00b7ren", ",", "Ke\u00b7cke", ".", "\u00ab", "\u2013", "\u00bb", "Nein", ",", "die", "Mut\u00b7ter", "hat's", "ge\u00b7sagt", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "VVINF", "$,", "NN", "$.", "$(", "$(", "$(", "PTKANT", "$,", "ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Als an dem Sarg des Vaters das Gesinde Furcht geklagt:", "tokens": ["Als", "an", "dem", "Sarg", "des", "Va\u00b7ters", "das", "Ge\u00b7sin\u00b7de", "Furcht", "ge\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.29": {"line.1": {"text": "Da sprach die liebe Mutter: \u00bbIhr Leute, zaget nicht!", "tokens": ["Da", "sprach", "die", "lie\u00b7be", "Mut\u00b7ter", ":", "\u00bb", "Ihr", "Leu\u00b7te", ",", "za\u00b7get", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mich wird ein Held besch\u00fctzen, dem keiner den Schild zerbricht.\u00ab", "tokens": ["Mich", "wird", "ein", "Held", "be\u00b7sch\u00fct\u00b7zen", ",", "dem", "kei\u00b7ner", "den", "Schild", "zer\u00b7bricht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,", "PRELS", "PIS", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.30": {"line.1": {"text": "Da furchte grimm der Alte die Brauen und fuhr ans Schwert:", "tokens": ["Da", "furch\u00b7te", "grimm", "der", "Al\u00b7te", "die", "Brau\u00b7en", "und", "fuhr", "ans", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbha, wer ist dieser Sch\u00fctzer? Wie hei\u00dft der Degen wert?\u00ab", "tokens": ["\u00bb", "ha", ",", "wer", "ist", "die\u00b7ser", "Sch\u00fct\u00b7zer", "?", "Wie", "hei\u00dft", "der", "De\u00b7gen", "wert", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PWS", "VAFIN", "PDAT", "NN", "$.", "PWAV", "VVFIN", "ART", "NN", "ADJD", "$.", "$("], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.31": {"line.1": {"text": "\u00bbso fragt' auch ich die Mutter. Die sprach: \u203aDas ist ein Mann,", "tokens": ["\u00bb", "so", "fragt'", "auch", "ich", "die", "Mut\u00b7ter", ".", "Die", "sprach", ":", "\u203a", "Das", "ist", "ein", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "$.", "ART", "VVFIN", "$.", "$(", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den ich in fr\u00fcher Jugend zum lieben Freund gewann.", "tokens": ["Den", "ich", "in", "fr\u00fc\u00b7her", "Ju\u00b7gend", "zum", "lie\u00b7ben", "Freund", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.32": {"line.1": {"text": "Lord Warwick ist sein Name. Er trug mir Groll im Sinn:", "tokens": ["Lord", "War\u00b7wick", "ist", "sein", "Na\u00b7me", ".", "Er", "trug", "mir", "Groll", "im", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Doch nun, da ich von allen so ganz verlassen bin,", "tokens": ["Doch", "nun", ",", "da", "ich", "von", "al\u00b7len", "so", "ganz", "ver\u00b7las\u00b7sen", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "APPR", "PIS", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.33": {"line.1": {"text": "Da mir auf weiter Erde nicht ein Beschirmer lebt,", "tokens": ["Da", "mir", "auf", "wei\u00b7ter", "Er\u00b7de", "nicht", "ein", "Be\u00b7schir\u00b7mer", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nun wei\u00df ich ganz gewi\u00dflich \u2013\u2039 Ei, wie dir die Lippe bebt?\u00ab \u2013", "tokens": ["Nun", "wei\u00df", "ich", "ganz", "ge\u00b7wi\u00df\u00b7lich", "\u2013", "\u2039", "Ei", ",", "wie", "dir", "die", "Lip\u00b7pe", "bebt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$(", "$(", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$.", "$(",