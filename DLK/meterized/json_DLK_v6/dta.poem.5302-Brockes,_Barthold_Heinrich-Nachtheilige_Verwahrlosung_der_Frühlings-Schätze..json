{"dta.poem.5302": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nachtheilige Verwahrlosung der  \n Fr\u00fchlings-Sch\u00e4tze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Fr\u00fchling scheint auf Wiesen und Gefilde,", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "scheint", "auf", "Wie\u00b7sen", "und", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob in einem neuen Bilde", "tokens": ["Als", "ob", "in", "ei\u00b7nem", "neu\u00b7en", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich eine neue Sch\u00f6pfung zeiget.", "tokens": ["Sich", "ei\u00b7ne", "neu\u00b7e", "Sch\u00f6p\u00b7fung", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da so, wie dort,", "tokens": ["Da", "so", ",", "wie", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch des Allm\u00e4cht\u2019gen Wunder-Wort,", "tokens": ["Durch", "des", "All\u00b7m\u00e4cht'\u00b7gen", "Wun\u00b7der\u00b7Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Kraut, Bl\u00fcthe, Gras, und Laub, neu aus der Erden", "tokens": ["Kraut", ",", "Bl\u00fc\u00b7the", ",", "Gras", ",", "und", "Laub", ",", "neu", "aus", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "steiget.", "tokens": ["stei\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Es gehn, in einem neuen Flor,", "tokens": ["Es", "gehn", ",", "in", "ei\u00b7nem", "neu\u00b7en", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Blumen aus der Erd\u2019 hervor.", "tokens": ["Die", "Blu\u00b7men", "aus", "der", "Erd'", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn wir uns nicht an dieser Sch\u00f6nheit laben;", "tokens": ["Wenn", "wir", "uns", "nicht", "an", "die\u00b7ser", "Sch\u00f6n\u00b7heit", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So scheints, als w\u00fcrden wir, zu unsers Sch\u00f6pfers Ehren,", "tokens": ["So", "scheints", ",", "als", "w\u00fcr\u00b7den", "wir", ",", "zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn wir in Eden selbst gewesen w\u00e4ren,", "tokens": ["Wenn", "wir", "in", "E\u00b7den", "selbst", "ge\u00b7we\u00b7sen", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "An allen neu erschafnen Gaben,", "tokens": ["An", "al\u00b7len", "neu", "er\u00b7schaf\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Uns gleichfals nicht vergn\u00fcget haben.", "tokens": ["Uns", "gleich\u00b7fals", "nicht", "ver\u00b7gn\u00fc\u00b7get", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}