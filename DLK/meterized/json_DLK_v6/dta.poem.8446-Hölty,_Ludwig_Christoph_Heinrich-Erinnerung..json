{"dta.poem.8446": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Kaiser und dem K\u00f6nig gleich,", "tokens": ["Dem", "Kai\u00b7ser", "und", "dem", "K\u00f6\u00b7nig", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Minnejahren,", "tokens": ["In", "mei\u00b7nen", "Min\u00b7ne\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Julia, das sch\u00f6nste Kind,", "tokens": ["Als", "Ju\u00b7lia", ",", "das", "sch\u00f6ns\u00b7te", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sch\u00f6n, wie die lieben Engel sind,", "tokens": ["Sch\u00f6n", ",", "wie", "die", "lie\u00b7ben", "En\u00b7gel", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ich beisammen waren!", "tokens": ["Und", "ich", "bei\u00b7sam\u00b7men", "wa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sah sie, wann die V\u00f6gelein", "tokens": ["Ich", "sah", "sie", ",", "wann", "die", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Morgens trillerten im Hain,", "tokens": ["Des", "Mor\u00b7gens", "tril\u00b7ler\u00b7ten", "im", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Im leichten Fr\u00fchlingskleide,", "tokens": ["Im", "leich\u00b7ten", "Fr\u00fch\u00b7lings\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald vor dem offnen Fenster stehn,", "tokens": ["Bald", "vor", "dem", "off\u00b7nen", "Fens\u00b7ter", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald durch den gr\u00fcnen Anger gehn,", "tokens": ["Bald", "durch", "den", "gr\u00fc\u00b7nen", "An\u00b7ger", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach Gott, mit welcher Freude!", "tokens": ["Ach", "Gott", ",", "mit", "wel\u00b7cher", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sah sie, wann der Abend floh,", "tokens": ["Ich", "sah", "sie", ",", "wann", "der", "A\u00b7bend", "floh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der linden Maienk\u00fchle froh,", "tokens": ["Der", "lin\u00b7den", "Mai\u00b7en\u00b7k\u00fch\u00b7le", "froh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im kleinen Blumengarten,", "tokens": ["Im", "klei\u00b7nen", "Blu\u00b7men\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Eva vor dem S\u00fcndenfall,", "tokens": ["Wie", "E\u00b7va", "vor", "dem", "S\u00fcn\u00b7den\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Begr\u00fcsset von der Nachtigall,", "tokens": ["Be\u00b7gr\u00fcs\u00b7set", "von", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fr\u00fchlingsblumen warten.", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie gab mir manchen s\u00fcssen Blick,", "tokens": ["Sie", "gab", "mir", "man\u00b7chen", "s\u00fcs\u00b7sen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zog niemals ihre Hand zur\u00fcck,", "tokens": ["Zog", "nie\u00b7mals", "ih\u00b7re", "Hand", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann ich die Hand ihr dr\u00fcckte;", "tokens": ["Wann", "ich", "die", "Hand", "ihr", "dr\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sah immer aus, wie Milch und Blut,", "tokens": ["Sah", "im\u00b7mer", "aus", ",", "wie", "Milch", "und", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War immer froh und wohlgemut,", "tokens": ["War", "im\u00b7mer", "froh", "und", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So oft ich sie erblickte.", "tokens": ["So", "oft", "ich", "sie", "er\u00b7blick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie war ich doch so wonnereich,", "tokens": ["Wie", "war", "ich", "doch", "so", "won\u00b7ne\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Kaiser und dem K\u00f6nig gleich,", "tokens": ["Dem", "Kai\u00b7ser", "und", "dem", "K\u00f6\u00b7nig", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinen Minnejahren,", "tokens": ["In", "mei\u00b7nen", "Min\u00b7ne\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Julia, das sch\u00f6nste Kind,", "tokens": ["Als", "Ju\u00b7lia", ",", "das", "sch\u00f6ns\u00b7te", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sch\u00f6n, wie die lieben Engel sind,", "tokens": ["Sch\u00f6n", ",", "wie", "die", "lie\u00b7ben", "En\u00b7gel", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich beisammen waren.", "tokens": ["Und", "ich", "bei\u00b7sam\u00b7men", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}