{"dta.poem.8803": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.57", "af:0.42"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df duftig sey der Trank, gewobne Rosenringe!", "tokens": ["Da\u00df", "duf\u00b7tig", "sey", "der", "Trank", ",", "ge\u00b7wob\u00b7ne", "Ro\u00b7sen\u00b7rin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verletzen m\u00f6gt ihr mich, ihr kalten, Liebelosen,", "tokens": ["Ver\u00b7let\u00b7zen", "m\u00f6gt", "ihr", "mich", ",", "ihr", "kal\u00b7ten", ",", "Lie\u00b7be\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,", "PPOSAT", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wenn berauscht ich bin, eracht' ich euch geringe!", "tokens": ["Doch", "wenn", "be\u00b7rauscht", "ich", "bin", ",", "er\u00b7acht'", "ich", "euch", "ge\u00b7rin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVFIN", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ihr ergr\u00fcbeln wollt, es raubt mir nicht die Ruhe:", "tokens": ["Was", "ihr", "er\u00b7gr\u00fc\u00b7beln", "wollt", ",", "es", "raubt", "mir", "nicht", "die", "Ru\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geheim entsteht das Ich, geheim entstehn die Dinge;", "tokens": ["Ge\u00b7heim", "ent\u00b7steht", "das", "Ich", ",", "ge\u00b7heim", "ent\u00b7stehn", "die", "Din\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PPER", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch h\u00f6rt, was ", "tokens": ["Doch", "h\u00f6rt", ",", "was"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "PWS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Die Schaale? Halber Mond. Die Sonn' im Monde", "tokens": ["Die", "Schaa\u00b7le", "?", "Hal\u00b7ber", "Mond", ".", "Die", "Sonn'", "im", "Mon\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "NN", "$.", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}