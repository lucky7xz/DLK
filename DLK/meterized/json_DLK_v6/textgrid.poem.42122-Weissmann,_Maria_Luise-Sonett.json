{"textgrid.poem.42122": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schien die alte Feindschaft nun besiegelt", "tokens": ["Wie", "schien", "die", "al\u00b7te", "Feind\u00b7schaft", "nun", "be\u00b7sie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "In g\u00fcltigem Vertrag: ein L\u00e4cheln hing", "tokens": ["In", "g\u00fcl\u00b7ti\u00b7gem", "Ver\u00b7trag", ":", "ein", "L\u00e4\u00b7cheln", "hing"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geheimnislos von einem L\u00e4chelnden; verriegelt", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7los", "von", "ei\u00b7nem", "L\u00e4\u00b7cheln\u00b7den", ";", "ver\u00b7rie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schlo\u00df sich der Mund dem Schweigenden, es fing", "tokens": ["Schlo\u00df", "sich", "der", "Mund", "dem", "Schwei\u00b7gen\u00b7den", ",", "es", "fing"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PRF", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die Rede sich in anmutvollem Spiel", "tokens": ["Die", "Re\u00b7de", "sich", "in", "an\u00b7mut\u00b7vol\u00b7lem", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verschlungen wandelnd, schwesterlich vertraut:", "tokens": ["Ver\u00b7schlun\u00b7gen", "wan\u00b7delnd", ",", "schwes\u00b7ter\u00b7lich", "ver\u00b7traut", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie war den Wandernden verwandte Ziel", "tokens": ["Wie", "war", "den", "Wan\u00b7dern\u00b7den", "ver\u00b7wand\u00b7te", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sicher schien das (alte) Haus gebaut:", "tokens": ["Wie", "si\u00b7cher", "schien", "das", "(", "al\u00b7te", ")", "Haus", "ge\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "$(", "ADJA", "$(", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da traf ein Blick aus alten Feindschaftstagen:", "tokens": ["Da", "traf", "ein", "Blick", "aus", "al\u00b7ten", "Feind\u00b7schafts\u00b7ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachtdunkler Weg erstand ins Unbekannte", "tokens": ["Nacht\u00b7dunk\u00b7ler", "Weg", "er\u00b7stand", "ins", "Un\u00b7be\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus sanften Worten str\u00f6mten wilde Klagen.", "tokens": ["Aus", "sanf\u00b7ten", "Wor\u00b7ten", "str\u00f6m\u00b7ten", "wil\u00b7de", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gesprochenes erlosch ins Nie-Genannte....", "tokens": ["Ge\u00b7spro\u00b7che\u00b7nes", "er\u00b7losch", "ins", "Nie\u00b7Ge\u00b7nann\u00b7te", "...."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}