{"textgrid.poem.37876": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00f6nigstochter aus Engelland", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vionetus in Engelland", "tokens": ["Vi\u00b7o\u00b7ne\u00b7tus", "in", "En\u00b7gel\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War K\u00f6nig m\u00e4chtig sehr,", "tokens": ["War", "K\u00f6\u00b7nig", "m\u00e4ch\u00b7tig", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Tochter, Ursula genannt,", "tokens": ["Sein", "Toch\u00b7ter", ",", "Ur\u00b7su\u00b7la", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Jungfrauschaft ein Ehr;", "tokens": ["Der", "Jung\u00b7frausc\u00b7haft", "ein", "Ehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil sie mit Christi Blut erkauft,", "tokens": ["Weil", "sie", "mit", "Chris\u00b7ti", "Blut", "er\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nach des H\u00f6chsten Will getauft,", "tokens": ["Und", "nach", "des", "H\u00f6chs\u00b7ten", "Will", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat sie sich ihm verm\u00e4hlt allein,", "tokens": ["Hat", "sie", "sich", "ihm", "ver\u00b7m\u00e4hlt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Keuschheit stets zu dienen rein.", "tokens": ["In", "Keuschheit", "stets", "zu", "die\u00b7nen", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh da, eins Heidnischen K\u00f6nigs Sohn,", "tokens": ["Sieh", "da", ",", "eins", "Heid\u00b7ni\u00b7schen", "K\u00f6\u00b7nigs", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "CARD", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nach Ursula stand sein Sinn,", "tokens": ["Nach", "Ur\u00b7su\u00b7la", "stand", "sein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fragt, ob sie wollte seinen Thron,", "tokens": ["Fragt", ",", "ob", "sie", "woll\u00b7te", "sei\u00b7nen", "Thron", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als seine K\u00f6nigin?", "tokens": ["Als", "sei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verhie\u00df ihr Land und wilde See,", "tokens": ["Ver\u00b7hie\u00df", "ihr", "Land", "und", "wil\u00b7de", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sehr gro\u00dfe Sch\u00e4tze zu der Eh',", "tokens": ["Sehr", "gro\u00b7\u00dfe", "Sch\u00e4t\u00b7ze", "zu", "der", "Eh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sonst wollt er streiten mit Gefahr", "tokens": ["Sonst", "wollt", "er", "strei\u00b7ten", "mit", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um ihre sch\u00f6ne Jugend klar.", "tokens": ["Um", "ih\u00b7re", "sch\u00f6\u00b7ne", "Ju\u00b7gend", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als Vionetus dies erh\u00f6rt,", "tokens": ["Als", "Vi\u00b7o\u00b7ne\u00b7tus", "dies", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bek\u00fcmmert er sich hart,", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "er", "sich", "hart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Reich wollt halten unzerst\u00f6rt", "tokens": ["Sein", "Reich", "wollt", "hal\u00b7ten", "un\u00b7zer\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Heiden b\u00f6ser Art,", "tokens": ["Von", "Hei\u00b7den", "b\u00f6\u00b7ser", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Darzu sein Tochter fromm und sch\u00f6n,", "tokens": ["Dar\u00b7zu", "sein", "Toch\u00b7ter", "fromm", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt er dem Mann nicht zugestehn,", "tokens": ["Wollt", "er", "dem", "Mann", "nicht", "zu\u00b7ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jedoch des F\u00fcrsten Drohwort gro\u00df,", "tokens": ["Je\u00b7doch", "des", "F\u00fcrs\u00b7ten", "Droh\u00b7wort", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Herzen sein gab harten Sto\u00df.", "tokens": ["Dem", "Her\u00b7zen", "sein", "gab", "har\u00b7ten", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ursula in ihr Zimmer trat,", "tokens": ["Ur\u00b7su\u00b7la", "in", "ihr", "Zim\u00b7mer", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ausgo\u00df vor Gott ihr Herz,", "tokens": ["Aus\u00b7go\u00df", "vor", "Gott", "ihr", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich in des Herren Willen gab,", "tokens": ["Sich", "in", "des", "Her\u00b7ren", "Wil\u00b7len", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn Trauren und ohn Schmerz;", "tokens": ["Ohn", "Trau\u00b7ren", "und", "ohn", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In einen Schlaf fiel sie zur Hand,", "tokens": ["In", "ei\u00b7nen", "Schlaf", "fiel", "sie", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Alsbald ihr Gott ein Engel sandt,", "tokens": ["Als\u00b7bald", "ihr", "Gott", "ein", "En\u00b7gel", "sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Derselbig bracht ihr gute M\u00e4hr,", "tokens": ["Der\u00b7sel\u00b7big", "bracht", "ihr", "gu\u00b7te", "M\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was Gott der Herr von ihr begehr.", "tokens": ["Was", "Gott", "der", "Herr", "von", "ihr", "be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nachdem sie wohl war unterricht,", "tokens": ["Nach\u00b7dem", "sie", "wohl", "war", "un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Engelische Lehr,", "tokens": ["Durch", "En\u00b7ge\u00b7li\u00b7sche", "Lehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Stund zu ihrem Vater spricht,", "tokens": ["Von", "Stund", "zu", "ih\u00b7rem", "Va\u00b7ter", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit fr\u00f6hlicher Geb\u00e4rd:", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7cher", "Ge\u00b7b\u00e4rd", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbsey nicht betr\u00fcbt, Gott ist mit uns,", "tokens": ["\u00bb", "sey", "nicht", "be\u00b7tr\u00fcbt", ",", "Gott", "ist", "mit", "uns", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "VVPP", "$,", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor ihm besteht kein Macht, noch Kunst,", "tokens": ["Vor", "ihm", "be\u00b7steht", "kein", "Macht", ",", "noch", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Mensch mag je verlassen seyn,", "tokens": ["Kein", "Mensch", "mag", "je", "ver\u00b7las\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der nur auf ihn vertraut allein.", "tokens": ["Der", "nur", "auf", "ihn", "ver\u00b7traut", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich will den J\u00fcngling nehmen an,", "tokens": ["Ich", "will", "den", "J\u00fcng\u00b7ling", "neh\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch unter dem Beding:", "tokens": ["Doch", "un\u00b7ter", "dem", "Be\u00b7ding", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du sammt meinem Br\u00e4utigam", "tokens": ["Da\u00df", "du", "sammt", "mei\u00b7nem", "Br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschaffest mir geschwind,", "tokens": ["Ver\u00b7schaf\u00b7fest", "mir", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zehn F\u00fcrstliche Jungfr\u00e4ulein zart,", "tokens": ["Zehn", "F\u00fcrst\u00b7li\u00b7che", "Jung\u00b7fr\u00e4u\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Zu den Eilftausend guter Art,", "tokens": ["Zu", "den", "Eilf\u00b7tau\u00b7send", "gu\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Adlich, jung, sch\u00f6n und tugendreich,", "tokens": ["Ad\u00b7lich", ",", "jung", ",", "sch\u00f6n", "und", "tu\u00b7gen\u00b7dreich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Zu Gottes Ehr, im Himmelreich.", "tokens": ["Zu", "Got\u00b7tes", "Ehr", ",", "im", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dazu eilf Schiff gar wohl versehn", "tokens": ["Da\u00b7zu", "eilf", "Schiff", "gar", "wohl", "ver\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "CARD", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit R\u00fcstung allerhand,", "tokens": ["Mit", "R\u00fcs\u00b7tung", "al\u00b7ler\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir drey Jahr von dannen ziehn,", "tokens": ["Da\u00df", "wir", "drey", "Jahr", "von", "dan\u00b7nen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fern in fremde Land,", "tokens": ["So", "fern", "in", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und unsrer Keuschheit heilgen Preis", "tokens": ["Und", "uns\u00b7rer", "Keuschheit", "heil\u00b7gen", "Preis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Erhalten rein durch diese Rei\u00df,", "tokens": ["Er\u00b7hal\u00b7ten", "rein", "durch", "die\u00b7se", "Rei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Br\u00e4utigam im Himmels-Thron,", "tokens": ["Dem", "Br\u00e4u\u00b7ti\u00b7gam", "im", "Him\u00b7mels\u00b7Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Herrn Jesu Christ, Mari\u00e4 Sohn.\u00ab", "tokens": ["Herrn", "Je\u00b7su", "Christ", ",", "Ma\u00b7ri\u00e4", "Sohn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "NE", "$,", "NE", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da nun der K\u00f6nig dies verstund,", "tokens": ["Da", "nun", "der", "K\u00f6\u00b7nig", "dies", "ver\u00b7stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward er von Herzen froh,", "tokens": ["Ward", "er", "von", "Her\u00b7zen", "froh", ","], "tok