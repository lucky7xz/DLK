{"textgrid.poem.58363": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "401. Unser Hirtenamt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein Hirtenamt: Begierden und Gedanken", "tokens": ["Ich", "hab'", "ein", "Hir\u00b7ten\u00b7amt", ":", "Be\u00b7gier\u00b7den", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind meine Schafe, drauf mein Auge stets mu\u00df sehn,", "tokens": ["Sind", "mei\u00b7ne", "Scha\u00b7fe", ",", "drauf", "mein", "Au\u00b7ge", "stets", "mu\u00df", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PAV", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich halte sie in Eins gesammelt in den Schranken,", "tokens": ["Ich", "hal\u00b7te", "sie", "in", "Eins", "ge\u00b7sam\u00b7melt", "in", "den", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie zerstreuet nicht auf fremder Weide gehn.", "tokens": ["Da\u00df", "sie", "zer\u00b7streu\u00b7et", "nicht", "auf", "frem\u00b7der", "Wei\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}