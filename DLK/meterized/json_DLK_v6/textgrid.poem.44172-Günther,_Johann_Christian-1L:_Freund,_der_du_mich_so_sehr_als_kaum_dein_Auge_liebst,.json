{"textgrid.poem.44172": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, der du mich so sehr als kaum dein Auge liebst,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, der du mich so sehr als kaum dein Auge liebst,", "tokens": ["Freund", ",", "der", "du", "mich", "so", "sehr", "als", "kaum", "dein", "Au\u00b7ge", "liebst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "ADV", "ADV", "KOKOM", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Freund, sag ich, der du mich, ich weis, durch nichts betr\u00fcbst", "tokens": ["Freund", ",", "sag", "ich", ",", "der", "du", "mich", ",", "ich", "weis", ",", "durch", "nichts", "be\u00b7tr\u00fcbst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PRELS", "PPER", "PRF", "$,", "PPER", "PTKVZ", "$,", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als etwan durch die Furcht, du m\u00f6chtest auf der Erden", "tokens": ["Als", "et\u00b7wan", "durch", "die", "Furcht", ",", "du", "m\u00f6ch\u00b7test", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Leibe nach einmahl von mir geri\u00dfen werden,", "tokens": ["Dem", "Lei\u00b7be", "nach", "ein\u00b7mahl", "von", "mir", "ge\u00b7ri\u00b7\u00dfen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Freund, sag ich, nimm auch hier ein schriftlich Denckmahl hin,", "tokens": ["Freund", ",", "sag", "ich", ",", "nimm", "auch", "hier", "ein", "schrift\u00b7lich", "Denck\u00b7mahl", "hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVIMP", "ADV", "ADV", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem ich ganz und gar bereits dein Eigen bin,", "tokens": ["Nach\u00b7dem", "ich", "ganz", "und", "gar", "be\u00b7reits", "dein", "Ei\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KON", "ADV", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und glaube, da\u00df ich mich in reiner Gegenliebe", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "ich", "mich", "in", "rei\u00b7ner", "Ge\u00b7gen\u00b7lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wie dort Jonathan vor seinen David \u00fcbe.", "tokens": ["So", "wie", "dort", "Jo\u00b7na\u00b7than", "vor", "sei\u00b7nen", "Da\u00b7vid", "\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "NE", "APPR", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das B\u00fcndn\u00fc\u00df unter uns ist klug und mit Bedacht,", "tokens": ["Das", "B\u00fcnd\u00b7n\u00fc\u00df", "un\u00b7ter", "uns", "ist", "klug", "und", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht durch zerbrechlich Glas noch Bier und Wein gemacht;", "tokens": ["Nicht", "durch", "zer\u00b7brech\u00b7lich", "Glas", "noch", "Bier", "und", "Wein", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die angebohrne Treu, ein rein und deutsch Gebl\u00fcte,", "tokens": ["Die", "an\u00b7ge\u00b7bohr\u00b7ne", "Treu", ",", "ein", "rein", "und", "deutsch", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Lust zur Wi\u00dfenschaft und dann ein gleich Gem\u00fcthe", "tokens": ["Die", "Lust", "zur", "Wi\u00b7\u00dfen\u00b7schaft", "und", "dann", "ein", "gleich", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "ADV", "ART", "ADV", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Macht aus zwey Herzen eins und bindet uns so scharf,", "tokens": ["Macht", "aus", "zwey", "Her\u00b7zen", "eins", "und", "bin\u00b7det", "uns", "so", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "CARD", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df weder Zeit noch Neid das Band zerrei\u00dfen darf;", "tokens": ["Da\u00df", "we\u00b7der", "Zeit", "noch", "Neid", "das", "Band", "zer\u00b7rei\u00b7\u00dfen", "darf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "NN", "ADV", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Himmel kn\u00fcpft es selbst von Jahr zu Jahren fester", "tokens": ["Der", "Him\u00b7mel", "kn\u00fcpft", "es", "selbst", "von", "Jahr", "zu", "Jah\u00b7ren", "fes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wird uns, wenn einmahl der Feinde Spottgel\u00e4ster", "tokens": ["Und", "wird", "uns", ",", "wenn", "ein\u00b7mahl", "der", "Fein\u00b7de", "Spott\u00b7ge\u00b7l\u00e4s\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit Schaden ausgerast, nach viel Verdru\u00df und Pein", "tokens": ["Mit", "Scha\u00b7den", "aus\u00b7ge\u00b7rast", ",", "nach", "viel", "Ver\u00b7dru\u00df", "und", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Fr\u00fcchte der Gedult in s\u00fc\u00dfer Ruh verleihn.", "tokens": ["Die", "Fr\u00fcch\u00b7te", "der", "Ge\u00b7dult", "in", "s\u00fc\u00b7\u00dfer", "Ruh", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Betrachte deinen Zweck und richte die Gedancken", "tokens": ["Be\u00b7trach\u00b7te", "dei\u00b7nen", "Zweck", "und", "rich\u00b7te", "die", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nach dem, was droben ist; lauf in den Wei\u00dfheitsschrancken", "tokens": ["Nach", "dem", ",", "was", "dro\u00b7ben", "ist", ";", "lauf", "in", "den", "Wei\u00df\u00b7heits\u00b7schran\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "VAFIN", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Mit feuriger Begier gelehrten Seelen nach,", "tokens": ["Mit", "feu\u00b7ri\u00b7ger", "Be\u00b7gier", "ge\u00b7lehr\u00b7ten", "See\u00b7len", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Verachte die Gefahr, verlache Neid und Schmach", "tokens": ["Ver\u00b7ach\u00b7te", "die", "Ge\u00b7fahr", ",", "ver\u00b7la\u00b7che", "Neid", "und", "Schmach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und zeuch dich aus der Nacht, in der der P\u00f6bel stecket,", "tokens": ["Und", "zeuch", "dich", "aus", "der", "Nacht", ",", "in", "der", "der", "P\u00f6\u00b7bel", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der niedertr\u00e4chtig schwermt und keine Wollust schmecket,", "tokens": ["Der", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "schwermt", "und", "kei\u00b7ne", "Wol\u00b7lust", "schme\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die unter Schwei\u00df und Creuz aus wahrer Tugend quillt", "tokens": ["Die", "un\u00b7ter", "Schwei\u00df", "und", "Creuz", "aus", "wah\u00b7rer", "Tu\u00b7gend", "quillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und mehr als Cronengold und alle W\u00fcrde gilt.", "tokens": ["Und", "mehr", "als", "Cro\u00b7nen\u00b7gold", "und", "al\u00b7le", "W\u00fcr\u00b7de", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "NE", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bem\u00fch dich \u00fcberall, die Warheit zu ergr\u00fcnden,", "tokens": ["Be\u00b7m\u00fch", "dich", "\u00fc\u00b7be\u00b7rall", ",", "die", "War\u00b7heit", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du wirst ihr holdes Bild in deiner Seele finden;", "tokens": ["Du", "wirst", "ihr", "hol\u00b7des", "Bild", "in", "dei\u00b7ner", "See\u00b7le", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Verkl\u00e4re den Verstand und l\u00e4utre seine Kraft", "tokens": ["Ver\u00b7kl\u00e4\u00b7re", "den", "Ver\u00b7stand", "und", "l\u00e4ut\u00b7re", "sei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Durch gr\u00fcndlichen Beweis, zwing jede Leidenschaft", "tokens": ["Durch", "gr\u00fcnd\u00b7li\u00b7chen", "Be\u00b7weis", ",", "zwing", "je\u00b7de", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zum Dienste der Vernunft; beschau des H\u00f6chsten Wercke,", "tokens": ["Zum", "Diens\u00b7te", "der", "Ver\u00b7nunft", ";", "be\u00b7schau", "des", "H\u00f6chs\u00b7ten", "Wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Bewundre seine Macht und seiner Liebe St\u00e4rcke", "tokens": ["Be\u00b7wund\u00b7re", "sei\u00b7ne", "Macht", "und", "sei\u00b7ner", "Lie\u00b7be", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "In jeder Creatur; kein St\u00e4ubchen ist so klein,", "tokens": ["In", "je\u00b7der", "Crea\u00b7tur", ";", "kein", "St\u00e4ub\u00b7chen", "ist", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Es wird dir eine Welt voll sch\u00f6ner Ordnung seyn.", "tokens": ["Es", "wird", "dir", "ei\u00b7ne", "Welt", "voll", "sch\u00f6\u00b7ner", "Ord\u00b7nung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADJD", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Thu, was dir m\u00f6glich ist, des Nechsten Heil zu mehren,", "tokens": ["Thu", ",", "was", "dir", "m\u00f6g\u00b7lich", "ist", ",", "des", "Nechs\u00b7ten", "Heil", "zu", "meh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Im Wohlthun las dich nie des Undancks Grobheit st\u00f6ren,", "tokens": ["Im", "Wohl\u00b7thun", "las", "dich", "nie", "des", "Un\u00b7dancks", "Grob\u00b7heit", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Erkennt auch gar kein Mensch dein treu Gem\u00fcthe nicht,", "tokens": ["Er\u00b7kennt", "auch", "gar", "kein", "Mensch", "dein", "treu", "Ge\u00b7m\u00fc\u00b7the", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN", "PPOSAT", "ADJD", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Gnug, wenn in deiner Brust der stumme Zeuge spricht,", "tokens": ["Gnug", ",", "wenn", "in", "dei\u00b7ner", "Brust", "der", "stum\u00b7me", "Zeu\u00b7ge", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Du habest recht gethan; ein unbesorgt Gewi\u00dfen", "tokens": ["Du", "ha\u00b7best", "recht", "ge\u00b7than", ";", "ein", "un\u00b7be\u00b7sorgt", "Ge\u00b7wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Giebt auch auf Holz und Streu das weichste Schwanenk\u00fc\u00dfen.", "tokens": ["Giebt", "auch", "auf", "Holz", "und", "Streu", "das", "weichs\u00b7te", "Schwa\u00b7nen\u00b7k\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "l