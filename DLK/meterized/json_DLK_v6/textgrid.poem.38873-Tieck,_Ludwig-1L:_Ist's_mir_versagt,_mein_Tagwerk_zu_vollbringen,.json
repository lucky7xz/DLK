{"textgrid.poem.38873": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist's mir versagt, mein Tagwerk zu vollbringen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's mir versagt, mein Tagwerk zu vollbringen,", "tokens": ["Ist's", "mir", "ver\u00b7sagt", ",", "mein", "Tag\u00b7werk", "zu", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll mir das Licht des Tages bald verschwinden,", "tokens": ["Soll", "mir", "das", "Licht", "des", "Ta\u00b7ges", "bald", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird mich die Nacht froh und ger\u00fcstet finden,", "tokens": ["Wird", "mich", "die", "Nacht", "froh", "und", "ge\u00b7r\u00fcs\u00b7tet", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "KON", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ich gewollt wird k\u00fcnftig dir gelingen.", "tokens": ["Was", "ich", "ge\u00b7wollt", "wird", "k\u00fcnf\u00b7tig", "dir", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMPP", "VAFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vertrau den k\u00fchnen jugendlichen Schwingen,", "tokens": ["Ver\u00b7trau", "den", "k\u00fch\u00b7nen", "ju\u00b7gend\u00b7li\u00b7chen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df nimmer dich von Furcht und Zweifel binden,", "tokens": ["La\u00df", "nim\u00b7mer", "dich", "von", "Furcht", "und", "Zwei\u00b7fel", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nein, r\u00f6ther mu\u00df die Rose sich entz\u00fcnden,", "tokens": ["Nein", ",", "r\u00f6\u00b7ther", "mu\u00df", "die", "Ro\u00b7se", "sich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr duftend Blut durch alle Bl\u00e4tter dringen.", "tokens": ["Ihr", "duf\u00b7tend", "Blut", "durch", "al\u00b7le", "Bl\u00e4t\u00b7ter", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du kennst den gr\u00fcnen Wald, des Himmels Bl\u00e4ue,", "tokens": ["Du", "kennst", "den", "gr\u00fc\u00b7nen", "Wald", ",", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast von seliger Musik getrunken,", "tokens": ["Du", "hast", "von", "se\u00b7li\u00b7ger", "Mu\u00b7sik", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den ewgen Rausch dem goldnen Kelch entnommen,", "tokens": ["Den", "ew\u00b7gen", "Rausch", "dem", "gold\u00b7nen", "Kelch", "ent\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du wei\u00dft, was uns der gro\u00dfe Wahnsinn leihe,", "tokens": ["Du", "wei\u00dft", ",", "was", "uns", "der", "gro\u00b7\u00dfe", "Wahn\u00b7sinn", "lei\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Dunkel ist auf immer dir versunken,", "tokens": ["Das", "Dun\u00b7kel", "ist", "auf", "im\u00b7mer", "dir", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein unausl\u00f6schlich Morgenroth entglommen.", "tokens": ["Ein", "un\u00b7aus\u00b7l\u00f6schlich", "Mor\u00b7gen\u00b7roth", "ent\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}