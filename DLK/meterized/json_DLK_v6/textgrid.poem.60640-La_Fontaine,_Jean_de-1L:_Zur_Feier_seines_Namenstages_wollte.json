{"textgrid.poem.60640": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zur Feier seines Namenstages wollte", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Feier seines Namenstages wollte", "tokens": ["Zur", "Fei\u00b7er", "sei\u00b7nes", "Na\u00b7mens\u00b7ta\u00b7ges", "woll\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tiere K\u00f6nig einen Jagdzug machen.", "tokens": ["Der", "Tie\u00b7re", "K\u00f6\u00b7nig", "ei\u00b7nen", "Jagd\u00b7zug", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sperlinge sind kein Wild f\u00fcr L\u00f6wenrachen;", "tokens": ["Sper\u00b7lin\u00b7ge", "sind", "kein", "Wild", "f\u00fcr", "L\u00f6\u00b7wen\u00b7ra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Wildpark, der die Beute liefern sollte,", "tokens": ["Sein", "Wild\u00b7park", ",", "der", "die", "Beu\u00b7te", "lie\u00b7fern", "soll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "War reich belebt von feisten wilden Schweinen,", "tokens": ["War", "reich", "be\u00b7lebt", "von", "feis\u00b7ten", "wil\u00b7den", "Schwei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Rehwild und Hirschen mit geschwinden Beinen,", "tokens": ["Reh\u00b7wild", "und", "Hir\u00b7schen", "mit", "ge\u00b7schwin\u00b7den", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und da\u00df die Jagd gelinge, bot der Grimme", "tokens": ["Und", "da\u00df", "die", "Jagd", "ge\u00b7lin\u00b7ge", ",", "bot", "der", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Esel auf, um seiner Stentorstimme", "tokens": ["Den", "E\u00b7sel", "auf", ",", "um", "sei\u00b7ner", "Sten\u00b7tor\u00b7stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUI", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sich bei dem Treiben zu bedienen.", "tokens": ["Sich", "bei", "dem", "Trei\u00b7ben", "zu", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der L\u00f6we wies ihm seinen Platz, bedeckte", "tokens": ["Der", "L\u00f6\u00b7we", "wies", "ihm", "sei\u00b7nen", "Platz", ",", "be\u00b7deck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mit Laubwerk ihn und gab Befehl zu schreien,", "tokens": ["Mit", "Laub\u00b7werk", "ihn", "und", "gab", "Be\u00b7fehl", "zu", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gewi\u00df, da\u00df alle Tiere dann erschienen", "tokens": ["Ge\u00b7wi\u00df", ",", "da\u00df", "al\u00b7le", "Tie\u00b7re", "dann", "er\u00b7schie\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PIAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Aus ihren Lagern, weil der Schrei sie schreckte,", "tokens": ["Aus", "ih\u00b7ren", "La\u00b7gern", ",", "weil", "der", "Schrei", "sie", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der etwas Ungewohntes war im Freien.", "tokens": ["Der", "et\u00b7was", "Un\u00b7ge\u00b7wohn\u00b7tes", "war", "im", "Frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und so geschah's. Der L\u00e4rm ert\u00f6nte gr\u00e4\u00dflich,", "tokens": ["Und", "so", "ge\u00b7scha\u00b7h'", "s.", "Der", "L\u00e4rm", "er\u00b7t\u00f6n\u00b7te", "gr\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVIMP", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Entsetzen fiel des Waldes G\u00e4ste an,", "tokens": ["Ent\u00b7set\u00b7zen", "fiel", "des", "Wal\u00b7des", "G\u00e4s\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sie flohn \u2013 jedoch dem lauernden Tyrann", "tokens": ["Sie", "flohn", "\u2013", "je\u00b7doch", "dem", "lau\u00b7ern\u00b7den", "Ty\u00b7rann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ins offne Maul. Der Fang war unerme\u00dflich.", "tokens": ["Ins", "off\u00b7ne", "Maul", ".", "Der", "Fang", "war", "un\u00b7er\u00b7me\u00df\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Esel kam und prahlte stolzerf\u00fcllt:", "tokens": ["Der", "E\u00b7sel", "kam", "und", "prahl\u00b7te", "stol\u00b7zer\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "\u00bbich hab's geschafft, da\u00df uns kein Schwanz entrann!\u00ab", "tokens": ["\u00bb", "ich", "hab's", "ge\u00b7schafft", ",", "da\u00df", "uns", "kein", "Schwanz", "ent\u00b7rann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "NE", "VVPP", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Er tat, als falle ihm die ganze Ehre zu.", "tokens": ["Er", "tat", ",", "als", "fal\u00b7le", "ihm", "die", "gan\u00b7ze", "Eh\u00b7re", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der L\u00f6we sagte: \u00bbGut gebr\u00fcllt!", "tokens": ["Der", "L\u00f6\u00b7we", "sag\u00b7te", ":", "\u00bb", "Gut", "ge\u00b7br\u00fcllt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$.", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und h\u00e4tt ich nicht gewu\u00dft, da\u00df einzig du,", "tokens": ["Und", "h\u00e4tt", "ich", "nicht", "ge\u00b7wu\u00dft", ",", "da\u00df", "ein\u00b7zig", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "KOUS", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ein Langohr, das Geschrei vollf\u00fchrt,", "tokens": ["Ein", "Lan\u00b7gohr", ",", "das", "Ge\u00b7schrei", "voll\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ich h\u00e4tte selber Angst versp\u00fcrt.\u00ab", "tokens": ["Ich", "h\u00e4t\u00b7te", "sel\u00b7ber", "Angst", "ver\u00b7sp\u00fcrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Esel schwieg, um seinen Zorn nicht zu verraten.", "tokens": ["Der", "E\u00b7sel", "schwieg", ",", "um", "sei\u00b7nen", "Zorn", "nicht", "zu", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Traf ihn des L\u00f6wen Spott nicht ganz mit Recht?", "tokens": ["Traf", "ihn", "des", "L\u00f6\u00b7wen", "Spott", "nicht", "ganz", "mit", "Recht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die Ruhmsucht pa\u00dft zu einem Esel schlecht,", "tokens": ["Die", "Ruhm\u00b7sucht", "pa\u00dft", "zu", "ei\u00b7nem", "E\u00b7sel", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So schlecht wie gro\u00dfe Heldentaten!", "tokens": ["So", "schlecht", "wie", "gro\u00b7\u00dfe", "Hel\u00b7den\u00b7ta\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}