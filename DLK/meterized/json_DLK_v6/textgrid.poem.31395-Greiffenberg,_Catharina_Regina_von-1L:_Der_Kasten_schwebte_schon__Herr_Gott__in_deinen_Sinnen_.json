{"textgrid.poem.31395": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Kasten schwebte schon / Herr Gott / in deinen Sinnen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kasten schwebte schon / Herr Gott / in deinen Sinnen /", "tokens": ["Der", "Kas\u00b7ten", "schweb\u00b7te", "schon", "/", "Herr", "Gott", "/", "in", "dei\u00b7nen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "NN", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als sich der Himmel tr\u00fcbt und sich die Flut anhebt'.", "tokens": ["als", "sich", "der", "Him\u00b7mel", "tr\u00fcbt", "und", "sich", "die", "Flut", "an\u00b7hebt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "KON", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eh die alt' Erd' ertrank / schon in der neuen lebt", "tokens": ["Eh", "die", "alt'", "Erd'", "er\u00b7trank", "/", "schon", "in", "der", "neu\u00b7en", "lebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$(", "ADV", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der beeder Welten Held / auf deines Rahts schaub\u00fchnen.", "tokens": ["der", "bee\u00b7der", "Wel\u00b7ten", "Held", "/", "auf", "dei\u00b7nes", "Rahts", "schau\u00b7b\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Feur war schon gek\u00fclt / als jene Drey darinnen.", "tokens": ["Das", "Feur", "war", "schon", "ge\u00b7k\u00fclt", "/", "als", "je\u00b7ne", "Drey", "da\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "KOUS", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch David war gekr\u00f6nt / weil er in Elend schwebt.", "tokens": ["Auch", "Da\u00b7vid", "war", "ge\u00b7kr\u00f6nt", "/", "weil", "er", "in", "E\u00b7lend", "schwebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "das Weib war schon entzuckt / eh ihr der Drach nachstrebt.", "tokens": ["das", "Weib", "war", "schon", "ent\u00b7zuckt", "/", "eh", "ihr", "der", "Drach", "nach\u00b7strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gott pflegt die Schnur / eh man in Irrgang kommt / zu spinnen.", "tokens": ["Gott", "pflegt", "die", "Schnur", "/", "eh", "man", "in", "Irr\u00b7gang", "kommt", "/", "zu", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$(", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Schlange war entgifft / eh Paulus sie ber\u00fchrt.", "tokens": ["Die", "Schlan\u00b7ge", "war", "ent\u00b7gifft", "/", "eh", "Pau\u00b7lus", "sie", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KOUS", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der Freuden-Lehre", "tokens": ["der", "Freu\u00b7den\u00b7Leh\u00b7re"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "ehe man ein F\u00fcncklein noch in allen Seelen sp\u00fcrt.", "tokens": ["e\u00b7he", "man", "ein", "F\u00fcnc\u00b7klein", "noch", "in", "al\u00b7len", "See\u00b7len", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Vor Vngl\u00fccks Schickung / ist der H\u00f6chst auf H\u00fclff befli\u00dfen.", "tokens": ["Vor", "Vn\u00b7gl\u00fccks", "Schi\u00b7ckung", "/", "ist", "der", "H\u00f6chst", "auf", "H\u00fclff", "be\u00b7fli\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "dr\u00fcm folget ihm / wie fremd und seltsam Er euch f\u00fchrt.", "tokens": ["dr\u00fcm", "fol\u00b7get", "ihm", "/", "wie", "fremd", "und", "selt\u00b7sam", "Er", "euch", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "PWAV", "ADJD", "KON", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "sein' Hand hat aus der H\u00f6ll / geschweig aus Noht / geri\u00dfen.", "tokens": ["sein'", "Hand", "hat", "aus", "der", "H\u00f6ll", "/", "ge\u00b7schweig", "aus", "Noht", "/", "ge\u00b7ri\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "ADJD", "APPR", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}