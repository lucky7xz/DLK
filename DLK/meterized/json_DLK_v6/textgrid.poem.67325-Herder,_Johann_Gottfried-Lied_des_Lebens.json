{"textgrid.poem.67325": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fl\u00fcchtiger als Wind und Welle", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ger", "als", "Wind", "und", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flieht die Zeit; was h\u00e4lt sie auf?", "tokens": ["Flieht", "die", "Zeit", ";", "was", "h\u00e4lt", "sie", "auf", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie genie\u00dfen auf der Stelle,", "tokens": ["Sie", "ge\u00b7nie\u00b7\u00dfen", "auf", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ergreifen schnell im Lauf,", "tokens": ["Sie", "er\u00b7grei\u00b7fen", "schnell", "im", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das, Ihr Br\u00fcder, h\u00e4lt ihr Schweben,", "tokens": ["Das", ",", "Ihr", "Br\u00fc\u00b7der", ",", "h\u00e4lt", "ihr", "Schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4lt die Flucht der Tage ein.", "tokens": ["H\u00e4lt", "die", "Flucht", "der", "Ta\u00b7ge", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schneller Gang ist unser Leben;", "tokens": ["Schnel\u00b7ler", "Gang", "ist", "un\u00b7ser", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00dft uns Rosen auf ihn streun!", "tokens": ["La\u00dft", "uns", "Ro\u00b7sen", "auf", "ihn", "streun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rosen, denn die Tage sinken", "tokens": ["Ro\u00b7sen", ",", "denn", "die", "Ta\u00b7ge", "sin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Winters Nebelmeer;", "tokens": ["In", "des", "Win\u00b7ters", "Ne\u00b7bel\u00b7meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosen, denn sie bl\u00fchn und blinken", "tokens": ["Ro\u00b7sen", ",", "denn", "sie", "bl\u00fchn", "und", "blin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "PPER", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Links und rechts noch um uns her.", "tokens": ["Links", "und", "rechts", "noch", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosen stehn auf jedem Zweige", "tokens": ["Ro\u00b7sen", "stehn", "auf", "je\u00b7dem", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder sch\u00f6nen Jugendthat.", "tokens": ["Je\u00b7der", "sch\u00f6\u00b7nen", "Ju\u00b7gendt\u00b7hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wohl ihm, der bis auf die Neige", "tokens": ["Wohl", "ihm", ",", "der", "bis", "auf", "die", "Nei\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rein gelebt sein Leben hat!", "tokens": ["Rein", "ge\u00b7lebt", "sein", "Le\u00b7ben", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tage, werdet uns zum Kranze,", "tokens": ["Ta\u00b7ge", ",", "wer\u00b7det", "uns", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der des Greises Schl\u00e4f' umzieht", "tokens": ["Der", "des", "Grei\u00b7ses", "Schl\u00e4f'", "um\u00b7zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und um sie in frischem Glanze", "tokens": ["Und", "um", "sie", "in", "fri\u00b7schem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wie ein Traum der Jugend bl\u00fcht.", "tokens": ["Wie", "ein", "Traum", "der", "Ju\u00b7gend", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch die dunkeln Blumen k\u00fchlen", "tokens": ["Auch", "die", "dun\u00b7keln", "Blu\u00b7men", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns mit Ruhe, doppelt s\u00fc\u00df;", "tokens": ["Uns", "mit", "Ru\u00b7he", ",", "dop\u00b7pelt", "s\u00fc\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die lauen L\u00fcfte spielen", "tokens": ["Und", "die", "lau\u00b7en", "L\u00fcf\u00b7te", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Freundlich uns ins Paradies.", "tokens": ["Freund\u00b7lich", "uns", "ins", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}