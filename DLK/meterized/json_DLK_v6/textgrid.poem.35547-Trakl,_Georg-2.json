{"textgrid.poem.35547": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille schafft sie in der Kammer", "tokens": ["Stil\u00b7le", "schafft", "sie", "in", "der", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Hof liegt l\u00e4ngst ver\u00f6det.", "tokens": ["Und", "der", "Hof", "liegt", "l\u00e4ngst", "ver\u00b7\u00f6\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Hollunder vor der Kammer", "tokens": ["Im", "Hol\u00b7lun\u00b7der", "vor", "der", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kl\u00e4glich eine Amsel fl\u00f6tet.", "tokens": ["Kl\u00e4g\u00b7lich", "ei\u00b7ne", "Am\u00b7sel", "fl\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Silbern schaut ihr Bild im Spiegel", "tokens": ["Sil\u00b7bern", "schaut", "ihr", "Bild", "im", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremd sie an im Zwielichtscheine", "tokens": ["Fremd", "sie", "an", "im", "Zwie\u00b7licht\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verd\u00e4mmert fahl im Spiegel", "tokens": ["Und", "ver\u00b7d\u00e4m\u00b7mert", "fahl", "im", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr graut vor seiner Reine.", "tokens": ["Und", "ihr", "graut", "vor", "sei\u00b7ner", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traumhaft singt ein Knecht im Dunkel", "tokens": ["Traum\u00b7haft", "singt", "ein", "Knecht", "im", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie starrt von Schmerz gesch\u00fcttelt.", "tokens": ["Und", "sie", "starrt", "von", "Schmerz", "ge\u00b7sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00f6te tr\u00e4ufelt durch das Dunkel.", "tokens": ["R\u00f6\u00b7te", "tr\u00e4u\u00b7felt", "durch", "das", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00e4h am Tor der S\u00fcdwind r\u00fcttelt.", "tokens": ["J\u00e4h", "am", "Tor", "der", "S\u00fcd\u00b7wind", "r\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter