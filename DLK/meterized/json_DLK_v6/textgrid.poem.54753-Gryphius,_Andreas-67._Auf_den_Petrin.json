{"textgrid.poem.54753": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "67. Auf den Petrin", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Artzt gab gestern mir f\u00fcnff starcker Pillen ein/", "tokens": ["Der", "Artzt", "gab", "ge\u00b7stern", "mir", "f\u00fcnff", "star\u00b7cker", "Pil\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "CARD", "ADJA", "NN", "ART", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wiewol umbsonst/ ich blib schir bi\u00df umb zehn allein/", "tokens": ["Wie\u00b7wol", "um\u00b7bsonst", "/", "ich", "blib", "schir", "bi\u00df", "umb", "zehn", "al\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "CARD", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da kam ", "tokens": ["Da", "kam"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Dar\u00fcber hab ich Lung' und Hertz schir au\u00dfgebrochen.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hab", "ich", "Lung'", "und", "Hertz", "schir", "au\u00df\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NE", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich liff wol zwantzig mahl. Zuletzt must ich mich legen/", "tokens": ["Ich", "liff", "wol", "zwant\u00b7zig", "mahl", ".", "Zu\u00b7letzt", "must", "ich", "mich", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "ADV", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sprecht mehr ", "tokens": ["Sprecht", "mehr"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}}}}}