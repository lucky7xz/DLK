{"textgrid.poem.46541": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sie haben nun ihre Possen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben nun ihre Possen", "tokens": ["Sie", "ha\u00b7ben", "nun", "ih\u00b7re", "Pos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Getrieben, mir war's kein Scherz,", "tokens": ["Ge\u00b7trie\u00b7ben", ",", "mir", "wa\u00b7r's", "kein", "Scherz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Neujahr angeschossen,", "tokens": ["Das", "Neu\u00b7jahr", "an\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jeder Schu\u00df traf mein Herz;", "tokens": ["Je\u00b7der", "Schu\u00df", "traf", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Gesichtchen todtenbleiche", "tokens": ["Ge\u00b7sicht\u00b7chen", "tod\u00b7ten\u00b7blei\u00b7che"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Bettchen mir aufgeschreckt,", "tokens": ["Im", "Bett\u00b7chen", "mir", "auf\u00b7ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Nebenzimmer die Leiche", "tokens": ["Im", "Ne\u00b7ben\u00b7zim\u00b7mer", "die", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Haben sie nicht erweckt.", "tokens": ["Ha\u00b7ben", "sie", "nicht", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich kann es nicht begreifen,", "tokens": ["Ich", "kann", "es", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ich's einst mitgemacht,", "tokens": ["Wie", "ich's", "einst", "mit\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch die Stra\u00dfen zu schweifen", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen", "zu", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Jubelnd um Mitternacht.", "tokens": ["Ju\u00b7belnd", "um", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Hier und dort sieht man brennen", "tokens": ["Hier", "und", "dort", "sieht", "man", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus einem Fenster ein Licht;", "tokens": ["Aus", "ei\u00b7nem", "Fens\u00b7ter", "ein", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Was drinnen f\u00fcr Bande sich trennen,", "tokens": ["Was", "drin\u00b7nen", "f\u00fcr", "Ban\u00b7de", "sich", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das wei\u00df man drau\u00dfen nicht.", "tokens": ["Das", "wei\u00df", "man", "drau\u00b7\u00dfen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}