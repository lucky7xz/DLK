{"textgrid.poem.52113": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann dich, da\u00df sie dir Leib und Seele m\u00f6gen schw\u00e4chen,", "tokens": ["Wann", "dich", ",", "da\u00df", "sie", "dir", "Leib", "und", "See\u00b7le", "m\u00f6\u00b7gen", "schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUS", "PPER", "PPER", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Rasende Begier, der w\u00fctende Gebrechen,", "tokens": ["Die", "Ra\u00b7sen\u00b7de", "Be\u00b7gier", ",", "der", "w\u00fc\u00b7ten\u00b7de", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das stinckend' Ungemach, gleichwie die Noth kommt an,", "tokens": ["Das", "stin\u00b7ckend'", "Un\u00b7ge\u00b7mach", ",", "gleich\u00b7wie", "die", "Noth", "kommt", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Kn\u00f6pff und Senckel l\u00f6st, ist es um dich gethan.", "tokens": ["Die", "Kn\u00f6pff", "und", "Sen\u00b7ckel", "l\u00f6st", ",", "ist", "es", "um", "dich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wil die Brunst, als wie die Fraas ihr Toben sch\u00e4rffen,", "tokens": ["Denn", "wil", "die", "Brunst", ",", "als", "wie", "die", "Fraas", "ihr", "To\u00b7ben", "sch\u00e4rf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und deinen Leib herum auff einem Aa\u00dfe werffen,", "tokens": ["Und", "dei\u00b7nen", "Leib", "he\u00b7rum", "auff", "ei\u00b7nem", "Aa\u00b7\u00dfe", "werf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APZR", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das wie ein Klotz im Sumpff auf ieden Schlag sich henckt,", "tokens": ["Das", "wie", "ein", "Klotz", "im", "Sumpff", "auf", "ie\u00b7den", "Schlag", "sich", "henckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "APPRART", "NN", "APPR", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Augenblicklich dich in Sumpff der H\u00f6llen senckt.", "tokens": ["Und", "Au\u00b7gen\u00b7blick\u00b7lich", "dich", "in", "Sumpff", "der", "H\u00f6l\u00b7len", "senckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Mensch der Geilheit Aa\u00df, da\u00df du nicht seyst verlohren", "tokens": ["O", "Mensch", "der", "Geil\u00b7heit", "Aa\u00df", ",", "da\u00df", "du", "nicht", "seyst", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch schn\u00f6de Hurerey, ward Gott ein Mensch gebohren:", "tokens": ["Durch", "schn\u00f6\u00b7de", "Hu\u00b7re\u00b7rey", ",", "ward", "Gott", "ein", "Mensch", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VAFIN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er wird mit dir verm\u00e4hlt, Ach denck an die Geburt,", "tokens": ["Er", "wird", "mit", "dir", "ver\u00b7m\u00e4hlt", ",", "Ach", "denck", "an", "die", "Ge\u00b7burt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,", "ITJ", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schau auff was vor Art du l\u00f6sest deinen Gurt.", "tokens": ["Und", "schau", "auff", "was", "vor", "Art", "du", "l\u00f6\u00b7sest", "dei\u00b7nen", "Gurt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PRELS", "APPR", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}