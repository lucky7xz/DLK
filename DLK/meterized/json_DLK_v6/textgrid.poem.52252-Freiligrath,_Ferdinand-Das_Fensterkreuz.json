{"textgrid.poem.52252": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Fensterkreuz", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Neuhaus in dem Schlosse war's: \u2013 der Kurf\u00fcrst", "tokens": ["Zu", "Neu\u00b7haus", "in", "dem", "Schlos\u00b7se", "wa\u00b7r's", ":", "\u2013", "der", "Kur\u00b7f\u00fcrst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VAFIN", "$.", "$(", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die G\u00e4ste sa\u00dfen dichtgereiht, und H\u00f6rner schmetterten im Saal.", "tokens": ["Die", "G\u00e4s\u00b7te", "sa\u00b7\u00dfen", "dicht\u00b7ge\u00b7reiht", ",", "und", "H\u00f6r\u00b7ner", "schmet\u00b7ter\u00b7ten", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Der Mundschenk go\u00df die Gl\u00e4ser voll, die Diener dr\u00e4ngten sich zuhauf \u2013", "tokens": ["Der", "Mund\u00b7schenk", "go\u00df", "die", "Gl\u00e4\u00b7ser", "voll", ",", "die", "Die\u00b7ner", "dr\u00e4ng\u00b7ten", "sich", "zu\u00b7hauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "PRF", "ADV", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Es war ein schw\u00fcler Sommertag, die Fenster alle standen auf.", "tokens": ["Es", "war", "ein", "schw\u00fc\u00b7ler", "Som\u00b7mer\u00b7tag", ",", "die", "Fens\u00b7ter", "al\u00b7le", "stan\u00b7den", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Und durch die offnen Fenster rings sah man den k\u00fchlen, gr\u00fcnen Wald;", "tokens": ["Und", "durch", "die", "off\u00b7nen", "Fens\u00b7ter", "rings", "sah", "man", "den", "k\u00fch\u00b7len", ",", "gr\u00fc\u00b7nen", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "VVFIN", "PIS", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der Wald, das war zu dieser Zeit des F\u00fcrsten liebster Aufenthalt!", "tokens": ["Der", "Wald", ",", "das", "war", "zu", "die\u00b7ser", "Zeit", "des", "F\u00fcrs\u00b7ten", "liebs\u00b7ter", "Auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "APPR", "PDAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "In dem verga\u00df er, hell umt\u00f6nt von Hirschgeschrei und Rosseshuf,", "tokens": ["In", "dem", "ver\u00b7ga\u00df", "er", ",", "hell", "um\u00b7t\u00f6nt", "von", "Hirschge\u00b7schrei", "und", "Ros\u00b7ses\u00b7huf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Den \u00c4rger, den zu K\u00f6nigsberg der b\u00f6se Landtag dreist ihm schuf.", "tokens": ["Den", "\u00c4r\u00b7ger", ",", "den", "zu", "K\u00f6\u00b7nigs\u00b7berg", "der", "b\u00f6\u00b7se", "Land\u00b7tag", "dreist", "ihm", "schuf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ei, dieses starre K\u00f6nigsberg! Ei, dies verwegne Preu\u00dfenland!", "tokens": ["Ei", ",", "die\u00b7ses", "star\u00b7re", "K\u00f6\u00b7nigs\u00b7berg", "!", "Ei", ",", "dies", "ver\u00b7weg\u00b7ne", "Preu\u00b7\u00dfen\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "ADJA", "NN", "$.", "NN", "$,", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ei, wie beharrlich und beherzt auf seinen Rechten es bestand!", "tokens": ["Ei", ",", "wie", "be\u00b7harr\u00b7lich", "und", "be\u00b7herzt", "auf", "sei\u00b7nen", "Rech\u00b7ten", "es", "be\u00b7stand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.3": {"text": "Und nicht sein Adel blo\u00df! O nein, auch seine St\u00e4dte sprachen mit!", "tokens": ["Und", "nicht", "sein", "A\u00b7del", "blo\u00df", "!", "O", "nein", ",", "auch", "sei\u00b7ne", "St\u00e4d\u00b7te", "spra\u00b7chen", "mit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "NN", "ADV", "$.", "NE", "PTKANT", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wer war's, der die Leibeigenschaft des armen Bauernvolks bestritt?", "tokens": ["Wer", "wa\u00b7r's", ",", "der", "die", "Leib\u00b7ei\u00b7gen\u00b7schaft", "des", "ar\u00b7men", "Bau\u00b7ern\u00b7volks", "be\u00b7stritt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "O frischer, freier B\u00fcrgertrotz! O Erbteil, das der Ostsee blieb!", "tokens": ["O", "fri\u00b7scher", ",", "frei\u00b7er", "B\u00fcr\u00b7ger\u00b7trotz", "!", "O", "Erb\u00b7teil", ",", "das", "der", "Ost\u00b7see", "blieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$.", "NE", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Du sprudelst aus der Flut hervor, mehr als den Brandenburgern lieb!", "tokens": ["Du", "spru\u00b7delst", "aus", "der", "Flut", "her\u00b7vor", ",", "mehr", "als", "den", "Bran\u00b7den\u00b7bur\u00b7gern", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "PIAT", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wie heute noch der Krone Schein bei deinem Brausen zag erbla\u00dft,", "tokens": ["Wie", "heu\u00b7te", "noch", "der", "Kro\u00b7ne", "Schein", "bei", "dei\u00b7nem", "Brau\u00b7sen", "zag", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "So warst du auch dem kurhut schon in deiner Freudigkeit verha\u00dft! \u2013", "tokens": ["So", "warst", "du", "auch", "dem", "kur\u00b7hut", "schon", "in", "dei\u00b7ner", "Freu\u00b7dig\u00b7keit", "ver\u00b7ha\u00dft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "\u00bbwas k\u00fcmmern mich die St\u00e4nde heut zu Neuhaus hier auf meinem Schlo\u00df?\u00ab", "tokens": ["\u00bb", "was", "k\u00fcm\u00b7mern", "mich", "die", "St\u00e4n\u00b7de", "heut", "zu", "Neu\u00b7haus", "hier", "auf", "mei\u00b7nem", "Schlo\u00df", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Da stapfte klirrend in den Sall ein Reiter mit entbl\u00f6\u00dftem Haupt;", "tokens": ["Da", "stapf\u00b7te", "klir\u00b7rend", "in", "den", "Sall", "ein", "Rei\u00b7ter", "mit", "ent\u00b7bl\u00f6\u00df\u00b7tem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Ein Bote war's von K\u00f6nigsberg, Blut an den Sporen und bestaubt.", "tokens": ["Ein", "Bo\u00b7te", "wa\u00b7r's", "von", "K\u00f6\u00b7nigs\u00b7berg", ",", "Blut", "an", "den", "Spo\u00b7ren", "und", "be\u00b7staubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "$,", "NN", "APPR", "ART", "NN", "KON", "VVPP", "$."], "meter": "-+--+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Briefschaften kn\u00f6pft' er aus dem Wams: \u2013 Ei, wiederum ein Ostseestreich? \u2013", "tokens": ["Brief\u00b7schaf\u00b7ten", "kn\u00f6pft'", "er", "aus", "dem", "Wams", ":", "\u2013", "Ei", ",", "wie\u00b7de\u00b7rum", "ein", "Ost\u00b7see\u00b7streich", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$(", "NN", "$,", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der hohe J\u00e4ger ri\u00df sie auf; er flog sie durch; er wurde bleich.", "tokens": ["Der", "ho\u00b7he", "J\u00e4\u00b7ger", "ri\u00df", "sie", "auf", ";", "er", "flog", "sie", "durch", ";", "er", "wur\u00b7de", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Auf seiner Stirne zuckt' empor gehemmter arger Groll:", "tokens": ["Auf", "sei\u00b7ner", "Stir\u00b7ne", "zuckt'", "em\u00b7por", "ge\u00b7hemm\u00b7ter", "ar\u00b7ger", "Groll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "\u00bbdas war dein letzter Widerspruch! Hochnasig Volk, dein Ma\u00df ist voll!", "tokens": ["\u00bb", "das", "war", "dein", "letz\u00b7ter", "Wi\u00b7der\u00b7spruch", "!", "Hoch\u00b7na\u00b7sig", "Volk", ",", "dein", "Ma\u00df", "ist", "voll", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "ADJD", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "So wahr ich jetzt den Apfel hier\u00ab \u2013 Und siehe da, vom vollen Tisch", "tokens": ["So", "wahr", "ich", "jetzt", "den", "Ap\u00b7fel", "hier", "\u00ab", "\u2013", "Und", "sie\u00b7he", "da", ",", "vom", "vol\u00b7len", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "ADV", "$(", "$(", "KON", "VVIMP", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Rafft'er mit ungest\u00fcmer Hans sich einen Apfel rot und frisch! \u2013", "tokens": ["Rafft'\u00b7er", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Hans", "sich", "ei\u00b7nen", "Ap\u00b7fel", "rot", "und", "frisch", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ADJA", "NE", "PRF", "ART", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "\u00bbso wahr ich den durchs fenster jetzt fortschleudre weit ins Freie hin,", "tokens": ["\u00bb", "so", "wahr", "ich", "den", "durchs", "fens\u00b7ter", "jetzt", "fort\u00b7schleud\u00b7re", "weit", "ins", "Frei\u00b7e", "hin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "ART", "APPRART", "NN", "ADV", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "So wahr noch brech' ich Preu\u00dfens Trotz, brech' ich der Ostsee Eigensinn!", "tokens": ["So", "wahr", "noch", "brech'", "ich", "Preu\u00b7\u00dfens", "Trotz", ",", "brech'", "ich", "der", "Ost\u00b7see", "Ei\u00b7gen\u00b7sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "NE", "NE", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "So wahr noch soll als Oberherrn mich diese Bersteink\u00fcste sehn!", "tokens": ["So", "wahr", "noch", "soll", "als", "O\u00b7ber\u00b7herrn", "mich", "die\u00b7se", "Bers\u00b7tein\u00b7k\u00fcs\u00b7te", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VMFIN", "KOUS", "NN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "So wahr noch unterwerf' ich mir dies \u00fcberm\u00fct'ge Polenlehn!", "tokens": ["So", "wahr", "noch", "un\u00b7ter\u00b7wer\u00b7f'", "ich", "mir", "dies", "\u00fc\u00b7ber\u00b7m\u00fct'\u00b7ge", "Po\u00b7len\u00b7lehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "PPER", "PDS", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "So wahr noch \u2013\u00ab Und er sprang empor! Ausholt' er wild zum Wurfe dann!", "tokens": ["So", "wahr", "noch", "\u2013", "\u00ab", "Und", "er", "sprang", "em\u00b7por", "!", "Aus\u00b7holt'", "er", "wild", "zum", "Wur\u00b7fe", "dann", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$(", "$(", "KON", "PPER", "VVFIN", "PTKVZ", "$.", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wer mit am Tisch sa\u00df, duckte sich und hielt gespannt den Atem an.", "tokens": ["Wer", "mit", "am", "Tisch", "sa\u00df", ",", "duck\u00b7te", "sich", "und", "hielt", "ge\u00b7spannt", "den", "A\u00b7tem", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "PRF", "KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Der Apfel flog \u2013 fort in den Wald? \u2013 Nicht doch, fehl warf die hohe Kur!", "tokens": ["Der", "Ap\u00b7fel", "flog", "\u2013", "fort", "in", "den", "Wald", "?", "\u2013", "Nicht", "doch", ",", "fehl", "warf", "die", "ho\u00b7he", "Kur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PTKVZ", "APPR", "ART", "NN", "$.", "$(", "PTKNEG", "ADV", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-++--+-++--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Hinflog er sausend durchs Gemach und \u2013 traf das Kreuz des Fensters nur!", "tokens": ["Hin\u00b7flog", "er", "sau\u