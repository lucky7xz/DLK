{"dta.poem.10620": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma au\u00df dem Hol\u00e4ndischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dieweil man mu\u00df/ zukriegen Himmlisch Gut/", "tokens": ["Die\u00b7weil", "man", "mu\u00df", "/", "zu\u00b7krie\u00b7gen", "Himm\u00b7lisch", "Gut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "$(", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00fcnde beichten vnd beklagen/", "tokens": ["Die", "S\u00fcn\u00b7de", "beich\u00b7ten", "vnd", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd zu entfliehn der H\u00f6llen heisse Glut", "tokens": ["Vnd", "zu", "ent\u00b7fliehn", "der", "H\u00f6l\u00b7len", "heis\u00b7se", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In seinem Hertzen Rewe tragen/", "tokens": ["In", "sei\u00b7nem", "Hert\u00b7zen", "Re\u00b7we", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bitt ich doch/ O meines Lebens schein/", "tokens": ["So", "bitt", "ich", "doch", "/", "O", "mei\u00b7nes", "Le\u00b7bens", "schein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "NE", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gebt mir mein Hertz/ das ich verlie\u00df bey euch/", "tokens": ["Gebt", "mir", "mein", "Hertz", "/", "das", "ich", "ver\u00b7lie\u00df", "bey", "euch", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wolt aber jhrs behalten ja allein/", "tokens": ["Wolt", "a\u00b7ber", "jhrs", "be\u00b7hal\u00b7ten", "ja", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So beichtet mein vnd ewre S\u00fcnd zugleich.", "tokens": ["So", "beich\u00b7tet", "mein", "vnd", "ew\u00b7re", "S\u00fcnd", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}