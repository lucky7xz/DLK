{"dta.poem.17329": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "179.  Wie GOtt so hertzlich liebt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott liebt so hertzlich dich; Er w\u00fcrde sich betr\u00fcben/", "tokens": ["Gott", "liebt", "so", "hertz\u00b7lich", "dich", ";", "Er", "w\u00fcr\u00b7de", "sich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "PPER", "$.", "PPER", "VAFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm fall es m\u00f6glich w\u00e4r/ da\u00df du Jhn nicht wilt lieben.", "tokens": ["Jm", "fall", "es", "m\u00f6g\u00b7lich", "w\u00e4r", "/", "da\u00df", "du", "Jhn", "nicht", "wilt", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "VAFIN", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}