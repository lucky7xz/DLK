{"textgrid.poem.61027": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Tag geschieden", "tokens": ["Nun", "ist", "der", "Tag", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinem Drang und Schall,", "tokens": ["Mit", "sei\u00b7nem", "Drang", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es weht ein k\u00fchler Frieden", "tokens": ["Es", "weht", "ein", "k\u00fch\u00b7ler", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchs Dunkel \u00fcberall.", "tokens": ["Durchs", "Dun\u00b7kel", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie still die Felder liegen!", "tokens": ["Wie", "still", "die", "Fel\u00b7der", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wald nur ist erwacht,", "tokens": ["Der", "Wald", "nur", "ist", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was er dem Lichte verschwiegen,", "tokens": ["Und", "was", "er", "dem", "Lich\u00b7te", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das singt er leise der Nacht.", "tokens": ["Das", "singt", "er", "lei\u00b7se", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und was ich am lauten Tage", "tokens": ["Und", "was", "ich", "am", "lau\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir nimmer sagen kann,", "tokens": ["Dir", "nim\u00b7mer", "sa\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun m\u00f6cht' ich dir's sagen und klagen -", "tokens": ["Nun", "m\u00f6cht'", "ich", "dir's", "sa\u00b7gen", "und", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "KON", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O komm und h\u00f6r' mich an!", "tokens": ["O", "komm", "und", "h\u00f6r'", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}