{"textgrid.poem.61401": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wie K\u00f6nig Sigurd Alfsonnen traf", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lenz war gekommen. Der lichte Schnee zerschmolz", "tokens": ["Lenz", "war", "ge\u00b7kom\u00b7men", ".", "Der", "lich\u00b7te", "Schnee", "zer\u00b7schmolz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "$.", "ART", "ADJA", "NN", "NE"], "meter": "+--+----+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "An den Bergeshalden, in Veilchen stand das Holz;", "tokens": ["An", "den", "Ber\u00b7ges\u00b7hal\u00b7den", ",", "in", "Veil\u00b7chen", "stand", "das", "Holz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NE", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die blaue Meereswoge gl\u00e4nzte frei von Eis,", "tokens": ["Die", "blau\u00b7e", "Mee\u00b7res\u00b7wo\u00b7ge", "gl\u00e4nz\u00b7te", "frei", "von", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da ging zu Schiffe Sigurd, der k\u00f6nigliche Greis.", "tokens": ["Da", "ging", "zu", "Schif\u00b7fe", "Si\u00b7gurd", ",", "der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Greis", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Umfahrt wollt' er halten von Upsalas Strand", "tokens": ["Um\u00b7fahrt", "wollt'", "er", "hal\u00b7ten", "von", "Up\u00b7sa\u00b7las", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VMFIN", "PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Entlang die hohen K\u00fcsten, da\u00df \u00fcberall am Land", "tokens": ["Ent\u00b7lang", "die", "ho\u00b7hen", "K\u00fcs\u00b7ten", ",", "da\u00df", "\u00fc\u00b7be\u00b7rall", "am", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er n\u00e4hme Scho\u00df und Gaben und mit Spruch und Schwert", "tokens": ["Er", "n\u00e4h\u00b7me", "Scho\u00df", "und", "Ga\u00b7ben", "und", "mit", "Spruch", "und", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des alten Rechtes pflegte, so jemand h\u00e4tte des begehrt.", "tokens": ["Des", "al\u00b7ten", "Rech\u00b7tes", "pfleg\u00b7te", ",", "so", "je\u00b7mand", "h\u00e4t\u00b7te", "des", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "PIS", "VAFIN", "ART", "VVPP", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Es war der neunte Morgen, seit die Fahrt begann;", "tokens": ["Es", "war", "der", "neun\u00b7te", "Mor\u00b7gen", ",", "seit", "die", "Fahrt", "be\u00b7gann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehalten war der Fr\u00fchtrunk von Skald' und Rittersmann,", "tokens": ["Ge\u00b7hal\u00b7ten", "war", "der", "Fr\u00fch\u00b7trunk", "von", "Skald'", "und", "Rit\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Segel und die Taue rauschten allzumal", "tokens": ["Die", "Se\u00b7gel", "und", "die", "Tau\u00b7e", "rauschten", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vom lauen Maienwinde: da kamen sie gen Skiris-Sal.", "tokens": ["Vom", "lau\u00b7en", "Mai\u00b7en\u00b7win\u00b7de", ":", "da", "ka\u00b7men", "sie", "gen", "Ski\u00b7ris\u00b7Sal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Als das Schiff gelandet, da sprach der K\u00f6nig gut:", "tokens": ["Als", "das", "Schiff", "ge\u00b7lan\u00b7det", ",", "da", "sprach", "der", "K\u00f6\u00b7nig", "gut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "\u00bbwie singt mein Herz so fr\u00f6hlich, wie fleugt so hoch mein Mut!", "tokens": ["\u00bb", "wie", "singt", "mein", "Herz", "so", "fr\u00f6h\u00b7lich", ",", "wie", "fleugt", "so", "hoch", "mein", "Mut", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,", "PWAV", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich wei\u00df nicht, tut's der Fr\u00fchling, oder tut's der Wein;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "tut's", "der", "Fr\u00fch\u00b7ling", ",", "o\u00b7der", "tut's", "der", "Wein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "ART", "NN", "$,", "KON", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir ist, als sollt' ich heute ein J\u00fcngling wieder sein.\u00ab", "tokens": ["Mir", "ist", ",", "als", "sollt'", "ich", "heu\u00b7te", "ein", "J\u00fcng\u00b7ling", "wie\u00b7der", "sein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "VAINF", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie schritten hastig f\u00fcrder auf gelbem Ufersand,", "tokens": ["Sie", "schrit\u00b7ten", "has\u00b7tig", "f\u00fcr\u00b7der", "auf", "gel\u00b7bem", "U\u00b7fer\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Land in acht zu nehmen; da trafen sie am Strand", "tokens": ["Das", "Land", "in", "acht", "zu", "neh\u00b7men", ";", "da", "tra\u00b7fen", "sie", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "CARD", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Eine Schar von M\u00e4gden, die am Erlenbusch,", "tokens": ["Ei\u00b7ne", "Schar", "von", "M\u00e4g\u00b7den", ",", "die", "am", "Er\u00b7len\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wo in das Meer ein Bach ging, Gewand und Linnen wusch.", "tokens": ["Wo", "in", "das", "Meer", "ein", "Bach", "ging", ",", "Ge\u00b7wand", "und", "Lin\u00b7nen", "wusch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Es lachten und es sangen bei der Arbeit frei", "tokens": ["Es", "lach\u00b7ten", "und", "es", "san\u00b7gen", "bei", "der", "Ar\u00b7beit", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die frohgemuten Dirnen, eine Jungfrau stand dabei;", "tokens": ["Die", "froh\u00b7ge\u00b7mu\u00b7ten", "Dir\u00b7nen", ",", "ei\u00b7ne", "Jung\u00b7frau", "stand", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Aller Herrin schien sie, da sie des Werks verga\u00df.", "tokens": ["Al\u00b7ler", "Her\u00b7rin", "schien", "sie", ",", "da", "sie", "des", "Werks", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auch trug sie g\u00fcldne Spangen; ein Falk auf ihrer Schulter sa\u00df.", "tokens": ["Auch", "trug", "sie", "g\u00fcld\u00b7ne", "Span\u00b7gen", ";", "ein", "Falk", "auf", "ih\u00b7rer", "Schul\u00b7ter", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie stand in s\u00fc\u00dfer Jugend; ihr rosig Antlitz war", "tokens": ["Sie", "stand", "in", "s\u00fc\u00b7\u00dfer", "Ju\u00b7gend", ";", "ihr", "ro\u00b7sig", "Ant\u00b7litz", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "PPER", "ADJD", "NN", "VAFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie die Morgenfr\u00fche, es flo\u00df ihr goldenes Haar", "tokens": ["Wie", "die", "Mor\u00b7gen\u00b7fr\u00fc\u00b7he", ",", "es", "flo\u00df", "ihr", "gol\u00b7de\u00b7nes", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In langen Ringeln schimmernd herab auf ihr Gewand,", "tokens": ["In", "lan\u00b7gen", "Rin\u00b7geln", "schim\u00b7mernd", "her\u00b7ab", "auf", "ihr", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df schier der lichten Spangen Gefunkel davor schwand.", "tokens": ["Da\u00df", "schier", "der", "lich\u00b7ten", "Span\u00b7gen", "Ge\u00b7fun\u00b7kel", "da\u00b7vor", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Da dachte Sigurd bei sich in seinem Sinn:", "tokens": ["Da", "dach\u00b7te", "Si\u00b7gurd", "bei", "sich", "in", "sei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbholdselig ist die Jungfrau, so wahr ich K\u00f6nig bin!", "tokens": ["\u00bb", "hold\u00b7se\u00b7lig", "ist", "die", "Jung\u00b7frau", ",", "so", "wahr", "ich", "K\u00f6\u00b7nig", "bin", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "NN", "VAFIN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Trotz meiner achtzig Jahre die f\u00fchr' ich heim als Braut.", "tokens": ["Trotz", "mei\u00b7ner", "acht\u00b7zig", "Jah\u00b7re", "die", "f\u00fchr'", "ich", "heim", "als", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "ART", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sonst bricht mein Herz vor Liebe.\u00ab Doch sagt' er das nicht laut.", "tokens": ["Sonst", "bricht", "mein", "Herz", "vor", "Lie\u00b7be", ".", "\u00ab", "Doch", "sagt'", "er", "das", "nicht", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$.", "$(", "KON", "VVFIN", "PPER", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Nach ihr den Skalden fragt' er. Der sprach: \u00bbHerr K\u00f6nig, wi\u00dft,", "tokens": ["Nach", "ihr", "den", "Skal\u00b7den", "fragt'", "er", ".", "Der", "sprach", ":", "\u00bb", "Herr", "K\u00f6\u00b7nig", ",", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "PPER", "$.", "ART", "VVFIN", "$.", "$(", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df sie Alfs des Weisen vielhohe Tochter ist;", "tokens": ["Da\u00df", "sie", "Alfs", "des", "Wei\u00b7sen", "viel\u00b7ho\u00b7he", "Toch\u00b7ter", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ihr Leib ist frisch und wonnig, die sch\u00f6nste wohl im Land,", "tokens": ["Ihr", "Leib", "ist", "frisch", "und", "won\u00b7nig", ",", "die", "sch\u00f6ns\u00b7te", "wohl", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PRELS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihr Goldhaar strahlt sonnig. Alf-Sonne ist sie drum genannt.", "tokens": ["Ihr", "Gold\u00b7haar", "strahlt", "son\u00b7nig", ".", "Al\u00b7fSon\u00b7ne", "ist", "sie", "drum", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$.", "NE", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "Mit wundersamer Tugend geg\u00fcrtet ist die Maid;", "tokens": ["Mit", "wun\u00b7der\u00b7sa\u00b7mer", "Tu\u00b7gend", "ge\u00b7g\u00fcr\u00b7tet", "ist", "die", "Maid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es pflegen ihrer Jugend ihre Br\u00fcder beid',", "tokens": ["Es", "pfle\u00b7gen", "ih\u00b7rer", "Ju\u00b7gend", "ih\u00b7re", "Br\u00fc\u00b7der", "beid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alf gehei\u00dfen Blondbart und Erek Harfenschall,", "tokens": ["Alf", "ge\u00b7hei\u00b7\u00dfen", "Blond\u00b7bart", "und", "E\u00b7rek", "Har\u00b7fen\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "NE", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Seit Alf der Weise zechet mit Odin in Walhall.\u00ab", "tokens": ["Sei