{"textgrid.poem.40603": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Widmung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ich einst spazieren ging,", "tokens": ["Der", "ich", "einst", "spa\u00b7zie\u00b7ren", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Raste nun in gr\u00fcnen Lauben;", "tokens": ["Ras\u00b7te", "nun", "in", "gr\u00fc\u00b7nen", "Lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem wechselvollen Ring", "tokens": ["In", "dem", "wech\u00b7sel\u00b7vol\u00b7len", "Ring"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb mir Eines doch: mein Glauben!", "tokens": ["Blieb", "mir", "Ei\u00b7nes", "doch", ":", "mein", "Glau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Glauben an die Sonnenkraft,", "tokens": ["Glau\u00b7ben", "an", "die", "Son\u00b7nen\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die im Menschengeiste lodert;", "tokens": ["Die", "im", "Men\u00b7schen\u00b7geis\u00b7te", "lo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glauben an den Lenz in Haft,", "tokens": ["Glau\u00b7ben", "an", "den", "Lenz", "in", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sein Recht des Freien fodert;", "tokens": ["Der", "sein", "Recht", "des", "Frei\u00b7en", "fo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Glauben an das Vaterland,", "tokens": ["Glau\u00b7ben", "an", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An das gro\u00dfe, deutsche, Eine,", "tokens": ["An", "das", "gro\u00b7\u00dfe", ",", "deut\u00b7sche", ",", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob auf ein geri\u00dfnes Band", "tokens": ["Ob", "auf", "ein", "ge\u00b7ri\u00df\u00b7nes", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heute noch manch Auge weine.", "tokens": ["Heu\u00b7te", "noch", "manch", "Au\u00b7ge", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Vor mir liegt, wie sonst, das Feld,", "tokens": ["Vor", "mir", "liegt", ",", "wie", "sonst", ",", "das", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PWAV", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch kein Halm ist mehr der alte;", "tokens": ["Doch", "kein", "Halm", "ist", "mehr", "der", "al\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Andre Saat ist ihm bestellt,", "tokens": ["And\u00b7re", "Saat", "ist", "ihm", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es andre Ernten halte.", "tokens": ["Da\u00df", "es", "and\u00b7re", "Ern\u00b7ten", "hal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier noch rauscht im Thal der Flu\u00df,", "tokens": ["Hier", "noch", "rauscht", "im", "Thal", "der", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Noch derselb' und doch ein andrer,", "tokens": ["Noch", "der\u00b7selb'", "und", "doch", "ein", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der stets fliehn, stets bleiben mu\u00df,", "tokens": ["Der", "stets", "fliehn", ",", "stets", "blei\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Well' ein fl\u00fccht'ger Wandrer!", "tokens": ["Je\u00b7de", "Well'", "ein", "fl\u00fccht'\u00b7ger", "Wand\u00b7rer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von Granit der Alpen Wand", "tokens": ["Von", "Gra\u00b7nit", "der", "Al\u00b7pen", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort am Thalsaum, wie seit Jahren;", "tokens": ["Dort", "am", "Thal\u00b7saum", ",", "wie", "seit", "Jah\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wie oft ihr Laubgewand", "tokens": ["Doch", "wie", "oft", "ihr", "Laub\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Tauschten die Unwandelbaren!", "tokens": ["Tauschten", "die", "Un\u00b7wan\u00b7del\u00b7ba\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Ueber mir in festem Fug", "tokens": ["Ue\u00b7ber", "mir", "in", "fes\u00b7tem", "Fug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die gew\u00f6lbte Himmelshalle;", "tokens": ["Die", "ge\u00b7w\u00f6lb\u00b7te", "Him\u00b7mels\u00b7hal\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sternenzug und Wolkenflug,", "tokens": ["Ster\u00b7nen\u00b7zug", "und", "Wol\u00b7ken\u00b7flug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wechseln all' und wandern alle!", "tokens": ["Wech\u00b7seln", "all'", "und", "wan\u00b7dern", "al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KON", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr Gesetz \u00fcbt die Natur", "tokens": ["Ihr", "Ge\u00b7setz", "\u00fcbt", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unerbittlich und gewaltsam;", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich", "und", "ge\u00b7walt\u00b7sam", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch mein Herz auch zieht die Spur", "tokens": ["Durch", "mein", "Herz", "auch", "zieht", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ew'gen Wandels unaufhaltsam.", "tokens": ["Ew'\u00b7gen", "Wan\u00b7dels", "un\u00b7auf\u00b7halt\u00b7sam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "An dem Ast im Laubgewind'", "tokens": ["An", "dem", "Ast", "im", "Laub\u00b7ge\u00b7wind'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00df ich meine Harfe h\u00e4ngen;", "tokens": ["Lie\u00df", "ich", "mei\u00b7ne", "Har\u00b7fe", "h\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00e4mm'rung wirds; der Abendwind", "tokens": ["D\u00e4m\u00b7m'\u00b7rung", "wirds", ";", "der", "Ab\u00b7end\u00b7wind"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "$.", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Streift und weckt sie noch zu Kl\u00e4ngen.", "tokens": ["Streift", "und", "weckt", "sie", "noch", "zu", "Kl\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Klang von Bechern, l\u00e4ngst geleert,", "tokens": ["Klang", "von", "Be\u00b7chern", ",", "l\u00e4ngst", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fernen Donners harmlos Rollen,", "tokens": ["Fer\u00b7nen", "Don\u00b7ners", "harm\u00b7los", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klang der Zeit, die nimmer kehrt,", "tokens": ["Klang", "der", "Zeit", ",", "die", "nim\u00b7mer", "kehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Altes Lieben, altes Grollen.", "tokens": ["Al\u00b7tes", "Lie\u00b7ben", ",", "al\u00b7tes", "Grol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn ihr Ton als Pfeil sich schwingt,", "tokens": ["Wenn", "ihr", "Ton", "als", "Pfeil", "sich", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KOUS", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trifft er nimmer Ziel und Feinde;", "tokens": ["Trifft", "er", "nim\u00b7mer", "Ziel", "und", "Fein\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er mild wie Glocken klingt,", "tokens": ["Wenn", "er", "mild", "wie", "Glo\u00b7cken", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fehlt dem Rufe die Gemeinde.", "tokens": ["Fehlt", "dem", "Ru\u00b7fe", "die", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Dort und da vielleicht von fern", "tokens": ["Dort", "und", "da", "viel\u00b7leicht", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ADV", "APPR", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt ein Graukopf halbverdrossen;", "tokens": ["Kommt", "ein", "Grau\u00b7kopf", "halb\u00b7ver\u00b7dros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst, wie lauschten mir so gern", "tokens": ["Einst", ",", "wie", "lauschten", "mir", "so", "gern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Meines Morgenlieds Genossen!", "tokens": ["Mei\u00b7nes", "Mor\u00b7gen\u00b7lieds", "Ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Nimmer h\u00f6ren sie den Ton,", "tokens": ["Nim\u00b7mer", "h\u00f6\u00b7ren", "sie", "den", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Gebraus der Lebenswogen;", "tokens": ["Das", "Ge\u00b7braus", "der", "Le\u00b7bens\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben Schlummerdecken schon", "tokens": ["Ha\u00b7ben", "Schlum\u00b7mer\u00b7de\u00b7cken", "schon"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Haupt und Brust gezogen.", "tokens": ["Ue\u00b7ber", "Haupt", "und", "Brust", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "An den Dom zur Leidenszeit", "tokens": ["An", "den", "Dom", "zur", "Lei\u00b7dens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnt in Wehmut mich die\u00df Wandern,", "tokens": ["Mahnt", "in", "Weh\u00b7mut", "mich", "die\u00df", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie Kerzen lichtgereiht", "tokens": ["Wenn", "sie", "Ker\u00b7zen", "licht\u00b7ge\u00b7reiht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine l\u00f6schen nach der andern.", "tokens": ["Ei\u00b7ne", "l\u00f6\u00b7schen", "nach", "der", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Flackernd tropft die letzte ab,", "tokens": ["Fla\u00b7ckernd", "tropft", "die", "letz\u00b7te", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von Thr\u00e4nenfall befeuchtet;", "tokens": ["Wie", "von", "Thr\u00e4\u00b7nen\u00b7fall", "be\u00b7feuch\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so l\u00f6schte mir das Grab", "tokens": ["Ach", ",", "so", "l\u00f6schte", "mir", "das", "Grab"], "token_info": [