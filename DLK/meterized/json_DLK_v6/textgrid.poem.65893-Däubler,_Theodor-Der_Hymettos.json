{"textgrid.poem.65893": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Hymettos", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Winde Fichten sind Poseidons Gabe", "tokens": ["Im", "Win\u00b7de", "Fich\u00b7ten", "sind", "Po\u00b7sei\u00b7dons", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "VAFIN", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Zeus, der den Hymettos grau umdr\u00e4ut,", "tokens": ["An", "Zeus", ",", "der", "den", "Hy\u00b7met\u00b7tos", "grau", "um\u00b7dr\u00e4ut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Weil er mit Wolkungen den Berg betreut,", "tokens": ["Weil", "er", "mit", "Wol\u00b7kun\u00b7gen", "den", "Berg", "be\u00b7treut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und um Gewitterk\u00fcnfte kreist der Rabe.", "tokens": ["Und", "um", "Ge\u00b7wit\u00b7ter\u00b7k\u00fcnf\u00b7te", "kreist", "der", "Ra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df Regengegenwart den Bauern labe,", "tokens": ["Da\u00df", "Re\u00b7gen\u00b7ge\u00b7gen\u00b7wart", "den", "Bau\u00b7ern", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward Zeus ein Heiligtum, das ihn erfreut,", "tokens": ["Ward", "Zeus", "ein", "Hei\u00b7lig\u00b7tum", ",", "das", "ihn", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Gipfelhaupt, wo er dem Blitz gebeut,", "tokens": ["Als", "Gip\u00b7fel\u00b7haupt", ",", "wo", "er", "dem", "Blitz", "ge\u00b7beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Emporgereicht aus des Atheners Habe.", "tokens": ["Em\u00b7por\u00b7ge\u00b7reicht", "aus", "des", "A\u00b7the\u00b7ners", "Ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Bei seiner Ahnenkammer unterm Grabe,", "tokens": ["Bei", "sei\u00b7ner", "Ah\u00b7nen\u00b7kam\u00b7mer", "un\u00b7term", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besummt von goldner Bienen Schwebekranz,", "tokens": ["Be\u00b7summt", "von", "gold\u00b7ner", "Bie\u00b7nen", "Schwe\u00b7be\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stand, als ich ankam, ein gewogner Knabe.", "tokens": ["Stand", ",", "als", "ich", "an\u00b7kam", ",", "ein", "ge\u00b7wog\u00b7ner", "Kna\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er w\u00e4hnte wohl des eignen Blutes Glanz,", "tokens": ["Er", "w\u00e4hn\u00b7te", "wohl", "des", "eig\u00b7nen", "Blu\u00b7tes", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bedachte uns mit S\u00fc\u00dfe seiner Wabe \u2013", "tokens": ["Be\u00b7dach\u00b7te", "uns", "mit", "S\u00fc\u00b7\u00dfe", "sei\u00b7ner", "Wa\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Attika \u2013 und du umfingst mich ganz.", "tokens": ["Mein", "At\u00b7ti\u00b7ka", "\u2013", "und", "du", "um\u00b7fingst", "mich", "ganz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}