{"dta.poem.9886": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "S. K.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zeuch/ Cynthia! zeuch deine strahlen ein/", "tokens": ["Zeuch", "/", "Cyn\u00b7thia", "!", "zeuch", "dei\u00b7ne", "strah\u00b7len", "ein", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$.", "VVIMP", "PPOSAT", "VVFIN", "ART", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich will hinfort ja dein gefangner seyn/", "tokens": ["Ich", "will", "hin\u00b7fort", "ja", "dein", "ge\u00b7fang\u00b7ner", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPOSAT", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ist umsonst da\u00df man den feind bekriegt/", "tokens": ["Es", "ist", "um\u00b7sonst", "da\u00df", "man", "den", "feind", "be\u00b7kriegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PIS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ohne di\u00df schon untern f\u00fcssen liegt.", "tokens": ["Der", "oh\u00b7ne", "di\u00df", "schon", "un\u00b7tern", "f\u00fcs\u00b7sen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDS", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein hoher geist und meine niedrigkeit/", "tokens": ["Dein", "ho\u00b7her", "geist", "und", "mei\u00b7ne", "nied\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die stecken mir der liebe ziel zu weit/", "tokens": ["Die", "ste\u00b7cken", "mir", "der", "lie\u00b7be", "ziel", "zu", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die bl\u00f6de scham/ der liebe widerpart/", "tokens": ["Die", "bl\u00f6\u00b7de", "scham", "/", "der", "lie\u00b7be", "wi\u00b7der\u00b7part", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Macht/ da\u00df ich dir nicht eh hab auffgewarte.", "tokens": ["Macht", "/", "da\u00df", "ich", "dir", "nicht", "eh", "hab", "auff\u00b7ge\u00b7war\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "KOUS", "NE", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Jtzt seh ich erst/ wenn ein verzagter liebt/", "tokens": ["Jtzt", "seh", "ich", "erst", "/", "wenn", "ein", "ver\u00b7zag\u00b7ter", "liebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie er vers\u00e4umt was ihm das gl\u00fccke giebt.", "tokens": ["Wie", "er", "ver\u00b7s\u00e4umt", "was", "ihm", "das", "gl\u00fc\u00b7cke", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weg! bl\u00f6digkeit/ ich bin mir selber feind/", "tokens": ["Weg", "!", "bl\u00f6\u00b7dig\u00b7keit", "/", "ich", "bin", "mir", "sel\u00b7ber", "feind", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$(", "PPER", "VAFIN", "PPER", "ADV", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ichs nicht eh mit dir hab gut gemeint.", "tokens": ["Da\u00df", "ichs", "nicht", "eh", "mit", "dir", "hab", "gut", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "KOUS", "APPR", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00df deine gunst die du mir kund gethan/", "tokens": ["La\u00df", "dei\u00b7ne", "gunst", "die", "du", "mir", "kund", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "PPER", "PPER", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hertzliebstes kind/ auffs neue fangen an/", "tokens": ["Hertz\u00b7liebs\u00b7tes", "kind", "/", "auffs", "neu\u00b7e", "fan\u00b7gen", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "APPRART", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wirfft sich dir der ausversch\u00e4mte sinn/", "tokens": ["Es", "wirfft", "sich", "dir", "der", "aus\u00b7ver\u00b7sch\u00e4m\u00b7te", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Cynthia! gantz willig f\u00fcr dich hin.", "tokens": ["O", "Cyn\u00b7thia", "!", "gantz", "wil\u00b7lig", "f\u00fcr", "dich", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Der hohe fels der meine qual geh\u00f6rt/", "tokens": ["Der", "ho\u00b7he", "fels", "der", "mei\u00b7ne", "qual", "ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wei\u00df sehr wohl/ wie ich mich abgezehrt.", "tokens": ["Der", "wei\u00df", "sehr", "wohl", "/", "wie", "ich", "mich", "ab\u00b7ge\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$(", "PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach Cynthia! ach was ist das vor pein!", "tokens": ["Ach", "Cyn\u00b7thia", "!", "ach", "was", "ist", "das", "vor", "pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ITJ", "PWS", "VAFIN", "ART", "APPR", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So sehr verliebt und nicht behertzt zu seyn.", "tokens": ["So", "sehr", "ver\u00b7liebt", "und", "nicht", "be\u00b7hertzt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "PTKNEG", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nun bin ich frey/ indem ich nicht mehr frey/", "tokens": ["Nun", "bin", "ich", "frey", "/", "in\u00b7dem", "ich", "nicht", "mehr", "frey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jm fall mir nur dein mund gewogen sey.", "tokens": ["Jm", "fall", "mir", "nur", "dein", "mund", "ge\u00b7wo\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich schwer dir treu/ ich schwere dir bestand:", "tokens": ["Ich", "schwer", "dir", "treu", "/", "ich", "schwe\u00b7re", "dir", "be\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPER", "ADJD", "$(", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sieh Cynthia! hier hast du hertz und hand.", "tokens": ["Sieh", "Cyn\u00b7thia", "!", "hier", "hast", "du", "hertz", "und", "hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Gib mir hingegen die genaden-hand/", "tokens": ["Gib", "mir", "hin\u00b7ge\u00b7gen", "die", "ge\u00b7na\u00b7den\u00b7hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Den sch\u00f6nen blick/ daran ich mich verbrand/", "tokens": ["Den", "sch\u00f6\u00b7nen", "blick", "/", "da\u00b7ran", "ich", "mich", "ver\u00b7brand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PAV", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df da\u00df du nicht blo\u00df nur sch\u00f6ne bist/", "tokens": ["Ich", "wei\u00df", "da\u00df", "du", "nicht", "blo\u00df", "nur", "sch\u00f6\u00b7ne", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df freundligkeit der sch\u00f6nheit boden ist.", "tokens": ["Da\u00df", "freund\u00b7lig\u00b7keit", "der", "sch\u00f6n\u00b7heit", "bo\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}