{"dta.poem.21611": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchl ich, o Hehrste, dich", "tokens": ["F\u00fchl", "ich", ",", "o", "Hehrs\u00b7te", ",", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$,", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor\u00fcbergehen,", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "F\u00e4llt\u2019s wild in meine Brust", "tokens": ["F\u00e4llt's", "wild", "in", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie Sturmes Wehen.", "tokens": ["Wie", "Stur\u00b7mes", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ein Beben fa\u00dft mein Herz,", "tokens": ["Ein", "Be\u00b7ben", "fa\u00dft", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein banges Dr\u00e4ngen,", "tokens": ["Ein", "ban\u00b7ges", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und jeden Widerstand", "tokens": ["Und", "je\u00b7den", "Wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "M\u00f6cht\u2019 es zersprengen.", "tokens": ["M\u00f6cht'", "es", "zer\u00b7spren\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Es facht mein Feuer an", "tokens": ["Es", "facht", "mein", "Feu\u00b7er", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu hellen Flammen,", "tokens": ["Zu", "hel\u00b7len", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auflodert all mein Muth", "tokens": ["Auf\u00b7lo\u00b7dert", "all", "mein", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und bricht zusammen.", "tokens": ["Und", "bricht", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du weichst \u2014 ich seh\u2019 den Staub", "tokens": ["Du", "weichst", "ich", "seh'", "den", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Noch deinen Fu\u00df umkr\u00e4useln, \u2014", "tokens": ["Noch", "dei\u00b7nen", "Fu\u00df", "um\u00b7kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch die Seele zieht\u2019s", "tokens": ["Und", "durch", "die", "See\u00b7le", "zieht's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie sanftes S\u00e4useln.", "tokens": ["Wie", "sanf\u00b7tes", "S\u00e4u\u00b7seln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}