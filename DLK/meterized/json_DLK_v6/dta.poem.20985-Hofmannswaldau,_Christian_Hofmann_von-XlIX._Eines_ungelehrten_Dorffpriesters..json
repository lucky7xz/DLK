{"dta.poem.20985": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.  \n Eines ungelehrten Dorffpriesters.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Frembder in der Schrifft/ ein B\u00fcrger in Po-", "tokens": ["Ein", "Fremb\u00b7der", "in", "der", "Schrifft", "/", "ein", "B\u00fcr\u00b7ger", "in", "Po"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "stillen/", "tokens": ["stil\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mu\u00df diesen engen Raum mit seinem Leibe f\u00fcllen;", "tokens": ["Mu\u00df", "die\u00b7sen", "en\u00b7gen", "Raum", "mit", "sei\u00b7nem", "Lei\u00b7be", "f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es weint das gantze Dorff/ es schalt in allen Ohren/", "tokens": ["Es", "weint", "das", "gant\u00b7ze", "Dorff", "/", "es", "schalt", "in", "al\u00b7len", "Oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Kretschem und Altar hat seinen Schatz ver-", "tokens": ["Der", "Kret\u00b7schem", "und", "Al\u00b7tar", "hat", "sei\u00b7nen", "Schatz", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "lohren.", "tokens": ["loh\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}