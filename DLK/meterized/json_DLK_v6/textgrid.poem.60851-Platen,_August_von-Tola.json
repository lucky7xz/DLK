{"textgrid.poem.60851": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Tola", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich in der Bl\u00fcte der Jugend erschlug die bezepterte Memme;", "tokens": ["Dich", "in", "der", "Bl\u00fc\u00b7te", "der", "Ju\u00b7gend", "er\u00b7schlug", "die", "be\u00b7zep\u00b7ter\u00b7te", "Mem\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Doch du erwartetest voll Ruhe das t\u00f6dliche Blei.", "tokens": ["Doch", "du", "er\u00b7war\u00b7te\u00b7test", "voll", "Ru\u00b7he", "das", "t\u00f6d\u00b7li\u00b7che", "Blei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf die verlassene Gruft warf n\u00e4chtliche Kr\u00e4nze die Freundschaft,", "tokens": ["Auf", "die", "ver\u00b7las\u00b7se\u00b7ne", "Gruft", "warf", "n\u00e4cht\u00b7li\u00b7che", "Kr\u00e4n\u00b7ze", "die", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Einer Antigone Hand malte die Worte darauf:", "tokens": ["Ei\u00b7ner", "An\u00b7ti\u00b7go\u00b7ne", "Hand", "mal\u00b7te", "die", "Wor\u00b7te", "da\u00b7rauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "\u00bbschlummer in Frieden, o Tola, die Rache befl\u00fcgelt den Schritt schon!\u00ab", "tokens": ["\u00bb", "schlum\u00b7mer", "in", "Frie\u00b7den", ",", "o", "To\u00b7la", ",", "die", "Ra\u00b7che", "be\u00b7fl\u00fc\u00b7gelt", "den", "Schritt", "schon", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "APPR", "NN", "$,", "FM", "FM", "$,", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Traun, der Tyrann wird nicht finden so ruhigen Tod.", "tokens": ["Traun", ",", "der", "Ty\u00b7rann", "wird", "nicht", "fin\u00b7den", "so", "ru\u00b7hi\u00b7gen", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "VVINF", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}