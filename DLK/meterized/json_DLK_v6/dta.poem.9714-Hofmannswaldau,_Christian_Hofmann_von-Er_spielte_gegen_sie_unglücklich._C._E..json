{"dta.poem.9714": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er spielte gegen sie ungl\u00fccklich.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sagt/ da\u00df ich nicht solt im spielen gl\u00fccklich seyn?", "tokens": ["Wer", "sagt", "/", "da\u00df", "ich", "nicht", "solt", "im", "spie\u00b7len", "gl\u00fcck\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "VMFIN", "APPRART", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gantz falsch! wer also spielt wie ich/ der darff nicht klagen/", "tokens": ["Gantz", "falsch", "!", "wer", "al\u00b7so", "spielt", "wie", "ich", "/", "der", "darff", "nicht", "kla\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PWS", "ADV", "VVFIN", "KOKOM", "PPER", "$(", "ART", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kan gewinn und gl\u00fcck doch stets vom spiele tragen/", "tokens": ["Ich", "kan", "ge\u00b7winn", "und", "gl\u00fcck", "doch", "stets", "vom", "spie\u00b7le", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "ADV", "ADV", "APPRART", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bekomm ich gleich kein spiel/ ist doch Celinde mein.", "tokens": ["Be\u00b7komm", "ich", "gleich", "kein", "spiel", "/", "ist", "doch", "Ce\u00b7lin\u00b7de", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "VVFIN", "$(", "VAFIN", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}