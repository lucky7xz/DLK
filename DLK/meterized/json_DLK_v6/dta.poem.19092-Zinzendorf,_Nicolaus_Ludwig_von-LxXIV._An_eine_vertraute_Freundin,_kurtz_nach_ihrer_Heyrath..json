{"dta.poem.19092": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXIV.   An eine vertraute Freundin, kurtz  \n nach ihrer Heyrath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe hat dich schon ins Ehe-Band gekn\u00fcpft", "tokens": ["Die", "Lie\u00b7be", "hat", "dich", "schon", "ins", "E\u00b7he\u00b7Band", "ge\u00b7kn\u00fcpft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einer solchen Zeit, da du allein zu leben", "tokens": ["Zu", "ei\u00b7ner", "sol\u00b7chen", "Zeit", ",", "da", "du", "al\u00b7lein", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir noch nicht viele M\u00fch auf dieser Welt gegeben.", "tokens": ["Dir", "noch", "nicht", "vie\u00b7le", "M\u00fch", "auf", "die\u00b7ser", "Welt", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "PIAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es scheinet, da\u00df sein Rath dir manches \u00fcberh\u00fcpft:", "tokens": ["Es", "schei\u00b7net", ",", "da\u00df", "sein", "Rath", "dir", "man\u00b7ches", "\u00fc\u00b7ber\u00b7h\u00fcpft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du solst vielleicht gar bald von aller Last der Erden", "tokens": ["Du", "solst", "viel\u00b7leicht", "gar", "bald", "von", "al\u00b7ler", "Last", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erkauft und frey gemacht und ihm vollendet werden.", "tokens": ["Er\u00b7kauft", "und", "frey", "ge\u00b7macht", "und", "ihm", "voll\u00b7en\u00b7det", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "KON", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gewi\u00df, wer seinen Zug an deiner Seele sah,", "tokens": ["Ge\u00b7wi\u00df", ",", "wer", "sei\u00b7nen", "Zug", "an", "dei\u00b7ner", "See\u00b7le", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie er dich so gleich an seinen Hals gerissen,", "tokens": ["Und", "wie", "er", "dich", "so", "gleich", "an", "sei\u00b7nen", "Hals", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch, eh er dich den Weg zum Leben lassen wissen;", "tokens": ["Noch", ",", "eh", "er", "dich", "den", "Weg", "zum", "Le\u00b7ben", "las\u00b7sen", "wis\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kaum war sein Winck geschehn, so war dein Hertze da.", "tokens": ["Kaum", "war", "sein", "Win\u00b7ck", "ge\u00b7schehn", ",", "so", "war", "dein", "Hert\u00b7ze", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VAFIN", "PPOSAT", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wer deines Br\u00e4utgams Ruf und meine Werbung kennt,", "tokens": ["Wer", "dei\u00b7nes", "Br\u00e4ut\u00b7gams", "Ruf", "und", "mei\u00b7ne", "Wer\u00b7bung", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der hat dich schon gar oft ein selig Weib genennt.", "tokens": ["Der", "hat", "dich", "schon", "gar", "oft", "ein", "se\u00b7lig", "Weib", "ge\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du giengest bi\u00df daher in grosser Stille hin:", "tokens": ["Du", "gien\u00b7gest", "bi\u00df", "da\u00b7her", "in", "gros\u00b7ser", "Stil\u00b7le", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der harte Eigensinn verfolgte zwar dein Leben,", "tokens": ["Der", "har\u00b7te", "Ei\u00b7gen\u00b7sinn", "ver\u00b7folg\u00b7te", "zwar", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allein, er muste dich dem Br\u00e4utigam ergeben;", "tokens": ["Al\u00b7lein", ",", "er", "mus\u00b7te", "dich", "dem", "Br\u00e4u\u00b7ti\u00b7gam", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PRF", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Br\u00e4utigam ergrif dein Hertz und deinen Sinn.", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "er\u00b7grif", "dein", "Hertz", "und", "dei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich unterlie\u00df denn nicht, so viel ich nur kan dencken,", "tokens": ["Ich", "un\u00b7ter\u00b7lie\u00df", "denn", "nicht", ",", "so", "viel", "ich", "nur", "kan", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "ADV", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein Wesen mehr und mehr ins rechte Glei\u00df zu lencken.", "tokens": ["Dein", "We\u00b7sen", "mehr", "und", "mehr", "ins", "rech\u00b7te", "Glei\u00df", "zu", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KON", "ADV", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch JEsu Fleisch und Blut, das dein geworden war,", "tokens": ["Doch", "Je\u00b7su", "Fleisch", "und", "Blut", ",", "das", "dein", "ge\u00b7wor\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "KON", "NN", "$,", "PRELS", "PPOSAT", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "That ohne Zweiffel hier das Beste bey der Sache!", "tokens": ["That", "oh\u00b7ne", "Zweif\u00b7fel", "hier", "das", "Bes\u00b7te", "bey", "der", "Sa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und also blieb der HErr nun unter deinem Dache,", "tokens": ["Und", "al\u00b7so", "blieb", "der", "Herr", "nun", "un\u00b7ter", "dei\u00b7nem", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und sch\u00fctzte deinen Geist vor mancherley Gefahr;", "tokens": ["Und", "sch\u00fctz\u00b7te", "dei\u00b7nen", "Geist", "vor", "man\u00b7cher\u00b7ley", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "An seinem Creutze ward dein strenger Sinn zerrieben,", "tokens": ["An", "sei\u00b7nem", "Creut\u00b7ze", "ward", "dein", "stren\u00b7ger", "Sinn", "zer\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und meines K\u00f6nigs Wort in dein Gem\u00fcth geschrieben.", "tokens": ["Und", "mei\u00b7nes", "K\u00f6\u00b7nigs", "Wort", "in", "dein", "Ge\u00b7m\u00fcth", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So f\u00fchrte er dich auch zu deiner Ehe ein,", "tokens": ["So", "f\u00fchr\u00b7te", "er", "dich", "auch", "zu", "dei\u00b7ner", "E\u00b7he", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Tage, da du dich auf deine Hochzeit schicktest,", "tokens": ["Die", "Ta\u00b7ge", ",", "da", "du", "dich", "auf", "dei\u00b7ne", "Hoch\u00b7zeit", "schick\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und in des Br\u00e4utgams Hertz mit Ehrerbietung blicktest,", "tokens": ["Und", "in", "des", "Br\u00e4ut\u00b7gams", "Hertz", "mit", "Ehr\u00b7er\u00b7bie\u00b7tung", "blick\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die musten dir gewi\u00df ", "tokens": ["Die", "mus\u00b7ten", "dir", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Und da du in dem Sinn des Lammes GOttes standest,", "tokens": ["Und", "da", "du", "in", "dem", "Sinn", "des", "Lam\u00b7mes", "Got\u00b7tes", "stan\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was Wunder, da\u00df du auch sein Liebes-Hertz empfandest.", "tokens": ["Was", "Wun\u00b7der", ",", "da\u00df", "du", "auch", "sein", "Lie\u00b7bes\u00b7Hertz", "emp\u00b7fan\u00b7dest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vergi\u00df der Liebe nicht. Die H\u00f6lle k\u00f6nte dir,", "tokens": ["Ver\u00b7gi\u00df", "der", "Lie\u00b7be", "nicht", ".", "Die", "H\u00f6l\u00b7le", "k\u00f6n\u00b7te", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "$.", "ART", "NN", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wann du nicht Treue hieltst, kein gnugsam Feuer heitzen,", "tokens": ["Wann", "du", "nicht", "Treu\u00b7e", "hieltst", ",", "kein", "gnug\u00b7sam", "Feu\u00b7er", "heit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "NN", "VVFIN", "$,", "PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Zorn des Lammes w\u00fcrd\u2019 ein Heer von Zeugen reitzen,", "tokens": ["Der", "Zorn", "des", "Lam\u00b7mes", "w\u00fcrd'", "ein", "Heer", "von", "Zeu\u00b7gen", "reit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die hielten dir sein Hertz und deine Bo\u00dfheit f\u00fcr.", "tokens": ["Die", "hiel\u00b7ten", "dir", "sein", "Hertz", "und", "dei\u00b7ne", "Bo\u00df\u00b7heit", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich selber m\u00fcste dir in JEsu Nahmen fluchen,", "tokens": ["Ich", "sel\u00b7ber", "m\u00fcs\u00b7te", "dir", "in", "Je\u00b7su", "Nah\u00b7men", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und du w\u00fcrdst einen Raum zur Reu vergebens suchen.", "tokens": ["Und", "du", "w\u00fcrdst", "ei\u00b7nen", "Raum", "zur", "Reu", "ver\u00b7ge\u00b7bens", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "APPRART", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun, Schwester! lasset uns dem HErrn mit Leib und Sinn", "tokens": ["Nun", ",", "Schwes\u00b7ter", "!", "las\u00b7set", "uns", "dem", "Herrn", "mit", "Leib", "und", "Sinn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die ihm doch ewiglich geschwohrne Treue halten,", "tokens": ["Die", "ihm", "doch", "e\u00b7wig\u00b7lich", "ge\u00b7schwohr\u00b7ne", "Treu\u00b7e", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und solt uns Angst und Schmertz den Eigenwillen spalten;", "tokens": ["Und", "solt", "uns", "Angst", "und", "Schmertz", "den", "Ei\u00b7gen\u00b7wil\u00b7len", "spal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So geh er in den Tod und Untergehen hin.", "tokens": ["So", "geh", "er", "in", "den", "Tod", "und", "Un\u00b7ter\u00b7ge\u00b7hen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Herr JEsu! nimm di\u00df Hertz, und opfers deiner Treue,", "tokens": ["Herr", "Je\u00b7su", "!", "nimm",