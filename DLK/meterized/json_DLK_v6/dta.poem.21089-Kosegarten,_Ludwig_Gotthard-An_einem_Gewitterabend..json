{"dta.poem.21089": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An einem Gewitterabend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, wahrlich, du bist sch\u00f6n! bist einer ew'gen               ", "tokens": ["Ja", ",", "wahr\u00b7lich", ",", "du", "bist", "sch\u00f6n", "!", "bist", "ei\u00b7ner", "ew'\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$.", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Milde", "tokens": ["Mil\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und einer ewgen Kraft unsterbliches Gebilde,", "tokens": ["Und", "ei\u00b7ner", "ew\u00b7gen", "Kraft", "uns\u00b7terb\u00b7li\u00b7ches", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du meiner Wallfahrt Land, du Land, das mich gebar,", "tokens": ["Du", "mei\u00b7ner", "Wall\u00b7fahrt", "Land", ",", "du", "Land", ",", "das", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,", "PPER", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich s\u00e4ugte, mich erzog, mir Wieg' und Amme war;", "tokens": ["Mich", "s\u00e4ug\u00b7te", ",", "mich", "er\u00b7zog", ",", "mir", "Wieg'", "und", "Am\u00b7me", "war", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich dreissig Fr\u00fchlinge mit seinen Rosen kr\u00e4nzte,", "tokens": ["Mich", "dreis\u00b7sig", "Fr\u00fch\u00b7lin\u00b7ge", "mit", "sei\u00b7nen", "Ro\u00b7sen", "kr\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Mir im kristallnen Schnee durch dreissig Winter", "tokens": ["Mir", "im", "kris\u00b7tall\u00b7nen", "Schnee", "durch", "dreis\u00b7sig", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "gl\u00e4nzte,", "tokens": ["gl\u00e4nz\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und einstens diesen Staub, durch Gottes Hauch", "tokens": ["Und", "eins\u00b7tens", "die\u00b7sen", "Staub", ",", "durch", "Got\u00b7tes", "Hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "NN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "belebt,", "tokens": ["be\u00b7lebt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "In seinen Schooss begr\u00e4bt.", "tokens": ["In", "sei\u00b7nen", "Scho\u00b7oss", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n bist du, Erde, sch\u00f6n im goldnen Sommer-", "tokens": ["Sch\u00f6n", "bist", "du", ",", "Er\u00b7de", ",", "sch\u00f6n", "im", "gold\u00b7nen", "Som\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$,", "NN", "$,", "ADJD", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "kleide.", "tokens": ["klei\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dich gr\u00fcsst mein Preisgesang; dich ehret meine", "tokens": ["Dich", "gr\u00fcs\u00b7st", "mein", "Preis\u00b7ge\u00b7sang", ";", "dich", "eh\u00b7ret", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Freude.", "tokens": ["Freu\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sieh, wie die gelbe Saat die schweren H\u00e4upter", "tokens": ["Sieh", ",", "wie", "die", "gel\u00b7be", "Saat", "die", "schwe\u00b7ren", "H\u00e4up\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "neigt!", "tokens": ["neigt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Wie unter seiner Last das schwanke Reis sich", "tokens": ["Wie", "un\u00b7ter", "sei\u00b7ner", "Last", "das", "schwan\u00b7ke", "Reis", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "beugt!", "tokens": ["beugt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Wie auf der fetten Trift die satte Heerde h\u00fcpfet!", "tokens": ["Wie", "auf", "der", "fet\u00b7ten", "Trift", "die", "sat\u00b7te", "Heer\u00b7de", "h\u00fcp\u00b7fet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie durch das hohe Gras das Sonnenw\u00fcrmchen", "tokens": ["Wie", "durch", "das", "ho\u00b7he", "Gras", "das", "Son\u00b7nen\u00b7w\u00fcrm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "schl\u00fcpfet!", "tokens": ["schl\u00fcp\u00b7fet", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Horch, wie der Wachtel Schlag im Weizen, tief", "tokens": ["Horch", ",", "wie", "der", "Wach\u00b7tel", "Schlag", "im", "Wei\u00b7zen", ",", "tief"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NN", "APPRART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "im Wald", "tokens": ["im", "Wald"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Der Drossel Fl\u00f6te schallt!", "tokens": ["Der", "Dros\u00b7sel", "Fl\u00f6\u00b7te", "schallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch schw\u00fcler wird die Luft; die Kreaturen", "tokens": ["Doch", "schw\u00fc\u00b7ler", "wird", "die", "Luft", ";", "die", "Kre\u00b7a\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00e4chzen;", "tokens": ["\u00e4ch\u00b7zen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die matte Sch\u00f6pfung st\u00f6hnt; die welken Fluren", "tokens": ["Die", "mat\u00b7te", "Sch\u00f6p\u00b7fung", "st\u00f6hnt", ";", "die", "wel\u00b7ken", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "lechzen.", "tokens": ["lech\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Allvater winkt, und schnell klimmt schwarze Wet-", "tokens": ["All\u00b7va\u00b7ter", "winkt", ",", "und", "schnell", "klimmt", "schwar\u00b7ze", "Wet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ternacht", "tokens": ["ter\u00b7nacht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Herauf aus S\u00fcd und West. Des Sturmes Kraft erwacht.", "tokens": ["Her\u00b7auf", "aus", "S\u00fcd", "und", "West", ".", "Des", "Stur\u00b7mes", "Kraft", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$.", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es blitzt. Der Donner grollt. Das Bodenfeste zittert.", "tokens": ["Es", "blitzt", ".", "Der", "Don\u00b7ner", "grollt", ".", "Das", "Bo\u00b7den\u00b7fes\u00b7te", "zit\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das wilde Weltmeer tobt. Der Eichwald dampft", "tokens": ["Das", "wil\u00b7de", "Welt\u00b7meer", "tobt", ".", "Der", "Eich\u00b7wald", "dampft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und splittert.", "tokens": ["und", "split\u00b7tert", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Der Haingesang erstummt. Das scheue Ross ent-", "tokens": ["Der", "Hain\u00b7ge\u00b7sang", "er\u00b7stummt", ".", "Das", "scheu\u00b7e", "Ross", "ent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "fleucht,", "tokens": ["fleucht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Und Held und Memm' erbleicht!", "tokens": ["Und", "Held", "und", "Memm'", "er\u00b7bleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Allvater l\u00e4chelt. Schnell verbraust der Donner", "tokens": ["All\u00b7va\u00b7ter", "l\u00e4\u00b7chelt", ".", "Schnell", "ver\u00b7braust", "der", "Don\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rasen.", "tokens": ["Ra\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Blitze Flamm' erlischt; des Sturms verheerend", "tokens": ["Der", "Blit\u00b7ze", "Flamm'", "er\u00b7lischt", ";", "des", "Sturms", "ver\u00b7hee\u00b7rend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "$.", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Blasen", "tokens": ["Bla\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wird leises Wehn; es schweigt das aufgew\u00fchlte", "tokens": ["Wird", "lei\u00b7ses", "Wehn", ";", "es", "schweigt", "das", "auf\u00b7ge\u00b7w\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Meer \u2014", "tokens": ["Meer"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Sch\u00f6n, Erde, ist dein Ruhn nach Wettern, sch\u00f6n", "tokens": ["Sch\u00f6n", ",", "Er\u00b7de", ",", "ist", "dein", "Ruhn", "nach", "Wet\u00b7tern", ",", "sch\u00f6n"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und hehr.", "tokens": ["und", "hehr", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Des Donners Drohn wird Huld, sein Schelten mil-", "tokens": ["Des", "Don\u00b7ners", "Drohn", "wird", "Huld", ",", "sein", "Schel\u00b7ten", "mil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "der Segen.", "tokens": ["der", "Se\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Der Wolken F\u00fclle rauscht; es rieseln laue Regen.", "tokens": ["Der", "Wol\u00b7ken", "F\u00fcl\u00b7le", "rauscht", ";", "es", "rie\u00b7seln", "lau\u00b7e", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun trinkt, was durstete; nun labt sich die Natur;", "tokens": ["Nun", "trinkt", ",", "was", "durs\u00b7te\u00b7te", ";", "nun", "labt", "sich", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VVFIN", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun jubeln Wald und Flur.", "tokens": ["Nun", "ju\u00b7beln", "Wald", "und", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die D\u00fcnste fliehn. Die Luft verkl\u00e4rt sich.", "tokens": ["Die", "D\u00fcns\u00b7te", "fliehn", ".", "Die", "Luft", "ver\u00b7kl\u00e4rt", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gross und milde", "tokens": ["Gross", "und", "mil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Begl\u00e4nzt die Abendsonn das tr\u00e4ufelnde Gefilde.", "tokens": ["Be\u00b7gl\u00e4nzt", "die", "A\u00b7bend\u00b7sonn", "das", "tr\u00e4u\u00b7feln\u00b7de", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wie blitzt in ihrem Glanz, wie funkeln Bach und", "tokens": ["Wie", "blitzt", "in", "ih\u00b7rem", "Glanz", ",", "wie", "fun\u00b7keln", "Bach", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "NE", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Au!", "tokens": ["Au", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wie d\u00fcster steht der Wald, das ferne Meer, wie", "tokens": ["Wie", "d\u00fcs\u00b7ter", "steht", "der", "Wald", ",", "das", "fer\u00b7ne", "Meer", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+-", "measure