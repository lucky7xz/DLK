{"textgrid.poem.27682": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Hofe-Leben ist ein rechtes Hoffe-Leben;", "tokens": ["Das", "Ho\u00b7fe\u00b7Le\u00b7ben", "ist", "ein", "rech\u00b7tes", "Hof\u00b7fe\u00b7Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn da verspricht man Gunst, und Ungunst wird gegeben.", "tokens": ["Denn", "da", "ver\u00b7spricht", "man", "Gunst", ",", "und", "Un\u00b7gunst", "wird", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "NN", "$,", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}