{"dta.poem.12791": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als die Durchlauchtigste Prince\u00dfin,  \n Wilhelmina Carolina, Marggr\u00e4-  \n fin zu Brandenburg rc. rc. durch  \n Wittenberg reisete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, thr\u00e4nen-voller wolcken-saal!", "tokens": ["Auf", ",", "thr\u00e4\u00b7nen\u00b7vol\u00b7ler", "wol\u00b7cken\u00b7saal", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df einen angenehmen strahl", "tokens": ["La\u00df", "ei\u00b7nen", "an\u00b7ge\u00b7neh\u00b7men", "strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf unsern hohen berg und seine grentzen flie\u00dfen,", "tokens": ["Auf", "un\u00b7sern", "ho\u00b7hen", "berg", "und", "sei\u00b7ne", "grent\u00b7zen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df der umb\u00fcllten sterne schein", "tokens": ["La\u00df", "der", "um\u00b7b\u00fcll\u00b7ten", "ster\u00b7ne", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch st\u00e4rcker als den schatten seyn,", "tokens": ["Doch", "st\u00e4r\u00b7cker", "als", "den", "schat\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit demselbigen ein hohes Haupt begr\u00fc\u00dfen.", "tokens": ["Und", "mit", "dem\u00b7sel\u00b7bi\u00b7gen", "ein", "ho\u00b7hes", "Haupt", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt aber nicht des sturmes lauf", "tokens": ["H\u00f6rt", "a\u00b7ber", "nicht", "des", "stur\u00b7mes", "lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch unsers bitten opffer auf,", "tokens": ["Durch", "un\u00b7sers", "bit\u00b7ten", "opf\u00b7fer", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So soll dein schatten doch nicht unsern vorsatz trennen,", "tokens": ["So", "soll", "dein", "schat\u00b7ten", "doch", "nicht", "un\u00b7sern", "vor\u00b7satz", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die hertzen, die zu dieser zeit", "tokens": ["Die", "hert\u00b7zen", ",", "die", "zu", "die\u00b7ser", "zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein hohes Sonnen-licht erfreut,", "tokens": ["Ein", "ho\u00b7hes", "Son\u00b7nen\u00b7licht", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sollen wie ein stern der ersten gr\u00f6\u00dfe brennen.", "tokens": ["Die", "sol\u00b7len", "wie", "ein", "stern", "der", "ers\u00b7ten", "gr\u00f6\u00b7\u00dfe", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wird ihr strahl mehr funcken streun,", "tokens": ["Doch", "wird", "ihr", "strahl", "mehr", "fun\u00b7cken", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sie dein hoher sonnenschein,", "tokens": ["Wo", "sie", "dein", "ho\u00b7her", "son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kostbarer Diamant Durchlauchter Prince\u00dfinnen!", "tokens": ["Kost\u00b7ba\u00b7rer", "Di\u00b7a\u00b7mant", "Durch\u00b7lauch\u00b7ter", "Prin\u00b7ce\u00b7\u00dfin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Den Francken, ja die gantze welt,", "tokens": ["Den", "Fran\u00b7cken", ",", "ja", "die", "gant\u00b7ze", "welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor ein unsch\u00e4tzbar kleinod h\u00e4lt,", "tokens": ["Vor", "ein", "un\u00b7sch\u00e4tz\u00b7bar", "klei\u00b7nod", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Von seiner gnade l\u00e4st nur einen blick gewinnen.", "tokens": ["Von", "sei\u00b7ner", "gna\u00b7de", "l\u00e4st", "nur", "ei\u00b7nen", "blick", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wem F\u00fcrsten-augen gn\u00e4dig sind,", "tokens": ["Wem", "F\u00fcrs\u00b7ten\u00b7au\u00b7gen", "gn\u00e4\u00b7dig", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem wird sein opffer bald entz\u00fcndt,", "tokens": ["Dem", "wird", "sein", "opf\u00b7fer", "bald", "ent\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das gl\u00fccke lacht ihn an mit himmlischem gedeyen,", "tokens": ["Das", "gl\u00fc\u00b7cke", "lacht", "ihn", "an", "mit", "himm\u00b7li\u00b7schem", "ge\u00b7de\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er lacht, wenn gleich der himmel weint,", "tokens": ["Er", "lacht", ",", "wenn", "gleich", "der", "him\u00b7mel", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ihm ein\u2019 andre Sonne scheint,", "tokens": ["Weil", "ihm", "ein'", "and\u00b7re", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dar\u00fcber geist und brust sich h\u00f6chlich kan erfreuen.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "geist", "und", "brust", "sich", "h\u00f6ch\u00b7lich", "kan", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "KON", "VVFIN", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie ist der tag doch so begl\u00fcckt,", "tokens": ["Wie", "ist", "der", "tag", "doch", "so", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da uns der himmel zugeschickt,", "tokens": ["Da", "uns", "der", "him\u00b7mel", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des vaterlandes wohl und himmlisches vergn\u00fcgen,", "tokens": ["Des", "va\u00b7ter\u00b7lan\u00b7des", "wohl", "und", "himm\u00b7li\u00b7sches", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da Carolinens hoher schlu\u00df", "tokens": ["Da", "Ca\u00b7ro\u00b7li\u00b7nens", "ho\u00b7her", "schlu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Gab unsern grentzen einen gru\u00df,", "tokens": ["Gab", "un\u00b7sern", "grent\u00b7zen", "ei\u00b7nen", "gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und unsre hertzen lie\u00df zu ihren f\u00fc\u00dfen liegen.", "tokens": ["Und", "uns\u00b7re", "hert\u00b7zen", "lie\u00df", "zu", "ih\u00b7ren", "f\u00fc\u00b7\u00dfen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der himmel, der vor Cronen wacht,", "tokens": ["Der", "him\u00b7mel", ",", "der", "vor", "Cro\u00b7nen", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf der F\u00fcrsten heyl bedacht,", "tokens": ["Und", "auf", "der", "F\u00fcrs\u00b7ten", "heyl", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der l\u00e4st dein reisen ihm auch h\u00f6chst seyn angelegen,", "tokens": ["Der", "l\u00e4st", "dein", "rei\u00b7sen", "ihm", "auch", "h\u00f6chst", "seyn", "an\u00b7ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir schau\u2019n, wie dessen reines licht", "tokens": ["Wir", "schau'n", ",", "wie", "des\u00b7sen", "rei\u00b7nes", "licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinf\u00fchro durch die wolcken bricht,", "tokens": ["Hin\u00b7f\u00fch\u00b7ro", "durch", "die", "wol\u00b7cken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stets zugegen ist auf deinen F\u00fcrsten-wegen.", "tokens": ["Und", "stets", "zu\u00b7ge\u00b7gen", "ist", "auf", "dei\u00b7nen", "F\u00fcrs\u00b7ten\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Berlin, die wunder-volle stadt,", "tokens": ["Ber\u00b7lin", ",", "die", "wun\u00b7der\u00b7vol\u00b7le", "stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die eine welt fast in sich hat,", "tokens": ["Die", "ei\u00b7ne", "welt", "fast", "in", "sich", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Legt ihren purpur an, weil sie von deinen strahlen,", "tokens": ["Legt", "ih\u00b7ren", "pur\u00b7pur", "an", ",", "weil", "sie", "von", "dei\u00b7nen", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die nur des himmels starcke hand", "tokens": ["Die", "nur", "des", "him\u00b7mels", "star\u00b7cke", "hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In deinen blick vor helden band,", "tokens": ["In", "dei\u00b7nen", "blick", "vor", "hel\u00b7den", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schant ihren sch\u00f6nen krei\u00df noch angenehmer mahlen.", "tokens": ["Schant", "ih\u00b7ren", "sch\u00f6\u00b7nen", "krei\u00df", "noch", "an\u00b7ge\u00b7neh\u00b7mer", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "So fahre denn vergn\u00fcgt dahin,", "tokens": ["So", "fah\u00b7re", "denn", "ver\u00b7gn\u00fcgt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo dich, Durchlauchter F\u00fcrsten-sinn,", "tokens": ["Wo", "dich", ",", "Durch\u00b7lauch\u00b7ter", "F\u00fcrs\u00b7ten\u00b7sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die zarte regung f\u00fchrt, wo Friederich regieret,", "tokens": ["Die", "zar\u00b7te", "re\u00b7gung", "f\u00fchrt", ",", "wo", "Frie\u00b7de\u00b7rich", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der himmel lencke deinen fu\u00df,", "tokens": ["Der", "him\u00b7mel", "len\u00b7cke", "dei\u00b7nen", "fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch seinen wunder-weisen schlu\u00df,", "tokens": ["Durch", "sei\u00b7nen", "wun\u00b7der\u00b7wei\u00b7sen", "schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er mache, da\u00df dein haupt auch einsten cronen f\u00fchret.", "tokens": ["Er", "ma\u00b7che", ",", "da\u00df", "dein", "haupt", "auch", "eins\u00b7ten", "cro\u00b7nen", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}