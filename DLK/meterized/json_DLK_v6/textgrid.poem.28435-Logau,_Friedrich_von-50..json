{"textgrid.poem.28435": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schickt sich nicht, da\u00df der ein friedlich Sch\u00e4flein heisset,", "tokens": ["Es", "schickt", "sich", "nicht", ",", "da\u00df", "der", "ein", "fried\u00b7lich", "Sch\u00e4f\u00b7lein", "heis\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "KOUS", "ART", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der raubet, mordet, w\u00fcrgt und um sich reisset, beisset.", "tokens": ["Der", "rau\u00b7bet", ",", "mor\u00b7det", ",", "w\u00fcrgt", "und", "um", "sich", "reis\u00b7set", ",", "beis\u00b7set", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "APPR", "PRF", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Peltz zwar deckt den Mann, macht aber keinen Mann;", "tokens": ["Der", "Peltz", "zwar", "deckt", "den", "Mann", ",", "macht", "a\u00b7ber", "kei\u00b7nen", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wolff bleibt Wolff, ob er ein Schafskleid gleich zeucht an.", "tokens": ["Der", "Wolff", "bleibt", "Wolff", ",", "ob", "er", "ein", "Schafs\u00b7kleid", "gleich", "zeucht", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}