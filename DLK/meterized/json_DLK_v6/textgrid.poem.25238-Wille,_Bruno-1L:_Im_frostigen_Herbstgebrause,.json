{"textgrid.poem.25238": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im frostigen Herbstgebrause,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im frostigen Herbstgebrause,", "tokens": ["Im", "fros\u00b7ti\u00b7gen", "Herbst\u00b7ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Nebelregen umgraut,", "tokens": ["Von", "Ne\u00b7bel\u00b7re\u00b7gen", "um\u00b7graut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "D\u00fcster tr\u00e4umte die F\u00f6hre/", "tokens": ["D\u00fcs\u00b7ter", "tr\u00e4um\u00b7te", "die", "F\u00f6h\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wie eine verlassene Braut.", "tokens": ["Wie", "ei\u00b7ne", "ver\u00b7las\u00b7se\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Auf einmal spaltet die Sonne", "tokens": ["Auf", "ein\u00b7mal", "spal\u00b7tet", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Blaugraue Wolkenfetzen,", "tokens": ["Blau\u00b7grau\u00b7e", "Wol\u00b7ken\u00b7fet\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mit goldiger Abendflamme", "tokens": ["Mit", "gol\u00b7di\u00b7ger", "A\u00b7bend\u00b7flam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das F\u00f6hrenhaupt zu netzen.", "tokens": ["Das", "F\u00f6h\u00b7ren\u00b7haupt", "zu", "net\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da rinnt durch starre Adern", "tokens": ["Da", "rinnt", "durch", "star\u00b7re", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Hauch von Jugendglut;", "tokens": ["Ein", "Hauch", "von", "Ju\u00b7gend\u00b7glut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Antlitz wallt es z\u00e4rtlich,", "tokens": ["Zum", "Ant\u00b7litz", "wallt", "es", "z\u00e4rt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "St\u00fcrmisch schmachtendes Blut.", "tokens": ["St\u00fcr\u00b7misch", "schmach\u00b7ten\u00b7des", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Der Stamm und alle Zweige", "tokens": ["Der", "Stamm", "und", "al\u00b7le", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ergl\u00fchen purpurrot,", "tokens": ["Er\u00b7gl\u00fc\u00b7hen", "pur\u00b7pur\u00b7rot", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als weihe tr\u00e4umend sich die Braut", "tokens": ["Als", "wei\u00b7he", "tr\u00e4u\u00b7mend", "sich", "die", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADJD", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Liebesflammentod.", "tokens": ["Dem", "Lie\u00b7bes\u00b7flam\u00b7men\u00b7tod", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun lischt der hehre Feuerball,", "tokens": ["Nun", "lischt", "der", "heh\u00b7re", "Feu\u00b7er\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Wolkenkl\u00fcfte versunken ...", "tokens": ["In", "Wol\u00b7ken\u00b7kl\u00fcf\u00b7te", "ver\u00b7sun\u00b7ken", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die F\u00f6hre starrt dem Liebsten nach", "tokens": ["Die", "F\u00f6h\u00b7re", "starrt", "dem", "Liebs\u00b7ten", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verz\u00fcckt und flammentrunken.", "tokens": ["Ver\u00b7z\u00fcckt", "und", "flam\u00b7men\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es war nur ein fl\u00fcchtig Umfangen,", "tokens": ["Es", "war", "nur", "ein", "fl\u00fcch\u00b7tig", "Um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Flackern; doch war's einmal", "tokens": ["Ein", "Fla\u00b7ckern", ";", "doch", "wa\u00b7r's", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lohnt die Seufzer alle", "tokens": ["Und", "lohnt", "die", "Seuf\u00b7zer", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In grauer Lebensqual.", "tokens": ["In", "grau\u00b7er", "Le\u00b7bens\u00b7qual", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So komm denn, Nacht und \u00d6de,", "tokens": ["So", "komm", "denn", ",", "Nacht", "und", "\u00d6\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umh\u00fclle den F\u00f6hrenbaum/", "tokens": ["Um\u00b7h\u00fcl\u00b7le", "den", "F\u00f6h\u00b7ren\u00b7baum", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er tr\u00e4gt an seligem Herzen", "tokens": ["Er", "tr\u00e4gt", "an", "se\u00b7li\u00b7gem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gestillter Liebe Traum.", "tokens": ["Ge\u00b7still\u00b7ter", "Lie\u00b7be", "Traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}