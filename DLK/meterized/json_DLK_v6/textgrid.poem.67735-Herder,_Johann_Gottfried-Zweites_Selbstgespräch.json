{"textgrid.poem.67735": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Zweites Selbstgespr\u00e4ch", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bin ich? Alles erwacht in mir! mein Geist! \u2013", "tokens": ["Wer", "bin", "ich", "?", "Al\u00b7les", "er\u00b7wacht", "in", "mir", "!", "mein", "Geist", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PIS", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "NN", "$.", "$("], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "H\u00f6hen \u2013 Tiefen! \u2013 ich schaudre! \u2013 die nur Gott durchmi\u00dft! \u2013", "tokens": ["H\u00f6\u00b7hen", "\u2013", "Tie\u00b7fen", "!", "\u2013", "ich", "schaud\u00b7re", "!", "\u2013", "die", "nur", "Gott", "durch\u00b7mi\u00dft", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$(", "NN", "$.", "$(", "PPER", "VVFIN", "$.", "$(", "PRELS", "ADV", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dunkel liegt mein Grund! \u2013 Leidenschaft durchfleu\u00dft", "tokens": ["Dun\u00b7kel", "liegt", "mein", "Grund", "!", "\u2013", "Lei\u00b7den\u00b7schaft", "durch\u00b7fleu\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "$(", "NN", "VVFIN"], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Ihn unendlich und braust! \u2013 braust! \u2013 Geist, Du bist", "tokens": ["Ihn", "un\u00b7end\u00b7lich", "und", "braust", "!", "\u2013", "braust", "!", "\u2013", "Geist", ",", "Du", "bist"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "$.", "$(", "VVFIN", "$.", "$(", "NN", "$,", "PPER", "VAFIN"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Eine Welt, ein All, ein Gott, Ich! \u2013", "tokens": ["Ei\u00b7ne", "Welt", ",", "ein", "All", ",", "ein", "Gott", ",", "Ich", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PPER", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein! aufrecht stehn und denken will ich mich!", "tokens": ["Nein", "!", "auf\u00b7recht", "stehn", "und", "den\u00b7ken", "will", "ich", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VVINF", "KON", "VVINF", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du jeder mein Gedank, des st\u00e4rksten Selbsttriebs Blut", "tokens": ["Du", "je\u00b7der", "mein", "Ge\u00b7dank", ",", "des", "st\u00e4rks\u00b7ten", "Selbst\u00b7triebs", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und jede Nerv sei Kraft und jede Ader Gluth,", "tokens": ["Und", "je\u00b7de", "Nerv", "sei", "Kraft", "und", "je\u00b7de", "A\u00b7der", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ich mich f\u00fchlen, fassen, lenken kann!", "tokens": ["Da\u00df", "ich", "mich", "f\u00fch\u00b7len", ",", "fas\u00b7sen", ",", "len\u00b7ken", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,", "VVINF", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es schl\u00e4ft in mir! Im Schoo\u00df des Chaos schl\u00e4ft", "tokens": ["Es", "schl\u00e4ft", "in", "mir", "!", "Im", "Schoo\u00df", "des", "Chaos", "schl\u00e4ft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Welche Gedankenwelt!", "tokens": ["Wel\u00b7che", "Ge\u00b7dan\u00b7ken\u00b7welt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PWAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Um ", "tokens": ["Um"], "token_info": ["word"], "pos": ["KOUI"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sich in der Ferne Schatten. Es schl\u00e4ft", "tokens": ["Sich", "in", "der", "Fer\u00b7ne", "Schat\u00b7ten", ".", "Es", "schl\u00e4ft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Um mein Jetzt die Asche von Vergangen,", "tokens": ["Um", "mein", "Jetzt", "die", "A\u00b7sche", "von", "Ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In ihr der Keim der ganzen K\u00fcnftigkeit.", "tokens": ["In", "ihr", "der", "Keim", "der", "gan\u00b7zen", "K\u00fcnf\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie keimt im Todtenkrug die Asche von Vergangen", "tokens": ["Wie", "keimt", "im", "Tod\u00b7ten\u00b7krug", "die", "A\u00b7sche", "von", "Ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zum Keim der K\u00fcnftigkeit!", "tokens": ["Zum", "Keim", "der", "K\u00fcnf\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wolkenhoch erwach' ich am Segel, und unter mir", "tokens": ["Wol\u00b7ken\u00b7hoch", "er\u00b7wach'", "ich", "am", "Se\u00b7gel", ",", "und", "un\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "APPR", "PPER"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ruht ein Ocean! doch in den hohlen Tiefen", "tokens": ["Ruht", "ein", "O\u00b7cean", "!", "doch", "in", "den", "hoh\u00b7len", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Donnert herauf Neptun. So steigen hier", "tokens": ["Don\u00b7nert", "her\u00b7auf", "Nep\u00b7tun", ".", "So", "stei\u00b7gen", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "NN", "$.", "ADV", "VVFIN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gedanken empor; es rauscht das Feld in mir", "tokens": ["Ge\u00b7dan\u00b7ken", "em\u00b7por", ";", "es", "rauscht", "das", "Feld", "in", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Von Todten, die sich ins Leben riefen.", "tokens": ["Von", "Tod\u00b7ten", ",", "die", "sich", "ins", "Le\u00b7ben", "rie\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "O, spr\u00e4ch' ich: \u00bbSei!\u00ab und meine ganze Welt", "tokens": ["O", ",", "spr\u00e4ch'", "ich", ":", "\u00bb", "Sei", "!", "\u00ab", "und", "mei\u00b7ne", "gan\u00b7ze", "Welt"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "$.", "$(", "VAFIN", "$.", "$(", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erst\u00fcnde mir, dem Gott, so! welche Millionen!", "tokens": ["Er\u00b7st\u00fcn\u00b7de", "mir", ",", "dem", "Gott", ",", "so", "!", "wel\u00b7che", "Mil\u00b7lion\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "$,", "ADV", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Zoll der ganzen Sch\u00f6pfung, tief versenkt", "tokens": ["Der", "Zoll", "der", "gan\u00b7zen", "Sch\u00f6p\u00b7fung", ",", "tief", "ver\u00b7senkt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins Meer der Nacht! So ruht das Gold, umschr\u00e4nkt", "tokens": ["Ins", "Meer", "der", "Nacht", "!", "So", "ruht", "das", "Gold", ",", "um\u00b7schr\u00e4nkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Acherons, von Cerbers rings umbellt,", "tokens": ["Von", "A\u00b7che\u00b7rons", ",", "von", "Cer\u00b7bers", "rings", "um\u00b7bellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da Alpen, Kl\u00fcfte, Plutons auf ihm thronen!", "tokens": ["Da", "Al\u00b7pen", ",", "Kl\u00fcf\u00b7te", ",", "Plu\u00b7tons", "auf", "ihm", "thro\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NE", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So ruhn im Meere Sch\u00e4tze Millionen,", "tokens": ["So", "ruhn", "im", "Mee\u00b7re", "Sch\u00e4t\u00b7ze", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Raub der Indiens, im Schiffbruch, ach! ertr\u00e4nkt!", "tokens": ["Der", "Raub", "der", "In\u00b7di\u00b7ens", ",", "im", "Schiff\u00b7bruch", ",", "ach", "!", "er\u00b7tr\u00e4nkt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPRART", "NN", "$,", "ITJ", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So schlummert unter Eis und Schneesthronen", "tokens": ["So", "schlum\u00b7mert", "un\u00b7ter", "Eis", "und", "Schneest\u00b7hro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Des Fr\u00fchlings bunte Blumenwelt!", "tokens": ["Des", "Fr\u00fch\u00b7lings", "bun\u00b7te", "Blu\u00b7men\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer ruft Dich, Fr\u00fchlingswind, der mich von Banden", "tokens": ["Wer", "ruft", "Dich", ",", "Fr\u00fch\u00b7lings\u00b7wind", ",", "der", "mich", "von", "Ban\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Enteist! O welche Sonne gebiert", "tokens": ["Ent\u00b7eist", "!", "O", "wel\u00b7che", "Son\u00b7ne", "ge\u00b7biert"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "PWAT", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Aus mir ein Tempe und weckt ein hohes Aehrenheer,", "tokens": ["Aus", "mir", "ein", "Tem\u00b7pe", "und", "weckt", "ein", "ho\u00b7hes", "A\u00b7eh\u00b7ren\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "---+--+-+----+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wie Riesen aus Jason's Saat entstanden!", "tokens": ["Wie", "Rie\u00b7sen", "aus", "Ja\u00b7son's", "Saat", "ent\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Entw\u00e4lzt kein Hercules die Felsen mir und entf\u00fchrt", "tokens": ["Ent\u00b7w\u00e4lzt", "kein", "Her\u00b7cu\u00b7les", "die", "Fel\u00b7sen", "mir", "und", "ent\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "PPER", "KON", "VVFIN"], "meter": "-+-+---+-++-+", "measure": "unknown.measure.hexa"}, "line.16": {"text": "Der H\u00f6lle mein Gold! Wer spricht zum Meer:", "tokens": ["Der", "H\u00f6l\u00b7le", "mein", "Gold", "!", "Wer", "spricht", "zum", "Meer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "\u00bbgieb Deine Todten her!\u00ab", "tokens": ["\u00bb", "gieb", "Dei\u00b7ne", "Tod\u00b7ten", "her", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Und kann ich selbst nicht, selbst mir Hercul sein?", "tokens": ["Und", "kann", "ich", "selbst", "nicht", ",", "selbst", "mir", "Her\u00b7cul", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "$,", "ADV", "PPER", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Er, der den Cerber speiend, die Allmachtskeule", "tokens": ["Er", ",", "der", "den", "Cer\u00b7ber", "spei\u00b7end", ",", "die", "All\u00b7machts\u00b7keu\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.20": {"text": "Gefa\u00dft, im L\u00f6wenschmuck", "tokens": ["Ge\u00b7fa\u00dft", ",", "im", "L\u00f6\u00b7wen\u00b7schmuck"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Voll Hyderblut erschien und Ruh und S\u00e4ule", "tokens": ["Voll", "Hy\u00b7der\u00b7blut", "er\u00b7schien", "und", "Ruh", "und", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und Kampf Olympens nachlie\u00df; denn es trug", "tokens": ["Und", "Kampf", "O\u00b7lym\u00b7pens", "nach\u00b7lie\u00df", ";", "denn", "es", "trug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "NE", "VVFIN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Den Pappel-, Oel- und Lorbeer-Neugekr\u00f6nten", "tokens": ["Den", "Pap\u00b7pel", ",", "O\u00b7el", "und", "Lor\u00b7beer\u00b7Neu\u00b7ge\u00b7kr\u00f6n\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "TRUNC", "$,", "TRUNC", "KON", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Die Wolke himmelw\u00e4rts,", "tokens": ["Die", "Wol\u00b7ke", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "me