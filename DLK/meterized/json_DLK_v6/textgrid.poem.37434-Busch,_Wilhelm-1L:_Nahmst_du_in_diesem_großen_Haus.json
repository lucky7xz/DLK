{"textgrid.poem.37434": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nahmst du in diesem gro\u00dfen Haus", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nahmst du in diesem gro\u00dfen Haus", "tokens": ["Nahmst", "du", "in", "die\u00b7sem", "gro\u00b7\u00dfen", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht selbst Quartier?", "tokens": ["Nicht", "selbst", "Quar\u00b7tier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mi\u00dff\u00e4llt es dir, so zieh doch aus.", "tokens": ["Mi\u00df\u00b7f\u00e4llt", "es", "dir", ",", "so", "zieh", "doch", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer h\u00e4lt dich hier?", "tokens": ["Wer", "h\u00e4lt", "dich", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und schimpfe auf die Welt, mein Sohn,", "tokens": ["Und", "schimp\u00b7fe", "auf", "die", "Welt", ",", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht gar zu laut.", "tokens": ["Nicht", "gar", "zu", "laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Eh du geboren, hast du schon", "tokens": ["Eh", "du", "ge\u00b7bo\u00b7ren", ",", "hast", "du", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit dran gebaut.", "tokens": ["Mit", "dran", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PAV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}