{"dta.poem.19260": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sterne leuchten auch am Tag, nur siehst du's nicht,", "tokens": ["Die", "Ster\u00b7ne", "leuch\u00b7ten", "auch", "am", "Tag", ",", "nur", "siehst", "du's", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil deine Augen ganz erf\u00fcllet Sonnenlicht.", "tokens": ["Weil", "dei\u00b7ne", "Au\u00b7gen", "ganz", "er\u00b7f\u00fcl\u00b7let", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wird gesagt, da\u00df man am hellesten Mittag", "tokens": ["Doch", "wird", "ge\u00b7sagt", ",", "da\u00df", "man", "am", "hel\u00b7les\u00b7ten", "Mit\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "$,", "KOUS", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus tiefem Brunnengrund die Sterne sehen mag.", "tokens": ["Aus", "tie\u00b7fem", "Brun\u00b7nen\u00b7grund", "die", "Ster\u00b7ne", "se\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer so sich ganz vertieft, der hat sich ganz erhoben,", "tokens": ["Wer", "so", "sich", "ganz", "ver\u00b7tieft", ",", "der", "hat", "sich", "ganz", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "ADV", "VVPP", "$,", "PRELS", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm leuchtet h\u00f6hres Licht als von der Sonne droben.", "tokens": ["Ihm", "leuch\u00b7tet", "h\u00f6h\u00b7res", "Licht", "als", "von", "der", "Son\u00b7ne", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KOKOM", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Auch sah ich selbst am Tag die Sterne treten vor,", "tokens": ["Auch", "sah", "ich", "selbst", "am", "Tag", "die", "Ster\u00b7ne", "tre\u00b7ten", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als durch Verfinsterung die Sonn' ihr Licht verlor.", "tokens": ["Als", "durch", "Ver\u00b7fins\u00b7te\u00b7rung", "die", "Sonn'", "ihr", "Licht", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das ist ein pl\u00f6tzliches eingreifendes Geschick,", "tokens": ["Das", "ist", "ein", "pl\u00f6tz\u00b7li\u00b7ches", "ein\u00b7grei\u00b7fen\u00b7des", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das aufthut dem Gem\u00fcth ins Ewige den Blick.", "tokens": ["Das", "auf\u00b7thut", "dem", "Ge\u00b7m\u00fcth", "ins", "E\u00b7wi\u00b7ge", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch der gelinde Weg, wie man am sch\u00f6nsten sieht", "tokens": ["Doch", "der", "ge\u00b7lin\u00b7de", "Weg", ",", "wie", "man", "am", "sch\u00f6ns\u00b7ten", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PWAV", "PIS", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Stern', ist Nachts wann sich zur\u00fcck die Sonne zieht.", "tokens": ["Die", "Stern'", ",", "ist", "Nachts", "wann", "sich", "zu\u00b7r\u00fcck", "die", "Son\u00b7ne", "zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADV", "PWAV", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie tr\u00f6sten in der Nacht dein Auge, wenn es wacht,", "tokens": ["Sie", "tr\u00f6s\u00b7ten", "in", "der", "Nacht", "dein", "Au\u00b7ge", ",", "wenn", "es", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wachen, wenn es schl\u00e4ft, bis neu die Sonne lacht.", "tokens": ["Und", "wa\u00b7chen", ",", "wenn", "es", "schl\u00e4ft", ",", "bis", "neu", "die", "Son\u00b7ne", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}