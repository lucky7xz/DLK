{"textgrid.poem.24263": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Mai-Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich hat sichs eingemait!", "tokens": ["Wie", "lieb\u00b7lich", "hat", "sichs", "ein\u00b7ge\u00b7mait", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde schwimmt in Bl\u00fcten.", "tokens": ["Die", "Er\u00b7de", "schwimmt", "in", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die h\u00f6chst willkommne Zeit,", "tokens": ["Das", "ist", "die", "h\u00f6chst", "will\u00b7komm\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alles will beg\u00fcten.", "tokens": ["Die", "al\u00b7les", "will", "be\u00b7g\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun werden die h\u00e4rtesten Herzen gelinder,", "tokens": ["Nun", "wer\u00b7den", "die", "h\u00e4r\u00b7tes\u00b7ten", "Her\u00b7zen", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wir laufen ins Gr\u00fcne wie lachende Kinder,", "tokens": ["Wir", "lau\u00b7fen", "ins", "Gr\u00fc\u00b7ne", "wie", "la\u00b7chen\u00b7de", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Nun werden wir t\u00f6richt und werden gescheit.", "tokens": ["Nun", "wer\u00b7den", "wir", "t\u00f6\u00b7richt", "und", "wer\u00b7den", "ge\u00b7scheit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So geht es jedes liebe Jahr:", "tokens": ["So", "geht", "es", "je\u00b7des", "lie\u00b7be", "Jahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird man im Winter tr\u00fcbe,", "tokens": ["Wird", "man", "im", "Win\u00b7ter", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So ists im Maimond wunderbar,", "tokens": ["So", "ists", "im", "Mai\u00b7mond", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob sich alles h\u00fcbe.", "tokens": ["Als", "ob", "sich", "al\u00b7les", "h\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es fliehen die Wolken der Seele in Ballen,", "tokens": ["Es", "flie\u00b7hen", "die", "Wol\u00b7ken", "der", "See\u00b7le", "in", "Bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es will uns das Leben nun wieder gefallen,", "tokens": ["Es", "will", "uns", "das", "Le\u00b7ben", "nun", "wie\u00b7der", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wir f\u00fchlen, wie t\u00f6richt das Tr\u00fcbesein war.", "tokens": ["Wir", "f\u00fch\u00b7len", ",", "wie", "t\u00f6\u00b7richt", "das", "Tr\u00fc\u00b7be\u00b7sein", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Drum singen wir dem ersten Mai", "tokens": ["Drum", "sin\u00b7gen", "wir", "dem", "ers\u00b7ten", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach altem Brauch Willkommen.", "tokens": ["Nach", "al\u00b7tem", "Brauch", "Will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er mache alle Herzen frei", "tokens": ["Er", "ma\u00b7che", "al\u00b7le", "Her\u00b7zen", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00f6ge allen frommen.", "tokens": ["Und", "m\u00f6\u00b7ge", "al\u00b7len", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Insonderheit soll er verliebten Leuten", "tokens": ["In\u00b7son\u00b7der\u00b7heit", "soll", "er", "ver\u00b7lieb\u00b7ten", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch heuer die seligsten Stunden bedeuten.", "tokens": ["Auch", "heu\u00b7er", "die", "se\u00b7ligs\u00b7ten", "Stun\u00b7den", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Das ist unser Mai-Wunsch. Amen! Es sei!", "tokens": ["Das", "ist", "un\u00b7ser", "Mai\u00b7Wunsch", ".", "A\u00b7men", "!", "Es", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "NN", "$.", "PPER", "VAFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}