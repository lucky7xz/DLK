{"textgrid.poem.37652": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Krone", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein alter K\u00f6nig,", "tokens": ["Es", "war", "ein", "al\u00b7ter", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hatt' 'ne sch\u00f6ne Magd;", "tokens": ["Der", "hatt'", "'ne", "sch\u00f6\u00b7ne", "Magd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da freut' er sich nicht wenig,", "tokens": ["Da", "freut'", "er", "sich", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil sie ihm wohl behagt.", "tokens": ["Weil", "sie", "ihm", "wohl", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er l\u00e4\u00dft die Ritter laden,", "tokens": ["Er", "l\u00e4\u00dft", "die", "Rit\u00b7ter", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu seinem Hochzeitfest.", "tokens": ["Zu", "sei\u00b7nem", "Hoch\u00b7zeit\u00b7fest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbes wird dir wahrlich schaden!\u00ab", "tokens": ["\u00bb", "es", "wird", "dir", "wahr\u00b7lich", "scha\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sagt Einer seiner G\u00e4st.", "tokens": ["Sagt", "Ei\u00b7ner", "sei\u00b7ner", "G\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da sprechen sie gleich alle:", "tokens": ["Da", "spre\u00b7chen", "sie", "gleich", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbwir bleiben dir nicht treu,", "tokens": ["\u00bb", "wir", "blei\u00b7ben", "dir", "nicht", "treu", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn du uns aus dem Stalle", "tokens": ["Wenn", "du", "uns", "aus", "dem", "Stal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die K\u00f6nigin holst herbei.\u00ab", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "holst", "her\u00b7bei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er nimmt vom Haupt die Krone,", "tokens": ["Er", "nimmt", "vom", "Haupt", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sieht sie schweigend noch an,", "tokens": ["Er", "sieht", "sie", "schwei\u00b7gend", "noch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wirft sie von dem Throne", "tokens": ["Und", "wirft", "sie", "von", "dem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf den ersten, besten Mann.", "tokens": ["Auf", "den", "ers\u00b7ten", ",", "bes\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ruft: \u00bbWer sie gefangen,", "tokens": ["Und", "ruft", ":", "\u00bb", "Wer", "sie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der soll mein K\u00f6nig sein,", "tokens": ["Der", "soll", "mein", "K\u00f6\u00b7nig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' nicht mehr Verlangen", "tokens": ["Ich", "hab'", "nicht", "mehr", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu herrschen ledig allein.\u00ab", "tokens": ["Zu", "herr\u00b7schen", "le\u00b7dig", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbes mag ein jeder werden", "tokens": ["\u00bb", "es", "mag", "ein", "je\u00b7der", "wer\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ART", "PIS", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ich gewesen bin,", "tokens": ["Was", "ich", "ge\u00b7we\u00b7sen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dieweil ich nun auf Erden", "tokens": ["Die\u00b7weil", "ich", "nun", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erst lustig worden bin!\u00ab", "tokens": ["Erst", "lus\u00b7tig", "wor\u00b7den", "bin", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VAPP", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf den die Kron' gefallen,", "tokens": ["Auf", "den", "die", "Kron'", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem schlug sie ein das Hirn,", "tokens": ["Dem", "schlug", "sie", "ein", "das", "Hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das war der Eine von allen,", "tokens": ["Das", "war", "der", "Ei\u00b7ne", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ART", "APPR", "PIAT", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der mit der frechen Stirn.", "tokens": ["Der", "mit", "der", "fre\u00b7chen", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ja wem die Krone fallen,", "tokens": ["Ja", "wem", "die", "Kro\u00b7ne", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem f\u00e4llt ein schweres Loos,", "tokens": ["Dem", "f\u00e4llt", "ein", "schwe\u00b7res", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch vielen sie gefallen,", "tokens": ["Doch", "vie\u00b7len", "sie", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wird er sie bald los!", "tokens": ["So", "wird", "er", "sie", "bald", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}