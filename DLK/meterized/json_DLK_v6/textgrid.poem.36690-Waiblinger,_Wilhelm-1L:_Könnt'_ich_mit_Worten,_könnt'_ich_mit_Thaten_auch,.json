{"textgrid.poem.36690": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nnt' ich mit Worten, k\u00f6nnt' ich mit Thaten auch,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich mit Worten, k\u00f6nnt' ich mit Thaten auch,", "tokens": ["K\u00f6nnt'", "ich", "mit", "Wor\u00b7ten", ",", "k\u00f6nnt'", "ich", "mit", "Tha\u00b7ten", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$,", "VMFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die euer w\u00fcrdig, zeigen, wie dieses Herz", "tokens": ["Die", "eu\u00b7er", "w\u00fcr\u00b7dig", ",", "zei\u00b7gen", ",", "wie", "die\u00b7ses", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJD", "$,", "VVFIN", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Euch liebt, ihr ewig theuren Berge,", "tokens": ["Euch", "liebt", ",", "ihr", "e\u00b7wig", "theu\u00b7ren", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blumige Kette vom Fu\u00df des Cavo,", "tokens": ["Blu\u00b7mi\u00b7ge", "Ket\u00b7te", "vom", "Fu\u00df", "des", "Ca\u00b7vo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "ART", "NE", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Bis wo ihr sanft lieb\u00e4ugelt mit finsterern", "tokens": ["Bis", "wo", "ihr", "sanft", "lie\u00b7b\u00e4u\u00b7gelt", "mit", "fins\u00b7te\u00b7rern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "PPER", "ADJD", "VVFIN", "APPR", "VVINF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sabinernachbarn \u00fcber die Th\u00e4ler weg,", "tokens": ["Sa\u00b7bi\u00b7ner\u00b7nach\u00b7barn", "\u00fc\u00b7ber", "die", "Th\u00e4\u00b7ler", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit euren lind geschwungnen H\u00fcgeln,", "tokens": ["Mit", "eu\u00b7ren", "lind", "ge\u00b7schwung\u00b7nen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heimath des Fr\u00fchlings, des nie verbl\u00fch'nden!", "tokens": ["Hei\u00b7math", "des", "Fr\u00fch\u00b7lings", ",", "des", "nie", "ver\u00b7bl\u00fch'n\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADV", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn ich so still und doch so der Schmerzen voll", "tokens": ["Wenn", "ich", "so", "still", "und", "doch", "so", "der", "Schmer\u00b7zen", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADV", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um Roma's Mauern wandle, wenn mich der Drang", "tokens": ["Um", "Ro\u00b7ma's", "Mau\u00b7ern", "wand\u00b7le", ",", "wenn", "mich", "der", "Drang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "NE", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ins weite warme Feld hinaustreibt,", "tokens": ["Ins", "wei\u00b7te", "war\u00b7me", "Feld", "hin\u00b7aus\u00b7treibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo mir der Spuren von alter Gr\u00f6\u00dfe", "tokens": ["Wo", "mir", "der", "Spu\u00b7ren", "von", "al\u00b7ter", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "So viel begegnet; wenn ich der Appia", "tokens": ["So", "viel", "be\u00b7geg\u00b7net", ";", "wenn", "ich", "der", "Ap\u00b7pia"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$.", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vermorschte R\u00f6mergr\u00e4ber durchwandere,", "tokens": ["Ver\u00b7morschte", "R\u00f6\u00b7mer\u00b7gr\u00e4\u00b7ber", "durch\u00b7wan\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn ich die K\u00f6nigin von Janus", "tokens": ["Wenn", "ich", "die", "K\u00f6\u00b7ni\u00b7gin", "von", "Ja\u00b7nus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seligen Hainen mit Einem Blicke", "tokens": ["Se\u00b7li\u00b7gen", "Hai\u00b7nen", "mit", "Ei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Frei \u00fcberschau', wie l\u00e4chelt ihr da mir zu,", "tokens": ["Frei", "\u00fc\u00b7ber\u00b7schau'", ",", "wie", "l\u00e4\u00b7chelt", "ihr", "da", "mir", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und lockt mich an, als w\u00e4ret ihr Mutter mir,", "tokens": ["Und", "lockt", "mich", "an", ",", "als", "w\u00e4\u00b7ret", "ihr", "Mut\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als h\u00e4tt' ich mich aus eurem Schoo\u00dfe", "tokens": ["Als", "h\u00e4tt'", "ich", "mich", "aus", "eu\u00b7rem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch als ein Kind in die Welt verloren.", "tokens": ["Noch", "als", "ein", "Kind", "in", "die", "Welt", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Seit eure k\u00fchlungschattenden W\u00e4lder mich", "tokens": ["Seit", "eu\u00b7re", "k\u00fch\u00b7lung\u00b7schat\u00b7ten\u00b7den", "W\u00e4l\u00b7der", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In ihre F\u00fclle nahmen, und eure Stirn,", "tokens": ["In", "ih\u00b7re", "F\u00fcl\u00b7le", "nah\u00b7men", ",", "und", "eu\u00b7re", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die weinbekr\u00e4nzte, so unendlich", "tokens": ["Die", "wein\u00b7be\u00b7kr\u00e4nz\u00b7te", ",", "so", "un\u00b7end\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir das tyrrhenische Meer entfaltet,", "tokens": ["Mir", "das", "tyr\u00b7rhe\u00b7ni\u00b7sche", "Meer", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Seit in dreitausendj\u00e4hrigen St\u00e4dten dort", "tokens": ["Seit", "in", "drei\u00b7tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", "St\u00e4d\u00b7ten", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In wilden Massen s\u00fc\u00dfer Geb\u00fcsch', im Duft", "tokens": ["In", "wil\u00b7den", "Mas\u00b7sen", "s\u00fc\u00b7\u00dfer", "Ge\u00b7b\u00fcsch'", ",", "im", "Duft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Veilchen ich die sch\u00f6ne Last des", "tokens": ["Der", "Veil\u00b7chen", "ich", "die", "sch\u00f6\u00b7ne", "Last", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Maulthiers, die reizenden Frauen, zieh'n sah,", "tokens": ["Mault\u00b7hiers", ",", "die", "rei\u00b7zen\u00b7den", "Frau\u00b7en", ",", "zieh'n", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Seitdem verwehte jede Erinnerung", "tokens": ["Seit\u00b7dem", "ver\u00b7weh\u00b7te", "je\u00b7de", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An andre Berg', ihr seid mir so hei\u00df geliebt,", "tokens": ["An", "and\u00b7re", "Ber\u00b7g'", ",", "ihr", "seid", "mir", "so", "hei\u00df", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Da\u00df ich mich selbst vom Capitole", "tokens": ["Da\u00df", "ich", "mich", "selbst", "vom", "Ca\u00b7pi\u00b7to\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frevelnd in euer Elysium sehne.", "tokens": ["Fre\u00b7velnd", "in", "eu\u00b7er", "E\u00b7ly\u00b7si\u00b7um", "seh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Was ihr auch bergt an eurer Dianenbrust,", "tokens": ["Was", "ihr", "auch", "bergt", "an", "eu\u00b7rer", "Di\u00b7a\u00b7nen\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Holdsel'ge G\u00e4rten sch\u00f6pfrischer Fruchtbarkeit,", "tokens": ["Hold\u00b7sel'\u00b7ge", "G\u00e4r\u00b7ten", "sch\u00f6pf\u00b7ri\u00b7scher", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was ihr in Th\u00e4lern, H\u00f6h'n und Ufern", "tokens": ["Was", "ihr", "in", "Th\u00e4\u00b7lern", ",", "H\u00f6h'n", "und", "U\u00b7fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Himmlisches hegt, vor dem Auge steht mir's", "tokens": ["Himm\u00b7li\u00b7sches", "hegt", ",", "vor", "dem", "Au\u00b7ge", "steht", "mir's"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Endlos. Vor allem du, mein Albano, bist", "tokens": ["End\u00b7los", ".", "Vor", "al\u00b7lem", "du", ",", "mein", "Al\u00b7ba\u00b7no", ",", "bist"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "APPR", "PIS", "PPER", "$,", "PPOSAT", "NE", "$,", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dem sanft verj\u00fcngten Herzen die sch\u00f6ne Welt,", "tokens": ["Dem", "sanft", "ver\u00b7j\u00fcng\u00b7ten", "Her\u00b7zen", "die", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die es verlor, bist seine Kindheit,", "tokens": ["Die", "es", "ver\u00b7lor", ",", "bist", "sei\u00b7ne", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bist dem Verlassenen die Geliebte.", "tokens": ["Bist", "dem", "Ver\u00b7las\u00b7se\u00b7nen", "die", "Ge\u00b7lieb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.11": {"line.1": {"text": "O klare Augen ihr meines Latiums,", "tokens": ["O", "kla\u00b7re", "Au\u00b7gen", "ihr", "mei\u00b7nes", "La\u00b7ti\u00b7ums", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du See von Nemi, du mein Albanersee,", "tokens": ["Du", "See", "von", "Ne\u00b7mi", ",", "du", "mein", "Al\u00b7ba\u00b7ner\u00b7see", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie lauter strahlet eure Seele", "tokens": ["Wie", "lau\u00b7ter", "strah\u00b7let", "eu\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehnsucht und Liebe zu eurem Himmel! ", "tokens": ["Sehn\u00b7sucht", "und", "Lie\u00b7be", "zu", "eu\u00b7rem", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Jungfr\u00e4ulich hat die Mutter Natur euch schon", "tokens": ["Jung\u00b7fr\u00e4u\u00b7lich", "hat", "die", "Mut\u00b7ter", "Na\u00b7tur", "euch", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bekr\u00e4nzt mit nie verwelkendem Bl\u00fcthenreiz,", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "nie", "ver\u00b7wel\u00b7ken\u00b7dem", "Bl\u00fc\u00b7then\u00b7reiz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Dichter der Natur, die frohen", "tokens": ["Die", "Dich\u00b7ter", "der", "Na\u00b7tur", ",", "die", "fro\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "V\u00f6gel, sie jubeln schon euer Brautlied.", "tokens": ["V\u00f6\u00b7gel", ",", "sie", "ju\u00b7beln", "schon", "eu\u00b7er", "Braut\u00b7lied", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Und du Ariccia, Tochter Siculia's, ", "tokens": ["Und", "du", "A\u00b7ric\u00b7cia", ",", 