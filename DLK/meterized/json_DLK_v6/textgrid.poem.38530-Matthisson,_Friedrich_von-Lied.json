{"textgrid.poem.38530": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Strauche, den des M\u00e4dchens Hand", "tokens": ["Am", "Strau\u00b7che", ",", "den", "des", "M\u00e4d\u00b7chens", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Fr\u00fchlingstanze streifte,", "tokens": ["Im", "Fr\u00fch\u00b7lings\u00b7tan\u00b7ze", "streif\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Silberthau auf ihr Gewand", "tokens": ["Da\u00df", "Sil\u00b7bert\u00b7hau", "auf", "ihr", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus jeder Bl\u00fcthe tr\u00e4ufte:", "tokens": ["Aus", "je\u00b7der", "Bl\u00fc\u00b7the", "tr\u00e4uf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erinnrung! soll, zu deinem Preis,", "tokens": ["E\u00b7rinn\u00b7rung", "!", "soll", ",", "zu", "dei\u00b7nem", "Preis", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Altar sich erheben,", "tokens": ["Ein", "Al\u00b7tar", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Bekr\u00e4nzt mit Rosen, roth und wei\u00df,", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "Ro\u00b7sen", ",", "roth", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umgr\u00fcnt von jungen Reben.", "tokens": ["Um\u00b7gr\u00fcnt", "von", "jun\u00b7gen", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier wo, mit holdem Engelgru\u00df,", "tokens": ["Hier", "wo", ",", "mit", "hol\u00b7dem", "En\u00b7gel\u00b7gru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie mir ins Auge blickte,", "tokens": ["Sie", "mir", "ins", "Au\u00b7ge", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ich den ersten Flammenku\u00df", "tokens": ["Und", "ich", "den", "ers\u00b7ten", "Flam\u00b7men\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihre Lippen dr\u00fcckte:", "tokens": ["Auf", "ih\u00b7re", "Lip\u00b7pen", "dr\u00fcck\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Hofnung! dankbar weih' ich hier,", "tokens": ["O", "Hof\u00b7nung", "!", "dank\u00b7bar", "weih'", "ich", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit jedem jungen Lenze,", "tokens": ["Mit", "je\u00b7dem", "jun\u00b7gen", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor allen Himmelst\u00f6chtern dir", "tokens": ["Vor", "al\u00b7len", "Him\u00b7mel\u00b7st\u00f6ch\u00b7tern", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Gartens erste Kr\u00e4nze.", "tokens": ["Des", "Gar\u00b7tens", "ers\u00b7te", "Kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}