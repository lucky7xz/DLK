{"textgrid.poem.31492": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die sch\u00f6ne Morgenr\u00f6t lie\u00df Purpur-Perlen fallen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Morgenr\u00f6t lie\u00df Purpur-Perlen fallen /", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6t", "lie\u00df", "Pur\u00b7pur\u00b7Per\u00b7len", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Kindheit: bald darauf die Jesus-Sonne scheint /", "tokens": ["die", "Kind\u00b7heit", ":", "bald", "da\u00b7rauf", "die", "Je\u00b7sus\u00b7Son\u00b7ne", "scheint", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "PAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in welcher Gottheits Glanz und Liebes-Hitz vereint.", "tokens": ["in", "wel\u00b7cher", "Got\u00b7theits", "Glanz", "und", "Lie\u00b7bes\u00b7Hitz", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "All' Herzens-Gnadensch\u00e4tz aus diesem Heil-Liecht strahlen.", "tokens": ["All'", "Her\u00b7zens\u00b7Gna\u00b7den\u00b7sch\u00e4tz", "aus", "die\u00b7sem", "Heil\u00b7Liecht", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das L\u00f6sungs-Ader-Gold mu\u00df Lieb-zerschmelzet wallen.", "tokens": ["Das", "L\u00f6\u00b7sungs\u00b7A\u00b7der\u00b7Gold", "mu\u00df", "Lie\u00b7bzer\u00b7schmel\u00b7zet", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Wunder-g\u00fctig Kind vor gier und sehnen weint:", "tokens": ["Das", "Wun\u00b7der\u00b7g\u00fc\u00b7tig", "Kind", "vor", "gier", "und", "seh\u00b7nen", "weint", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "es komme nimmermehr die Heiles-Stund / es meynt /", "tokens": ["es", "kom\u00b7me", "nim\u00b7mer\u00b7mehr", "die", "Hei\u00b7les\u00b7Stund", "/", "es", "meynt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die Segen-Flut sein Blut zu giessen aus mit allen.", "tokens": ["die", "Se\u00b7gen\u00b7Flut", "sein", "Blut", "zu", "gies\u00b7sen", "aus", "mit", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Ewig Gottheit sich in Kindes-W\u00f6lklein h\u00fcllt.", "tokens": ["Die", "E\u00b7wig", "Got\u00b7theit", "sich", "in", "Kin\u00b7des\u00b7W\u00f6lk\u00b7lein", "h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gleich wie uns dunkt die Sonn' in eine Hand zufassen /", "tokens": ["Gleich", "wie", "uns", "dunkt", "die", "Sonn'", "in", "ei\u00b7ne", "Hand", "zu\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "von wegen grosser Weit: so Er / als Gott / erf\u00fcllt", "tokens": ["von", "we\u00b7gen", "gros\u00b7ser", "Weit", ":", "so", "Er", "/", "als", "Gott", "/", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "APPR", "ADJA", "NN", "$.", "ADV", "PPER", "$(", "KOUS", "NN", "$(", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das ganze All / und will sich gleichwol tragen lassen.", "tokens": ["das", "gan\u00b7ze", "All", "/", "und", "will", "sich", "gleich\u00b7wol", "tra\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VMFIN", "PRF", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es hat die selbste Ruh nit Ruhe / bis sie stillt", "tokens": ["Es", "hat", "die", "selbs\u00b7te", "Ruh", "nit", "Ru\u00b7he", "/", "bis", "sie", "stillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "NN", "$(", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der Menschen Vngl\u00fccks-Lauff / ihr Gter / und Gottes hassen.", "tokens": ["der", "Men\u00b7schen", "Vn\u00b7gl\u00fccks\u00b7Lauff", "/", "ihr", "Gter", "/", "und", "Got\u00b7tes", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PPOSAT", "NN", "$(", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}