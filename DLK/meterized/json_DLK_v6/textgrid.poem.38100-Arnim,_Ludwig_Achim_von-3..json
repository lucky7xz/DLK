{"textgrid.poem.38100": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Frau stand auf, sie eilet von dem Tische", "tokens": ["Die", "Frau", "stand", "auf", ",", "sie", "ei\u00b7let", "von", "dem", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verbarg sich in ihr Gemach,", "tokens": ["Ver\u00b7barg", "sich", "in", "ihr", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und dacht ihrs Herzens Schwere:", "tokens": ["Und", "dacht", "ihrs", "Her\u00b7zens", "Schwe\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbhilf Maria du himmlische K\u00f6nigin", "tokens": ["\u00bb", "hilf", "Ma\u00b7ria", "du", "himm\u00b7li\u00b7sche", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "NE", "NE", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Da\u00df mir nie so Leid geschah", "tokens": ["Da\u00df", "mir", "nie", "so", "Leid", "ge\u00b7schah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja an dem Brembergere.", "tokens": ["Ja", "an", "dem", "Brem\u00b7ber\u00b7ge\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Um meinetwillen litt er Noth,", "tokens": ["Um", "mei\u00b7net\u00b7wil\u00b7len", "litt", "er", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da war er gar unschuldig an,", "tokens": ["Da", "war", "er", "gar", "un\u00b7schul\u00b7dig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es mu\u00df mich immer reuen, um ihn so leid ich hier den Tod", "tokens": ["Es", "mu\u00df", "mich", "im\u00b7mer", "reu\u00b7en", ",", "um", "ihn", "so", "leid", "ich", "hier", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,", "KOUI", "PPER", "ADV", "ADJD", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Meines Leibes er nie gewaltig ward,", "tokens": ["Mei\u00b7nes", "Lei\u00b7bes", "er", "nie", "ge\u00b7wal\u00b7tig", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Red ich bey meinen Treuen;", "tokens": ["Red", "ich", "bey", "mei\u00b7nen", "Treu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Er kam mir nie so nah, da\u00df mir von ihm ward ein Umbefang,", "tokens": ["Er", "kam", "mir", "nie", "so", "nah", ",", "da\u00df", "mir", "von", "ihm", "ward", "ein", "Um\u00b7be\u00b7fang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Des trauer ich sehr, mir ist mein Leben worden krank,", "tokens": ["Des", "trau\u00b7er", "ich", "sehr", ",", "mir", "ist", "mein", "Le\u00b7ben", "wor\u00b7den", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VAPP", "ADJD", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Sich hat verkehrt Herz, Muth und all mein Sinn,", "tokens": ["Sich", "hat", "ver\u00b7kehrt", "Herz", ",", "Muth", "und", "all", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VAFIN", "VVPP", "NN", "$,", "NN", "KON", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und wenn meins Lebens nimmer ist,", "tokens": ["Und", "wenn", "meins", "Le\u00b7bens", "nim\u00b7mer", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So scheid mein arme Seel von mir dahin.\u00ab", "tokens": ["So", "scheid", "mein", "ar\u00b7me", "Seel", "von", "mir", "da\u00b7hin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "PAV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}