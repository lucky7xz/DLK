{"textgrid.poem.24348": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben dieselben und dero gebundene und ungebundene Schrifften", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So viel Vortreffliches schreibt noch kein teutscher Kiel:", "tokens": ["So", "viel", "Vor\u00b7treff\u00b7li\u00b7ches", "schreibt", "noch", "kein", "teut\u00b7scher", "Kiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-++--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Des K\u00f6nigs Tugenden/ Feld-Schlachten/ gro\u00dfe Thaten", "tokens": ["Des", "K\u00f6\u00b7nigs", "Tu\u00b7gen\u00b7den", "/", "Feld\u00b7Schlach\u00b7ten", "/", "gro\u00b7\u00dfe", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "NN", "$(", "ADJA", "NN"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sind deinem klugen Geist und Eyfer wohl gerahten.", "tokens": ["Sind", "dei\u00b7nem", "klu\u00b7gen", "Geist", "und", "Ey\u00b7fer", "wohl", "ge\u00b7rah\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In deinen Schrifften ist kein leichtes Sinnen-Spiel.", "tokens": ["In", "dei\u00b7nen", "Schriff\u00b7ten", "ist", "kein", "leich\u00b7tes", "Sin\u00b7nen\u00b7Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6nsten Geister weckt in jenem Liljen Reich", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Geis\u00b7ter", "weckt", "in", "je\u00b7nem", "Lil\u00b7jen", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des K\u00f6nigs Tugend auf/ des Hofes Trefflichkeiten/", "tokens": ["Des", "K\u00f6\u00b7nigs", "Tu\u00b7gend", "auf", "/", "des", "Ho\u00b7fes", "Treff\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch darum k\u00f6nnen sie nicht ", "tokens": ["Doch", "da\u00b7rum", "k\u00f6n\u00b7nen", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "An Klugheit geht er vor/ in andern ist er gleich.", "tokens": ["An", "Klug\u00b7heit", "geht", "er", "vor", "/", "in", "an\u00b7dern", "ist", "er", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "APPR", "PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}