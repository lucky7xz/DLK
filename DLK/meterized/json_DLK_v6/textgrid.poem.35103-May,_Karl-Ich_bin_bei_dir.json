{"textgrid.poem.35103": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ich bin bei dir", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin bei dir, ob du mich kennst, ob nicht;", "tokens": ["Ich", "bin", "bei", "dir", ",", "ob", "du", "mich", "kennst", ",", "ob", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "KOUS", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6r oder h\u00f6r es nicht, was meine Stimme spricht.", "tokens": ["H\u00f6r", "o\u00b7der", "h\u00f6r", "es", "nicht", ",", "was", "mei\u00b7ne", "Stim\u00b7me", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast die Wahl; du hast auch deine Pflicht;", "tokens": ["Du", "hast", "die", "Wahl", ";", "du", "hast", "auch", "dei\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun st\u00fcrz ins Dunkel, oder steig zum Licht.", "tokens": ["Nun", "st\u00fcrz", "ins", "Dun\u00b7kel", ",", "o\u00b7der", "steig", "zum", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bin bei dir; es kann nicht anders sein;", "tokens": ["Ich", "bin", "bei", "dir", ";", "es", "kann", "nicht", "an\u00b7ders", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin der, der ich bin, ich ewig nur allein.", "tokens": ["Ich", "bin", "der", ",", "der", "ich", "bin", ",", "ich", "e\u00b7wig", "nur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "PRELS", "PPER", "VAFIN", "$,", "PPER", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm mich als Fels, nimm irdschen Sand und Stein;", "tokens": ["Nimm", "mich", "als", "Fels", ",", "nimm", "ird\u00b7schen", "Sand", "und", "Stein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "NN", "$,", "VVIMP", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich st\u00fctz dein Haus; auf Erde f\u00e4llt es ein.", "tokens": ["Ich", "st\u00fctz", "dein", "Haus", ";", "auf", "Er\u00b7de", "f\u00e4llt", "es", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bin bei dir; ich bleibe fort und fort;", "tokens": ["Ich", "bin", "bei", "dir", ";", "ich", "blei\u00b7be", "fort", "und", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich warte hier auf dich, doch warte ich nicht dort.", "tokens": ["Ich", "war\u00b7te", "hier", "auf", "dich", ",", "doch", "war\u00b7te", "ich", "nicht", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo strebst du hin, nach welchem letzten Ort?", "tokens": ["Wo", "strebst", "du", "hin", ",", "nach", "wel\u00b7chem", "letz\u00b7ten", "Ort", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun sprich es aus, sprich das Entscheidungswort!", "tokens": ["Nun", "sprich", "es", "aus", ",", "sprich", "das", "Ent\u00b7schei\u00b7dungs\u00b7wort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}