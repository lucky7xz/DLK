{"textgrid.poem.49219": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich hette Jupiter, nicht Paris, ihm erkoren,", "tokens": ["Dich", "het\u00b7te", "Ju\u00b7pi\u00b7ter", ",", "nicht", "Pa\u00b7ris", ",", "ihm", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PTKNEG", "NE", "$,", "PPER", "VVINF", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und w\u00fcrd' auch jetzt ein Schwan, wann dich kein Schwan geboren;", "tokens": ["Und", "w\u00fcrd'", "auch", "jetzt", "ein", "Schwan", ",", "wann", "dich", "kein", "Schwan", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hei\u00dfest Helena und bist auch so geziert,", "tokens": ["Du", "hei\u00b7\u00dfest", "He\u00b7le\u00b7na", "und", "bist", "auch", "so", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und werest du nicht keusch, du w\u00fcrdest auch entf\u00fchrt.", "tokens": ["Und", "we\u00b7rest", "du", "nicht", "keusch", ",", "du", "w\u00fcr\u00b7dest", "auch", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}