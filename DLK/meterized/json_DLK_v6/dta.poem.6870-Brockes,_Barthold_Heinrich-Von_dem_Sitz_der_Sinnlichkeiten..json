{"dta.poem.6870": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele wircket stets. Von Kunst und Wissen", "tokens": ["Die", "See\u00b7le", "wir\u00b7cket", "stets", ".", "Von", "Kunst", "und", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist sie die Meisterin. Hat sie sie nicht erdacht?", "tokens": ["Ist", "sie", "die", "Meis\u00b7te\u00b7rin", ".", "Hat", "sie", "sie", "nicht", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "VAFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind die Gesetze nicht von ihr gemacht,", "tokens": ["Sind", "die", "Ge\u00b7set\u00b7ze", "nicht", "von", "ihr", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wornach mit solcher Richtigkeit", "tokens": ["Wor\u00b7nach", "mit", "sol\u00b7cher", "Rich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ausgef\u00fchret werden m\u00fcssen?", "tokens": ["Sie", "aus\u00b7ge\u00b7f\u00fch\u00b7ret", "wer\u00b7den", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch feste Regeln wei\u00df des Schildrers kluge Hand", "tokens": ["Durch", "fes\u00b7te", "Re\u00b7geln", "wei\u00df", "des", "Schild\u00b7rers", "klu\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus dem Entwurff, den sein Verstand", "tokens": ["Aus", "dem", "Ent\u00b7wurff", ",", "den", "sein", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zuerst formirt, Entfernungen zu schildern,", "tokens": ["Zu\u00b7erst", "for\u00b7mirt", ",", "Ent\u00b7fer\u00b7nun\u00b7gen", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Wo, von den tr\u00fcgerischen Bildern", "tokens": ["Wo", ",", "von", "den", "tr\u00fc\u00b7ge\u00b7ri\u00b7schen", "Bil\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Z\u00fcge nicht daselbst zu sehn,", "tokens": ["Die", "Z\u00fc\u00b7ge", "nicht", "da\u00b7selbst", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wo sie durch seine Kunst gebildet stehn.", "tokens": ["Wo", "sie", "durch", "sei\u00b7ne", "Kunst", "ge\u00b7bil\u00b7det", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}