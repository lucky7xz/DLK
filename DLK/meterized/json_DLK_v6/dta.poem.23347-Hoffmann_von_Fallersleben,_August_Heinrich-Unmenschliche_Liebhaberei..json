{"dta.poem.23347": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unmenschliche Liebhaberei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4ngt nicht an Hund' und Katzen eure Herzen,", "tokens": ["H\u00e4ngt", "nicht", "an", "Hund'", "und", "Kat\u00b7zen", "eu\u00b7re", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Blumen, Pferd' und Pagagei'n \u2014", "tokens": ["An", "Blu\u00b7men", ",", "Pferd'", "und", "Pa\u00b7ga\u00b7gei'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O lernt doch erst der Menschheit Freud' und Schmerzen", "tokens": ["O", "lernt", "doch", "erst", "der", "Menschheit", "Freud'", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und unter Menschen Mensch zu sein!", "tokens": ["Und", "un\u00b7ter", "Men\u00b7schen", "Mensch", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist euch der Mensch nicht mehr als Hund' und K\u00e4tzchen,", "tokens": ["Ist", "euch", "der", "Mensch", "nicht", "mehr", "als", "Hund'", "und", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Blumen, Pferd' und Papagei'n,", "tokens": ["Als", "Blu\u00b7men", ",", "Pferd'", "und", "Pa\u00b7pa\u00b7gei'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hol' der Teufel jedes eurer Sch\u00e4tzchen,", "tokens": ["So", "hol'", "der", "Teu\u00b7fel", "je\u00b7des", "eu\u00b7rer", "Sch\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und euch, euch hol' er hinterdrein!", "tokens": ["Und", "euch", ",", "euch", "hol'", "er", "hin\u00b7ter\u00b7drein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}