{"textgrid.poem.49461": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will, o tr\u00fcbe Zeit, die mich zerst\u00f6rt im Innern,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will, o tr\u00fcbe Zeit, die mich zerst\u00f6rt im Innern,", "tokens": ["Ich", "will", ",", "o", "tr\u00fc\u00b7be", "Zeit", ",", "die", "mich", "zer\u00b7st\u00f6rt", "im", "In\u00b7nern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "FM", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich an die blauen Tage reiner Lieb' erinnern,", "tokens": ["Mich", "an", "die", "blau\u00b7en", "Ta\u00b7ge", "rei\u00b7ner", "Lieb'", "e\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Einwiegend meine Schmach und meine bittre Lust", "tokens": ["Ein\u00b7wie\u00b7gend", "mei\u00b7ne", "Schmach", "und", "mei\u00b7ne", "bitt\u00b7re", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Kuss auf ihre Hand, nicht auf der andern Brust!", "tokens": ["Im", "Kuss", "auf", "ih\u00b7re", "Hand", ",", "nicht", "auf", "der", "an\u00b7dern", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ich, Tiberius gleich, in diesen finstern Stunden,", "tokens": ["Und", "ich", ",", "Ti\u00b7be\u00b7ri\u00b7us", "gleich", ",", "in", "die\u00b7sen", "fins\u00b7tern", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Ob ich nun Freude oder ob ich Schmerz empfunden,", "tokens": ["Ob", "ich", "nun", "Freu\u00b7de", "o\u00b7der", "ob", "ich", "Schmerz", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will ruhn und denken, fern vom Gl\u00fcck, das schlecht uns lohnt,", "tokens": ["Will", "ruhn", "und", "den\u00b7ken", ",", "fern", "vom", "Gl\u00fcck", ",", "das", "schlecht", "uns", "lohnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVINF", "$,", "ADJD", "APPRART", "NN", "$,", "PDS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der blassen M\u00e4dchen, deren Ehre wir geschont,", "tokens": ["Der", "blas\u00b7sen", "M\u00e4d\u00b7chen", ",", "de\u00b7ren", "Eh\u00b7re", "wir", "ge\u00b7schont", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn auf dem Rasenplatz nach Tanz und frohen Tagen", "tokens": ["Wenn", "auf", "dem", "Ra\u00b7sen\u00b7platz", "nach", "Tanz", "und", "fro\u00b7hen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Im weissen Mondenschein die Kirchuhr zw\u00f6lf geschlagen.", "tokens": ["Im", "weis\u00b7sen", "Mon\u00b7den\u00b7schein", "die", "Kir\u00b7chuhr", "zw\u00f6lf", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}