{"dta.poem.20781": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrswunsch  \n an den Rinderhirten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebter Freund! des h\u00f6chsten G\u00fcte             ", "tokens": ["Ge\u00b7lieb\u00b7ter", "Freund", "!", "des", "h\u00f6chs\u00b7ten", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schenkt abermal ein neues Jahr,", "tokens": ["Schenkt", "a\u00b7ber\u00b7mal", "ein", "neu\u00b7es", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum bringt dir mein erfreut Gem\u00fcthe", "tokens": ["Drum", "bringt", "dir", "mein", "er\u00b7freut", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Pflicht ergebner W\u00fcnsche dar.", "tokens": ["Die", "Pflicht", "er\u00b7geb\u00b7ner", "W\u00fcn\u00b7sche", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Zeiten, Tag und Jahre schwinden,", "tokens": ["Wenn", "Zei\u00b7ten", ",", "Tag", "und", "Jah\u00b7re", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So gr\u00fcnet die Best\u00e4ndigkeit;", "tokens": ["So", "gr\u00fc\u00b7net", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man wird sie stets im Flore finden,", "tokens": ["Man", "wird", "sie", "stets", "im", "Flo\u00b7re", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie \u00e4ndert sich nicht mit der Zeit:", "tokens": ["Sie", "\u00e4n\u00b7dert", "sich", "nicht", "mit", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn sich verwechseln Jahr und Wochen,", "tokens": ["Wenn", "sich", "ver\u00b7wech\u00b7seln", "Jahr", "und", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So bleibt sie doch ununterbrochen.", "tokens": ["So", "bleibt", "sie", "doch", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wo Redlichkeit und Tugend bl\u00fchen,", "tokens": ["Wo", "Red\u00b7lich\u00b7keit", "und", "Tu\u00b7gend", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da ist die Falschheit schon verbannt,", "tokens": ["Da", "ist", "die", "Falschheit", "schon", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Es hei\u00dft vergebens ihr Bem\u00fchen,", "tokens": ["Es", "hei\u00dft", "ver\u00b7ge\u00b7bens", "ihr", "Be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sie findet einen Gegenstand,", "tokens": ["Sie", "fin\u00b7det", "ei\u00b7nen", "Ge\u00b7gen\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der ihr von lauter Treue saget", "tokens": ["Der", "ihr", "von", "lau\u00b7ter", "Treu\u00b7e", "sa\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und alle Flatterei verjaget.", "tokens": ["Und", "al\u00b7le", "Flat\u00b7te\u00b7rei", "ver\u00b7ja\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ich kenne schon dein reines Wesen,", "tokens": ["Ich", "ken\u00b7ne", "schon", "dein", "rei\u00b7nes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Du bist von zarter Kindheit an", "tokens": ["Du", "bist", "von", "zar\u00b7ter", "Kind\u00b7heit", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mein tugendhafter Freund gewesen,", "tokens": ["Mein", "tu\u00b7gend\u00b7haf\u00b7ter", "Freund", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Drum nimm die treuen W\u00fcnsche an,", "tokens": ["Drum", "nimm", "die", "treu\u00b7en", "W\u00fcn\u00b7sche", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die zwar aus schlechter Feder flie\u00dfen", "tokens": ["Die", "zwar", "aus", "schlech\u00b7ter", "Fe\u00b7der", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und sich in diese Zeilen schlie\u00dfen:", "tokens": ["Und", "sich", "in", "die\u00b7se", "Zei\u00b7len", "schlie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der Geber aller guten Gaben,", "tokens": ["Der", "Ge\u00b7ber", "al\u00b7ler", "gu\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der Herr, von dessen G\u00fctigkeit", "tokens": ["Der", "Herr", ",", "von", "des\u00b7sen", "G\u00fc\u00b7tig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wir Seel- und Leibeswohlfahrt haben,", "tokens": ["Wir", "Seel", "und", "Lei\u00b7bes\u00b7wohl\u00b7fahrt", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der wolle bei erneuter Zeit", "tokens": ["Der", "wol\u00b7le", "bei", "er\u00b7neu\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Dein Haupt mit Heil und Kraft belegen,", "tokens": ["Dein", "Haupt", "mit", "Heil", "und", "Kraft", "be\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Er kr\u00f6ne dich mit reichen Seegen.", "tokens": ["Er", "kr\u00f6\u00b7ne", "dich", "mit", "rei\u00b7chen", "See\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Er wende was dich kann betr\u00fcben,", "tokens": ["Er", "wen\u00b7de", "was", "dich", "kann", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und schenke was dein Wohlseyn mehrt,", "tokens": ["Und", "schen\u00b7ke", "was", "dein", "Wohl\u00b7seyn", "mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Er st\u00fcrze die dein Ungl\u00fcck lieben;", "tokens": ["Er", "st\u00fcr\u00b7ze", "die", "dein", "Un\u00b7gl\u00fcck", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und wenn er meinen Wunsch erh\u00f6rt,", "tokens": ["Und", "wenn", "er", "mei\u00b7nen", "Wunsch", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "La\u00df er dich bald was Sch\u00f6nes w\u00e4hlen,", "tokens": ["La\u00df", "er", "dich", "bald", "was", "Sch\u00f6\u00b7nes", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "PWS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und viel vergn\u00fcgte Jahre z\u00e4hlen.", "tokens": ["Und", "viel", "ver\u00b7gn\u00fcg\u00b7te", "Jah\u00b7re", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}