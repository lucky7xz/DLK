{"textgrid.poem.47872": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es t\u00f6nen Trompeten/ die Fahnen \u00fcmfliegen/", "tokens": ["Es", "t\u00f6\u00b7nen", "Trom\u00b7pe\u00b7ten", "/", "die", "Fah\u00b7nen", "\u00fcm\u00b7flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "es dr\u00f6nen die Waffen mit blutigen Siegen/", "tokens": ["es", "dr\u00f6\u00b7nen", "die", "Waf\u00b7fen", "mit", "blu\u00b7ti\u00b7gen", "Sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "man h\u00f6ret nur Klagen/ und sagen von Kriegen.", "tokens": ["man", "h\u00f6\u00b7ret", "nur", "Kla\u00b7gen", "/", "und", "sa\u00b7gen", "von", "Krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "$(", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie! m\u00fcssen die Musen verstummet erliegen/", "tokens": ["Wie", "!", "m\u00fcs\u00b7sen", "die", "Mu\u00b7sen", "ver\u00b7stum\u00b7met", "er\u00b7lie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "ins Elend verjaget/ und zagend sich schmiegen?", "tokens": ["ins", "E\u00b7lend", "ver\u00b7ja\u00b7get", "/", "und", "za\u00b7gend", "sich", "schmie\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$(", "KON", "ADJD", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "sol unsere Heldenzeit bleiben verschwiegen?", "tokens": ["sol", "un\u00b7se\u00b7re", "Hel\u00b7den\u00b7zeit", "blei\u00b7ben", "ver\u00b7schwie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ihr Teutsche! besinnet eur hohes Verm\u00fcgen/", "tokens": ["Ihr", "Teut\u00b7sche", "!", "be\u00b7sin\u00b7net", "eur", "ho\u00b7hes", "Ver\u00b7m\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "und hasset ausl\u00e4ndischer Sprachen Betr\u00fcgen.", "tokens": ["und", "has\u00b7set", "aus\u00b7l\u00e4n\u00b7di\u00b7scher", "Spra\u00b7chen", "Be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Euch solte nur euere Zunge ben\u00fcgen/", "tokens": ["Euch", "sol\u00b7te", "nur", "eu\u00b7e\u00b7re", "Zun\u00b7ge", "be\u00b7n\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "(in welcher die K\u00fcnste sich gleiches fals f\u00fcgen/)", "tokens": ["(", "in", "wel\u00b7cher", "die", "K\u00fcns\u00b7te", "sich", "glei\u00b7ches", "fals", "f\u00fc\u00b7gen", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PRELS", "ART", "NN", "PRF", "ADJA", "NN", "VVINF", "$(", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "ihr h\u00e4ttet den Helicon l\u00e4ngsten erstiegen/", "tokens": ["ihr", "h\u00e4t\u00b7tet", "den", "He\u00b7li\u00b7con", "l\u00e4ngs\u00b7ten", "er\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "und w\u00fcrde der Griechen Ber\u00fchmen versiegen.", "tokens": ["und", "w\u00fcr\u00b7de", "der", "Grie\u00b7chen", "Be\u00b7r\u00fch\u00b7men", "ver\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Es m\u00fcssen der Stimplere Reimen versiegen/", "tokens": ["Es", "m\u00fcs\u00b7sen", "der", "Stimp\u00b7le\u00b7re", "Rei\u00b7men", "ver\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bald Opitz so liebliche H\u00f6hen erstiegen/", "tokens": ["Bald", "O\u00b7pitz", "so", "lieb\u00b7li\u00b7che", "H\u00f6\u00b7hen", "er\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihm wolten auch r\u00fchmliche Folge nachf\u00fcgen", "tokens": ["Ihm", "wol\u00b7ten", "auch", "r\u00fchm\u00b7li\u00b7che", "Fol\u00b7ge", "nach\u00b7f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Herr Werder und Buchner der Musen Vern\u00fcgen.", "tokens": ["Herr", "Wer\u00b7der", "und", "Buch\u00b7ner", "der", "Mu\u00b7sen", "Ver\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Apelles/ Rist/ Schottel entdekken das Tr\u00fcgen", "tokens": ["A\u00b7pel\u00b7les", "/", "Rist", "/", "Schot\u00b7tel", "ent\u00b7dek\u00b7ken", "das", "Tr\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "NN", "$(", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "der Sprache Ver\u00e4chter: ihr Wunder verm\u00fcgen/", "tokens": ["der", "Spra\u00b7che", "Ver\u00b7\u00e4ch\u00b7ter", ":", "ihr", "Wun\u00b7der", "ver\u00b7m\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "und Schikklichkeit bleibet nun nimmer verschwiegen.", "tokens": ["und", "Schikk\u00b7lich\u00b7keit", "blei\u00b7bet", "nun", "nim\u00b7mer", "ver\u00b7schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Schaut frevele Kl\u00fcgelwitz schmeltzen und schmiegen.", "tokens": ["Schaut", "fre\u00b7ve\u00b7le", "Kl\u00fc\u00b7gel\u00b7witz", "schmelt\u00b7zen", "und", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Hier h\u00f6ret/ wie Christus nicht blieben erliegen", "tokens": ["Hier", "h\u00f6\u00b7ret", "/", "wie", "Chris\u00b7tus", "nicht", "blie\u00b7ben", "er\u00b7lie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KOKOM", "NE", "PTKNEG", "VVFIN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "im Grabe! hier sehet das H\u00f6llenbekriegen!", "tokens": ["im", "Gra\u00b7be", "!", "hier", "se\u00b7het", "das", "H\u00f6l\u00b7len\u00b7be\u00b7krie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Erstaunet Emanuels Himmlisches Siegen!", "tokens": ["Er\u00b7stau\u00b7net", "E\u00b7ma\u00b7nu\u00b7els", "Himm\u00b7li\u00b7sches", "Sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Nun gleichet der Mukken und Adeler Fliegen.", "tokens": ["Nun", "glei\u00b7chet", "der", "Muk\u00b7ken", "und", "A\u00b7de\u00b7ler", "Flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}