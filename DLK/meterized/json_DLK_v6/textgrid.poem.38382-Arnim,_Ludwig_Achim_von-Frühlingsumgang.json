{"textgrid.poem.38382": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsumgang", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut ist mitten in der Fasten,", "tokens": ["Heut", "ist", "mit\u00b7ten", "in", "der", "Fas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da leeren die Bauren die Kasten.", "tokens": ["Da", "lee\u00b7ren", "die", "Bau\u00b7ren", "die", "Kas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Kasten sind alle so leer,", "tokens": ["Die", "Kas\u00b7ten", "sind", "al\u00b7le", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Bescheer uns Gott ein andres Jahr!", "tokens": ["Be\u00b7scheer", "uns", "Gott", "ein", "and\u00b7res", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Fr\u00fcchte im Felde, sie kleiden so wohl,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "im", "Fel\u00b7de", ",", "sie", "klei\u00b7den", "so", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie kleiden dem B\u00e4uerlein die Scheuerlein voll.", "tokens": ["Sie", "klei\u00b7den", "dem", "B\u00e4u\u00b7er\u00b7lein", "die", "Scheu\u00b7er\u00b7lein", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wo sind unsere hiesigen Knaben,", "tokens": ["Wo", "sind", "un\u00b7se\u00b7re", "hie\u00b7si\u00b7gen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die uns den Sommerkranz helfen rumme tragen.", "tokens": ["Die", "uns", "den", "Som\u00b7mer\u00b7kranz", "hel\u00b7fen", "rum\u00b7me", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "VVFIN", "VVINF", "$."], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Sie liegen wohl hinter dem Wingertsberg,", "tokens": ["Sie", "lie\u00b7gen", "wohl", "hin\u00b7ter", "dem", "Win\u00b7gerts\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schaffen ihre H\u00e4ndelein rauh.", "tokens": ["Und", "schaf\u00b7fen", "ih\u00b7re", "H\u00e4n\u00b7del\u00b7ein", "rauh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Jetzt gehn wir vor des Wirten Haus,", "tokens": ["Jetzt", "gehn", "wir", "vor", "des", "Wir\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schaut der Herr zum Fenster raus.", "tokens": ["Da", "schaut", "der", "Herr", "zum", "Fens\u00b7ter", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er schaut wohl raus und wieder n'ein,", "tokens": ["Er", "schaut", "wohl", "raus", "und", "wie\u00b7der", "n'\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Er schenkt uns was ins Beutelein n'ein.", "tokens": ["Er", "schenkt", "uns", "was", "ins", "Beu\u00b7tel\u00b7ein", "n'\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wir schreibens wohl auf ein Lilienblatt,", "tokens": ["Wir", "schrei\u00b7bens", "wohl", "auf", "ein", "Li\u00b7li\u00b7en\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir w\u00fcnschen dem Herrn einen guten Tag.", "tokens": ["Wir", "w\u00fcn\u00b7schen", "dem", "Herrn", "ei\u00b7nen", "gu\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Wir w\u00fcnschen dem Herrn einen goldenen Tisch,", "tokens": ["Wir", "w\u00fcn\u00b7schen", "dem", "Herrn", "ei\u00b7nen", "gol\u00b7de\u00b7nen", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf jeden Spitzen gebackene Fisch.", "tokens": ["Auf", "je\u00b7den", "Spit\u00b7zen", "ge\u00b7ba\u00b7cke\u00b7ne", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Mitten darinnen eine Kante voll Wein,", "tokens": ["Mit\u00b7ten", "da\u00b7rin\u00b7nen", "ei\u00b7ne", "Kan\u00b7te", "voll", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Damit soll er brav lustig seyn.", "tokens": ["Da\u00b7mit", "soll", "er", "brav", "lus\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.11": {"line.1": {"text": "Wir w\u00fcnschen der Frau eine goldene Wiege,", "tokens": ["Wir", "w\u00fcn\u00b7schen", "der", "Frau", "ei\u00b7ne", "gol\u00b7de\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Damit soll sie ihr Kindelein wiegen.", "tokens": ["Da\u00b7mit", "soll", "sie", "ihr", "Kin\u00b7de\u00b7lein", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Wir w\u00fcnschen der Frau eine goldene Schnur,", "tokens": ["Wir", "w\u00fcn\u00b7schen", "der", "Frau", "ei\u00b7ne", "gol\u00b7de\u00b7ne", "Schnur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Damit bindt sie ihr Kindelein zu.", "tokens": ["Da\u00b7mit", "bindt", "sie", "ihr", "Kin\u00b7de\u00b7lein", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Wir w\u00fcnschen dem Herrn einen silbernen Wagen,", "tokens": ["Wir", "w\u00fcn\u00b7schen", "dem", "Herrn", "ei\u00b7nen", "sil\u00b7ber\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Damit soll er ins Himmelreich fahren!", "tokens": ["Da\u00b7mit", "soll", "er", "ins", "Him\u00b7mel\u00b7reich", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}