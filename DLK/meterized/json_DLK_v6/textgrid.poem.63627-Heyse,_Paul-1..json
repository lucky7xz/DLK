{"textgrid.poem.63627": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rispetti singt man abends in der K\u00fchle", "tokens": ["Ris\u00b7pet\u00b7ti", "singt", "man", "a\u00b7bends", "in", "der", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mitternachts zur Stunde der Gespenster.", "tokens": ["Und", "mit\u00b7ter\u00b7nachts", "zur", "Stun\u00b7de", "der", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein wenig aufzuatmen nach der Schw\u00fcle,", "tokens": ["Ein", "we\u00b7nig", "auf\u00b7zu\u00b7at\u00b7men", "nach", "der", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Singt sie ein Liebender am Kammerfenster.", "tokens": ["Singt", "sie", "ein", "Lie\u00b7ben\u00b7der", "am", "Kam\u00b7mer\u00b7fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich singe sie an einem kleinen Grabe,", "tokens": ["Ich", "sin\u00b7ge", "sie", "an", "ei\u00b7nem", "klei\u00b7nen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drin ruht, was ich zumeist geliebet habe.", "tokens": ["Drin", "ruht", ",", "was", "ich", "zu\u00b7meist", "ge\u00b7lie\u00b7bet", "ha\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es kommt kein Gru\u00df, kein Fl\u00fcsterwort zur\u00fccke;", "tokens": ["Es", "kommt", "kein", "Gru\u00df", ",", "kein", "Fl\u00fcs\u00b7ter\u00b7wort", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein armer Spuk nur blieb von so viel Gl\u00fccke.", "tokens": ["Ein", "ar\u00b7mer", "Spuk", "nur", "blieb", "von", "so", "viel", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}