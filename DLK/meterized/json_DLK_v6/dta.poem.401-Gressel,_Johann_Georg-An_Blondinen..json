{"dta.poem.401": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Blondinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist der Sch\u00f6nheit-Bild annehmliche Blondine,", "tokens": ["Du", "bist", "der", "Sch\u00f6n\u00b7heit\u00b7Bild", "an\u00b7nehm\u00b7li\u00b7che", "Blon\u00b7di\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sch\u00f6ne ", "tokens": ["Die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich aber ", "tokens": ["Ich", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und wegen deiner Pracht zu Grund und Boden geht.", "tokens": ["Und", "we\u00b7gen", "dei\u00b7ner", "Pracht", "zu", "Grund", "und", "Bo\u00b7den", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sehe meinen Fall/ ich sp\u00fchre meine Flammen/", "tokens": ["Ich", "se\u00b7he", "mei\u00b7nen", "Fall", "/", "ich", "sp\u00fch\u00b7re", "mei\u00b7ne", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch weil dieselben her aus deinen Augen stammen/", "tokens": ["Doch", "weil", "die\u00b7sel\u00b7ben", "her", "aus", "dei\u00b7nen", "Au\u00b7gen", "stam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDAT", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sehe ich mit Lust wie Dach und Giebel f\u00e4llt", "tokens": ["So", "se\u00b7he", "ich", "mit", "Lust", "wie", "Dach", "und", "Gie\u00b7bel", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KOKOM", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn nur mein Hertz dadurch des ", "tokens": ["Wenn", "nur", "mein", "Hertz", "da\u00b7durch", "des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PAV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}