{"textgrid.poem.33921": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dir Schlimmes oder Gutes", "tokens": ["Was", "dir", "Schlim\u00b7mes", "o\u00b7der", "Gu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch das Leben bringen kann,", "tokens": ["Auch", "das", "Le\u00b7ben", "brin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmst du stets gela\u00dfnen Muthes", "tokens": ["Nimmst", "du", "stets", "ge\u00b7la\u00df\u00b7nen", "Mu\u00b7thes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und zufried'nen Sinnes an.", "tokens": ["Und", "zu\u00b7frie\u00b7d'\u00b7nen", "Sin\u00b7nes", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Nur das Ganze macht dir Sorgen,", "tokens": ["Nur", "das", "Gan\u00b7ze", "macht", "dir", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur, was nie ein Mensch ermi\u00dft,", "tokens": ["Nur", ",", "was", "nie", "ein", "Mensch", "er\u00b7mi\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ein R\u00e4thsel d'rin verborgen,", "tokens": ["Ob", "ein", "R\u00e4th\u00b7sel", "d'\u00b7rin", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ob die\u00df zu l\u00f6sen ist.", "tokens": ["Und", "ob", "die\u00df", "zu", "l\u00f6\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kann der Buchstab' denn ergr\u00fcnden,", "tokens": ["Kann", "der", "Buch\u00b7stab'", "denn", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was das Wort bedeuten soll?", "tokens": ["Was", "das", "Wort", "be\u00b7deu\u00b7ten", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sich Alle treu verb\u00fcnden,", "tokens": ["Wenn", "sich", "Al\u00b7le", "treu", "ver\u00b7b\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird es ja von selber voll.", "tokens": ["Wird", "es", "ja", "von", "sel\u00b7ber", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nimm die Traube, wie die Beere,", "tokens": ["Nimm", "die", "Trau\u00b7be", ",", "wie", "die", "Bee\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm das Leben, wie den Tag!", "tokens": ["Nimm", "das", "Le\u00b7ben", ",", "wie", "den", "Tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was es auch zuletzt bescheere,", "tokens": ["Was", "es", "auch", "zu\u00b7letzt", "be\u00b7schee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer bleibt's ein Lustgelag!", "tokens": ["Im\u00b7mer", "bleibt's", "ein", "Lust\u00b7ge\u00b7lag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}