{"textgrid.poem.60317": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Auf meinen ausgeb\u00e4lgten Geier", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du stehst so still und ernst, mein ausgeb\u00e4lgter Geier,", "tokens": ["Du", "stehst", "so", "still", "und", "ernst", ",", "mein", "aus\u00b7ge\u00b7b\u00e4lg\u00b7ter", "Gei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bringe dir ein Lied mit meiner ernsten Leier.", "tokens": ["Ich", "brin\u00b7ge", "dir", "ein", "Lied", "mit", "mei\u00b7ner", "erns\u00b7ten", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar h\u00f6rst du nichts davon, dir geht mein Gru\u00df verloren;", "tokens": ["Zwar", "h\u00f6rst", "du", "nichts", "da\u00b7von", ",", "dir", "geht", "mein", "Gru\u00df", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PAV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Dichter sind gewohnt, zu singen toten Ohren.", "tokens": ["Doch", "Dich\u00b7ter", "sind", "ge\u00b7wohnt", ",", "zu", "sin\u00b7gen", "to\u00b7ten", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$,", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es lebt ja noch der Geist, der einst dir gab die Schwingen,", "tokens": ["Es", "lebt", "ja", "noch", "der", "Geist", ",", "der", "einst", "dir", "gab", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den traf der J\u00e4ger nicht, er h\u00f6rt mein Lied erklingen.", "tokens": ["Den", "traf", "der", "J\u00e4\u00b7ger", "nicht", ",", "er", "h\u00f6rt", "mein", "Lied", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn kein Menschenohr auch meinem Sange lauschte,", "tokens": ["Und", "wenn", "kein", "Men\u00b7schen\u00b7ohr", "auch", "mei\u00b7nem", "San\u00b7ge", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00f6rt mich doch der Geist, der mir das Herz berauschte.", "tokens": ["So", "h\u00f6rt", "mich", "doch", "der", "Geist", ",", "der", "mir", "das", "Herz", "be\u00b7rauschte", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich wollt, ich w\u00e4re jetzt in fernen Felsenkl\u00fcften,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4\u00b7re", "jetzt", "in", "fer\u00b7nen", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du hoch \u00fcber mir, still kreisend in den L\u00fcften;", "tokens": ["Und", "du", "hoch", "\u00fc\u00b7ber", "mir", ",", "still", "krei\u00b7send", "in", "den", "L\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPR", "PPER", "$,", "ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich lie\u00dfe froh mein Aug mit deinem Fluge schweifen,", "tokens": ["Ich", "lie\u00b7\u00dfe", "froh", "mein", "Aug", "mit", "dei\u00b7nem", "Flu\u00b7ge", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie du niederf\u00e4hrst, die Beute zu ergreifen;", "tokens": ["Und", "wie", "du", "nie\u00b7der\u00b7f\u00e4hrst", ",", "die", "Beu\u00b7te", "zu", "er\u00b7grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie du, atmender Blitz, zu Boden niederz\u00fcckest", "tokens": ["Wie", "du", ",", "at\u00b7men\u00b7der", "Blitz", ",", "zu", "Bo\u00b7den", "nie\u00b7der\u00b7z\u00fc\u00b7ckest"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und mit den Krallen scharf ein warmes Leben pfl\u00fcckest;", "tokens": ["Und", "mit", "den", "Kral\u00b7len", "scharf", "ein", "war\u00b7mes", "Le\u00b7ben", "pfl\u00fc\u00b7ckest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie du das volle Herz ansetzest als ein Zecher,", "tokens": ["Wie", "du", "das", "vol\u00b7le", "Herz", "an\u00b7set\u00b7zest", "als", "ein", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mit dem Leben trinkt der Tod aus einem Becher.", "tokens": ["Da\u00df", "mit", "dem", "Le\u00b7ben", "trinkt", "der", "Tod", "aus", "ei\u00b7nem", "Be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Traun! milder ist der Tod, trotz Blut und Jammerstimme", "tokens": ["Traun", "!", "mil\u00b7der", "ist", "der", "Tod", ",", "trotz", "Blut", "und", "Jam\u00b7mers\u00b7tim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADJD", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo hei\u00dfe Lebenslust sich paart mit seinem Grimme,", "tokens": ["Wo", "hei\u00b7\u00dfe", "Le\u00b7bens\u00b7lust", "sich", "paart", "mit", "sei\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Als wo kein Leben ist beim letzten Hauch zu sehen,", "tokens": ["Als", "wo", "kein", "Le\u00b7ben", "ist", "beim", "letz\u00b7ten", "Hauch", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PIAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo still der Tod uns d\u00fcnkt ein einsames Vergehen.", "tokens": ["Wo", "still", "der", "Tod", "uns", "d\u00fcnkt", "ein", "ein\u00b7sa\u00b7mes", "Ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Ihr Weinenden am Sarg, an seinem dichten Schleier,", "tokens": ["Ihr", "Wei\u00b7nen\u00b7den", "am", "Sarg", ",", "an", "sei\u00b7nem", "dich\u00b7ten", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O kommt ins Felsental mit mir und meinem Geier!", "tokens": ["O", "kommt", "ins", "Fel\u00b7sen\u00b7tal", "mit", "mir", "und", "mei\u00b7nem", "Gei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "O kommt, Unsterblichkeit will die Natur euch lehren,", "tokens": ["O", "kommt", ",", "U\u00b7nsterb\u00b7lich\u00b7keit", "will", "die", "Na\u00b7tur", "euch", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Mit diesem Blute will sie tr\u00f6sten eure Z\u00e4hren.", "tokens": ["Mit", "die\u00b7sem", "Blu\u00b7te", "will", "sie", "tr\u00f6s\u00b7ten", "eu\u00b7re", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Im Kreischen dieses Aars, mags auch die Sinne st\u00f6ren,", "tokens": ["Im", "Krei\u00b7schen", "die\u00b7ses", "Aars", ",", "mags", "auch", "die", "Sin\u00b7ne", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist f\u00fcr die Seele doch ein s\u00fc\u00dfer Klang zu h\u00f6ren.", "tokens": ["Ist", "f\u00fcr", "die", "See\u00b7le", "doch", "ein", "s\u00fc\u00b7\u00dfer", "Klang", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Hier findet Trost ein Mann, ward ihm ein Gl\u00fcck zunichte,", "tokens": ["Hier", "fin\u00b7det", "Trost", "ein", "Mann", ",", "ward", "ihm", "ein", "Gl\u00fcck", "zu\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und n\u00e4her tritt er hier dem R\u00e4tsel der Geschichte.", "tokens": ["Und", "n\u00e4\u00b7her", "tritt", "er", "hier", "dem", "R\u00e4t\u00b7sel", "der", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Der Geist, der hei\u00df nach Blut hie\u00df diesen Geier schmachten,", "tokens": ["Der", "Geist", ",", "der", "hei\u00df", "nach", "Blut", "hie\u00df", "die\u00b7sen", "Gei\u00b7er", "schmach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist der starke Geist zugleich der V\u00f6lkerschlachten;", "tokens": ["Es", "ist", "der", "star\u00b7ke", "Geist", "zu\u00b7gleich", "der", "V\u00f6l\u00b7ker\u00b7schlach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Ein rasches Pochen ists, ein ungeduldigs Dr\u00e4ngen", "tokens": ["Ein", "ra\u00b7sches", "Po\u00b7chen", "ists", ",", "ein", "un\u00b7ge\u00b7dul\u00b7digs", "Dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Seele, ihren Leib, den Kerker, aufzusprengen.", "tokens": ["Der", "See\u00b7le", ",", "ih\u00b7ren", "Leib", ",", "den", "Ker\u00b7ker", ",", "auf\u00b7zu\u00b7spren\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Den gro\u00dfen Kaiser hat einst dieser Geist durchdrungen,", "tokens": ["Den", "gro\u00b7\u00dfen", "Kai\u00b7ser", "hat", "einst", "die\u00b7ser", "Geist", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat ihm hoch sein Schwert zur V\u00f6lkermahd geschwungen;", "tokens": ["Er", "hat", "ihm", "hoch", "sein", "Schwert", "zur", "V\u00f6l\u00b7ker\u00b7mahd", "ge\u00b7schwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Dem J\u00e4ger, der als Wild die Menschheit trieb im Zorne", "tokens": ["Dem", "J\u00e4\u00b7ger", ",", "der", "als", "Wild", "die", "Menschheit", "trieb", "im", "Zor\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "NE", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchs Dickicht seines Heers und Bajonettendorne;", "tokens": ["Durchs", "Di\u00b7ckicht", "sei\u00b7nes", "Heers", "und", "Ba\u00b7jo\u00b7net\u00b7ten\u00b7dor\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Der, wie das Schicksal, fest beim Wehgeheul der Schmerzen,", "tokens": ["Der", ",", "wie", "das", "Schick\u00b7sal", ",", "fest", "beim", "Weh\u00b7ge\u00b7heul", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Saatk\u00f6rner seines Ruhms, warf Kugeln in die Herzen;", "tokens": ["Saat\u00b7k\u00f6r\u00b7ner", "sei\u00b7nes", "Ruhms", ",", "warf", "Ku\u00b7geln", "in", "die", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.20": {"line.1": {"text": "Und der auf Helena, wenn ri