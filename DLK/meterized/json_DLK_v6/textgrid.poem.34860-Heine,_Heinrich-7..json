{"textgrid.poem.34860": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Traum siehst du die stillen", "tokens": ["In", "dem", "Traum", "siehst", "du", "die", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fabelhaften Blumen prangen;", "tokens": ["Fa\u00b7bel\u00b7haf\u00b7ten", "Blu\u00b7men", "pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Sehnsucht und Verlangen", "tokens": ["Und", "mit", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre D\u00fcfte dich erf\u00fcllen.", "tokens": ["Ih\u00b7re", "D\u00fcf\u00b7te", "dich", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch von diesen Blumen scheidet", "tokens": ["Doch", "von", "die\u00b7sen", "Blu\u00b7men", "schei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich ein Abgrund tief und schaurig,", "tokens": ["Dich", "ein", "Ab\u00b7grund", "tief", "und", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Herz wird endlich traurig,", "tokens": ["Und", "dein", "Herz", "wird", "end\u00b7lich", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es blutet und es leidet.", "tokens": ["Und", "es", "blu\u00b7tet", "und", "es", "lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie sie locken, wie sie schimmern!", "tokens": ["Wie", "sie", "lo\u00b7cken", ",", "wie", "sie", "schim\u00b7mern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, wie komm ich da hin\u00fcber?", "tokens": ["Ach", ",", "wie", "komm", "ich", "da", "hin\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meister H\u00e4mmerling, mein Lieber,", "tokens": ["Meis\u00b7ter", "H\u00e4m\u00b7mer\u00b7ling", ",", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannst du mir die Br\u00fccke zimmern?", "tokens": ["Kannst", "du", "mir", "die", "Br\u00fc\u00b7cke", "zim\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}