{"dta.poem.11569": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An eine Lieblingsbuche meines Gartens,  \n in deren Stamm ich  H\u00f6lty's  Namen schnitt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Holde Dryas, halte mir still! es schmerzet nur wenig;", "tokens": ["Hol\u00b7de", "Dryas", ",", "hal\u00b7te", "mir", "still", "!", "es", "schmer\u00b7zet", "nur", "we\u00b7nig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "PIS", "$."], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit woll\u00fcstigem Reiz schlie\u00dft sich die Wunde geschwind.", "tokens": ["Mit", "wol\u00b7l\u00fcs\u00b7ti\u00b7gem", "Reiz", "schlie\u00dft", "sich", "die", "Wun\u00b7de", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Eines Dichters Namen zu tragen bist du gew\u00fcrdigt,", "tokens": ["Ei\u00b7nes", "Dich\u00b7ters", "Na\u00b7men", "zu", "tra\u00b7gen", "bist", "du", "ge\u00b7w\u00fcr\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Keinen Lieberen hat Wiese noch Wald mir genannt.", "tokens": ["Kei\u00b7nen", "Lie\u00b7be\u00b7ren", "hat", "Wie\u00b7se", "noch", "Wald", "mir", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "ADV", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "K\u00fcnftig sey du die Erste von allen deinen Geschwistern,", "tokens": ["K\u00fcnf\u00b7tig", "sey", "du", "die", "Ers\u00b7te", "von", "al\u00b7len", "dei\u00b7nen", "Ge\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "ADJA", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Welche der kommende Lenz wecket und reichlich belaubt.", "tokens": ["Wel\u00b7che", "der", "kom\u00b7men\u00b7de", "Lenz", "we\u00b7cket", "und", "reich\u00b7lich", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Und ein liebendes M\u00e4dchen, von deinem Dunkel umduftet,", "tokens": ["Und", "ein", "lie\u00b7ben\u00b7des", "M\u00e4d\u00b7chen", ",", "von", "dei\u00b7nem", "Dun\u00b7kel", "um\u00b7duf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Sieht den Namen, der, halb nur verborgen, ihr winkt;", "tokens": ["Sieht", "den", "Na\u00b7men", ",", "der", ",", "halb", "nur", "ver\u00b7bor\u00b7gen", ",", "ihr", "winkt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "$,", "ADJD", "ADV", "VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Leise dr\u00fcckt sie, gedankenvoll, die Lippen auf diese", "tokens": ["Lei\u00b7se", "dr\u00fcckt", "sie", ",", "ge\u00b7dan\u00b7ken\u00b7voll", ",", "die", "Lip\u00b7pen", "auf", "die\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "$,", "ART", "NN", "APPR", "PDAT"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Lettern, es dringet ihr Ku\u00df dir an das innerste Mark.", "tokens": ["Let\u00b7tern", ",", "es", "drin\u00b7get", "ihr", "Ku\u00df", "dir", "an", "das", "in\u00b7ners\u00b7te", "Mark", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Wehe der Hand, die dich zu sch\u00e4digen waget! Ihr gl\u00fccke", "tokens": ["We\u00b7he", "der", "Hand", ",", "die", "dich", "zu", "sch\u00e4\u00b7di\u00b7gen", "wa\u00b7get", "!", "Ihr", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Nimmer, in Feld und Haus, nimmer ein friedliches", "tokens": ["Nim\u00b7mer", ",", "in", "Feld", "und", "Haus", ",", "nim\u00b7mer", "ein", "fried\u00b7li\u00b7ches"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "NN", "KON", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}}}}}