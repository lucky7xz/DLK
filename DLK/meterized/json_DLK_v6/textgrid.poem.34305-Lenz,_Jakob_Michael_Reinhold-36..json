{"textgrid.poem.34305": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwach' ich zum Gef\u00fchl, st\u00f6\u00dft die beklemmte Brust", "tokens": ["Er\u00b7wach'", "ich", "zum", "Ge\u00b7f\u00fchl", ",", "st\u00f6\u00dft", "die", "be\u00b7klemm\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seufzer aus, die sie erstickt, sich unbewu\u00dft.", "tokens": ["Die", "Seuf\u00b7zer", "aus", ",", "die", "sie", "er\u00b7stickt", ",", "sich", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVPP", "$,", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist's recht auch, da\u00df zu deinem Grab die Tr\u00e4nen fliessen,", "tokens": ["Ist's", "recht", "auch", ",", "da\u00df", "zu", "dei\u00b7nem", "Grab", "die", "Tr\u00e4\u00b7nen", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die zur Erleichtrung sich aus tr\u00fcbem Aug' ergiessen?", "tokens": ["Die", "zur", "Er\u00b7leich\u00b7trung", "sich", "aus", "tr\u00fc\u00b7bem", "Aug'", "er\u00b7gies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist's Pflicht, sich sinnenlos um eingestandne Pein,", "tokens": ["Ist's", "Pflicht", ",", "sich", "sin\u00b7nen\u00b7los", "um", "ein\u00b7ge\u00b7stand\u00b7ne", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRF", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verstummend, unerkl\u00e4rt im Herzen zu verzeihn?", "tokens": ["Ver\u00b7stum\u00b7mend", ",", "un\u00b7er\u00b7kl\u00e4rt", "im", "Her\u00b7zen", "zu", "ver\u00b7zeihn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verdunkelt sind nunmehr die Freuden meiner Tage,", "tokens": ["Ver\u00b7dun\u00b7kelt", "sind", "nun\u00b7mehr", "die", "Freu\u00b7den", "mei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein traurig Schicksal bleibt der Vorwurf meiner Klage.", "tokens": ["Dein", "trau\u00b7rig", "Schick\u00b7sal", "bleibt", "der", "Vor\u00b7wurf", "mei\u00b7ner", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und la\u00df die Welt mich schm\u00e4hn, Albert wird mir", "tokens": ["Und", "la\u00df", "die", "Welt", "mich", "schm\u00e4hn", ",", "Al\u00b7bert", "wird", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "PPER", "VVFIN", "$,", "NE", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dich liebt ich als den Freund h\u00f6chst z\u00e4rtlich, engelrein.", "tokens": ["Dich", "liebt", "ich", "als", "den", "Freund", "h\u00f6chst", "z\u00e4rt\u00b7lich", ",", "en\u00b7gel\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein allzu z\u00e4rtlich Herz verlangte Albert nicht.", "tokens": ["Ein", "all\u00b7zu", "z\u00e4rt\u00b7lich", "Herz", "ver\u00b7lang\u00b7te", "Al\u00b7bert", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "NN", "VVFIN", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gern h\u00e4tt' es eingestimmt zu der ", "tokens": ["Gern", "h\u00e4tt'", "es", "ein\u00b7ge\u00b7stimmt", "zu", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dem ungl\u00fcckseel'gen Freund ein Hofnungsblick gegeben,", "tokens": ["Dem", "un\u00b7gl\u00fcck\u00b7seel'\u00b7gen", "Freund", "ein", "Hof\u00b7nungs\u00b7blick", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Um ihm die stille Glut im Busen zu beleben.", "tokens": ["Um", "ihm", "die", "stil\u00b7le", "Glut", "im", "Bu\u00b7sen", "zu", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ja ich ich war's, die's ihm aus seiner Brust fortri\u00df.", "tokens": ["Ja", "ich", "ich", "wa\u00b7r's", ",", "die's", "ihm", "aus", "sei\u00b7ner", "Brust", "for\u00b7tri\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "PPER", "VAFIN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "Durch mich beweint in hofnungsloser K\u00fcmmerniss", "tokens": ["Durch", "mich", "be\u00b7weint", "in", "hof\u00b7nungs\u00b7lo\u00b7ser", "K\u00fcm\u00b7mer\u00b7niss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Mutter den geraubten Sohn", "tokens": ["Die", "Mut\u00b7ter", "den", "ge\u00b7raub\u00b7ten", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und Wilhelm seinen Freund, den er dort fern vom Thron,", "tokens": ["Und", "Wil\u00b7helm", "sei\u00b7nen", "Freund", ",", "den", "er", "dort", "fern", "vom", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Dem Abadona gleich, vielleicht von weiten sieht", "tokens": ["Dem", "A\u00b7ba\u00b7do\u00b7na", "gleich", ",", "viel\u00b7leicht", "von", "wei\u00b7ten", "sieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und heiliger, verkl\u00e4rt, von ihm nun traurig flieht.", "tokens": ["Und", "hei\u00b7li\u00b7ger", ",", "ver\u00b7kl\u00e4rt", ",", "von", "ihm", "nun", "trau\u00b7rig", "flieht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "VVPP", "$,", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dort wo du einsam ruhst, dort irrt die Phantasey", "tokens": ["Dort", "wo", "du", "ein\u00b7sam", "ruhst", ",", "dort", "irrt", "die", "Phan\u00b7ta\u00b7sey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "In schwermutvolle Lust und bricht in Melodey", "tokens": ["In", "schwer\u00b7mut\u00b7vol\u00b7le", "Lust", "und", "bricht", "in", "Me\u00b7lo\u00b7dey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Trauert\u00f6ne aus, h\u00e4uft Vorwurf auf den Schmerz,", "tokens": ["Der", "Trau\u00b7er\u00b7t\u00f6\u00b7ne", "aus", ",", "h\u00e4uft", "Vor\u00b7wurf", "auf", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der langsam t\u00f6dtend auch zerrei\u00dft mein leidend Herz.", "tokens": ["Der", "lang\u00b7sam", "t\u00f6d\u00b7tend", "auch", "zer\u00b7rei\u00dft", "mein", "lei\u00b7dend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach w\u00e4r' es mein Geschick dich einst zu \u00fcberleben,", "tokens": ["Ach", "w\u00e4r'", "es", "mein", "Ge\u00b7schick", "dich", "einst", "zu", "\u00fc\u00b7berl\u00b7e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "PPOSAT", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "F\u00fcr was f\u00fcr einen Prei\u00df h\u00e4tt' ich mich dir ergeben! \u2013", "tokens": ["F\u00fcr", "was", "f\u00fcr", "ei\u00b7nen", "Prei\u00df", "h\u00e4tt'", "ich", "mich", "dir", "er\u00b7ge\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das r\u00fchrende Geschenk, das deine Hand mir gab,", "tokens": ["Das", "r\u00fch\u00b7ren\u00b7de", "Ge\u00b7schenk", ",", "das", "dei\u00b7ne", "Hand", "mir", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.28": {"text": "Erinnert mich ans Grab.", "tokens": ["E\u00b7rin\u00b7nert", "mich", "ans", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Ich schaudre, f\u00fchl' es kalt durch meine Adern gehen,", "tokens": ["Ich", "schaud\u00b7re", ",", "f\u00fchl'", "es", "kalt", "durch", "mei\u00b7ne", "A\u00b7dern", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Versteinert bleibt mein Herz als Monument hier stehen.", "tokens": ["Ver\u00b7stei\u00b7nert", "bleibt", "mein", "Herz", "als", "Mo\u00b7nu\u00b7ment", "hier", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "KOUS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}