{"dta.poem.10462": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.57", "no:0.42"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich trat in jene Hallen,", "tokens": ["Ich", "trat", "in", "je\u00b7ne", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo sie mir Treue versprochen;", "tokens": ["Wo", "sie", "mir", "Treu\u00b7e", "ver\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo einst ihre Thr\u00e4nen gefallen,", "tokens": ["Wo", "einst", "ih\u00b7re", "Thr\u00e4\u00b7nen", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sind Schlangen hervor gekrochen.", "tokens": ["Sind", "Schlan\u00b7gen", "her\u00b7vor", "ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}