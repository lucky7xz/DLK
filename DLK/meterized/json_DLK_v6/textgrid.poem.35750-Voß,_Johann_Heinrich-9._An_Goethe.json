{"textgrid.poem.35750": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "9. An Goethe", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch du, der, sinnreich durch Athenes Schenkung,", "tokens": ["Auch", "du", ",", "der", ",", "sinn\u00b7reich", "durch", "A\u00b7the\u00b7nes", "Schen\u00b7kung", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "$,", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Fl\u00fcgelro\u00df, wenn's unf\u00fcgsam sich b\u00e4umet,", "tokens": ["Sein", "Fl\u00fc\u00b7gel\u00b7ro\u00df", ",", "wenn's", "un\u00b7f\u00fcg\u00b7sam", "sich", "b\u00e4u\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Funken schnaubt, mit Kunst und Milde z\u00e4umet,", "tokens": ["Und", "Fun\u00b7ken", "schnaubt", ",", "mit", "Kunst", "und", "Mil\u00b7de", "z\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Hemmen niemals, nur zu freier Lenkung:", "tokens": ["Zum", "Hem\u00b7men", "nie\u00b7mals", ",", "nur", "zu", "frei\u00b7er", "Len\u00b7kung", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du hast, nicht abhold k\u00fcnstelnder Beschr\u00e4nkung,", "tokens": ["Du", "hast", ",", "nicht", "ab\u00b7hold", "k\u00fcns\u00b7teln\u00b7der", "Be\u00b7schr\u00e4n\u00b7kung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PTKNEG", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zwei Vierling' und zwei Dreiling' uns gereimet?", "tokens": ["Zwei", "Vier\u00b7ling'", "und", "zwei", "Drei\u00b7ling'", "uns", "ge\u00b7rei\u00b7met", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "CARD", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wiewohl man hier Kernholz verhaut, hier leimet,", "tokens": ["Wie\u00b7wohl", "man", "hier", "Kern\u00b7holz", "ver\u00b7haut", ",", "hier", "lei\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NN", "VVPP", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Den Geist mit St\u00fcmmlung l\u00e4hmend und Verrenkung?", "tokens": ["Den", "Geist", "mit", "St\u00fcmm\u00b7lung", "l\u00e4h\u00b7mend", "und", "Ver\u00b7ren\u00b7kung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "La\u00df, Freund, die Unform alter Trouvaduren,", "tokens": ["La\u00df", ",", "Freund", ",", "die", "Un\u00b7form", "al\u00b7ter", "Trou\u00b7va\u00b7du\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die einst vor Barbarn, halb galant, halb mystisch,", "tokens": ["Die", "einst", "vor", "Bar\u00b7barn", ",", "halb", "ga\u00b7lant", ",", "halb", "mys\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "ADJD", "ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ableierten ihr klingelndes Sonetto;", "tokens": ["Ab\u00b7lei\u00b7er\u00b7ten", "ihr", "klin\u00b7geln\u00b7des", "So\u00b7net\u00b7to", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und l\u00e4chle mit, wo \u00e4ffische Naturen", "tokens": ["Und", "l\u00e4ch\u00b7le", "mit", ",", "wo", "\u00e4f\u00b7fi\u00b7sche", "Na\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mit rohem Sang' und Klingklang' afterchristisch,", "tokens": ["Mit", "ro\u00b7hem", "Sang'", "und", "Kling\u00b7klang'", "af\u00b7ter\u00b7chris\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Als Lumpenpilgrim, wallen nach Loretto.", "tokens": ["Als", "Lum\u00b7pen\u00b7pil\u00b7grim", ",", "wal\u00b7len", "nach", "Lo\u00b7ret\u00b7to", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}