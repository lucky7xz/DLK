{"textgrid.poem.61859": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ringt bei Fr\u00fchlingswettern", "tokens": ["Nun", "ringt", "bei", "Fr\u00fch\u00b7lings\u00b7wet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich aus der Erde Scho\u00df", "tokens": ["Sich", "aus", "der", "Er\u00b7de", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Blume, Bl\u00fct' und Bl\u00e4ttern", "tokens": ["In", "Blu\u00b7me", ",", "Bl\u00fct'", "und", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alte Sehnsucht los.", "tokens": ["Die", "al\u00b7te", "Sehn\u00b7sucht", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die B\u00e4che h\u00f6r' ich brausen", "tokens": ["Die", "B\u00e4\u00b7che", "h\u00f6r'", "ich", "brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von fern durchs Waldgebiet,", "tokens": ["Von", "fern", "durchs", "Wald\u00b7ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es geht im Wipfelsausen", "tokens": ["Es", "geht", "im", "Wip\u00b7fel\u00b7sau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Auferstehungslied.", "tokens": ["Ein", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7lied", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da schwillt vom dunkeln Triebe", "tokens": ["Da", "schwillt", "vom", "dun\u00b7keln", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allm\u00e4chtig ausgedehnt", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7dehnt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz auch, das der Liebe", "tokens": ["Das", "Herz", "auch", ",", "das", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlornes Gl\u00fcck ersehnt.", "tokens": ["Ver\u00b7lor\u00b7nes", "Gl\u00fcck", "er\u00b7sehnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nicht mehr den Tr\u00e4nenb\u00e4chen", "tokens": ["Nicht", "mehr", "den", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwehrt es streng den Lauf,", "tokens": ["Ver\u00b7wehrt", "es", "streng", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seine Wunden brechen", "tokens": ["Und", "sei\u00b7ne", "Wun\u00b7den", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Woll\u00fcstig blutend auf.", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "blu\u00b7tend", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}