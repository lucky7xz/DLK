{"textgrid.poem.53787": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Pleite", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie Gesch\u00e4fte gehn nicht. Kein Mensch hat Geld.", "tokens": ["\u00bb", "die", "Ge\u00b7sch\u00e4f\u00b7te", "gehn", "nicht", ".", "Kein", "Mensch", "hat", "Geld", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKNEG", "$.", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist ein Elend auf der Welt!", "tokens": ["Es", "ist", "ein", "E\u00b7lend", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Keine Kredite und keine Kunden!", "tokens": ["Kei\u00b7ne", "Kre\u00b7di\u00b7te", "und", "kei\u00b7ne", "Kun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie soll Deutschland dabei gesunden?", "tokens": ["Wie", "soll", "Deutschland", "da\u00b7bei", "ge\u00b7sun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NE", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gesch\u00e4fte machen hat gar keinen Sinn.", "tokens": ["Ge\u00b7sch\u00e4f\u00b7te", "ma\u00b7chen", "hat", "gar", "kei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Herzliche Gr\u00fc\u00dfe! Wir sitzen hier in", "tokens": ["Herz\u00b7li\u00b7che", "Gr\u00fc\u00b7\u00dfe", "!", "Wir", "sit\u00b7zen", "hier", "in"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Lugano.\u00ab", "tokens": ["Lu\u00b7ga\u00b7no", ".", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "\u00bbheut habe ich wieder welche entlassen.", "tokens": ["\u00bb", "heut", "ha\u00b7be", "ich", "wie\u00b7der", "wel\u00b7che", "ent\u00b7las\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wissen Sie, eins kann ich gar nicht fassen,", "tokens": ["Wis\u00b7sen", "Sie", ",", "eins", "kann", "ich", "gar", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PIS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "ununterbrochen frage ich mich:", "tokens": ["un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "fra\u00b7ge", "ich", "mich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wovon leben die Leute eigentlich?", "tokens": ["Wo\u00b7von", "le\u00b7ben", "die", "Leu\u00b7te", "ei\u00b7gent\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Kredite . . . Aufwertung . . . Gro\u00dfindustrie . . .", "tokens": ["Kre\u00b7di\u00b7te", ".", ".", ".", "Auf\u00b7wer\u00b7tung", ".", ".", ".", "Gro\u00df\u00b7in\u00b7dust\u00b7rie", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$.", "NN", "$.", "$.", "$.", "NN", "$.", "$.", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Agrarpolitik . . . Wo wohnen Sie?", "tokens": ["Ag\u00b7rar\u00b7po\u00b7li\u00b7tik", ".", ".", ".", "Wo", "woh\u00b7nen", "Sie", "?"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$.", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ich im Palace.\u00ab", "tokens": ["Ich", "im", "Pa\u00b7la\u00b7ce", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Alle klagen und alle st\u00f6hnen;", "tokens": ["Al\u00b7le", "kla\u00b7gen", "und", "al\u00b7le", "st\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "KON", "PIS", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "keiner kann sich an Friede gew\u00f6hnen.", "tokens": ["kei\u00b7ner", "kann", "sich", "an", "Frie\u00b7de", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Alle st\u00f6hnen und alle klagen", "tokens": ["Al\u00b7le", "st\u00f6h\u00b7nen", "und", "al\u00b7le", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "KON", "PIS", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "und jammern nach alten Dollartagen.", "tokens": ["und", "jam\u00b7mern", "nach", "al\u00b7ten", "Dol\u00b7lar\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Manche h\u00f6rt man aber nicht jammern.", "tokens": ["Man\u00b7che", "h\u00f6rt", "man", "a\u00b7ber", "nicht", "jam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zu sechsen und zehnen in engen Kammern.", "tokens": ["Zu", "sech\u00b7sen", "und", "zeh\u00b7nen", "in", "en\u00b7gen", "Kam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Ausgesperrt. Arbeitslos. Ohne Zeitung,", "tokens": ["Aus\u00b7ge\u00b7sperrt", ".", "Ar\u00b7beits\u00b7los", ".", "Oh\u00b7ne", "Zei\u00b7tung", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "NN", "$.", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ohne Leitartikel zur Klagenverbreitung.", "tokens": ["oh\u00b7ne", "Leit\u00b7ar\u00b7ti\u00b7kel", "zur", "Kla\u00b7gen\u00b7ver\u00b7brei\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Tuberkulose sei ihnen gn\u00e4dig . . .", "tokens": ["Die", "Tu\u00b7ber\u00b7ku\u00b7lo\u00b7se", "sei", "ih\u00b7nen", "gn\u00e4\u00b7dig", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die andern jammern in Rom und Venedig.", "tokens": ["Die", "an\u00b7dern", "jam\u00b7mern", "in", "Rom", "und", "Ve\u00b7ne\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "KON", "NE", "$."], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbkein Geld!\u00ab in den Bergen. \u00bbKein Kredit!\u00ab am Strand.", "tokens": ["\u00bb", "kein", "Geld", "!", "\u00ab", "in", "den", "Ber\u00b7gen", ".", "\u00bb", "Kein", "Kre\u00b7dit", "!", "\u00ab", "am", "Strand", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$.", "$(", "APPR", "ART", "NN", "$.", "$(", "PIAT", "NN", "$.", "$(", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Armes Land.", "tokens": ["Ar\u00b7mes", "Land", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Armes Land.", "tokens": ["Ar\u00b7mes", "Land", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}