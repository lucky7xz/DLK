{"textgrid.poem.56564": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Inn Winter, inn Winter, denn knackert dat Is,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Inn Winter, inn Winter, denn knackert dat Is,", "tokens": ["Inn", "Win\u00b7ter", ",", "inn", "Win\u00b7ter", ",", "denn", "kna\u00b7ckert", "dat", "Is", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "APPR", "NN", "$,", "KON", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "De B\u00f6m hangt vull Rip un de Koppeln s\u00fcnd gris,", "tokens": ["De", "B\u00f6m", "hangt", "vull", "Rip", "un", "de", "Kop\u00b7peln", "s\u00fcnd", "gris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Denn n\u0119hm ik min Scheetpr\u00fcgl un slenker to Lann',", "tokens": ["Denn", "n\u0119hm", "ik", "min", "Scheet\u00b7pr\u00fcgl", "un", "slen\u00b7ker", "to", "Lann'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Umme Ohren de Pudelm\u00fctz un Fusthannschen an.", "tokens": ["Um\u00b7me", "Oh\u00b7ren", "de", "Pu\u00b7del\u00b7m\u00fctz", "un", "Fust\u00b7hann\u00b7schen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "FM", "FM", "FM", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Frenz Buhmann hett Eierbeer, de Kachlabnd is hitt,", "tokens": ["Frenz", "Buh\u00b7mann", "hett", "Ei\u00b7er\u00b7beer", ",", "de", "Kach\u00b7labnd", "is", "hitt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "NN", "$,", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "De S\u00fcnn schint int Finster, dat Feld is so witt:", "tokens": ["De", "S\u00fcnn", "schint", "int", "Fins\u00b7ter", ",", "dat", "Feld", "is", "so", "witt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "NN", "$,", "ART", "NN", "FM", "ADV", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dar knappt wat inn Krattbusch \u2013 wat gift dat en Schall!", "tokens": ["Dar", "knappt", "wat", "inn", "Kratt\u00b7bu\u00b7sch", "\u2013", "wat", "gift", "dat", "en", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Frenz, g\u0119v mi de B\u00fcss, dat ik ok doch mal knall!", "tokens": ["Frenz", ",", "g\u0119v", "mi", "de", "B\u00fcss", ",", "dat", "ik", "ok", "doch", "mal", "knall", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "FM", "FM", "$,", "FM.nl", "FM.nl", "FM.nl", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Oppe Geest is dat lusti! all' Dag oppe Jagd!", "tokens": ["Op\u00b7pe", "Geest", "is", "dat", "lus\u00b7ti", "!", "all'", "Dag", "op\u00b7pe", "Jagd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dat Hart springt asn Hund, wenn de Knappb\u00fcssen kracht!", "tokens": ["Dat", "Hart", "springt", "asn", "Hund", ",", "wenn", "de", "Knapp\u00b7b\u00fcs\u00b7sen", "kracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "KOUS", "NE", "NE", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "De Has' sch\u00fctt koppheister, dat Blot spritt in Snee,", "tokens": ["De", "Has'", "sch\u00fctt", "kop\u00b7pheis\u00b7ter", ",", "dat", "Blot", "spritt", "in", "Snee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "ADJA", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "De Bom sch\u00fctt den Griskopp, as d\u0119 em dat weh.", "tokens": ["De", "Bom", "sch\u00fctt", "den", "Gris\u00b7kopp", ",", "as", "d\u0119", "em", "dat", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "D\u0153r Moor un d\u0153r Wischen, lik\u0153wer, likan,", "tokens": ["D\u0153r", "Moor", "un", "d\u0153r", "Wi\u00b7schen", ",", "li\u00b7k\u0153wer", ",", "li\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "ADJD", "$,", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "D\u0153r Str\u00fcnk un d\u0153r R\u00fcschen, de Stakbusch v\u0153ran!", "tokens": ["D\u0153r", "Str\u00fcnk", "un", "d\u0153r", "R\u00fc\u00b7schen", ",", "de", "Stak\u00b7busch", "v\u0153ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "De Snee is so witt un dat Is is so blank!", "tokens": ["De", "Snee", "is", "so", "witt", "un", "dat", "Is", "is", "so", "blank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ik glitsch as en Dampwagn de Gr\u00f6ben hentlank.", "tokens": ["Ik", "glitsch", "as", "en", "Damp\u00b7wagn", "de", "Gr\u00f6\u00b7ben", "hent\u00b7lank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Sitt j\u00fcm inne Heid bi j\u00fcm Zeitung un Bok,", "tokens": ["Sitt", "j\u00fcm", "in\u00b7ne", "Heid", "bi", "j\u00fcm", "Zei\u00b7tung", "un", "Bok", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Drinkt Thee mit de Frunsl\u00fcd, int Landschopshus Grog,", "tokens": ["Drinkt", "Thee", "mit", "de", "Fruns\u00b7l\u00fcd", ",", "int", "Land\u00b7schops\u00b7hus", "Grog", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NE", "NN", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Un r\u0119dt Politik un sp\u0119lt L'hombre un Whist:", "tokens": ["Un", "r\u0119dt", "Po\u00b7li\u00b7tik", "un", "sp\u0119lt", "L'\u00b7hom\u00b7bre", "un", "Whist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ik lach na den Narrnkram, min Flint inne F\u00fcst!", "tokens": ["Ik", "lach", "na", "den", "Narrn\u00b7kram", ",", "min", "Flint", "in\u00b7ne", "F\u00fcst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Heff Knaken as Isen, en Magn as en P\u0119rd,", "tokens": ["Heff", "Kna\u00b7ken", "as", "I\u00b7sen", ",", "en", "Magn", "as", "en", "P\u0119rd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "NE", "$,", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "B\u00fcn w\u0119li asn T\u0153t mit't Leid \u00fcnnern Steert;", "tokens": ["B\u00fcn", "w\u0119\u00b7li", "asn", "T\u0153t", "mit't", "Leid", "\u00fcn\u00b7nern", "Steert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sla'k mal utn Swengel, ik kam wul int Spor:", "tokens": ["Sla'k", "mal", "utn", "Swen\u00b7gel", ",", "ik", "kam", "wul", "int", "Spor", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Hurrah! d\u0153r den Krattbusch, d\u0153r Wischen un Moor!", "tokens": ["Hur\u00b7rah", "!", "d\u0153r", "den", "Kratt\u00b7bu\u00b7sch", ",", "d\u0153r", "Wi\u00b7schen", "un", "Moor", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ART", "NN", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}