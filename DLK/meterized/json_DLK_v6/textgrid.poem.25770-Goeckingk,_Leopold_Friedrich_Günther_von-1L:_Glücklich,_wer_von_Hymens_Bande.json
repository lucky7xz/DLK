{"textgrid.poem.25770": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fccklich, wer von Hymens Bande", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich, wer von Hymens Bande", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wer", "von", "Hy\u00b7mens", "Ban\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWS", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht zu s\u00fc\u00dfe Hoffnung hegt,", "tokens": ["Nicht", "zu", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit pr\u00fcfendem Verstande,", "tokens": ["Und", "mit", "pr\u00fc\u00b7fen\u00b7dem", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbefangen auf die Wage,", "tokens": ["Un\u00b7be\u00b7fan\u00b7gen", "auf", "die", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So Verdru\u00df als Freude, legt.", "tokens": ["So", "Ver\u00b7dru\u00df", "als", "Freu\u00b7de", ",", "legt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00fccklich, die von ihrem Gatten", "tokens": ["Gl\u00fcck\u00b7lich", ",", "die", "von", "ih\u00b7rem", "Gat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ewges L\u00e4cheln nicht verlangt,", "tokens": ["Ew\u00b7ges", "L\u00e4\u00b7cheln", "nicht", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mitleidend wird verstatten,", "tokens": ["Und", "mit\u00b7lei\u00b7dend", "wird", "ver\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df bei dr\u00fcckenden Gesch\u00e4ften", "tokens": ["Da\u00df", "bei", "dr\u00fc\u00b7cken\u00b7den", "Ge\u00b7sch\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dann und wann das Herz ihm bangt.", "tokens": ["Dann", "und", "wann", "das", "Herz", "ihm", "bangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn bei beiden Herzensg\u00fcte", "tokens": ["Wenn", "bei", "bei\u00b7den", "Her\u00b7zens\u00b7g\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Sanft mit Duldung sich durchflicht,", "tokens": ["Sanft", "mit", "Dul\u00b7dung", "sich", "durch\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Kann zwar ihres Fr\u00fchlings Bl\u00fcthe", "tokens": ["Kann", "zwar", "ih\u00b7res", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wohl ein kleiner Reif befallen,", "tokens": ["Wohl", "ein", "klei\u00b7ner", "Reif", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch erfrieren kann sie nicht.", "tokens": ["Doch", "er\u00b7frie\u00b7ren", "kann", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}