{"dta.poem.21206": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, heiliges Gebirge, sollst also", "tokens": ["Du", ",", "hei\u00b7li\u00b7ges", "Ge\u00b7bir\u00b7ge", ",", "sollst", "al\u00b7so"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Wanderern nicht unbesuchet liegen!", "tokens": ["Von", "Wan\u00b7de\u00b7rern", "nicht", "un\u00b7be\u00b7su\u00b7chet", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel schaut, ihr Pilger, mit Vergn\u00fcgen", "tokens": ["Der", "Him\u00b7mel", "schaut", ",", "ihr", "Pil\u00b7ger", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Anbetung, die ihr hier Jesu weiht.", "tokens": ["Die", "An\u00b7be\u00b7tung", ",", "die", "ihr", "hier", "Je\u00b7su", "weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sollte nicht der Mensch, voll Dankbarkeit,", "tokens": ["Und", "soll\u00b7te", "nicht", "der", "Mensch", ",", "voll", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dich, Golgatha, mit Thr\u00e4nen netzen,", "tokens": ["Dich", ",", "Gol\u00b7ga\u00b7tha", ",", "mit", "Thr\u00e4\u00b7nen", "net\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da alles, was darauf geschah,", "tokens": ["Da", "al\u00b7les", ",", "was", "da\u00b7rauf", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr ihn allein geschah?", "tokens": ["F\u00fcr", "ihn", "al\u00b7lein", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "F\u00fcr Engel zitterte nicht Golgatha;", "tokens": ["F\u00fcr", "En\u00b7gel", "zit\u00b7ter\u00b7te", "nicht", "Gol\u00b7ga\u00b7tha", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PTKNEG", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "F\u00fcr Engel blutete nicht Gottes Lamm,", "tokens": ["F\u00fcr", "En\u00b7gel", "blu\u00b7te\u00b7te", "nicht", "Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PTKNEG", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcr Menschen ganz allein starb es am Kreuzesstamm.", "tokens": ["F\u00fcr", "Men\u00b7schen", "ganz", "al\u00b7lein", "starb", "es", "am", "Kreu\u00b7zes\u00b7stamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und dennoch sehn auch Engel mit Ergetzen,", "tokens": ["Und", "den\u00b7noch", "sehn", "auch", "En\u00b7gel", "mit", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Auf dies Gebirg, und steigen oft herab,", "tokens": ["Auf", "dies", "Ge\u00b7birg", ",", "und", "stei\u00b7gen", "oft", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$,", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und singen Lieder um sein Grab.", "tokens": ["Und", "sin\u00b7gen", "Lie\u00b7der", "um", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "O welche Leiden ohne Zahl", "tokens": ["O", "wel\u00b7che", "Lei\u00b7den", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hat dazumal", "tokens": ["Hat", "da\u00b7zu\u00b7mal"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Der ganze Himmel nicht empfunden,", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", "nicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Als Golgatha zerri\u00df,", "tokens": ["Als", "Gol\u00b7ga\u00b7tha", "zer\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und Todesfinsterni\u00df", "tokens": ["Und", "To\u00b7des\u00b7fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Das Auge des Erl\u00f6sers deckte!", "tokens": ["Das", "Au\u00b7ge", "des", "Er\u00b7l\u00f6\u00b7sers", "deck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}