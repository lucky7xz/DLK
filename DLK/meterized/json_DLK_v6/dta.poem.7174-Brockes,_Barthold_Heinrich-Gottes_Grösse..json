{"dta.poem.7174": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich dann noch weiter gehe,", "tokens": ["Wenn", "ich", "dann", "noch", "wei\u00b7ter", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf einen dritten Stern,", "tokens": ["Und", "auf", "ei\u00b7nen", "drit\u00b7ten", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein erstauntes Auge drehe,", "tokens": ["Mein", "er\u00b7staun\u00b7tes", "Au\u00b7ge", "dre\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der vom ersten etwas fern,", "tokens": ["Der", "vom", "ers\u00b7ten", "et\u00b7was", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erwege drauf die Breite,", "tokens": ["Und", "er\u00b7we\u00b7ge", "drauf", "die", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gr\u00f6ss\u2019 und ungeheure Weite", "tokens": ["Gr\u00f6ss'", "und", "un\u00b7ge\u00b7heu\u00b7re", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses Orts; so k\u00f6mmt mir ein:", "tokens": ["Die\u00b7ses", "Orts", ";", "so", "k\u00f6mmt", "mir", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kein Platz k\u00f6nne gr\u00f6sser seyn.", "tokens": ["Kein", "Platz", "k\u00f6n\u00b7ne", "gr\u00f6s\u00b7ser", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}