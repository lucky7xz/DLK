{"dta.poem.5804": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bewegung ist ein Weg des C\u00f6rpers, der sich r\u00fchret,", "tokens": ["Be\u00b7we\u00b7gung", "ist", "ein", "Weg", "des", "C\u00f6r\u00b7pers", ",", "der", "sich", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er sich aus der Ordnung f\u00fchret", "tokens": ["Wenn", "er", "sich", "aus", "der", "Ord\u00b7nung", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der C\u00f6rper, welche ihn umgeben,", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "ihn", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die in Ruhe, wie wir meinen,", "tokens": ["Und", "die", "in", "Ru\u00b7he", ",", "wie", "wir", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhn zu ber\u00fchren scheinen:", "tokens": ["Jhn", "zu", "be\u00b7r\u00fch\u00b7ren", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um sich nach andern zu erheben,", "tokens": ["Um", "sich", "nach", "an\u00b7dern", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die ihm, indem sie weichen,", "tokens": ["Die", "ihm", ",", "in\u00b7dem", "sie", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den neuen Ort, wonach er strebet, gleichsam reichen.", "tokens": ["Den", "neu\u00b7en", "Ort", ",", "wo\u00b7nach", "er", "stre\u00b7bet", ",", "gleich\u00b7sam", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}