{"textgrid.poem.49181": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "4. Neujahrs-Getichte", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn' hat ihre Reis' auff dieses Jahr vollendet,", "tokens": ["Die", "Sonn'", "hat", "ih\u00b7re", "Reis'", "auff", "die\u00b7ses", "Jahr", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Lieb, du endest noch die harten Sinnen nicht;", "tokens": ["Mein", "Lieb", ",", "du", "en\u00b7dest", "noch", "die", "har\u00b7ten", "Sin\u00b7nen", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie hat den klaren Schein nun wider her gewendet,", "tokens": ["Sie", "hat", "den", "kla\u00b7ren", "Schein", "nun", "wi\u00b7der", "her", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du wendest von mir ab der sch\u00f6nen Augen Liecht.", "tokens": ["Du", "wen\u00b7dest", "von", "mir", "ab", "der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was w\u00fcntsch' ich dir dann jetzt, mein bester Trost, vor Gaben", "tokens": ["Was", "w\u00fcnt\u00b7sch'", "ich", "dir", "dann", "jetzt", ",", "mein", "bes\u00b7ter", "Trost", ",", "vor", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Auff dieses neue Jahr? Geld? Dieses hastu schon.", "tokens": ["Auff", "die\u00b7ses", "neu\u00b7e", "Jahr", "?", "Geld", "?", "Die\u00b7ses", "has\u00b7tu", "schon", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "NN", "$.", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Viel Gl\u00fcck? Auch das ist hier. Wiltu dann Sch\u00f6nheit haben?", "tokens": ["Viel", "Gl\u00fcck", "?", "Auch", "das", "ist", "hier", ".", "Wil\u00b7tu", "dann", "Sch\u00f6n\u00b7heit", "ha\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "ADV", "PDS", "VAFIN", "ADV", "$.", "VMFIN", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du hast sie allbereit und weissest wol darvon.", "tokens": ["Du", "hast", "sie", "all\u00b7be\u00b7reit", "und", "weis\u00b7sest", "wol", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KON", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch etwas ist in dir, wofern' ich es mag sagen,", "tokens": ["Noch", "et\u00b7was", "ist", "in", "dir", ",", "wo\u00b7fern'", "ich", "es", "mag", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "APPR", "PPER", "$,", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Darvon k\u00f6mpt alles Leid und Trauren bey mir her.", "tokens": ["Dar\u00b7von", "k\u00f6mpt", "al\u00b7les", "Leid", "und", "Trau\u00b7ren", "bey", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein grosses Bollwerck steht umb deinen Sinn geschlagen,", "tokens": ["Ein", "gros\u00b7ses", "Boll\u00b7werck", "steht", "umb", "dei\u00b7nen", "Sinn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Di\u00df m\u00f6cht' ich gerne sehn, da\u00df es gef\u00e4llet wer.", "tokens": ["Di\u00df", "m\u00f6cht'", "ich", "ger\u00b7ne", "sehn", ",", "da\u00df", "es", "ge\u00b7f\u00e4l\u00b7let", "wer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVPP", "PWS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier diese Mauer macht, da\u00df meine freye Sinnen,", "tokens": ["Hier", "die\u00b7se", "Mau\u00b7er", "macht", ",", "da\u00df", "mei\u00b7ne", "frey\u00b7e", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein' unverf\u00e4lschte Lieb' und treue Dienste nicht", "tokens": ["Mein'", "un\u00b7ver\u00b7f\u00e4lschte", "Lieb'", "und", "treu\u00b7e", "Diens\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "De\u00df Hertzens hohes Schlo\u00df verm\u00f6gen zu gewinnen;", "tokens": ["De\u00df", "Hert\u00b7zens", "ho\u00b7hes", "Schlo\u00df", "ver\u00b7m\u00f6\u00b7gen", "zu", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Schantz' ist st\u00e4rcker noch als da\u00df sie ein Mensch bricht.", "tokens": ["Die", "Schant\u00b7z'", "ist", "st\u00e4r\u00b7cker", "noch", "als", "da\u00df", "sie", "ein", "Mensch", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "KOKOM", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Ach, wann durch diesen Wall Cupido wolte schiessen!", "tokens": ["Ach", ",", "wann", "durch", "die\u00b7sen", "Wall", "Cu\u00b7pi\u00b7do", "wol\u00b7te", "schies\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "APPR", "PDAT", "NN", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo nicht, so geb' er mir den Bogen und Gewalt,", "tokens": ["Wo", "nicht", ",", "so", "geb'", "er", "mir", "den", "Bo\u00b7gen", "und", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich solt ein grosses Loch bald haben durchgerissen,", "tokens": ["Ich", "solt", "ein", "gros\u00b7ses", "Loch", "bald", "ha\u00b7ben", "durch\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da ich mein L\u00e4ger hett' und steten Auffenthalt.", "tokens": ["Da", "ich", "mein", "L\u00e4\u00b7ger", "hett'", "und", "ste\u00b7ten", "Auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sey nun mit meinem Schatz', o auch mein Schatz, verehret,", "tokens": ["Sey", "nun", "mit", "mei\u00b7nem", "Schatz'", ",", "o", "auch", "mein", "Schatz", ",", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "FM", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dem Hertzen, das ich dir zu schencken au\u00dferkiest;", "tokens": ["Dem", "Hert\u00b7zen", ",", "das", "ich", "dir", "zu", "schen\u00b7cken", "au\u00b7\u00dfer\u00b7kiest", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Verwahr' es ja mir wol, da\u00df es nicht wird versehret,", "tokens": ["Ver\u00b7wahr'", "es", "ja", "mir", "wol", ",", "da\u00df", "es", "nicht", "wird", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie vor das alte Jahr, so jetzt vergangen ist.", "tokens": ["Wie", "vor", "das", "al\u00b7te", "Jahr", ",", "so", "jetzt", "ver\u00b7gan\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}