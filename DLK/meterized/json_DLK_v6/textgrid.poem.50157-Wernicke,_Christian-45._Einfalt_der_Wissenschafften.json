{"textgrid.poem.50157": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "45. Einfalt der Wissenschafften", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am ", "tokens": ["Am"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und auf der ", "tokens": ["Und", "auf", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der durch ein ", "tokens": ["Der", "durch", "ein"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Manch K\u00f6nigreich umsonst in das ", "tokens": ["Manch", "K\u00f6\u00b7nig\u00b7reich", "um\u00b7sonst", "in", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie seyd ihr in der Schul von der Vernunfft so ferne!", "tokens": ["Wie", "seyd", "ihr", "in", "der", "Schul", "von", "der", "Ver\u00b7nunfft", "so", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil ihr, giebt man euch nur Geh\u00f6r,", "tokens": ["Weil", "ihr", ",", "giebt", "man", "euch", "nur", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "PIS", "PPER", "ADV", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}}}}