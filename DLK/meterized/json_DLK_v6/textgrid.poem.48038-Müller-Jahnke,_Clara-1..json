{"textgrid.poem.48038": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Platz, auf dem ich stand", "tokens": ["Das", "ist", "der", "Platz", ",", "auf", "dem", "ich", "stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zum letzten Mal, zum letztem Mal", "tokens": ["zum", "letz\u00b7ten", "Mal", ",", "zum", "letz\u00b7tem", "Mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "an deiner Seite Hand in Hand \u2013 \u2013 \u2013", "tokens": ["an", "dei\u00b7ner", "Sei\u00b7te", "Hand", "in", "Hand", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nun ging ein Wetter \u00fcbers Land,", "tokens": ["nun", "ging", "ein", "Wet\u00b7ter", "\u00fc\u00b7bers", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Luft ward k\u00fchl, das Laub wird fahl.", "tokens": ["die", "Luft", "ward", "k\u00fchl", ",", "das", "Laub", "wird", "fahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jenseits der D\u00fcne sch\u00e4umt das Meer,", "tokens": ["Jen\u00b7seits", "der", "D\u00fc\u00b7ne", "sch\u00e4umt", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sein Rauschen klingt wie Klagesang;", "tokens": ["sein", "Rau\u00b7schen", "klingt", "wie", "Kla\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "scharf weht der Wind von Osten her \u2013 \u2013 \u2013", "tokens": ["scharf", "weht", "der", "Wind", "von", "Os\u00b7ten", "her", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mir pocht das Herz so sehnsuchtsschwer:", "tokens": ["mir", "pocht", "das", "Herz", "so", "sehn\u00b7suchts\u00b7schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ich seh dich nicht, wei\u00df Gott, wie lang!", "tokens": ["ich", "seh", "dich", "nicht", ",", "wei\u00df", "Gott", ",", "wie", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So schleppt sich m\u00fcde Tag f\u00fcr Tag,", "tokens": ["So", "schleppt", "sich", "m\u00fc\u00b7de", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "schon f\u00e4rbt sich rot der Waldessaum;", "tokens": ["schon", "f\u00e4rbt", "sich", "rot", "der", "Wal\u00b7des\u00b7saum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in Tr\u00e4nen steht der Rosenhag \u2013 \u2013 \u2013", "tokens": ["in", "Tr\u00e4\u00b7nen", "steht", "der", "Ro\u00b7sen\u00b7hag", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df ich in deinen Armen lag,", "tokens": ["da\u00df", "ich", "in", "dei\u00b7nen", "Ar\u00b7men", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es d\u00fcnkt mich wie ein Traum . . .", "tokens": ["es", "d\u00fcnkt", "mich", "wie", "ein", "Traum", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}