{"textgrid.poem.59234": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit vielem will die Heimat mich erfreuen,", "tokens": ["Mit", "vie\u00b7lem", "will", "die", "Hei\u00b7mat", "mich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein heitres Schlo\u00df an blaugewundnem Flusse,", "tokens": ["Ein", "heit\u00b7res", "Schlo\u00df", "an", "blau\u00b7ge\u00b7wund\u00b7nem", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gesell'ge Lust, Mutwill und frohe Mu\u00dfe,", "tokens": ["Ge\u00b7sell'\u00b7ge", "Lust", ",", "Mut\u00b7will", "und", "fro\u00b7he", "Mu\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe heitres Spiel, s\u00fc\u00df zu zerstreuen.", "tokens": ["Der", "Lie\u00b7be", "heit\u00b7res", "Spiel", ",", "s\u00fc\u00df", "zu", "zer\u00b7streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wie die Tage freundlich sich erneuen,", "tokens": ["Doch", "wie", "die", "Ta\u00b7ge", "freund\u00b7lich", "sich", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fehlt doch des Freundes Brust in Tat und Mu\u00dfe,", "tokens": ["Fehlt", "doch", "des", "Freun\u00b7des", "Brust", "in", "Tat", "und", "Mu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Ernst, der herrlich schwelget im Genusse,", "tokens": ["Der", "Ernst", ",", "der", "herr\u00b7lich", "schwel\u00b7get", "im", "Ge\u00b7nus\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des reichen Blicks sich wahr und recht zu freuen.", "tokens": ["Des", "rei\u00b7chen", "Blicks", "sich", "wahr", "und", "recht", "zu", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo zwei sich treulich nehmen und erg\u00e4nzen,", "tokens": ["Wo", "zwei", "sich", "treu\u00b7lich", "neh\u00b7men", "und", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "PRF", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4chst unvermerkt das freud'ge Werk der Musen.", "tokens": ["W\u00e4chst", "un\u00b7ver\u00b7merkt", "das", "freu\u00b7d'\u00b7ge", "Werk", "der", "Mu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drum la\u00df mich wieder, Freund, ans Herz dich dr\u00fccken!", "tokens": ["Drum", "la\u00df", "mich", "wie\u00b7der", ",", "Freund", ",", "ans", "Herz", "dich", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Uns beide will noch sch\u00f6n das Leben schm\u00fccken", "tokens": ["Uns", "bei\u00b7de", "will", "noch", "sch\u00f6n", "das", "Le\u00b7ben", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VMFIN", "ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinen reichen, heitern, vollen Kr\u00e4nzen,", "tokens": ["Mit", "sei\u00b7nen", "rei\u00b7chen", ",", "hei\u00b7tern", ",", "vol\u00b7len", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Morgenwind w\u00fchlt um den offnen Busen!", "tokens": ["Der", "Mor\u00b7gen\u00b7wind", "w\u00fchlt", "um", "den", "off\u00b7nen", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}