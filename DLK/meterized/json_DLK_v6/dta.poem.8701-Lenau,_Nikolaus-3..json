{"dta.poem.8701": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Stille wird's im Walde, die lieben, kleinen", "tokens": ["Stil\u00b7le", "wird's", "im", "Wal\u00b7de", ",", "die", "lie\u00b7ben", ",", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$,", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "S\u00e4nger pr\u00fcfen schaukelnd den Ast, der durch die", "tokens": ["S\u00e4n\u00b7ger", "pr\u00fc\u00b7fen", "schau\u00b7kelnd", "den", "Ast", ",", "der", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "ART", "NN", "$,", "PRELS", "APPR", "ART"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nacht dem neuen Fluge sie tr\u00e4gt, den neuen", "tokens": ["Nacht", "dem", "neu\u00b7en", "Flu\u00b7ge", "sie", "tr\u00e4gt", ",", "den", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Liedern entgegen.", "tokens": ["Lie\u00b7dern", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Bald versinkt die Sonne; des Waldes Riesen", "tokens": ["Bald", "ver\u00b7sinkt", "die", "Son\u00b7ne", ";", "des", "Wal\u00b7des", "Rie\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heben h\u00f6her sich in die L\u00fcfte, um noch", "tokens": ["He\u00b7ben", "h\u00f6\u00b7her", "sich", "in", "die", "L\u00fcf\u00b7te", ",", "um", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "APPR", "ART", "NN", "$,", "KOUI", "ADV"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit des Abends fl\u00fcchtigen Rosen sich ihr", "tokens": ["Mit", "des", "A\u00b7bends", "fl\u00fcch\u00b7ti\u00b7gen", "Ro\u00b7sen", "sich", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "PRF", "PPOSAT"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Haupt zu bekr\u00e4nzen.", "tokens": ["Haupt", "zu", "be\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Schon verstummt die Matte, den satten Rindern", "tokens": ["Schon", "ver\u00b7stummt", "die", "Mat\u00b7te", ",", "den", "sat\u00b7ten", "Rin\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Selten nur enthallt das Geglock am Halse,", "tokens": ["Sel\u00b7ten", "nur", "ent\u00b7hallt", "das", "Ge\u00b7glock", "am", "Hal\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Und es pfl\u00fcckt der w\u00e4hlende Zahn nur l\u00e4\u00dfig", "tokens": ["Und", "es", "pfl\u00fcckt", "der", "w\u00e4h\u00b7len\u00b7de", "Zahn", "nur", "l\u00e4\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dunklere Gr\u00e4ser.", "tokens": ["Dunk\u00b7le\u00b7re", "Gr\u00e4\u00b7ser", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und dort blickt der schuldlose Hirt der Sonne", "tokens": ["Und", "dort", "blickt", "der", "schuld\u00b7lo\u00b7se", "Hirt", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sinnend nach, dem Sinnenden jezt entfallen", "tokens": ["Sin\u00b7nend", "nach", ",", "dem", "Sin\u00b7nen\u00b7den", "jezt", "ent\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Fl\u00f6t' und Stab, es falten die H\u00e4nde sich zum", "tokens": ["Fl\u00f6t'", "und", "Stab", ",", "es", "fal\u00b7ten", "die", "H\u00e4n\u00b7de", "sich", "zum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PRF", "APPRART"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Stillen Gebete.", "tokens": ["Stil\u00b7len", "Ge\u00b7be\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}