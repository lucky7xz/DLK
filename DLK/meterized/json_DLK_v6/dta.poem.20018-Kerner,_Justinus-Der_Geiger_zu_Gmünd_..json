{"dta.poem.20018": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Geiger zu Gm\u00fcnd .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst ein Kirchlein sonder gleichen,", "tokens": ["Einst", "ein", "Kirch\u00b7lein", "son\u00b7der", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein Stein von ihm steht da,", "tokens": ["Noch", "ein", "Stein", "von", "ihm", "steht", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Baute Gm\u00fcnd der sangesreichen", "tokens": ["Bau\u00b7te", "Gm\u00fcnd", "der", "san\u00b7ge\u00b7srei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heiligen C\u00e4cilia,", "tokens": ["Hei\u00b7li\u00b7gen", "C\u00e4\u00b7ci\u00b7lia", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Lilien von Silber gl\u00e4nzten", "tokens": ["Li\u00b7li\u00b7en", "von", "Sil\u00b7ber", "gl\u00e4nz\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob der Heil'gen mondenklar,", "tokens": ["Ob", "der", "Heil'\u00b7gen", "mon\u00b7den\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell wie Morgenroth bekr\u00e4nzten", "tokens": ["Hell", "wie", "Mor\u00b7gen\u00b7roth", "be\u00b7kr\u00e4nz\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Goldne Rosen den Altar.", "tokens": ["Gold\u00b7ne", "Ro\u00b7sen", "den", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schuh' aus reinem Gold geschlagen,", "tokens": ["Schuh'", "aus", "rei\u00b7nem", "Gold", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von Silber hell ein Kleid", "tokens": ["Und", "von", "Sil\u00b7ber", "hell", "ein", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJD", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat die Heilige getragen:", "tokens": ["Hat", "die", "Hei\u00b7li\u00b7ge", "ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn da war's noch gute Zeit:", "tokens": ["Denn", "da", "wa\u00b7r's", "noch", "gu\u00b7te", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zeit, wo \u00fcberm fernen Meere,", "tokens": ["Zeit", ",", "wo", "\u00fc\u00b7berm", "fer\u00b7nen", "Mee\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht nur in der Heimat Land,", "tokens": ["Nicht", "nur", "in", "der", "Hei\u00b7mat", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Man der Gm\u00fcnd'schen K\u00fcnstler Ehre", "tokens": ["Man", "der", "Gm\u00fcn\u00b7d'\u00b7schen", "K\u00fcnst\u00b7ler", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hell in Gold und Silber fand.", "tokens": ["Hell", "in", "Gold", "und", "Sil\u00b7ber", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und der fremden Pilger wallten", "tokens": ["Und", "der", "frem\u00b7den", "Pil\u00b7ger", "wall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu C\u00e4cilias Kirchlein viel;", "tokens": ["Zu", "C\u00e4\u00b7ci\u00b7li\u00b7as", "Kirch\u00b7lein", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ungeseh'n woher, erschallten", "tokens": ["Un\u00b7ge\u00b7seh'n", "wo\u00b7her", ",", "er\u00b7schall\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "ADJD", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin Gesang und Orgelspiel.", "tokens": ["Drin", "Ge\u00b7sang", "und", "Or\u00b7gel\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Einst ein Geiger kam gegangen,", "tokens": ["Einst", "ein", "Gei\u00b7ger", "kam", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, den dr\u00fcckte gro\u00dfe Noth,", "tokens": ["Ach", ",", "den", "dr\u00fcck\u00b7te", "gro\u00b7\u00dfe", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Matte Beine, bleiche Wangen,", "tokens": ["Mat\u00b7te", "Bei\u00b7ne", ",", "blei\u00b7che", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Sack kein Geld, kein Brod!", "tokens": ["Und", "im", "Sack", "kein", "Geld", ",", "kein", "Brod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Vor dem Bild hat er gesungen", "tokens": ["Vor", "dem", "Bild", "hat", "er", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gespielet all sein Leid,", "tokens": ["Und", "ge\u00b7spie\u00b7let", "all", "sein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der Heil'gen Herz durchdrungen:", "tokens": ["Hat", "der", "Heil'\u00b7gen", "Herz", "durch\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horch! melodisch rauscht ihr Kleid!", "tokens": ["Horch", "!", "me\u00b7lo\u00b7disch", "rauscht", "ihr", "Kleid", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "L\u00e4chelnd b\u00fcckt das Bild sich nieder", "tokens": ["L\u00e4\u00b7chelnd", "b\u00fcckt", "das", "Bild", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der lebenlosen Ruh,", "tokens": ["Aus", "der", "le\u00b7ben\u00b7lo\u00b7sen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirft dem armen Sohn der Lieder", "tokens": ["Wirft", "dem", "ar\u00b7men", "Sohn", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin den rechten goldnen Schuh.", "tokens": ["Hin", "den", "rech\u00b7ten", "gold\u00b7nen", "Schuh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nach des n\u00e4chsten Goldschmids Hause", "tokens": ["Nach", "des", "n\u00e4chs\u00b7ten", "Gold\u00b7schmids", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt er, ganz vom Gl\u00fcck berauscht,", "tokens": ["Eilt", "er", ",", "ganz", "vom", "Gl\u00fcck", "be\u00b7rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singt und tr\u00e4umt von besten Schmause,", "tokens": ["Singt", "und", "tr\u00e4umt", "von", "bes\u00b7ten", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Schuh um Geld vertauscht.", "tokens": ["Wenn", "der", "Schuh", "um", "Geld", "ver\u00b7tauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aber kaum den Schuh ersehen,", "tokens": ["A\u00b7ber", "kaum", "den", "Schuh", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchrt der Goldschmidt rauhen Ton,", "tokens": ["F\u00fchrt", "der", "Gold\u00b7schmidt", "rau\u00b7hen", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Richter wird mit Schm\u00e4hen", "tokens": ["Und", "zum", "Rich\u00b7ter", "wird", "mit", "Schm\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wild geschleppt des Liedes Sohn.", "tokens": ["Wild", "ge\u00b7schleppt", "des", "Lie\u00b7des", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Bald ist der Proce\u00df geschlichtet,", "tokens": ["Bald", "ist", "der", "Pro\u00b7ce\u00df", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Allen ist es offenbar,", "tokens": ["Al\u00b7len", "ist", "es", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df das Wunder nur erdichtet,", "tokens": ["Da\u00df", "das", "Wun\u00b7der", "nur", "er\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er der frechste R\u00e4uber war.", "tokens": ["Er", "der", "frechs\u00b7te", "R\u00e4u\u00b7ber", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Weh! du armer Sohn der Lieder,", "tokens": ["Weh", "!", "du", "ar\u00b7mer", "Sohn", "der", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sangest wohl den lezten Sang!", "tokens": ["San\u00b7gest", "wohl", "den", "lez\u00b7ten", "Sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Galgen auf und nieder", "tokens": ["An", "dem", "Gal\u00b7gen", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst, ein Vogel, fliegen bang.", "tokens": ["Sollst", ",", "ein", "Vo\u00b7gel", ",", "flie\u00b7gen", "bang", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "NE", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Hell ein Gl\u00f6cklein h\u00f6rt man schallen,", "tokens": ["Hell", "ein", "Gl\u00f6c\u00b7klein", "h\u00f6rt", "man", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und man sieht den schwarzen Zug", "tokens": ["Und", "man", "sieht", "den", "schwar\u00b7zen", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dir zu der St\u00e4tte wallen,", "tokens": ["Mit", "dir", "zu", "der", "St\u00e4t\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo beginnen soll dein Flug.", "tokens": ["Wo", "be\u00b7gin\u00b7nen", "soll", "dein", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Bu\u00dfges\u00e4nge h\u00f6rt man singen", "tokens": ["Bu\u00df\u00b7ge\u00b7s\u00e4n\u00b7ge", "h\u00f6rt", "man", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nonnen und der M\u00f6nche Chor,", "tokens": ["Non\u00b7nen", "und", "der", "M\u00f6n\u00b7che", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber hell auch h\u00f6rt man dringen", "tokens": ["A\u00b7ber", "hell", "auch", "h\u00f6rt", "man", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geigent\u00f6ne draus hervor.", "tokens": ["Gei\u00b7gen\u00b7t\u00f6\u00b7ne", "draus", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Seine Geige mit zu f\u00fchren,", "tokens": ["Sei\u00b7ne", "Gei\u00b7ge", "mit", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War des Geigers letzte Bitt'.", "tokens": ["War", "des", "Gei\u00b7gers", "letz\u00b7te", "Bitt'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ewo so viele musiciren,", "