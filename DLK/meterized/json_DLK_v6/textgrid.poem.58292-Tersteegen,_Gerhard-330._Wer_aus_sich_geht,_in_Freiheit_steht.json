{"textgrid.poem.58292": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "330. Wer aus sich geht, in Freiheit steht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie selig ist ein Mensch, der aus sich selbst gegangen,", "tokens": ["Wie", "se\u00b7lig", "ist", "ein", "Mensch", ",", "der", "aus", "sich", "selbst", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Geiste ganz entbl\u00f6\u00dft und nur des Glaubens lebt,", "tokens": ["Im", "Geis\u00b7te", "ganz", "ent\u00b7bl\u00f6\u00dft", "und", "nur", "des", "Glau\u00b7bens", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der gar nichts will, als Gott zu seinem Trost anhangen,", "tokens": ["Der", "gar", "nichts", "will", ",", "als", "Gott", "zu", "sei\u00b7nem", "Trost", "an\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VMFIN", "$,", "KOUS", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der wie ein Adler stets frei \u00fcber alles schwebt!", "tokens": ["Der", "wie", "ein", "Ad\u00b7ler", "stets", "frei", "\u00fc\u00b7ber", "al\u00b7les", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ADV", "ADJD", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da kriegt der Geist erst Raum, das Herz wird frei und leicht;", "tokens": ["Da", "kriegt", "der", "Geist", "erst", "Raum", ",", "das", "Herz", "wird", "frei", "und", "leicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der dank' in Demut Gott, wer diesen Stand erreicht!", "tokens": ["Der", "dank'", "in", "De\u00b7mut", "Gott", ",", "wer", "die\u00b7sen", "Stand", "er\u00b7reicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "NN", "$,", "PWS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}