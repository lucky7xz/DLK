{"textgrid.poem.43087": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe mich zu erh\u00e4ngen gesucht:", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich zu erh\u00e4ngen gesucht:", "tokens": ["Ich", "ha\u00b7be", "mich", "zu", "er\u00b7h\u00e4n\u00b7gen", "ge\u00b7sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Strick ist abgerissen.", "tokens": ["Der", "Strick", "ist", "ab\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin in's Wasser gesprungen:", "tokens": ["Ich", "bin", "in's", "Was\u00b7ser", "ge\u00b7sprun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie erwischten mich bei den F\u00fc\u00dfen.", "tokens": ["Sie", "er\u00b7wischten", "mich", "bei", "den", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich habe die Adern ge\u00f6ffnet mir:", "tokens": ["Ich", "ha\u00b7be", "die", "A\u00b7dern", "ge\u00b7\u00f6ff\u00b7net", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Man hat mich noch gerettet.", "tokens": ["Man", "hat", "mich", "noch", "ge\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich sprang auch einmal zum Fenster hinaus:", "tokens": ["Ich", "sprang", "auch", "ein\u00b7mal", "zum", "Fens\u00b7ter", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Weich hat der Sand mich gebettet.", "tokens": ["Weich", "hat", "der", "Sand", "mich", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Den Teufel! ich habe nun alles versucht,", "tokens": ["Den", "Teu\u00b7fel", "!", "ich", "ha\u00b7be", "nun", "al\u00b7les", "ver\u00b7sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Woran man sonst kann verderben \u2013", "tokens": ["Wo\u00b7ran", "man", "sonst", "kann", "ver\u00b7der\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VMFIN", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Nun werd' ich wieder zu leben versuchen:", "tokens": ["Nun", "werd'", "ich", "wie\u00b7der", "zu", "le\u00b7ben", "ver\u00b7su\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Vielleicht kann ich dann sterben.", "tokens": ["Viel\u00b7leicht", "kann", "ich", "dann", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}