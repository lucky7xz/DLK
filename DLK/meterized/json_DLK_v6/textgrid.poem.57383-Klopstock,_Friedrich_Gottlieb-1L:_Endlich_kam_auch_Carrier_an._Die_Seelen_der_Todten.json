{"textgrid.poem.57383": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich kam auch Carrier an. Die Seelen der Todten", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich kam auch Carrier an. Die Seelen der Todten", "tokens": ["End\u00b7lich", "kam", "auch", "Car\u00b7rier", "an", ".", "Die", "See\u00b7len", "der", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PTKVZ", "$.", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hielten im Fluge vor Graun.", "tokens": ["Hiel\u00b7ten", "im", "Flu\u00b7ge", "vor", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Einer der hohen Geister sprach zu dem andern, (Sie wolten", "tokens": ["Ei\u00b7ner", "der", "ho\u00b7hen", "Geis\u00b7ter", "sprach", "zu", "dem", "an\u00b7dern", ",", "(", "Sie", "wol\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "$,", "$(", "PPER", "VMFIN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bilden der Seele den schwebenden Leib.)", "tokens": ["Bil\u00b7den", "der", "See\u00b7le", "den", "schwe\u00b7ben\u00b7den", "Leib", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Also sprach er: Leite mir Pestluft her; mit ihr hundert", "tokens": ["Al\u00b7so", "sprach", "er", ":", "Lei\u00b7te", "mir", "Pest\u00b7luft", "her", ";", "mit", "ihr", "hun\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "PPER", "NN", "PTKVZ", "$.", "APPR", "PPOSAT", "CARD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Funken des Abgrunds. Sondere nun", "tokens": ["Fun\u00b7ken", "des", "Ab\u00b7grunds", ".", "Son\u00b7de\u00b7re", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "NN", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Einen Atom des Urlichts ab. \u00bbAch Einen nur?\u00ab Einen!", "tokens": ["Ei\u00b7nen", "A\u00b7tom", "des", "Ur\u00b7lichts", "ab", ".", "\u00bb", "Ach", "Ei\u00b7nen", "nur", "?", "\u00ab", "Ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$.", "$(", "ITJ", "ART", "ADV", "$.", "$(", "ART", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Und der Todte schwebte; doch stets", "tokens": ["Und", "der", "Tod\u00b7te", "schweb\u00b7te", ";", "doch", "stets"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "ADV", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Zittert' er, musste das! Nah war eines Felsengew\u00f6lbes", "tokens": ["Zit\u00b7tert'", "er", ",", "muss\u00b7te", "das", "!", "Nah", "war", "ei\u00b7nes", "Fel\u00b7sen\u00b7ge\u00b7w\u00f6l\u00b7bes"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "PDS", "$.", "NN", "VAFIN", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "\u00d6fnung, in die sich ein reissender Strom", "tokens": ["\u00d6f\u00b7nung", ",", "in", "die", "sich", "ein", "reis\u00b7sen\u00b7der", "Strom"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELS", "PRF", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "St\u00fcrzete, dann in den langen unendlichen W\u00f6lbungen fortfloss,", "tokens": ["St\u00fcr\u00b7ze\u00b7te", ",", "dann", "in", "den", "lan\u00b7gen", "un\u00b7end\u00b7li\u00b7chen", "W\u00f6l\u00b7bun\u00b7gen", "fort\u00b7floss", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+--++--+", "measure": "dactylic.tetra.plus"}, "line.12": {"text": "Oft von steigenden Inseln ges\u00e4umt.", "tokens": ["Oft", "von", "stei\u00b7gen\u00b7den", "In\u00b7seln", "ge\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "\u00dcber dem Strome bebt der Todte. Pl\u00f6tzlich bef\u00e4llt ihn", "tokens": ["\u00dc\u00b7ber", "dem", "Stro\u00b7me", "bebt", "der", "Tod\u00b7te", ".", "Pl\u00f6tz\u00b7lich", "be\u00b7f\u00e4llt", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Immer wiederkehrender Wahn:", "tokens": ["Im\u00b7mer", "wie\u00b7der\u00b7keh\u00b7ren\u00b7der", "Wahn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "J\u00fcnglinge tanzten, und M\u00e4dchen nach Fl\u00f6tenspiel' am Gestade;", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "tanz\u00b7ten", ",", "und", "M\u00e4d\u00b7chen", "nach", "Fl\u00f6\u00b7ten\u00b7spiel'", "am", "Ge\u00b7sta\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Er entbrante mit Wuth, in dem Strom", "tokens": ["Er", "ent\u00b7bran\u00b7te", "mit", "Wuth", ",", "in", "dem", "Strom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Sie zu t\u00f6dten. Bindet sie, br\u00fcllet' er, Henker, zusammen!", "tokens": ["Sie", "zu", "t\u00f6d\u00b7ten", ".", "Bin\u00b7det", "sie", ",", "br\u00fcl\u00b7let'", "er", ",", "Hen\u00b7ker", ",", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und dem Grasslichen kam", "tokens": ["Und", "dem", "Grass\u00b7li\u00b7chen", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Hofnung, er w\u00fcrde morden! nach dem zweyten Gebr\u00fclle,", "tokens": ["Hof\u00b7nung", ",", "er", "w\u00fcr\u00b7de", "mor\u00b7den", "!", "nach", "dem", "zwey\u00b7ten", "Ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVINF", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Wurd' er gewiss!", "tokens": ["Wurd'", "er", "ge\u00b7wiss", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Dieser war der Augenblick, da ein Riesengeyer", "tokens": ["Die\u00b7ser", "war", "der", "Au\u00b7gen\u00b7blick", ",", "da", "ein", "Rie\u00b7sen\u00b7ge\u00b7yer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "\u00dcber ihm kreist', und sich senkend den Tod", "tokens": ["\u00dc\u00b7ber", "ihm", "kreist'", ",", "und", "sich", "sen\u00b7kend", "den", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "PRF", "ADJD", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Ihm ank\u00fcndete. Er entfloh in die Wogen, und lange", "tokens": ["Ihm", "an\u00b7k\u00fcn\u00b7de\u00b7te", ".", "Er", "ent\u00b7floh", "in", "die", "Wo\u00b7gen", ",", "und", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "ADV"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Starb er in ihrem Donnerger\u00e4usch.", "tokens": ["Starb", "er", "in", "ih\u00b7rem", "Don\u00b7ner\u00b7ge\u00b7r\u00e4usch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Aber itzt fasste der Geyer den wiederlebenden, stieg dann", "tokens": ["A\u00b7ber", "itzt", "fass\u00b7te", "der", "Ge\u00b7yer", "den", "wie\u00b7der\u00b7le\u00b7ben\u00b7den", ",", "stieg", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "$,", "VVFIN", "ADV"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.26": {"text": "Fern in die H\u00f6he mit ihm,", "tokens": ["Fern", "in", "die", "H\u00f6\u00b7he", "mit", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Liess von dort ihn fallen in eilende Strudel. Nun starb er", "tokens": ["Liess", "von", "dort", "ihn", "fal\u00b7len", "in", "ei\u00b7len\u00b7de", "Stru\u00b7del", ".", "Nun", "starb", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Wieder den langsamen Tod,", "tokens": ["Wie\u00b7der", "den", "lang\u00b7sa\u00b7men", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "H\u00f6rend das Fl\u00f6tenspiel, den leise wandelnden Nachhall,", "tokens": ["H\u00f6\u00b7rend", "das", "Fl\u00f6\u00b7ten\u00b7spiel", ",", "den", "lei\u00b7se", "wan\u00b7deln\u00b7den", "Nach\u00b7hall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Und der freudigen T\u00e4nzer Gesang.", "tokens": ["Und", "der", "freu\u00b7di\u00b7gen", "T\u00e4n\u00b7zer", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.31": {"text": "Jedesmal wenn er starb, dann standen Get\u00f6dtete, Schatten", "tokens": ["Je\u00b7des\u00b7mal", "wenn", "er", "starb", ",", "dann", "stan\u00b7den", "Ge\u00b7t\u00f6d\u00b7te\u00b7te", ",", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Seiner Opfer, vor ihm.", "tokens": ["Sei\u00b7ner", "Op\u00b7fer", ",", "vor", "ihm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Standen auch Schatten derer, die unter ihm w\u00fcrgten. Sie zuckten", "tokens": ["Stan\u00b7den", "auch", "Schat\u00b7ten", "de\u00b7rer", ",", "die", "un\u00b7ter", "ihm", "w\u00fcrg\u00b7ten", ".", "Sie", "zuck\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "NN", "PDS", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Blinkende Dolche nach ihm,", "tokens": ["Blin\u00b7ken\u00b7de", "Dol\u00b7che", "nach", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "Oder f\u00fcllten ihm bis zu dem triefenden Rande geraume", "tokens": ["O\u00b7der", "f\u00fcll\u00b7ten", "ihm", "bis", "zu", "dem", "trie\u00b7fen\u00b7den", "Ran\u00b7de", "ge\u00b7rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Becher mir Gift.", "tokens": ["Be\u00b7cher", "mir", "Gift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Von dem Geyer noch Einmal gefasst, entst\u00fcrzt' er der W\u00f6lbung;", "tokens": ["Von", "dem", "Ge\u00b7yer", "noch", "Ein\u00b7mal", "ge\u00b7fasst", ",", "ent\u00b7st\u00fcrzt'", "er", "der", "W\u00f6l\u00b7bung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Aber itzt kam er nicht um,", "tokens": ["A\u00b7ber", "itzt", "kam", "er", "nicht", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.39": {"text": "Klomm zu einer empor der Inseln, die rings aus der Ferne", "tokens": ["Klomm", "zu", "ei\u00b7ner", "em\u00b7por", "der", "In\u00b7seln", ",", "die", "rings", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.40": {"text": "Wurde vom wimmelnden Volke gesehn.", "tokens": ["Wur\u00b7de", "vom", "wim\u00b7meln\u00b7den", "Vol\u00b7ke", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.41": {"text": "Hier will er g\u00fcllotienen ein ganzes Heer, so mit Hohn ihm", "tokens": ["Hier", "will", "er", "g\u00fcl\u00b7lo\u00b7ti\u00b7e\u00b7nen", "ein", "gan\u00b7zes", "Heer", ",", "so", "mit", "Hohn", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "PPER"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Zuruft, dass er sich Quaal", "tokens": ["Zu\u00b7ruft", ",", "dass", "er", "sich", "Qua\u00b7al"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Tr\u00e4ume! Phantom sey alles, der Geyer, der Sturz in die Fluten,", "tokens": ["Tr\u00e4u\u00b7me", "!", "Phan\u00b7tom", "sey", "al\u00b7les", ",", "der", "Ge\u00b7yer", ",", "der", "Sturz", "in", "die", "Flu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "VAFIN", "PIS", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.44": {"text": "Und der Tanzenden Lied.", "tokens": ["Und", "der", "Tan\u00b7zen\u00b7den", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.45": {"text": "Aber kein Kunstgeb\u00e4u mit schnellabm\u00e4hender Sichel,", "tokens": ["A\u00b7ber", "kein", "Kunst\u00b7ge\u00b7b\u00e4u", "mit", "schnell\u00b7ab\u00b7m\u00e4\u00b7hen\u00b7der", "Si\u00b7chel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--++-+-+", "measure": "iambic.septa.invert"}, "line.46": {"text": "Keine G\u00fcllotine war da.", "tokens": ["Kei\u00b7ne", "G\u00fcl\u00b7lo\u00b7ti\u00b7ne", "war", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.47": {"text": "Grimmvoll entschloss er sich eine zu baun. Mit M\u00fchsal haut' er", "tokens": ["Grimm\u00b7voll", "ent\u00b7schloss", "er", "sich", "ei\u00b7ne", "zu", "baun", ".", "Mit", "M\u00fch\u00b7sal", "haut'", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ART", "PTKZU", "VVINF", "$.", "APPR", "NN", "VVFIN", "PPER"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.48": {"text": "B\u00e4um' um, zimmerte draus in dem Schweiss", "tokens": ["B\u00e4um'", "um", ",", "zim\u00b7mer\u00b7te", "draus", "in", "dem", "Schweiss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PAV", "APPR", "ART", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.49": {"text": "Seines Angesichts. Doch eine gerostete Sichel", "tokens": ["Sei\u00b7nes", "An\u00b7ge\u00b7sichts", ".", "Doch", "ei\u00b7ne", "ge\u00b7ros\u00b7te\u00b7te", "Si\u00b7chel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.50": {"text": "Lag vor ihm; er fasste sie, schliff", "tokens": ["Lag", "vor", "ihm", ";", "er", "fass\u00b7te", "sie", ",", "schliff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.51": {"text": "Sie mit \u00c4chzen. Nun ruft' er den 