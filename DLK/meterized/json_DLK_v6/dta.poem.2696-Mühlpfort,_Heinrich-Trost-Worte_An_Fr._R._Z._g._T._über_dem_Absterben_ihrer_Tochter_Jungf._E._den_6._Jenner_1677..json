{"dta.poem.2696": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Worte/  \n  An Fr. R. Z. g. T. \u00fcber dem Absterben ihrer  \n Tochter Jungf. E. den 6. Jenner 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df heist/ betr\u00fcbtste Frau/ in Angst und Noth be-\nschlossen/", "tokens": ["Di\u00df", "heist", "/", "be\u00b7tr\u00b7\u00fcbts\u00b7te", "Frau", "/", "in", "Angst", "und", "Noth", "be", "schlos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN", "TRUNC", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und in vermehrtem Leid ein Neu Jahr fangen an;", "tokens": ["Und", "in", "ver\u00b7mehr\u00b7tem", "Leid", "ein", "Neu", "Jahr", "fan\u00b7gen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df heist das Uberma\u00df der Thr\u00e4nen au\u00dfgegossen/", "tokens": ["Di\u00df", "heist", "das", "U\u00b7ber\u00b7ma\u00df", "der", "Thr\u00e4\u00b7nen", "au\u00df\u00b7ge\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df ihr Aug und Hertz fast nicht mehr bluten kan.", "tokens": ["So", "da\u00df", "ihr", "Aug", "und", "Hertz", "fast", "nicht", "mehr", "blu\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "KON", "NN", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat die Sonne noch den Thier Krei\u00df nicht durchgangen", "tokens": ["Es", "hat", "die", "Son\u00b7ne", "noch", "den", "Thier", "Krei\u00df", "nicht", "durch\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als ihres Hauptes Kron/ ihr ander Hertze fiel/", "tokens": ["Als", "ih\u00b7res", "Haup\u00b7tes", "Kron", "/", "ihr", "an\u00b7der", "Hert\u00b7ze", "fiel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "$(", "PPER", "ADJD", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Thr\u00e4nen solten noch recht trucknen auff den Wangen/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "sol\u00b7ten", "noch", "recht", "truck\u00b7nen", "auff", "den", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und bleiche K\u00fcmmern\u00fc\u00df biieb ihres Leidens Ziel.", "tokens": ["Und", "blei\u00b7che", "K\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "bi\u00b7i\u00b7eb", "ih\u00b7res", "Lei\u00b7dens", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Sie lebt\u2019 in Harm und Qual/ und sa\u00df in Staub und Aschen", "tokens": ["Sie", "lebt'", "in", "Harm", "und", "Qual", "/", "und", "sa\u00df", "in", "Staub", "und", "A\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$(", "KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Hoffnung/ da\u00df die Hand/ die alles hat gebaut", "tokens": ["In", "Hoff\u00b7nung", "/", "da\u00df", "die", "Hand", "/", "die", "al\u00b7les", "hat", "ge\u00b7baut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "KOUS", "ART", "NN", "$(", "ART", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch ihre Thr\u00e4nen wird au\u00df ihrem Aug abwaschen", "tokens": ["Auch", "ih\u00b7re", "Thr\u00e4\u00b7nen", "wird", "au\u00df", "ih\u00b7rem", "Aug", "ab\u00b7wa\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und da\u00df man nach der Nacht die g\u00fcldne Sonne schaut.", "tokens": ["Und", "da\u00df", "man", "nach", "der", "Nacht", "die", "g\u00fcld\u00b7ne", "Son\u00b7ne", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach! aber nur umbsonst! Die unerforschten Wege", "tokens": ["Ach", "!", "a\u00b7ber", "nur", "um\u00b7bsonst", "!", "Die", "un\u00b7er\u00b7forschten", "We\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ADV", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "So Gottes Allmacht zeigt sind Menschen nicht bekand.", "tokens": ["So", "Got\u00b7tes", "All\u00b7macht", "zeigt", "sind", "Men\u00b7schen", "nicht", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VVFIN", "VAFIN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es rufft ein h\u00f6her Schlu\u00df sie von dem Dornen-Stege", "tokens": ["Es", "rufft", "ein", "h\u00f6\u00b7her", "Schlu\u00df", "sie", "von", "dem", "Dor\u00b7nen\u00b7Ste\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ach \u00fcberh\u00e4ufftes Leid! jetzt in ein Hecken-Land.", "tokens": ["Ach", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7tes", "Leid", "!", "jetzt", "in", "ein", "He\u00b7cken\u00b7Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Erst Witwe m\u00fcssen seyn/ und bey de\u00df Lebens Ende", "tokens": ["Erst", "Wit\u00b7we", "m\u00fcs\u00b7sen", "seyn", "/", "und", "bey", "de\u00df", "Le\u00b7bens", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "VAINF", "$(", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da man fast gleiche Jahr\u2019 im Ehstand zu gebracht/", "tokens": ["Da", "man", "fast", "glei\u00b7che", "Jahr'", "im", "Eh\u00b7stand", "zu", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sehn/ wie die Sonne zeucht von ihrer Sonnen-Wende/", "tokens": ["Sehn", "/", "wie", "die", "Son\u00b7ne", "zeucht", "von", "ih\u00b7rer", "Son\u00b7nen\u00b7Wen\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Empfinden/ wie der schl\u00e4fft/ der vor das Haus gewacht/", "tokens": ["Emp\u00b7fin\u00b7den", "/", "wie", "der", "schl\u00e4fft", "/", "der", "vor", "das", "Haus", "ge\u00b7wacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "ART", "VVFIN", "$(", "ART", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn auch de\u00df Alters Trost die liebste Tochter missen", "tokens": ["Denn", "auch", "de\u00df", "Al\u00b7ters", "Trost", "die", "liebs\u00b7te", "Toch\u00b7ter", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Tochter/ so allhier der Jugend Blume war/", "tokens": ["Die", "Toch\u00b7ter", "/", "so", "all\u00b7hier", "der", "Ju\u00b7gend", "Blu\u00b7me", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "ART", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und nur der ", "tokens": ["Und", "nur", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Und derer reines Hertz de\u00df H\u00f6chsten Lob-Altar.", "tokens": ["Und", "de\u00b7rer", "rei\u00b7nes", "Hertz", "de\u00df", "H\u00f6chs\u00b7ten", "Lob\u00b7Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Di\u00df ist ein doppelt Schlag/ der Geist und Seele beuget/", "tokens": ["Di\u00df", "ist", "ein", "dop\u00b7pelt", "Schlag", "/", "der", "Geist", "und", "See\u00b7le", "beu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Sinnen und Vernunfft mit Macht zu Boden dr\u00fcckt.", "tokens": ["Der", "Sin\u00b7nen", "und", "Ver\u00b7nunfft", "mit", "Macht", "zu", "Bo\u00b7den", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wem so das Neue Jahr im Anfang sich erzeiget", "tokens": ["Wem", "so", "das", "Neu\u00b7e", "Jahr", "im", "An\u00b7fang", "sich", "er\u00b7zei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Gewi\u00df/ da\u00df er den Zweck der h\u00f6chsten Noth erblickt.", "tokens": ["Ge\u00b7wi\u00df", "/", "da\u00df", "er", "den", "Zweck", "der", "h\u00f6chs\u00b7ten", "Noth", "er\u00b7blickt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Allein/ ", "tokens": ["Al\u00b7lein", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Da\u00df Hoffnung zum Pauier/ Gedult zum Pantzer hat/", "tokens": ["Da\u00df", "Hoff\u00b7nung", "zum", "Pau\u00b7i\u00b7er", "/", "Ge\u00b7dult", "zum", "Pant\u00b7zer", "hat", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$(", "NN", "APPRART", "NN", "VAFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Traut/ wie es stets gethan/ de\u00df Sch\u00f6pffers H\u00fclff und G\u00fcte", "tokens": ["Traut", "/", "wie", "es", "stets", "ge\u00b7than", "/", "de\u00df", "Sch\u00f6pf\u00b7fers", "H\u00fclff", "und", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "PPER", "ADV", "VVPP", "$(", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wei\u00df/ da\u00df er ist und bleibt betr\u00fcbter Wittwen Rath.", "tokens": ["Wei\u00df", "/", "da\u00df", "er", "ist", "und", "bleibt", "be\u00b7tr\u00fcb\u00b7ter", "Witt\u00b7wen", "Rath", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "VAFIN", "KON", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie ist schon l\u00e4ngst bewehrt durch Creutz und bittre Zeiten/", "tokens": ["Sie", "ist", "schon", "l\u00e4ngst", "be\u00b7wehrt", "durch", "Creutz", "und", "bitt\u00b7re", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Kennt au\u00df Erfahrenheit/ die Plagen dieser Welt/", "tokens": ["Kennt", "au\u00df", "Er\u00b7fah\u00b7ren\u00b7heit", "/", "die", "Pla\u00b7gen", "die\u00b7ser", "Welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$(", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wenn Hunger/ Krieg und Pest ihr stunden an der Seiten/", "tokens": ["Wenn", "Hun\u00b7ger", "/", "Krieg", "und", "Pest", "ihr", "stun\u00b7den", "an", "der", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ward doch ihr Glaubens-Fels von keinem Sturm zerschellt.", "tokens": ["Ward", "doch", "ihr", "Glau\u00b7bens\u00b7Fels", "von", "kei\u00b7nem", "Sturm", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun/ der sie wunderlich von Jugend auff gef\u00fchret", "tokens": ["Nun", "/", "der", "sie", "wun\u00b7der\u00b7lich", "von", "Ju\u00b7gend", "auff", "ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "PRELS", "PPER", "ADJD", "APPR", "NN", "APPR", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Will auch im Alter noch ihr Stab und Leuchte seyn.", "tokens": ["Will", "auch", "im", "Al\u00b7ter", "noch", "ihr", "Stab", "und", "Leuch\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "ADV", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Versichert/ da\u00df nichts mehr warhaffte Christen ziehret", "tokens": ["Ver\u00b7si\u00b7chert", "/", "da\u00df", "nichts", "mehr", "war\u00b7haff\u00b7te", "Chris\u00b7ten", "zieh\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "KOUS", "PIS", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Als ein durchbeitzter Geist in scharffer Angst und Pein.", "tokens": ["Als", "ein", "durch\u00b7beitz\u00b7ter", "Geist", "in", "scharf\u00b7fer", "Angst", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ein Mensch ist ohne Creutz ein todtes Meer zu nennen/", "tokens": ["Ein", "Mensch", "ist", "oh\u00b7ne", "Creutz", "ein", "tod\u00b7tes", "Meer", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "