{"textgrid.poem.63189": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Pha\u00ebthon", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pha\u00ebthon,", "tokens": ["Pha\u00ebthon", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Der Mundschenk der G\u00f6tter,", "tokens": ["Der", "Mund\u00b7schenk", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mischte den G\u00f6ttern", "tokens": ["Mischte", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Schlaf in den Wein.", "tokens": ["Schlaf", "in", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie tranken,", "tokens": ["Sie", "tran\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Sie sanken", "tokens": ["Sie", "san\u00b7ken"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "In Traum und in Schlaf.", "tokens": ["In", "Traum", "und", "in", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "An seinen Sonnenwagen gelehnt", "tokens": ["An", "sei\u00b7nen", "Son\u00b7nen\u00b7wa\u00b7gen", "ge\u00b7lehnt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schlief Helios.", "tokens": ["Schlief", "He\u00b7li\u00b7os", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Z\u00fcgel schleiften", "tokens": ["Die", "Z\u00fc\u00b7gel", "schleif\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Auf Wolken.", "tokens": ["Auf", "Wol\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Da trat der Knabe Pha\u00ebthon herzu,", "tokens": ["Da", "trat", "der", "Kna\u00b7be", "Pha\u00ebthon", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprang auf das Brett,", "tokens": ["Sprang", "auf", "das", "Brett", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ergriff die Geissel", "tokens": ["Er\u00b7griff", "die", "Geis\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und liess sie \u00fcber die Rosse sausen,", "tokens": ["Und", "liess", "sie", "\u00fc\u00b7ber", "die", "Ros\u00b7se", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die goldenen.", "tokens": ["Die", "gol\u00b7de\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Sie wieherten jauchzend", "tokens": ["Sie", "wie\u00b7her\u00b7ten", "jauch\u00b7zend"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Unter der jungen Hand", "tokens": ["Un\u00b7ter", "der", "jun\u00b7gen", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und jagten durch den \u00c4ther,", "tokens": ["Und", "jag\u00b7ten", "durch", "den", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verliessen die alteingefahrne Bahn.", "tokens": ["Ver\u00b7lies\u00b7sen", "die", "al\u00b7tein\u00b7ge\u00b7fahr\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die goldenen Locken des Knaben,", "tokens": ["Die", "gol\u00b7de\u00b7nen", "Lo\u00b7cken", "des", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die goldenen M\u00e4hnen der Rosse", "tokens": ["Die", "gol\u00b7de\u00b7nen", "M\u00e4h\u00b7nen", "der", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Stoben im Sternensturm.", "tokens": ["Sto\u00b7ben", "im", "Ster\u00b7nen\u00b7sturm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Als er am Abend lenkte", "tokens": ["Als", "er", "am", "A\u00b7bend", "lenk\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das goldne Gef\u00e4hrt", "tokens": ["Das", "gold\u00b7ne", "Ge\u00b7f\u00e4hrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In den himmlischen Stall,", "tokens": ["In", "den", "himm\u00b7li\u00b7schen", "Stall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da waren die G\u00f6tter erwacht.", "tokens": ["Da", "wa\u00b7ren", "die", "G\u00f6t\u00b7ter", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Helios jammerte,", "tokens": ["He\u00b7li\u00b7os", "jam\u00b7mer\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Zeus grollte.", "tokens": ["Zeus", "groll\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Schneeweiss war des G\u00f6ttervaters Haar geworden,", "tokens": ["Schnee\u00b7weiss", "war", "des", "G\u00f6t\u00b7ter\u00b7va\u00b7ters", "Haar", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "VAPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Schnee lag auf dem G\u00f6tterberg.", "tokens": ["Schnee", "lag", "auf", "dem", "G\u00f6t\u00b7ter\u00b7berg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn allzuweit hatte der Knabe sich von ihm entfernt", "tokens": ["Denn", "all\u00b7zu\u00b7weit", "hat\u00b7te", "der", "Kna\u00b7be", "sich", "von", "ihm", "ent\u00b7fernt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PRF", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Mit dem Sonnenwagen.", "tokens": ["Mit", "dem", "Son\u00b7nen\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Zu nah war er der Erde gekommen,", "tokens": ["Zu", "nah", "war", "er", "der", "Er\u00b7de", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Denn tausend Steppen standen in Flammen", "tokens": ["Denn", "tau\u00b7send", "Step\u00b7pen", "stan\u00b7den", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und W\u00e4lder bluteten rot.", "tokens": ["Und", "W\u00e4l\u00b7der", "blu\u00b7te\u00b7ten", "rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Das grosse Feuer kam", "tokens": ["Das", "gros\u00b7se", "Feu\u00b7er", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie einst das grosse Wasser war gekommen.", "tokens": ["Wie", "einst", "das", "gros\u00b7se", "Was\u00b7ser", "war", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lava rollte schwarz.", "tokens": ["Die", "La\u00b7va", "roll\u00b7te", "schwarz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die heilige Zeder", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Ze\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Brannte.", "tokens": ["Brann\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Aus den verkohlten Wurzeln stiegen", "tokens": ["Aus", "den", "ver\u00b7kohl\u00b7ten", "Wur\u00b7zeln", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gew\u00fcrm und Engerling ans Licht.", "tokens": ["Ge\u00b7w\u00fcrm", "und", "En\u00b7ger\u00b7ling", "ans", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und Kypris, die die Nacht wie stets", "tokens": ["Und", "Kyp\u00b7ris", ",", "die", "die", "Nacht", "wie", "stets"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "PRELS", "ART", "NN", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Erden zugebracht,", "tokens": ["Auf", "Er\u00b7den", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Riss ihren Knaben", "tokens": ["Riss", "ih\u00b7ren", "Kna\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Eros", "tokens": ["E\u00b7ros"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hinter sich auf das gefl\u00fcgelte Pferd.", "tokens": ["Hin\u00b7ter", "sich", "auf", "das", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Das galoppierte \u00fcber den wandernden Insekten", "tokens": ["Das", "ga\u00b7lop\u00b7pier\u00b7te", "\u00fc\u00b7ber", "den", "wan\u00b7dern\u00b7den", "In\u00b7sek\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Auf den Leibern der D\u00e4monen", "tokens": ["Auf", "den", "Lei\u00b7bern", "der", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und hob sich wie ein Adler dann", "tokens": ["Und", "hob", "sich", "wie", "ein", "Ad\u00b7ler", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KOKOM", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und galoppierte auf den Wolken \u2013", "tokens": ["Und", "ga\u00b7lop\u00b7pier\u00b7te", "auf", "den", "Wol\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und kam zum G\u00f6tterberg.", "tokens": ["Und", "kam", "zum", "G\u00f6t\u00b7ter\u00b7berg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Eiszapfen hingen von dem Ritt durch die \u00c4onen", "tokens": ["Eis\u00b7zap\u00b7fen", "hin\u00b7gen", "von", "dem", "Ritt", "durch", "die", "\u00c4\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem Pferde in den M\u00e4hnen.", "tokens": ["Dem", "Pfer\u00b7de", "in", "den", "M\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kypris mondblondes Haar war weiss beschneit,", "tokens": ["Kyp\u00b7ris", "mond\u00b7blon\u00b7des", "Haar", "war", "weiss", "be\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und Eros", "tokens": ["Und", "E\u00b7ros"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Schlug die erstarrten Finger aneinander", "tokens": ["Schlug", "die", "er\u00b7starr\u00b7ten", "Fin\u00b7ger", "an\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Glockenkl\u00f6ppel.", "tokens": ["Wie", "Glo\u00b7cken\u00b7kl\u00f6p\u00b7pel", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Ich friere, sagte Helios.", "tokens": ["Ich", "frie\u00b7re", ",", "sag\u00b7te", "He\u00b7li\u00b7os", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was tatest du,", "tokens": ["Was", "ta\u00b7test", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vorwitziger Knabe,", "tokens": ["Vor\u00b7wit\u00b7zi\u00b7ger", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Pha\u00ebthon?", "tokens": ["Pha\u00ebthon", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die G\u00f6tter frieren,", "tokens": ["Die", "G\u00f6t\u00b7ter", "frie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und der Menschen viele sind verbrannt", "tokens": ["Und", "der", "Men\u00b7schen", "vie\u00b7le", "sind", "ver\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIS", "VAFIN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie K\u00e4lber am Spiess.", "tokens": ["Wie", "K\u00e4l\u00b7ber", "am", "Spiess", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN"