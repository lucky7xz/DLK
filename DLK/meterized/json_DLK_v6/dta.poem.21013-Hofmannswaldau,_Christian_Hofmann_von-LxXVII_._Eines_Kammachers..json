{"dta.poem.21013": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXVII .  \n Eines Kammachers.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht spotte/ da\u00df mein Hau\u00df stets voller H\u00f6rner", "tokens": ["Nicht", "spot\u00b7te", "/", "da\u00df", "mein", "Hau\u00df", "stets", "vol\u00b7ler", "H\u00f6r\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "stund/", "tokens": ["stund", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und das verachte Wort ernehrte meinen Mund.", "tokens": ["Und", "das", "ver\u00b7ach\u00b7te", "Wort", "er\u00b7nehr\u00b7te", "mei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sage dir ein Wort/ und bist du noch so edel/", "tokens": ["Ich", "sa\u00b7ge", "dir", "ein", "Wort", "/", "und", "bist", "du", "noch", "so", "e\u00b7del", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich trag es in der Hand/ du aber auf dem Schedel.", "tokens": ["Ich", "trag", "es", "in", "der", "Hand", "/", "du", "a\u00b7ber", "auf", "dem", "Sche\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}