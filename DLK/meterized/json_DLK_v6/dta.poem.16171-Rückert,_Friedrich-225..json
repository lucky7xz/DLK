{"dta.poem.16171": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "225.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Unentschiedenheit und Zweifelmuth beklommner!", "tokens": ["In", "Un\u00b7ent\u00b7schie\u00b7den\u00b7heit", "und", "Zwei\u00b7fel\u00b7muth", "be\u00b7klomm\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einst wirst du gl\u00fccklicher, einst wirst du seyn vollkommner.", "tokens": ["Einst", "wirst", "du", "gl\u00fcck\u00b7li\u00b7cher", ",", "einst", "wirst", "du", "seyn", "voll\u00b7komm\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Einst wirst du wissender, einst besser als du bist;", "tokens": ["Einst", "wirst", "du", "wis\u00b7sen\u00b7der", ",", "einst", "bes\u00b7ser", "als", "du", "bist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil jeder das nur wird, was er schon strebend ist.", "tokens": ["Weil", "je\u00b7der", "das", "nur", "wird", ",", "was", "er", "schon", "stre\u00b7bend", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PDS", "ADV", "VAFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein fremdes Streben reicht weit \u00fcber dich hinaus", "tokens": ["Dein", "frem\u00b7des", "Stre\u00b7ben", "reicht", "weit", "\u00fc\u00b7ber", "dich", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du dich selbst erreichst, da bist du erst zu Haus.", "tokens": ["Wo", "du", "dich", "selbst", "er\u00b7reichst", ",", "da", "bist", "du", "erst", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}