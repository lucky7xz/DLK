{"textgrid.poem.42682": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein alter B\u00e4r, den die Musik", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein alter B\u00e4r, den die Musik", "tokens": ["Ein", "al\u00b7ter", "B\u00e4r", ",", "den", "die", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Jagdhorns einst aus seinem Walde jagte,", "tokens": ["Des", "Jagd\u00b7horns", "einst", "aus", "sei\u00b7nem", "Wal\u00b7de", "jag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhohlte nach und nach sich von der Angst, und wagte", "tokens": ["Er\u00b7hohl\u00b7te", "nach", "und", "nach", "sich", "von", "der", "Angst", ",", "und", "wag\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "KON", "APPR", "PRF", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00fcbsch sachte sich nach seinem Hain zur\u00fcck.", "tokens": ["H\u00fcbsch", "sach\u00b7te", "sich", "nach", "sei\u00b7nem", "Hain", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bey seiner Ankunft war die erste seiner Sorgen,", "tokens": ["Bey", "sei\u00b7ner", "An\u00b7kunft", "war", "die", "ers\u00b7te", "sei\u00b7ner", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sich nach dem Eichbaum umzusehn,", "tokens": ["Sich", "nach", "dem", "Eich\u00b7baum", "um\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In dessen hohlem Bauch er sich beym kalten When", "tokens": ["In", "des\u00b7sen", "hoh\u00b7lem", "Bauch", "er", "sich", "beym", "kal\u00b7ten", "When"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Wintersturmes oft verborgen.", "tokens": ["Des", "Win\u00b7ter\u00b7stur\u00b7mes", "oft", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als er der Eiche nahe kam,", "tokens": ["Als", "er", "der", "Ei\u00b7che", "na\u00b7he", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Entdeckt' er mit Verdruss und Gram", "tokens": ["Ent\u00b7deckt'", "er", "mit", "Ver\u00b7druss", "und", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf einem Zweig ein Nest voll junger Kr\u00e4hen.", "tokens": ["Auf", "ei\u00b7nem", "Zweig", "ein", "Nest", "voll", "jun\u00b7ger", "Kr\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du Metze! fieng er flugs die Mutter an zu schm\u00e4hen,", "tokens": ["Du", "Met\u00b7ze", "!", "fi\u00b7eng", "er", "flugs", "die", "Mut\u00b7ter", "an", "zu", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Was hast du hier auf meinem Baum zu thun?", "tokens": ["Was", "hast", "du", "hier", "auf", "mei\u00b7nem", "Baum", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Fort! packe dich von dannen ohne Zaudern!", "tokens": ["Fort", "!", "pa\u00b7cke", "dich", "von", "dan\u00b7nen", "oh\u00b7ne", "Zau\u00b7dern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PRF", "APPR", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Denn deiner Fratzen st\u00e4tes Plaudern", "tokens": ["Denn", "dei\u00b7ner", "Frat\u00b7zen", "st\u00e4\u00b7tes", "Plau\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und Zwitschern liesse mich den ganzen Tag nicht ruhn,", "tokens": ["Und", "Zwit\u00b7schern", "lies\u00b7se", "mich", "den", "gan\u00b7zen", "Tag", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und falls mich auch ihr L\u00e4rm nicht molestirte,", "tokens": ["Und", "falls", "mich", "auch", "ihr", "L\u00e4rm", "nicht", "mo\u00b7les\u00b7tir\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "So m\u00fcsst' ich st\u00e4ts in Sorgen seyn,", "tokens": ["So", "m\u00fcsst'", "ich", "st\u00e4ts", "in", "Sor\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ob deine junge Brut nicht etwan obendrein", "tokens": ["Ob", "dei\u00b7ne", "jun\u00b7ge", "Brut", "nicht", "et\u00b7wan", "o\u00b7ben\u00b7drein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mir auf den Kopf herab hofierte.", "tokens": ["Mir", "auf", "den", "Kopf", "her\u00b7ab", "ho\u00b7fier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der B\u00e4r schloss seine Rede kaum,", "tokens": ["Der", "B\u00e4r", "schloss", "sei\u00b7ne", "Re\u00b7de", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fieng die alte Kr\u00e4h' ihr Recht auf diesen Baum", "tokens": ["So", "fi\u00b7eng", "die", "al\u00b7te", "Kr\u00e4h'", "ihr", "Recht", "auf", "die\u00b7sen", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Durch manchen Grund vor Meister Petzen", "tokens": ["Durch", "man\u00b7chen", "Grund", "vor", "Meis\u00b7ter", "Pet\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weitl\u00e4ufig an in's Licht zu setzen.", "tokens": ["Weit\u00b7l\u00e4u\u00b7fig", "an", "in's", "Licht", "zu", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch der erbosste B\u00e4r vertrug", "tokens": ["Doch", "der", "er\u00b7boss\u00b7te", "B\u00e4r", "ver\u00b7trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht gerne Widerspruch. Er kletterte die Eiche", "tokens": ["Nicht", "ger\u00b7ne", "Wi\u00b7der\u00b7spruch", ".", "Er", "klet\u00b7ter\u00b7te", "die", "Ei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hinan mit Brummen, und erschlug", "tokens": ["Hi\u00b7nan", "mit", "Brum\u00b7men", ",", "und", "er\u00b7schlug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die junge Brut mit einem Streiche.", "tokens": ["Die", "jun\u00b7ge", "Brut", "mit", "ei\u00b7nem", "Strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gespornt von Wuth und Rachbegier,", "tokens": ["Ge\u00b7spornt", "von", "Wuth", "und", "Rach\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flog Mutter Kr\u00e4he nun zum J\u00e4ger, und entdeckte", "tokens": ["Flog", "Mut\u00b7ter", "Kr\u00e4\u00b7he", "nun", "zum", "J\u00e4\u00b7ger", ",", "und", "ent\u00b7deck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "NN", "ADV", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihm das verwilderte Revier,", "tokens": ["Ihm", "das", "ver\u00b7wil\u00b7der\u00b7te", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sich der alte B\u00e4r versteckte.", "tokens": ["Wo", "sich", "der", "al\u00b7te", "B\u00e4r", "ver\u00b7steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der J\u00e4ger wandert' alsobald", "tokens": ["Der", "J\u00e4\u00b7ger", "wan\u00b7dert'", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinen Doggen in den Wald,", "tokens": ["Mit", "sei\u00b7nen", "Dog\u00b7gen", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und fand den armen Petz in seines Baumes L\u00fccke.", "tokens": ["Und", "fand", "den", "ar\u00b7men", "Petz", "in", "sei\u00b7nes", "Bau\u00b7mes", "L\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vergebens sucht der B\u00e4r dem Tode zu entfliehn:", "tokens": ["Ver\u00b7ge\u00b7bens", "sucht", "der", "B\u00e4r", "dem", "To\u00b7de", "zu", "ent\u00b7fliehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die tapfern Hunde fassen ihn", "tokens": ["Die", "tap\u00b7fern", "Hun\u00b7de", "fas\u00b7sen", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erbarmungslos bey der Per\u00fccke.", "tokens": ["Er\u00b7bar\u00b7mungs\u00b7los", "bey", "der", "Pe\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Vertrage dich mit jedermann,", "tokens": ["Ver\u00b7tra\u00b7ge", "dich", "mit", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um niemands Hass auf dich zu laden;", "tokens": ["Um", "nie\u00b7mands", "Hass", "auf", "dich", "zu", "la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wer dir auch nicht n\u00fctzen kann,", "tokens": ["Denn", "wer", "dir", "auch", "nicht", "n\u00fct\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann doch in manchem Fall dir schaden.", "tokens": ["Kann", "doch", "in", "man\u00b7chem", "Fall", "dir", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}