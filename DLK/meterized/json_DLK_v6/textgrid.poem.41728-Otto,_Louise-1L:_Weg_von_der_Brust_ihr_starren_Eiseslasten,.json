{"textgrid.poem.41728": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weg von der Brust ihr starren Eiseslasten,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg von der Brust ihr starren Eiseslasten,", "tokens": ["Weg", "von", "der", "Brust", "ihr", "star\u00b7ren", "Ei\u00b7ses\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ich kann auch f\u00fcrder nicht darauf ertragen!", "tokens": ["Ich", "kann", "auch", "f\u00fcr\u00b7der", "nicht", "da\u00b7rauf", "er\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf einem Strom seh gern ich stolze Masten", "tokens": ["Auf", "ei\u00b7nem", "Strom", "seh", "gern", "ich", "stol\u00b7ze", "Mas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00f6re gern die leichten Ruder schlagen.", "tokens": ["Und", "h\u00f6\u00b7re", "gern", "die", "leich\u00b7ten", "Ru\u00b7der", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und f\u00fchl ich einen Strom nicht in mir wallen", "tokens": ["Und", "f\u00fchl", "ich", "ei\u00b7nen", "Strom", "nicht", "in", "mir", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Herzens F\u00fchlen in bewegten Wogen?", "tokens": ["Des", "Her\u00b7zens", "F\u00fch\u00b7len", "in", "be\u00b7weg\u00b7ten", "Wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drauf h\u00f6rt ich jetzt kein Fischerliedlein schallen,", "tokens": ["Drauf", "h\u00f6rt", "ich", "jetzt", "kein", "Fi\u00b7scher\u00b7lied\u00b7lein", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Schiff der Hoffnung kam einhergezogen.", "tokens": ["Kein", "Schiff", "der", "Hoff\u00b7nung", "kam", "ein\u00b7her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, alle Schiffe, die sonst schwer befrachtet", "tokens": ["Ja", ",", "al\u00b7le", "Schif\u00b7fe", ",", "die", "sonst", "schwer", "be\u00b7frach\u00b7tet"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit gold'nen Liedern da und dorthin fahren,", "tokens": ["Mit", "gold'\u00b7nen", "Lie\u00b7dern", "da", "und", "dor\u00b7thin", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie lagen still, zerfroren und verachtet,", "tokens": ["Sie", "la\u00b7gen", "still", ",", "zer\u00b7fro\u00b7ren", "und", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Traurig wie Todesbilder zu gewahren.", "tokens": ["Trau\u00b7rig", "wie", "To\u00b7des\u00b7bil\u00b7der", "zu", "ge\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Der Weg der T\u00f6ne zu des Ruhms Alt\u00e4ren", "tokens": ["Der", "Weg", "der", "T\u00f6\u00b7ne", "zu", "des", "Ruhms", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War jetzt mir nur zur glatten Eisbahn worden,", "tokens": ["War", "jetzt", "mir", "nur", "zur", "glat\u00b7ten", "Eis\u00b7bahn", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "APPRART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gefroren fest von den erstarrten Z\u00e4hren", "tokens": ["Ge\u00b7fro\u00b7ren", "fest", "von", "den", "er\u00b7starr\u00b7ten", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tausende die sich mit Liedern morden \u2013", "tokens": ["Der", "Tau\u00b7sen\u00b7de", "die", "sich", "mit", "Lie\u00b7dern", "mor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Denn Lieder Waffen sind, die uns verwunden,", "tokens": ["Denn", "Lie\u00b7der", "Waf\u00b7fen", "sind", ",", "die", "uns", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht fromme Kreuze um uns selbst zu segnen!", "tokens": ["Nicht", "from\u00b7me", "Kreu\u00b7ze", "um", "uns", "selbst", "zu", "seg\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn Poesie schafft allen Schmerzensstunden", "tokens": ["Denn", "Poe\u00b7sie", "schafft", "al\u00b7len", "Schmer\u00b7zenss\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gef\u00e4hrlich ist's der Muse zu begegnen.", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "ist's", "der", "Mu\u00b7se", "zu", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nicht einmal diese Schmerzen sollt ich haben!", "tokens": ["Nicht", "ein\u00b7mal", "die\u00b7se", "Schmer\u00b7zen", "sollt", "ich", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PDAT", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In mir war's starr, kalt wie ein Wintermorgen.", "tokens": ["In", "mir", "wa\u00b7r's", "starr", ",", "kalt", "wie", "ein", "Win\u00b7ter\u00b7mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Nur Eis und Schnee mein trotzig Herz umgaben,", "tokens": ["Nur", "Eis", "und", "Schnee", "mein", "trot\u00b7zig", "Herz", "um\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich konnte nimmer seinen Schl\u00e4gen horchen.", "tokens": ["Ich", "konn\u00b7te", "nim\u00b7mer", "sei\u00b7nen", "Schl\u00e4\u00b7gen", "hor\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Was drau\u00dfen noch das Leben frisch bewegte,", "tokens": ["Was", "drau\u00b7\u00dfen", "noch", "das", "Le\u00b7ben", "frisch", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das h\u00f6rt ich an wie eine fremde Kunde,", "tokens": ["Das", "h\u00f6rt", "ich", "an", "wie", "ei\u00b7ne", "frem\u00b7de", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu keiner Thr\u00e4ne sich das Auge regte.", "tokens": ["Zu", "kei\u00b7ner", "Thr\u00e4\u00b7ne", "sich", "das", "Au\u00b7ge", "reg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Seufzer kam, kein Wort aus meinem Munde", "tokens": ["Kein", "Seuf\u00b7zer", "kam", ",", "kein", "Wort", "aus", "mei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "In mir war's still und dumpf, ja todesstille.", "tokens": ["In", "mir", "wa\u00b7r's", "still", "und", "dumpf", ",", "ja", "to\u00b7des\u00b7stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Nicht Schmerz, nicht Freude f\u00fchlt ich in mir wogen,", "tokens": ["Nicht", "Schmerz", ",", "nicht", "Freu\u00b7de", "f\u00fchlt", "ich", "in", "mir", "wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gebrochen war die Thatkraft, war der Wille,", "tokens": ["Ge\u00b7bro\u00b7chen", "war", "die", "That\u00b7kraft", ",", "war", "der", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Kr\u00e4hen kr\u00e4chzend um das Haupt mir flogen.", "tokens": ["Wo", "Kr\u00e4\u00b7hen", "kr\u00e4ch\u00b7zend", "um", "das", "Haupt", "mir", "flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und da geschah's. Es kam ein mildes Wehen", "tokens": ["Und", "da", "ge\u00b7scha\u00b7h'", "s.", "Es", "kam", "ein", "mil\u00b7des", "We\u00b7hen"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann kam ein Sturm und brach des Stromes Banden", "tokens": ["Dann", "kam", "ein", "Sturm", "und", "brach", "des", "Stro\u00b7mes", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Eisesschollen lie\u00df er fast zergehen.", "tokens": ["Die", "Ei\u00b7ses\u00b7schol\u00b7len", "lie\u00df", "er", "fast", "zer\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und rief im Trotz: ", "tokens": ["Und", "rief", "im", "Trotz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Und von den T\u00fcrmen hallte Festgel\u00e4ute,", "tokens": ["Und", "von", "den", "T\u00fcr\u00b7men", "hall\u00b7te", "Fest\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Glocke rief: ", "tokens": ["Die", "Glo\u00b7cke", "rief", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Schnee ertrug nicht diese Osterfreude,", "tokens": ["Der", "Schnee", "er\u00b7trug", "nicht", "die\u00b7se", "Os\u00b7ter\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er ward zunicht, er war vor ihr zu Schanden.", "tokens": ["Er", "ward", "zu\u00b7nicht", ",", "er", "war", "vor", "ihr", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Da sank ich auf die Kniee br\u00fcnstig nieder", "tokens": ["Da", "sank", "ich", "auf", "die", "Kni\u00b7ee", "br\u00fcns\u00b7tig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meine Seele rief: \u00bb", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "rief", ":", "\u00bb"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ostern, einen Fr\u00fchling hab ich wieder,", "tokens": ["Ein", "Os\u00b7tern", ",", "ei\u00b7nen", "Fr\u00fch\u00b7ling", "hab", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den freien Strom begr\u00fc\u00df ich als Verwandten.", "tokens": ["Den", "frei\u00b7en", "Strom", "be\u00b7gr\u00fc\u00df", "ich", "als", "Ver\u00b7wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ein freier Strom braust wieder meine Lieder", "tokens": ["Ein", "frei\u00b7er", "Strom", "braust", "wie\u00b7der", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und gr\u00fc\u00dft mein Volk in allen deutschen Landen", "tokens": ["Und", "gr\u00fc\u00dft", "mein", "Volk", "in", "al\u00b7len", "deut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und alle Dichter gr\u00fc\u00dft die Hoffnung wieder:", "tokens": ["Und", "al\u00b7le", "Dich\u00b7ter", "gr\u00fc\u00dft", "die", "Hoff\u00b7nung", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einst singen wir: \u00bb", "tokens": ["Einst", "sin\u00b7gen", "wir", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}