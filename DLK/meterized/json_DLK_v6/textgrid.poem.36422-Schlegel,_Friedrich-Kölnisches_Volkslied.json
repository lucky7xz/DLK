{"textgrid.poem.36422": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6lnisches Volkslied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Preu\u00df' der ist ein Lotterbub,", "tokens": ["Der", "Preu\u00df'", "der", "ist", "ein", "Lot\u00b7ter\u00b7bub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt er so \u2013 zu K\u00f6ln am Rheine", "tokens": ["Sagt", "er", "so", "\u2013", "zu", "K\u00f6ln", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "APPR", "NE", "APPRART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen da drei Bauern", "tokens": ["Sa\u00b7\u00dfen", "da", "drei", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In gr\u00fcner Gartenlauben", "tokens": ["In", "gr\u00fc\u00b7ner", "Gar\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf der Bank zu Weine.", "tokens": ["Auf", "der", "Bank", "zu", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Er hat des Geldes nie genug,", "tokens": ["Er", "hat", "des", "Gel\u00b7des", "nie", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich wollt er krieg' die Kr\u00e4nke,", "tokens": ["Ich", "wollt", "er", "krieg'", "die", "Kr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mu\u00df hoch am Galgen h\u00e4ngen.", "tokens": ["Mu\u00df", "hoch", "am", "Gal\u00b7gen", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Bohnenfarz mit seiner List,", "tokens": ["Der", "Boh\u00b7nen\u00b7farz", "mit", "sei\u00b7ner", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt er so \u2013 zu K\u00f6ln am Rheine", "tokens": ["Sagt", "er", "so", "\u2013", "zu", "K\u00f6ln", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "APPR", "NE", "APPRART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen da drei Bauern", "tokens": ["Sa\u00b7\u00dfen", "da", "drei", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In gr\u00fcner Gartenlauben", "tokens": ["In", "gr\u00fc\u00b7ner", "Gar\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf der Bank zu Weine.", "tokens": ["Auf", "der", "Bank", "zu", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hat sie all' in seinem Strick,", "tokens": ["Hat", "sie", "all'", "in", "sei\u00b7nem", "Strick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zahlt er hie, zahlt er da,", "tokens": ["Zahlt", "er", "hie", ",", "zahlt", "er", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Findet Schelmen \u00fcberall.", "tokens": ["Fin\u00b7det", "Schel\u00b7men", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Ru\u00df' der ist mir auch ein Geck,", "tokens": ["Der", "Ru\u00df'", "der", "ist", "mir", "auch", "ein", "Geck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt er so \u2013 zu K\u00f6ln am Rheine", "tokens": ["Sagt", "er", "so", "\u2013", "zu", "K\u00f6ln", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "APPR", "NE", "APPRART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen da drei Bauern", "tokens": ["Sa\u00b7\u00dfen", "da", "drei", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In gr\u00fcner Gartenlauben", "tokens": ["In", "gr\u00fc\u00b7ner", "Gar\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf der Bank zu Weine.", "tokens": ["Auf", "der", "Bank", "zu", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "O was kriegt' der da f\u00fcr Schl\u00e4g',", "tokens": ["O", "was", "kriegt'", "der", "da", "f\u00fcr", "Schl\u00e4g'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich wollt' er h\u00e4tt' noch mehr gekriegt,", "tokens": ["Ich", "wollt'", "er", "h\u00e4tt'", "noch", "mehr", "ge\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Tanzt er da, ist gar kein Russe nicht.", "tokens": ["Tanzt", "er", "da", ",", "ist", "gar", "kein", "Rus\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, unser Kaiser ist geschlagen,", "tokens": ["Ja", ",", "un\u00b7ser", "Kai\u00b7ser", "ist", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt er so \u2013 zu K\u00f6ln am Rheine", "tokens": ["Sagt", "er", "so", "\u2013", "zu", "K\u00f6ln", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "APPR", "NE", "APPRART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen da drei Bauern", "tokens": ["Sa\u00b7\u00dfen", "da", "drei", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In gr\u00fcner Gartenlauben", "tokens": ["In", "gr\u00fc\u00b7ner", "Gar\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf der Bank zu Weine.", "tokens": ["Auf", "der", "Bank", "zu", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Das m\u00fcssen wir K\u00f6lner alle klagen,", "tokens": ["Das", "m\u00fcs\u00b7sen", "wir", "K\u00f6l\u00b7ner", "al\u00b7le", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NN", "PIS", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Gott woll' ihn ewig st\u00e4rken,", "tokens": ["Gott", "woll'", "ihn", "e\u00b7wig", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm das R\u00f6m'sche Reich wieder unterwerfen.", "tokens": ["Ihm", "das", "R\u00f6m'\u00b7sche", "Reich", "wie\u00b7der", "un\u00b7ter\u00b7wer\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.5": {"line.1": {"text": "Soll nur die Stadt K\u00f6ln bestahn,", "tokens": ["Soll", "nur", "die", "Stadt", "K\u00f6ln", "be\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NE", "VVPP", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.2": {"text": "Sagt er so \u2013 zu K\u00f6ln am Rheine", "tokens": ["Sagt", "er", "so", "\u2013", "zu", "K\u00f6ln", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "APPR", "NE", "APPRART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen da drei Bauern", "tokens": ["Sa\u00b7\u00dfen", "da", "drei", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In gr\u00fcner Gartenlauben", "tokens": ["In", "gr\u00fc\u00b7ner", "Gar\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf der Bank zu Weine,", "tokens": ["Auf", "der", "Bank", "zu", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "M\u00f6gen sie alle zum Teufel gahn,", "tokens": ["M\u00f6\u00b7gen", "sie", "al\u00b7le", "zum", "Teu\u00b7fel", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Doch der Franzos' zu allererst,", "tokens": ["Doch", "der", "Fran\u00b7zos'", "zu", "al\u00b7le\u00b7rerst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df der in der H\u00f6ll' gebraten werd'!", "tokens": ["Da\u00df", "der", "in", "der", "H\u00f6ll'", "ge\u00b7bra\u00b7ten", "werd'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}