{"dta.poem.354": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Dorinde  an dem unentpfindlichen  Silvio.  \n Aus dem Jtali\u00e4nischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df ich den holden Blick nach frembden Augen streichen", "tokens": ["La\u00df", "ich", "den", "hol\u00b7den", "Blick", "nach", "fremb\u00b7den", "Au\u00b7gen", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wilt du diese Gunst den Ehe-bruch vergleichen", "tokens": ["So", "wilt", "du", "die\u00b7se", "Gunst", "den", "E\u00b7he\u00b7bruch", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann aber ich von dir verlang\u2019 ein heisses Hertz/", "tokens": ["Wann", "a\u00b7ber", "ich", "von", "dir", "ver\u00b7lang'", "ein", "heis\u00b7ses", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So krieg\u2019 ich nichts als Ey\u00df/ vor Feude herben Schmertz.", "tokens": ["So", "krieg'", "ich", "nichts", "als", "Ey\u00df", "/", "vor", "Feu\u00b7de", "her\u00b7ben", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$(", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schau zwar Himmels-Lust in deinen sch\u00f6nen Augen/", "tokens": ["Ich", "schau", "zwar", "Him\u00b7mels\u00b7Lust", "in", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mu\u00df doch H\u00f6llen-Pein aus den Crystallen saugen/", "tokens": ["Und", "mu\u00df", "doch", "H\u00f6l\u00b7len\u00b7Pein", "aus", "den", "Crys\u00b7tal\u00b7len", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum h\u00f6r; ich habe dich Kaltsinn\u2019ger gung geliebt", "tokens": ["Drum", "h\u00f6r", ";", "ich", "ha\u00b7be", "dich", "Kalt\u00b7sinn'\u00b7ger", "gung", "ge\u00b7liebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJA", "NN", "VVPP"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die Treu ist nun erbla\u00dft/ die ich sonst ausge\u00fcbt.", "tokens": ["Die", "Treu", "ist", "nun", "er\u00b7bla\u00dft", "/", "die", "ich", "sonst", "aus\u00b7ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}