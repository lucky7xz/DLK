{"dta.poem.1217": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich finde mich im Mittel meiner Schmertzen", "tokens": ["Ich", "fin\u00b7de", "mich", "im", "Mit\u00b7tel", "mei\u00b7ner", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey Amaranthen wieder ein/", "tokens": ["Bey", "A\u00b7ma\u00b7ran\u00b7then", "wie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein s\u00fcsser Blick kan meinem krancken Hertzen", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Blick", "kan", "mei\u00b7nem", "kran\u00b7cken", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergelten die erlittne Pein.", "tokens": ["Ver\u00b7gel\u00b7ten", "die", "er\u00b7litt\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch was soll f\u00fcr H\u00fclffe meinen Schmertzen", "tokens": ["Je\u00b7doch", "was", "soll", "f\u00fcr", "H\u00fclf\u00b7fe", "mei\u00b7nen", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VMFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch ihrer Augen Glantz geschehn:", "tokens": ["Durch", "ih\u00b7rer", "Au\u00b7gen", "Glantz", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich habe sie zu Schaden meinem Hertzen", "tokens": ["Ich", "ha\u00b7be", "sie", "zu", "Scha\u00b7den", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bereits nur allzuviel gesehn.", "tokens": ["Be\u00b7reits", "nur", "all\u00b7zu\u00b7viel", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}