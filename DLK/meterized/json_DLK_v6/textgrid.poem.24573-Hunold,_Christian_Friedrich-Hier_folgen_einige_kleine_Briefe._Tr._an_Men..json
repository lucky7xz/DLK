{"textgrid.poem.24573": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hier folgen einige kleine Briefe. Tr. an Men.", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund/ ", "tokens": ["Mein", "Freund", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Sein Hertz geht Himmel an/ meins klebt noch an der Welt.", "tokens": ["Sein", "Hertz", "geht", "Him\u00b7mel", "an", "/", "meins", "klebt", "noch", "an", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "$(", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil meine Jugend mir noch keine Predigt h\u00e4lt:", "tokens": ["Weil", "mei\u00b7ne", "Ju\u00b7gend", "mir", "noch", "kei\u00b7ne", "Pre\u00b7digt", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan meine Feder noch mit Liebes-Dinte fliessen.", "tokens": ["Kan", "mei\u00b7ne", "Fe\u00b7der", "noch", "mit", "Lie\u00b7bes\u00b7Din\u00b7te", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vieleicht sieht er von mir nach zwey und zwantzig Jahren/", "tokens": ["Vie\u00b7leicht", "sieht", "er", "von", "mir", "nach", "zwey", "und", "zwant\u00b7zig", "Jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPR", "CARD", "KON", "CARD", "NN", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Etwas/ so auch ", "tokens": ["Et\u00b7was", "/", "so", "auch"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$(", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Fehlt mir ", "tokens": ["Fehlt", "mir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Will ich doch keinen Flei\u00df was erlernen sparen.", "tokens": ["Will", "ich", "doch", "kei\u00b7nen", "Flei\u00df", "was", "er\u00b7ler\u00b7nen", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}