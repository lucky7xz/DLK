{"textgrid.poem.61456": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn es rote Rosen schneit,", "tokens": ["Wenn", "es", "ro\u00b7te", "Ro\u00b7sen", "schneit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es Liebe regnet,", "tokens": ["Wenn", "es", "Lie\u00b7be", "reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00d6ffne, Herz, dem Gl\u00fcck dich weit,", "tokens": ["\u00d6ff\u00b7ne", ",", "Herz", ",", "dem", "Gl\u00fcck", "dich", "weit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das so hold dich segnet.", "tokens": ["Das", "so", "hold", "dich", "seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Halt im Liede fest den Glanz", "tokens": ["Halt", "im", "Lie\u00b7de", "fest", "den", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solcher Freudentage,", "tokens": ["Sol\u00b7cher", "Freu\u00b7den\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch ins Heut versunken ganz", "tokens": ["Doch", "ins", "Heut", "ver\u00b7sun\u00b7ken", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht nach Morgen frage.", "tokens": ["Nicht", "nach", "Mor\u00b7gen", "fra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wei\u00dft du doch, der Rosenzeit", "tokens": ["Wei\u00dft", "du", "doch", ",", "der", "Ro\u00b7sen\u00b7zeit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgt die Sonnenwende,", "tokens": ["Folgt", "die", "Son\u00b7nen\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Liebe lohnt mit Leid", "tokens": ["Und", "die", "Lie\u00b7be", "lohnt", "mit", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immerdar am Ende.", "tokens": ["Im\u00b7mer\u00b7dar", "am", "En\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}