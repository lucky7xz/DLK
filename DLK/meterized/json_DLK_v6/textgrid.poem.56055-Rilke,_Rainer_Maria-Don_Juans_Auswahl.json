{"textgrid.poem.56055": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Don Juans Auswahl", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und der Engel trat ihn an: Bereite", "tokens": ["Und", "der", "En\u00b7gel", "trat", "ihn", "an", ":", "Be\u00b7rei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "dich mir ganz. Und da ist mein Gebot.", "tokens": ["dich", "mir", "ganz", ".", "Und", "da", "ist", "mein", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "$.", "KON", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn da\u00df einer jene \u00fcberschreite,", "tokens": ["Denn", "da\u00df", "ei\u00b7ner", "je\u00b7ne", "\u00fc\u00b7bersc\u00b7hrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "die die S\u00fc\u00dfesten an ihrer Seite", "tokens": ["die", "die", "S\u00fc\u00b7\u00dfes\u00b7ten", "an", "ih\u00b7rer", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "bitter machen, tut mir not.", "tokens": ["bit\u00b7ter", "ma\u00b7chen", ",", "tut", "mir", "not", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwar auch du kannst wenig besser lieben,", "tokens": ["Zwar", "auch", "du", "kannst", "we\u00b7nig", "bes\u00b7ser", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "(unterbrich mich nicht: du irrst),", "tokens": ["(", "un\u00b7ter\u00b7brich", "mich", "nicht", ":", "du", "irrst", ")", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "PTKNEG", "$.", "PPER", "ADV", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "doch du gl\u00fchest, und es steht geschrieben,", "tokens": ["doch", "du", "gl\u00fc\u00b7hest", ",", "und", "es", "steht", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "da\u00df du viele f\u00fchren wirst", "tokens": ["da\u00df", "du", "vie\u00b7le", "f\u00fch\u00b7ren", "wirst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "zu der Einsamkeit, die diesen", "tokens": ["zu", "der", "Ein\u00b7sam\u00b7keit", ",", "die", "die\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "tiefen Eingang hat. La\u00df ein", "tokens": ["tie\u00b7fen", "Ein\u00b7gang", "hat", ".", "La\u00df", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "$.", "VVIMP", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "die, die ich dir zugewiesen,", "tokens": ["die", ",", "die", "ich", "dir", "zu\u00b7ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "da\u00df sie wachsend Helo\u00efsen", "tokens": ["da\u00df", "sie", "wach\u00b7send", "He\u00b7lo\u00ef\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "\u00fcberstehn und \u00fcberschrein.", "tokens": ["\u00fc\u00b7bers\u00b7tehn", "und", "\u00fc\u00b7bersc\u00b7hrein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVINF", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}