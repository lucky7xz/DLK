{"textgrid.poem.57098": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es bl\u00e4st wer in der Winterluft", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00e4st wer in der Winterluft", "tokens": ["Es", "bl\u00e4st", "wer", "in", "der", "Win\u00b7ter\u00b7luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zum Blut der Abendr\u00f6te ...", "tokens": ["zum", "Blut", "der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein fragender Vorfr\u00fchlingsduft", "tokens": ["Ein", "fra\u00b7gen\u00b7der", "Vor\u00b7fr\u00fch\u00b7lings\u00b7duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mischt sich dem Klagen der Fl\u00f6te.", "tokens": ["mischt", "sich", "dem", "Kla\u00b7gen", "der", "Fl\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Vor einer Sch\u00e4nke steht ein Kind,", "tokens": ["Vor", "ei\u00b7ner", "Sch\u00e4n\u00b7ke", "steht", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein schlankes, mit kurzen R\u00f6cken.", "tokens": ["ein", "schlan\u00b7kes", ",", "mit", "kur\u00b7zen", "R\u00f6\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es steht mit seinen Locken im Wind", "tokens": ["Es", "steht", "mit", "sei\u00b7nen", "Lo\u00b7cken", "im", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "wie ein erstes Fr\u00fchlings-Erschrecken ...", "tokens": ["wie", "ein", "ers\u00b7tes", "Fr\u00fch\u00b7lings\u00b7Er\u00b7schre\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dahinter flammt durch Pappelreihn,", "tokens": ["Da\u00b7hin\u00b7ter", "flammt", "durch", "Pap\u00b7pel\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Welt mit Schmerz durchseelend,", "tokens": ["die", "Welt", "mit", "Schmerz", "durch\u00b7see\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "der tiefe himmlische Widerschein", "tokens": ["der", "tie\u00b7fe", "himm\u00b7li\u00b7sche", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "von unendlichem Gl\u00fcck und Elend.", "tokens": ["von", "un\u00b7end\u00b7li\u00b7chem", "Gl\u00fcck", "und", "E\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}