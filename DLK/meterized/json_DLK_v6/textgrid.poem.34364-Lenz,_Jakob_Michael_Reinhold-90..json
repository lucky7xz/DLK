{"textgrid.poem.34364": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach soll so viele Trefflichkeit", "tokens": ["Ach", "soll", "so", "vie\u00b7le", "Treff\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VMFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wenig Erde decken", "tokens": ["So", "we\u00b7nig", "Er\u00b7de", "de\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In diesem d\u00fcrren Moosekleid", "tokens": ["In", "die\u00b7sem", "d\u00fcr\u00b7ren", "Moo\u00b7se\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit k\u00fcmmerlichen Hecken?", "tokens": ["Mit", "k\u00fcm\u00b7mer\u00b7li\u00b7chen", "He\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist dieses schlechte Kissen werth,", "tokens": ["Ist", "die\u00b7ses", "schlech\u00b7te", "Kis\u00b7sen", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df hier dein Haupt der Ruh begehrt?", "tokens": ["Da\u00df", "hier", "dein", "Haupt", "der", "Ruh", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"