{"textgrid.poem.61782": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberm Schlo\u00df und seinen G\u00e4rten", "tokens": ["\u00dc\u00b7berm", "Schlo\u00df", "und", "sei\u00b7nen", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fctet hei\u00df im Dunst der Mittag;", "tokens": ["Br\u00fc\u00b7tet", "hei\u00df", "im", "Dunst", "der", "Mit\u00b7tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie in einem M\u00e4rchen wandl' ich", "tokens": ["Wie", "in", "ei\u00b7nem", "M\u00e4r\u00b7chen", "wandl'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die schw\u00fcle Totenstille.", "tokens": ["Durch", "die", "schw\u00fc\u00b7le", "To\u00b7tens\u00b7til\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schlummertrunken um die T\u00fcrme", "tokens": ["Schlum\u00b7mer\u00b7trun\u00b7ken", "um", "die", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4ngt der Efeu; vor den Fenstern", "tokens": ["H\u00e4ngt", "der", "E\u00b7feu", ";", "vor", "den", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Liegen Schalter, mit geschlo\u00dfnen", "tokens": ["Lie\u00b7gen", "Schal\u00b7ter", ",", "mit", "ge\u00b7schlo\u00df\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wimpern scheint das Haus zu tr\u00e4umen.", "tokens": ["Wim\u00b7pern", "scheint", "das", "Haus", "zu", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch die hohen roten Blumen", "tokens": ["Auch", "die", "ho\u00b7hen", "ro\u00b7ten", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicken wie im Schlaf gespenstisch,", "tokens": ["Ni\u00b7cken", "wie", "im", "Schlaf", "ge\u00b7spens\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigend am verfallnen Springborn", "tokens": ["Schwei\u00b7gend", "am", "ver\u00b7fall\u00b7nen", "Spring\u00b7born"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnt sich eine gr\u00fcne Schlange.", "tokens": ["Sonnt", "sich", "ei\u00b7ne", "gr\u00fc\u00b7ne", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zum smaragdnen Ring verschlungen", "tokens": ["Zum", "sma\u00b7ragd\u00b7nen", "Ring", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Z\u00fcngelt sie und blickt mit klugen", "tokens": ["Z\u00fcn\u00b7gelt", "sie", "und", "blickt", "mit", "klu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Augen zu mir auf, als w\u00fc\u00dfte", "tokens": ["Au\u00b7gen", "zu", "mir", "auf", ",", "als", "w\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manch Geheimnis sie zu melden,", "tokens": ["Manch", "Ge\u00b7heim\u00b7nis", "sie", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Manch verschollenes Geheimnis", "tokens": ["Manch", "ver\u00b7schol\u00b7le\u00b7nes", "Ge\u00b7heim\u00b7nis"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der sch\u00f6nen K\u00f6nigstochter,", "tokens": ["Von", "der", "sch\u00f6\u00b7nen", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die des Abends hier gewandelt,", "tokens": ["Die", "des", "A\u00b7bends", "hier", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der blonde Page seufzte,", "tokens": ["Wenn", "der", "blon\u00b7de", "Pa\u00b7ge", "seufz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von den Schw\u00fcren, die die Mondnacht", "tokens": ["Von", "den", "Schw\u00fc\u00b7ren", ",", "die", "die", "Mond\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "H\u00f6rt' im Dunkel jener Lauben,", "tokens": ["H\u00f6rt'", "im", "Dun\u00b7kel", "je\u00b7ner", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Blut, das dort geflossen,", "tokens": ["Von", "dem", "Blut", ",", "das", "dort", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die roten Blumen schwanken.", "tokens": ["Wo", "die", "ro\u00b7ten", "Blu\u00b7men", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schon beschleicht ein heimlich Grauen", "tokens": ["Schon", "be\u00b7schleicht", "ein", "heim\u00b7lich", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir das Herz, da dr\u00f6hnt die Schlo\u00dfuhr", "tokens": ["Mir", "das", "Herz", ",", "da", "dr\u00f6hnt", "die", "Schlo\u00b7\u00dfuhr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eins, und raschelnd in die B\u00fcsche", "tokens": ["Eins", ",", "und", "ra\u00b7schelnd", "in", "die", "B\u00fc\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schl\u00fcpft zur\u00fcck die gr\u00fcne Schlange.", "tokens": ["Schl\u00fcpft", "zu\u00b7r\u00fcck", "die", "gr\u00fc\u00b7ne", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}