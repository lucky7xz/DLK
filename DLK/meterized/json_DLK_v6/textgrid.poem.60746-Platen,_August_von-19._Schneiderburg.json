{"textgrid.poem.60746": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "19. Schneiderburg", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schneider flink mit der Ziege sein", "tokens": ["Ein", "Schnei\u00b7der", "flink", "mit", "der", "Zie\u00b7ge", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Behauste den Krempenstein,", "tokens": ["Be\u00b7haus\u00b7te", "den", "Krem\u00b7pen\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sah oft von felsiger Schwelle", "tokens": ["Sah", "oft", "von", "fel\u00b7si\u00b7ger", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hinab zu der Donauwelle,", "tokens": ["Hin\u00b7ab", "zu", "der", "Do\u00b7nau\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In rei\u00dfende Wirbel hinein.", "tokens": ["In", "rei\u00b7\u00dfen\u00b7de", "Wir\u00b7bel", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "So sa\u00df er oft und so sang er dabei:", "tokens": ["So", "sa\u00df", "er", "oft", "und", "so", "sang", "er", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie leb ich sorgenfrei!", "tokens": ["Wie", "leb", "ich", "sor\u00b7gen\u00b7frei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meine Ziege, die n\u00e4hrt und letzt mich,", "tokens": ["Mei\u00b7ne", "Zie\u00b7ge", ",", "die", "n\u00e4hrt", "und", "letzt", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "KON", "ADV", "PPER", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Manch Liedchen klingt und ergetzt mich,", "tokens": ["Manch", "Lied\u00b7chen", "klingt", "und", "er\u00b7getzt", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "F\u00e4hrt unten ein Schiffer vorbei!", "tokens": ["F\u00e4hrt", "un\u00b7ten", "ein", "Schif\u00b7fer", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Doch ach, die Ziege, sie starb und ihr", "tokens": ["Doch", "ach", ",", "die", "Zie\u00b7ge", ",", "sie", "starb", "und", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rief nach er: Wehe mir!", "tokens": ["Rief", "nach", "er", ":", "We\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wirst du mich nicht mehr laben,", "tokens": ["So", "wirst", "du", "mich", "nicht", "mehr", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So mu\u00df ich dich hier begraben,", "tokens": ["So", "mu\u00df", "ich", "dich", "hier", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Im Bette der Donau hier?", "tokens": ["Im", "Bet\u00b7te", "der", "Do\u00b7nau", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch als er sie schleudern will hinein,", "tokens": ["Doch", "als", "er", "sie", "schleu\u00b7dern", "will", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verwickelt, o Todespein!", "tokens": ["Ver\u00b7wi\u00b7ckelt", ",", "o", "To\u00b7de\u00b7spein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "FM", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Horn sich ihm in die Kleider:", "tokens": ["Ihr", "Horn", "sich", "ihm", "in", "die", "Klei\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nun liegen Zieg und Schneider", "tokens": ["Nun", "lie\u00b7gen", "Zieg", "und", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tief unter dem Krempenstein!", "tokens": ["Tief", "un\u00b7ter", "dem", "Krem\u00b7pen\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}