{"textgrid.poem.56538": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Werbung", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schauderst du, wenn unser Blick entflammt?", "tokens": ["Was", "schau\u00b7derst", "du", ",", "wenn", "un\u00b7ser", "Blick", "ent\u00b7flammt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es krampft sich deine Hand dir unbewu\u00dft.", "tokens": ["Es", "krampft", "sich", "dei\u00b7ne", "Hand", "dir", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sind wir denn, ich und du, verdammt,", "tokens": ["Sind", "wir", "denn", ",", "ich", "und", "du", ",", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PPER", "KON", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Brand zu w\u00fchlen in die Brust?", "tokens": ["Den", "Brand", "zu", "w\u00fch\u00b7len", "in", "die", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchl's an deinen feuchten H\u00e4nden,", "tokens": ["Ich", "f\u00fchl's", "an", "dei\u00b7nen", "feuch\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft es, was wir einsam tr\u00e4umen.", "tokens": ["Du", "wei\u00dft", "es", ",", "was", "wir", "ein\u00b7sam", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schicksal k\u00f6nnen wir nicht wenden.", "tokens": ["Das", "Schick\u00b7sal", "k\u00f6n\u00b7nen", "wir", "nicht", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es hilft uns nicht, da\u00df wir uns b\u00e4umen.", "tokens": ["Es", "hilft", "uns", "nicht", ",", "da\u00df", "wir", "uns", "b\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von Sch\u00f6nheit trunken mu\u00df ich dich genie\u00dfen,", "tokens": ["Von", "Sch\u00f6n\u00b7heit", "trun\u00b7ken", "mu\u00df", "ich", "dich", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Wunderleibes Pracht begreifen,", "tokens": ["Des", "Wun\u00b7der\u00b7lei\u00b7bes", "Pracht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du und ich im Jauchzen eins verflie\u00dfen,", "tokens": ["Da\u00df", "du", "und", "ich", "im", "Jauch\u00b7zen", "eins", "ver\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "APPRART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df wir uns br\u00fcnstig lichtw\u00e4rts reifen.", "tokens": ["Da\u00df", "wir", "uns", "br\u00fcns\u00b7tig", "licht\u00b7w\u00e4rts", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du, schlag dir deine Brust, da\u00df du bek\u00e4mpft", "tokens": ["Du", ",", "schlag", "dir", "dei\u00b7ne", "Brust", ",", "da\u00df", "du", "be\u00b7k\u00e4mpft"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In falscher Scham die unerh\u00f6rte Glut.", "tokens": ["In", "fal\u00b7scher", "Scham", "die", "un\u00b7er\u00b7h\u00f6r\u00b7te", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aufs Lager flie\u00dft das Licht ged\u00e4mpft.", "tokens": ["Aufs", "La\u00b7ger", "flie\u00dft", "das", "Licht", "ge\u00b7d\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir rast nach dir und h\u00e4mmert dumpf mein Blut.", "tokens": ["Mir", "rast", "nach", "dir", "und", "h\u00e4m\u00b7mert", "dumpf", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich mu\u00df verbrennen, es verzehrt mich.", "tokens": ["Ich", "mu\u00df", "ver\u00b7bren\u00b7nen", ",", "es", "ver\u00b7zehrt", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Arme werden d\u00fcrr vor Glut.", "tokens": ["Die", "Ar\u00b7me", "wer\u00b7den", "d\u00fcrr", "vor", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon viel zu lang hast du gewehrt dich.", "tokens": ["Schon", "viel", "zu", "lang", "hast", "du", "ge\u00b7wehrt", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "VAFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kraft ringt sich hinauf, es schwillt die Flut.", "tokens": ["Die", "Kraft", "ringt", "sich", "hin\u00b7auf", ",", "es", "schwillt", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}