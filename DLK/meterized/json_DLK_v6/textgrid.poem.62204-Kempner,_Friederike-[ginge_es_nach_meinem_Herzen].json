{"textgrid.poem.62204": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ginge es nach meinem Herzen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ginge es nach meinem Herzen,", "tokens": ["Gin\u00b7ge", "es", "nach", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrde allen ich vergeben,", "tokens": ["W\u00fcr\u00b7de", "al\u00b7len", "ich", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen denen, welche leben:", "tokens": ["Al\u00b7len", "de\u00b7nen", ",", "wel\u00b7che", "le\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PDS", "$,", "PRELS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene tausend Qual und Schmerzen,", "tokens": ["Je\u00b7ne", "tau\u00b7send", "Qual", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "CARD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche sie mich lie\u00dfen leiden,", "tokens": ["Wel\u00b7che", "sie", "mich", "lie\u00b7\u00dfen", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann sie darum nicht beneiden \u2013", "tokens": ["Kann", "sie", "da\u00b7rum", "nicht", "be\u00b7nei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4lzten sich in Gold und Kot \u2013", "tokens": ["W\u00e4lz\u00b7ten", "sich", "in", "Gold", "und", "Kot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach \u2013 ihr Leben gleicht dem Tod.", "tokens": ["Ach", "\u2013", "ihr", "Le\u00b7ben", "gleicht", "dem", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}