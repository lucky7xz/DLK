{"textgrid.poem.47458": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wehre nicht, o Lieb]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehre nicht, o Lieb,", "tokens": ["Weh\u00b7re", "nicht", ",", "o", "Lieb", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "FM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00fchlen in den Locken", "tokens": ["W\u00fch\u00b7len", "in", "den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deines holden Hauptes", "tokens": ["Dei\u00b7nes", "hol\u00b7den", "Haup\u00b7tes"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "La\u00df mich ohne Stocken!", "tokens": ["La\u00df", "mich", "oh\u00b7ne", "Sto\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn ein eigner Trieb", "tokens": ["Denn", "ein", "eig\u00b7ner", "Trieb"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Waltet, es bew\u00e4ltigt", "tokens": ["Wal\u00b7tet", ",", "es", "be\u00b7w\u00e4l\u00b7tigt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Eine sympathetisch", "tokens": ["Ei\u00b7ne", "sym\u00b7pa\u00b7the\u00b7tisch"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Eigne Zaubermacht.", "tokens": ["Eig\u00b7ne", "Zau\u00b7ber\u00b7macht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Wisse, meine Seele", "tokens": ["Wis\u00b7se", ",", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Such' ich auf, die arme,", "tokens": ["Such'", "ich", "auf", ",", "die", "ar\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Die sich in die dunkle,", "tokens": ["Die", "sich", "in", "die", "dunk\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Labyrinth'sche Pracht,", "tokens": ["La\u00b7by\u00b7rinth'\u00b7sche", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Tief hinein verloren", "tokens": ["Tief", "hin\u00b7ein", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "In die sch\u00f6ne Nacht.", "tokens": ["In", "die", "sch\u00f6\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}