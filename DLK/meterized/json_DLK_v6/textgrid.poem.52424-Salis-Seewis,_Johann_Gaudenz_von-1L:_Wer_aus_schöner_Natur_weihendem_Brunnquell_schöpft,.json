{"textgrid.poem.52424": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer aus sch\u00f6ner Natur weihendem Brunnquell sch\u00f6pft,", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer aus sch\u00f6ner Natur weihendem Brunnquell sch\u00f6pft,", "tokens": ["Wer", "aus", "sch\u00f6\u00b7ner", "Na\u00b7tur", "wei\u00b7hen\u00b7dem", "Brunn\u00b7quell", "sch\u00f6pft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Misset gerne den niedern Prunk.", "tokens": ["Mis\u00b7set", "ger\u00b7ne", "den", "nie\u00b7dern", "Prunk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Froh durchirrt er die Flur; froh, wenn auch seinen Fu\u00df", "tokens": ["Froh", "durch\u00b7irrt", "er", "die", "Flur", ";", "froh", ",", "wenn", "auch", "sei\u00b7nen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$.", "ADJD", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Keine blitzende Schnall' umw\u00f6lbt.", "tokens": ["Kei\u00b7ne", "blit\u00b7zen\u00b7de", "Schnall'", "um\u00b7w\u00f6lbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Perlen achtet er Spreu; Spinnengewebe nur", "tokens": ["Per\u00b7len", "ach\u00b7tet", "er", "Spreu", ";", "Spin\u00b7nen\u00b7ge\u00b7we\u00b7be", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "NN", "$.", "VVFIN", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Brabants Spitzen; er l\u00e4chelt kalt", "tokens": ["Bra\u00b7bants", "Spit\u00b7zen", ";", "er", "l\u00e4\u00b7chelt", "kalt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "ADJD"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Auf den funklenden Ring oder der Dose Schmelz", "tokens": ["Auf", "den", "funk\u00b7len\u00b7den", "Ring", "o\u00b7der", "der", "Do\u00b7se", "Schmelz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "In des prunkenden Thoren Hand.", "tokens": ["In", "des", "prun\u00b7ken\u00b7den", "Tho\u00b7ren", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Gerne mi\u00dft er die Stadt, blickt auf vermummten Tanz,", "tokens": ["Ger\u00b7ne", "mi\u00dft", "er", "die", "Stadt", ",", "blickt", "auf", "ver\u00b7mumm\u00b7ten", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auf belastete Tafeln Hohn.", "tokens": ["Auf", "be\u00b7las\u00b7te\u00b7te", "Ta\u00b7feln", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Nimmer reizt ihn der Hof, nimmer der Goldpalast,", "tokens": ["Nim\u00b7mer", "reizt", "ihn", "der", "Hof", ",", "nim\u00b7mer", "der", "Gold\u00b7pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Noch der marmorne F\u00fcrstensaal.", "tokens": ["Noch", "der", "mar\u00b7mor\u00b7ne", "F\u00fcrs\u00b7ten\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Aber Seelengef\u00fchl trinkt sein geweihter Blick;", "tokens": ["A\u00b7ber", "See\u00b7len\u00b7ge\u00b7f\u00fchl", "trinkt", "sein", "ge\u00b7weih\u00b7ter", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihn entz\u00fccken des Buchenwalds", "tokens": ["Ihn", "ent\u00b7z\u00fc\u00b7cken", "des", "Bu\u00b7chen\u00b7walds"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "S\u00e4ulenhallen, der Luft sternenbes\u00e4eter Dom,", "tokens": ["S\u00e4u\u00b7len\u00b7hal\u00b7len", ",", "der", "Luft", "ster\u00b7nen\u00b7be\u00b7s\u00e4e\u00b7ter", "Dom", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und der Spiegel des klaren Sees.", "tokens": ["Und", "der", "Spie\u00b7gel", "des", "kla\u00b7ren", "Sees", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Silber gie\u00dft ihm des Monds ruhiges Flimmerlicht,", "tokens": ["Sil\u00b7ber", "gie\u00dft", "ihm", "des", "Monds", "ru\u00b7hi\u00b7ges", "Flim\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gold der scheidende Sonnenstrahl;", "tokens": ["Gold", "der", "schei\u00b7den\u00b7de", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Perlen streut ihm der Tau, f\u00e4rbt sich zum Edelstein", "tokens": ["Per\u00b7len", "streut", "ihm", "der", "Tau", ",", "f\u00e4rbt", "sich", "zum", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Auf dem wankenden Tulpenkelch.", "tokens": ["Auf", "dem", "wan\u00b7ken\u00b7den", "Tul\u00b7pen\u00b7kelch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Kr\u00e4uselnd bl\u00e4ht sich das Moos, polstert den Felsensitz,", "tokens": ["Kr\u00e4u\u00b7selnd", "bl\u00e4ht", "sich", "das", "Moos", ",", "pols\u00b7tert", "den", "Fel\u00b7sen\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schwellt zum Sopha den Rasenbank;", "tokens": ["Schwellt", "zum", "So\u00b7pha", "den", "Ra\u00b7sen\u00b7bank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gef\u00e4llige Lenz sticket ihm Teppiche", "tokens": ["Der", "ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Lenz", "sti\u00b7cket", "ihm", "Tep\u00b7pi\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN"], "meter": "+-+--++--++-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Mit Violen und G\u00fcldenklee.", "tokens": ["Mit", "Vi\u00b7o\u00b7len", "und", "G\u00fcl\u00b7den\u00b7klee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Frische haucht ihm die Kluft, atmet das Birkenlaub,", "tokens": ["Fri\u00b7sche", "haucht", "ihm", "die", "Kluft", ",", "at\u00b7met", "das", "Bir\u00b7ken\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Das vom duftigen Fr\u00fchtau tr\u00e4uft;", "tokens": ["Das", "vom", "duf\u00b7ti\u00b7gen", "Fr\u00fch\u00b7tau", "tr\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schatten br\u00e4unen sich ihm, und der ummooste Bach", "tokens": ["Schat\u00b7ten", "br\u00e4u\u00b7nen", "sich", "ihm", ",", "und", "der", "um\u00b7moos\u00b7te", "Bach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "PPER", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Rauscht ihm K\u00fchlung und Schlummerton.", "tokens": ["Rauscht", "ihm", "K\u00fch\u00b7lung", "und", "Schlum\u00b7mer\u00b7ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Baldachine von Laub w\u00f6lbt ihm der Eiche Schirm", "tokens": ["Bal\u00b7da\u00b7chi\u00b7ne", "von", "Laub", "w\u00f6lbt", "ihm", "der", "Ei\u00b7che", "Schirm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Um den l\u00e4ndlichen Traualtar;", "tokens": ["Um", "den", "l\u00e4nd\u00b7li\u00b7chen", "Trau\u00b7al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und der Nachtigall Lied t\u00f6nt um sein Brautgemach,", "tokens": ["Und", "der", "Nach\u00b7ti\u00b7gall", "Lied", "t\u00f6nt", "um", "sein", "Braut\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Statt des feiernden Abendchors.", "tokens": ["Statt", "des", "fei\u00b7ern\u00b7den", "A\u00b7bend\u00b7chors", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}