{"dta.poem.9402": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .  \n  Dactylische Ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heute sol Freude die Stunde verjagen/", "tokens": ["Heu\u00b7te", "sol", "Freu\u00b7de", "die", "Stun\u00b7de", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Heute sol Freude regieren den Tag/", "tokens": ["Heu\u00b7te", "sol", "Freu\u00b7de", "re\u00b7gie\u00b7ren", "den", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Freude so sonsten in Traurigkeit lag/", "tokens": ["Freu\u00b7de", "so", "sons\u00b7ten", "in", "Trau\u00b7rig\u00b7keit", "lag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Freude sol heute verjagen das Klagen/", "tokens": ["Freu\u00b7de", "sol", "heu\u00b7te", "ver\u00b7ja\u00b7gen", "das", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Klagen und zagen sich endet hierbey:", "tokens": ["Kla\u00b7gen", "und", "za\u00b7gen", "sich", "en\u00b7det", "hier\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "VVFIN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Heute sol Ph\u00f6bus und alle G\u00f6ttinnen", "tokens": ["Heu\u00b7te", "sol", "Ph\u00f6\u00b7bus", "und", "al\u00b7le", "G\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Oden und sch\u00f6ne Ges\u00e4nge beginnen/", "tokens": ["O\u00b7den", "und", "sch\u00f6\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Lauten und Geigen erklingen auch frey!", "tokens": ["Lau\u00b7ten", "und", "Gei\u00b7gen", "er\u00b7klin\u00b7gen", "auch", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}