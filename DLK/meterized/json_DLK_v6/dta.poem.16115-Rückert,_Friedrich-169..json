{"dta.poem.16115": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "169.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gute Absicht macht das B\u00f6se niemals gut,", "tokens": ["Die", "gu\u00b7te", "Ab\u00b7sicht", "macht", "das", "B\u00f6\u00b7se", "nie\u00b7mals", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn gute Absicht hat gar nie, wer B\u00f6ses thut.", "tokens": ["Denn", "gu\u00b7te", "Ab\u00b7sicht", "hat", "gar", "nie", ",", "wer", "B\u00f6\u00b7ses", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "ADV", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Gute aber was du thust, wo nicht dabei", "tokens": ["Das", "Gu\u00b7te", "a\u00b7ber", "was", "du", "thust", ",", "wo", "nicht", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PWS", "PPER", "VVFIN", "$,", "PWAV", "PTKNEG", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gute Absicht ist, sag' ich, da\u00df b\u00f6s' es sei.", "tokens": ["Die", "gu\u00b7te", "Ab\u00b7sicht", "ist", ",", "sag'", "ich", ",", "da\u00df", "b\u00f6s'", "es", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch etwas, weder gut noch b\u00f6se, was vollbracht", "tokens": ["Doch", "et\u00b7was", ",", "we\u00b7der", "gut", "noch", "b\u00f6\u00b7se", ",", "was", "voll\u00b7bracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "$,", "KON", "ADJD", "ADV", "ADJD", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In guter Absicht wird, das hat sie gut gemacht.", "tokens": ["In", "gu\u00b7ter", "Ab\u00b7sicht", "wird", ",", "das", "hat", "sie", "gut", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}