{"dta.poem.2988": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Persisch und Sanskrit, in Griechisch und Latein,", "tokens": ["In", "Per\u00b7sisch", "und", "Sans\u00b7krit", ",", "in", "Grie\u00b7chisch", "und", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In Deutsch und Slavisch siehst du Eine Sprach' allein.", "tokens": ["In", "Deutsch", "und", "Sla\u00b7visch", "siehst", "du", "Ei\u00b7ne", "Sprach'", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie weit die Gegens\u00e4tz' auch auseinander wichen,", "tokens": ["Wie", "weit", "die", "Ge\u00b7gen\u00b7s\u00e4tz'", "auch", "aus\u00b7ein\u00b7an\u00b7der", "wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast sie innerlich zur Einheit ausgeglichen.", "tokens": ["Du", "hast", "sie", "in\u00b7ner\u00b7lich", "zur", "Ein\u00b7heit", "aus\u00b7ge\u00b7gli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum nicht auch, wie in den Sprachen offenbart,", "tokens": ["Wa\u00b7rum", "nicht", "auch", ",", "wie", "in", "den", "Spra\u00b7chen", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willst du das gleiche sehn in Denk- und Glaubensart?", "tokens": ["Willst", "du", "das", "glei\u00b7che", "sehn", "in", "Denk", "und", "Glau\u00b7ben\u00b7sart", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wieweit die Gegens\u00e4tz' auch auseinander weichen,", "tokens": ["Wie\u00b7weit", "die", "Ge\u00b7gen\u00b7s\u00e4tz'", "auch", "aus\u00b7ein\u00b7an\u00b7der", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vermagst du nicht auch sie zur Einheit auszugleichen?", "tokens": ["Ver\u00b7magst", "du", "nicht", "auch", "sie", "zur", "Ein\u00b7heit", "aus\u00b7zu\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In Wahrheit noch nicht kund ward dir der Menschheit Grund,", "tokens": ["In", "Wahr\u00b7heit", "noch", "nicht", "kund", "ward", "dir", "der", "Menschheit", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKNEG", "PTKVZ", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und Weisheit f\u00fchrest du und Lieb' umsonst im Mund.", "tokens": ["Und", "Weis\u00b7heit", "f\u00fch\u00b7rest", "du", "und", "Lieb'", "um\u00b7sonst", "im", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "KON", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}