{"textgrid.poem.41584": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zweiten Weihnachtstage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jerusalem! Jerusalem!", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "!", "Je\u00b7ru\u00b7sa\u00b7lem", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie oft erschollen ist sein Ruf;", "tokens": ["Wie", "oft", "er\u00b7schol\u00b7len", "ist", "sein", "Ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du spieltest sorglos unter dem", "tokens": ["Du", "spiel\u00b7test", "sorg\u00b7los", "un\u00b7ter", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verderben, unter Rosses Huf", "tokens": ["Ver\u00b7der\u00b7ben", ",", "un\u00b7ter", "Ros\u00b7ses", "Huf"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Rades Wucht, schau! darum ist", "tokens": ["Und", "Ra\u00b7des", "Wucht", ",", "schau", "!", "da\u00b7rum", "ist"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "NN", "$,", "PTKVZ", "$.", "PAV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ver\u00f6det deine St\u00e4tte worden.", "tokens": ["Ver\u00b7\u00f6\u00b7det", "dei\u00b7ne", "St\u00e4t\u00b7te", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und du ein irres K\u00fcchlein bist,", "tokens": ["Und", "du", "ein", "ir\u00b7res", "K\u00fcch\u00b7lein", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich duckend unter Geierhorden.", "tokens": ["Sich", "du\u00b7ckend", "un\u00b7ter", "Gei\u00b7er\u00b7hor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor\u00fcber ist die heil'ge Zeit,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "die", "heil'\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo deinen Sinnen er bekannt", "tokens": ["Wo", "dei\u00b7nen", "Sin\u00b7nen", "er", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seiner Wunder Herrlichkeit", "tokens": ["Und", "sei\u00b7ner", "Wun\u00b7der", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zieht nur als Sage durch das Land.", "tokens": ["Zieht", "nur", "als", "Sa\u00b7ge", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Weise wiegt sein schweres Haupt,", "tokens": ["Der", "Wei\u00b7se", "wiegt", "sein", "schwe\u00b7res", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tor will dessen sich entschlagen,", "tokens": ["Der", "Tor", "will", "des\u00b7sen", "sich", "ent\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PDS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nur die fromme Einfalt glaubt", "tokens": ["Und", "nur", "die", "from\u00b7me", "Ein\u00b7falt", "glaubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mag die Opfergabe tragen.", "tokens": ["Und", "mag", "die", "Op\u00b7fer\u00b7ga\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O bringt sie nur ein willig Tun,", "tokens": ["O", "bringt", "sie", "nur", "ein", "wil\u00b7lig", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein treues K\u00e4mpfen zum Altar,", "tokens": ["Ein", "treu\u00b7es", "K\u00e4mp\u00b7fen", "zum", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wird auf ihr die Gnade ruhn,", "tokens": ["Dann", "wird", "auf", "ihr", "die", "Gna\u00b7de", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein innres Wunder, ewig klar.", "tokens": ["Ein", "inn\u00b7res", "Wun\u00b7der", ",", "e\u00b7wig", "klar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ist es so, der Gegenwart", "tokens": ["Doch", "ist", "es", "so", ",", "der", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gebrochen sind gewalt'ge St\u00fctzen,", "tokens": ["Ge\u00b7bro\u00b7chen", "sind", "ge\u00b7walt'\u00b7ge", "St\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Seit unsern Sinnen tr\u00fcb und hart", "tokens": ["Seit", "un\u00b7sern", "Sin\u00b7nen", "tr\u00fcb", "und", "hart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verh\u00fcllt ward seiner Zeichen Blitzen.", "tokens": ["Ver\u00b7h\u00fcllt", "ward", "sei\u00b7ner", "Zei\u00b7chen", "Blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "War einst erhellt der schwanke Steg,", "tokens": ["War", "einst", "er\u00b7hellt", "der", "schwan\u00b7ke", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und klaffte klar der Abgrund auf,", "tokens": ["Und", "klaff\u00b7te", "klar", "der", "Ab\u00b7grund", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir m\u00fcssen suchen unsern Weg", "tokens": ["Wir", "m\u00fcs\u00b7sen", "su\u00b7chen", "un\u00b7sern", "Weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Heiderauch ein armer Hauf.", "tokens": ["Im", "Hei\u00b7de\u00b7rauch", "ein", "ar\u00b7mer", "Hauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Glaubens k\u00f6stlich teurer Preis", "tokens": ["Des", "Glau\u00b7bens", "k\u00f6st\u00b7lich", "teu\u00b7rer", "Preis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ward wie gestellt auf Gletschers H\u00f6hen;", "tokens": ["Ward", "wie", "ge\u00b7stellt", "auf", "Glet\u00b7schers", "H\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "VVPP", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir m\u00fcssen klimmen \u00fcber Eis", "tokens": ["Wir", "m\u00fcs\u00b7sen", "klim\u00b7men", "\u00fc\u00b7ber", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schwindelnd uns am Schlunde drehen.", "tokens": ["Und", "schwin\u00b7delnd", "uns", "am", "Schlun\u00b7de", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was, Herr, du lie\u00dfest fort und fort,", "tokens": ["Was", ",", "Herr", ",", "du", "lie\u00b7\u00dfest", "fort", "und", "fort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat uns die Seele wohl gebrannt;", "tokens": ["Hat", "uns", "die", "See\u00b7le", "wohl", "ge\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleibt es ein geschriebnes Wort,", "tokens": ["Doch", "bleibt", "es", "ein", "ge\u00b7schrieb\u00b7nes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unsichtbar die lebend'ge Hand.", "tokens": ["Un\u00b7sicht\u00b7bar", "die", "le\u00b7ben\u00b7d'\u00b7ge", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Ach nur wo Gr\u00fcbeln nicht und Stolz", "tokens": ["Ach", "nur", "wo", "Gr\u00fc\u00b7beln", "nicht", "und", "Stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "PWAV", "NN", "PTKNEG", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Stamme nagt seit Tag und Jahren,", "tokens": ["Am", "Stam\u00b7me", "nagt", "seit", "Tag", "und", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Blieb frisch genug das mark'ge Holz,", "tokens": ["Blieb", "frisch", "ge\u00b7nug", "das", "mark'\u00b7ge", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Frei durch Jahrtausende zu fahren.", "tokens": ["Frei", "durch", "Jahr\u00b7tau\u00b7sen\u00b7de", "zu", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "So ist es, wehe, schrecklich wahr,", "tokens": ["So", "ist", "es", ",", "we\u00b7he", ",", "schreck\u00b7lich", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mancher, wie zum starken Mast", "tokens": ["Da\u00df", "man\u00b7cher", ",", "wie", "zum", "star\u00b7ken", "Mast"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geschaffen, in der Zeit Gefahr", "tokens": ["Ge\u00b7schaf\u00b7fen", ",", "in", "der", "Zeit", "Ge\u00b7fahr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Glaubenssegel hat gebra\u00dft,", "tokens": ["Die", "Glau\u00b7bens\u00b7se\u00b7gel", "hat", "ge\u00b7bra\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun, d\u00fcrre S\u00e4ule, nackt und schwer", "tokens": ["Nun", ",", "d\u00fcr\u00b7re", "S\u00e4u\u00b7le", ",", "nackt", "und", "schwer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur krachend k\u00fcndet durch das Wehen,", "tokens": ["Nur", "kra\u00b7chend", "k\u00fcn\u00b7det", "durch", "das", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hier sei in Zweifels w\u00fcstem Meer", "tokens": ["Hier", "sei", "in", "Zwei\u00b7fels", "w\u00fcs\u00b7tem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein wuchtig Schiff am Untergehen!", "tokens": ["Ein", "wuch\u00b7tig", "Schiff", "am", "Un\u00b7ter\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O sende, Retter, deinen Blitz,", "tokens": ["O", "sen\u00b7de", ",", "Ret\u00b7ter", ",", "dei\u00b7nen", "Blitz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der ihm den frommen Hafen hellt,", "tokens": ["Der", "ihm", "den", "from\u00b7men", "Ha\u00b7fen", "hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo einst der starke Mast als Sitz", "tokens": ["Wo", "einst", "der", "star\u00b7ke", "Mast", "als", "Sitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pharuslampe sei gestellt;", "tokens": ["Der", "Pha\u00b7rus\u00b7lam\u00b7pe", "sei", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es tr\u00e4gt Gebirge ja dein Land,", "tokens": ["Es", "tr\u00e4gt", "Ge\u00b7bir\u00b7ge", "ja", "dein", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Zedern sich zu Zedern einen,", "tokens": ["Wo", "Ze\u00b7dern", "sich", "zu", "Ze\u00b7dern", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df nicht ein Sturmlicht den Verstand", "tokens": ["La\u00df", "nicht", "ein", "Sturm\u00b7licht", "den", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und einen Fluch die Kraft erscheinen!", "tokens": ["Und", "ei\u00b7nen", "Fluch", "die", "Kraft", "er\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON"