{"textgrid.poem.38775": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[was hast du mir denn, Leben, schon geg\u00f6nnet]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hast du mir denn, Leben, schon geg\u00f6nnet,", "tokens": ["Was", "hast", "du", "mir", "denn", ",", "Le\u00b7ben", ",", "schon", "ge\u00b7g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "ADV", "$,", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich als Gut dich theuer sollte sch\u00e4tzen?", "tokens": ["Da\u00df", "ich", "als", "Gut", "dich", "theu\u00b7er", "soll\u00b7te", "sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warst du ein gier'ger Dolch nicht im Verletzen", "tokens": ["Warst", "du", "ein", "gier'\u00b7ger", "Dolch", "nicht", "im", "Ver\u00b7let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Brust, die immerdar in Wunden brennet?", "tokens": ["Der", "Brust", ",", "die", "im\u00b7mer\u00b7dar", "in", "Wun\u00b7den", "bren\u00b7net", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der liebe dich, der dich noch nicht erkennet,", "tokens": ["Der", "lie\u00b7be", "dich", ",", "der", "dich", "noch", "nicht", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer blind unwissend l\u00fcstert deinen Sch\u00e4tzen:", "tokens": ["Wer", "blind", "un\u00b7wis\u00b7send", "l\u00fcs\u00b7tert", "dei\u00b7nen", "Sch\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Magst du nur Weh und Jammer auf mich hetzen,", "tokens": ["Magst", "du", "nur", "Weh", "und", "Jam\u00b7mer", "auf", "mich", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein wildes Heer, das uns zum Grab nachrennet,", "tokens": ["Dein", "wil\u00b7des", "Heer", ",", "das", "uns", "zum", "Grab", "nach\u00b7ren\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So kann ich auch als argen Feind dich hassen;", "tokens": ["So", "kann", "ich", "auch", "als", "ar\u00b7gen", "Feind", "dich", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KOUS", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur nicht mehr t\u00e4usche mit holdselgen Mienen,", "tokens": ["Nur", "nicht", "mehr", "t\u00e4u\u00b7sche", "mit", "hold\u00b7sel\u00b7gen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zeig mir dein Furien-Antlitz, Haar von Schlangen!", "tokens": ["Zeig", "mir", "dein", "Fu\u00b7ri\u00b7en\u00b7Ant\u00b7litz", ",", "Haar", "von", "Schlan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Davor wird nie mein starkes Herz erbangen:", "tokens": ["Da\u00b7vor", "wird", "nie", "mein", "star\u00b7kes", "Herz", "er\u00b7ban\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch da\u00df du mir als Liebe bist erschienen,", "tokens": ["Doch", "da\u00df", "du", "mir", "als", "Lie\u00b7be", "bist", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "KOUS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Trost, Schmerz, Trug, wei\u00df ich noch nicht zu fassen.", "tokens": ["Den", "Trost", ",", "Schmerz", ",", "Trug", ",", "wei\u00df", "ich", "noch", "nicht", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}