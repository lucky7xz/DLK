{"textgrid.poem.34462": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbvon meinen Br\u00fcsten leise schlich", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbvon meinen Br\u00fcsten leise schlich", "tokens": ["\u00bb", "von", "mei\u00b7nen", "Br\u00fcs\u00b7ten", "lei\u00b7se", "schlich"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dein Blick und stahl sich in die Nacht?", "tokens": ["dein", "Blick", "und", "stahl", "sich", "in", "die", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O sage, was bek\u00fcmmert dich,", "tokens": ["O", "sa\u00b7ge", ",", "was", "be\u00b7k\u00fcm\u00b7mert", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "woher die Thr\u00e4ne, unbewacht?\u00ab", "tokens": ["wo\u00b7her", "die", "Thr\u00e4\u00b7ne", ",", "un\u00b7be\u00b7wacht", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u2013 Du Weib, das mir ergeben sich", "tokens": ["\u2013", "Du", "Weib", ",", "das", "mir", "er\u00b7ge\u00b7ben", "sich"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und ruht in meiner H\u00e4nde Haft,", "tokens": ["und", "ruht", "in", "mei\u00b7ner", "H\u00e4n\u00b7de", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "o d\u00fcrft ich erst ersehnen dich,", "tokens": ["o", "d\u00fcrft", "ich", "erst", "er\u00b7seh\u00b7nen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VMFIN", "PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "voll zagend keuscher Leidenschaft!", "tokens": ["voll", "za\u00b7gend", "keu\u00b7scher", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sehne mich nach Fr\u00fchlingsthau,", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "Fr\u00fch\u00b7lingst\u00b7hau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zur\u00fcck nach scheuem Knaben-Sinn:", "tokens": ["zu\u00b7r\u00fcck", "nach", "scheu\u00b7em", "Kna\u00b7ben\u00b7Sinn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 dass ich mich nicht zu sagen trau,", "tokens": ["\u2013", "dass", "ich", "mich", "nicht", "zu", "sa\u00b7gen", "trau", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wovon ich heimlich selig bin.", "tokens": ["wo\u00b7von", "ich", "heim\u00b7lich", "se\u00b7lig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}