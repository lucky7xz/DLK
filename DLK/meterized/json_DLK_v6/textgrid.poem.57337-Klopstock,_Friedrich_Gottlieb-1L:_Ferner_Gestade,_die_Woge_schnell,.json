{"textgrid.poem.57337": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ferner Gestade, die Woge schnell,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ferner Gestade, die Woge schnell,", "tokens": ["Fer\u00b7ner", "Ge\u00b7sta\u00b7de", ",", "die", "Wo\u00b7ge", "schnell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dem Blicke gehellt bis zum Kiesel ist,", "tokens": ["Dem", "Bli\u00b7cke", "ge\u00b7hellt", "bis", "zum", "Kie\u00b7sel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das Geb\u00fcsch blinket er durch, oder wallt", "tokens": ["Das", "Ge\u00b7b\u00fcsch", "blin\u00b7ket", "er", "durch", ",", "o\u00b7der", "wallt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In die Luft, hohes Gew\u00f6lk duftend, der Strom;", "tokens": ["In", "die", "Luft", ",", "ho\u00b7hes", "Ge\u00b7w\u00f6lk", "duf\u00b7tend", ",", "der", "Strom", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Wirbelchen drehn mit ihm fort. So str\u00f6mt", "tokens": ["Wir\u00b7bel\u00b7chen", "drehn", "mit", "ihm", "fort", ".", "So", "str\u00f6mt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "CARD", "APPR", "PPER", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sprache, die, Hermann, dein Ursohn spricht.", "tokens": ["Die", "Spra\u00b7che", ",", "die", ",", "Her\u00b7mann", ",", "dein", "Ur\u00b7sohn", "spricht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "NE", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "(o auch du glichest dem Strom, Mann des Volks,", "tokens": ["(", "o", "auch", "du", "gli\u00b7chest", "dem", "Strom", ",", "Mann", "des", "Volks", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADV", "PPER", "VVFIN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da dir Roms steigender Damm lockert', und brach!)", "tokens": ["Da", "dir", "Roms", "stei\u00b7gen\u00b7der", "Damm", "lo\u00b7ckert'", ",", "und", "brach", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "NE", "ADJA", "NN", "VVFIN", "$,", "KON", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Tieferen Quellen entstr\u00f6met sie.", "tokens": ["Tie\u00b7fe\u00b7ren", "Quel\u00b7len", "ent\u00b7str\u00f6\u00b7met", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Erst wenige Zeit, da der eine Quell", "tokens": ["Erst", "we\u00b7ni\u00b7ge", "Zeit", ",", "da", "der", "ei\u00b7ne", "Quell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "KOUS", "ART", "ART", "NN"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch in Sand floss, sich verlor. S\u00e4umend jetzt,", "tokens": ["Noch", "in", "Sand", "floss", ",", "sich", "ver\u00b7lor", ".", "S\u00e4u\u00b7mend", "jetzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,", "PRF", "VVFIN", "$.", "NN", "ADV", "$,"], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und mit Eil hallte der jetzt aus dem Gekl\u00fcft;", "tokens": ["Und", "mit", "Eil", "hall\u00b7te", "der", "jetzt", "aus", "dem", "Ge\u00b7kl\u00fcft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+---+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Aber er rann in den Kies. Nun kam", "tokens": ["A\u00b7ber", "er", "rann", "in", "den", "Kies", ".", "Nun", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Gl\u00fccklichen Einer, und leitet' ihn", "tokens": ["Der", "Gl\u00fcck\u00b7li\u00b7chen", "Ei\u00b7ner", ",", "und", "lei\u00b7tet'", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "$,", "KON", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In den Strom. Schatten umher pflanzt man schon", "tokens": ["In", "den", "Strom", ".", "Schat\u00b7ten", "um\u00b7her", "pflanzt", "man", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN", "PTKVZ", "VVFIN", "PIS", "ADV"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "An der Kluft; weilen da schon Wanderer gern,", "tokens": ["An", "der", "Kluft", ";", "wei\u00b7len", "da", "schon", "Wan\u00b7de\u00b7rer", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "VVFIN", "ADV", "ADV", "NN", "ADV", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Stehen, und sinnen: \u00bbVersiegt vielleicht", "tokens": ["Ste\u00b7hen", ",", "und", "sin\u00b7nen", ":", "\u00bb", "Ver\u00b7siegt", "viel\u00b7leicht"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "VVINF", "$.", "$(", "VVFIN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ein \u00e4hnlicher Quell in dem Sand' auch uns?", "tokens": ["Ein", "\u00e4hn\u00b7li\u00b7cher", "Quell", "in", "dem", "Sand'", "auch", "uns", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und gebricht Leitung ihm nur?\u00ab Doch verweht", "tokens": ["Und", "ge\u00b7bricht", "Lei\u00b7tung", "ihm", "nur", "?", "\u00ab", "Doch", "ver\u00b7weht"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VVFIN", "NN", "PPER", "ADV", "$.", "$(", "KON", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wird ihr Wunsch; Doppelgekling bleibt ihr Gesang,", "tokens": ["Wird", "ihr", "Wunsch", ";", "Dop\u00b7pel\u00b7ge\u00b7kling", "bleibt", "ihr", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Sage verbreitet, es schweb' umher,", "tokens": ["Sa\u00b7ge", "ver\u00b7brei\u00b7tet", ",", "es", "schweb'", "um\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie Griechengestalten, bey Nacht am Quell;", "tokens": ["Wie", "Grie\u00b7chen\u00b7ge\u00b7stal\u00b7ten", ",", "bey", "Nacht", "am", "Quell", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und behorcht werde sein Fall, werd' es, wenn", "tokens": ["Und", "be\u00b7horcht", "wer\u00b7de", "sein", "Fall", ",", "werd'", "es", ",", "wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "$,", "KOUS"], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Erguss t\u00f6net Verein, Gegenklang rauscht.", "tokens": ["Der", "Er\u00b7guss", "t\u00f6\u00b7net", "Ver\u00b7ein", ",", "Ge\u00b7gen\u00b7klang", "rauscht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Der ist geheimere Kunst, der trift's", "tokens": ["Der", "ist", "ge\u00b7hei\u00b7me\u00b7re", "Kunst", ",", "der", "trift's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Weise, wie Orpheus der Zelt' es traf.", "tokens": ["Zur", "Wei\u00b7se", ",", "wie", "Or\u00b7pheus", "der", "Zelt'", "es", "traf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NE", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dem Verein komt nur der Wald: aber t\u00f6nt", "tokens": ["Dem", "Ver\u00b7ein", "komt", "nur", "der", "Wald", ":", "a\u00b7ber", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Genoss auch in das Lied; wandelt der Hain.", "tokens": ["Ge\u00b7noss", "auch", "in", "das", "Lied", ";", "wan\u00b7delt", "der", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}}}}}