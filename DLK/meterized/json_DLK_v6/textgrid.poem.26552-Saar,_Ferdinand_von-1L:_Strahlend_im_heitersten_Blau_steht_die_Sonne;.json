{"textgrid.poem.26552": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Strahlend im heitersten Blau steht die Sonne;", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strahlend im heitersten Blau steht die Sonne;", "tokens": ["Strah\u00b7lend", "im", "hei\u00b7ters\u00b7ten", "Blau", "steht", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Aber fr\u00fch noch ist es im Lenz", "tokens": ["A\u00b7ber", "fr\u00fch", "noch", "ist", "es", "im", "Lenz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und eisige L\u00fcfte hauchen noch", "tokens": ["Und", "ei\u00b7si\u00b7ge", "L\u00fcf\u00b7te", "hau\u00b7chen", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von den Bergen her\u00fcber,", "tokens": ["Von", "den", "Ber\u00b7gen", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wo hartn\u00e4ckig der Winter sich fest gefroren", "tokens": ["Wo", "hart\u00b7n\u00e4\u00b7ckig", "der", "Win\u00b7ter", "sich", "fest", "ge\u00b7fro\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "ADJD", "VVPP"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In tannenumdunkelten Kl\u00fcften.", "tokens": ["In", "tan\u00b7nen\u00b7um\u00b7dun\u00b7kel\u00b7ten", "Kl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Dennoch vom erstarrten Blachfeld", "tokens": ["Den\u00b7noch", "vom", "er\u00b7starr\u00b7ten", "Blach\u00b7feld"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Schwingt sich mit k\u00e4mpfendem Fl\u00fcgel", "tokens": ["Schwingt", "sich", "mit", "k\u00e4mp\u00b7fen\u00b7dem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die Lerche empor,", "tokens": ["Die", "Ler\u00b7che", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Hin und her geschleudert vom Sturm,", "tokens": ["Hin", "und", "her", "ge\u00b7schleu\u00b7dert", "vom", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Aber die jauchzende Brust umfunkelt", "tokens": ["A\u00b7ber", "die", "jauch\u00b7zen\u00b7de", "Brust", "um\u00b7fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Vom ewigen Licht \u2013", "tokens": ["Vom", "e\u00b7wi\u00b7gen", "Licht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Schwing' dich ihr nach, du mein gefl\u00fcgeltes Lied!", "tokens": ["Schwing'", "dich", "ihr", "nach", ",", "du", "mein", "ge\u00b7fl\u00fc\u00b7gel\u00b7tes", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}