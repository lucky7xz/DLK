{"dta.poem.3341": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r G*.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Freund! liebster G*! ist jemals wahr gewesen,", "tokens": ["Freund", "!", "liebs\u00b7ter", "G", "*", "!", "ist", "je\u00b7mals", "wahr", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "XY", "XY", "$.", "VAFIN", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was wir von Gnid, Cytherens Lustsitz, lesen?", "tokens": ["Was", "wir", "von", "Gnid", ",", "Cy\u00b7the\u00b7rens", "Lust\u00b7sitz", ",", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "$,", "NE", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Flora stets, im Schoos des Fr\u00fchlings lacht,", "tokens": ["Wo", "Flo\u00b7ra", "stets", ",", "im", "Schoos", "des", "Fr\u00fch\u00b7lings", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "$,", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und alles liebt, und Liebe gl\u00fccklich macht?", "tokens": ["Und", "al\u00b7les", "liebt", ",", "und", "Lie\u00b7be", "gl\u00fcck\u00b7lich", "macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo reine Lust nie unter bittern Thr\u00e4nen,", "tokens": ["Wo", "rei\u00b7ne", "Lust", "nie", "un\u00b7ter", "bit\u00b7tern", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Wollust herrscht, stets fern von tr\u00e4gem G\u00e4hnen;", "tokens": ["Und", "Wol\u00b7lust", "herrscht", ",", "stets", "fern", "von", "tr\u00e4\u00b7gem", "G\u00e4h\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nichts Ehre macht, als einer Hirtinn Ku\u00df,", "tokens": ["Nichts", "Eh\u00b7re", "macht", ",", "als", "ei\u00b7ner", "Hir\u00b7tinn", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wer nicht liebt, allein err\u00f6then mu\u00df?", "tokens": ["Und", "wer", "nicht", "liebt", ",", "al\u00b7lein", "er\u00b7r\u00f6\u00b7then", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wo \u00fcberall die V\u00f6gel br\u00fcnstig schwirren,", "tokens": ["Wo", "\u00fc\u00b7be\u00b7rall", "die", "V\u00f6\u00b7gel", "br\u00fcns\u00b7tig", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf iedem Baum die Tauben schn\u00e4belnd girren;", "tokens": ["Auf", "ie\u00b7dem", "Baum", "die", "Tau\u00b7ben", "schn\u00e4\u00b7belnd", "gir\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und ieder Busch, am schattigten Cephy\u00df,", "tokens": ["Und", "ie\u00b7der", "Busch", ",", "am", "schat\u00b7tig\u00b7ten", "Ce\u00b7phy\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und ieder Busch, voll holder Finsterni\u00df,", "tokens": ["Und", "ie\u00b7der", "Busch", ",", "voll", "hol\u00b7der", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Jm stillen Thal und auf bebl\u00fchmter H\u00f6he,", "tokens": ["Jm", "stil\u00b7len", "Thal", "und", "auf", "be\u00b7bl\u00fchm\u00b7ter", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Von Liebe schallt, und niemals von der Ehe?", "tokens": ["Von", "Lie\u00b7be", "schallt", ",", "und", "nie\u00b7mals", "von", "der", "E\u00b7he", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}