{"textgrid.poem.47295": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ein verzaubert Reich ist die Natur,", "tokens": ["Ach", ",", "ein", "ver\u00b7zau\u00b7bert", "Reich", "ist", "die", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "VVFIN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stets hoffend, da\u00df man sie des Banns entbinde.", "tokens": ["Stets", "hof\u00b7fend", ",", "da\u00df", "man", "sie", "des", "Banns", "ent\u00b7bin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Fr\u00fchling ahnt sie der Erl\u00f6sung Spur;", "tokens": ["Im", "Fr\u00fch\u00b7ling", "ahnt", "sie", "der", "Er\u00b7l\u00f6\u00b7sung", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie hofft, da\u00df ganz in Glanz und Duft sie schwinde.", "tokens": ["Sie", "hofft", ",", "da\u00df", "ganz", "in", "Glanz", "und", "Duft", "sie", "schwin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der s\u00fc\u00dfe Todesschauer kam und fuhr", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "To\u00b7des\u00b7schau\u00b7er", "kam", "und", "fuhr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vor\u00fcber, wirkungslos im Fr\u00fchlingswinde.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "wir\u00b7kungs\u00b7los", "im", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O Liebe, komm! in deinen Blicken nur", "tokens": ["O", "Lie\u00b7be", ",", "komm", "!", "in", "dei\u00b7nen", "Bli\u00b7cken", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ist Hoffnung, da\u00df die Welt in Feuer schwinde.", "tokens": ["Ist", "Hoff\u00b7nung", ",", "da\u00df", "die", "Welt", "in", "Feu\u00b7er", "schwin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}