{"dta.poem.10402": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.71", "af:0.28"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erde war so lange geizig,", "tokens": ["Die", "Er\u00b7de", "war", "so", "lan\u00b7ge", "gei\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam der Mai, und sie ward spendabel,", "tokens": ["Da", "kam", "der", "Mai", ",", "und", "sie", "ward", "spen\u00b7da\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alles lacht, und jauchzt, und freut sich,", "tokens": ["Und", "al\u00b7les", "lacht", ",", "und", "jauchzt", ",", "und", "freut", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber bin nicht zu lachen kapabel.", "tokens": ["Ich", "a\u00b7ber", "bin", "nicht", "zu", "la\u00b7chen", "ka\u00b7pa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Blumen sprie\u00dfen, die Gl\u00f6cklein schallen,", "tokens": ["Die", "Blu\u00b7men", "sprie\u00b7\u00dfen", ",", "die", "Gl\u00f6c\u00b7klein", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die V\u00f6gel sprechen wie in der Fabel;", "tokens": ["Die", "V\u00f6\u00b7gel", "spre\u00b7chen", "wie", "in", "der", "Fa\u00b7bel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mir aber will das Gespr\u00e4ch nicht gefallen,", "tokens": ["Mir", "a\u00b7ber", "will", "das", "Ge\u00b7spr\u00e4ch", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich finde Alles miserabel.", "tokens": ["Ich", "fin\u00b7de", "Al\u00b7les", "mi\u00b7se\u00b7ra\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Das Menschenvolk mich ennuyiret,", "tokens": ["Das", "Men\u00b7schen\u00b7volk", "mich", "en\u00b7nuy\u00b7i\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sogar der Freund, der sonst passabel; \u2014", "tokens": ["So\u00b7gar", "der", "Freund", ",", "der", "sonst", "pas\u00b7sa\u00b7bel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das k\u00f6mmt, weil man Madame tituliret", "tokens": ["Das", "k\u00f6mmt", ",", "weil", "man", "Ma\u00b7da\u00b7me", "ti\u00b7tu\u00b7li\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PIS", "FM", "FM"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mein s\u00fc\u00dfes Liebchen, so s\u00fc\u00df und aimabel.", "tokens": ["Mein", "s\u00fc\u00b7\u00dfes", "Lieb\u00b7chen", ",", "so", "s\u00fc\u00df", "und", "ai\u00b7ma\u00b7bel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}