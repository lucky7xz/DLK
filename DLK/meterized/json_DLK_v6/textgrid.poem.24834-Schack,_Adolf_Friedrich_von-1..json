{"textgrid.poem.24834": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df still die Thr\u00e4ne rinnen", "tokens": ["La\u00df", "still", "die", "Thr\u00e4\u00b7ne", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf deinen Heimatherd!", "tokens": ["Auf", "dei\u00b7nen", "Hei\u00b7ma\u00b7therd", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Genesest du nicht innen,", "tokens": ["Ge\u00b7ne\u00b7sest", "du", "nicht", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ist das Au\u00dfen wert?", "tokens": ["Was", "ist", "das", "Au\u00b7\u00dfen", "wert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vergebens in die Weite", "tokens": ["Ver\u00b7ge\u00b7bens", "in", "die", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sp\u00e4ht hoffend dein Gesicht;", "tokens": ["Sp\u00e4ht", "hof\u00b7fend", "dein", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein d\u00fcsteres Geleite,", "tokens": ["Dein", "d\u00fcs\u00b7te\u00b7res", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Trauer, l\u00e4\u00dft dich nicht.", "tokens": ["Die", "Trau\u00b7er", ",", "l\u00e4\u00dft", "dich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ob L\u00e4nder auch und Meere", "tokens": ["Ob", "L\u00e4n\u00b7der", "auch", "und", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Ferne dir enth\u00fcllt:", "tokens": ["Die", "Fer\u00b7ne", "dir", "ent\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deiner Brust die Leere", "tokens": ["In", "dei\u00b7ner", "Brust", "die", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird nimmer ausgef\u00fcllt.", "tokens": ["Wird", "nim\u00b7mer", "aus\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Durch alle Zonen fl\u00fcchte,", "tokens": ["Durch", "al\u00b7le", "Zo\u00b7nen", "fl\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchschweife jede Flur:", "tokens": ["Durch\u00b7schwei\u00b7fe", "je\u00b7de", "Flur", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du siehst verdorrte Fr\u00fcchte", "tokens": ["Du", "siehst", "ver\u00b7dorr\u00b7te", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und welke Bl\u00fcten nur.", "tokens": ["Und", "wel\u00b7ke", "Bl\u00fc\u00b7ten", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Nebeldunst, ein gelber,", "tokens": ["Ein", "Ne\u00b7bel\u00b7dunst", ",", "ein", "gel\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umh\u00fcllt das Himmelszelt,", "tokens": ["Um\u00b7h\u00fcllt", "das", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und finster, wie du selber,", "tokens": ["Und", "fins\u00b7ter", ",", "wie", "du", "sel\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist um dich her die Welt.", "tokens": ["Ist", "um", "dich", "her", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "APZR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}