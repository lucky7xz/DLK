{"dta.poem.3004": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Anschauung, wo sie fehlt, mag etwa Geist ersetzen;", "tokens": ["An\u00b7schau\u00b7ung", ",", "wo", "sie", "fehlt", ",", "mag", "et\u00b7wa", "Geist", "er\u00b7set\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei Geistes Mangel wird Anschauung nie dich letzen.", "tokens": ["Bei", "Geis\u00b7tes", "Man\u00b7gel", "wird", "An\u00b7schau\u00b7ung", "nie", "dich", "let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch nur wo Geist sich h\u00e4lt zusammen mit Anschauung,", "tokens": ["Doch", "nur", "wo", "Geist", "sich", "h\u00e4lt", "zu\u00b7sam\u00b7men", "mit", "An\u00b7schau\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWAV", "NN", "PRF", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entsteht vor dir die Welt in gl\u00e4nzender Erbauung.", "tokens": ["Ent\u00b7steht", "vor", "dir", "die", "Welt", "in", "gl\u00e4n\u00b7zen\u00b7der", "Er\u00b7bau\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN