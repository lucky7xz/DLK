{"textgrid.poem.62539": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Sonntage Abends", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sinkt am Himmelsbogen,", "tokens": ["Die", "Son\u00b7ne", "sinkt", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und strahlenreich k\u00f6mmt durch die Nacht", "tokens": ["Und", "strah\u00b7len\u00b7reich", "k\u00f6mmt", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sterne Heer hinaufgezogen", "tokens": ["Der", "Ster\u00b7ne", "Heer", "hin\u00b7auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In tausendf\u00e4lt'ger Sonnenpracht.", "tokens": ["In", "tau\u00b7send\u00b7f\u00e4lt'\u00b7ger", "Son\u00b7nen\u00b7pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df, Herr! mich dieses Bild erquicken,", "tokens": ["La\u00df", ",", "Herr", "!", "mich", "die\u00b7ses", "Bild", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$.", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn meine Lebenssonne sinkt;", "tokens": ["Wenn", "mei\u00b7ne", "Le\u00b7bens\u00b7son\u00b7ne", "sinkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df mich die Herrlichkeit erblicken,", "tokens": ["La\u00df", "mich", "die", "Herr\u00b7lich\u00b7keit", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die aus der Nacht des Todes winkt.", "tokens": ["Die", "aus", "der", "Nacht", "des", "To\u00b7des", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}