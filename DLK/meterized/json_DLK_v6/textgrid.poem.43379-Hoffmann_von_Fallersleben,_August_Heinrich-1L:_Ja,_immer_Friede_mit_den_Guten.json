{"textgrid.poem.43379": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, immer Friede mit den Guten", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, immer Friede mit den Guten", "tokens": ["Ja", ",", "im\u00b7mer", "Frie\u00b7de", "mit", "den", "Gu\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit den B\u00f6sen immer Krieg!", "tokens": ["Und", "mit", "den", "B\u00f6\u00b7sen", "im\u00b7mer", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herr, f\u00fchr' uns in der H\u00f6lle Gluten,", "tokens": ["Herr", ",", "f\u00fchr'", "uns", "in", "der", "H\u00f6l\u00b7le", "Glu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nur immer f\u00fchr' uns, Herr, zum Sieg!", "tokens": ["Nur", "im\u00b7mer", "f\u00fchr'", "uns", ",", "Herr", ",", "zum", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df Recht und Wahrheit nicht verderben", "tokens": ["La\u00df", "Recht", "und", "Wahr\u00b7heit", "nicht", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fallen durch der Feinde Hand,", "tokens": ["Und", "fal\u00b7len", "durch", "der", "Fein\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df lieber uns im Kampfe sterben", "tokens": ["La\u00df", "lie\u00b7ber", "uns", "im", "Kamp\u00b7fe", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rette du das Vaterland!", "tokens": ["Und", "ret\u00b7te", "du", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}