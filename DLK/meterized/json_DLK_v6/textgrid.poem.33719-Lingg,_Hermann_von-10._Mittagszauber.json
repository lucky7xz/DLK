{"textgrid.poem.33719": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "10. Mittagszauber", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Wonne zitternd hat die Mittagsschw\u00fcle", "tokens": ["Vor", "Won\u00b7ne", "zit\u00b7ternd", "hat", "die", "Mit\u00b7tags\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Tal und H\u00f6h' in Stille sich gebreitet;", "tokens": ["Auf", "Tal", "und", "H\u00f6h'", "in", "Stil\u00b7le", "sich", "ge\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man h\u00f6rt nur, wie der Specht im Tannicht scheitet,", "tokens": ["Man", "h\u00f6rt", "nur", ",", "wie", "der", "Specht", "im", "Tan\u00b7nicht", "schei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie durchs Tobel rauscht die S\u00e4gem\u00fchle.", "tokens": ["Und", "wie", "durchs", "To\u00b7bel", "rauscht", "die", "S\u00e4\u00b7ge\u00b7m\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und schneller flie\u00dft der Bach, als such' er K\u00fchle.", "tokens": ["Und", "schnel\u00b7ler", "flie\u00dft", "der", "Bach", ",", "als", "such'", "er", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blume schaut ihm durstig nach und spreitet", "tokens": ["Die", "Blu\u00b7me", "schaut", "ihm", "durs\u00b7tig", "nach", "und", "sprei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Bl\u00e4tter sehnend aus, und trunken gleitet", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "seh\u00b7nend", "aus", ",", "und", "trun\u00b7ken", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Schmetterling vom seidnen Bl\u00fctenpf\u00fchle.", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", "vom", "seid\u00b7nen", "Bl\u00fc\u00b7ten\u00b7pf\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Ufer sucht der F\u00e4hrmann sich im Nachen", "tokens": ["Am", "U\u00b7fer", "sucht", "der", "F\u00e4hr\u00b7mann", "sich", "im", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Weidenlaub ein Sonnendach zu zimmern", "tokens": ["Aus", "Wei\u00b7den\u00b7laub", "ein", "Son\u00b7nen\u00b7dach", "zu", "zim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieht ins Wasser, was die Wolken machen.", "tokens": ["Und", "sieht", "ins", "Was\u00b7ser", ",", "was", "die", "Wol\u00b7ken", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt ist die Zeit, wo oft im Schilf ein Wimmern", "tokens": ["Jetzt", "ist", "die", "Zeit", ",", "wo", "oft", "im", "Schilf", "ein", "Wim\u00b7mern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Fischer weckt; der J\u00e4ger h\u00f6rt ein Lachen,", "tokens": ["Den", "Fi\u00b7scher", "weckt", ";", "der", "J\u00e4\u00b7ger", "h\u00f6rt", "ein", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und golden sieht der Hirt die Felsen schimmern.", "tokens": ["Und", "gol\u00b7den", "sieht", "der", "Hirt", "die", "Fel\u00b7sen", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}