{"dta.poem.1196": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem/ Melinde/ dir mein Seufftzen kund gemacht", "tokens": ["Nach\u00b7dem", "/", "Me\u00b7lin\u00b7de", "/", "dir", "mein", "Seufft\u00b7zen", "kund", "ge\u00b7macht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "NE", "$(", "PPER", "PPOSAT", "NN", "PTKVZ", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Theil der herben Schmertzen/", "tokens": ["Ein", "Theil", "der", "her\u00b7ben", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darein mich deine Zier und meine Liebe bracht/", "tokens": ["Da\u00b7rein", "mich", "dei\u00b7ne", "Zier", "und", "mei\u00b7ne", "Lie\u00b7be", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und du noch thr\u00e4nen siehst der Augen dunckle Kertzen/", "tokens": ["Und", "du", "noch", "thr\u00e4\u00b7nen", "siehst", "der", "Au\u00b7gen", "dunck\u00b7le", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So dencke/ da\u00df noch mehr verborgen ist im Hertzen.", "tokens": ["So", "den\u00b7cke", "/", "da\u00df", "noch", "mehr", "ver\u00b7bor\u00b7gen", "ist", "im", "Hert\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "ADV", "ADV", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Seufftzer haben dir alleine kund gethan/", "tokens": ["Die", "Seufft\u00b7zer", "ha\u00b7ben", "dir", "al\u00b7lei\u00b7ne", "kund", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie Lieb und Furcht mich plagen:", "tokens": ["Wie", "Lieb", "und", "Furcht", "mich", "pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wilt du nicht f\u00fcr bekandt di\u00df Zeugnis nehmen an/", "tokens": ["Wilt", "du", "nicht", "f\u00fcr", "be\u00b7kandt", "di\u00df", "Zeug\u00b7nis", "neh\u00b7men", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "ADJD", "PDS", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Thr\u00e4nen werden dir in ihrer Sprache sagen/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "wer\u00b7den", "dir", "in", "ih\u00b7rer", "Spra\u00b7che", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df deine Grausamkeit mich wird zu Grabe tragen.", "tokens": ["Da\u00df", "dei\u00b7ne", "Grau\u00b7sam\u00b7keit", "mich", "wird", "zu", "Gra\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}