{"textgrid.poem.45165": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[es g\u00e4be kein verkanntes Genie?]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es g\u00e4be kein verkanntes Genie?", "tokens": ["Es", "g\u00e4\u00b7be", "kein", "ver\u00b7kann\u00b7tes", "Ge\u00b7nie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In unserer Zeit zum wenigsten nie?", "tokens": ["In", "un\u00b7se\u00b7rer", "Zeit", "zum", "we\u00b7nigs\u00b7ten", "nie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "PIS", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das mag dir glauben, wems beliebt;", "tokens": ["Das", "mag", "dir", "glau\u00b7ben", ",", "wems", "be\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,", "PWS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Solangs gepriesene Dummk\u00f6pfe gibt,", "tokens": ["So\u00b7langs", "ge\u00b7prie\u00b7se\u00b7ne", "Dumm\u00b7k\u00f6p\u00b7fe", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gibts auch verkanntes Genie.", "tokens": ["Gibts", "auch", "ver\u00b7kann\u00b7tes", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}