{"dta.poem.18051": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tanne kam in Ubermuth,", "tokens": ["Die", "Tan\u00b7ne", "kam", "in", "U\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie noch seithero mancher thut,", "tokens": ["Wie", "noch", "sei\u00b7the\u00b7ro", "man\u00b7cher", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der \u00f6fters dessen mu\u00df entgelten.", "tokens": ["Der", "\u00f6f\u00b7ters", "des\u00b7sen", "mu\u00df", "ent\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie fieng voll Stoltzes an, den Dornstrauch auszuschelten,", "tokens": ["Sie", "fi\u00b7eng", "voll", "Stolt\u00b7zes", "an", ",", "den", "Dorn\u00b7strauch", "aus\u00b7zu\u00b7schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Sie sprach: ich bin gantz lang und breit;", "tokens": ["Sie", "sprach", ":", "ich", "bin", "gantz", "lang", "und", "breit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Aesten bin ich wohl bekleidt.", "tokens": ["Mit", "A\u00b7es\u00b7ten", "bin", "ich", "wohl", "be\u00b7kleidt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Gr\u00fcn ist die Kleidung meiner Zweigen,", "tokens": ["Gr\u00fcn", "ist", "die", "Klei\u00b7dung", "mei\u00b7ner", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Den Wipfel sieht man Luft an steigen.", "tokens": ["Den", "Wip\u00b7fel", "sieht", "man", "Luft", "an", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mich lobt die Frau und auch ihr Mann.", "tokens": ["Mich", "lobt", "die", "Frau", "und", "auch", "ihr", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ohn\u2019 alles Lob sieht man dich an.", "tokens": ["Ohn'", "al\u00b7les", "Lob", "sieht", "man", "dich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "PRF", "PTKVZ", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Du bist sonst zu nichts besserm gut,", "tokens": ["Du", "bist", "sonst", "zu", "nichts", "bes\u00b7serm", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "PIS", "ADJD", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Als in ein Feuer f\u00fcr die Glut.", "tokens": ["Als", "in", "ein", "Feu\u00b7er", "f\u00fcr", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wer dich ber\u00fchrt der wird bald wund.", "tokens": ["Wer", "dich", "be\u00b7r\u00fchrt", "der", "wird", "bald", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dein Stachel ist gar ungesund,", "tokens": ["Dein", "Sta\u00b7chel", "ist", "gar", "un\u00b7ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Versehrt er eines Menschen Leib.", "tokens": ["Ver\u00b7sehrt", "er", "ei\u00b7nes", "Men\u00b7schen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dich ha\u00dft der Mann, und auch sein Weib.", "tokens": ["Dich", "ha\u00dft", "der", "Mann", ",", "und", "auch", "sein", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Noch manch ver\u00e4chtlich Wort flo\u00df ihr vom Munde,", "tokens": ["Noch", "manch", "ver\u00b7\u00e4cht\u00b7lich", "Wort", "flo\u00df", "ihr", "vom", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und sehet, in derselben Stunde,", "tokens": ["Und", "se\u00b7het", ",", "in", "der\u00b7sel\u00b7ben", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "K\u00f6mmt unverwarnt ein Zimmermann,", "tokens": ["K\u00f6mmt", "un\u00b7ver\u00b7warnt", "ein", "Zim\u00b7mer\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mit blanken Sagen, Aext\u2019 und Beilen,", "tokens": ["Mit", "blan\u00b7ken", "Sa\u00b7gen", ",", "A\u00b7ext'", "und", "Bei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Und scharfgeschliffnen harten Keilen.", "tokens": ["Und", "scharf\u00b7ge\u00b7schliff\u00b7nen", "har\u00b7ten", "Kei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Er braucht den starken Arm, und streckt sie auf den Plan", "tokens": ["Er", "braucht", "den", "star\u00b7ken", "Arm", ",", "und", "streckt", "sie", "auf", "den", "Plan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Dornstrauch stuhnd gantz sicher in dem Porte.", "tokens": ["Der", "Dorn\u00b7strauch", "stuhnd", "gantz", "si\u00b7cher", "in", "dem", "Por\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Derselbe sagt hiern\u00e4chst zur Tanne diese Worte:", "tokens": ["Der\u00b7sel\u00b7be", "sagt", "hier\u00b7n\u00e4chst", "zur", "Tan\u00b7ne", "die\u00b7se", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "O wie bist du gefallen, stoltzer Baum!", "tokens": ["O", "wie", "bist", "du", "ge\u00b7fal\u00b7len", ",", "stolt\u00b7zer", "Baum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Wie bald, wie tief, von welcher H\u00f6he!", "tokens": ["Wie", "bald", ",", "wie", "tief", ",", "von", "wel\u00b7cher", "H\u00f6\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADJD", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da ich verachtester noch aufrecht stehe.", "tokens": ["Da", "ich", "ver\u00b7ach\u00b7tes\u00b7ter", "noch", "auf\u00b7recht", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Dein Schmuck und deine W\u00fcrdigkeit,", "tokens": ["Dein", "Schmuck", "und", "dei\u00b7ne", "W\u00fcr\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Dieselben thaten dir dies Leid.", "tokens": ["Die\u00b7sel\u00b7ben", "tha\u00b7ten", "dir", "dies", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die Sch\u00f6nheit ists, was dir geschadet hat.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ists", ",", "was", "dir", "ge\u00b7scha\u00b7det", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Nun ist dein Ruhm zu Boden und schachmatt. ", "tokens": ["Nun", "ist", "dein", "Ruhm", "zu", "Bo\u00b7den", "und", "schach\u00b7matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Wovon du dachtest zu genesen, ", "tokens": ["Wo\u00b7von", "du", "dach\u00b7test", "zu", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Dasselbe ist dein Tod gewesen. ", "tokens": ["Das\u00b7sel\u00b7be", "ist", "dein", "Tod", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}