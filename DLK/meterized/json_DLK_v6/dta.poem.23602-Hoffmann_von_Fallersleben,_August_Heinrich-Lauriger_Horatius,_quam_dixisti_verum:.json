{"dta.poem.23602": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lauriger Horatius, quam dixisti  \n verum:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr m\u00fcsst durch alle Schulen wandern", "tokens": ["Ihr", "m\u00fcsst", "durch", "al\u00b7le", "Schu\u00b7len", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schon von Kindesbeinen an,", "tokens": ["Und", "schon", "von", "Kin\u00b7des\u00b7bei\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Lehrer zu dem andern,", "tokens": ["Von", "ei\u00b7nem", "Leh\u00b7rer", "zu", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu lernen was man lernen kann.", "tokens": ["Zu", "ler\u00b7nen", "was", "man", "ler\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PWS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr m\u00fcsset immerfort studieren,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "im\u00b7mer\u00b7fort", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das halbe liebe Leben lang,", "tokens": ["Das", "hal\u00b7be", "lie\u00b7be", "Le\u00b7ben", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr m\u00fcsset zeitig euch dressieren", "tokens": ["Ihr", "m\u00fcs\u00b7set", "zei\u00b7tig", "euch", "dres\u00b7sie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einen schulgerechten Zwang.", "tokens": ["In", "ei\u00b7nen", "schul\u00b7ge\u00b7rech\u00b7ten", "Zwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr m\u00fcsset Pr\u00fcfungen bestehen,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "Pr\u00fc\u00b7fun\u00b7gen", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die selbst ein Hiob kaum bestand,", "tokens": ["Die", "selbst", "ein", "Hiob", "kaum", "be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NE", "ADV", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und dann noch bitten, betteln, flehen,", "tokens": ["Und", "dann", "noch", "bit\u00b7ten", ",", "bet\u00b7teln", ",", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$,", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als suchtet ihr's gelobte Land.", "tokens": ["Als", "such\u00b7tet", "ih\u00b7r's", "ge\u00b7lob\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was ist denn euer Ziel auf Erden", "tokens": ["Was", "ist", "denn", "eu\u00b7er", "Ziel", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr soviel Kr\u00e4fte, Geld und Zeit?", "tokens": ["F\u00fcr", "so\u00b7viel", "Kr\u00e4f\u00b7te", ",", "Geld", "und", "Zeit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr wollet nur Bedienten werden", "tokens": ["Ihr", "wol\u00b7let", "nur", "Be\u00b7dien\u00b7ten", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleiben bis in Ewigkeit.", "tokens": ["Und", "blei\u00b7ben", "bis", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}