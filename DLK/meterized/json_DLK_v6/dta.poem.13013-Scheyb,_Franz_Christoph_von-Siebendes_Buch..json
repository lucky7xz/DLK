{"dta.poem.13013": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Balb stund\u2019 ich auf die Zehn und hielte mich erh\u00f6ht;", "tokens": ["Balb", "stund'", "ich", "auf", "die", "Zehn", "und", "hiel\u00b7te", "mich", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "CARD", "KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald hatt\u2019 ich mich dahin, und bald hieher gedreht", "tokens": ["Bald", "hatt'", "ich", "mich", "da\u00b7hin", ",", "und", "bald", "hie\u00b7her", "ge\u00b7dreht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PAV", "$,", "KON", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "15Aus vielerley Gespr\u00e4ch nur eines zu vernehmen;", "tokens": ["vie\u00b7ler\u00b7ley", "Ge\u00b7spr\u00e4ch", "nur", "ei\u00b7nes", "zu", "ver\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Umsonst: ich mu\u00dfte mich zu der Geduld bequemen:", "tokens": ["Um\u00b7sonst", ":", "ich", "mu\u00df\u00b7te", "mich", "zu", "der", "Ge\u00b7duld", "be\u00b7que\u00b7men", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bis eine graue Frau die Stimme sehr erhob", "tokens": ["Bis", "ei\u00b7ne", "grau\u00b7e", "Frau", "die", "Stim\u00b7me", "sehr", "er\u00b7hob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sagte: \u201eGibt man dann der Tugend gar kein Lob", "tokens": ["Und", "sag\u00b7te", ":", "\u201e", "Gibt", "man", "dann", "der", "Tu\u00b7gend", "gar", "kein", "Lob"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "PIS", "ADV", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edie mehr als andere mit M\u00fche, Sorg und Kr\u00e4ften,", "tokens": ["\u201e", "die", "mehr", "als", "an\u00b7de\u00b7re", "mit", "M\u00fc\u00b7he", ",", "Sorg", "und", "Kr\u00e4f\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "KOKOM", "PIS", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "20\u201d(Ich rede von dem Flei\u00df:) in Staats- und Kriegs-Gesch\u00e4fften", "tokens": ["\"", "(", "Ich", "re\u00b7de", "von", "dem", "Flei\u00df", ":)", "in", "Staats", "und", "Kriegs\u00b7Ge\u00b7sch\u00e4ff\u00b7ten"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "emoticon", "word", "word", "word", "word"], "pos": ["$(", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$(", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201esich angewendet hat? Kaum aber redte die,", "tokens": ["\u201e", "sich", "an\u00b7ge\u00b7wen\u00b7det", "hat", "?", "Kaum", "a\u00b7ber", "red\u00b7te", "die", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "VVPP", "VAFIN", "$.", "ADV", "ADV", "VVFIN", "ART", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So fiel die ", "tokens": ["So", "fiel", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "\u201ewann meine Munterkeit, mein Feuer dich verliessen?", "tokens": ["\u201e", "wann", "mei\u00b7ne", "Mun\u00b7ter\u00b7keit", ",", "mein", "Feu\u00b7er", "dich", "ver\u00b7lies\u00b7sen", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201egesezt \u201e, sprach Eine drauf, ich wollte mich entschliessen,", "tokens": ["\u201e", "ge\u00b7sezt", "\u201e", ",", "sprach", "Ei\u00b7ne", "drauf", ",", "ich", "woll\u00b7te", "mich", "ent\u00b7schlies\u00b7sen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$(", "$,", "VVFIN", "ART", "PAV", "$,", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "25\u201dEuch allen insgesammt mehr Feind als Freund zu seyn:", "tokens": ["\"", "Euch", "al\u00b7len", "ins\u00b7ge\u00b7sammt", "mehr", "Feind", "als", "Freund", "zu", "seyn", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PIS", "ADV", "PIAT", "NN", "KOUS", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201etr\u00e4ff nicht der alte Spruch in Wort- und Wercken ein:", "tokens": ["\u201e", "tr\u00e4ff", "nicht", "der", "al\u00b7te", "Spruch", "in", "Wor\u00b7t", "und", "Wer\u00b7cken", "ein", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "APPR", "TRUNC", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "\u201ewer ist der seinen Feind sucht, sieht, und \u00fcberwindet,", "tokens": ["\u201e", "wer", "ist", "der", "sei\u00b7nen", "Feind", "sucht", ",", "sieht", ",", "und", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eder nicht erst durch das ", "tokens": ["\u201e", "der", "nicht", "erst", "durch", "das"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "PTKNEG", "ADV", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Der wiedersprach man auch. Sie sezte dannoch fort", "tokens": ["Der", "wie\u00b7der\u00b7sprach", "man", "auch", ".", "Sie", "sez\u00b7te", "dan\u00b7noch", "fort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "$.", "PPER", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "30Und sagte: \u201dZeige man nur den geringsten Ort,", "tokens": ["sag\u00b7te", ":", "\"", "Zei\u00b7ge", "man", "nur", "den", "ge\u00b7rings\u00b7ten", "Ort", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "NN", "PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "\u201ewo man wu\u00dft ohne mich so vieles auszusinnen:", "tokens": ["\u201e", "wo", "man", "wu\u00dft", "oh\u00b7ne", "mich", "so", "vie\u00b7les", "aus\u00b7zu\u00b7sin\u00b7nen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "VVFIN", "APPR", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eich mu\u00dfte Schritt vor Schritt, was man verlohr, gewinnen.", "tokens": ["\u201e", "ich", "mu\u00df\u00b7te", "Schritt", "vor", "Schritt", ",", "was", "man", "ver\u00b7lohr", ",", "ge\u00b7win\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "NN", "APPR", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}