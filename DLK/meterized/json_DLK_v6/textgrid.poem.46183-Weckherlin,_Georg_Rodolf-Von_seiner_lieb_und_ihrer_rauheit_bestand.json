{"textgrid.poem.46183": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von seiner lieb und ihrer rauheit bestand", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An sch\u00f6n und rauheit kan man euch,", "tokens": ["An", "sch\u00f6n", "und", "rau\u00b7heit", "kan", "man", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "NN", "VMFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "o Myrta, keine Nymf vergleichen:", "tokens": ["o", "Myr\u00b7ta", ",", "kei\u00b7ne", "Nymf", "ver\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "an lieb ist mir auch keiner gleich", "tokens": ["an", "lieb", "ist", "mir", "auch", "kei\u00b7ner", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "VAFIN", "PPER", "ADV", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und an treu will ich keinem weichen:", "tokens": ["und", "an", "treu", "will", "ich", "kei\u00b7nem", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "dan herzen haben wir von stein,", "tokens": ["dan", "her\u00b7zen", "ha\u00b7ben", "wir", "von", "stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wie, Myrta, m\u00e4niglich mag sehen;", "tokens": ["wie", ",", "Myr\u00b7ta", ",", "m\u00e4\u00b7nig\u00b7lich", "mag", "se\u00b7hen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NE", "$,", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "ich, auszustehen solche pein,", "tokens": ["ich", ",", "aus\u00b7zu\u00b7ste\u00b7hen", "sol\u00b7che", "pein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ihr, meine pein nicht zu verstehen.", "tokens": ["ihr", ",", "mei\u00b7ne", "pein", "nicht", "zu", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dan wir ja l\u00e4ngst ohn widerstand", "tokens": ["Dan", "wir", "ja", "l\u00e4ngst", "ohn", "wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "beed weichend w\u00e4ren \u00fcberwunden:", "tokens": ["beed", "wei\u00b7chend", "w\u00e4\u00b7ren", "\u00fc\u00b7berw\u00b7un\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ihr, Myrt, von meiner lieb bestand", "tokens": ["ihr", ",", "Myrt", ",", "von", "mei\u00b7ner", "lieb", "be\u00b7stand"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich von unz\u00e4hlich schweren wunden:", "tokens": ["ich", "von", "un\u00b7z\u00e4h\u00b7lich", "schwe\u00b7ren", "wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJD", "ADJA", "ADJA", "$."], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "also, da\u00df die selbs st\u00f6ck und stein,", "tokens": ["al\u00b7so", ",", "da\u00df", "die", "selbs", "st\u00f6ck", "und", "stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "die zu halsstarrig uns nicht sehen,", "tokens": ["die", "zu", "hals\u00b7star\u00b7rig", "uns", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "mich, auszustehen solche pein,", "tokens": ["mich", ",", "aus\u00b7zu\u00b7ste\u00b7hen", "sol\u00b7che", "pein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "euch, solche pein nicht zu verstehen.", "tokens": ["euch", ",", "sol\u00b7che", "pein", "nicht", "zu", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ganz steinin ist gewi\u00df mein herz,", "tokens": ["Ganz", "stei\u00b7nin", "ist", "ge\u00b7wi\u00df", "mein", "herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gequ\u00e4let stets euch stets zu lieben;", "tokens": ["ge\u00b7qu\u00e4\u00b7let", "stets", "euch", "stets", "zu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "das eurig auch, dieweil mein schmerz", "tokens": ["das", "eu\u00b7rig", "auch", ",", "die\u00b7weil", "mein", "schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "euch, gnadlos, gar nicht kan betr\u00fcben:", "tokens": ["euch", ",", "gnad\u00b7los", ",", "gar", "nicht", "kan", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$,", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "also mu\u00df man uns beede stein,", "tokens": ["al\u00b7so", "mu\u00df", "man", "uns", "bee\u00b7de", "stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "ja harte felsen sein wol sehen,", "tokens": ["ja", "har\u00b7te", "fel\u00b7sen", "sein", "wol", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "mich, wegen gro\u00dfer lieb und pein,", "tokens": ["mich", ",", "we\u00b7gen", "gro\u00b7\u00dfer", "lieb", "und", "pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "ADJD", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "euch, meine pein nicht zu verstehen.", "tokens": ["euch", ",", "mei\u00b7ne", "pein", "nicht", "zu", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hab best\u00e4ndig meine treu", "tokens": ["Ich", "hab", "be\u00b7st\u00e4n\u00b7dig", "mei\u00b7ne", "treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "alsbald ich euch sah, euch geschworen:", "tokens": ["als\u00b7bald", "ich", "euch", "sah", ",", "euch", "ge\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "bei euch, best\u00e4ndig stolz und frei,", "tokens": ["bei", "euch", ",", "be\u00b7st\u00e4n\u00b7dig", "stolz", "und", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ist all mein leid und lieb verloren:", "tokens": ["ist", "all", "mein", "leid", "und", "lieb", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df also ich an treu ein stein", "tokens": ["da\u00df", "al\u00b7so", "ich", "an", "treu", "ein", "stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und ihr an stolz ein fels zu sehen:", "tokens": ["und", "ihr", "an", "stolz", "ein", "fels", "zu", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "ich, ohn abla\u00df so schwere pein,", "tokens": ["ich", ",", "ohn", "ab\u00b7la\u00df", "so", "schwe\u00b7re", "pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ihr, ohn leid mein leid auszustehen.", "tokens": ["ihr", ",", "ohn", "leid", "mein", "leid", "aus\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "ADJD", "PPOSAT", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wan dan, o fels an h\u00e4rtigkeit,", "tokens": ["Wan", "dan", ",", "o", "fels", "an", "h\u00e4r\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "FM", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein weinen euch nicht kan erweichen:", "tokens": ["mein", "wei\u00b7nen", "euch", "nicht", "kan", "er\u00b7wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wan ich fels an best\u00e4ndigkeit", "tokens": ["wan", "ich", "fels", "an", "be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch undank nicht kan tods verbleichen;", "tokens": ["durch", "un\u00b7dank", "nicht", "kan", "tods", "ver\u00b7blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "so seind wir ja zween harte stein,", "tokens": ["so", "seind", "wir", "ja", "zween", "har\u00b7te", "stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich, solche marter auszustehen,", "tokens": ["ich", ",", "sol\u00b7che", "mar\u00b7ter", "aus\u00b7zu\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "ihr, eure macht und meine pein", "tokens": ["ihr", ",", "eu\u00b7re", "macht", "und", "mei\u00b7ne", "pein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "VVFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und unsern verlust nicht zu sehen.", "tokens": ["und", "un\u00b7sern", "ver\u00b7lust", "nicht", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}