{"textgrid.poem.38131": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "6. Zug nach Liefland, Heidentaufe, Tod", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach diesem Zug des Thedels Weib,", "tokens": ["Nach", "die\u00b7sem", "Zug", "des", "The\u00b7dels", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschied aus dieser Zeitlichkeit.", "tokens": ["Ver\u00b7schied", "aus", "die\u00b7ser", "Zeit\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er brachte sie mit grosser Pracht", "tokens": ["Er", "brach\u00b7te", "sie", "mit", "gros\u00b7ser", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Fackelschein in schwarzer Nacht,", "tokens": ["Bey", "Fa\u00b7ckel\u00b7schein", "in", "schwar\u00b7zer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach Goslar in die Kaiserstadt,", "tokens": ["Nach", "Gos\u00b7lar", "in", "die", "Kai\u00b7ser\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Berief da einen edlen Rath", "tokens": ["Be\u00b7rief", "da", "ei\u00b7nen", "ed\u00b7len", "Rath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcbergab da seinem Sohn", "tokens": ["Und", "\u00fc\u00b7berg\u00b7ab", "da", "sei\u00b7nem", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die G\u00fcter all und zog davon.", "tokens": ["Die", "G\u00fc\u00b7ter", "all", "und", "zog", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er zog auf seinem schwarzen Pferd", "tokens": ["Er", "zog", "auf", "sei\u00b7nem", "schwar\u00b7zen", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zum Orden von dem heilgen Schwerdt", "tokens": ["Zum", "Or\u00b7den", "von", "dem", "heil\u00b7gen", "Schwerdt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nach Liefland, Heiden zu bekehren,", "tokens": ["Nach", "Lie\u00b7fland", ",", "Hei\u00b7den", "zu", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Darin war er ganz unverfehren,", "tokens": ["Da\u00b7rin", "war", "er", "ganz", "un\u00b7ver\u00b7feh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.13": {"text": "In kurzer Zeit das ganz Liefland", "tokens": ["In", "kur\u00b7zer", "Zeit", "das", "ganz", "Lie\u00b7fland"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADV", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Kam meist durch ihn in Ordenshand.", "tokens": ["Kam", "meist", "durch", "ihn", "in", "Or\u00b7dens\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Deutschmeister ihn den Unverfehrt", "tokens": ["Der", "Deutsc\u00b7hmeis\u00b7ter", "ihn", "den", "Un\u00b7ver\u00b7fehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Vor allen hielt so lieb und werth,", "tokens": ["Vor", "al\u00b7len", "hielt", "so", "lieb", "und", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Er lie\u00df den Heiden keine Ruh,", "tokens": ["Er", "lie\u00df", "den", "Hei\u00b7den", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er taufte sie nur immer zu,", "tokens": ["Er", "tauf\u00b7te", "sie", "nur", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Es mu\u00dften dran, arm oder reich,", "tokens": ["Es", "mu\u00df\u00b7ten", "dran", ",", "arm", "o\u00b7der", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "Jung, alt, gro\u00df, klein wohl alle gleich.", "tokens": ["Jung", ",", "alt", ",", "gro\u00df", ",", "klein", "wohl", "al\u00b7le", "gleich", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der Teutschmeister da zu wissen begehrt,", "tokens": ["Der", "Teutsc\u00b7hmeis\u00b7ter", "da", "zu", "wis\u00b7sen", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Wie er gekommen zu dem Pferd,", "tokens": ["Wie", "er", "ge\u00b7kom\u00b7men", "zu", "dem", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das sicher ihn in den Gefahren", "tokens": ["Das", "si\u00b7cher", "ihn", "in", "den", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Vor allen andern kann bewahren.", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "kann", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Herr Thedel bat, davon zu schweigen,", "tokens": ["Herr", "The\u00b7del", "bat", ",", "da\u00b7von", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Am dritten Tag es w\u00fcrd sich zeigen,", "tokens": ["Am", "drit\u00b7ten", "Tag", "es", "w\u00fcrd", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wenn er es h\u00e4tt bekannt gemacht,", "tokens": ["Wenn", "er", "es", "h\u00e4tt", "be\u00b7kannt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Er w\u00fcrd verscheiden in der Nacht,", "tokens": ["Er", "w\u00fcrd", "ver\u00b7schei\u00b7den", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Doch w\u00fcrd er treu der Ordenspflicht,", "tokens": ["Doch", "w\u00fcrd", "er", "treu", "der", "Or\u00b7dens\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Es sagen, wie er es gekriegt.", "tokens": ["Es", "sa\u00b7gen", ",", "wie", "er", "es", "ge\u00b7kriegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Der Meister sich verwundert sehr,", "tokens": ["Der", "Meis\u00b7ter", "sich", "ver\u00b7wun\u00b7dert", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Steht doch nicht ab von Ordensehr,", "tokens": ["Steht", "doch", "nicht", "ab", "von", "Or\u00b7den\u00b7sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Hofft, da\u00df Herr Thedel k\u00f6nn entgehen,", "tokens": ["Hofft", ",", "da\u00df", "Herr", "The\u00b7del", "k\u00f6nn", "ent\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Will vom Befehle nicht abstehen.", "tokens": ["Will", "vom", "Be\u00b7feh\u00b7le", "nicht", "ab\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Herr Thedel bat um vierzehn Tag,", "tokens": ["Herr", "The\u00b7del", "bat", "um", "vier\u00b7zehn", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Da\u00df er der Welt den Abschied sag,", "tokens": ["Da\u00df", "er", "der", "Welt", "den", "Ab\u00b7schied", "sag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Empfing das heilge Sakrament,", "tokens": ["Emp\u00b7fing", "das", "heil\u00b7ge", "Sak\u00b7ra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Bereitet sich zum lezten End,", "tokens": ["Be\u00b7rei\u00b7tet", "sich", "zum", "lez\u00b7ten", "End", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Besteiget dann sein schwarzes Pferd,", "tokens": ["Be\u00b7stei\u00b7get", "dann", "sein", "schwar\u00b7zes", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Erz\u00e4hlt sein Leben unverfehrt,", "tokens": ["Er\u00b7z\u00e4hlt", "sein", "Le\u00b7ben", "un\u00b7ver\u00b7fehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Da geht das Pferd gleich mit ihm durch,", "tokens": ["Da", "geht", "das", "Pferd", "gleich", "mit", "ihm", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Drey Tage irrt er im Gebirg,", "tokens": ["Drey", "Ta\u00b7ge", "irrt", "er", "im", "Ge\u00b7birg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Die dritte Nacht beym Christusbild", "tokens": ["Die", "drit\u00b7te", "Nacht", "beym", "Chris\u00b7tus\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Er sinkt herab, entschlafen mild.", "tokens": ["Er", "sinkt", "her\u00b7ab", ",", "ent\u00b7schla\u00b7fen", "mild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Also kam er aus dem Elend,", "tokens": ["Al\u00b7so", "kam", "er", "aus", "dem", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Also hat die Geschicht ein End.", "tokens": ["Al\u00b7so", "hat", "die", "Ge\u00b7schicht", "ein", "End", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}