{"textgrid.poem.66703": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dr\u00fcben thront die gro\u00dfe Tanne,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00fcben thront die gro\u00dfe Tanne,", "tokens": ["Dr\u00fc\u00b7ben", "thront", "die", "gro\u00b7\u00dfe", "Tan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gipfeleinsam, kerzengrade,", "tokens": ["Gip\u00b7fel\u00b7ein\u00b7sam", ",", "ker\u00b7zen\u00b7gra\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der \u00c4ste breitem Banne", "tokens": ["Mit", "der", "\u00c4s\u00b7te", "brei\u00b7tem", "Ban\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrstlich \u00fcber Busch und Baum.", "tokens": ["F\u00fcrst\u00b7lich", "\u00fc\u00b7ber", "Busch", "und", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sucht das Auge seine Pfade,", "tokens": ["Sucht", "das", "Au\u00b7ge", "sei\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt es aus des Gartens F\u00fclle,", "tokens": ["Steigt", "es", "aus", "des", "Gar\u00b7tens", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df im Abendrot es bade,", "tokens": ["Da\u00df", "im", "A\u00b7ben\u00b7drot", "es", "ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem stolzen Stamm empor.", "tokens": ["An", "dem", "stol\u00b7zen", "Stamm", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbhebe mit mir, von der H\u00fclle", "tokens": ["\u00bb", "he\u00b7be", "mit", "mir", ",", "von", "der", "H\u00fcl\u00b7le"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Duftender Kastanienbl\u00fcten\u00ab \u2013", "tokens": ["Duf\u00b7ten\u00b7der", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7bl\u00fc\u00b7ten", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Spricht sie \u2013 \u00bbin der \u00c4therstille", "tokens": ["Spricht", "sie", "\u2013", "\u00bb", "in", "der", "\u00c4\u00b7ther\u00b7stil\u00b7le"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meerestiefen deinen Blick!", "tokens": ["Mee\u00b7res\u00b7tie\u00b7fen", "dei\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlechte D\u00fcnste sah ich br\u00fcten,", "tokens": ["Schlech\u00b7te", "D\u00fcns\u00b7te", "sah", "ich", "br\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Atem mir umkrallten,", "tokens": ["Die", "den", "A\u00b7tem", "mir", "um\u00b7krall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wetterst\u00fcrme mich umw\u00fcten,", "tokens": ["Wet\u00b7ter\u00b7st\u00fcr\u00b7me", "mich", "um\u00b7w\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Toben sch\u00fctternd Ungemach \u2013", "tokens": ["To\u00b7ben", "sch\u00fct\u00b7ternd", "Un\u00b7ge\u00b7mach", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wilde Blitze wollten spalten", "tokens": ["Wil\u00b7de", "Blit\u00b7ze", "woll\u00b7ten", "spal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Wuchs mit brandiger Lohe \u2013", "tokens": ["Die\u00b7sen", "Wuchs", "mit", "bran\u00b7di\u00b7ger", "Lo\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Meine Krone zu behalten", "tokens": ["Mei\u00b7ne", "Kro\u00b7ne", "zu", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward verg\u00f6nnt vom Weltengott.", "tokens": ["Ward", "ver\u00b7g\u00f6nnt", "vom", "Wel\u00b7ten\u00b7gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und so heb' ich meine hohe", "tokens": ["Und", "so", "heb'", "ich", "mei\u00b7ne", "ho\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stirne stetig und gelassen,", "tokens": ["Stir\u00b7ne", "ste\u00b7tig", "und", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbek\u00fcmmert, was auch drohe,", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", ",", "was", "auch", "dro\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das himmlische Gefild ...", "tokens": ["In", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7fild", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Willst du mit mir Wurzel fassen", "tokens": ["Willst", "du", "mit", "mir", "Wur\u00b7zel", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Lebens ewigem Grunde,", "tokens": ["In", "des", "Le\u00b7bens", "e\u00b7wi\u00b7gem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sei mir gleich! \u2013 Doch zu mir passen", "tokens": ["Sei", "mir", "gleich", "!", "\u2013", "Doch", "zu", "mir", "pas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$.", "$(", "KON", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die stark und einsam sind.\u00ab", "tokens": ["Nur", "die", "stark", "und", "ein\u00b7sam", "sind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJD", "KON", "ADJD", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}