{"textgrid.poem.32500": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Lobspruch des sch\u00f6nen Geschlechts", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir M\u00e4nner stecken voller M\u00e4ngel;", "tokens": ["Wir", "M\u00e4n\u00b7ner", "ste\u00b7cken", "vol\u00b7ler", "M\u00e4n\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es leugne, wer es will!", "tokens": ["Es", "leug\u00b7ne", ",", "wer", "es", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Weiber gegen uns sind Engel.", "tokens": ["Die", "Wei\u00b7ber", "ge\u00b7gen", "uns", "sind", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur taugen, wie ein Kenner will,", "tokens": ["Nur", "tau\u00b7gen", ",", "wie", "ein", "Ken\u00b7ner", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drei kleine St\u00fcck' \u2013 und die sind zu erraten, \u2013", "tokens": ["Drei", "klei\u00b7ne", "St\u00fcck", "\u2013", "und", "die", "sind", "zu", "er\u00b7ra\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "ADJA", "NN", "$(", "KON", "PDS", "VAFIN", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An diesen Engeln nicht gar zu viel!", "tokens": ["An", "die\u00b7sen", "En\u00b7geln", "nicht", "gar", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKNEG", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Gedanken, Wort und Taten.", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "Wort", "und", "Ta\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}