{"textgrid.poem.30560": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer geschm\u00fcnckte Worte gibt, ist nur Freund von Angesicht;", "tokens": ["Wer", "ge\u00b7schm\u00fcnck\u00b7te", "Wor\u00b7te", "gibt", ",", "ist", "nur", "Freund", "von", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn das Hertze liegt verdeckt, darff also der Schm\u00fcncke nicht.", "tokens": ["Denn", "das", "Hert\u00b7ze", "liegt", "ver\u00b7deckt", ",", "darff", "al\u00b7so", "der", "Schm\u00fcn\u00b7cke", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "VVFIN", "VVPP", "$,", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}}}}