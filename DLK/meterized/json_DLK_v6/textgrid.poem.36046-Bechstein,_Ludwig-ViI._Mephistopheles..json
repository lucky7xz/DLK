{"textgrid.poem.36046": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "ViI. Mephistopheles.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo Wissensdrang, nun wird dir gewiss Befriedigung!", "tokens": ["\u00bb", "o", "Wis\u00b7sens\u00b7drang", ",", "nun", "wird", "dir", "ge\u00b7wiss", "Be\u00b7frie\u00b7di\u00b7gung", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich kann zu Sternen schweben mit Adlerfl\u00fcgelschwung!", "tokens": ["Ich", "kann", "zu", "Ster\u00b7nen", "schwe\u00b7ben", "mit", "Ad\u00b7ler\u00b7fl\u00fc\u00b7gel\u00b7schwung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich kann durch Felsenmauern mit Geisterh\u00fclfe gehn,", "tokens": ["Ich", "kann", "durch", "Fel\u00b7sen\u00b7mau\u00b7ern", "mit", "Geis\u00b7ter\u00b7h\u00fcl\u00b7fe", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kann gr\u00fcssen den Aequator und \u00fcberm Nordpol stehn!\u00ab", "tokens": ["Kann", "gr\u00fcs\u00b7sen", "den", "A\u00b7e\u00b7qua\u00b7tor", "und", "\u00fc\u00b7berm", "Nord\u00b7pol", "stehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "KON", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "So ruft voll k\u00fchner Tr\u00e4ume der zauberm\u00e4chtge Mann,", "tokens": ["So", "ruft", "voll", "k\u00fch\u00b7ner", "Tr\u00e4u\u00b7me", "der", "zau\u00b7ber\u00b7m\u00e4cht\u00b7ge", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des neuen Bundes fr\u00f6hlich, der Macht, die er gewann.", "tokens": ["Des", "neu\u00b7en", "Bun\u00b7des", "fr\u00f6h\u00b7lich", ",", "der", "Macht", ",", "die", "er", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie tr\u00e4umt er sich allm\u00e4chtig, wie gross! Sein Auge trifft", "tokens": ["Wie", "tr\u00e4umt", "er", "sich", "all\u00b7m\u00e4ch\u00b7tig", ",", "wie", "gross", "!", "Sein", "Au\u00b7ge", "trifft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADJD", "$,", "PWAV", "ADJD", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im dunkeln Buch der Zukunft schon seines Namens Schrift.", "tokens": ["Im", "dun\u00b7keln", "Buch", "der", "Zu\u00b7kunft", "schon", "sei\u00b7nes", "Na\u00b7mens", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wie die d\u00fcstre D\u00e4mmrung den Tag in Schleier h\u00fcllt,", "tokens": ["Und", "wie", "die", "d\u00fcst\u00b7re", "D\u00e4mm\u00b7rung", "den", "Tag", "in", "Schlei\u00b7er", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da sicht er das Versprechen der Abgrundsmacht erf\u00fcllt;", "tokens": ["Da", "sicht", "er", "das", "Ver\u00b7spre\u00b7chen", "der", "Ab\u00b7grunds\u00b7macht", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da klopft an seine Zelle ganz leise Geisterhand,", "tokens": ["Da", "klopft", "an", "sei\u00b7ne", "Zel\u00b7le", "ganz", "lei\u00b7se", "Geis\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es tritt herein der Diener im grauen M\u00f6nchsgewand.", "tokens": ["Es", "tritt", "her\u00b7ein", "der", "Die\u00b7ner", "im", "grau\u00b7en", "M\u00f6n\u00b7chs\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbo Faustus, Hochgewaltger,\u00ab spricht er, und neigt sein Haupt:", "tokens": ["\u00bb", "o", "Faus\u00b7tus", ",", "Hoch\u00b7ge\u00b7walt\u00b7ger", ",", "\u00ab", "spricht", "er", ",", "und", "neigt", "sein", "Haupt", ":"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbwie hast Du mich des Glanzes, der Herrlichkeit beraubt!", "tokens": ["\u00bb", "wie", "hast", "Du", "mich", "des", "Glan\u00b7zes", ",", "der", "Herr\u00b7lich\u00b7keit", "be\u00b7raubt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "PPER", "PRF", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich, stolz einst und undienstbar dem, der im All gebeut,", "tokens": ["Ich", ",", "stolz", "einst", "und", "un\u00b7di\u00b7enst\u00b7bar", "dem", ",", "der", "im", "All", "ge\u00b7beut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "ADV", "KON", "ADJD", "ART", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Muss nun vor Dir mich beugen, ungern und unerfrent!\u00ab", "tokens": ["Muss", "nun", "vor", "Dir", "mich", "beu\u00b7gen", ",", "un\u00b7gern", "und", "un\u00b7er\u00b7frent", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "PRF", "VVINF", "$,", "ADV", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbdoch wird die Zeit verrinnen, wie jede Zeit verrinnt,", "tokens": ["\u00bb", "doch", "wird", "die", "Zeit", "ver\u00b7rin\u00b7nen", ",", "wie", "je\u00b7de", "Zeit", "ver\u00b7rinnt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Freiheit wird mir beginnen, wenn Knechtschaft Dir beginnt.", "tokens": ["Frei\u00b7heit", "wird", "mir", "be\u00b7gin\u00b7nen", ",", "wenn", "Knecht\u00b7schaft", "Dir", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Knechtschaft, die nimmer endet, bis dahin \u2013 dien' ich treu,", "tokens": ["Knecht\u00b7schaft", ",", "die", "nim\u00b7mer", "en\u00b7det", ",", "bis", "da\u00b7hin", "\u2013", "dien'", "ich", "treu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KON", "PAV", "$(", "VVIMP", "PPER", "ADJD", "$,"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und rufst du nur: ", "tokens": ["Und", "rufst", "du", "nur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "So spricht der graue Diener, und Faustus steht verwirrt;", "tokens": ["So", "spricht", "der", "grau\u00b7e", "Die\u00b7ner", ",", "und", "Faus\u00b7tus", "steht", "ver\u00b7wirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ist's Wahrheit, ist es T\u00e4uschung, die neckend ihn umschwirrt?", "tokens": ["Ist's", "Wahr\u00b7heit", ",", "ist", "es", "T\u00e4u\u00b7schung", ",", "die", "ne\u00b7ckend", "ihn", "um\u00b7schwirrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So steht er nah dem Ziele? So l\u00fcgt die H\u00f6lle nicht?", "tokens": ["So", "steht", "er", "nah", "dem", "Zie\u00b7le", "?", "So", "l\u00fcgt", "die", "H\u00f6l\u00b7le", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und freudig, seiner Macht froh, ermannt er sich, und spricht:", "tokens": ["Und", "freu\u00b7dig", ",", "sei\u00b7ner", "Macht", "froh", ",", "er\u00b7mannt", "er", "sich", ",", "und", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "$,", "VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbgleich ists, ob Du mir freudig, ob Du mir unfroh dien'st,", "tokens": ["\u00bb", "gleich", "ists", ",", "ob", "Du", "mir", "freu\u00b7dig", ",", "ob", "Du", "mir", "un\u00b7froh", "dien'st", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nur dass Du keine Fordrung zu weigern Dich erk\u00fchn'st.", "tokens": ["Nur", "dass", "Du", "kei\u00b7ne", "Ford\u00b7rung", "zu", "wei\u00b7gern", "Dich", "er\u00b7k\u00fchn'", "st."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "NN", "PTKZU", "VVFIN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur dass Du reich das Haus mir, die Tafel mir versiehst,", "tokens": ["Nur", "dass", "Du", "reich", "das", "Haus", "mir", ",", "die", "Ta\u00b7fel", "mir", "ver\u00b7siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "ART", "NN", "PPER", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dass bei dem Quell der Weisheit der Born der Freude fliesst!\u00ab", "tokens": ["Dass", "bei", "dem", "Quell", "der", "Weis\u00b7heit", "der", "Born", "der", "Freu\u00b7de", "fliesst", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "\u203anichts, nichts, was Dir gel\u00fcstet, wird Faustus, Dir versagt;", "tokens": ["\u203a", "nichts", ",", "nichts", ",", "was", "Dir", "ge\u00b7l\u00fcs\u00b7tet", ",", "wird", "Faus\u00b7tus", ",", "Dir", "ver\u00b7sagt", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIS", "$,", "PIS", "$,", "PRELS", "PPER", "VVPP", "$,", "VAFIN", "NE", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich bin Dein Koch, Dein Kellner, Dein Knecht und Deine Magd.", "tokens": ["Ich", "bin", "Dein", "Koch", ",", "Dein", "Kell\u00b7ner", ",", "Dein", "Knecht", "und", "Dei\u00b7ne", "Magd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sei guter Dinge, fr\u00f6hlich, fort mit dem Doktorgewand,", "tokens": ["Sei", "gu\u00b7ter", "Din\u00b7ge", ",", "fr\u00f6h\u00b7lich", ",", "fort", "mit", "dem", "Dok\u00b7tor\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "ADJD", "$,", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es wandle mit ", "tokens": ["Es", "wand\u00b7le", "mit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "\u00bbha, Diener, Du gef\u00e4llst mir; machst mich der Sorge quitt,", "tokens": ["\u00bb", "ha", ",", "Die\u00b7ner", ",", "Du", "ge\u00b7f\u00e4llst", "mir", ";", "machst", "mich", "der", "Sor\u00b7ge", "quitt", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die mir f\u00fcr Haus und Nahrung erwuchs bei jedem Schritt!", "tokens": ["Die", "mir", "f\u00fcr", "Haus", "und", "Nah\u00b7rung", "er\u00b7wuchs", "bei", "je\u00b7dem", "Schritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie freier werd' ich f\u00fchlen des Geistes rege Kraft!", "tokens": ["Wie", "frei\u00b7er", "werd'", "ich", "f\u00fch\u00b7len", "des", "Geis\u00b7tes", "re\u00b7ge", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie froher werd' ich k\u00fchlen den Durst nach Wissenschaft!\u00ab", "tokens": ["Wie", "fro\u00b7her", "werd'", "ich", "k\u00fch\u00b7len", "den", "Durst", "nach", "Wis\u00b7sen\u00b7schaft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u203ader soll Dir bald gestillt sein; Du bist noch namenlos,", "tokens": ["\u203a", "der", "soll", "Dir", "bald", "ge\u00b7stillt", "sein", ";", "Du", "bist", "noch", "na\u00b7men\u00b7los", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Bald aber vor den V\u00f6lkern, Faust, wird Dein Name gross!", "tokens": ["Bald", "a\u00b7ber", "vor", "den", "V\u00f6l\u00b7kern", ",", "Faust", ",", "wird", "Dein", "Na\u00b7me", "gross", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dein Ruhm der hohen Thaten reicht an die Sterne weit,", "tokens": ["Dein", "Ruhm", "der", "ho\u00b7hen", "Tha\u00b7ten", "reicht", "an", "die", "Ster\u00b7ne", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er wird, wie Zaubersaaten, in alles Land gestreut!\u2039", "tokens": ["Er", "wird", ",", "wie", "Zau\u00b7ber\u00b7saa\u00b7ten", ",", "in", "al\u00b7les", "Land", "ge\u00b7streut", "!", "\u2039"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "NN", "$,", "APPR", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbund kannst Du mir erhellen, was dunkel meinem Geist?", "tokens": ["\u00bb", "und", "kannst", "Du", "mir", "er\u00b7hel\u00b7len", ",", "was", "dun\u00b7kel", "mei\u00b7nem", "Geist", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "PPER", "VVINF", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weisst Du des Lichtes Quellen, Du, der so viel verheisst?", "tokens": ["Weisst", "Du", "des", "Lich\u00b7tes", "Quel\u00b7len", ",", "Du", ",", "der", "so", "viel", "ver\u00b7heisst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kannst durch die Luft mich tragen, mich halten \u00fcberm Mcer?", "tokens": ["Kannst", "durch", "die", "Luft", "mich", "tra\u00b7gen", ",", "mich", "hal\u00b7ten", "\u00fc\u00b7berm", "Mcer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Darf ich Dich alles fragen, ist nichts f\u00fcr Dich zu schwer? \u2013\u00ab", "tokens": ["Darf", "ich", "Dich", "al\u00b7les", "fra\u00b7gen", ",", "ist", "nichts", "f\u00fcr", "Dich", "zu", "schwer", "?", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PIS", "VVINF", "$,", "VAFIN", "PIS", "APPR", "PPER", "PTKA", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u203awohl ist mir vieles kundig, und vieles offenbar,", "tokens": ["\u203a", "wohl", "ist", "mir", "vie\u00b7les", "kun\u00b7dig", ",", "und", "vie\u00b7les", "of\u00b7fen\u00b7bar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$,", "KON", "PIS", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die H\u00f6hen und die Tiefen durchschaut mein Augo klar.", "tokens": ["Die", "H\u00f6\u00b7hen", "und", "die", "Tie\u00b7fen", "durch\u00b7schaut", "mein", "Au\u00b7go", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich trage Dich durch L\u00fcfte, trag' \u00fcbers Meer Dich weit,", "tokens": ["Ich", "tra\u00b7ge", "Dich", "durch", "L\u00fcf\u00b7te", ",", "trag'", "\u00fc\u00b7bers", "Meer", "Dich", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich gebe, wenn auch ungern, auf Fragen Dir Bescheid.\u2039", "tokens": ["Ich", "ge\u00b7be", ",", "wenn", "auch", "un\u00b7gern", ",", "auf", "Fra\u00b7gen", "Dir", "Be\u00b7scheid", ".", "\u2039"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ADV", "$,", "APPR", "NN", "PPER", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbund soll ich, wenn ich rufe, als M\u00f6nch Dich immer schaun?", "tokens": ["\u00bb", "und", "soll", "ich", ",", "wenn", "ich", "ru\u00b7fe", ",", "als", "M\u00f6nch", "Dich", "im\u00b7mer", "schaun", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "F\u00fcrwahr, es wird mich h\u00f6chlich die heilge Tracht erbaun!", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "wird", "mich", "h\u00f6ch\u00b7lich", "die", "heil\u00b7ge", "Tracht", "er\u00b7baun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich will vor Dir mich neigen: ", "tokens": ["Ich", "will", "vor", "Dir", "mich", "nei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du sprichst mit frommer Salbung Dein ", "tokens": ["Du", "sprichst", "mit", "from\u00b7mer", "Sal\u00b7bung", "Dein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "\u203adu darfst f\u00fcr Deinen Diener nur w\u00e4hlen die Livrei,", "tokens": ["\u203a", "du", "darfst", "f\u00fcr", "Dei\u00b7nen", "Die\u00b7ner", "nur", "w\u00e4h\u00b7len", "die", "Liv\u00b7rei", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PAV", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mir passen alle Trachten, sind mir auch einerlei.", "tokens": ["Mir", "pas\u00b7sen", "al\u00b7le", "Trach\u00b7ten", ",", "sind", "mir", "auch", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "VAFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich kroch nur in die Kutte f\u00fcr diesesmal hinein,", "tokens": ["Ich", "kroch", "nur", "in", "die", "Kut\u00b7te", "f\u00fcr", "die\u00b7ses\u00b7mal", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weil sie f\u00fcr T\u00fccke, Schalkheit und List ein Heilgeuschein.\u2039", "tokens": ["Weil", "sie", "f\u00fcr", "T\u00fc\u00b7cke", ",", "Schalk\u00b7heit", "und", "List", "ein", "Heil\u00b7geu\u00b7schein", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "\u203adie Kutte ist ein R\u00f6cklein, drin man sich hat bequem;", "tokens": ["\u203a", "die", "Kut\u00b7te", "ist", "ein", "R\u00f6ck\u00b7lein", ",", "drin", "man", "sich", "hat", "be\u00b7quem", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "PIS", "PRF", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Hauskleid f\u00fcr die S\u00fcnde, weich, warm und angenehm.", "tokens": ["Ein", "Haus\u00b7kleid", "f\u00fcr", "die", "S\u00fcn\u00b7de", ",", "weich", ",", "warm", "und", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Beichtstuhl, drin das Pf\u00e4fflein der Nonne Liebe schw\u00f6rt,", "tokens": ["Ein", "Beicht\u00b7stuhl", ",", "drin", "das", "Pf\u00e4f\u00b7flein", "der", "Non\u00b7ne", "Lie\u00b7be", "schw\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ich \u2013 bin eben einer, der in die Kutte geh\u00f6rt.\u2039", "tokens": ["Und", "ich", "\u2013", "bin", "e\u00b7ben", "ei\u00b7ner", ",", "der", "in", "die", "Kut\u00b7te", "ge\u00b7h\u00f6rt", ".", "\u2039"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$(", "VAFIN", "ADV", "PIS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "\u00bbmich freut's, Dich so zu finden, Du scheinst von leichtem Sinn,", "tokens": ["\u00bb", "mich", "freut's", ",", "Dich", "so", "zu", "fin\u00b7den", ",", "Du", "scheinst", "von", "leich\u00b7tem", "Sinn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-++--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du wirst mich gut erheitern, wenn ich verdrossen bin.", "tokens": ["Du", "wirst", "mich", "gut", "er\u00b7hei\u00b7tern", ",", "wenn", "ich", "ver\u00b7dros\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach, ich bin oft verdrossen, und z\u00fcrne dem Geschick:", "tokens": ["Ach", ",", "ich", "bin", "oft", "ver\u00b7dros\u00b7sen", ",", "und", "z\u00fcr\u00b7ne", "dem", "Ge\u00b7schick", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weil Andres mir verschlossen, sucht' ich bei Euch das Gl\u00fcck.\u00ab", "tokens": ["Weil", "And\u00b7res", "mir", "ver\u00b7schlos\u00b7sen", ",", "sucht'", "ich", "bei", "Euch", "das", "Gl\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "\u203ada wohnt's just; sich, wir fassen mit Menschensinn die Welt;", "tokens": ["\u203a", "da", "wohnt's", "just", ";", "sich", ",", "wir", "fas\u00b7sen", "mit", "Men\u00b7schen\u00b7sinn", "die", "Welt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NE", "VVFIN", "$.", "PRF", "$,", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Lieben und im Hassen sind wir Euch gleichgestellt.", "tokens": ["Im", "Lie\u00b7ben", "und", "im", "Has\u00b7sen", "sind", "wir", "Euch", "gleich\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "APPRART", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur dass die Wirkung m\u00e4chtger, doch kaum begreifst Du das,", "tokens": ["Nur", "dass", "die", "Wir\u00b7kung", "m\u00e4cht\u00b7ger", ",", "doch", "kaum", "be\u00b7greifst", "Du", "das", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJA", "$,", "ADV", "ADV", "VVFIN", "PPER", "PDS", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn unsre Lieb' ist t\u00f6dlich, wie t\u00f6dlich unser Hass.\u2039", "tokens": ["Denn", "uns\u00b7re", "Lieb'", "ist", "t\u00f6d\u00b7lich", ",", "wie", "t\u00f6d\u00b7lich", "un\u00b7ser", "Hass", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "\u00bbwohlan, Du sollst mich lehren, scheinst mir just alt genung!", "tokens": ["\u00bb", "wo\u00b7hlan", ",", "Du", "sollst", "mich", "leh\u00b7ren", ",", "scheinst", "mir", "just", "alt", "ge\u00b7nung", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird dem Drang nach Wissen gewiss Befriedigung.", "tokens": ["So", "wird", "dem", "Drang", "nach", "Wis\u00b7sen", "ge\u00b7wiss", "Be\u00b7frie\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fort mit der trocknen Weisheit, fort alter B\u00fccherwust!", "tokens": ["Fort", "mit", "der", "trock\u00b7nen", "Weis\u00b7heit", ",", "fort", "al\u00b7ter", "B\u00fc\u00b7cher\u00b7wust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "PTKVZ", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich will die Welt umfassen mit hoher G\u00f6tterlust!\u00ab", "tokens": ["Ich", "will", "die", "Welt", "um\u00b7fas\u00b7sen", "mit", "ho\u00b7her", "G\u00f6t\u00b7ter\u00b7lust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}