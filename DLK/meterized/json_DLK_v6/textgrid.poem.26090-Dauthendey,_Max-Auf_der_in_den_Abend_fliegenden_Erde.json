{"textgrid.poem.26090": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Auf der in den Abend fliegenden Erde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne vergrub sich hinter den Gartenz\u00e4unen,", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7grub", "sich", "hin\u00b7ter", "den", "Gar\u00b7ten\u00b7z\u00e4u\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tat manchen Acker wie mit R\u00f6tel br\u00e4unen.", "tokens": ["Tat", "man\u00b7chen", "A\u00b7cker", "wie", "mit", "R\u00f6\u00b7tel", "br\u00e4u\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KOKOM", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Birken, denen du hold, sahen gr\u00fc\u00dfend aus,", "tokens": ["Die", "Bir\u00b7ken", ",", "de\u00b7nen", "du", "hold", ",", "sa\u00b7hen", "gr\u00fc\u00b7\u00dfend", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schwarze Wegschnecken schlichen um Steine nach Haus;", "tokens": ["Schwar\u00b7ze", "Weg\u00b7schne\u00b7cken", "schli\u00b7chen", "um", "Stei\u00b7ne", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Ein kleiner Vogel baute noch am Nest im Gem\u00e4uer,", "tokens": ["Ein", "klei\u00b7ner", "Vo\u00b7gel", "bau\u00b7te", "noch", "am", "Nest", "im", "Ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Blaue Nebelfiguren machten das Tal nicht geheuer.", "tokens": ["Blau\u00b7e", "Ne\u00b7bel\u00b7fi\u00b7gu\u00b7ren", "mach\u00b7ten", "das", "Tal", "nicht", "ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Wir sa\u00dfen auf mancher Bank, w\u00e4hlten manchen Gedank,", "tokens": ["Wir", "sa\u00b7\u00dfen", "auf", "man\u00b7cher", "Bank", ",", "w\u00e4hl\u00b7ten", "man\u00b7chen", "Ge\u00b7dank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Sahen schreitenden Paaren nach und manchem, der einsamkeitkrank,", "tokens": ["Sa\u00b7hen", "schrei\u00b7ten\u00b7den", "Paa\u00b7ren", "nach", "und", "man\u00b7chem", ",", "der", "ein\u00b7sam\u00b7keit\u00b7krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "KON", "PIAT", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+--+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Alles, auf der in den Abend fliegenden Erde, wie auf einem Schiff ohne Steuer.", "tokens": ["Al\u00b7les", ",", "auf", "der", "in", "den", "A\u00b7bend", "flie\u00b7gen\u00b7den", "Er\u00b7de", ",", "wie", "auf", "ei\u00b7nem", "Schiff", "oh\u00b7ne", "Steu\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "APPR", "ART", "APPR", "ART", "NN", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+--+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}}}}