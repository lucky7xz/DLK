{"textgrid.poem.57017": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Km 21", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Rabe sa\u00df auf einem Meilenstein", "tokens": ["Ein", "Ra\u00b7be", "sa\u00df", "auf", "ei\u00b7nem", "Mei\u00b7len\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und rief Ka-em-zwei-ein, Ka-em-zwei-ein ...", "tokens": ["und", "rief", "Ka\u00b7em\u00b7zwei\u00b7ein", ",", "Ka\u00b7em\u00b7zwei\u00b7ein", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Werhund lief vorbei, im Maul ein Bein,", "tokens": ["Der", "Wer\u00b7hund", "lief", "vor\u00b7bei", ",", "im", "Maul", "ein", "Bein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Rabe rief Ka-em-zwei-ein, zwei-ein.", "tokens": ["Der", "Ra\u00b7be", "rief", "Ka\u00b7em\u00b7zwei\u00b7ein", ",", "zwei\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Vor\u00fcber zottelte das Zapfenschwein,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zot\u00b7tel\u00b7te", "das", "Zap\u00b7fen\u00b7schwein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Rabe rief und rief Ka-em-zwei-ein.", "tokens": ["der", "Ra\u00b7be", "rief", "und", "rief", "Ka\u00b7em\u00b7zwei\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bber ist besessen!\u00ab \u2013 kam man \u00fcberein.", "tokens": ["\u00bb", "er", "ist", "be\u00b7ses\u00b7sen", "!", "\u00ab", "\u2013", "kam", "man", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$.", "$(", "$(", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u00bbman f\u00fchre ihn hinweg von diesem Stein!\u00ab", "tokens": ["\u00bb", "man", "f\u00fch\u00b7re", "ihn", "hin\u00b7weg", "von", "die\u00b7sem", "Stein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zwei Hasen brachten ihn zum Kr\u00e4uterdachs.", "tokens": ["Zwei", "Ha\u00b7sen", "brach\u00b7ten", "ihn", "zum", "Kr\u00e4u\u00b7ter\u00b7dachs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Hirn war ganz verst\u00f6rt und weich wie Wachs.", "tokens": ["Sein", "Hirn", "war", "ganz", "ver\u00b7st\u00f6rt", "und", "weich", "wie", "Wachs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch sterbend rief er (denn er starb dort) sein", "tokens": ["Noch", "ster\u00b7bend", "rief", "er", "(", "denn", "er", "starb", "dort", ")", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$(", "KON", "PPER", "VVFIN", "ADV", "$(", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ka-em-zwei-ein, Ka-em-Ka-em-zwei-ein ...", "tokens": ["Ka\u00b7em\u00b7zwei\u00b7ein", ",", "Ka\u00b7em\u00b7Ka\u00b7em\u00b7zwei\u00b7ein", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}