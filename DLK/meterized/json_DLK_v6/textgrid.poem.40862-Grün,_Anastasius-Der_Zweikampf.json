{"textgrid.poem.40862": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Der Zweikampf", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allmorgens wenn das Fr\u00fchroth durch Goldgew\u00f6lke stob", "tokens": ["All\u00b7mor\u00b7gens", "wenn", "das", "Fr\u00fch\u00b7roth", "durch", "Gold\u00b7ge\u00b7w\u00f6l\u00b7ke", "stob"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und gl\u00fchende Purpurrosen um Berg' und Th\u00fcrme wob,", "tokens": ["Und", "gl\u00fc\u00b7hen\u00b7de", "Pur\u00b7pur\u00b7ro\u00b7sen", "um", "Ber\u00b7g'", "und", "Th\u00fcr\u00b7me", "wob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da sprengt' ein fr\u00e4nkischer Ritter zum deutschen Lagerfeld", "tokens": ["Da", "sprengt'", "ein", "fr\u00e4n\u00b7ki\u00b7scher", "Rit\u00b7ter", "zum", "deut\u00b7schen", "La\u00b7ger\u00b7feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und trabt' auf stolzem Rosse ringsum von Zelt zu Zelt.", "tokens": ["Und", "trabt'", "auf", "stol\u00b7zem", "Ros\u00b7se", "ring\u00b7sum", "von", "Zelt", "zu", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der zog mit h\u00f6hnischem L\u00e4cheln die b\u00e4rt'gen Lippen schief", "tokens": ["Der", "zog", "mit", "h\u00f6h\u00b7ni\u00b7schem", "L\u00e4\u00b7cheln", "die", "b\u00e4rt'\u00b7gen", "Lip\u00b7pen", "schief"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und hielt vor jedem Zelte, schlug an den Schild und rief:", "tokens": ["Und", "hielt", "vor", "je\u00b7dem", "Zel\u00b7te", ",", "schlug", "an", "den", "Schild", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbheraus, du k\u00fchner Deutscher, der mit mir wagt den Streit,", "tokens": ["\u00bb", "he\u00b7raus", ",", "du", "k\u00fch\u00b7ner", "Deut\u00b7scher", ",", "der", "mit", "mir", "wagt", "den", "Streit", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zur Ehre seines Landes, zur Ehre seiner Maid!\u00ab", "tokens": ["Zur", "Eh\u00b7re", "sei\u00b7nes", "Lan\u00b7des", ",", "zur", "Eh\u00b7re", "sei\u00b7ner", "Maid", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie lie\u00dfen ihn's so treiben \u2013 das waren Deutsche nicht!", "tokens": ["Sie", "lie\u00b7\u00dfen", "ihn's", "so", "trei\u00b7ben", "\u2013", "das", "wa\u00b7ren", "Deut\u00b7sche", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$(", "PDS", "VAFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein jeder blieb im Zelte und that, als h\u00f6rt' er's nicht!", "tokens": ["Ein", "je\u00b7der", "blieb", "im", "Zel\u00b7te", "und", "that", ",", "als", "h\u00f6rt'", "er's", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$,", "KOUS", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drauf sprengte der tolle Ritter in stolzem Satz davon,", "tokens": ["Drauf", "spreng\u00b7te", "der", "tol\u00b7le", "Rit\u00b7ter", "in", "stol\u00b7zem", "Satz", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wie zehntausend Teufel scholl ferne noch sein Hohn.", "tokens": ["Und", "wie", "zehn\u00b7tau\u00b7send", "Teu\u00b7fel", "scholl", "fer\u00b7ne", "noch", "sein", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "NN", "ADJD", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wieder flammt' im Osten der lichte Purpurschein,", "tokens": ["Und", "wie\u00b7der", "flammt'", "im", "Os\u00b7ten", "der", "lich\u00b7te", "Pur\u00b7pur\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und wieder brach den Landen der goldne Tag herein,", "tokens": ["Und", "wie\u00b7der", "brach", "den", "Lan\u00b7den", "der", "gold\u00b7ne", "Tag", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wieder sprengt der Franzmann zum deutschen Lager heran,", "tokens": ["Und", "wie\u00b7der", "sprengt", "der", "Franz\u00b7mann", "zum", "deut\u00b7schen", "La\u00b7ger", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In Erzgewand ger\u00fcstet vom Fu\u00df zum Haupt hinan.", "tokens": ["In", "Erz\u00b7ge\u00b7wand", "ge\u00b7r\u00fcs\u00b7tet", "vom", "Fu\u00df", "zum", "Haupt", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein rother Helmbusch wogte k\u00fchn um sein stolzes Haupt,", "tokens": ["Ein", "ro\u00b7ther", "Helm\u00b7busch", "wog\u00b7te", "k\u00fchn", "um", "sein", "stol\u00b7zes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit rothen Federn hatt' er des Rosses Stirn umlaubt,", "tokens": ["Mit", "ro\u00b7then", "Fe\u00b7dern", "hatt'", "er", "des", "Ros\u00b7ses", "Stirn", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Um seine Schultern spielte ein rothes Wappenkleid,", "tokens": ["Um", "sei\u00b7ne", "Schul\u00b7tern", "spiel\u00b7te", "ein", "ro\u00b7thes", "Wap\u00b7pen\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Rosses R\u00fccken deckte manch purpurroth Geschmeid.", "tokens": ["Des", "Ros\u00b7ses", "R\u00fc\u00b7cken", "deck\u00b7te", "manch", "pur\u00b7pur\u00b7roth", "Ge\u00b7schmeid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.6": {"line.1": {"text": "Und eine Sch\u00e4rpe trug er, so roth wie junges Blut,", "tokens": ["Und", "ei\u00b7ne", "Sch\u00e4r\u00b7pe", "trug", "er", ",", "so", "roth", "wie", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn von des Meeres Borden bis tief ins Franzenland", "tokens": ["Denn", "von", "des", "Mee\u00b7res", "Bor\u00b7den", "bis", "tief", "ins", "Fran\u00b7zen\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "ADJD", "APPRART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "War er ", "tokens": ["War", "er"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Und wieder zog er h\u00f6hnisch die b\u00e4rt'gen Lippen schief", "tokens": ["Und", "wie\u00b7der", "zog", "er", "h\u00f6h\u00b7nisch", "die", "b\u00e4rt'\u00b7gen", "Lip\u00b7pen", "schief"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sah aufs deutsche Lager, pocht' an den Schild und rief:", "tokens": ["Und", "sah", "aufs", "deut\u00b7sche", "La\u00b7ger", ",", "pocht'", "an", "den", "Schild", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbheran, du wackrer Deutscher, der mit mir pr\u00fcft die Wehr,", "tokens": ["\u00bb", "he\u00b7ran", ",", "du", "wack\u00b7rer", "Deut\u00b7scher", ",", "der", "mit", "mir", "pr\u00fcft", "die", "Wehr", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zur Ehre seiner Dame, zu seines Landes Ehr'!\u00ab", "tokens": ["Zur", "Eh\u00b7re", "sei\u00b7ner", "Da\u00b7me", ",", "zu", "sei\u00b7nes", "Lan\u00b7des", "Ehr'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Dem Vollmond gleich, wenn pl\u00f6tzlich er durch Gew\u00f6lk sich dr\u00e4ngt,", "tokens": ["Dem", "Voll\u00b7mond", "gleich", ",", "wenn", "pl\u00f6tz\u00b7lich", "er", "durch", "Ge\u00b7w\u00f6lk", "sich", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "ADJD", "PPER", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-++--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kam jetzt auf schnellem Zelter ein Rittersmann gesprengt,", "tokens": ["Kam", "jetzt", "auf", "schnel\u00b7lem", "Zel\u00b7ter", "ein", "Rit\u00b7ters\u00b7mann", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der hat sein k\u00fchnes Antlitz in Gittererz vermummt,", "tokens": ["Der", "hat", "sein", "k\u00fch\u00b7nes", "Ant\u00b7litz", "in", "Git\u00b7ter\u00b7erz", "ver\u00b7mummt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihn kennt nicht Frank' und Deutscher, und Alles rings verstummt.", "tokens": ["Ihn", "kennt", "nicht", "Frank'", "und", "Deut\u00b7scher", ",", "und", "Al\u00b7les", "rings", "ver\u00b7stummt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$,", "KON", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Auf seinem Helme zeigt sich kein schmucker Federstrau\u00df,", "tokens": ["Auf", "sei\u00b7nem", "Hel\u00b7me", "zeigt", "sich", "kein", "schmu\u00b7cker", "Fe\u00b7der\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein goldner Stern nur neigt sich aus blanken Oehrlein heraus:", "tokens": ["Ein", "gold\u00b7ner", "Stern", "nur", "neigt", "sich", "aus", "blan\u00b7ken", "O\u00b7e\u00b7hrlein", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ist's der Purpurstern der Liebe, der, ach, so schnell vergeht?", "tokens": ["Ist's", "der", "Pur\u00b7purs\u00b7tern", "der", "Lie\u00b7be", ",", "der", ",", "ach", ",", "so", "schnell", "ver\u00b7geht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "PRELS", "$,", "ITJ", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-++--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ist's der blasse Stern der Hoffnung, der ewig leuchtend steht?", "tokens": ["Ist's", "der", "blas\u00b7se", "Stern", "der", "Hoff\u00b7nung", ",", "der", "e\u00b7wig", "leuch\u00b7tend", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.10": {"line.1": {"text": "Es wogt um seine Schultern kein schmuckes Wappenkleid,", "tokens": ["Es", "wogt", "um", "sei\u00b7ne", "Schul\u00b7tern", "kein", "schmu\u00b7ckes", "Wap\u00b7pen\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein rauher Eisenpanzer ist seiner Brust Geschmeid,", "tokens": ["Ein", "rau\u00b7her", "Ei\u00b7sen\u00b7pan\u00b7zer", "ist", "sei\u00b7ner", "Brust", "Ge\u00b7schmeid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur eine Silbersch\u00e4rpe wallt um des Busens Wehr,", "tokens": ["Nur", "ei\u00b7ne", "Sil\u00b7ber\u00b7sch\u00e4r\u00b7pe", "wallt", "um", "des", "Bu\u00b7sens", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drauf steht mit g\u00fcldnen Z\u00fcgen gar zierlich: ", "tokens": ["Drauf", "steht", "mit", "g\u00fcld\u00b7nen", "Z\u00fc\u00b7gen", "gar", "zier\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Als k\u00f6nnt' er unterliegen, so zog d