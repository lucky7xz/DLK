{"dta.poem.7346": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die beschr\u00e4nkte Frau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Kr\u00e4mer hatte eine Frau,", "tokens": ["Ein", "Kr\u00e4\u00b7mer", "hat\u00b7te", "ei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die war ihm schier zu sanft und milde,", "tokens": ["Die", "war", "ihm", "schier", "zu", "sanft", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Haar zu licht, ihr Aug' zu blau,", "tokens": ["Ihr", "Haar", "zu", "licht", ",", "ihr", "Aug'", "zu", "blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu gleich ihr Blick dem Mondenschilde;", "tokens": ["Zu", "gleich", "ihr", "Blick", "dem", "Mon\u00b7den\u00b7schil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn er sie sah so still und sacht", "tokens": ["Wenn", "er", "sie", "sah", "so", "still", "und", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Hause gleiten wie ein Schemen,", "tokens": ["Im", "Hau\u00b7se", "glei\u00b7ten", "wie", "ein", "Sche\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann fa\u00dft es ihn wie b\u00f6se Macht,", "tokens": ["Dann", "fa\u00dft", "es", "ihn", "wie", "b\u00f6\u00b7se", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er mu\u00dfte sich zusammen nehmen.", "tokens": ["Er", "mu\u00df\u00b7te", "sich", "zu\u00b7sam\u00b7men", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor Allem macht ihm Ueberdru\u00df", "tokens": ["Vor", "Al\u00b7lem", "macht", "ihm", "Ue\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wort, das sie an Alles kn\u00fcpfte,", "tokens": ["Ein", "Wort", ",", "das", "sie", "an", "Al\u00b7les", "kn\u00fcpf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das freilich in der Rede Flu\u00df", "tokens": ["Das", "frei\u00b7lich", "in", "der", "Re\u00b7de", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankenlos dem Mund entschl\u00fcpfte:", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "dem", "Mund", "ent\u00b7schl\u00fcpf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ein Gottes Namen\u201c, sprach sie dann,", "tokens": ["\u201e", "in", "Got\u00b7tes", "Na\u00b7men", "\u201c", ",", "sprach", "sie", "dann", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "NN", "$(", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn schwere Pr\u00fcfungsstunden kamen,", "tokens": ["Wenn", "schwe\u00b7re", "Pr\u00fc\u00b7fungs\u00b7stun\u00b7den", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn zu Weine ging ihr Mann,", "tokens": ["Und", "wenn", "zu", "Wei\u00b7ne", "ging", "ihr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann sprach sie auch: \u201ein Gottes Namen.\u201c", "tokens": ["Dann", "sprach", "sie", "auch", ":", "\u201e", "in", "Got\u00b7tes", "Na\u00b7men", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "$(", "APPR", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das schien ihm l\u00e4cherlich und dumm,", "tokens": ["Das", "schien", "ihm", "l\u00e4\u00b7cher\u00b7lich", "und", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mitunter frevelhaft vermessen;", "tokens": ["Mi\u00b7tun\u00b7ter", "fre\u00b7vel\u00b7haft", "ver\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Oft schalt er und sie weinte drum,", "tokens": ["Oft", "schalt", "er", "und", "sie", "wein\u00b7te", "drum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat es immer doch vergessen.", "tokens": ["Und", "hat", "es", "im\u00b7mer", "doch", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gew\u00f6hnung war es fr\u00fcher Zeit", "tokens": ["Ge\u00b7w\u00f6h\u00b7nung", "war", "es", "fr\u00fc\u00b7her", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kl\u00f6sterlich verlebter Jugend;", "tokens": ["Und", "kl\u00f6s\u00b7ter\u00b7lich", "ver\u00b7leb\u00b7ter", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So war es keine S\u00fcndlichkeit", "tokens": ["So", "war", "es", "kei\u00b7ne", "S\u00fcnd\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und war auch eben keine Tugend.", "tokens": ["Und", "war", "auch", "e\u00b7ben", "kei\u00b7ne", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Sprichwort sagt: wem gar nichts fehlt,", "tokens": ["Ein", "Sprich\u00b7wort", "sagt", ":", "wem", "gar", "nichts", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den \u00e4rgert an der Wand die Fliege;", "tokens": ["Den", "\u00e4r\u00b7gert", "an", "der", "Wand", "die", "Flie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So hat dies Wort ihn mehr gequ\u00e4lt,", "tokens": ["So", "hat", "dies", "Wort", "ihn", "mehr", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Andre Hinterlist und L\u00fcge.", "tokens": ["Als", "And\u00b7re", "Hin\u00b7ter\u00b7list", "und", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sprach sie sanft: \u201ees pa\u00dfte schlecht!\u201c", "tokens": ["Und", "sprach", "sie", "sanft", ":", "\u201e", "es", "pa\u00df\u00b7te", "schlecht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "$(", "PPER", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Demuth seinen Groll zu z\u00e4hmen,", "tokens": ["Durch", "De\u00b7muth", "sei\u00b7nen", "Groll", "zu", "z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So schwur er, \u00fcbel oder recht,", "tokens": ["So", "schwur", "er", ",", "\u00fc\u00b7bel", "o\u00b7der", "recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Werd' es ihn \u00e4rgern und besch\u00e4men.", "tokens": ["Werd'", "es", "ihn", "\u00e4r\u00b7gern", "und", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Bl\u00fcthenhaag war seine Lust.", "tokens": ["Ein", "Bl\u00fc\u00b7then\u00b7haag", "war", "sei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst sah die Frau ihn sinnend stehen,", "tokens": ["Einst", "sah", "die", "Frau", "ihn", "sin\u00b7nend", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ganz versunken, unbewu\u00dft,", "tokens": ["Und", "ganz", "ver\u00b7sun\u00b7ken", ",", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So Zweig an Zweig vom Strauche drehen;", "tokens": ["So", "Zweig", "an", "Zweig", "vom", "Strau\u00b7che", "dre\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ein Gottes Namen!\u201c rief sie, \u201eMann,", "tokens": ["\u201e", "in", "Got\u00b7tes", "Na\u00b7men", "!", "\u201c", "rief", "sie", ",", "\u201e", "Mann", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "APPR", "NN", "NN", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edu ruinirst den ganzen Hagen!\u201c", "tokens": ["\u201e", "du", "ru\u00b7i\u00b7nirst", "den", "gan\u00b7zen", "Ha\u00b7gen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Gatte sah sie grimmig an,", "tokens": ["Der", "Gat\u00b7te", "sah", "sie", "grim\u00b7mig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcrwahr, fast h\u00e4tt' er sie geschlagen.", "tokens": ["F\u00fcr\u00b7wahr", ",", "fast", "h\u00e4tt'", "er", "sie", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch wer da Ungl\u00fcck sucht und Reu,", "tokens": ["Doch", "wer", "da", "Un\u00b7gl\u00fcck", "sucht", "und", "Reu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VVFIN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem werden sie entgegen eilen,", "tokens": ["Dem", "wer\u00b7den", "sie", "ent\u00b7ge\u00b7gen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Handel ist ein zart Geb\u00e4u,", "tokens": ["Der", "Han\u00b7del", "ist", "ein", "zart", "Ge\u00b7b\u00e4u", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruht gar sehr auf fremden S\u00e4ulen.", "tokens": ["Und", "ruht", "gar", "sehr", "auf", "frem\u00b7den", "S\u00e4u\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Freund fallirt, ein Schuldner flieht,", "tokens": ["Ein", "Freund", "fall\u00b7irt", ",", "ein", "Schuld\u00b7ner", "flieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Gl\u00e4ub'ger will sich nicht gedulden,", "tokens": ["Ein", "Gl\u00e4ub'\u00b7ger", "will", "sich", "nicht", "ge\u00b7dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und eh ein halbes Jahr verzieht", "tokens": ["Und", "eh", "ein", "hal\u00b7bes", "Jahr", "ver\u00b7zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wei\u00df unser Kr\u00e4mer sich in Schulden.", "tokens": ["Wei\u00df", "un\u00b7ser", "Kr\u00e4\u00b7mer", "sich", "in", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Gattin hat ihn oft gesehn", "tokens": ["Die", "Gat\u00b7tin", "hat", "ihn", "oft", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedankenvoll im Sande waten,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "im", "San\u00b7de", "wa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Contobuche seufzend stehn,", "tokens": ["Am", "Con\u00b7to\u00b7bu\u00b7che", "seuf\u00b7zend", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat ihn endlich auch errathen;", "tokens": ["Und", "hat", "ihn", "end\u00b7lich", "auch", "er\u00b7ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie \u00f6ffnet heimlich ihren Schrein,", "tokens": ["Sie", "\u00f6ff\u00b7net", "heim\u00b7lich", "ih\u00b7ren", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Langt aus verborgner F\u00e4cher Grube,", "tokens": ["Langt", "aus", "ver\u00b7borg\u00b7ner", "F\u00e4\u00b7cher", "Gru\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann, leise wie der Mondenschein,", "tokens": ["Dann", ",", "lei\u00b7se", "wie", "der", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schl\u00fcpft sie in ihres Mannes Stube.", "tokens": ["Schl\u00fcpft", "sie", "in", "ih\u00b7res", "Man\u00b7nes", "Stu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der sa\u00df, die schwere Stirn gest\u00fctzt,", "tokens": ["Der", "sa\u00df", ",", "die", "schwe\u00b7re", "Stirn", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rauchte fort am kalten Rohre:", "tokens": ["Und", "rauch\u00b7te", "fort", "am", "kal\u00b7ten", "Roh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ecarl!\u201c drang ein scheues Fl\u00fcstern itzt,", "tokens": ["\u201e", "carl", "!", "\u201c", "drang", "ein", "scheu\u00b7es", "Fl\u00fcs\u00b7tern", "itzt", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder \u201eCarl!\u201c zu seinem Ohre;", "tokens": ["Und", "wie\u00b7der", "\u201e", "Carl", "!", "\u201c", "zu", "sei\u00b7nem", "Oh\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "NE", "$.", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie stand vor ihm, wie Blut so roth,", "tokens": ["Sie", "stand", "vor", "ihm", ",", "wie", "Blut", "so", "roth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als g\u00e4lt' es eine Schuld gestehen.", "tokens": ["Als", "g\u00e4lt'", "es", "ei\u00b7ne", "Schuld", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201ecarl\u201c sprach sie, \u201ewenn uns Unheil droht,", "tokens": ["\u201e", "carl", "\u201c", "sprach", "sie", ",", "\u201e", "wenn", "uns", "Un\u00b7heil", "droht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "VVFIN", "PPER", "$,", "$(", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist's denn unm\u00f6glich, ihm entgehen?\u201c", "tokens": ["Ist's", "denn", "un\u00b7m\u00f6g\u00b7lich", ",", "ihm", "ent\u00b7ge\u00b7hen", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drauf reicht sie aus der Sch\u00fcrze dar", "tokens": ["Drauf", "reicht", "sie", "aus", "der", "Sch\u00fcr\u00b7ze", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein S\u00e4ckchen, stramm und schwer zu tragen,", "tokens": ["Ein", "S\u00e4ck\u00b7chen", ",", "stramm", "und", "schwer", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drinn Alles was sie achtzehn Jahr", "tokens": ["Drinn", "Al\u00b7les", "was", "sie", "acht\u00b7zehn", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "PWS", "PPER", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erspart am eigenen Behagen.", "tokens": ["Er\u00b7spart", "am", "ei\u00b7ge\u00b7nen", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er sah sie an mit raschem Blick,", "tokens": ["Er", "sah", "sie", "an", "mit", "ra\u00b7schem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und z\u00e4hlte, z\u00e4hlte nun auf's Neue,", "tokens": ["Und", "z\u00e4hl\u00b7te", ",", "z\u00e4hl\u00b7te", "nun", "auf's", "Neu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann sprach er seufzend: \u201emein Geschick", "tokens": ["Dann", "sprach", "er", "seuf\u00b7zend", ":", "\u201e", "mein", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist zu verwirrt, \u2014 dies langt wie Spreue!\u201c", "tokens": ["Ist", "zu", "ver\u00b7wirrt", ",", "dies", "langt", "wie", "Spreu\u00b7e", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "$,", "$(", "PDS", "VVFIN", "KOKOM", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Sie bot ein Blatt, und wandt' sich um,", "tokens": ["Sie", "bot", "ein", "Blatt", ",", "und", "wandt'", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erzitternd, gl\u00fch gleich der Granate;", "tokens": ["Er\u00b7zit\u00b7ternd", ",", "gl\u00fch", "gleich", "der", "Gra\u00b7na\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es war ihr kleines Eigenthum,", "tokens": ["Es", "war", "ihr", "klei\u00b7nes", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Erbtheil einer frommen Pathe.", "tokens": ["Das", "E\u00b7rbtheil", "ei\u00b7ner", "from\u00b7men", "Pa\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u201enein\u201c sprach der Mann, \u201edas soll nicht seyn!\u201c", "tokens": ["\u201e", "nein", "\u201c", "sprach", "der", "Mann", ",", "\u201e", "das", "soll", "nicht", "seyn", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$(", "VVFIN", "ART", "NN", "$,", "$(", "PDS", "VMFIN", "PTKNEG", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und klopfte freundlich ihre Wangen.", "tokens": ["Und", "klopf\u00b7te", "freund\u00b7lich", "ih\u00b7re", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann warf er einen Blick hinein", "tokens": ["Dann", "warf", "er", "ei\u00b7nen", "Blick", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sagte dumpf: \u201eschier m\u00f6cht' es langen.\u201c", "tokens": ["Und", "sag\u00b7te", "dumpf", ":", "\u201e", "schier", "m\u00f6cht'", "es", "lan\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "ADJD", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Nun nahm sie, aus der Sch\u00fcrze Grund,", "tokens": ["Nun", "nahm", "sie", ",", "aus", "der", "Sch\u00fcr\u00b7ze", "Grund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All ihre armen Herrlichkeiten,", "tokens": ["All", "ih\u00b7re", "ar\u00b7men", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Theel\u00f6ffelchen, Dukaten rund,", "tokens": ["Thee\u00b7l\u00f6f\u00b7fel\u00b7chen", ",", "Du\u00b7ka\u00b7ten", "rund", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Was ihr geschenkt von Kindeszeiten.", "tokens": ["Was", "ihr", "ge\u00b7schenkt", "von", "Kin\u00b7des\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie gab es mit so freud'gem Zug!", "tokens": ["Sie", "gab", "es", "mit", "so", "freu\u00b7d'\u00b7gem", "Zug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Doch war's als ob ihr Mund sich regte,", "tokens": ["Doch", "wa\u00b7r's", "als", "ob", "ihr", "Mund", "sich", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Als sie zuletzt auf's Contobuch", "tokens": ["Als", "sie", "zu\u00b7letzt", "auf's", "Con\u00b7to\u00b7buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der sel'gen Mutter Trauring legte.", "tokens": ["Der", "sel'\u00b7gen", "Mut\u00b7ter", "Trau\u00b7ring", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u201efast langt es\u201c, sprach ger\u00fchrt der Mann,", "tokens": ["\u201e", "fast", "langt", "es", "\u201c", ",", "sprach", "ge\u00b7r\u00fchrt", "der", "Mann", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$(", "$,", "VVFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eund dennoch kann es schm\u00e4hlich enden;", "tokens": ["\u201e", "und", "den\u00b7noch", "kann", "es", "schm\u00e4h\u00b7lich", "en\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du dein Leben dann fortan,", "tokens": ["Willst", "du", "dein", "Le\u00b7ben", "dann", "for\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gepl\u00fcndert, fristen mit den H\u00e4nden?\u201c", "tokens": ["Ge\u00b7pl\u00fcn\u00b7dert", ",", "fris\u00b7ten", "mit", "den", "H\u00e4n\u00b7den", "?", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sah ihn an, \u2014 nur Liebe wei\u00df", "tokens": ["Sie", "sah", "ihn", "an", ",", "nur", "Lie\u00b7be", "wei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "$(", "ADV", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "An liebem Blicke so zu hangen \u2014", "tokens": ["An", "lie\u00b7bem", "Bli\u00b7cke", "so", "zu", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201ein Gottes Namen!\u201c sprach sie leis,", "tokens": ["\u201e", "in", "Got\u00b7tes", "Na\u00b7men", "!", "\u201c", "sprach", "sie", "leis", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und weinend hielt er sie umfangen.", "tokens": ["Und", "wei\u00b7nend", "hielt", "er", "sie", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}