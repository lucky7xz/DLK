{"dta.poem.7982": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.  Die Nachtschw\u00e4rmer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ei! was kann man nicht erleben!", "tokens": ["Ei", "!", "was", "kann", "man", "nicht", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute war doch Sommerhitze,", "tokens": ["Heu\u00b7te", "war", "doch", "Som\u00b7mer\u00b7hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun hat\u2019s Glatteis gegeben;", "tokens": ["Und", "nun", "hat's", "Glat\u00b7teis", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich noch auf\u2019s Pflaster sitze,", "tokens": ["Da\u00df", "ich", "noch", "auf's", "Pflas\u00b7ter", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich jeden Schritt erbeben;", "tokens": ["Mu\u00df", "ich", "je\u00b7den", "Schritt", "er\u00b7be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die H\u00e4user taumeln alle,", "tokens": ["Und", "die", "H\u00e4u\u00b7ser", "tau\u00b7meln", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich kaum an eines pralle.", "tokens": ["Wenn", "ich", "kaum", "an", "ei\u00b7nes", "pral\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00fcte sich in diesen Zeiten", "tokens": ["H\u00fc\u00b7te", "sich", "in", "die\u00b7sen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wer da wandelt, auszugleiten,", "tokens": ["Wer", "da", "wan\u00b7delt", ",", "aus\u00b7zu\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und wer steht, da\u00df er nicht falle!", "tokens": ["Und", "wer", "steht", ",", "da\u00df", "er", "nicht", "fal\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}