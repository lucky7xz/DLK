{"textgrid.poem.65178": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schaar der Frommen h\u00f6r' ich seufzen, klagen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schaar der Frommen h\u00f6r' ich seufzen, klagen,", "tokens": ["Die", "Schaar", "der", "From\u00b7men", "h\u00f6r'", "ich", "seuf\u00b7zen", ",", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df von dem Sturm, der jetzt die Welt ersch\u00fcttert,", "tokens": ["Da\u00df", "von", "dem", "Sturm", ",", "der", "jetzt", "die", "Welt", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Schutt und Tr\u00fcmmer Christi Reich zersplittert,", "tokens": ["In", "Schutt", "und", "Tr\u00fcm\u00b7mer", "Chris\u00b7ti", "Reich", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit allem Segen, den es je getragen.", "tokens": ["Mit", "al\u00b7lem", "Se\u00b7gen", ",", "den", "es", "je", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir aber scheint es h\u00f6her nur zu ragen,", "tokens": ["Mir", "a\u00b7ber", "scheint", "es", "h\u00f6\u00b7her", "nur", "zu", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit es, von Dogmen l\u00e4nger nicht umgittert,", "tokens": ["Seit", "es", ",", "von", "Dog\u00b7men", "l\u00e4n\u00b7ger", "nicht", "um\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Strahl der Liebe durch die Seelen zittert,", "tokens": ["Als", "Strahl", "der", "Lie\u00b7be", "durch", "die", "See\u00b7len", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie nie zuvor in den vergang'nen Tagen.", "tokens": ["Wie", "nie", "zu\u00b7vor", "in", "den", "ver\u00b7gang'\u00b7nen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sagt an! wann griff das fremde Leid so hart,", "tokens": ["Sagt", "an", "!", "wann", "griff", "das", "frem\u00b7de", "Leid", "so", "hart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So dr\u00e4ngend an die Herzen der Begl\u00fcckten,", "tokens": ["So", "dr\u00e4n\u00b7gend", "an", "die", "Her\u00b7zen", "der", "Be\u00b7gl\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie in der vielgeschm\u00e4hten Gegenwart?", "tokens": ["Wie", "in", "der", "viel\u00b7ge\u00b7schm\u00e4h\u00b7ten", "Ge\u00b7gen\u00b7wart", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im echten Sinne christlich ist die Zeit,", "tokens": ["Im", "ech\u00b7ten", "Sin\u00b7ne", "christ\u00b7lich", "ist", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihre Kraft dem Schutz der Unterdr\u00fcckten,", "tokens": ["Die", "ih\u00b7re", "Kraft", "dem", "Schutz", "der", "Un\u00b7ter\u00b7dr\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Dienst der Armen und Verlass'nen weiht!", "tokens": ["Dem", "Dienst", "der", "Ar\u00b7men", "und", "Ver\u00b7lass'\u00b7nen", "weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}