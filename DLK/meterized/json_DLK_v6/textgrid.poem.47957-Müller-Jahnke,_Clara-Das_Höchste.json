{"textgrid.poem.47957": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Das H\u00f6chste", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mir das H\u00f6chste ist, das sing ich nicht;", "tokens": ["Was", "mir", "das", "H\u00f6chs\u00b7te", "ist", ",", "das", "sing", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verschlossen bleibt des Herzens Heiligtum \u2013", "tokens": ["ver\u00b7schlos\u00b7sen", "bleibt", "des", "Her\u00b7zens", "Hei\u00b7lig\u00b7tum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und seines Wesens keusches Siegel bricht", "tokens": ["und", "sei\u00b7nes", "We\u00b7sens", "keu\u00b7sches", "Sie\u00b7gel", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "kein Beifallsl\u00e4cheln und kein Dichterruhm;", "tokens": ["kein", "Bei\u00b7fall\u00b7sl\u00e4\u00b7cheln", "und", "kein", "Dich\u00b7ter\u00b7ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "doch ist mein Schaffen nur von ihm belebt:", "tokens": ["doch", "ist", "mein", "Schaf\u00b7fen", "nur", "von", "ihm", "be\u00b7lebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie in der Bl\u00fcte Kelch, der Felsenglieder", "tokens": ["Wie", "in", "der", "Bl\u00fc\u00b7te", "Kelch", ",", "der", "Fel\u00b7sen\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "granitner Pracht das Unsichtbare webt,", "tokens": ["gra\u00b7nit\u00b7ner", "Pracht", "das", "Un\u00b7sicht\u00b7ba\u00b7re", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "so str\u00f6mt sein Hauch durch alle meine Lieder.", "tokens": ["so", "str\u00f6mt", "sein", "Hauch", "durch", "al\u00b7le", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}