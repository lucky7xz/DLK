{"textgrid.poem.39373": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: In klarem Deutsch die heil'ge Schrift", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In klarem Deutsch die heil'ge Schrift", "tokens": ["In", "kla\u00b7rem", "Deutsch", "die", "heil'\u00b7ge", "Schrift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar gro\u00dfen Segen hat gestift't.", "tokens": ["Gar", "gro\u00b7\u00dfen", "Se\u00b7gen", "hat", "ge\u00b7stift'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "VVPP", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ward verdeutscht durch Martin Lutz,", "tokens": ["Sie", "ward", "ver\u00b7deutscht", "durch", "Mar\u00b7tin", "Lutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Papst und Clerisei zum Trutz.", "tokens": ["Dem", "Papst", "und", "Cle\u00b7ri\u00b7sei", "zum", "Trutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ketzer war ein frommer Mann:", "tokens": ["Der", "Ket\u00b7zer", "war", "ein", "from\u00b7mer", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott woll' ihn ewig selig han.", "tokens": ["Gott", "woll'", "ihn", "e\u00b7wig", "se\u00b7lig", "han", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}