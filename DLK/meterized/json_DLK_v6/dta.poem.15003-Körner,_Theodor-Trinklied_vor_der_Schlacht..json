{"dta.poem.15003": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied vor der Schlacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00dft sie in freudigem Kreise,", "tokens": ["Gr\u00fc\u00dft", "sie", "in", "freu\u00b7di\u00b7gem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Laut nach germanischer Weise.", "tokens": ["Laut", "nach", "ger\u00b7ma\u00b7ni\u00b7scher", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Br\u00fcder heran!", "tokens": ["Br\u00fc\u00b7der", "he\u00b7ran", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Noch perlt der Wein;", "tokens": ["Noch", "perlt", "der", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Eh' die Posaunen erdr\u00f6hnen,", "tokens": ["Eh'", "die", "Po\u00b7sau\u00b7nen", "er\u00b7dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00dft uns das Leben vers\u00f6hnen.", "tokens": ["La\u00dft", "uns", "das", "Le\u00b7ben", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Br\u00fcder schenkt ein.", "tokens": ["Br\u00fc\u00b7der", "schenkt", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Gott Vater h\u00f6rt,", "tokens": ["Gott", "Va\u00b7ter", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was an des Grabes Thoren", "tokens": ["Was", "an", "des", "Gra\u00b7bes", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vaterlands S\u00f6hne geschworen,", "tokens": ["Va\u00b7ter\u00b7lands", "S\u00f6h\u00b7ne", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Br\u00fcder, ihr schw\u00f6rt.", "tokens": ["Br\u00fc\u00b7der", ",", "ihr", "schw\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Vaterlands Hort,", "tokens": ["Va\u00b7ter\u00b7lands", "Hort", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Woll'n wir's aus gl\u00fchenden Ketten", "tokens": ["Woll'n", "wir's", "aus", "gl\u00fc\u00b7hen\u00b7den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Todt oder siegend erretten. \u2014", "tokens": ["Todt", "o\u00b7der", "sie\u00b7gend", "er\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Handschlag und Wort!", "tokens": ["Hand\u00b7schlag", "und", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt ihr sie nahn!", "tokens": ["H\u00f6rt", "ihr", "sie", "nahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Liebe und Freuden und Leiden,", "tokens": ["Lie\u00b7be", "und", "Freu\u00b7den", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Tod! du kannst uns nicht scheiden!", "tokens": ["Tod", "!", "du", "kannst", "uns", "nicht", "schei\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Br\u00fcder sto\u00dft an!", "tokens": ["Br\u00fc\u00b7der", "sto\u00dft", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Schlacht ruft! hinaus!", "tokens": ["Schlacht", "ruft", "!", "hin\u00b7aus", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Horch, die Trompeten werben.", "tokens": ["Horch", ",", "die", "Trom\u00b7pe\u00b7ten", "wer\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Vorw\u00e4rts auf Leben und Sterben!", "tokens": ["Vor\u00b7w\u00e4rts", "auf", "Le\u00b7ben", "und", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Br\u00fcder trinkt aus!", "tokens": ["Br\u00fc\u00b7der", "trinkt", "aus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}