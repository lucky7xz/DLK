{"textgrid.poem.40476": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, k\u00f6nnt' ich die Welle sein,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, k\u00f6nnt' ich die Welle sein,", "tokens": ["Ach", ",", "k\u00f6nnt'", "ich", "die", "Wel\u00b7le", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie freut' ich mich so!", "tokens": ["Wie", "freut'", "ich", "mich", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch k\u00f6nnt' ich die Quelle sein,", "tokens": ["Doch", "k\u00f6nnt'", "ich", "die", "Quel\u00b7le", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "W\u00e4r' doppelt ich froh!", "tokens": ["W\u00e4r'", "dop\u00b7pelt", "ich", "froh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnt' ich die Welle sein,", "tokens": ["K\u00f6nnt'", "ich", "die", "Wel\u00b7le", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "H\u00fcpft' ich mit frohem Sinn,", "tokens": ["H\u00fcpft'", "ich", "mit", "fro\u00b7hem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo sie im Bade weilt,", "tokens": ["Wo", "sie", "im", "Ba\u00b7de", "weilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Rasch zur Geliebten hin;", "tokens": ["Rasch", "zur", "Ge\u00b7lieb\u00b7ten", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "H\u00e4tte sie schnell ereilt,", "tokens": ["H\u00e4t\u00b7te", "sie", "schnell", "er\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wogte mit stillem Gru\u00df", "tokens": ["Wog\u00b7te", "mit", "stil\u00b7lem", "Gru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Rasch um den lieben Fu\u00df,", "tokens": ["Rasch", "um", "den", "lie\u00b7ben", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bl\u00e4hte mich stolzer dann,", "tokens": ["Bl\u00e4h\u00b7te", "mich", "stol\u00b7zer", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Schw\u00f6lle und stieg' hinan", "tokens": ["Schw\u00f6l\u00b7le", "und", "stieg'", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Bis an des Busens Rund,", "tokens": ["Bis", "an", "des", "Bu\u00b7sens", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Bis an den Purpurmund,", "tokens": ["Bis", "an", "den", "Pur\u00b7pur\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Gr\u00fc\u00dfte und k\u00fc\u00dfte sie,", "tokens": ["Gr\u00fc\u00df\u00b7te", "und", "k\u00fc\u00df\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Kos'te und neckte sie,", "tokens": ["Kos'\u00b7te", "und", "neck\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "Und sie erlitt es gern,", "tokens": ["Und", "sie", "er\u00b7litt", "es", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Glaubt' ja, ich seh' es nicht,", "tokens": ["Glaubt'", "ja", ",", "ich", "seh'", "es", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Glaubt' mich ja fern!", "tokens": ["Glaubt'", "mich", "ja", "fern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt' ich die Quelle sein,", "tokens": ["K\u00f6nnt'", "ich", "die", "Quel\u00b7le", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ganz nach Verlangen", "tokens": ["Ganz", "nach", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "W\u00e4re sie mein;", "tokens": ["W\u00e4\u00b7re", "sie", "mein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Liebend umfangen", "tokens": ["Lie\u00b7bend", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wollt' ich die Holde,", "tokens": ["Wollt'", "ich", "die", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Aber so bald nicht", "tokens": ["A\u00b7ber", "so", "bald", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Lie\u00df ich sie los.", "tokens": ["Lie\u00df", "ich", "sie", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dann zu dem Herzchen", "tokens": ["Dann", "zu", "dem", "Herz\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Rauscht' ich empor,", "tokens": ["Rauscht'", "ich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Pochte und schl\u00fcge", "tokens": ["Poch\u00b7te", "und", "schl\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Rege daran,", "tokens": ["Re\u00b7ge", "da\u00b7ran", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Pochte und fr\u00fcge", "tokens": ["Poch\u00b7te", "und", "fr\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Liebend mich an.", "tokens": ["Lie\u00b7bend", "mich", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Dann zu den H\u00e4nden", "tokens": ["Dann", "zu", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Wogt' ich dahin;", "tokens": ["Wogt'", "ich", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Aber das Ringlein,", "tokens": ["A\u00b7ber", "das", "Rin\u00b7glein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Das sie als fremder", "tokens": ["Das", "sie", "als", "frem\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "KOUS", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Seligkeit Pfand", "tokens": ["Se\u00b7lig\u00b7keit", "Pfand"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Tr\u00e4gt an der kleinen", "tokens": ["Tr\u00e4gt", "an", "der", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Blendenden Hand;", "tokens": ["Blen\u00b7den\u00b7den", "Hand", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Wollt' ich ihr raubend", "tokens": ["Wollt'", "ich", "ihr", "rau\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Tief in der Wogen", "tokens": ["Tief", "in", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "N\u00e4chtliche Brandung", "tokens": ["N\u00e4cht\u00b7li\u00b7che", "Bran\u00b7dung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Heimlich verbergen;", "tokens": ["Heim\u00b7lich", "ver\u00b7ber\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Rauschte zur Hand dann", "tokens": ["Rauschte", "zur", "Hand", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Wieder hinan", "tokens": ["Wie\u00b7der", "hi\u00b7nan"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Und nur ", "tokens": ["Und", "nur"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Lie\u00df ich daran.", "tokens": ["Lie\u00df", "ich", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}