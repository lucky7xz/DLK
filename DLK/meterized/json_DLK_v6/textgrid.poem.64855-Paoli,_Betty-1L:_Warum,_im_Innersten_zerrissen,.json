{"textgrid.poem.64855": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum, im Innersten zerrissen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum, im Innersten zerrissen,", "tokens": ["Wa\u00b7rum", ",", "im", "In\u00b7ners\u00b7ten", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein gramerbleichtes Angesicht", "tokens": ["Mein", "gra\u00b7mer\u00b7bleich\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich von dir wende, willst du wissen?", "tokens": ["Ich", "von", "dir", "wen\u00b7de", ",", "willst", "du", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O frage nicht!", "tokens": ["O", "fra\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ob deiner Stimme Zaubergr\u00fc\u00dfe,", "tokens": ["Ob", "dei\u00b7ner", "Stim\u00b7me", "Zau\u00b7ber\u00b7gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sch\u00f6nheit, die dein Haupt umflicht,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "die", "dein", "Haupt", "um\u00b7flicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr mich verloren ihre S\u00fc\u00dfe?", "tokens": ["F\u00fcr", "mich", "ver\u00b7lo\u00b7ren", "ih\u00b7re", "S\u00fc\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O frage nicht!", "tokens": ["O", "fra\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ob unsre Trennung, schmerzdurchlodert,", "tokens": ["Ob", "uns\u00b7re", "Tren\u00b7nung", ",", "schmerz\u00b7durch\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr die mein Mund nun bebend spricht,", "tokens": ["F\u00fcr", "die", "mein", "Mund", "nun", "be\u00b7bend", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von mir, ob vom Geschick gefordert?", "tokens": ["Von", "mir", ",", "ob", "vom", "Ge\u00b7schick", "ge\u00b7for\u00b7dert", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "O frage nicht!", "tokens": ["O", "fra\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ob, was ich leide, ohn' Verzagen", "tokens": ["Ob", ",", "was", "ich", "lei\u00b7de", ",", "ohn'", "Ver\u00b7za\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und ohne da\u00df vor Qual es bricht,", "tokens": ["Und", "oh\u00b7ne", "da\u00df", "vor", "Qual", "es", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KOUS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein Menschenherz vermag zu tragen?", "tokens": ["Ein", "Men\u00b7schen\u00b7herz", "ver\u00b7mag", "zu", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "O frage nicht!", "tokens": ["O", "fra\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}