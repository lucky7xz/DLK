{"textgrid.poem.52063": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr d\u00fcrstet so: dabey sol man ihn lernen kennen,", "tokens": ["Ihr", "d\u00fcrs\u00b7tet", "so", ":", "da\u00b7bey", "sol", "man", "ihn", "ler\u00b7nen", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$.", "PAV", "VMFIN", "PIS", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sol man den Torstensohn, sprach Criticillus, nennen:", "tokens": ["Sol", "man", "den", "Tors\u00b7ten\u00b7sohn", ",", "sprach", "Cri\u00b7ti\u00b7cil\u00b7lus", ",", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "$,", "VVFIN", "NE", "$,", "VVINF", "$."], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mein Freund ihr irrt: da\u00df er (dis kommt mir be\u00dfer bey)", "tokens": ["Mein", "Freund", "ihr", "irrt", ":", "da\u00df", "er", "(", "dis", "kommt", "mir", "be\u00b7\u00dfer", "bey", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$.", "KOUS", "PPER", "$(", "PDS", "VVFIN", "PPER", "ADJD", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht durstig, sondern gar zu Thurstig vor uns sey.", "tokens": ["Nicht", "durs\u00b7tig", ",", "son\u00b7dern", "gar", "zu", "Thurs\u00b7tig", "vor", "uns", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "w