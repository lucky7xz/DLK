{"textgrid.poem.37015": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu faul, da\u00df f\u00fcr's gemeine Wohl", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu faul, da\u00df f\u00fcr's gemeine Wohl", "tokens": ["Zu", "faul", ",", "da\u00df", "f\u00fcr's", "ge\u00b7mei\u00b7ne", "Wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er jemals sich bem\u00fchte,", "tokens": ["Er", "je\u00b7mals", "sich", "be\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "War's Kegelspiel nur, Wurst und Kohl,", "tokens": ["Wa\u00b7r's", "Ke\u00b7gel\u00b7spiel", "nur", ",", "Wurst", "und", "Kohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr die sein Herz ergl\u00fchte,", "tokens": ["F\u00fcr", "die", "sein", "Herz", "er\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und dennoch war sein drittes Wort:", "tokens": ["Und", "den\u00b7noch", "war", "sein", "drit\u00b7tes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin ein ", "tokens": ["Ich", "bin", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und dennoch wollt' die\u00df Menschenschwein", "tokens": ["Und", "den\u00b7noch", "wollt'", "die\u00df", "Men\u00b7schen\u00b7schwein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Freiherr im freien Staate sein!", "tokens": ["Frei\u00b7herr", "im", "frei\u00b7en", "Staa\u00b7te", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn alle Wir als Menschenkind", "tokens": ["Wenn", "al\u00b7le", "Wir", "als", "Men\u00b7schen\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Hause \u00bbStaub zu Staube\u00ab sind,", "tokens": ["Vom", "Hau\u00b7se", "\u00bb", "Staub", "zu", "Stau\u00b7be", "\u00ab", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "APPR", "NN", "$(", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Er, der ruht auf diesem Fleck,", "tokens": ["Ist", "Er", ",", "der", "ruht", "auf", "die\u00b7sem", "Fleck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bastard vom Geschlechte \u00bbDreck.\u00ab", "tokens": ["Ein", "Bas\u00b7tard", "vom", "Ge\u00b7schlech\u00b7te", "\u00bb", "Dreck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$(", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}