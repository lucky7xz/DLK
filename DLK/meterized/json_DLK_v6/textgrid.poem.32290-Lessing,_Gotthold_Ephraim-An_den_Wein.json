{"textgrid.poem.32290": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "An den Wein", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wein, wenn ich dich jetzo trinke,", "tokens": ["Wein", ",", "wenn", "ich", "dich", "jet\u00b7zo", "trin\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich dich als J\u00fcngling trinke,", "tokens": ["Wenn", "ich", "dich", "als", "J\u00fcng\u00b7ling", "trin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst du mich in allen Sachen", "tokens": ["Sollst", "du", "mich", "in", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dreist und klug, beherzt und weise,", "tokens": ["Dreist", "und", "klug", ",", "be\u00b7herzt", "und", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir zum Nutz, und dir zum Preise,", "tokens": ["Mir", "zum", "Nutz", ",", "und", "dir", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kurz, zu einem Alten machen.", "tokens": ["Kurz", ",", "zu", "ei\u00b7nem", "Al\u00b7ten", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wein, werd' ich dich k\u00fcnftig trinken,", "tokens": ["Wein", ",", "werd'", "ich", "dich", "k\u00fcnf\u00b7tig", "trin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd' ich dich als Alter trinken,", "tokens": ["Werd'", "ich", "dich", "als", "Al\u00b7ter", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst du mich geneigt zum Lachen,", "tokens": ["Sollst", "du", "mich", "ge\u00b7neigt", "zum", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbesorgt f\u00fcr Tod und L\u00fcgen,", "tokens": ["Un\u00b7be\u00b7sorgt", "f\u00fcr", "Tod", "und", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir zum Ruhm, mir zum Vergn\u00fcgen,", "tokens": ["Dir", "zum", "Ruhm", ",", "mir", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kurz, zu einem J\u00fcngling machen.", "tokens": ["Kurz", ",", "zu", "ei\u00b7nem", "J\u00fcng\u00b7ling", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}