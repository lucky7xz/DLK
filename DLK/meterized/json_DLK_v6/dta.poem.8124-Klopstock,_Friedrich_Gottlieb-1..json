{"dta.poem.8124": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.57", "da:0.28", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fanget bebend an, athmet kaum", "tokens": ["Fan\u00b7get", "be\u00b7bend", "an", ",", "ath\u00b7met", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "VVFIN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Leisen Laut; denn es ist Christus Lob,", "tokens": ["Lei\u00b7sen", "Laut", ";", "denn", "es", "ist", "Chris\u00b7tus", "Lob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "$.", "KON", "PPER", "VAFIN", "NE", "NN", "$,"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Was zu singen ihr wagt. Die Ewigkeit", "tokens": ["Was", "zu", "sin\u00b7gen", "ihr", "wagt", ".", "Die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PTKZU", "VVFIN", "PPER", "VVFIN", "$.", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durchstr\u00f6mt\u2019s, t\u00f6nt von Aevon fort zu Aeoon!", "tokens": ["Durch\u00b7str\u00f6mt's", ",", "t\u00f6nt", "von", "A\u00b7e\u00b7von", "fort", "zu", "A\u00b7e\u00b7oon", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "NE", "PTKVZ", "APPR", "NE", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}