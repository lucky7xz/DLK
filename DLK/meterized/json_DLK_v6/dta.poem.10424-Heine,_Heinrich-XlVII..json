{"dta.poem.10424": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Es leuchtet meine Liebe,", "tokens": ["Es", "leuch\u00b7tet", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihrer dunkeln Pracht,", "tokens": ["In", "ih\u00b7rer", "dun\u00b7keln", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie'n M\u00e4hrchen traurig und tr\u00fcbe,", "tokens": ["Wie'n", "M\u00e4hr\u00b7chen", "trau\u00b7rig", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erz\u00e4hlt in der Sommernacht.", "tokens": ["Er\u00b7z\u00e4hlt", "in", "der", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Zaubergarten wallen", "tokens": ["Im", "Zau\u00b7berg\u00b7ar\u00b7ten", "wal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Buhlen, stumm und allein;", "tokens": ["Zwei", "Buh\u00b7len", ",", "stumm", "und", "al\u00b7lein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KON", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es singen die Nachtigallen,", "tokens": ["Es", "sin\u00b7gen", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es flimmert der Mondenschein.", "tokens": ["Es", "flim\u00b7mert", "der", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Jungfrau steht still wie ein Bildni\u00df,", "tokens": ["Die", "Jung\u00b7frau", "steht", "still", "wie", "ein", "Bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Ritter vor ihr kniet.", "tokens": ["Der", "Rit\u00b7ter", "vor", "ihr", "kniet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt der Riese der Wildni\u00df,", "tokens": ["Da", "kommt", "der", "Rie\u00b7se", "der", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die bange Jungfrau flieht.", "tokens": ["Die", "ban\u00b7ge", "Jung\u00b7frau", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Ritter sinkt blutend zur Erde,", "tokens": ["Der", "Rit\u00b7ter", "sinkt", "blu\u00b7tend", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es stolpert der Riese nach Haus;", "tokens": ["Es", "stol\u00b7pert", "der", "Rie\u00b7se", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wenn ich begraben werde,", "tokens": ["Wenn", "ich", "be\u00b7gra\u00b7ben", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann ist das M\u00e4hrchen aus.", "tokens": ["Dann", "ist", "das", "M\u00e4hr\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}