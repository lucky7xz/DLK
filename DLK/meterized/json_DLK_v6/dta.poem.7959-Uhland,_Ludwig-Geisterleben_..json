{"dta.poem.7959": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Geisterleben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Von dir getrennet, lieg\u2019 ich wie begraben,", "tokens": ["Von", "dir", "ge\u00b7tren\u00b7net", ",", "lieg'", "ich", "wie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich gr\u00fc\u00dft kein S\u00e4useln linder Fr\u00fchlingsl\u00fcfte;", "tokens": ["Mich", "gr\u00fc\u00dft", "kein", "S\u00e4u\u00b7seln", "lin\u00b7der", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Lerchensang, kein Balsam s\u00fc\u00dfer D\u00fcfte,", "tokens": ["Kein", "Ler\u00b7chen\u00b7sang", ",", "kein", "Bal\u00b7sam", "s\u00fc\u00b7\u00dfer", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Stral der Morgensonne kann mich laben.", "tokens": ["Kein", "Stral", "der", "Mor\u00b7gen\u00b7son\u00b7ne", "kann", "mich", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wann sich die Lebenden dem Schlummer gaben,", "tokens": ["Wann", "sich", "die", "Le\u00b7ben\u00b7den", "dem", "Schlum\u00b7mer", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann Todte steigen aus dem Schoo\u00df der Gr\u00fcfte,", "tokens": ["Wann", "Tod\u00b7te", "stei\u00b7gen", "aus", "dem", "Schoo\u00df", "der", "Gr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann schweb\u2019 ich tr\u00e4umend \u00fcber H\u00f6hn und Kl\u00fcfte,", "tokens": ["Dann", "schweb'", "ich", "tr\u00e4u\u00b7mend", "\u00fc\u00b7ber", "H\u00f6hn", "und", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mich so fern von dir gedr\u00e4nget haben.", "tokens": ["Die", "mich", "so", "fern", "von", "dir", "ge\u00b7dr\u00e4n\u00b7get", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Durch den verbotnen Garten darf ich gehen,", "tokens": ["Durch", "den", "ver\u00b7bot\u00b7nen", "Gar\u00b7ten", "darf", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Th\u00fcren wandl\u2019 ich, die mir sonst verriegelt,", "tokens": ["Durch", "Th\u00fc\u00b7ren", "wandl'", "ich", ",", "die", "mir", "sonst", "ver\u00b7rie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis zu der Sch\u00f6nheit stillem Heiligthume.", "tokens": ["Bis", "zu", "der", "Sch\u00f6n\u00b7heit", "stil\u00b7lem", "Hei\u00b7lig\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erschreckt dich Geisterhauch, du zarte Blume?", "tokens": ["Er\u00b7schreckt", "dich", "Geis\u00b7ter\u00b7hauch", ",", "du", "zar\u00b7te", "Blu\u00b7me", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist der Liebe Wehn, das dich umfl\u00fcgelt.", "tokens": ["Es", "ist", "der", "Lie\u00b7be", "Wehn", ",", "das", "dich", "um\u00b7fl\u00fc\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leb wohl! ich mu\u00df in\u2019s Grab, die H\u00e4hne kr\u00e4hen.", "tokens": ["Leb", "wohl", "!", "ich", "mu\u00df", "in's", "Grab", ",", "die", "H\u00e4h\u00b7ne", "kr\u00e4\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VMFIN", "APPRART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}