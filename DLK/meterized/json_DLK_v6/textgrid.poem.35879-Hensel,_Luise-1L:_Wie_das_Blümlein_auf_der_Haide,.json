{"textgrid.poem.35879": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie das Bl\u00fcmlein auf der Haide,", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das Bl\u00fcmlein auf der Haide,", "tokens": ["Wie", "das", "Bl\u00fcm\u00b7lein", "auf", "der", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Veilchen still im Thal,", "tokens": ["Wie", "das", "Veil\u00b7chen", "still", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das L\u00e4mmlein auf der Weide", "tokens": ["Wie", "das", "L\u00e4mm\u00b7lein", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich erfreut am Sonnenstrahl,", "tokens": ["Sich", "er\u00b7freut", "am", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die muntern V\u00f6glein singen,", "tokens": ["Wie", "die", "mun\u00b7tern", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ein B\u00e4chlein sich ergie\u00dft,", "tokens": ["Wo", "ein", "B\u00e4ch\u00b7lein", "sich", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Frohe Kindlein Kr\u00e4nze schlingen:", "tokens": ["Fro\u00b7he", "Kin\u00b7dlein", "Kr\u00e4n\u00b7ze", "schlin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So sei Du, sei Du gegr\u00fc\u00dft.", "tokens": ["So", "sei", "Du", ",", "sei", "Du", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "O Maria! lieblich t\u00f6net", "tokens": ["O", "Ma\u00b7ria", "!", "lieb\u00b7lich", "t\u00f6\u00b7net"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir der holde Name Dein.", "tokens": ["Mir", "der", "hol\u00b7de", "Na\u00b7me", "Dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O Maria! nach Dir sehnet", "tokens": ["O", "Ma\u00b7ria", "!", "nach", "Dir", "seh\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Immerdar die Seele mein.", "tokens": ["Im\u00b7mer\u00b7dar", "die", "See\u00b7le", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O der Wonne, die dort dr\u00fcben", "tokens": ["O", "der", "Won\u00b7ne", ",", "die", "dort", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Seele wartet nun,", "tokens": ["Mei\u00b7ner", "See\u00b7le", "war\u00b7tet", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich zu schauen, Dich zu lieben,", "tokens": ["Dich", "zu", "schau\u00b7en", ",", "Dich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig Dir im Arm zu ruhn!", "tokens": ["E\u00b7wig", "Dir", "im", "Arm", "zu", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}