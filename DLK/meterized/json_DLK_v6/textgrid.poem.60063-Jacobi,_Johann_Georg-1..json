{"textgrid.poem.60063": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ersten Lerchen sangen:", "tokens": ["Die", "ers\u00b7ten", "Ler\u00b7chen", "san\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da k\u00fc\u00dft' ich deine Wangen,", "tokens": ["Da", "k\u00fc\u00dft'", "ich", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fragte: Liebst du mich?", "tokens": ["Und", "frag\u00b7te", ":", "Liebst", "du", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ersten Zephyrs wehten:", "tokens": ["Die", "ers\u00b7ten", "Ze\u00b7phyrs", "weh\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da sagte dein Err\u00f6then:", "tokens": ["Da", "sag\u00b7te", "dein", "Er\u00b7r\u00f6\u00b7then", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich liebe dich!", "tokens": ["Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da warst du ganz die meine;", "tokens": ["Da", "warst", "du", "ganz", "die", "mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da rauschten es die Haine;", "tokens": ["Da", "rauschten", "es", "die", "Hai\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die B\u00e4che priesen mich,", "tokens": ["Die", "B\u00e4\u00b7che", "prie\u00b7sen", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und murmelten vertrauter;", "tokens": ["Und", "mur\u00b7mel\u00b7ten", "ver\u00b7trau\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Die Lerchen sangen lauter:", "tokens": ["Die", "Ler\u00b7chen", "san\u00b7gen", "lau\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich liebe dich!", "tokens": ["Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und Epheuranken hingen", "tokens": ["Und", "E\u00b7pheu\u00b7ran\u00b7ken", "hin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An jedem Baum, und fingen,", "tokens": ["An", "je\u00b7dem", "Baum", ",", "und", "fin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In s\u00fc\u00dfer Irre, sich", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Ir\u00b7re", ",", "sich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vor Wollust an zu regen;", "tokens": ["Vor", "Wol\u00b7lust", "an", "zu", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie bebten mir entgegen:", "tokens": ["Sie", "beb\u00b7ten", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich liebe dich!", "tokens": ["Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Gepaarte Blumen standen", "tokens": ["Ge\u00b7paar\u00b7te", "Blu\u00b7men", "stan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im gr\u00fcnen Thal, empfanden,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Thal", ",", "emp\u00b7fan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00fc\u00dften schwesterlich", "tokens": ["Und", "k\u00fc\u00df\u00b7ten", "schwes\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich meiner Chloe wegen;", "tokens": ["Sich", "mei\u00b7ner", "Chloe", "we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "APPR", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie hauchten mir entgegen:", "tokens": ["Sie", "hauch\u00b7ten", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich liebe dich!", "tokens": ["Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Vereinte W\u00f6lkchen mahlten", "tokens": ["Ver\u00b7ein\u00b7te", "W\u00f6lk\u00b7chen", "mahl\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Himmel; sie umstrahlten", "tokens": ["Den", "Him\u00b7mel", ";", "sie", "um\u00b7strahl\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Abendglanze sich,", "tokens": ["Im", "A\u00b7bend\u00b7glan\u00b7ze", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der nie so sch\u00f6n gewesen;", "tokens": ["Der", "nie", "so", "sch\u00f6n", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Himmel war zu lesen:", "tokens": ["Am", "Him\u00b7mel", "war", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich liebe dich!", "tokens": ["Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Als nach und nach die Farben", "tokens": ["Als", "nach", "und", "nach", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In D\u00e4mmerung erstarben,", "tokens": ["In", "D\u00e4m\u00b7me\u00b7rung", "er\u00b7star\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die letzte Sonne wich;", "tokens": ["Die", "letz\u00b7te", "Son\u00b7ne", "wich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O wie so lachend blinkten", "tokens": ["O", "wie", "so", "la\u00b7chend", "blink\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sterne noch, und winkten:", "tokens": ["Die", "Ster\u00b7ne", "noch", ",", "und", "wink\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich liebe dich!", "tokens": ["Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}