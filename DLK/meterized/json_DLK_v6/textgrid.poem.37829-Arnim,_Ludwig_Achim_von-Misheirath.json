{"textgrid.poem.37829": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Misheirath", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie Wasserr\u00fcben und der Kohl,", "tokens": ["\u00bb", "die", "Was\u00b7ser\u00b7r\u00fc\u00b7ben", "und", "der", "Kohl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die haben mich vertrieben wohl,", "tokens": ["Die", "ha\u00b7ben", "mich", "ver\u00b7trie\u00b7ben", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt' meine Mutter Fleisch gekocht,", "tokens": ["H\u00e4tt'", "mei\u00b7ne", "Mut\u00b7ter", "Fleisch", "ge\u00b7kocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00e4r geblieben immer noch.", "tokens": ["Ich", "w\u00e4r", "ge\u00b7blie\u00b7ben", "im\u00b7mer", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich nur einmal J\u00e4ger w\u00e4r,", "tokens": ["Wenn", "ich", "nur", "ein\u00b7mal", "J\u00e4\u00b7ger", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drey sch\u00f6ne Flinten kauft ich mir,", "tokens": ["Drey", "sch\u00f6\u00b7ne", "Flin\u00b7ten", "kauft", "ich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drey sch\u00f6ne Flinten, einen Hund,", "tokens": ["Drey", "sch\u00f6\u00b7ne", "Flin\u00b7ten", ",", "ei\u00b7nen", "Hund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sch\u00f6nes M\u00e4dchen kugelrund.\u00ab", "tokens": ["Ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "ku\u00b7gel\u00b7rund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die sch\u00f6ne J\u00e4grin fand er bald,", "tokens": ["Die", "sch\u00f6\u00b7ne", "J\u00e4g\u00b7rin", "fand", "er", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem Weg im dichten Wald,", "tokens": ["Auf", "sei\u00b7nem", "Weg", "im", "dich\u00b7ten", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jungfer war wohl kugelrund,", "tokens": ["Die", "Jung\u00b7fer", "war", "wohl", "ku\u00b7gel\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie nahm ihn ohne Flint und Hund.", "tokens": ["Sie", "nahm", "ihn", "oh\u00b7ne", "Flint", "und", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er geht mit ihr vor Mutters Haus,", "tokens": ["Er", "geht", "mit", "ihr", "vor", "Mut\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter guckt zum Schornstein raus:", "tokens": ["Die", "Mut\u00b7ter", "guckt", "zum", "Schorn\u00b7stein", "raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbach Sohn! ach lieber Sohne mein,", "tokens": ["\u00bb", "ach", "Sohn", "!", "ach", "lie\u00b7ber", "Soh\u00b7ne", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$.", "ADV", "ADV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was bringst mir f\u00fcr ein Stachelschwein?\u00ab", "tokens": ["Was", "bringst", "mir", "f\u00fcr", "ein", "Sta\u00b7chel\u00b7schwein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbes ist f\u00fcrwahr kein Stachelschwein,", "tokens": ["\u00bb", "es", "ist", "f\u00fcr\u00b7wahr", "kein", "Sta\u00b7chel\u00b7schwein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist die Herzallerliebste mein!\u00ab", "tokens": ["Es", "ist", "die", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "mein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbist es die Herzallerliebste dein,", "tokens": ["\u00bb", "ist", "es", "die", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "dein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bring sie zu mir in Saal herein.", "tokens": ["Bring", "sie", "zu", "mir", "in", "Saal", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich will auftragen R\u00fcb und Kohl.\u00ab", "tokens": ["Ich", "will", "auf\u00b7tra\u00b7gen", "R\u00fcb", "und", "Kohl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbfrau Mutter, das der Henker hohl,", "tokens": ["\u00bb", "frau", "Mut\u00b7ter", ",", "das", "der", "Hen\u00b7ker", "hohl", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin Mosje, den Kohl veracht,", "tokens": ["Ich", "bin", "Mos\u00b7je", ",", "den", "Kohl", "ver\u00b7acht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schl\u00fcssel gebt, das Huhn ich schlacht.\u00ab", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "gebt", ",", "das", "Huhn", "ich", "schlacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Alte h\u00e4lt den Jungen auf,", "tokens": ["Die", "Al\u00b7te", "h\u00e4lt", "den", "Jun\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Springt zu und h\u00e4lt zehn Finger drauf:", "tokens": ["Springt", "zu", "und", "h\u00e4lt", "zehn", "Fin\u00b7ger", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdu Bub, das H\u00fchnlein leget frey", "tokens": ["\u00bb", "du", "Bub", ",", "das", "H\u00fchn\u00b7lein", "le\u00b7get", "frey"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "NN", "$,", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir alle Tag vier golden Ey.", "tokens": ["Mir", "al\u00b7le", "Tag", "vier", "gol\u00b7den", "Ey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Bub will alle Tage mehr,", "tokens": ["Der", "Bub", "will", "al\u00b7le", "Ta\u00b7ge", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun schleppt er gar ein M\u00e4dchen her.\u00ab", "tokens": ["Nun", "schleppt", "er", "gar", "ein", "M\u00e4d\u00b7chen", "her", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbnun dann Frau Mutter gebet her,", "tokens": ["\u00bb", "nun", "dann", "Frau", "Mut\u00b7ter", "ge\u00b7bet", "her", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein ander Fleisch, das ich verehr.\u00ab", "tokens": ["Ein", "an\u00b7der", "Fleisch", ",", "das", "ich", "ver\u00b7ehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Alte winkt ihm freundlich zu,", "tokens": ["Die", "Al\u00b7te", "winkt", "ihm", "freund\u00b7lich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sohn sich setzt in guter Ruh,", "tokens": ["Der", "Sohn", "sich", "setzt", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schlachtet einen Kater ab,", "tokens": ["Sie", "schlach\u00b7tet", "ei\u00b7nen", "Ka\u00b7ter", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bratet ihn am Zauberstab.", "tokens": ["Und", "bra\u00b7tet", "ihn", "am", "Zau\u00b7ber\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die J\u00e4grin sprach: \u00bbHerr Br\u00e4utigam,", "tokens": ["Die", "J\u00e4g\u00b7rin", "sprach", ":", "\u00bb", "Herr", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solch Wildprett ist mir gar zu zahm,", "tokens": ["Solch", "Wild\u00b7prett", "ist", "mir", "gar", "zu", "zahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es widersteht mir dies Geschlecht,", "tokens": ["Es", "wi\u00b7der\u00b7steht", "mir", "dies", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bleib Mamsell und e\u00df was recht.\u00ab", "tokens": ["Ich", "bleib", "Mam\u00b7sell", "und", "e\u00df", "was", "recht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "PWS", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbwas Wildpret!\u00ab schreit der Br\u00e4utigam,", "tokens": ["\u00bb", "was", "Wild\u00b7pret", "!", "\u00ab", "schreit", "der", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$.", "$(", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbder Kater war von edlem Stamm,", "tokens": ["\u00bb", "der", "Ka\u00b7ter", "war", "von", "ed\u00b7lem", "Stamm", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies ist und bleibt das Wildpret mein!\u00ab", "tokens": ["Dies", "ist", "und", "bleibt", "das", "Wild\u00b7pret", "mein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "KON", "VVFIN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die J\u00e4grin l\u00e4uft in'n Wald hinein.", "tokens": ["Die", "J\u00e4g\u00b7rin", "l\u00e4uft", "in'n", "Wald", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbwas doch der Braut mocht kommen ein,", "tokens": ["\u00bb", "was", "doch", "der", "Braut", "mocht", "kom\u00b7men", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "ART", "NN", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Weggehn war nun gar nicht fein!\u00ab", "tokens": ["Das", "Weg\u00b7gehn", "war", "nun", "gar", "nicht", "fein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie setzen sich zum Braten hin,", "tokens": ["Sie", "set\u00b7zen", "sich", "zum", "Bra\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uneins und doch in einem Sinn.", "tokens": ["Un\u00b7eins", "und", "doch", "in", "ei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Die Alte lehrt den Sohn beim Mahl:", "tokens": ["Die", "Al\u00b7te", "lehrt", "den", "Sohn", "beim", "Mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdie Welt wird vornehm auf einmal,", "tokens": ["\u00bb", "die", "Welt", "wird", "vor\u00b7nehm", "auf", "ein\u00b7mal", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir war die magre Wildkatz recht,", "tokens": ["Dir", "war", "die", "mag\u00b7re", "Wild\u00b7katz", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr schien der fette Kater schlecht.\u00ab", "tokens": ["Ihr", "schien", "der", "fet\u00b7te", "Ka\u00b7ter", "schlecht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}