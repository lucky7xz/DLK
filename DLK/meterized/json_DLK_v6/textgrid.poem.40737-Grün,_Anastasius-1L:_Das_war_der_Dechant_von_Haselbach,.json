, "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "APPR", "ADV", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Studenten auf Wanderpfaden.", "tokens": ["Stu\u00b7den\u00b7ten", "auf", "Wan\u00b7der\u00b7pfa\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Und der Euch gesungen diesen Reih'n,", "tokens": ["Und", "der", "Euch", "ge\u00b7sun\u00b7gen", "die\u00b7sen", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVPP", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "War selber bei der Geschichte,", "tokens": ["War", "sel\u00b7ber", "bei", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "War Einer von den fahrenden Drei'n;", "tokens": ["War", "Ei\u00b7ner", "von", "den", "fah\u00b7ren\u00b7den", "Drei'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er hat getrunken des Dechants Wein,", "tokens": ["Er", "hat", "ge\u00b7trun\u00b7ken", "des", "De\u00b7ch\u00b7ants", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gek\u00fc\u00dft des Dechants Nichte.", "tokens": ["Ge\u00b7k\u00fc\u00dft", "des", "De\u00b7ch\u00b7ants", "Nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+