{"textgrid.poem.40737": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war der Dechant von Haselbach,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war der Dechant von Haselbach,", "tokens": ["Das", "war", "der", "De\u00b7chant", "von", "Ha\u00b7sel\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der gastfrei' und ehrenfeste,", "tokens": ["Der", "gast\u00b7frei'", "und", "eh\u00b7ren\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er segnet beim Opfer Brod und Wein,", "tokens": ["Er", "seg\u00b7net", "beim", "Op\u00b7fer", "Brod", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch trinkt und i\u00dft er nicht gern allein,", "tokens": ["Doch", "trinkt", "und", "i\u00dft", "er", "nicht", "gern", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und denkt schon der kommenden G\u00e4ste.", "tokens": ["Und", "denkt", "schon", "der", "kom\u00b7men\u00b7den", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Da steht mit dem K\u00e4nnlein der Ministrant", "tokens": ["Da", "steht", "mit", "dem", "K\u00e4nn\u00b7lein", "der", "Mi\u00b7nis\u00b7trant"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und fl\u00fcstert ins Ohr ihm leise:", "tokens": ["Und", "fl\u00fcs\u00b7tert", "ins", "Ohr", "ihm", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbsie kommen nicht! Denn der Eine jagt,", "tokens": ["\u00bb", "sie", "kom\u00b7men", "nicht", "!", "Denn", "der", "Ei\u00b7ne", "jagt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$.", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Andr' erwartet die neue Magd,", "tokens": ["Der", "Andr'", "er\u00b7war\u00b7tet", "die", "neu\u00b7e", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Dritte r\u00fcstet zur Reise.\u00ab", "tokens": ["Der", "Drit\u00b7te", "r\u00fcs\u00b7tet", "zur", "Rei\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dem Alten entglitt der Me\u00dfkelch fast,", "tokens": ["Dem", "Al\u00b7ten", "ent\u00b7glitt", "der", "Me\u00df\u00b7kelch", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des heiligen Orts vergessen:", "tokens": ["Des", "hei\u00b7li\u00b7gen", "Orts", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbder Dachs im Bau nur schmaust allein,", "tokens": ["\u00bb", "der", "Dachs", "im", "Bau", "nur", "schmaust", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da lad' ich mir lieber drei Teufel ein!\u00ab", "tokens": ["Da", "lad'", "ich", "mir", "lie\u00b7ber", "drei", "Teu\u00b7fel", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "CARD", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Im Schmerze schw\u00f6rt er's vermessen.", "tokens": ["Im", "Schmer\u00b7ze", "schw\u00f6rt", "er's", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch kaum gesprochen bereut er's schon;", "tokens": ["Doch", "kaum", "ge\u00b7spro\u00b7chen", "be\u00b7reut", "er's", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Pfarrhaus sitzt er jetzt betend,", "tokens": ["Im", "Pfarr\u00b7haus", "sitzt", "er", "jetzt", "be\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da klappert im Hofe Pferdegetrab,", "tokens": ["Da", "klap\u00b7pert", "im", "Ho\u00b7fe", "Pfer\u00b7de\u00b7ge\u00b7trab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drei seltsame Junker springen ab,", "tokens": ["Drei", "selt\u00b7sa\u00b7me", "Jun\u00b7ker", "sprin\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Flink in die Hausflur tretend.", "tokens": ["Flink", "in", "die", "Haus\u00b7flur", "tre\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er seufzt: \u00bbAha, da sind sie schon!\u00ab", "tokens": ["Er", "seufzt", ":", "\u00bb", "A\u00b7ha", ",", "da", "sind", "sie", "schon", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch artiglich gr\u00fc\u00dfen die andern:", "tokens": ["Doch", "ar\u00b7tig\u00b7lich", "gr\u00fc\u00b7\u00dfen", "die", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbwir h\u00f6rten vom gastlichen geistlichen Herrn", "tokens": ["\u00bb", "wir", "h\u00f6r\u00b7ten", "vom", "gast\u00b7li\u00b7chen", "geist\u00b7li\u00b7chen", "Herrn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und l\u00fcden auch uns zu Tische gern", "tokens": ["Und", "l\u00fc\u00b7den", "auch", "uns", "zu", "Ti\u00b7sche", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPR", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit Hunger und Durst vom Wandern.\u00ab", "tokens": ["Mit", "Hun\u00b7ger", "und", "Durst", "vom", "Wan\u00b7dern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Er nickt sein Ja, schl\u00e4gt still sein Kreuz", "tokens": ["Er", "nickt", "sein", "Ja", ",", "schl\u00e4gt", "still", "sein", "Kreuz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00df sich schnell zu fassen;", "tokens": ["Und", "wei\u00df", "sich", "schnell", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch reicht er den G\u00e4sten nicht die Hand,", "tokens": ["Doch", "reicht", "er", "den", "G\u00e4s\u00b7ten", "nicht", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In ihrem Handschuh glimmt ja ein Brand,", "tokens": ["In", "ih\u00b7rem", "Hand\u00b7schuh", "glimmt", "ja", "ein", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Drum wagt er nicht ihn zu fassen.", "tokens": ["Drum", "wagt", "er", "nicht", "ihn", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Er mustert die Drei vom Scheitel zur Zeh,", "tokens": ["Er", "mus\u00b7tert", "die", "Drei", "vom", "Schei\u00b7tel", "zur", "Zeh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "CARD", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein B\u00fcschlein am Hut tr\u00e4gt jeder,", "tokens": ["Ein", "B\u00fc\u00b7schlein", "am", "Hut", "tr\u00e4gt", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PIS", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Das Schuhwerk scheint nicht von zierlichstem Bau,", "tokens": ["Das", "Schuh\u00b7werk", "scheint", "nicht", "von", "zier\u00b7lichs\u00b7tem", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Pferdfu\u00df drunter erkennt er genau,", "tokens": ["Den", "Pferd\u00b7fu\u00df", "drun\u00b7ter", "er\u00b7kennt", "er", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie oben die Hahnenfeder.", "tokens": ["Wie", "o\u00b7ben", "die", "Hah\u00b7nen\u00b7fe\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Er denkt: die Mahlzeit verleid' ich euch,", "tokens": ["Er", "denkt", ":", "die", "Mahl\u00b7zeit", "ver\u00b7leid'", "ich", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr sollt's nicht zweimal wagen!", "tokens": ["Ihr", "sollt's", "nicht", "zwei\u00b7mal", "wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann winkt er den Me\u00dfnerjungen herbei:", "tokens": ["Dann", "winkt", "er", "den", "Me\u00df\u00b7ner\u00b7jun\u00b7gen", "her\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbzieh deinen Chorrock an als Livrei", "tokens": ["\u00bb", "zieh", "dei\u00b7nen", "Chor\u00b7rock", "an", "als", "Liv\u00b7rei"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "APPR", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und rothen Talar und Kragen.", "tokens": ["Und", "ro\u00b7then", "Ta\u00b7lar", "und", "Kra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ins Salzfa\u00df streu' Sankt Stefanssalz,", "tokens": ["Ins", "Salz\u00b7fa\u00df", "streu'", "Sankt", "Ste\u00b7fans\u00b7salz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kruzifix begleit' es,", "tokens": ["Ein", "Kru\u00b7zi\u00b7fix", "be\u00b7gleit'", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gie\u00df' Weihbrunn in die Kannen ein,", "tokens": ["Gie\u00df'", "Weih\u00b7brunn", "in", "die", "Kan\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kr\u00fcge f\u00fcll' mit Kirchenwein,", "tokens": ["Die", "Kr\u00fc\u00b7ge", "f\u00fcll'", "mit", "Kir\u00b7chen\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Imbi\u00df bring' nur Geweihtes.\u00ab", "tokens": ["Zum", "Im\u00b7bi\u00df", "bring'", "nur", "Ge\u00b7weih\u00b7tes", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Me\u00dfgl\u00f6cklein rufen die Junker zum Mal,", "tokens": ["Me\u00df\u00b7gl\u00f6c\u00b7klein", "ru\u00b7fen", "die", "Jun\u00b7ker", "zum", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Doch tafeln sie unerschrocken;", "tokens": ["Doch", "ta\u00b7feln", "sie", "un\u00b7er\u00b7schro\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weihwasser lassen sie Wasser sein,", "tokens": ["Weih\u00b7was\u00b7ser", "las\u00b7sen", "sie", "Was\u00b7ser", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "VAINF", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sie tauchen den Gaum in den Opferwein,", "tokens": ["Sie", "tau\u00b7chen", "den", "Gaum", "in", "den", "Op\u00b7fer\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ins heilige Salz die Brocken.", "tokens": ["Ins", "hei\u00b7li\u00b7ge", "Salz", "die", "Bro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Und Abend wird's; vom Altare holt", "tokens": ["Und", "A\u00b7bend", "wird's", ";", "vom", "Al\u00b7ta\u00b7re", "holt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Knabe geweihte Kerzen;", "tokens": ["Der", "Kna\u00b7be", "ge\u00b7weih\u00b7te", "Ker\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie z\u00fcnden am Licht die Pfeifen an,", "tokens": ["Sie", "z\u00fcn\u00b7den", "am", "Licht", "die", "Pfei\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verschwinden in Nebeln und Wolken dann,", "tokens": ["Ver\u00b7schwin\u00b7den", "in", "Ne\u00b7beln", "und", "Wol\u00b7ken", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Man h\u00f6rt nur ihr Singen und Scherzen.", "tokens": ["Man", "h\u00f6rt", "nur", "ihr", "Sin\u00b7gen", "und", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.12": {"line.1": {"text": "Wie er so tapfer sie zechen sieht,", "tokens": ["Wie", "er", "so", "tap\u00b7fer", "sie", "ze\u00b7chen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dem Dechant beginnt zu bangen:", "tokens": ["Dem", "De\u00b7chant", "be\u00b7ginnt", "zu", "ban\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbdie Zeiten werden gar schlimm und schwer,", "tokens": ["\u00bb", "die", "Zei\u00b7ten", "wer\u00b7den", "gar", "schlimm", "und", "schwer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Selbst Teufel glauben an gar nichts mehr!", "tokens": ["Selbst", "Teu\u00b7fel", "glau\u00b7ben", "an", "gar", "nichts", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ADV", "PIS", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mein Mittel will nicht verfangen.\u00ab", "tokens": ["Mein", "Mit\u00b7tel", "will", "nicht", "ver\u00b7fan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.13": {"line.1": {"text": "Da w\u00fcnschen die Junker ihm: \u00bbWohl bekomm's!\u00ab", "tokens": ["Da", "w\u00fcn\u00b7schen", "die", "Jun\u00b7ker", "ihm", ":", "\u00bb", "Wohl", "be\u00b7kom\u00b7m's", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$.", "$(", "ADV", "NE", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und danken f\u00fcr Trank und Speisen:", "tokens": ["Und", "dan\u00b7ken", "f\u00fcr", "Trank", "und", "Spei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbwenn wir dereinst im eigenen Haus,", "tokens": ["\u00bb", "wenn", "wir", "de\u00b7reinst", "im", "ei\u00b7ge\u00b7nen", "Haus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vergelten wir gern den heutigen Schmaus,", "tokens": ["Ver\u00b7gel\u00b7ten", "wir", "gern", "den", "heu\u00b7ti\u00b7gen", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann wollt uns die Ehr' erweisen.\u00ab", "tokens": ["Dann", "wollt", "uns", "die", "Ehr'", "er\u00b7wei\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "\u00bbverzeiht, ihr Herrn; mir thun nicht gut", "tokens": ["\u00bb", "ver\u00b7zeiht", ",", "ihr", "Herrn", ";", "mir", "thun", "nicht", "gut"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00fcberheizten Gem\u00e4cher;", "tokens": ["Die", "\u00fc\u00b7berh\u00b7eiz\u00b7ten", "Ge\u00b7m\u00e4\u00b7cher", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch schmeckt verbrannter Braten nicht fein,", "tokens": ["Auch", "schmeckt", "ver\u00b7brann\u00b7ter", "Bra\u00b7ten", "nicht", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.