{"textgrid.poem.42296": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der kluge Held", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tags vor der Schlacht ger\u00e4t ein junger Held", "tokens": ["Tags", "vor", "der", "Schlacht", "ge\u00b7r\u00e4t", "ein", "jun\u00b7ger", "Held"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "In allerlei bedenkliche Bewegung;", "tokens": ["In", "al\u00b7ler\u00b7lei", "be\u00b7denk\u00b7li\u00b7che", "Be\u00b7we\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nimmt dies und das in ernste \u00dcberlegung", "tokens": ["Nimmt", "dies", "und", "das", "in", "erns\u00b7te", "\u00dc\u00b7ber\u00b7le\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "KON", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bringt heraus: Dein bi\u00dfchen L\u00f6hnungsgeld", "tokens": ["Und", "bringt", "he\u00b7raus", ":", "Dein", "bi\u00df\u00b7chen", "L\u00f6h\u00b7nungs\u00b7geld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Lumpenruhm, mein guter K\u00f6nig,", "tokens": ["Und", "Lum\u00b7pen\u00b7ruhm", ",", "mein", "gu\u00b7ter", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Reizt wahrlich unsereinen wenig,", "tokens": ["Reizt", "wahr\u00b7lich", "un\u00b7ser\u00b7ei\u00b7nen", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er daf\u00fcr im Mordgemetzel f\u00e4llt! \u2013", "tokens": ["Da\u00df", "er", "da\u00b7f\u00fcr", "im", "Mord\u00b7ge\u00b7met\u00b7zel", "f\u00e4llt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als er kaum fertig ist mit Gr\u00fcbeln,", "tokens": ["Als", "er", "kaum", "fer\u00b7tig", "ist", "mit", "Gr\u00fc\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "L\u00e4uft er zum Chef: \u00bbSie werdens nicht ver\u00fcbeln,", "tokens": ["L\u00e4uft", "er", "zum", "Chef", ":", "\u00bb", "Sie", "wer\u00b7dens", "nicht", "ver\u00b7\u00fc\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "PPER", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df ich, zu meinem bittersten Verdru\u00df,", "tokens": ["Da\u00df", "ich", ",", "zu", "mei\u00b7nem", "bit\u00b7ters\u00b7ten", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.11": {"text": "Gerade jetzt um Urlaub bitten mu\u00df.", "tokens": ["Ge\u00b7ra\u00b7de", "jetzt", "um", "Ur\u00b7laub", "bit\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Denn ach! mein Vater liegt an Todesenden nieder,", "tokens": ["Denn", "ach", "!", "mein", "Va\u00b7ter", "liegt", "an", "To\u00b7de\u00b7sen\u00b7den", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So schreibt man mir; ich seh' ihn sonst nicht wieder;", "tokens": ["So", "schreibt", "man", "mir", ";", "ich", "seh'", "ihn", "sonst", "nicht", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und ihn verlangt nach mir und meinem letzten Gru\u00df;", "tokens": ["Und", "ihn", "ver\u00b7langt", "nach", "mir", "und", "mei\u00b7nem", "letz\u00b7ten", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O g\u00f6nnen Sie mir seinen Abschiedsku\u00df!\u00ab \u2013", "tokens": ["O", "g\u00f6n\u00b7nen", "Sie", "mir", "sei\u00b7nen", "Ab\u00b7schieds\u00b7ku\u00df", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVINF", "PPER", "PPER", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbsehr wohl! versetzt der Chef, und l\u00e4chelt vor sich nieder;", "tokens": ["\u00bb", "sehr", "wohl", "!", "ver\u00b7setzt", "der", "Chef", ",", "und", "l\u00e4\u00b7chelt", "vor", "sich", "nie\u00b7der", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Reis' hurtig ab, mein Sohn! Denn nach der Bibel mu\u00df", "tokens": ["Reis'", "hur\u00b7tig", "ab", ",", "mein", "Sohn", "!", "Denn", "nach", "der", "Bi\u00b7bel", "mu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "KON", "APPR", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Vater nach Geb\u00fchr von dir geehret werden,", "tokens": ["Dein", "Va\u00b7ter", "nach", "Ge\u00b7b\u00fchr", "von", "dir", "ge\u00b7eh\u00b7ret", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf da\u00df dirs wohlergeh' und du lang' leb'st auf Erden.\u00ab", "tokens": ["Auf", "da\u00df", "dirs", "wohl\u00b7er\u00b7geh'", "und", "du", "lang'", "le\u00b7b'st", "auf", "Er\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "PIS", "VVFIN", "KON", "PPER", "ADV", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}