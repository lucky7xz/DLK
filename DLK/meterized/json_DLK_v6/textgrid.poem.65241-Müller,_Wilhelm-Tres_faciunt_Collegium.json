{"textgrid.poem.65241": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tres faciunt Collegium", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir zwei und ein Pokal!", "tokens": ["Wir", "zwei", "und", "ein", "Po\u00b7kal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zwei sitzen, Einer geht herum", "tokens": ["Zwei", "sit\u00b7zen", ",", "Ei\u00b7ner", "geht", "he\u00b7rum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "VVINF", "$,", "PIS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unsrer vollen Zahl.", "tokens": ["In", "uns\u00b7rer", "vol\u00b7len", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und einig sind wir alle drei,", "tokens": ["Und", "ei\u00b7nig", "sind", "wir", "al\u00b7le", "drei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PIAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Rebensaft kein Wasser sei.", "tokens": ["Da\u00df", "Re\u00b7ben\u00b7saft", "kein", "Was\u00b7ser", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lisett' und ich sind zwei,", "tokens": ["Li\u00b7sett'", "und", "ich", "sind", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "VAFIN", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Nachtigall ist auch nicht stumm,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ist", "auch", "nicht", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und also werden's drei.", "tokens": ["Und", "al\u00b7so", "wer\u00b7den's", "drei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und einig sind wir ohne Frist,", "tokens": ["Und", "ei\u00b7nig", "sind", "wir", "oh\u00b7ne", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df es am Abend d\u00fcster ist.", "tokens": ["Da\u00df", "es", "am", "A\u00b7bend", "d\u00fcs\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Doktor, ein Barbier,", "tokens": ["Ein", "Dok\u00b7tor", ",", "ein", "Bar\u00b7bier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ich dazu, macht um und um", "tokens": ["Und", "ich", "da\u00b7zu", ",", "macht", "um", "und", "um"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PAV", "$,", "VVFIN", "PTKVZ", "KON", "KOUI"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Eins weniger als vier.", "tokens": ["Eins", "we\u00b7ni\u00b7ger", "als", "vier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KOKOM", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und einig sind wir ohne Noth:", "tokens": ["Und", "ei\u00b7nig", "sind", "wir", "oh\u00b7ne", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es w\u00e4chst kein Kraut uns f\u00fcr den Tod.", "tokens": ["Es", "w\u00e4chst", "kein", "Kraut", "uns", "f\u00fcr", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drei Spr\u00fcche gab ich aus,", "tokens": ["Drei", "Spr\u00fc\u00b7che", "gab", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein richtiges Trifolium \u2013", "tokens": ["Ein", "rich\u00b7ti\u00b7ges", "Tri\u00b7fo\u00b7li\u00b7um", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die drei auch stimmen \u00fcberein,", "tokens": ["Die", "drei", "auch", "stim\u00b7men", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie k\u00f6nnten ihrer vier wohl sein.", "tokens": ["Sie", "k\u00f6nn\u00b7ten", "ih\u00b7rer", "vier", "wohl", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "CARD", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}