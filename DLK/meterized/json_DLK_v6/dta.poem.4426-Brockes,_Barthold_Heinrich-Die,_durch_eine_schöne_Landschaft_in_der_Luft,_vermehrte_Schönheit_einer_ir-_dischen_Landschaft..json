{"dta.poem.4426": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die, durch eine sch\u00f6ne Landschaft in  \n der Luft, vermehrte Sch\u00f6nheit einer ir-  \n dischen Landschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grosses All! unendlichs Wesen,", "tokens": ["Gros\u00b7ses", "All", "!", "un\u00b7end\u00b7lichs", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Natur Buch giebt mir hier,", "tokens": ["Der", "Na\u00b7tur", "Buch", "giebt", "mir", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Voller Wunder, Glanz und Zier,", "tokens": ["Vol\u00b7ler", "Wun\u00b7der", ",", "Glanz", "und", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Herrlichkeit zu lesen.", "tokens": ["Dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns\u2019re Selen wissen nicht,", "tokens": ["Un\u00b7s'\u00b7re", "Se\u00b7len", "wis\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sich was sch\u00f6ners vorzubilden;", "tokens": ["Sich", "was", "sch\u00f6\u00b7ners", "vor\u00b7zu\u00b7bil\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PWS", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber ach, was mu\u00df Dein Licht", "tokens": ["A\u00b7ber", "ach", ",", "was", "mu\u00df", "Dein", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PWS", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In den himmlischen Gefilden", "tokens": ["In", "den", "himm\u00b7li\u00b7schen", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ohne Schranken, sonder Grenzen,", "tokens": ["Oh\u00b7ne", "Schran\u00b7ken", ",", "son\u00b7der", "Gren\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo es unverh\u00fcllet, gl\u00e4nzen!", "tokens": ["Wo", "es", "un\u00b7ver\u00b7h\u00fcl\u00b7let", ",", "gl\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Welch ein Abgrund voller Lust,", "tokens": ["Welch", "ein", "Ab\u00b7grund", "vol\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Welche Tiefen voller Wonne", "tokens": ["Wel\u00b7che", "Tie\u00b7fen", "vol\u00b7ler", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sind, o aller Sonnen Sonne,", "tokens": ["Sind", ",", "o", "al\u00b7ler", "Son\u00b7nen", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Denen, die Dich sehn, bewust!", "tokens": ["De\u00b7nen", ",", "die", "Dich", "sehn", ",", "be\u00b7wust", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Welch ein Meer von heil\u2019ger Gluht", "tokens": ["Welch", "ein", "Meer", "von", "heil'\u00b7ger", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Mu\u00df aus Deinem Throne qvillen!", "tokens": ["Mu\u00df", "aus", "Dei\u00b7nem", "Thro\u00b7ne", "qvil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Welche sel\u2019ge Liebes-Flut", "tokens": ["Wel\u00b7che", "sel'\u00b7ge", "Lie\u00b7bes\u00b7Flut"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Mu\u00df der Himmel Himmel f\u00fcllen!", "tokens": ["Mu\u00df", "der", "Him\u00b7mel", "Him\u00b7mel", "f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ach wie mu\u00df so \u00fcberschwenglich", "tokens": ["Ach", "wie", "mu\u00df", "so", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAV", "VMFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Dort des Sch\u00f6pfers eig\u2019ner Schein,", "tokens": ["Dort", "des", "Sch\u00f6p\u00b7fers", "eig'\u00b7ner", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Da schon das, was nur verg\u00e4nglich", "tokens": ["Da", "schon", "das", ",", "was", "nur", "ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PDS", "$,", "PRELS", "ADV", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.22": {"text": "So gar herrlich ist, doch seyn!", "tokens": ["So", "gar", "herr\u00b7lich", "ist", ",", "doch", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Ach lasst uns in diesem Leben", "tokens": ["Ach", "lasst", "uns", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Deine Weisheit, Lieb\u2019 und Macht", "tokens": ["Dei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "In der Creaturen Pracht", "tokens": ["In", "der", "Crea\u00b7tu\u00b7ren", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Zu bewundern uns bestreben!", "tokens": ["Zu", "be\u00b7wun\u00b7dern", "uns", "be\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Sind wir bey den ird\u2019schen Sch\u00e4tzen", "tokens": ["Sind", "wir", "bey", "den", "ird'\u00b7schen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ueber wenig treu gewesen;", "tokens": ["Ue\u00b7ber", "we\u00b7nig", "treu", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wird Er uns zu mehr erlesen,", "tokens": ["Wird", "Er", "uns", "zu", "mehr", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Und dort \u00fcber vieles setzen.", "tokens": ["Und", "dort", "\u00fc\u00b7ber", "vie\u00b7les", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}