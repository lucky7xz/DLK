{"textgrid.poem.39080": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Nachts", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Tag gesunken,", "tokens": ["Nun", "ist", "der", "Tag", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "vom Berge steigt die Nacht,", "tokens": ["vom", "Ber\u00b7ge", "steigt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und hell mit tausend Funken", "tokens": ["und", "hell", "mit", "tau\u00b7send", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Sternlein sind erwacht.", "tokens": ["die", "Stern\u00b7lein", "sind", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun \u00fcber Tal und H\u00fcgel,", "tokens": ["Nun", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "herab vom Sternenzelt,", "tokens": ["her\u00b7ab", "vom", "Ster\u00b7nen\u00b7zelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "nun schwebt mit leisem Fl\u00fcgel", "tokens": ["nun", "schwebt", "mit", "lei\u00b7sem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Freiheit durch die Welt.", "tokens": ["die", "Frei\u00b7heit", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie tritt an alle H\u00fctten,", "tokens": ["Sie", "tritt", "an", "al\u00b7le", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sie pocht an jedes Tor,", "tokens": ["sie", "pocht", "an", "je\u00b7des", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sie fl\u00fcstert leise Bitten", "tokens": ["sie", "fl\u00fcs\u00b7tert", "lei\u00b7se", "Bit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dem Schlummernden ins Ohr.", "tokens": ["dem", "Schlum\u00b7mern\u00b7den", "ins", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie weiht mit hei\u00dfem Kusse", "tokens": ["Sie", "weiht", "mit", "hei\u00b7\u00dfem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "den J\u00fcngling und den Mann", "tokens": ["den", "J\u00fcng\u00b7ling", "und", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und haucht mit leisem Gru\u00dfe", "tokens": ["und", "haucht", "mit", "lei\u00b7sem", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "auch den Gefangnen an.", "tokens": ["auch", "den", "Ge\u00b7fang\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Sie pr\u00fcft am Schwert die Schneide,", "tokens": ["Sie", "pr\u00fcft", "am", "Schwert", "die", "Schnei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sie tritt zum Pulverfa\u00df,", "tokens": ["sie", "tritt", "zum", "Pul\u00b7ver\u00b7fa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sie z\u00e4hlt mit stummem Neide", "tokens": ["sie", "z\u00e4hlt", "mit", "stum\u00b7mem", "Nei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "den Sand im Stundenglas:", "tokens": ["den", "Sand", "im", "Stun\u00b7den\u00b7glas", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df alle Seelen tr\u00e4umen,", "tokens": ["Da\u00df", "al\u00b7le", "See\u00b7len", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df alle Herzen gl\u00fchn,", "tokens": ["da\u00df", "al\u00b7le", "Her\u00b7zen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "von Rossen, die sich b\u00e4umen,", "tokens": ["von", "Ros\u00b7sen", ",", "die", "sich", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "von Taten, stolz und k\u00fchn,", "tokens": ["von", "Ta\u00b7ten", ",", "stolz", "und", "k\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "da\u00df hinter Eisengittern,", "tokens": ["da\u00df", "hin\u00b7ter", "Ei\u00b7sen\u00b7git\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "selbst der Gefangne lacht?", "tokens": ["selbst", "der", "Ge\u00b7fang\u00b7ne", "lacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df im Palast, mit Zittern,", "tokens": ["Da\u00df", "im", "Pa\u00b7last", ",", "mit", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein bleicher Mann erwacht!", "tokens": ["ein", "blei\u00b7cher", "Mann", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}