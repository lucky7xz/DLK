{"textgrid.poem.46391": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose in der Knospe", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aurorens Kind du junge Rose du", "tokens": ["Au\u00b7ro\u00b7rens", "Kind", "du", "jun\u00b7ge", "Ro\u00b7se", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "PPER", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was schlie\u00dft du noch die volle Knospe zu?", "tokens": ["Was", "schlie\u00dft", "du", "noch", "die", "vol\u00b7le", "Knos\u00b7pe", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es scheint du scheust das rauhe Wetter", "tokens": ["Es", "scheint", "du", "scheust", "das", "rau\u00b7he", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00fctest drum die zarten Bl\u00e4tter,", "tokens": ["Und", "h\u00fc\u00b7test", "drum", "die", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Dorilis wenn sie ein L\u00fcfftchen schreckt,", "tokens": ["Wie", "Do\u00b7ri\u00b7lis", "wenn", "sie", "ein", "L\u00fcfft\u00b7chen", "schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den jungen Busen deckt.", "tokens": ["Den", "jun\u00b7gen", "Bu\u00b7sen", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}