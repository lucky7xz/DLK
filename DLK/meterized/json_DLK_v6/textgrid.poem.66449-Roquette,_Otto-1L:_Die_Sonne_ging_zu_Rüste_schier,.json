{"textgrid.poem.66449": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne ging zu R\u00fcste schier,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne ging zu R\u00fcste schier,", "tokens": ["Die", "Son\u00b7ne", "ging", "zu", "R\u00fcs\u00b7te", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam ich sp\u00e4t in mein Quartier", "tokens": ["Da", "kam", "ich", "sp\u00e4t", "in", "mein", "Quar\u00b7tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit m\u00fcdgelaufnen F\u00fc\u00dfen.", "tokens": ["Mit", "m\u00fcd\u00b7ge\u00b7lauf\u00b7nen", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frau Wirthin stand wohl vor der Th\u00fcr,", "tokens": ["Frau", "Wirt\u00b7hin", "stand", "wohl", "vor", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hatt' eine blaue Sch\u00fcrze f\u00fcr,", "tokens": ["Sie", "hatt'", "ei\u00b7ne", "blau\u00b7e", "Sch\u00fcr\u00b7ze", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie th\u00e4t so freundlich gr\u00fc\u00dfen.", "tokens": ["Sie", "th\u00e4t", "so", "freund\u00b7lich", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbgr\u00fc\u00df Gott, Er wandernder Gesell,", "tokens": ["\u00bb", "gr\u00fc\u00df", "Gott", ",", "Er", "wan\u00b7dern\u00b7der", "Ge\u00b7sell", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tret' Er nur \u00fcber meine Schwell',", "tokens": ["Tret'", "Er", "nur", "\u00fc\u00b7ber", "mei\u00b7ne", "Schwell'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ruh soll Ihm bekommen!\u00ab", "tokens": ["Die", "Ruh", "soll", "Ihm", "be\u00b7kom\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00f6n Dank, sch\u00f6n Dank, Frau Wirthin mein,", "tokens": ["Sch\u00f6n", "Dank", ",", "sch\u00f6n", "Dank", ",", "Frau", "Wirt\u00b7hin", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "NN", "$,", "NN", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr H\u00e4uslein hat gar saubren Schein,", "tokens": ["Ihr", "H\u00e4us\u00b7lein", "hat", "gar", "sau\u00b7bren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann auch Ihr Wein mir frommen?", "tokens": ["Kann", "auch", "Ihr", "Wein", "mir", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Wein ist pures Rebenna\u00df,", "tokens": ["\u00bb", "mein", "Wein", "ist", "pu\u00b7res", "Re\u00b7benn\u00b7a\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schenk' ihn roth, ich schenk' ihn bla\u00df,", "tokens": ["Ich", "schenk'", "ihn", "roth", ",", "ich", "schenk'", "ihn", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sag' Er mir nur Eines:", "tokens": ["Doch", "sag'", "Er", "mir", "nur", "Ei\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist blond Sein Schatz, oder ist er braun?", "tokens": ["Ist", "blond", "Sein", "Schatz", ",", "o\u00b7der", "ist", "er", "braun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und sagt Er mir's, so wei\u00df ich traun", "tokens": ["Und", "sagt", "Er", "mir's", ",", "so", "wei\u00df", "ich", "traun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NE", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Farbe schon des Weines!\u00ab", "tokens": ["Die", "Far\u00b7be", "schon", "des", "Wei\u00b7nes", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Frau Wirthin, Sie ist flink und klug,", "tokens": ["Frau", "Wirt\u00b7hin", ",", "Sie", "ist", "flink", "und", "klug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dazu auch jung und h\u00fcbsch genug,", "tokens": ["Da\u00b7zu", "auch", "jung", "und", "h\u00fcbsch", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wird mich recht verstehen:", "tokens": ["Sie", "wird", "mich", "recht", "ver\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was man so recht im Herzen h\u00e4lt,", "tokens": ["Was", "man", "so", "recht", "im", "Her\u00b7zen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das sagt man nicht der ganzen Welt,", "tokens": ["Das", "sagt", "man", "nicht", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr wird es auch so gehen!", "tokens": ["Ihr", "wird", "es", "auch", "so", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Frau Wirthin warf das Aug' herum,", "tokens": ["Frau", "Wirt\u00b7hin", "warf", "das", "Aug'", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4chelt s\u00fc\u00df, und l\u00e4chelt stumm,", "tokens": ["Und", "l\u00e4\u00b7chelt", "s\u00fc\u00df", ",", "und", "l\u00e4\u00b7chelt", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und th\u00e4t zwei Becher bringen:", "tokens": ["Und", "th\u00e4t", "zwei", "Be\u00b7cher", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbdies Weinchen hatt' ich still verwahrt,", "tokens": ["\u00bb", "dies", "Wein\u00b7chen", "hatt'", "ich", "still", "ver\u00b7wahrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis da\u00df nach seiner Wanderfahrt", "tokens": ["Bis", "da\u00df", "nach", "sei\u00b7ner", "Wan\u00b7der\u00b7fahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Schatz mich th\u00e4t umschlingen.\u00ab", "tokens": ["Mein", "Schatz", "mich", "th\u00e4t", "um\u00b7schlin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbdoch Er soll's trinken heut mit mir,", "tokens": ["\u00bb", "doch", "Er", "soll's", "trin\u00b7ken", "heut", "mit", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "PIS", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil versteh'nde Seelen wir,", "tokens": ["Die\u00b7weil", "ver\u00b7steh'n\u00b7de", "See\u00b7len", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich geb's umsonst und gerne!\u00ab \u2013", "tokens": ["Ich", "ge\u00b7b's", "um\u00b7sonst", "und", "ger\u00b7ne", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich aber dacht' in frohem Muth,", "tokens": ["Ich", "a\u00b7ber", "dacht'", "in", "fro\u00b7hem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach h\u00e4tte doch jede Frau Wirthin gut", "tokens": ["Ach", "h\u00e4t\u00b7te", "doch", "je\u00b7de", "Frau", "Wirt\u00b7hin", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "ADV", "PIAT", "NN", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Einen Schatz in weiter Ferne!", "tokens": ["Ei\u00b7nen", "Schatz", "in", "wei\u00b7ter", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}