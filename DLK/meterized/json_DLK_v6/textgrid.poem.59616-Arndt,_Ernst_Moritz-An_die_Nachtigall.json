{"textgrid.poem.59616": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An die Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe Klage,", "tokens": ["S\u00fc\u00b7\u00dfe", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Kleine Nachtigall,", "tokens": ["Klei\u00b7ne", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Klang der N\u00e4chte, sage,", "tokens": ["Klang", "der", "N\u00e4ch\u00b7te", ",", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wer gab dir den Schall?", "tokens": ["Wer", "gab", "dir", "den", "Schall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Fielst von Sternen", "tokens": ["Fielst", "von", "Ster\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Du, ein Engeltraum,", "tokens": ["Du", ",", "ein", "En\u00b7gel\u00b7traum", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df wir Sehnsucht lernen", "tokens": ["Da\u00df", "wir", "Sehn\u00b7sucht", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nach dem lichten Raum?", "tokens": ["Nach", "dem", "lich\u00b7ten", "Raum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wurdest Leier", "tokens": ["Wur\u00b7dest", "Lei\u00b7er"], "token_info": ["word", "word"], "pos": ["VAFIN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "F\u00fcr der Liebe Leid,", "tokens": ["F\u00fcr", "der", "Lie\u00b7be", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Singst der Seelen Feier", "tokens": ["Singst", "der", "See\u00b7len", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nun im Federkleid?", "tokens": ["Nun", "im", "Fe\u00b7der\u00b7kleid", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Philomele,", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Holdes Himmelskind,", "tokens": ["Hol\u00b7des", "Him\u00b7mel\u00b7skind", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zarte Geisterseele,", "tokens": ["Zar\u00b7te", "Geis\u00b7ter\u00b7see\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie die Engel sind!", "tokens": ["Wie", "die", "En\u00b7gel", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O der Herzen", "tokens": ["O", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Goldner Leierklang!", "tokens": ["Gold\u00b7ner", "Lei\u00b7er\u00b7klang", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Klinge, Lust der Schmerzen,", "tokens": ["Klin\u00b7ge", ",", "Lust", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Klinge laut, Gesang!", "tokens": ["Klin\u00b7ge", "laut", ",", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Klinge, Liebe,", "tokens": ["Klin\u00b7ge", ",", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Klinge, Sehnsucht, drein!", "tokens": ["Klin\u00b7ge", ",", "Sehn\u00b7sucht", ",", "drein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Funkelt, helle Triebe,", "tokens": ["Fun\u00b7kelt", ",", "hel\u00b7le", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hell wie Sternenschein!", "tokens": ["Hell", "wie", "Ster\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Stimmt Ges\u00e4nge", "tokens": ["Stimmt", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Gleich der Nachtigall,", "tokens": ["Gleich", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und im Strom der Kl\u00e4nge", "tokens": ["Und", "im", "Strom", "der", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Flutet hin zum All!", "tokens": ["Flu\u00b7tet", "hin", "zum", "All", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}