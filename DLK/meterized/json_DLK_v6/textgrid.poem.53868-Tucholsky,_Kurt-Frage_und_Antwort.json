{"textgrid.poem.53868": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Frage und Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sage: \u00bbSagen Sie mal\u00ab, sage ich neulich", "tokens": ["Ich", "sa\u00b7ge", ":", "\u00bb", "Sa\u00b7gen", "Sie", "mal", "\u00ab", ",", "sa\u00b7ge", "ich", "neu\u00b7lich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "$(", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "zu einem Reichsgerichtsrat;", "tokens": ["zu", "ei\u00b7nem", "Reichs\u00b7ge\u00b7richts\u00b7rat", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdas ist ja ganz und gar abscheulich,", "tokens": ["\u00bb", "das", "ist", "ja", "ganz", "und", "gar", "ab\u00b7scheu\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "ADV", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das mit dem vierten Senat!", "tokens": ["das", "mit", "dem", "vier\u00b7ten", "Se\u00b7nat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Ob ich daf\u00fcr wohl meine Steuern bezahle?\u00ab", "tokens": ["Ob", "ich", "da\u00b7f\u00fcr", "wohl", "mei\u00b7ne", "Steu\u00b7ern", "be\u00b7zah\u00b7le", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "sag ich, \u00bbIhr macht ja nicht schlecht . . .", "tokens": ["sag", "ich", ",", "\u00bb", "Ihr", "macht", "ja", "nicht", "schlecht", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "$.", "$.", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Seid ihr die Filiale,", "tokens": ["Seid", "ihr", "die", "Fi\u00b7li\u00b7a\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "seid ihr die Filiale", "tokens": ["seid", "ihr", "die", "Fi\u00b7li\u00b7a\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "vom Recht \u2013?\u00ab", "tokens": ["vom", "Recht", "\u2013", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$(", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Da nuckelte der aus dem Reichsgerichtshaus", "tokens": ["Da", "nu\u00b7ckel\u00b7te", "der", "aus", "dem", "Reichs\u00b7ge\u00b7richts\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und sagte: \u00bbSehn wir vielleicht so aus \u2013?\u00ab", "tokens": ["und", "sag\u00b7te", ":", "\u00bb", "Sehn", "wir", "viel\u00b7leicht", "so", "aus", "\u2013", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "ADV", "APPR", "$(", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich sage: \u00bbSagen Sie mal\u00ab, sag ich nun wieder,", "tokens": ["Ich", "sa\u00b7ge", ":", "\u00bb", "Sa\u00b7gen", "Sie", "mal", "\u00ab", ",", "sag", "ich", "nun", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "$(", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbverehrter Herr Reichsgerichtsrat:", "tokens": ["\u00bb", "ver\u00b7ehr\u00b7ter", "Herr", "Reichs\u00b7ge\u00b7richts\u00b7rat", ":"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "ihr kn\u00fcppelt die Kommunisten nieder", "tokens": ["ihr", "kn\u00fcp\u00b7pelt", "die", "Kom\u00b7mu\u00b7nis\u00b7ten", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mit Hoch- und Landesverrat.", "tokens": ["mit", "Hoch", "und", "Lan\u00b7des\u00b7ver\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Hinter euerm Sandsteinportale", "tokens": ["Hin\u00b7ter", "eu\u00b7erm", "Sand\u00b7ste\u00b7in\u00b7por\u00b7ta\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "da gehts los! Da geschiehts!", "tokens": ["da", "gehts", "los", "!", "Da", "ge\u00b7schiehts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seid ihr die Filiale,", "tokens": ["Seid", "ihr", "die", "Fi\u00b7li\u00b7a\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "seid ihr die Filiale", "tokens": ["seid", "ihr", "die", "Fi\u00b7li\u00b7a\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "der Justiz \u2013?\u00ab", "tokens": ["der", "Jus\u00b7tiz", "\u2013", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$(", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Da nuckelte der aus dem Reichsgerichtshaus", "tokens": ["Da", "nu\u00b7ckel\u00b7te", "der", "aus", "dem", "Reichs\u00b7ge\u00b7richts\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "und sagte: \u00bbSehn wir vielleicht so aus \u2013?\u00ab", "tokens": ["und", "sag\u00b7te", ":", "\u00bb", "Sehn", "wir", "viel\u00b7leicht", "so", "aus", "\u2013", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "ADV", "APPR", "$(", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich sage: \u00bbSagen Sie mal\u00ab, sag ich geduldig,", "tokens": ["Ich", "sa\u00b7ge", ":", "\u00bb", "Sa\u00b7gen", "Sie", "mal", "\u00ab", ",", "sag", "ich", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "$(", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbbitte! Besinnen Sie sich!", "tokens": ["\u00bb", "bit\u00b7te", "!", "Be\u00b7sin\u00b7nen", "Sie", "sich", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "NN", "PPER", "PRF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihr sprecht die politischen Gegner schuldig \u2013", "tokens": ["Ihr", "sprecht", "die", "po\u00b7li\u00b7ti\u00b7schen", "Geg\u00b7ner", "schul\u00b7dig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer seid ihr denn eigentlich \u2013?\u00ab", "tokens": ["Wer", "seid", "ihr", "denn", "ei\u00b7gent\u00b7lich", "\u2013", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "$(", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da lachte der Richter zum ersten Male:", "tokens": ["Da", "lach\u00b7te", "der", "Rich\u00b7ter", "zum", "ers\u00b7ten", "Ma\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "\u00bbfragen Sie nicht so dumm!", "tokens": ["\u00bb", "fra\u00b7gen", "Sie", "nicht", "so", "dumm", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wir sind die Filiale,", "tokens": ["Wir", "sind", "die", "Fi\u00b7li\u00b7a\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "wir sind die Filiale", "tokens": ["wir", "sind", "die", "Fi\u00b7li\u00b7a\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "vom Rrrrr . . . rataplan!", "tokens": ["vom", "Rrrrr", ".", ".", ".", "ra\u00b7ta\u00b7plan", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "$.", "$.", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "vom Rrrrr . . . rataplan!", "tokens": ["vom", "Rrrrr", ".", ".", ".", "ra\u00b7ta\u00b7plan", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "$.", "$.", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "vom Reichswehrministerium \u2013!\u00ab", "tokens": ["vom", "Reichs\u00b7wehr\u00b7mi\u00b7nis\u00b7te\u00b7ri\u00b7um", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$(", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}