{"dta.poem.20074": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Alles still !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.85", "fr:0.14"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Alles still! es tanzt den Reigen", "tokens": ["Al\u00b7les", "still", "!", "es", "tanzt", "den", "Rei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mondenstrahl in Wald und Flur,", "tokens": ["Mon\u00b7dens\u00b7trahl", "in", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dar\u00fcber thront das Schweigen", "tokens": ["Und", "da\u00b7r\u00fc\u00b7ber", "thront", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und der Winterhimmel nur.", "tokens": ["Und", "der", "Win\u00b7ter\u00b7him\u00b7mel", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles still! vergeblich lauschet", "tokens": ["Al\u00b7les", "still", "!", "ver\u00b7geb\u00b7lich", "lau\u00b7schet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "PTKVZ", "$.", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Man der Kr\u00e4he heisrem Schrei,", "tokens": ["Man", "der", "Kr\u00e4\u00b7he", "heis\u00b7rem", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner Fichte Wipfel rauschet", "tokens": ["Kei\u00b7ner", "Fich\u00b7te", "Wip\u00b7fel", "rau\u00b7schet"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein B\u00e4chlein summt vorbei.", "tokens": ["Und", "kein", "B\u00e4ch\u00b7lein", "summt", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles still! die Dorfes-H\u00fctten", "tokens": ["Al\u00b7les", "still", "!", "die", "Dor\u00b7fes\u00b7H\u00fct\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "PTKVZ", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wie Gr\u00e4ber anzusehn,", "tokens": ["Sind", "wie", "Gr\u00e4\u00b7ber", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, von Schnee bedeckt, inmitten", "tokens": ["Die", ",", "von", "Schnee", "be\u00b7deckt", ",", "in\u00b7mit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "$,", "APPR", "NN", "VVPP", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines weiten Friedhofs stehn.", "tokens": ["Ei\u00b7nes", "wei\u00b7ten", "Fried\u00b7hofs", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles still! nichts h\u00f6r\u2019 ich klopfen", "tokens": ["Al\u00b7les", "still", "!", "nichts", "h\u00f6r'", "ich", "klop\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "PTKVZ", "$.", "PIS", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als mein Herze durch die Nacht; \u2014", "tokens": ["Als", "mein", "Her\u00b7ze", "durch", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00dfe Thr\u00e4nen niedertropfen", "tokens": ["Hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "nie\u00b7der\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die kalte Winterpracht.", "tokens": ["Auf", "die", "kal\u00b7te", "Win\u00b7ter\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}