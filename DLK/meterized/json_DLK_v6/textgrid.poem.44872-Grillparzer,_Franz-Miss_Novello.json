{"textgrid.poem.44872": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Miss Novello", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wirkst nicht, trotz des Richters Witz,", "tokens": ["Du", "wirkst", "nicht", ",", "trotz", "des", "Rich\u00b7ters", "Witz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der furchtbar sonst im Grimme.", "tokens": ["Der", "furcht\u00b7bar", "sonst", "im", "Grim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das macht: der Richter hat nur Sitz,", "tokens": ["Das", "macht", ":", "der", "Rich\u00b7ter", "hat", "nur", "Sitz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du hast nichts als Stimme.", "tokens": ["Und", "du", "hast", "nichts", "als", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}