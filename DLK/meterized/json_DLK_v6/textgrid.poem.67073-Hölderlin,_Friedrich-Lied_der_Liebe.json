{"textgrid.poem.67073": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Engelfreuden ahndend wallen", "tokens": ["En\u00b7gel\u00b7freu\u00b7den", "ahn\u00b7dend", "wal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir hinaus auf Gottes Flur,", "tokens": ["Wir", "hin\u00b7aus", "auf", "Got\u00b7tes", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APZR", "APPR", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wo die Jubel widerhallen", "tokens": ["Wo", "die", "Ju\u00b7bel", "wi\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Tempel der Natur;", "tokens": ["In", "dem", "Tem\u00b7pel", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute soll kein Auge tr\u00fcbe,", "tokens": ["Heu\u00b7te", "soll", "kein", "Au\u00b7ge", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorge nicht hienieden sein,", "tokens": ["Sor\u00b7ge", "nicht", "hien\u00b7ie\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jedes Wesen soll der Liebe", "tokens": ["Je\u00b7des", "We\u00b7sen", "soll", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wonniglich, wie wir, sich freun.", "tokens": ["Won\u00b7nig\u00b7lich", ",", "wie", "wir", ",", "sich", "freun", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Singt den Jubel, Schwestern! Br\u00fcder!", "tokens": ["Singt", "den", "Ju\u00b7bel", ",", "Schwes\u00b7tern", "!", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Festgeschlungen! Hand in Hand!", "tokens": ["Fest\u00b7ge\u00b7schlun\u00b7gen", "!", "Hand", "in", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singt das heiligste der Lieder", "tokens": ["Singt", "das", "hei\u00b7ligs\u00b7te", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Steigt hinauf am Rebenh\u00fcgel,", "tokens": ["Steigt", "hin\u00b7auf", "am", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blickt hinab ins Schattental!", "tokens": ["Blickt", "hin\u00b7ab", "ins", "Schat\u00b7ten\u00b7tal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcberall der Liebe Fl\u00fcgel,", "tokens": ["\u00dc\u00b7be\u00b7rall", "der", "Lie\u00b7be", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wonnerauschend \u00fcberall!", "tokens": ["Won\u00b7ne\u00b7rau\u00b7schend", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebe lehrt das L\u00fcftchen kosen", "tokens": ["Lie\u00b7be", "lehrt", "das", "L\u00fcft\u00b7chen", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Blumen auf der Au,", "tokens": ["Mit", "den", "Blu\u00b7men", "auf", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lockt zu jungen Fr\u00fchlingsrosen", "tokens": ["Lockt", "zu", "jun\u00b7gen", "Fr\u00fch\u00b7lings\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Wolke Morgentau,", "tokens": ["Aus", "der", "Wol\u00b7ke", "Mor\u00b7gen\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe ziehet Well an Welle", "tokens": ["Lie\u00b7be", "zie\u00b7het", "Well", "an", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundlichmurmelnd n\u00e4her hin,", "tokens": ["Freund\u00b7lich\u00b7mur\u00b7melnd", "n\u00e4\u00b7her", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leitet aus der Kluft die Quelle", "tokens": ["Lei\u00b7tet", "aus", "der", "Kluft", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sanft hinab ins Wiesengr\u00fcn.", "tokens": ["Sanft", "hin\u00b7ab", "ins", "Wie\u00b7sen\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Berge kn\u00fcpft mit ehrner Kette", "tokens": ["Ber\u00b7ge", "kn\u00fcpft", "mit", "ehr\u00b7ner", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe an das Firmament,", "tokens": ["Lie\u00b7be", "an", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Donner ruft sie an die St\u00e4tte,", "tokens": ["Don\u00b7ner", "ruft", "sie", "an", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Sand die Pflanze brennt,", "tokens": ["Wo", "der", "Sand", "die", "Pflan\u00b7ze", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die hehre Sonne leitet", "tokens": ["Um", "die", "heh\u00b7re", "Son\u00b7ne", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie die treuen Sterne her,", "tokens": ["Sie", "die", "treu\u00b7en", "Ster\u00b7ne", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Folgsam ihrem Winke gleitet", "tokens": ["Folg\u00b7sam", "ih\u00b7rem", "Win\u00b7ke", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder Strom ins weite Meer.", "tokens": ["Je\u00b7der", "Strom", "ins", "wei\u00b7te", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebe wallt in W\u00fcsteneien,", "tokens": ["Lie\u00b7be", "wallt", "in", "W\u00fcs\u00b7te\u00b7nei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6hnt des Dursts im d\u00fcrren Sand,", "tokens": ["H\u00f6hnt", "des", "Dursts", "im", "d\u00fcr\u00b7ren", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieget, wo Tyrannen dr\u00e4uen,", "tokens": ["Sie\u00b7get", ",", "wo", "Ty\u00b7ran\u00b7nen", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Steigt hinab ins Totenland;", "tokens": ["Steigt", "hin\u00b7ab", "ins", "To\u00b7ten\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe tr\u00fcmmert Felsen nieder,", "tokens": ["Lie\u00b7be", "tr\u00fcm\u00b7mert", "Fel\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zaubert Paradiese hin,", "tokens": ["Zau\u00b7bert", "Pa\u00b7ra\u00b7die\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schaffet Erd und Himmel wieder", "tokens": ["Schaf\u00b7fet", "Erd", "und", "Him\u00b7mel", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00f6ttlich, wie im Anbeginn.", "tokens": ["G\u00f6tt\u00b7lich", ",", "wie", "im", "An\u00b7be\u00b7ginn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Liebe schwingt den Seraphsfl\u00fcgel,", "tokens": ["Lie\u00b7be", "schwingt", "den", "Se\u00b7raphs\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Gott der G\u00f6tter wohnt,", "tokens": ["Wo", "der", "Gott", "der", "G\u00f6t\u00b7ter", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lohnt den Schwei\u00df am Felsenh\u00fcgel,", "tokens": ["Lohnt", "den", "Schwei\u00df", "am", "Fel\u00b7sen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann der Richter einst belohnt,", "tokens": ["Wann", "der", "Rich\u00b7ter", "einst", "be\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die K\u00f6nigsst\u00fchle tr\u00fcmmern,", "tokens": ["Wann", "die", "K\u00f6\u00b7nigs\u00b7st\u00fch\u00b7le", "tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hin ist jede Scheidewand,", "tokens": ["Hin", "ist", "je\u00b7de", "Schei\u00b7de\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Adeltaten heller schimmern,", "tokens": ["A\u00b7del\u00b7ta\u00b7ten", "hel\u00b7ler", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reiner, denn der Krone Tand.", "tokens": ["Rei\u00b7ner", ",", "denn", "der", "Kro\u00b7ne", "Tand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mag uns jetzt die Stunde schlagen,", "tokens": ["Mag", "uns", "jetzt", "die", "Stun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt der letzte Othem wehn!", "tokens": ["Jetzt", "der", "letz\u00b7te", "O\u00b7them", "wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Br\u00fcder! dr\u00fcben wird es tagen,", "tokens": ["Br\u00fc\u00b7der", "!", "dr\u00fc\u00b7ben", "wird", "es", "ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwestern! dort ist Wiedersehn;", "tokens": ["Schwes\u00b7tern", "!", "dort", "ist", "Wie\u00b7der\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jauchzt dem heiligsten der Triebe,", "tokens": ["Jauchzt", "dem", "hei\u00b7ligs\u00b7ten", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Die der Gott der G\u00f6tter gab,", "tokens": ["Die", "der", "Gott", "der", "G\u00f6t\u00b7ter", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Br\u00fcder! Schwestern! jauchzt der Liebe!", "tokens": ["Br\u00fc\u00b7der", "!", "Schwes\u00b7tern", "!", "jauchzt", "der", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie besieget Zeit und Grab!", "tokens": ["Sie", "be\u00b7sie\u00b7get", "Zeit", "und", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}