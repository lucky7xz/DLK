{"textgrid.poem.62015": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bist als Meteor erschienen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist als Meteor erschienen", "tokens": ["Bist", "als", "Me\u00b7te\u00b7or", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer kalten Wirklichkeit,", "tokens": ["Uns\u00b7rer", "kal\u00b7ten", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach erschienen und entschwunden", "tokens": ["Ach", "er\u00b7schie\u00b7nen", "und", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "VVINF", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzufr\u00fch zur Ewigkeit.", "tokens": ["All\u00b7zu\u00b7fr\u00fch", "zur", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gab'st die Liebe zur Parole,", "tokens": ["Gab'st", "die", "Lie\u00b7be", "zur", "Pa\u00b7ro\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zum Panier Gerechtigkeit,", "tokens": ["Zum", "Pa\u00b7nier", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie eine Wahrheit werde", "tokens": ["Da\u00df", "sie", "ei\u00b7ne", "Wahr\u00b7heit", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ertr\u00e4umte Menschlichkeit.", "tokens": ["Die", "er\u00b7tr\u00e4um\u00b7te", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n erw\u00e4rmet ward die Erde,", "tokens": ["Sch\u00f6n", "er\u00b7w\u00e4r\u00b7met", "ward", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neu erstand das Ideal", "tokens": ["Neu", "er\u00b7stand", "das", "I\u00b7deal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Don des Dritten Friedrich's Geiste", "tokens": ["Don", "des", "Drit\u00b7ten", "Fried\u00b7rich's", "Geis\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Herzens Sonnenstrahl.", "tokens": ["Sei\u00b7nes", "Her\u00b7zens", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dritter Friedrich, Du wirst leben,", "tokens": ["Drit\u00b7ter", "Fried\u00b7rich", ",", "Du", "wirst", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier und dort der Edle lebt.", "tokens": ["Hier", "und", "dort", "der", "Ed\u00b7le", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nievergessen wird auf Erden", "tokens": ["Nie\u00b7ver\u00b7ges\u00b7sen", "wird", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer schon hier zum Himmel schwebt.", "tokens": ["Wer", "schon", "hier", "zum", "Him\u00b7mel", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}