{"textgrid.poem.40535": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort ein Palmbaum auf der H\u00f6he", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort ein Palmbaum auf der H\u00f6he", "tokens": ["Dort", "ein", "Palm\u00b7baum", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Klostergarten ragt;", "tokens": ["Aus", "dem", "Klos\u00b7ter\u00b7gar\u00b7ten", "ragt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erste Palme, die ich sehe,", "tokens": ["Ers\u00b7te", "Pal\u00b7me", ",", "die", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringst du mir den Ost, der tagt?", "tokens": ["Bringst", "du", "mir", "den", "Ost", ",", "der", "tagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Luftig schwankt wie Pfaugefieder", "tokens": ["Luf\u00b7tig", "schwankt", "wie", "Pfau\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Kron' am schlanken Schaft", "tokens": ["Ih\u00b7re", "Kron'", "am", "schlan\u00b7ken", "Schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberm Rauschen laub'ger Br\u00fcder,", "tokens": ["Ue\u00b7berm", "Rau\u00b7schen", "laub'\u00b7ger", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stumm, durchsichtig, geisterhaft.", "tokens": ["Stumm", ",", "durch\u00b7sich\u00b7tig", ",", "geis\u00b7ter\u00b7haft", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In dem Grase schl\u00e4ft am Baume", "tokens": ["In", "dem", "Gra\u00b7se", "schl\u00e4ft", "am", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Novize, jung und sch\u00f6n;", "tokens": ["Ein", "No\u00b7vi\u00b7ze", ",", "jung", "und", "sch\u00f6n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat gelispelt seinem Traume", "tokens": ["Hat", "ge\u00b7lis\u00b7pelt", "sei\u00b7nem", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ostens Wonne aus den H\u00f6hn?", "tokens": ["Os\u00b7tens", "Won\u00b7ne", "aus", "den", "H\u00f6hn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn er sieht in \u00fcpp'gem Kleide", "tokens": ["Denn", "er", "sieht", "in", "\u00fcpp'\u00b7gem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in Sammt und Golde nun", "tokens": ["Sich", "in", "Sammt", "und", "Gol\u00b7de", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Kissen weicher Seide", "tokens": ["Auf", "den", "Kis\u00b7sen", "wei\u00b7cher", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern in einem Garten ruhn.", "tokens": ["Fern", "in", "ei\u00b7nem", "Gar\u00b7ten", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Blumen, ries'ge, wunderbare,", "tokens": ["Blu\u00b7men", ",", "ries'\u00b7ge", ",", "wun\u00b7der\u00b7ba\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gaukeln, duften, spr\u00fchn um ihn;", "tokens": ["Gau\u00b7keln", ",", "duf\u00b7ten", ",", "spr\u00fchn", "um", "ihn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebliche Gazellenpaare", "tokens": ["Lieb\u00b7li\u00b7che", "Ga\u00b7zel\u00b7len\u00b7paa\u00b7re"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Durch die fernen B\u00fcsche ziehn.", "tokens": ["Durch", "die", "fer\u00b7nen", "B\u00fc\u00b7sche", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wundersame V\u00f6gel singen", "tokens": ["Wun\u00b7der\u00b7sa\u00b7me", "V\u00f6\u00b7gel", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings so sch\u00f6n, doch unsichtbar:", "tokens": ["Rings", "so", "sch\u00f6n", ",", "doch", "un\u00b7sicht\u00b7bar", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00e4tschernde Fontainen springen", "tokens": ["Pl\u00e4t\u00b7schern\u00b7de", "Fon\u00b7tai\u00b7nen", "sprin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus den Marmorbecken klar.", "tokens": ["Aus", "den", "Mar\u00b7mor\u00b7be\u00b7cken", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In dem Wellenglanz sich spiegelt", "tokens": ["In", "dem", "Wel\u00b7len\u00b7glanz", "sich", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Palast in gold'ner Zier;", "tokens": ["Sein", "Pa\u00b7last", "in", "gold'\u00b7ner", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosenb\u00fcsche sind gefl\u00fcgelt", "tokens": ["Ro\u00b7sen\u00b7b\u00fc\u00b7sche", "sind", "ge\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Paradiesesv\u00f6gel hier.", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7v\u00f6\u00b7gel", "hier", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Durch der Palmen S\u00e4ulenhallen,", "tokens": ["Durch", "der", "Pal\u00b7men", "S\u00e4u\u00b7len\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlank sich streckend kuppelan,", "tokens": ["Schlank", "sich", "stre\u00b7ckend", "kup\u00b7pe\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stumm in weh'nden Schleiern wallen", "tokens": ["Stumm", "in", "weh'n\u00b7den", "Schlei\u00b7ern", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ne Frauen stolz heran.", "tokens": ["Sch\u00f6\u00b7ne", "Frau\u00b7en", "stolz", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und die wei\u00dfen Schleier sinken!", "tokens": ["Und", "die", "wei\u00b7\u00dfen", "Schlei\u00b7er", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, der Augen Flammenschein!", "tokens": ["Ach", ",", "der", "Au\u00b7gen", "Flam\u00b7men\u00b7schein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sultanlaunisch will er winken,", "tokens": ["Sul\u00b7tan\u00b7lau\u00b7nisch", "will", "er", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie sind ja alle sein!", "tokens": ["Denn", "sie", "sind", "ja", "al\u00b7le", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PIS", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Horch, Geschrei von allen Seiten,", "tokens": ["Horch", ",", "Ge\u00b7schrei", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heulen, Jammern ihn erschreckt!", "tokens": ["Heu\u00b7len", ",", "Jam\u00b7mern", "ihn", "er\u00b7schreckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, des Klosters Vesperl\u00e4uten", "tokens": ["Ach", ",", "des", "Klos\u00b7ters", "Ves\u00b7per\u00b7l\u00e4u\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schrillen Tons hat ihn geweckt!", "tokens": ["Schril\u00b7len", "Tons", "hat", "ihn", "ge\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ei getrost! Zum Chor ist's eben", "tokens": ["Ei", "ge\u00b7trost", "!", "Zum", "Chor", "ist's", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "APPRART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Harem nicht allzuweit!", "tokens": ["Vom", "Ha\u00b7rem", "nicht", "all\u00b7zu\u00b7weit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "M\u00f6nch und Sultan, beide leben", "tokens": ["M\u00f6nch", "und", "Sul\u00b7tan", ",", "bei\u00b7de", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In bequemem Faltenkleid!", "tokens": ["In", "be\u00b7que\u00b7mem", "Fal\u00b7ten\u00b7kleid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und noch blickt dein Osten nieder,", "tokens": ["Und", "noch", "blickt", "dein", "Os\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Palm', am schlanken Schaft", "tokens": ["Dei\u00b7ne", "Palm'", ",", "am", "schlan\u00b7ken", "Schaft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwankend leis wie Pfaugefieder,", "tokens": ["Schwan\u00b7kend", "leis", "wie", "Pfau\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stumm, durchsichtig, geisterhaft.", "tokens": ["Stumm", ",", "durch\u00b7sich\u00b7tig", ",", "geis\u00b7ter\u00b7haft", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}