{"textgrid.poem.31372": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die H\u00e4upter der Riesen, hoch in der L\u00fcfte Meer,", "tokens": ["\u00dc\u00b7ber", "die", "H\u00e4up\u00b7ter", "der", "Rie\u00b7sen", ",", "hoch", "in", "der", "L\u00fcf\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Tr\u00e4gt mich, Vater der Riesen, dein dreigezackigter Fels.", "tokens": ["Tr\u00e4gt", "mich", ",", "Va\u00b7ter", "der", "Rie\u00b7sen", ",", "dein", "drei\u00b7ge\u00b7za\u00b7ckig\u00b7ter", "Fels", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nebel walten", "tokens": ["Ne\u00b7bel", "wal\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wie Nachtgestalten,", "tokens": ["Wie", "Nacht\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Um die Scheitel der Riesen her,", "tokens": ["Um", "die", "Schei\u00b7tel", "der", "Rie\u00b7sen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und ich erwarte dich, Leuchtender!", "tokens": ["Und", "ich", "er\u00b7war\u00b7te", "dich", ",", "Leuch\u00b7ten\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Deinen pr\u00e4chtigen Glanz borge der Finsternis,", "tokens": ["Dei\u00b7nen", "pr\u00e4ch\u00b7ti\u00b7gen", "Glanz", "bor\u00b7ge", "der", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Allerleuchtender Stern! Du der unendlichen Welt", "tokens": ["Al\u00b7ler\u00b7leuch\u00b7ten\u00b7der", "Stern", "!", "Du", "der", "un\u00b7end\u00b7li\u00b7chen", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ewiger Herrscher,", "tokens": ["E\u00b7wi\u00b7ger", "Herr\u00b7scher", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du des Lebens", "tokens": ["Du", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Unversiegbarer Quell, gie\u00dfe die Strahlen herauf,", "tokens": ["Un\u00b7ver\u00b7sieg\u00b7ba\u00b7rer", "Quell", ",", "gie\u00b7\u00dfe", "die", "Strah\u00b7len", "her\u00b7auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Helios! w\u00e4lze dein Flammenrad!", "tokens": ["He\u00b7li\u00b7os", "!", "w\u00e4l\u00b7ze", "dein", "Flam\u00b7men\u00b7rad", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Sieh! Er w\u00e4lzt es herauf! Die N\u00e4chte, wie sie entfliehn \u2013", "tokens": ["Sieh", "!", "Er", "w\u00e4lzt", "es", "her\u00b7auf", "!", "Die", "N\u00e4ch\u00b7te", ",", "wie", "sie", "ent\u00b7fliehn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$("], "meter": "--+--+-+-++-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Leuchtend schreibet der Gott seinen Namen dahin,", "tokens": ["Leuch\u00b7tend", "schrei\u00b7bet", "der", "Gott", "sei\u00b7nen", "Na\u00b7men", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PAV", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hingeschrieben", "tokens": ["Hin\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mit dem Griffel des Strahles,", "tokens": ["Mit", "dem", "Grif\u00b7fel", "des", "Strah\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u00bbkreaturen, huldigt ihr mir?\u00ab", "tokens": ["\u00bb", "kre\u00b7a\u00b7tu\u00b7ren", ",", "hul\u00b7digt", "ihr", "mir", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u2013 Leuchte, Herrscher! wir huldigen dir!", "tokens": ["\u2013", "Leuch\u00b7te", ",", "Herr\u00b7scher", "!", "wir", "hul\u00b7di\u00b7gen", "dir", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}