{"dta.poem.14303": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es qualmt' der eitle Markt in Staub und Schw\u00fcle,", "tokens": ["Es", "qualmt'", "der", "eit\u00b7le", "Markt", "in", "Staub", "und", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So klanglos \u00f6de wallend auf und nieder,", "tokens": ["So", "klang\u00b7los", "\u00f6\u00b7de", "wal\u00b7lend", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie dacht' ich da an meine Berge wieder,", "tokens": ["Wie", "dacht'", "ich", "da", "an", "mei\u00b7ne", "Ber\u00b7ge", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An frischen Sang, Felsquell und Waldesk\u00fchle!", "tokens": ["An", "fri\u00b7schen", "Sang", ",", "Fels\u00b7quell", "und", "Wal\u00b7des\u00b7k\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}}}}