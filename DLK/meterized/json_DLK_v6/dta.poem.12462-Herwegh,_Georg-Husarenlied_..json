{"dta.poem.12462": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Husarenlied   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es flammt mein Herz, es schwillt mein Muth,", "tokens": ["Es", "flammt", "mein", "Herz", ",", "es", "schwillt", "mein", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schwinge meinen Stahl,", "tokens": ["Ich", "schwin\u00b7ge", "mei\u00b7nen", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00e4tt' ich einen Federhut,", "tokens": ["Und", "h\u00e4tt'", "ich", "ei\u00b7nen", "Fe\u00b7der\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00e4r' ich General!", "tokens": ["So", "w\u00e4r'", "ich", "Ge\u00b7ne\u00b7ral", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie klingen die Trompeten hell", "tokens": ["Wie", "klin\u00b7gen", "die", "Trom\u00b7pe\u00b7ten", "hell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Des Morgens um die Vier!", "tokens": ["Des", "Mor\u00b7gens", "um", "die", "Vier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tambour schl\u00e4gt sein Eselsfell,", "tokens": ["Der", "Tam\u00b7bour", "schl\u00e4gt", "sein", "E\u00b7sels\u00b7fell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Esel schlagen wir.", "tokens": ["Die", "E\u00b7sel", "schla\u00b7gen", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zur Seite blitzt uns das Gewehr,", "tokens": ["Zur", "Sei\u00b7te", "blitzt", "uns", "das", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tod aus unsrer Hand;", "tokens": ["Der", "Tod", "aus", "uns\u00b7rer", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir reiten hin, wir reiten her,", "tokens": ["Wir", "rei\u00b7ten", "hin", ",", "wir", "rei\u00b7ten", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir reiten um's Vaterland.", "tokens": ["Wir", "rei\u00b7ten", "um's", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ob sich auch manch sch\u00f6nes Kind", "tokens": ["Und", "ob", "sich", "auch", "manch", "sch\u00f6\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Aeuglein schier zerweint:", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7glein", "schier", "zer\u00b7weint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Husaren sausen wie der Wind", "tokens": ["Hu\u00b7sa\u00b7ren", "sau\u00b7sen", "wie", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vor\u00fcber in den Feind.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "in", "den", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das ist ein Leben auf der Wacht,", "tokens": ["Das", "ist", "ein", "Le\u00b7ben", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lustig und so frei!", "tokens": ["So", "lus\u00b7tig", "und", "so", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das geht so leicht in hei\u00dfer Schlacht", "tokens": ["Das", "geht", "so", "leicht", "in", "hei\u00b7\u00dfer", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor\u00fcber und vorbei!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "und", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Himmel wird uns aufgethan", "tokens": ["Der", "Him\u00b7mel", "wird", "uns", "auf\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein Juwelenschrein;", "tokens": ["Wie", "ein", "Ju\u00b7we\u00b7len\u00b7schrein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Husarens\u00e4bel klopfen dran", "tokens": ["Hu\u00b7sa\u00b7ren\u00b7s\u00e4\u00b7bel", "klop\u00b7fen", "dran"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PAV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und drinnen ruft's: Herein!", "tokens": ["Und", "drin\u00b7nen", "ruft's", ":", "Her\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$.", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}