{"dta.poem.1932": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ob die Eiche wird belaubt", "tokens": ["Ob", "die", "Ei\u00b7che", "wird", "be\u00b7laubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Frost die Bl\u00e4tter raubt/", "tokens": ["Und", "der", "Frost", "die", "Bl\u00e4t\u00b7ter", "raubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pflegt sie doch der Lentz zu weisen/", "tokens": ["Pflegt", "sie", "doch", "der", "Lentz", "zu", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbesiegt durch Ey\u00df und Eisen/", "tokens": ["Un\u00b7be\u00b7siegt", "durch", "Ey\u00df", "und", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Neue Zweige sch\u00fcssen nach", "tokens": ["Neu\u00b7e", "Zwei\u00b7ge", "sch\u00fcs\u00b7sen", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo man Laub und Aeste brach:", "tokens": ["Wo", "man", "Laub", "und", "A\u00b7es\u00b7te", "brach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Unter Sturm/ Gefahr und Gr\u00e4men", "tokens": ["Un\u00b7ter", "Sturm", "/", "Ge\u00b7fahr", "und", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wei\u00df die Tugend zuzunehmen.", "tokens": ["Wei\u00df", "die", "Tu\u00b7gend", "zu\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}