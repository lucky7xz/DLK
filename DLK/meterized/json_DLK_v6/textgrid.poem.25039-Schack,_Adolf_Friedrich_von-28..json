{"textgrid.poem.25039": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich ahnte meine Seele lange,", "tokens": ["Dich", "ahn\u00b7te", "mei\u00b7ne", "See\u00b7le", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bevor mein Auge dich gesehn,", "tokens": ["Be\u00b7vor", "mein", "Au\u00b7ge", "dich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und selig-s\u00fc\u00dfe Schauer bange", "tokens": ["Und", "se\u00b7lig\u00b7s\u00fc\u00b7\u00dfe", "Schau\u00b7er", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt' ich durch all mein Wesen gehn.", "tokens": ["F\u00fchlt'", "ich", "durch", "all", "mein", "We\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sog von unbekannten Bl\u00fcten", "tokens": ["Ich", "sog", "von", "un\u00b7be\u00b7kann\u00b7ten", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Duft, der mir entgegenquoll,", "tokens": ["Den", "Duft", ",", "der", "mir", "ent\u00b7ge\u00b7gen\u00b7quoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nie erblickte Sterne gl\u00fchten", "tokens": ["Und", "nie", "er\u00b7blick\u00b7te", "Ster\u00b7ne", "gl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu H\u00e4upten mir geheimnisvoll.", "tokens": ["Zu", "H\u00e4up\u00b7ten", "mir", "ge\u00b7heim\u00b7nis\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch immer sah ich deinen Schatten", "tokens": ["Doch", "im\u00b7mer", "sah", "ich", "dei\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur tr\u00fcbe wie durch Nebelflor;", "tokens": ["Nur", "tr\u00fc\u00b7be", "wie", "durch", "Ne\u00b7bel\u00b7flor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Antlitz schien daraus in matten,", "tokens": ["Dein", "Ant\u00b7litz", "schien", "da\u00b7raus", "in", "mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gebrochnen Z\u00fcgen nur hervor.", "tokens": ["Ge\u00b7broch\u00b7nen", "Z\u00fc\u00b7gen", "nur", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als der Schleier nun gesunken,", "tokens": ["Und", "als", "der", "Schlei\u00b7er", "nun", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dich vor mir verh\u00fcllt \u2013 vergieb,", "tokens": ["Der", "dich", "vor", "mir", "ver\u00b7h\u00fcllt", "\u2013", "ver\u00b7gieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVPP", "$(", "VVIMP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn lang ich sprachlos und wie trunken,", "tokens": ["Wenn", "lang", "ich", "sprach\u00b7los", "und", "wie", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADJD", "KON", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bet\u00e4ubt von all dem Gl\u00fccke blieb!", "tokens": ["Be\u00b7t\u00e4ubt", "von", "all", "dem", "Gl\u00fc\u00b7cke", "blieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}