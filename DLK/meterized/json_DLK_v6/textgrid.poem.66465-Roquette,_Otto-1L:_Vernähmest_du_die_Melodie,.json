{"textgrid.poem.66465": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vern\u00e4hmest du die Melodie,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vern\u00e4hmest du die Melodie,", "tokens": ["Ver\u00b7n\u00e4h\u00b7mest", "du", "die", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie mir durch die Seele klingt,", "tokens": ["Wie", "sie", "mir", "durch", "die", "See\u00b7le", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' sie dem Worte Fl\u00fcgel lieh", "tokens": ["Eh'", "sie", "dem", "Wor\u00b7te", "Fl\u00fc\u00b7gel", "lieh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Lied zu flattern leicht beschwingt:", "tokens": ["Als", "Lied", "zu", "flat\u00b7tern", "leicht", "be\u00b7schwingt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKZU", "VVINF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann in dem leisen Traumgesang", "tokens": ["Dann", "in", "dem", "lei\u00b7sen", "Traum\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ungesungnen Lieder all", "tokens": ["Der", "un\u00b7ge\u00b7sung\u00b7nen", "Lie\u00b7der", "all"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verstehend gr\u00fc\u00dfte dich der Klang", "tokens": ["Ver\u00b7ste\u00b7hend", "gr\u00fc\u00df\u00b7te", "dich", "der", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als deiner Worte Wiederhall.", "tokens": ["Als", "dei\u00b7ner", "Wor\u00b7te", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zog erst das arme Lied hinaus,", "tokens": ["Zog", "erst", "das", "ar\u00b7me", "Lied", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann sucht es lange durch die Welt", "tokens": ["Dann", "sucht", "es", "lan\u00b7ge", "durch", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach jenem Klang im Heimathhaus,", "tokens": ["Nach", "je\u00b7nem", "Klang", "im", "Hei\u00b7math\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der einst so s\u00fc\u00df sich ihm gesellt.", "tokens": ["Der", "einst", "so", "s\u00fc\u00df", "sich", "ihm", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur wenn es pocht an deine Brust,", "tokens": ["Nur", "wenn", "es", "pocht", "an", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die einst die Schwingen ihm verlieh,", "tokens": ["Die", "einst", "die", "Schwin\u00b7gen", "ihm", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann findet es die ganze Lust", "tokens": ["Dann", "fin\u00b7det", "es", "die", "gan\u00b7ze", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der alten Heimathmelodie.", "tokens": ["Der", "al\u00b7ten", "Hei\u00b7math\u00b7me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}