{"textgrid.poem.33591": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auferstandne Stadt der Heiden,", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auferstandne Stadt der Heiden,", "tokens": ["Auf\u00b7er\u00b7stand\u00b7ne", "Stadt", "der", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei gegr\u00fc\u00dft, Ersehnte du!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "Er\u00b7sehn\u00b7te", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut noch heiter wie beim Scheiden", "tokens": ["Heut", "noch", "hei\u00b7ter", "wie", "beim", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachst du deiner Sonne zu.", "tokens": ["Lachst", "du", "dei\u00b7ner", "Son\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00dcberall aus dunkler Lava", "tokens": ["\u00dc\u00b7be\u00b7rall", "aus", "dunk\u00b7ler", "La\u00b7va"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00e4ngen Blumen sich ans Licht,", "tokens": ["Dr\u00e4n\u00b7gen", "Blu\u00b7men", "sich", "ans", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Reseda, die Agava,", "tokens": ["Die", "Re\u00b7se\u00b7da", ",", "die", "A\u00b7ga\u00b7va", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch die Myrte fehlet nicht.", "tokens": ["Auch", "die", "Myr\u00b7te", "feh\u00b7let", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rosen bl\u00fchn im Schlafgemache;", "tokens": ["Ro\u00b7sen", "bl\u00fchn", "im", "Schlaf\u00b7ge\u00b7ma\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lippen, die schon l\u00e4ngst verdorrt,", "tokens": ["Lip\u00b7pen", ",", "die", "schon", "l\u00e4ngst", "ver\u00b7dorrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprachen in der sch\u00f6nsten Sprache", "tokens": ["Spra\u00b7chen", "in", "der", "sch\u00f6ns\u00b7ten", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier dereinst der Liebe Wort.", "tokens": ["Hier", "de\u00b7reinst", "der", "Lie\u00b7be", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Um die S\u00e4ulen rankt sich wilder", "tokens": ["Um", "die", "S\u00e4u\u00b7len", "rankt", "sich", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PRF", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Efeu, und wie fr\u00fcher schau'n", "tokens": ["E\u00b7feu", ",", "und", "wie", "fr\u00fc\u00b7her", "schau'n"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "PWAV", "ADJD", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die erstandnen Marmorbilder", "tokens": ["Die", "er\u00b7stand\u00b7nen", "Mar\u00b7mor\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf zum alten \u00c4therblau'n.", "tokens": ["Auf", "zum", "al\u00b7ten", "\u00c4\u00b7ther\u00b7blau'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "APPRART", "ADJA", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur des Meeres wechselvolle", "tokens": ["Nur", "des", "Mee\u00b7res", "wech\u00b7sel\u00b7vol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Woge, die sonst hier gekreis't,", "tokens": ["Wo\u00b7ge", ",", "die", "sonst", "hier", "ge\u00b7kreis't", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wich von ihrer Uferscholle,", "tokens": ["Wich", "von", "ih\u00b7rer", "U\u00b7fer\u00b7schol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie sie der Menschengeist.", "tokens": ["Und", "wie", "sie", "der", "Men\u00b7schen\u00b7geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Eine andre Menschheit baute", "tokens": ["Ei\u00b7ne", "and\u00b7re", "Menschheit", "bau\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Dieser Tempel heitern Raum,", "tokens": ["Die\u00b7ser", "Tem\u00b7pel", "hei\u00b7tern", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nur fremd sieht die ergraute", "tokens": ["Und", "nur", "fremd", "sieht", "die", "er\u00b7grau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihrer Jugend fernen Traum.", "tokens": ["Ih\u00b7rer", "Ju\u00b7gend", "fer\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur wie halbverstandne Dichtung", "tokens": ["Nur", "wie", "halb\u00b7ver\u00b7stand\u00b7ne", "Dich\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnt auch mich, was hier noch gl\u00e4nzt;", "tokens": ["Mahnt", "auch", "mich", ",", "was", "hier", "noch", "gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, ich f\u00fchl's, wie gut Vernichtung", "tokens": ["Ach", ",", "ich", "f\u00fchl's", ",", "wie", "gut", "Ver\u00b7nich\u00b7tung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Vollendung sich begrenzt.", "tokens": ["Und", "Vol\u00b7len\u00b7dung", "sich", "be\u00b7grenzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Freudig kam ich, Stadt der Alten,", "tokens": ["Freu\u00b7dig", "kam", "ich", ",", "Stadt", "der", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Wehmut scheid' ich nun.", "tokens": ["Und", "mit", "Weh\u00b7mut", "scheid'", "ich", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrdest unter deiner kalten", "tokens": ["W\u00fcr\u00b7dest", "un\u00b7ter", "dei\u00b7ner", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lava du nicht besser ruhn?", "tokens": ["La\u00b7va", "du", "nicht", "bes\u00b7ser", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Auf die Worte der Beschw\u00f6rung", "tokens": ["Auf", "die", "Wor\u00b7te", "der", "Be\u00b7schw\u00f6\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiegst du z\u00f6gernd aus der Gruft;", "tokens": ["Stiegst", "du", "z\u00f6\u00b7gernd", "aus", "der", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt erst fa\u00dft dich die Zerst\u00f6rung \u2013\u2013", "tokens": ["Jetzt", "erst", "fa\u00dft", "dich", "die", "Zer\u00b7st\u00f6\u00b7rung", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schatten taugt nicht Himmelsluft.", "tokens": ["Schat\u00b7ten", "taugt", "nicht", "Him\u00b7mel\u00b7sluft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}