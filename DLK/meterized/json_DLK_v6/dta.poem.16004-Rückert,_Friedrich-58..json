{"dta.poem.16004": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wasser strebt hinab, das Feuer strebt hinauf,", "tokens": ["Das", "Was\u00b7ser", "strebt", "hin\u00b7ab", ",", "das", "Feu\u00b7er", "strebt", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zwischen beiden hat die Luft den st\u00e4ten Lauf.", "tokens": ["Und", "zwi\u00b7schen", "bei\u00b7den", "hat", "die", "Luft", "den", "st\u00e4\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Erde aber ruht, geht weder auf noch nieder.", "tokens": ["Die", "Er\u00b7de", "a\u00b7ber", "ruht", ",", "geht", "we\u00b7der", "auf", "noch", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "KON", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sind des Weltgeb\u00e4us nothwendige vier Glieder.", "tokens": ["Das", "sind", "des", "Welt\u00b7ge\u00b7b\u00e4us", "noth\u00b7wen\u00b7di\u00b7ge", "vier", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dieselben sind in dir; dein Wasser kommt von oben,", "tokens": ["Die\u00b7sel\u00b7ben", "sind", "in", "dir", ";", "dein", "Was\u00b7ser", "kommt", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zu der H\u00f6he hat dein Feuer sich erhoben.", "tokens": ["Und", "zu", "der", "H\u00f6\u00b7he", "hat", "dein", "Feu\u00b7er", "sich", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Frei schwebet deine Luft, der Weltvermittlung Odem,", "tokens": ["Frei", "schwe\u00b7bet", "dei\u00b7ne", "Luft", ",", "der", "Welt\u00b7ver\u00b7mitt\u00b7lung", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unersch\u00fctterlich ruht deiner Erde Bodem.", "tokens": ["Und", "un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7lich", "ruht", "dei\u00b7ner", "Er\u00b7de", "Bo\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}