{"textgrid.poem.24059": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ernte", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonnengie\u00dfen durch den Tag,", "tokens": ["Son\u00b7nen\u00b7gie\u00b7\u00dfen", "durch", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wellenhoch im fr\u00f6hlichen Schlag", "tokens": ["Wel\u00b7len\u00b7hoch", "im", "fr\u00f6h\u00b7li\u00b7chen", "Schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Geht mein Herz, es schaukelt leise", "tokens": ["Geht", "mein", "Herz", ",", "es", "schau\u00b7kelt", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Wiener Walzerweise.", "tokens": ["Ei\u00b7ne", "Wie\u00b7ner", "Wal\u00b7zer\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sensenschwung und Sichelschnitt,", "tokens": ["Sen\u00b7sen\u00b7schwung", "und", "Si\u00b7chel\u00b7schnitt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gr\u00fcn und gelb f\u00e4llt Gras und Aehre,", "tokens": ["Gr\u00fcn", "und", "gelb", "f\u00e4llt", "Gras", "und", "A\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Meine Freude erntet mit:", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "ern\u00b7tet", "mit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Segenschwere! Segenschwere!", "tokens": ["Se\u00b7gen\u00b7schwe\u00b7re", "!", "Se\u00b7gen\u00b7schwe\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter einem Lindenbaum,", "tokens": ["Un\u00b7ter", "ei\u00b7nem", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf des wei\u00dfen Kirchleins H\u00fcgel,", "tokens": ["Auf", "des", "wei\u00b7\u00dfen", "Kirch\u00b7leins", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruht ich aus; da hub mein Traum", "tokens": ["Ruht", "ich", "aus", ";", "da", "hub", "mein", "Traum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Surrend die Libellenfl\u00fcgel:", "tokens": ["Sur\u00b7rend", "die", "Li\u00b7bel\u00b7len\u00b7fl\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Steht ein Feld im Korne schwer,", "tokens": ["Steht", "ein", "Feld", "im", "Kor\u00b7ne", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwankt in goldnem Ueberschwange,", "tokens": ["Schwankt", "in", "gold\u00b7nem", "Ue\u00b7ber\u00b7schwan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fcchtefroh und reifebange,", "tokens": ["Fr\u00fcch\u00b7te\u00b7froh", "und", "rei\u00b7fe\u00b7ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trocken rauschend hin und her.", "tokens": ["Tro\u00b7cken", "rau\u00b7schend", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An des Segens goldnem Rand,", "tokens": ["An", "des", "Se\u00b7gens", "gold\u00b7nem", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo des Himmels Blau sich breitet,", "tokens": ["Wo", "des", "Him\u00b7mels", "Blau", "sich", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine Sense in der Hand,", "tokens": ["Ei\u00b7ne", "Sen\u00b7se", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine Bauerndirne schreitet.", "tokens": ["Ei\u00b7ne", "Bau\u00b7ern\u00b7dir\u00b7ne", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weit aus, wuchtig ist ihr Schritt,", "tokens": ["Weit", "aus", ",", "wuch\u00b7tig", "ist", "ihr", "Schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ueberh\u00e4upten ihr der Stahl", "tokens": ["Ue\u00b7ber\u00b7h\u00e4up\u00b7ten", "ihr", "der", "Stahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Lacht in huchig hellem Glitzen;", "tokens": ["Lacht", "in", "huc\u00b7hig", "hel\u00b7lem", "Glit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Schnell im Schwung mit einemmal", "tokens": ["Schnell", "im", "Schwung", "mit", "ei\u00b7nem\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Seh ichs durch die Bl\u00e4ue blitzen,", "tokens": ["Seh", "ichs", "durch", "die", "Bl\u00e4u\u00b7e", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und die Magd beginnt den Schnitt.", "tokens": ["Und", "die", "Magd", "be\u00b7ginnt", "den", "Schnitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Bogenhalb dreht sich ihr Leib,", "tokens": ["Bo\u00b7gen\u00b7halb", "dreht", "sich", "ihr", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.16": {"text": "Bogenweit greift aus das Eisen,", "tokens": ["Bo\u00b7gen\u00b7weit", "greift", "aus", "das", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "N\u00e4her, n\u00e4her kommt das Weib", "tokens": ["N\u00e4\u00b7her", ",", "n\u00e4\u00b7her", "kommt", "das", "Weib"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Hinter breitem Messerkreisen.", "tokens": ["Hin\u00b7ter", "brei\u00b7tem", "Mes\u00b7ser\u00b7krei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Langsam r\u00fchrt mit steter Kraft", "tokens": ["Lang\u00b7sam", "r\u00fchrt", "mit", "ste\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Sie der schweren Sense Schaft.", "tokens": ["Sie", "der", "schwe\u00b7ren", "Sen\u00b7se", "Schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Brach schon dehnt sich Stoppelleere.", "tokens": ["Brach", "schon", "dehnt", "sich", "Stop\u00b7pel\u00b7lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo rauschgolden sich die Aehre", "tokens": ["Wo", "rauschgol\u00b7den", "sich", "die", "A\u00b7eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In des Windes Wehn gewiegt,", "tokens": ["In", "des", "Win\u00b7des", "Wehn", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterbestarr das Leben liegt.", "tokens": ["Ster\u00b7be\u00b7starr", "das", "Le\u00b7ben", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "N\u00e4her, n\u00e4her kommt sie her,", "tokens": ["N\u00e4\u00b7her", ",", "n\u00e4\u00b7her", "kommt", "sie", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Seele f\u00e4llt mirs schwer.", "tokens": ["Auf", "die", "See\u00b7le", "f\u00e4llt", "mirs", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Augen zu. Ich h\u00f6re den Schnitt,", "tokens": ["Au\u00b7gen", "zu", ".", "Ich", "h\u00f6\u00b7re", "den", "Schnitt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und ein Klagen h\u00f6r ich mit", "tokens": ["Und", "ein", "Kla\u00b7gen", "h\u00f6r", "ich", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von Millionen Sterbequalen.", "tokens": ["Von", "Mil\u00b7lion\u00b7en", "Ster\u00b7be\u00b7qua\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stille dann. Scheu schau ich hin:", "tokens": ["Stil\u00b7le", "dann", ".", "Scheu", "schau", "ich", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.7": {"text": "Ruhend steht die Schnitterin", "tokens": ["Ru\u00b7hend", "steht", "die", "Schnit\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter Abendsonnenstrahlen.", "tokens": ["Un\u00b7ter", "A\u00b7bend\u00b7son\u00b7nen\u00b7strah\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Von des vollen Goldes Rot", "tokens": ["Von", "des", "vol\u00b7len", "Gol\u00b7des", "Rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Einen Augenschein umloht,", "tokens": ["Ei\u00b7nen", "Au\u00b7gen\u00b7schein", "um\u00b7loht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Dann im letzten, hellen Licht,", "tokens": ["Dann", "im", "letz\u00b7ten", ",", "hel\u00b7len", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Umri\u00dfschwarz ... Bist du der Tod!?", "tokens": ["Um\u00b7ri\u00df\u00b7schwarz", "...", "Bist", "du", "der", "Tod", "!?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Klar blickt sie mir ins Gesicht,", "tokens": ["Klar", "blickt", "sie", "mir", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.14": {"text": "G\u00fctig, gro\u00df und m\u00fctterlich,", "tokens": ["G\u00fc\u00b7tig", ",", "gro\u00df", "und", "m\u00fct\u00b7ter\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wendet in die Helle sich;", "tokens": ["Wen\u00b7det", "in", "die", "Hel\u00b7le", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Geht. Sie \u00fcberw\u00e4chst den Schein,", "tokens": ["Geht", ".", "Sie", "\u00fc\u00b7berw\u00b7\u00e4chst", "den", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Dunkel bricht von ihr herein.", "tokens": ["Dun\u00b7kel", "bricht", "von", "ihr", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo rauschgolden sich die Aehre", "tokens": ["Wo", "rauschgol\u00b7den", "sich", "die", "A\u00b7eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In des Windes Wehn gewiegt,", "tokens": ["In", "des", "Win\u00b7des", "Wehn", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterbestarr das Leben liegt.", "tokens": ["Ster\u00b7be\u00b7starr", "das", "Le\u00b7ben", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Allhin dehnt sich Stoppelleere.", "tokens": ["A\u00b7llhin", "dehnt", "sich", "Stop\u00b7pel\u00b7lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}