{"textgrid.poem.65925": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Anakreon", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Lober des Dionysos, Preiser des Weines,", "tokens": ["O", "Lo\u00b7ber", "des", "Di\u00b7o\u00b7ny\u00b7sos", ",", "Prei\u00b7ser", "des", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An\u00e1kreon, S\u00e4nger so holder Gestalt,", "tokens": ["An\u00e1\u00b7kre\u00b7on", ",", "S\u00e4n\u00b7ger", "so", "hol\u00b7der", "Ge\u00b7stalt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ich huldige auch des Bacchos' naher Gewalt:", "tokens": ["Ich", "hul\u00b7di\u00b7ge", "auch", "des", "Bac\u00b7chos'", "na\u00b7her", "Ge\u00b7walt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ach, gliche dem Lied deiner Gottfeiern meines!", "tokens": ["Ach", ",", "gli\u00b7che", "dem", "Lied", "dei\u00b7ner", "Gott\u00b7fei\u00b7ern", "mei\u00b7nes", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PPOSAT", "$."], "meter": "-+--++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Entblitzte der Liebe ein hingefunkt-reines", "tokens": ["Ent\u00b7blitz\u00b7te", "der", "Lie\u00b7be", "ein", "hin\u00b7ge\u00b7funk\u00b7trei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u2013 Urfrommend, doch eignes \u2013 Gedicht mir, so alt", "tokens": ["\u2013", "Ur\u00b7from\u00b7mend", ",", "doch", "eig\u00b7nes", "\u2013", "Ge\u00b7dicht", "mir", ",", "so", "alt"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVPP", "$,", "ADV", "ADJA", "$(", "NN", "PPER", "$,", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie Weisen entz\u00fcndender Weltengehalt:", "tokens": ["Wie", "Wei\u00b7sen", "ent\u00b7z\u00fcn\u00b7den\u00b7der", "Wel\u00b7ten\u00b7ge\u00b7halt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gel\u00e4nge Erf\u00fclltsein Geh\u00fclltheit des Scheines!", "tokens": ["Ge\u00b7l\u00e4n\u00b7ge", "Er\u00b7f\u00fcll\u00b7tsein", "Ge\u00b7h\u00fcll\u00b7theit", "des", "Schei\u00b7nes", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "An\u00e1kreon, Sehnsuchten singender Seher,", "tokens": ["An\u00e1\u00b7kre\u00b7on", ",", "Sehn\u00b7such\u00b7ten", "sin\u00b7gen\u00b7der", "Se\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Dein Weh \u00fcberwellt uns ein inselndes Meer:", "tokens": ["Dein", "Weh", "\u00fc\u00b7ber\u00b7wellt", "uns", "ein", "in\u00b7seln\u00b7des", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Du pochst an die Pforten Verliebter; nicht eher", "tokens": ["Du", "pochst", "an", "die", "Pfor\u00b7ten", "Ver\u00b7lieb\u00b7ter", ";", "nicht", "e\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "PTKNEG", "ADV"], "meter": "-+--+--+-++-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Entweiche vom Weltweg, als bis der Begehr", "tokens": ["Ent\u00b7wei\u00b7che", "vom", "Welt\u00b7weg", ",", "als", "bis", "der", "Be\u00b7gehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach flammenden Herzen veratmet! Beweher", "tokens": ["Nach", "flam\u00b7men\u00b7den", "Her\u00b7zen", "ver\u00b7at\u00b7met", "!", "Be\u00b7we\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ergl\u00fchender, wettre auf Wolken einher!", "tokens": ["Er\u00b7gl\u00fc\u00b7hen\u00b7der", ",", "wett\u00b7re", "auf", "Wol\u00b7ken", "ein\u00b7her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}