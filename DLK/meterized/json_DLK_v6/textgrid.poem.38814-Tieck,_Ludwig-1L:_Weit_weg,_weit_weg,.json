{"textgrid.poem.38814": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit weg, weit weg,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit weg, weit weg,", "tokens": ["Weit", "weg", ",", "weit", "weg", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von allen Schmerzen weg,", "tokens": ["Von", "al\u00b7len", "Schmer\u00b7zen", "weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch die W\u00e4lder m\u00f6cht' ich eilen,", "tokens": ["Durch", "die", "W\u00e4l\u00b7der", "m\u00f6cht'", "ich", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niederw\u00e4rts,", "tokens": ["Nie\u00b7der\u00b7w\u00e4rts", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Aufw\u00e4rts,", "tokens": ["Auf\u00b7w\u00e4rts", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Kl\u00fcften vor\u00fcber und von den steilen", "tokens": ["Kl\u00fcf\u00b7ten", "vor\u00b7\u00fc\u00b7ber", "und", "von", "den", "stei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "APPR", "ART", "ADJA"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Gebirgen st\u00fcrzen zu tiefen Gr\u00fcnden,", "tokens": ["Ge\u00b7bir\u00b7gen", "st\u00fcr\u00b7zen", "zu", "tie\u00b7fen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ruhe zu finden.", "tokens": ["Ru\u00b7he", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Pfeifender Wind,", "tokens": ["Pfei\u00b7fen\u00b7der", "Wind", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Treibe geschwind", "tokens": ["Trei\u00b7be", "ge\u00b7schwind"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schnell und schneller die Rosse in's Dickicht hinein!", "tokens": ["Schnell", "und", "schnel\u00b7ler", "die", "Ros\u00b7se", "in's", "Di\u00b7ckicht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "La\u00df, o la\u00df die tr\u00fcben Stunden,", "tokens": ["La\u00df", ",", "o", "la\u00df", "die", "tr\u00fc\u00b7ben", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "FM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eilend verschwunden,", "tokens": ["Ei\u00b7lend", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Rastlos nimmer Stillstand seyn.", "tokens": ["Rast\u00b7los", "nim\u00b7mer", "Still\u00b7stand", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo soll ich sie suchen?", "tokens": ["Wo", "soll", "ich", "sie", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf Bergesh\u00f6hn?", "tokens": ["Auf", "Ber\u00b7ges\u00b7h\u00f6hn", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im Schatten der Buchen?", "tokens": ["Im", "Schat\u00b7ten", "der", "Bu\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wo werd' ich sie sehn?", "tokens": ["Wo", "werd'", "ich", "sie", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Die Stunden verfliegen,", "tokens": ["Die", "Stun\u00b7den", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tag wechselt mit Nacht,", "tokens": ["Tag", "wech\u00b7selt", "mit", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Schmerzen besiegen,", "tokens": ["Die", "Schmer\u00b7zen", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Freuden erliegen", "tokens": ["Die", "Freu\u00b7den", "er\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der st\u00fcrmenden Macht.", "tokens": ["Der", "st\u00fcr\u00b7men\u00b7den", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ach! weiter, weiter ohne Stillstand,", "tokens": ["Ach", "!", "wei\u00b7ter", ",", "wei\u00b7ter", "oh\u00b7ne", "Still\u00b7stand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hin wo der Strom braust,", "tokens": ["Hin", "wo", "der", "Strom", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wo von steiler moosger Felswand", "tokens": ["Wo", "von", "stei\u00b7ler", "moos\u00b7ger", "Fels\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wind und Woge niedersaust.", "tokens": ["Wind", "und", "Wo\u00b7ge", "nie\u00b7der\u00b7saust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo Walddunkel schattet,", "tokens": ["Wo", "Wald\u00b7dun\u00b7kel", "schat\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo Wolken sich jagen,", "tokens": ["Wo", "Wol\u00b7ken", "sich", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und Nacht und banges Zagen", "tokens": ["Und", "Nacht", "und", "ban\u00b7ges", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit schwarzen Tr\u00e4umen sich gattet.", "tokens": ["Mit", "schwar\u00b7zen", "Tr\u00e4u\u00b7men", "sich", "gat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Thalnieder, bergauf,", "tokens": ["Thal\u00b7nie\u00b7der", ",", "ber\u00b7gauf", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Echo spricht, und gr\u00fc\u00dft her\u00fcber;", "tokens": ["E\u00b7cho", "spricht", ",", "und", "gr\u00fc\u00dft", "her\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! statt dieses Treibens, ende lieber,", "tokens": ["Ach", "!", "statt", "die\u00b7ses", "Trei\u00b7bens", ",", "en\u00b7de", "lie\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PDAT", "NN", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ende, ende diesen tr\u00fcben Lauf.", "tokens": ["En\u00b7de", ",", "en\u00b7de", "die\u00b7sen", "tr\u00fc\u00b7ben", "Lauf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "K\u00e4m' ich nur zum fremden Orte", "tokens": ["K\u00e4m'", "ich", "nur", "zum", "frem\u00b7den", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In ein wundervolles Land", "tokens": ["In", "ein", "wun\u00b7der\u00b7vol\u00b7les", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das kein Auge je gekannt,", "tokens": ["Das", "kein", "Au\u00b7ge", "je", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber wechselnd hier mit dort", "tokens": ["A\u00b7ber", "wech\u00b7selnd", "hier", "mit", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df ich schon die Einsamkeiten", "tokens": ["Wei\u00df", "ich", "schon", "die", "Ein\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sich t\u00fcckisch mir bereiten,", "tokens": ["Die", "sich", "t\u00fc\u00b7ckisch", "mir", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kenne schon die tr\u00fcben Leiden;", "tokens": ["Ken\u00b7ne", "schon", "die", "tr\u00fc\u00b7ben", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leiden, Leiden.", "tokens": ["Lei\u00b7den", ",", "Lei\u00b7den", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}