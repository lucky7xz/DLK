{"dta.poem.19113": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XcV.   Auf die Salbung K\u00f6nig Christian  \n des   VI.   und Konigin Sophia  \n Magdalena.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wach auf, Die ersten drey Strophen beziehen sich auf die alte D\u00e4nische\nHistorie, da in verschiedenen Altern die Helden und K\u00f6nige\nverbrannt, du Helden-Geist! von dem in jenen H\u00fcgeln", "tokens": ["Wach", "auf", ",", "Die", "ers\u00b7ten", "drey", "Stro\u00b7phen", "be\u00b7zie\u00b7hen", "sich", "auf", "die", "al\u00b7te", "D\u00e4\u00b7ni\u00b7sche", "His\u00b7to\u00b7rie", ",", "da", "in", "ver\u00b7schie\u00b7de\u00b7nen", "Al\u00b7tern", "die", "Hel\u00b7den", "und", "K\u00f6\u00b7ni\u00b7ge", "ver\u00b7brannt", ",", "du", "Hel\u00b7den\u00b7Geist", "!", "von", "dem", "in", "je\u00b7nen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "ART", "ADJA", "CARD", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "ADJA", "NN", "$,", "KOUS", "APPR", "ADJA", "NN", "ART", "NN", "KON", "NN", "VVPP", "$,", "PPER", "NN", "$.", "APPR", "ART", "APPR", "PDAT", "NN"], "meter": "-+-+--+--+--+-+-+--+-+-+-+--+--+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Zur Zeit des Bruna Olds verschlo\u00dfnen Ritter-Staub!", "tokens": ["Zur", "Zeit", "des", "Bru\u00b7na", "Olds", "ver\u00b7schlo\u00df\u00b7nen", "Rit\u00b7ter\u00b7Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Feuer-Flammen leyht den \u00fcberla\u00dfnen Raub,", "tokens": ["Ihr", "Feu\u00b7er\u00b7Flam\u00b7men", "leyht", "den", "\u00fc\u00b7berl\u00b7a\u00df\u00b7nen", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An diesem grossen Fest den Pallast zu verriegeln.", "tokens": ["An", "die\u00b7sem", "gros\u00b7sen", "Fest", "den", "Pal\u00b7last", "zu", "ver\u00b7rie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die H\u00fctten, die der Geist belebte, sind verzehrt,", "tokens": ["Die", "H\u00fct\u00b7ten", ",", "die", "der", "Geist", "be\u00b7leb\u00b7te", ",", "sind", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Flammen sind zur\u00fcck ins Element gekehrt.", "tokens": ["Die", "Flam\u00b7men", "sind", "zu\u00b7r\u00fcck", "ins", "E\u00b7le\u00b7ment", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}