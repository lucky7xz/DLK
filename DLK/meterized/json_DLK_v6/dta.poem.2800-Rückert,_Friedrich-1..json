{"dta.poem.2800": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne steigt, mit Gott! und golden ist der Osten;", "tokens": ["Die", "Son\u00b7ne", "steigt", ",", "mit", "Gott", "!", "und", "gol\u00b7den", "ist", "der", "Os\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "$.", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie tritt ihr Tagwerk an, und ich an meinen Posten.", "tokens": ["Sie", "tritt", "ihr", "Tag\u00b7werk", "an", ",", "und", "ich", "an", "mei\u00b7nen", "Pos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie will der Welt herauf neu f\u00fchren einen Tag;", "tokens": ["Sie", "will", "der", "Welt", "her\u00b7auf", "neu", "f\u00fch\u00b7ren", "ei\u00b7nen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Sch\u00f6nes bring' ich euch, so viel ich noch vermag.", "tokens": ["Und", "Sch\u00f6\u00b7nes", "bring'", "ich", "euch", ",", "so", "viel", "ich", "noch", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O bring es schnell, mein Geist! der Tag ist kurz gemessen,", "tokens": ["O", "bring", "es", "schnell", ",", "mein", "Geist", "!", "der", "Tag", "ist", "kurz", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herbst ist nun, doch so klar, da\u00df ich mich freue dessen.", "tokens": ["Herbst", "ist", "nun", ",", "doch", "so", "klar", ",", "da\u00df", "ich", "mich", "freu\u00b7e", "des\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kahl ist der Rosenstrauch, die Rosen sind vergessen,", "tokens": ["Kahl", "ist", "der", "Ro\u00b7sen\u00b7strauch", ",", "die", "Ro\u00b7sen", "sind", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sanft im Fr\u00fchglanz wankt der Wipfel der Zipressen.", "tokens": ["Doch", "sanft", "im", "Fr\u00fch\u00b7glanz", "wankt", "der", "Wip\u00b7fel", "der", "Zip\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}