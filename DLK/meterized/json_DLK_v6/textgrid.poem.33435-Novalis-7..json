{"textgrid.poem.33435": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenige wissen", "tokens": ["We\u00b7ni\u00b7ge", "wis\u00b7sen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das Geheimni\u00df der Liebe,", "tokens": ["Das", "Ge\u00b7heim\u00b7ni\u00df", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "F\u00fchlen Uners\u00e4ttlichkeit", "tokens": ["F\u00fch\u00b7len", "Un\u00b7er\u00b7s\u00e4tt\u00b7lich\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ewigen Durst.", "tokens": ["Und", "e\u00b7wi\u00b7gen", "Durst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Des Abendmahls", "tokens": ["Des", "A\u00b7bend\u00b7mahls"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "G\u00f6ttliche Bedeutung", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Be\u00b7deu\u00b7tung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.7": {"text": "Ist den irdischen Sinnen R\u00e4thsel;", "tokens": ["Ist", "den", "ir\u00b7di\u00b7schen", "Sin\u00b7nen", "R\u00e4th\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Aber wer jemals", "tokens": ["A\u00b7ber", "wer", "je\u00b7mals"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Von hei\u00dfen, geliebten Lippen", "tokens": ["Von", "hei\u00b7\u00dfen", ",", "ge\u00b7lieb\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Athem des Lebens sog,", "tokens": ["A\u00b7them", "des", "Le\u00b7bens", "sog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.11": {"text": "Wem heilige Gluth", "tokens": ["Wem", "hei\u00b7li\u00b7ge", "Gluth"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "In zitternde Wellen das Herz schmolz,", "tokens": ["In", "zit\u00b7tern\u00b7de", "Wel\u00b7len", "das", "Herz", "schmolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Wem das Auge aufging,", "tokens": ["Wem", "das", "Au\u00b7ge", "auf\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Da\u00df er des Himmels", "tokens": ["Da\u00df", "er", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Unergr\u00fcndliche Tiefe ma\u00df,", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7che", "Tie\u00b7fe", "ma\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Wird essen von seinem Leibe", "tokens": ["Wird", "es\u00b7sen", "von", "sei\u00b7nem", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und trinken von seinem Blute", "tokens": ["Und", "trin\u00b7ken", "von", "sei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Ewiglich.", "tokens": ["E\u00b7wig\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Wer hat des irdischen Leibes", "tokens": ["Wer", "hat", "des", "ir\u00b7di\u00b7schen", "Lei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hohen Sinn errathen?", "tokens": ["Ho\u00b7hen", "Sinn", "er\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wer kann sagen,", "tokens": ["Wer", "kann", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da\u00df er das Blut versteht?", "tokens": ["Da\u00df", "er", "das", "Blut", "ver\u00b7steht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Einst ist alles Leib,", "tokens": ["Einst", "ist", "al\u00b7les", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In himmlischem Blute", "tokens": ["In", "himm\u00b7li\u00b7schem", "Blu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Schwimmt das selige Paar. \u2013", "tokens": ["Schwimmt", "das", "se\u00b7li\u00b7ge", "Paar", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "O! da\u00df das Weltmeer", "tokens": ["O", "!", "da\u00df", "das", "Welt\u00b7meer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schon err\u00f6thete,", "tokens": ["Schon", "er\u00b7r\u00f6t\u00b7he\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und in duftiges Fleisch", "tokens": ["Und", "in", "duf\u00b7ti\u00b7ges", "Fleisch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aufqu\u00f6lle der Fels!", "tokens": ["Auf\u00b7qu\u00f6l\u00b7le", "der", "Fels", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nie endet das s\u00fc\u00dfe Mahl,", "tokens": ["Nie", "en\u00b7det", "das", "s\u00fc\u00b7\u00dfe", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nie s\u00e4ttigt die Liebe sich.", "tokens": ["Nie", "s\u00e4t\u00b7tigt", "die", "Lie\u00b7be", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nicht innig, nicht eigen genug", "tokens": ["Nicht", "in\u00b7nig", ",", "nicht", "ei\u00b7gen", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Kann sie haben den Geliebten.", "tokens": ["Kann", "sie", "ha\u00b7ben", "den", "Ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Von immer z\u00e4rteren Lippen", "tokens": ["Von", "im\u00b7mer", "z\u00e4r\u00b7te\u00b7ren", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Verwandelt wird das Genossene", "tokens": ["Ver\u00b7wan\u00b7delt", "wird", "das", "Ge\u00b7nos\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Inniglicher und n\u00e4her.", "tokens": ["In\u00b7nig\u00b7li\u00b7cher", "und", "n\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Hei\u00dfere Wollust", "tokens": ["Hei\u00b7\u00dfe\u00b7re", "Wol\u00b7lust"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durchbebt die Seele.", "tokens": ["Durch\u00b7bebt", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durstiger und hungriger", "tokens": ["Durs\u00b7ti\u00b7ger", "und", "hung\u00b7ri\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird das Herz:", "tokens": ["Wird", "das", "Herz", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und so w\u00e4hret der Liebe Genu\u00df", "tokens": ["Und", "so", "w\u00e4h\u00b7ret", "der", "Lie\u00b7be", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Von Ewigkeit zu Ewigkeit.", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00e4tten die N\u00fcchternen", "tokens": ["H\u00e4t\u00b7ten", "die", "N\u00fcch\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Einmal gekostet,", "tokens": ["Ein\u00b7mal", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Alles verlie\u00dfen sie,", "tokens": ["Al\u00b7les", "ver\u00b7lie\u00b7\u00dfen", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Und setzten sich zu uns", "tokens": ["Und", "setz\u00b7ten", "sich", "zu", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "An den Tisch der Sehnsucht,", "tokens": ["An", "den", "Tisch", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Der nie leer wird.", "tokens": ["Der", "nie", "leer", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Sie erkennten der Liebe", "tokens": ["Sie", "er\u00b7kenn\u00b7ten", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Unendliche F\u00fclle,", "tokens": ["Un\u00b7end\u00b7li\u00b7che", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Und priesen die Nahrung", "tokens": ["Und", "prie\u00b7sen", "die", "Nah\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Von Leib und Blut.", "tokens": ["Von", "Leib", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}