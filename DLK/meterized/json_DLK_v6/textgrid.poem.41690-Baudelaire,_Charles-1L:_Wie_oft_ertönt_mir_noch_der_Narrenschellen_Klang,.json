{"textgrid.poem.41690": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie oft ert\u00f6nt mir noch der Narrenschellen Klang,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft ert\u00f6nt mir noch der Narrenschellen Klang,", "tokens": ["Wie", "oft", "er\u00b7t\u00f6nt", "mir", "noch", "der", "Nar\u00b7ren\u00b7schel\u00b7len", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie oft noch, Zerrbild, k\u00fcss ich deine Stirn voll Grauen?", "tokens": ["Wie", "oft", "noch", ",", "Zerr\u00b7bild", ",", "k\u00fcss", "ich", "dei\u00b7ne", "Stirn", "voll", "Grau\u00b7en", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wieviel Geschosse noch verfliegen mir im Blauen,", "tokens": ["Wie\u00b7viel", "Ge\u00b7schos\u00b7se", "noch", "ver\u00b7flie\u00b7gen", "mir", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein K\u00f6cher, eh ein Pfeil das seltne Ziel durchdrang?", "tokens": ["Mein", "K\u00f6\u00b7cher", ",", "eh", "ein", "Pfeil", "das", "selt\u00b7ne", "Ziel", "durch\u00b7drang", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir schmieden Plan auf Plan, das ganze Leben lang;", "tokens": ["Wir", "schmie\u00b7den", "Plan", "auf", "Plan", ",", "das", "gan\u00b7ze", "Le\u00b7ben", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch schwer Gewaffen wird im Kampfe noch zerhauen,", "tokens": ["Manch", "schwer", "Ge\u00b7waf\u00b7fen", "wird", "im", "Kamp\u00b7fe", "noch", "zer\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eh wir die Kreatur, die riesenhafte, schauen,", "tokens": ["Eh", "wir", "die", "Kre\u00b7a\u00b7tur", ",", "die", "rie\u00b7sen\u00b7haf\u00b7te", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gesucht in ewger Gier, die Tr\u00e4nen uns entrang.", "tokens": ["Ge\u00b7sucht", "in", "ew\u00b7ger", "Gier", ",", "die", "Tr\u00e4\u00b7nen", "uns", "ent\u00b7rang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und Menschen gibt es, die stets fern von dem Idole,", "tokens": ["Und", "Men\u00b7schen", "gibt", "es", ",", "die", "stets", "fern", "von", "dem", "I\u00b7do\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und diesen Bildnern, die verdammt und lichtberaubt,", "tokens": ["Und", "die\u00b7sen", "Bild\u00b7nern", ",", "die", "ver\u00b7dammt", "und", "licht\u00b7be\u00b7raubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gezeichnet von der Schmach, dir mei\u00dfeln Brust und Haupt,", "tokens": ["Ge\u00b7zeich\u00b7net", "von", "der", "Schmach", ",", "dir", "mei\u00b7\u00dfeln", "Brust", "und", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Winkt eine Hoffnung nur gleich finstrem Kapitole!", "tokens": ["Winkt", "ei\u00b7ne", "Hoff\u00b7nung", "nur", "gleich", "fins\u00b7trem", "Ka\u00b7pi\u00b7to\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihnen einst der Tod, ein neues Lichtgestirn,", "tokens": ["Da\u00df", "ih\u00b7nen", "einst", "der", "Tod", ",", "ein", "neu\u00b7es", "Licht\u00b7ges\u00b7tirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Blume sprie\u00dfen l\u00e4\u00dft und bl\u00fchn aus ihrem Hirn!", "tokens": ["Die", "Blu\u00b7me", "sprie\u00b7\u00dfen", "l\u00e4\u00dft", "und", "bl\u00fchn", "aus", "ih\u00b7rem", "Hirn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}