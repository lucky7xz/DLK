{"textgrid.poem.26199": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und immer geiler der Holunder im Dunkelgr\u00fcnen bl\u00fcht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und immer geiler der Holunder im Dunkelgr\u00fcnen bl\u00fcht", "tokens": ["Und", "im\u00b7mer", "gei\u00b7ler", "der", "Ho\u00b7lun\u00b7der", "im", "Dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und in der Nacht wie ein Verf\u00fchrer blind sich m\u00fcht.", "tokens": ["Und", "in", "der", "Nacht", "wie", "ein", "Ver\u00b7f\u00fch\u00b7rer", "blind", "sich", "m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hat sich in der schw\u00fclen Luft breitbr\u00fcstig aufgemacht.", "tokens": ["Er", "hat", "sich", "in", "der", "schw\u00fc\u00b7len", "Luft", "breit\u00b7br\u00fcs\u00b7tig", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Er lacht an allen Gartent\u00fcren, wie ein Brandstifter heimlich lacht,", "tokens": ["Er", "lacht", "an", "al\u00b7len", "Gar\u00b7ten\u00b7t\u00fc\u00b7ren", ",", "wie", "ein", "Brand\u00b7stif\u00b7ter", "heim\u00b7lich", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Die Wurzel seinen Rumpf mit viel Geheimem gern ern\u00e4hrt,", "tokens": ["Die", "Wur\u00b7zel", "sei\u00b7nen", "Rumpf", "mit", "viel", "Ge\u00b7hei\u00b7mem", "gern", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Und um ihn rings die Luft toll von den tollsten Schw\u00fcren g\u00e4hrt.", "tokens": ["Und", "um", "ihn", "rings", "die", "Luft", "toll", "von", "den", "tolls\u00b7ten", "Schw\u00fc\u00b7ren", "g\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "ADV", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Er hat schon manchen Schrei erstickt mit seiner Bl\u00fcten Brunstgeruch,", "tokens": ["Er", "hat", "schon", "man\u00b7chen", "Schrei", "er\u00b7stickt", "mit", "sei\u00b7ner", "Bl\u00fc\u00b7ten", "Brunst\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Und hat oft zweien Leib an Leib ein Dach f\u00fcr Lust und Fluch gew\u00e4hrt,", "tokens": ["Und", "hat", "oft", "zwei\u00b7en", "Leib", "an", "Leib", "ein", "Dach", "f\u00fcr", "Lust", "und", "Fluch", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "CARD", "NN", "APPR", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Da\u00df manche Hand nach Jahren noch ans Herz sich f\u00e4hrt.", "tokens": ["Da\u00df", "man\u00b7che", "Hand", "nach", "Jah\u00b7ren", "noch", "ans", "Herz", "sich", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NN", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}