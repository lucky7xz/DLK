{"dta.poem.1103": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "J. J!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ach herber Todes-Fall! Abschatz wird uns entrissen/ ", "tokens": ["Ach", "her\u00b7ber", "To\u00b7des\u00b7Fall", "!", "Ab\u00b7schatz", "wird", "uns", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$.", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Eh-Gemahl das folgt/ ein Merckmahl das was", "tokens": ["Das", "Eh\u00b7Ge\u00b7mahl", "das", "folgt", "/", "ein", "Merck\u00b7mahl", "das", "was"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VVFIN", "$(", "ART", "NN", "ART", "PRELS"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "hat ", "tokens": ["hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Von Seltenem in sich/ es klaget Land und Stadt/", "tokens": ["Von", "Sel\u00b7te\u00b7nem", "in", "sich", "/", "es", "kla\u00b7get", "Land", "und", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PRF", "$(", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es f\u00e4llet uns nun weg/ wir m\u00fcssen nun vermissen/", "tokens": ["Es", "f\u00e4l\u00b7let", "uns", "nun", "weg", "/", "wir", "m\u00fcs\u00b7sen", "nun", "ver\u00b7mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von dem der K\u00e4yser selbst und seine Grossen wissen.", "tokens": ["Von", "dem", "der", "K\u00e4y\u00b7ser", "selbst", "und", "sei\u00b7ne", "Gros\u00b7sen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier fand man Unterricht/ hier fand man klugen Rath:", "tokens": ["Hier", "fand", "man", "Un\u00b7ter\u00b7richt", "/", "hier", "fand", "man", "klu\u00b7gen", "Rath", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$(", "ADV", "VVFIN", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Tugend war sein Weg/ die Treue war sein Pfad.", "tokens": ["Die", "Tu\u00b7gend", "war", "sein", "Weg", "/", "die", "Treu\u00b7e", "war", "sein", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Teutsche Tichter-Welt kunt\u2019 ihn vor Meister gr\u00fcssen.", "tokens": ["Die", "Teut\u00b7sche", "Tich\u00b7ter\u00b7Welt", "kunt'", "ihn", "vor", "Meis\u00b7ter", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Edlen Auge schl\u00e4fft/ es schweigt der St\u00e4nde Mund:", "tokens": ["Der", "Ed\u00b7len", "Au\u00b7ge", "schl\u00e4fft", "/", "es", "schweigt", "der", "St\u00e4n\u00b7de", "Mund", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer wird uns ferner nun/ was n\u00f6thig/ machen kund?", "tokens": ["Wer", "wird", "uns", "fer\u00b7ner", "nun", "/", "was", "n\u00f6\u00b7thig", "/", "ma\u00b7chen", "kund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "$(", "PWS", "ADJD", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer wird des K\u00e4ysers Dienst/ des Landes Heyl erwe-", "tokens": ["Wer", "wird", "des", "K\u00e4y\u00b7sers", "Dienst", "/", "des", "Lan\u00b7des", "Heyl", "er\u00b7we"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "gen? ", "tokens": ["gen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Danckt unsrem ", "tokens": ["Danckt", "uns\u00b7rem"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "So wird des HErren Huld uns wieder blicken an:", "tokens": ["So", "wird", "des", "Her\u00b7ren", "Huld", "uns", "wie\u00b7der", "bli\u00b7cken", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Blo\u00df Danck und Danckbarkeit kan bringen uns den Se-", "tokens": ["Blo\u00df", "Danck", "und", "Dan\u00b7ck\u00b7bar\u00b7keit", "kan", "brin\u00b7gen", "uns", "den", "Se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VMFIN", "VVINF", "PPER", "ART", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "gen. ", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}}}}}