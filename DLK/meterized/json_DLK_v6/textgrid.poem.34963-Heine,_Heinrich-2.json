{"textgrid.poem.34963": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch aus dem blauen Himmelszelt", "tokens": ["Hoch", "aus", "dem", "blau\u00b7en", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel tausend Sterne schauen,", "tokens": ["Viel", "tau\u00b7send", "Ster\u00b7ne", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sehns\u00fcchtig gl\u00e4nzend, gro\u00df und klug,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "gl\u00e4n\u00b7zend", ",", "gro\u00df", "und", "klug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wie Augen von sch\u00f6nen Frauen.", "tokens": ["Wie", "Au\u00b7gen", "von", "sch\u00f6\u00b7nen", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie blicken hinunter in das Meer,", "tokens": ["Sie", "bli\u00b7cken", "hin\u00b7un\u00b7ter", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das weithin \u00fcberzogen", "tokens": ["Das", "weit\u00b7hin", "\u00fc\u00b7berz\u00b7o\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit phosphorstrahlendem Purpurduft;", "tokens": ["Mit", "phos\u00b7phor\u00b7strah\u00b7len\u00b7dem", "Pur\u00b7pur\u00b7duft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Woll\u00fcstig girren die Wogen.", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "gir\u00b7ren", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Segel flattert am Sklavenschiff,", "tokens": ["Kein", "Se\u00b7gel", "flat\u00b7tert", "am", "Skla\u00b7ven\u00b7schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es liegt wie abgetakelt;", "tokens": ["Es", "liegt", "wie", "ab\u00b7ge\u00b7ta\u00b7kelt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch schimmern Laternen auf dem Verdeck,", "tokens": ["Doch", "schim\u00b7mern", "La\u00b7ter\u00b7nen", "auf", "dem", "Ver\u00b7deck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wo Tanzmusik spektakelt.", "tokens": ["Wo", "Tanz\u00b7mu\u00b7sik", "spek\u00b7ta\u00b7kelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Fiedel streicht der Steuermann,", "tokens": ["Die", "Fie\u00b7del", "streicht", "der", "Steu\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Koch, der spielt die Fl\u00f6te,", "tokens": ["Der", "Koch", ",", "der", "spielt", "die", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Schiffsjung' schl\u00e4gt die Trommel dazu,", "tokens": ["Ein", "Schiffs\u00b7jung'", "schl\u00e4gt", "die", "Trom\u00b7mel", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Doktor bl\u00e4st die Trompete.", "tokens": ["Der", "Dok\u00b7tor", "bl\u00e4st", "die", "Trom\u00b7pe\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Wohl hundert Neger, M\u00e4nner und Fraun,", "tokens": ["Wohl", "hun\u00b7dert", "Ne\u00b7ger", ",", "M\u00e4n\u00b7ner", "und", "Fraun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie jauchzen und hopsen und kreisen", "tokens": ["Sie", "jauch\u00b7zen", "und", "hop\u00b7sen", "und", "krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie toll herum; bei jedem Sprung", "tokens": ["Wie", "toll", "he\u00b7rum", ";", "bei", "je\u00b7dem", "Sprung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PTKVZ", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Taktm\u00e4\u00dfig klirren die Eisen.", "tokens": ["Takt\u00b7m\u00e4\u00b7\u00dfig", "klir\u00b7ren", "die", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Sie stampfen den Boden mit tobender Lust,", "tokens": ["Sie", "stamp\u00b7fen", "den", "Bo\u00b7den", "mit", "to\u00b7ben\u00b7der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und manche schwarze Sch\u00f6ne", "tokens": ["Und", "man\u00b7che", "schwar\u00b7ze", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umschlingt woll\u00fcstig den nackten Geno\u00df \u2013", "tokens": ["Um\u00b7schlingt", "wol\u00b7l\u00fcs\u00b7tig", "den", "nack\u00b7ten", "Ge\u00b7no\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dazwischen \u00e4chzende T\u00f6ne.", "tokens": ["Da\u00b7zwi\u00b7schen", "\u00e4ch\u00b7zen\u00b7de", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Der B\u00fcttel ist Ma\u00eetre des plaisirs,", "tokens": ["Der", "B\u00fct\u00b7tel", "ist", "Ma\u00eetre", "des", "plai\u00b7sirs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ART", "ADJA", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und hat mit Peitschenhieben", "tokens": ["Und", "hat", "mit", "Peit\u00b7schen\u00b7hie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die l\u00e4ssigen T\u00e4nzer stimuliert,", "tokens": ["Die", "l\u00e4s\u00b7si\u00b7gen", "T\u00e4n\u00b7zer", "sti\u00b7mu\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Frohsinn angetrieben.", "tokens": ["Zum", "Froh\u00b7sinn", "an\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und Dideldumdei und Schnedderedeng!", "tokens": ["Und", "Di\u00b7del\u00b7dum\u00b7dei", "und", "Schned\u00b7de\u00b7re\u00b7deng", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der L\u00e4rm lockt aus den Tiefen", "tokens": ["Der", "L\u00e4rm", "lockt", "aus", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Unget\u00fcme der Wasserwelt,", "tokens": ["Die", "Un\u00b7ge\u00b7t\u00fc\u00b7me", "der", "Was\u00b7ser\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die dort bl\u00f6dsinnig schliefen.", "tokens": ["Die", "dort", "bl\u00f6d\u00b7sin\u00b7nig", "schlie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Schlaftrunken kommen geschwommen heran", "tokens": ["Schlaf\u00b7trun\u00b7ken", "kom\u00b7men", "ge\u00b7schwom\u00b7men", "he\u00b7ran"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVPP", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Haifische, viele hundert;", "tokens": ["Hai\u00b7fi\u00b7sche", ",", "vie\u00b7le", "hun\u00b7dert", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "CARD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie glotzen nach dem Schiff hinauf,", "tokens": ["Sie", "glot\u00b7zen", "nach", "dem", "Schiff", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sind verdutzt, verwundert.", "tokens": ["Sie", "sind", "ver\u00b7dutzt", ",", "ver\u00b7wun\u00b7dert", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie merken, da\u00df die Fr\u00fchst\u00fcckstund'", "tokens": ["Sie", "mer\u00b7ken", ",", "da\u00df", "die", "Fr\u00fch\u00b7st\u00fcck\u00b7stund'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch nicht gekommen, und g\u00e4hnen,", "tokens": ["Noch", "nicht", "ge\u00b7kom\u00b7men", ",", "und", "g\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aufsperrend den Rachen; die Kiefer sind", "tokens": ["Auf\u00b7sper\u00b7rend", "den", "Ra\u00b7chen", ";", "die", "Kie\u00b7fer", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bepflanzt mit S\u00e4gez\u00e4hnen.", "tokens": ["Be\u00b7pflanzt", "mit", "S\u00e4\u00b7ge\u00b7z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und Dideldumdei und Schnedderedeng \u2013", "tokens": ["Und", "Di\u00b7del\u00b7dum\u00b7dei", "und", "Schned\u00b7de\u00b7re\u00b7deng", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es nehmen kein Ende die T\u00e4nze.", "tokens": ["Es", "neh\u00b7men", "kein", "En\u00b7de", "die", "T\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Haifische bei\u00dfen vor Ungeduld", "tokens": ["Die", "Hai\u00b7fi\u00b7sche", "bei\u00b7\u00dfen", "vor", "Un\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sich selber in die Schw\u00e4nze.", "tokens": ["Sich", "sel\u00b7ber", "in", "die", "Schw\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich glaube, sie lieben nicht die Musik,", "tokens": ["Ich", "glau\u00b7be", ",", "sie", "lie\u00b7ben", "nicht", "die", "Mu\u00b7sik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie viele von ihrem Gelichter.", "tokens": ["Wie", "vie\u00b7le", "von", "ih\u00b7rem", "Ge\u00b7lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbtrau keiner Bestie, die nicht liebt", "tokens": ["\u00bb", "trau", "kei\u00b7ner", "Be\u00b7stie", ",", "die", "nicht", "liebt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Musik!\u00ab sagt Albions gro\u00dfer Dichter.", "tokens": ["Mu\u00b7sik", "!", "\u00ab", "sagt", "Al\u00b7bions", "gro\u00b7\u00dfer", "Dich\u00b7ter."], "token_info": ["word", "punct", "punct", "word", "word", "word", "abbreviation"], "pos": ["NN", "$.", "$(", "VVFIN", "NE", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.13": {"line.1": {"text": "Und Schnedderedeng und Dideldumdei \u2013", "tokens": ["Und", "Schned\u00b7de\u00b7re\u00b7deng", "und", "Di\u00b7del\u00b7dum\u00b7dei", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NE", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die T\u00e4nze nehmen kein Ende.", "tokens": ["Die", "T\u00e4n\u00b7ze", "neh\u00b7men", "kein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Am Fockmast steht Mynheer van Koek", "tokens": ["Am", "Fock\u00b7mast", "steht", "My\u00b7nheer", "van", "Ko\u00b7ek"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NE", "NE", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und faltet betend die H\u00e4nde:", "tokens": ["Und", "fal\u00b7tet", "be\u00b7tend", "die", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "\u00bbum Christi willen verschone, o Herr,", "tokens": ["\u00bb", "um", "Chris\u00b7ti", "wil\u00b7len", "ver\u00b7scho\u00b7ne", ",", "o", "Herr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "NN", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Leben der schwarzen S\u00fcnder!", "tokens": ["Das", "Le\u00b7ben", "der", "schwar\u00b7zen", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erz\u00fcrnten sie dich, so wei\u00dft du ja,", "tokens": ["Er\u00b7z\u00fcrn\u00b7ten", "sie", "dich", ",", "so", "wei\u00dft", "du", "ja", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie sind so dumm wie die Rinder.", "tokens": ["Sie", "sind", "so", "dumm", "wie", "die", "Rin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Verschone ihr Leben um Christi will'n,", "tokens": ["Ver\u00b7scho\u00b7ne", "ihr", "Le\u00b7ben", "um", "Chris\u00b7ti", "will'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der f\u00fcr uns alle gestorben!", "tokens": ["Der", "f\u00fcr", "uns", "al\u00b7le", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PIS", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Denn bleiben mir nicht dreihundert St\u00fcck,", "tokens": ["Denn", "blei\u00b7ben", "mir", "nicht", "drei\u00b7hun\u00b7dert", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "CARD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So ist mein Gesch\u00e4ft verdorben.\u00ab", "tokens": ["So", "ist", "mein", "Ge\u00b7sch\u00e4ft", "ver\u00b7dor\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}