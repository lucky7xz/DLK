{"dta.poem.21491": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Zu Gott !  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00fcber sturmgejagten,", "tokens": ["Wie", "\u00fc\u00b7ber", "sturm\u00b7ge\u00b7jag\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nachtwirren Wassern", "tokens": ["Nacht\u00b7wir\u00b7ren", "Was\u00b7sern"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Einsam der Mond wandelt,", "tokens": ["Ein\u00b7sam", "der", "Mond", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--++-", "measure": "dactylic.init"}, "line.4": {"text": "Durch Wolken verdeckt,", "tokens": ["Durch", "Wol\u00b7ken", "ver\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "So \u00fcber den Welten", "tokens": ["So", "\u00fc\u00b7ber", "den", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schreitet Gott dahin.", "tokens": ["Schrei\u00b7tet", "Gott", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Unser Auge schaut dich nicht,", "tokens": ["Un\u00b7ser", "Au\u00b7ge", "schaut", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn blind von den L\u00fcsten", "tokens": ["Denn", "blind", "von", "den", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Des staubgeborenen", "tokens": ["Des", "staub\u00b7ge\u00b7bo\u00b7re\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "S\u00fcndigen Leibes", "tokens": ["S\u00fcn\u00b7di\u00b7gen", "Lei\u00b7bes"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "H\u00e4ngt es am Boden.", "tokens": ["H\u00e4ngt", "es", "am", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ueber uns wallt, dicht wie Bergnebel,", "tokens": ["Ue\u00b7ber", "uns", "wallt", ",", "dicht", "wie", "Berg\u00b7ne\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nur Dampf und Rauch,", "tokens": ["Nur", "Dampf", "und", "Rauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Aufqualmend vom Blute,", "tokens": ["Auf\u00b7qual\u00b7mend", "vom", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Das die S\u00fcnde vergossen,", "tokens": ["Das", "die", "S\u00fcn\u00b7de", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wallt zwischen dir und uns,", "tokens": ["Wallt", "zwi\u00b7schen", "dir", "und", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df h\u00f6hnische Lippen murren:", "tokens": ["Da\u00df", "h\u00f6h\u00b7ni\u00b7sche", "Lip\u00b7pen", "mur\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Es ist kein Gott!", "tokens": ["Es", "ist", "kein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Denn alle Liebe, die du erschaust", "tokens": ["Denn", "al\u00b7le", "Lie\u00b7be", ",", "die", "du", "er\u00b7schaust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unter den Menschenkindern,", "tokens": ["Un\u00b7ter", "den", "Men\u00b7schen\u00b7kin\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ist Gemeinheit, Ekel,", "tokens": ["Ist", "Ge\u00b7mein\u00b7heit", ",", "E\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Des Weibes, des Mannes Gluth", "tokens": ["Des", "Wei\u00b7bes", ",", "des", "Man\u00b7nes", "Gluth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verl\u00f6schen im Schlamm der L\u00fcste,", "tokens": ["Ver\u00b7l\u00f6\u00b7schen", "im", "Schlamm", "der", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und keine Freude ist,", "tokens": ["Und", "kei\u00b7ne", "Freu\u00b7de", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die nicht in Thr\u00e4nen geboren,", "tokens": ["Die", "nicht", "in", "Thr\u00e4\u00b7nen", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In Thr\u00e4nen erstirbt.", "tokens": ["In", "Thr\u00e4\u00b7nen", "er\u00b7stirbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ich aber erkannte dich", "tokens": ["Ich", "a\u00b7ber", "er\u00b7kann\u00b7te", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In dunkler Thr\u00e4nennacht,", "tokens": ["In", "dunk\u00b7ler", "Thr\u00e4\u00b7nen\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Sehnsucht in mir schwoll,", "tokens": ["Als", "Sehn\u00b7sucht", "in", "mir", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mild wie ein Thautropfen", "tokens": ["Und", "mild", "wie", "ein", "Thau\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "In d\u00fcrres Laub,", "tokens": ["In", "d\u00fcr\u00b7res", "Laub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Fiel in meine Seele", "tokens": ["Fiel", "in", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dein Erkennen.", "tokens": ["Dein", "Er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Ich bin entbrannt in Liebe zu dir,", "tokens": ["Ich", "bin", "ent\u00b7brannt", "in", "Lie\u00b7be", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich lodre wie die Sonne,", "tokens": ["Ich", "lod\u00b7re", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich gl\u00fche wie ein Schwert", "tokens": ["Ich", "gl\u00fc\u00b7he", "wie", "ein", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In sausenden Feuern.", "tokens": ["In", "sau\u00b7sen\u00b7den", "Feu\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Empor, empor durch den Dampf,", "tokens": ["Em\u00b7por", ",", "em\u00b7por", "durch", "den", "Dampf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der L\u00fcfte finstern Graus!", "tokens": ["Der", "L\u00fcf\u00b7te", "fins\u00b7tern", "Graus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fl\u00fcgel! Fl\u00fcgel!", "tokens": ["Fl\u00fc\u00b7gel", "!", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Der du dein sch\u00f6nes heiliges Antlitz", "tokens": ["Der", "du", "dein", "sch\u00f6\u00b7nes", "hei\u00b7li\u00b7ges", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verbirgst uns schmerzbeladenen,", "tokens": ["Ver\u00b7birgst", "uns", "schmerz\u00b7be\u00b7la\u00b7de\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "M\u00fchsal-Leidenden", "tokens": ["M\u00fch\u00b7sa\u00b7lLei\u00b7den\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Unseligen Menschen,", "tokens": ["Un\u00b7se\u00b7li\u00b7gen", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Willst Du in Qualen uns lassen,", "tokens": ["Willst", "Du", "in", "Qua\u00b7len", "uns", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Ewig verschlie\u00dfen f\u00fcr uns dein Herz,", "tokens": ["E\u00b7wig", "ver\u00b7schlie\u00b7\u00dfen", "f\u00fcr", "uns", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nur allein trinken", "tokens": ["Nur", "al\u00b7lein", "trin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Vom Borne deiner Liebe,", "tokens": ["Vom", "Bor\u00b7ne", "dei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wie eine kalte sch\u00f6ne Geliebte", "tokens": ["Wie", "ei\u00b7ne", "kal\u00b7te", "sch\u00f6\u00b7ne", "Ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dich berauschen an dir selber?", "tokens": ["Dich", "be\u00b7rau\u00b7schen", "an", "dir", "sel\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber ich will dringen zu dir,", "tokens": ["A\u00b7ber", "ich", "will", "drin\u00b7gen", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ueber die Welten hinaus,", "tokens": ["Ue\u00b7ber", "die", "Wel\u00b7ten", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und an den morgigen Thoren,", "tokens": ["Und", "an", "den", "mor\u00b7gi\u00b7gen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo der Leib zerf\u00e4llt", "tokens": ["Wo", "der", "Leib", "zer\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "In m\u00fcrben Staub,", "tokens": ["In", "m\u00fcr\u00b7ben", "Staub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Soll meine Seele umfluthet", "tokens": ["Soll", "mei\u00b7ne", "See\u00b7le", "um\u00b7flut\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Von strahlenden Aetherfeuern", "tokens": ["Von", "strah\u00b7len\u00b7den", "A\u00b7e\u00b7ther\u00b7feu\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Mit dir ringen, H\u00fcft\u2019 an H\u00fcfte,", "tokens": ["Mit", "dir", "rin\u00b7gen", ",", "H\u00fcft'", "an", "H\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aug\u2019 in Auge gluthend,", "tokens": ["Aug'", "in", "Au\u00b7ge", "glu\u00b7thend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Nicht lassen von dir,", "tokens": ["Nicht", "las\u00b7sen", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Bis du mich gesegnet!", "tokens": ["Bis", "du", "mich", "ge\u00b7seg\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Da\u00df ich niedersteige", "tokens": ["Da\u00df", "ich", "nie\u00b7ders\u00b7tei\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ein besserer Prometheus,", "tokens": ["Ein", "bes\u00b7se\u00b7rer", "Pro\u00b7me\u00b7theus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "In beiden H\u00e4nden", "tokens": ["In", "bei\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Schwertragend eine feuergl\u00fchende", "tokens": ["Schwer\u00b7tra\u00b7gend", "ei\u00b7ne", "feu\u00b7er\u00b7gl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "ADJA"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Dampfende Opferschale,", "tokens": ["Damp\u00b7fen\u00b7de", "Op\u00