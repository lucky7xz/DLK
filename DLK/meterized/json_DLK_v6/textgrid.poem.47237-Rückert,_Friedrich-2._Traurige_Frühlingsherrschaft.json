{"textgrid.poem.47237": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Traurige Fr\u00fchlingsherrschaft", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr V\u00f6gel, wenn ihr warten wollt mit Singen,", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "wenn", "ihr", "war\u00b7ten", "wollt", "mit", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis meine Lieder hell wie sonst erklangen!", "tokens": ["Bis", "mei\u00b7ne", "Lie\u00b7der", "hell", "wie", "sonst", "er\u00b7klan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Blumen, wenn ihr eh'r nicht wollt entspringen,", "tokens": ["Ihr", "Blu\u00b7men", ",", "wenn", "ihr", "eh'r", "nicht", "wollt", "ent\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis Freude bl\u00fchn ihr seht auf meinen Wangen!", "tokens": ["Bis", "Freu\u00b7de", "bl\u00fchn", "ihr", "seht", "auf", "mei\u00b7nen", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O la\u00dft von mir euch keine St\u00f6rung bringen,", "tokens": ["O", "la\u00dft", "von", "mir", "euch", "kei\u00b7ne", "St\u00f6\u00b7rung", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Euch aufzuhalten ist nicht mein Verlangen;", "tokens": ["Euch", "auf\u00b7zu\u00b7hal\u00b7ten", "ist", "nicht", "mein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Singt nur und bl\u00fcht an aller Str\u00f6me Borden", "tokens": ["Singt", "nur", "und", "bl\u00fcht", "an", "al\u00b7ler", "Str\u00f6\u00b7me", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wartet nicht, bis Fr\u00fchling mir geworden!", "tokens": ["Und", "war\u00b7tet", "nicht", ",", "bis", "Fr\u00fch\u00b7ling", "mir", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "NN", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da wollten dennoch s\u00e4umen", "tokens": ["Da", "woll\u00b7ten", "den\u00b7noch", "s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die Bl\u00fcten an den B\u00e4umen,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "an", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die V\u00f6glein tief in Tr\u00e4umen;", "tokens": ["Die", "V\u00f6\u00b7glein", "tief", "in", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Kann man dem Dichter so viel Recht einr\u00e4umen?", "tokens": ["Kann", "man", "dem", "Dich\u00b7ter", "so", "viel", "Recht", "ein\u00b7r\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ihr V\u00f6gel, da ihr doch nicht wollet singen,", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "da", "ihr", "doch", "nicht", "wol\u00b7let", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis mit Gesang ich euch vorangegangen;", "tokens": ["Bis", "mit", "Ge\u00b7sang", "ich", "euch", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Blumen, da ihr doch nicht wollet springen,", "tokens": ["Ihr", "Blu\u00b7men", ",", "da", "ihr", "doch", "nicht", "wol\u00b7let", "sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis auch die Keim' in meiner Brust entsprangen!", "tokens": ["Bis", "auch", "die", "Keim'", "in", "mei\u00b7ner", "Brust", "ent\u00b7spran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Zaudern soll euch keine St\u00f6rung bringen,", "tokens": ["Mein", "Zau\u00b7dern", "soll", "euch", "kei\u00b7ne", "St\u00f6\u00b7rung", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und euer Losungswort sollt ihr empfangen:", "tokens": ["Und", "eu\u00b7er", "Lo\u00b7sungs\u00b7wort", "sollt", "ihr", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Auf! singt und bl\u00fcht an aller Str\u00f6me Borden!", "tokens": ["Auf", "!", "singt", "und", "bl\u00fcht", "an", "al\u00b7ler", "Str\u00f6\u00b7me", "Bor\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Habt Fr\u00fchling! Fr\u00fchling ist mir selbst geworden.", "tokens": ["Habt", "Fr\u00fch\u00b7ling", "!", "Fr\u00fch\u00b7ling", "ist", "mir", "selbst", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "NN", "VAFIN", "PPER", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da durften nicht mehr s\u00e4umen", "tokens": ["Da", "durf\u00b7ten", "nicht", "mehr", "s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die Bl\u00fcten an den B\u00e4umen,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "an", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die V\u00f6glein auch nicht tr\u00e4umen,", "tokens": ["Die", "V\u00f6\u00b7glein", "auch", "nicht", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und Fr\u00fchling bl\u00fcht' und t\u00f6nt' in allen R\u00e4umen.", "tokens": ["Und", "Fr\u00fch\u00b7ling", "bl\u00fcht'", "und", "t\u00f6nt'", "in", "al\u00b7len", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr V\u00f6gel, nun ihr einmal seid am Singen,", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "nun", "ihr", "ein\u00b7mal", "seid", "am", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PPER", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil euer S\u00e4nger euch vorangegangen!", "tokens": ["Weil", "eu\u00b7er", "S\u00e4n\u00b7ger", "euch", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Blumen, die ihr nun m\u00fc\u00dft vorw\u00e4rts dringen,", "tokens": ["Ihr", "Blu\u00b7men", ",", "die", "ihr", "nun", "m\u00fc\u00dft", "vor\u00b7w\u00e4rts", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Stuf' auf Stufe, bis zum h\u00f6chsten Prangen!", "tokens": ["Von", "Stuf'", "auf", "Stu\u00b7fe", ",", "bis", "zum", "h\u00f6chs\u00b7ten", "Pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich kann euch f\u00fcrder keine St\u00f6rung bringen,", "tokens": ["Ich", "kann", "euch", "f\u00fcr\u00b7der", "kei\u00b7ne", "St\u00f6\u00b7rung", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fortfahren m\u00fc\u00dft ihr, wie ihr angefangen;", "tokens": ["Fort\u00b7fah\u00b7ren", "m\u00fc\u00dft", "ihr", ",", "wie", "ihr", "an\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Singt denn und bl\u00fcht an aller Str\u00f6me Borden", "tokens": ["Singt", "denn", "und", "bl\u00fcht", "an", "al\u00b7ler", "Str\u00f6\u00b7me", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wi\u00dft, da\u00df Fr\u00fchling doch mir nicht geworden.", "tokens": ["Und", "wi\u00dft", ",", "da\u00df", "Fr\u00fch\u00b7ling", "doch", "mir", "nicht", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NE", "KON", "PPER", "PTKNEG", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da konnten nicht mehr s\u00e4umen", "tokens": ["Da", "konn\u00b7ten", "nicht", "mehr", "s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die Bl\u00fcten an den B\u00e4umen,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "an", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die V\u00f6glein auch nicht tr\u00e4umen;", "tokens": ["Die", "V\u00f6\u00b7glein", "auch", "nicht", "tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Lust flo\u00df der Welt, mir sollte Trauer sch\u00e4umen.", "tokens": ["Lust", "flo\u00df", "der", "Welt", ",", "mir", "soll\u00b7te", "Trau\u00b7er", "sch\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}