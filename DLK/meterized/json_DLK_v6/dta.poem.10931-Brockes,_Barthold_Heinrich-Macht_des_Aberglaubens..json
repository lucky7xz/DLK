fo": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PTKNEG", "ADV", "$,", "KOKOM", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.51": {"text": "Ein Unglaub, und der andern Unglaub ein recht-und wahrer", "tokens": ["Ein", "Un\u00b7glaub", ",", "und", "der", "an\u00b7dern", "Un\u00b7glaub", "ein", "recht\u00b7\u00b7und", "wah\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.52": {"text": "Man kann, aus dieser Handlung, noch ein \u00fcberzeuglich Bey-", "tokens": ["Man", "kann", ",", "aus", "die\u00b7ser", "Hand\u00b7lung", ",", "noch", "ein", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "Bey"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "$,", "APPR", "PDAT", "NN", "$,", "ADV", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.53": {"text": "Zu welchen irrigen Jdeen der Menschen Seelen sich bequemen,", "tokens": ["Zu", "wel\u00b7chen", "ir\u00b7ri\u00b7gen", "Jdeen", "der", "Men\u00b7schen", "See\u00b7len", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "ART", "NN", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.54": {"text": "Wenn ihnen etwas in der Jugend, und ehe sie zum Denken", "tokens": ["Wenn", "ih\u00b7nen", "et\u00b7was", "in", "der", "Ju\u00b7gend", ",", "und", "e\u00b7he", "sie", "zum", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,", "KON", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.55": {"text": "Als Wahrheit vorgestellet wird. Man nimmt so Wahr-als", "tokens": ["Als", "Wahr\u00b7heit", "vor\u00b7ge\u00b7stel\u00b7let", "wird", ".", "Man", "nimmt", "so", "Wahr\u00b7als"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$.", "PIS", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und beydes klebt so fest an uns, da\u00df man sich nicht befreyen kann,", "tokens": ["Und", "bey\u00b7des", "klebt", "so", "fest", "an", "uns", ",", "da\u00df", "man", "sich", "nicht", "be\u00b7fre\u00b7yen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "$,", "KOUS", "PIS", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.57": {"text": "So lange man hier lebt, es sey die Meynung n\u00e4rrisch oder", "tokens": ["So", "lan\u00b7ge", "man", "hier", "lebt", ",", "es", "sey", "die", "Mey\u00b7nung", "n\u00e4r\u00b7risch", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADJD", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}