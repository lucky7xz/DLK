{"dta.poem.10931": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Macht  \n des Aberglaubens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich las in einer Reis-Beschreibung erstaunet j\u00fcngst, da\u00df, auf\nder Erden,", "tokens": ["Ich", "las", "in", "ei\u00b7ner", "Reis\u00b7Be\u00b7schrei\u00b7bung", "er\u00b7stau\u00b7net", "j\u00fcngst", ",", "da\u00df", ",", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Gewisse V\u00f6lker sollen seyn,", "tokens": ["Ge\u00b7wis\u00b7se", "V\u00f6l\u00b7ker", "sol\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die bey dem fr\u00fchen Sonnenschein,", "tokens": ["Die", "bey", "dem", "fr\u00fc\u00b7hen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit laut-und eifrigem Gebeth, und mit and\u00e4chtigen Gebehrden,", "tokens": ["Mit", "laut\u00b7\u00b7und", "eif\u00b7ri\u00b7gem", "Ge\u00b7beth", ",", "und", "mit", "an\u00b7d\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.5": {"text": "Die H\u00e4nde gegen sie erheben,", "tokens": ["Die", "H\u00e4n\u00b7de", "ge\u00b7gen", "sie", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich, in einer solchen Stellung, in einen tiefen Strom begeben,", "tokens": ["Und", "sich", ",", "in", "ei\u00b7ner", "sol\u00b7chen", "Stel\u00b7lung", ",", "in", "ei\u00b7nen", "tie\u00b7fen", "Strom", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "$,", "APPR", "ART", "PIAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Den gr\u00e4uliche, gefr\u00e4\u00dfige, und blutbegierge Crocodillen,", "tokens": ["Den", "gr\u00e4u\u00b7li\u00b7che", ",", "ge\u00b7fr\u00e4\u00b7\u00dfi\u00b7ge", ",", "und", "blut\u00b7be\u00b7gier\u00b7ge", "Cro\u00b7co\u00b7dil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "In gro\u00dfer Meng, erf\u00fcllen,", "tokens": ["In", "gro\u00b7\u00dfer", "Meng", ",", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "In Hoffnung, durch derselben Rachen,", "tokens": ["In", "Hoff\u00b7nung", ",", "durch", "der\u00b7sel\u00b7ben", "Ra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich einen Weg ins Paradies zu machen.", "tokens": ["Sich", "ei\u00b7nen", "Weg", "ins", "Pa\u00b7ra\u00b7dies", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ob nun der Autor dieses Buchs die That, mit einem kal-", "tokens": ["Ob", "nun", "der", "Au\u00b7tor", "die\u00b7ses", "Buchs", "die", "That", ",", "mit", "ei\u00b7nem", "ka\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PDAT", "NN", "ART", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+---", "measure": "unknown.measure.hexa"}, "line.12": {"text": "F\u00fcr Grillen schilt: Fand ich mich doch, durch die\u00df Betragen,", "tokens": ["F\u00fcr", "Gril\u00b7len", "schilt", ":", "Fand", "ich", "mich", "doch", ",", "durch", "die\u00df", "Be\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$.", "VVFIN", "PPER", "PRF", "ADV", "$,", "APPR", "PDS", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und zwar dadurch um desto mehr, als dieser Menschen heilge", "tokens": ["Und", "zwar", "da\u00b7durch", "um", "des\u00b7to", "mehr", ",", "als", "die\u00b7ser", "Men\u00b7schen", "heil\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "APPR", "ADV", "ADV", "$,", "KOUS", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "In einem sch\u00f6nen Kupferst\u00fcck, womit dasselbe Buch geziert,", "tokens": ["In", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Kup\u00b7fer\u00b7st\u00fcck", ",", "wo\u00b7mit", "das\u00b7sel\u00b7be", "Buch", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.15": {"text": "Sehr deutlich vorgestellet war. Ich sah das sch\u00f6ne Sonnenlicht", "tokens": ["Sehr", "deut\u00b7lich", "vor\u00b7ge\u00b7stel\u00b7let", "war", ".", "Ich", "sah", "das", "sch\u00f6\u00b7ne", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.16": {"text": "Am Horizont, mit vielen Stralen,", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", ",", "mit", "vie\u00b7len", "Stra\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den Himmel, und zugleich den Flu\u00df im Wiederschein, recht", "tokens": ["Den", "Him\u00b7mel", ",", "und", "zu\u00b7gleich", "den", "Flu\u00df", "im", "Wie\u00b7der\u00b7schein", ",", "recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ART", "NN", "APPRART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich sahe ganze Heerden Menschen and\u00e4chtige Gebehrden zeigen,", "tokens": ["Ich", "sa\u00b7he", "gan\u00b7ze", "Heer\u00b7den", "Men\u00b7schen", "an\u00b7d\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7behr\u00b7den", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.19": {"text": "Und, mit erhabnen Aug-und H\u00e4nden, in das fatale Wasser steigen,", "tokens": ["Und", ",", "mit", "er\u00b7hab\u00b7nen", "Aug\u00b7\u00b7und", "H\u00e4n\u00b7den", ",", "in", "das", "fa\u00b7ta\u00b7le", "Was\u00b7ser", "stei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "Woraus bald hier, bald da, bald dort erschrecklich h\u00e4\u00dfliche", "tokens": ["Wo\u00b7raus", "bald", "hier", ",", "bald", "da", ",", "bald", "dort", "er\u00b7schreck\u00b7lich", "h\u00e4\u00df\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Von Crocodill-und Wasser-Drachen, mit aufgesperrten Ra-", "tokens": ["Von", "Cro\u00b7co\u00b7dill\u00b7\u00b7und", "Was\u00b7ser\u00b7Dra\u00b7chen", ",", "mit", "auf\u00b7ge\u00b7sperr\u00b7ten", "Ra"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Die ungl\u00fcckseligen Vergn\u00fcgte, die sich mit Flei\u00df zu ihnen drungen,", "tokens": ["Die", "un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7gen", "Ver\u00b7gn\u00fcg\u00b7te", ",", "die", "sich", "mit", "Flei\u00df", "zu", "ih\u00b7nen", "drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "APPR", "PPER", "PAV", "$,"], "meter": "-+--+--+--+-+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.23": {"text": "Bald hie bald dort ergrimmt ergriffen, zerfleischt-zerquetschten", "tokens": ["Bald", "hie", "bald", "dort", "er\u00b7grimmt", "er\u00b7grif\u00b7fen", ",", "zer\u00b7fleischt\u00b7zer\u00b7quetschten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$,", "ADJA"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Durch diesen Anblick fast erstarrt, empfand ich schreckende Jdeen", "tokens": ["Durch", "die\u00b7sen", "An\u00b7blick", "fast", "er\u00b7starrt", ",", "emp\u00b7fand", "ich", "schre\u00b7cken\u00b7de", "Jdeen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.25": {"text": "In meiner aufgebrachten Seele, mit Mitleid untermischt, ent-", "tokens": ["In", "mei\u00b7ner", "auf\u00b7ge\u00b7brach\u00b7ten", "See\u00b7le", ",", "mit", "Mit\u00b7leid", "un\u00b7ter\u00b7mischt", ",", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "So da\u00df ein tief gehohlter Seufzer aus meiner Brust voll", "tokens": ["So", "da\u00df", "ein", "tief", "ge\u00b7hohl\u00b7ter", "Seuf\u00b7zer", "aus", "mei\u00b7ner", "Brust", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Und ich, mit einem heilgen Gram erf\u00fcllet, zu mir selber sprach:", "tokens": ["Und", "ich", ",", "mit", "ei\u00b7nem", "heil\u00b7gen", "Gram", "er\u00b7f\u00fcl\u00b7let", ",", "zu", "mir", "sel\u00b7ber", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.28": {"text": "O Gott! du Sch\u00f6pfer aller Dinge, unendlichs All! voll-", "tokens": ["O", "Gott", "!", "du", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ",", "un\u00b7end\u00b7lichs", "All", "!", "voll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$.", "PPER", "NN", "PIAT", "NN", "$,", "ADJA", "NN", "$.", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Selbst\u00e4ndig ewge Lieb! o Vater von allem, was da Kinder hei\u00dft!", "tokens": ["Selb\u00b7st\u00e4n\u00b7dig", "ew\u00b7ge", "Lieb", "!", "o", "Va\u00b7ter", "von", "al\u00b7lem", ",", "was", "da", "Kin\u00b7der", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "FM", "NN", "APPR", "PIS", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.30": {"text": "Der du die innern Triebe siehst, die Absicht wei\u00dft, den Zweck", "tokens": ["Der", "du", "die", "in\u00b7nern", "Trie\u00b7be", "siehst", ",", "die", "Ab\u00b7sicht", "wei\u00dft", ",", "den", "Zweck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.31": {"text": "Von diesen irrenden Gesch\u00f6pfen, die Menschen sind, so wohl", "tokens": ["Von", "die\u00b7sen", "ir\u00b7ren\u00b7den", "Ge\u00b7sch\u00f6p\u00b7fen", ",", "die", "Men\u00b7schen", "sind", ",", "so", "wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "$,", "ADV", "ADV"], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Da sie ganz \u00fcberzeuglich glauben, und sich versichern, da\u00df sie dich,", "tokens": ["Da", "sie", "ganz", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "glau\u00b7ben", ",", "und", "sich", "ver\u00b7si\u00b7chern", ",", "da\u00df", "sie", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,", "KON", "PRF", "VVINF", "$,", "KOUS", "PPER", "PPER", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.33": {"text": "Mit ihres eignen Leibes Opfer, vers\u00f6hnen und dich ehren k\u00f6nnen,", "tokens": ["Mit", "ih\u00b7res", "eig\u00b7nen", "Lei\u00b7bes", "Op\u00b7fer", ",", "ver\u00b7s\u00f6h\u00b7nen", "und", "dich", "eh\u00b7ren", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,", "ADJA", "KON", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.34": {"text": "Wenn sie, voll Hoffnung, unverzagt, dem schwarzen Tod in Ra-", "tokens": ["Wenn", "sie", ",", "voll", "Hoff\u00b7nung", ",", "un\u00b7ver\u00b7zagt", ",", "dem", "schwar\u00b7zen", "Tod", "in", "Ra"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.35": {"text": "Wie unerforscht ist dein Gericht, wie unbegreiflich sind die Wege,", "tokens": ["Wie", "un\u00b7er\u00b7forscht", "ist", "dein", "Ge\u00b7richt", ",", "wie", "un\u00b7be\u00b7greif\u00b7lich", "sind", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.36": {"text": "Die du mit diesen Menschen gehst.", "tokens": ["Die", "du", "mit", "die\u00b7sen", "Men\u00b7schen", "gehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ich zittere, wenn ich mit Ernst, auf welche Weise solche That,", "tokens": ["Ich", "zit\u00b7te\u00b7re", ",", "wenn", "ich", "mit", "Ernst", ",", "auf", "wel\u00b7che", "Wei\u00b7se", "sol\u00b7che", "That", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NE", "$,", "APPR", "PWAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.38": {"text": "Die so viel Finsterni\u00df und Blindheit, als Lieb und Eifer, in", "tokens": ["Die", "so", "viel", "Fins\u00b7ter\u00b7ni\u00df", "und", "Blind\u00b7heit", ",", "als", "Lieb", "und", "Ei\u00b7fer", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "wor