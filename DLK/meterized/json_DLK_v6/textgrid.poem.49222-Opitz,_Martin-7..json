{"textgrid.poem.49222": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann nicht die Sonne scheint und netzt uns stets der Regen,", "tokens": ["Wann", "nicht", "die", "Son\u00b7ne", "scheint", "und", "netzt", "uns", "stets", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sp\u00fcren wir an uns, da\u00df Muth und Sinn sich legen;", "tokens": ["So", "sp\u00fc\u00b7ren", "wir", "an", "uns", ",", "da\u00df", "Muth", "und", "Sinn", "sich", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "KOUS", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Lieb, sei nicht best\u00fcrzt, da\u00df ich solch Trauren f\u00fchr';", "tokens": ["Mein", "Lieb", ",", "sei", "nicht", "be\u00b7st\u00fcrzt", ",", "da\u00df", "ich", "solch", "Trau\u00b7ren", "f\u00fchr'", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich regne selbst, und du, o Sonne, bist nicht hier.", "tokens": ["Ich", "reg\u00b7ne", "selbst", ",", "und", "du", ",", "o", "Son\u00b7ne", ",", "bist", "nicht", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}