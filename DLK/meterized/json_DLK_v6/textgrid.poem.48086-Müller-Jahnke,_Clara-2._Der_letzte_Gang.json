{"textgrid.poem.48086": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "2. Der letzte Gang", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reich mir den Kelch! Und deine Hand!", "tokens": ["Reich", "mir", "den", "Kelch", "!", "Und", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und komm! Das Dunkel bricht herein.", "tokens": ["Und", "komm", "!", "Das", "Dun\u00b7kel", "bricht", "her\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00e4tter schauern k\u00fchl und krank;", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "schau\u00b7ern", "k\u00fchl", "und", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "noch wehn sie nicht in unsern Wein.", "tokens": ["noch", "wehn", "sie", "nicht", "in", "un\u00b7sern", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Auge sieht bei Nacht uns nach", "tokens": ["Kein", "Au\u00b7ge", "sieht", "bei", "Nacht", "uns", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und weint um uns bei Morgenschein \u2013", "tokens": ["und", "weint", "um", "uns", "bei", "Mor\u00b7gen\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reich mir den Kelch! Und deine Hand!", "tokens": ["Reich", "mir", "den", "Kelch", "!", "Und", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und komm! Wir werden selig sein.", "tokens": ["Und", "komm", "!", "Wir", "wer\u00b7den", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}