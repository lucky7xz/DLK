{"dta.poem.19544": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "M\u00f6wenflug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6wen sah um einen Felsen kreisen", "tokens": ["M\u00f6\u00b7wen", "sah", "um", "ei\u00b7nen", "Fel\u00b7sen", "krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ich in unerm\u00fcdlich gleichen Gleisen,", "tokens": ["Ich", "in", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "glei\u00b7chen", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf gespannter Schwinge schweben bleibend,", "tokens": ["Auf", "ge\u00b7spann\u00b7ter", "Schwin\u00b7ge", "schwe\u00b7ben", "blei\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine schimmernd wei\u00dfe Bahn beschreibend,", "tokens": ["Ei\u00b7ne", "schim\u00b7mernd", "wei\u00b7\u00dfe", "Bahn", "be\u00b7schrei\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und zugleich in gr\u00fcnem Meeresspiegel", "tokens": ["Und", "zu\u00b7gleich", "in", "gr\u00fc\u00b7nem", "Mee\u00b7res\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sah ich um die selben Felsenspitzen", "tokens": ["Sah", "ich", "um", "die", "sel\u00b7ben", "Fel\u00b7sen\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Eine helle Jagd gestreckter Fl\u00fcgel", "tokens": ["Ei\u00b7ne", "hel\u00b7le", "Jagd", "ge\u00b7streck\u00b7ter", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unerm\u00fcdlich durch die Tiefe blitzen.", "tokens": ["Un\u00b7er\u00b7m\u00fcd\u00b7lich", "durch", "die", "Tie\u00b7fe", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Und der Spiegel hatte solche Klarheit,", "tokens": ["Und", "der", "Spie\u00b7gel", "hat\u00b7te", "sol\u00b7che", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Da\u00df sich anders nicht die Fl\u00fcgel hoben", "tokens": ["Da\u00df", "sich", "an\u00b7ders", "nicht", "die", "Fl\u00fc\u00b7gel", "ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "PTKNEG", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Tief im Meer, als hoch in L\u00fcften oben,", "tokens": ["Tief", "im", "Meer", ",", "als", "hoch", "in", "L\u00fcf\u00b7ten", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "KOUS", "ADJD", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Da\u00df sich v\u00f6llig glichen Trug und Wahrheit.", "tokens": ["Da\u00df", "sich", "v\u00f6l\u00b7lig", "gli\u00b7chen", "Trug", "und", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Allgemach beschlich es mich wie Grauen,", "tokens": ["All\u00b7ge\u00b7mach", "be\u00b7schlich", "es", "mich", "wie", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schein und Wesen so verwandt zu schauen,", "tokens": ["Schein", "und", "We\u00b7sen", "so", "ver\u00b7wandt", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich fragte mich, am Strand verharrend,", "tokens": ["Und", "ich", "frag\u00b7te", "mich", ",", "am", "Strand", "ver\u00b7har\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ins gespenstische Geflatter starrend:", "tokens": ["Ins", "ge\u00b7spens\u00b7ti\u00b7sche", "Ge\u00b7flat\u00b7ter", "star\u00b7rend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und du selber? Bist du \u00e4cht befl\u00fcgelt?", "tokens": ["Und", "du", "sel\u00b7ber", "?", "Bist", "du", "\u00e4cht", "be\u00b7fl\u00fc\u00b7gelt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$.", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Oder nur gemalt und abgespiegelt?", "tokens": ["O\u00b7der", "nur", "ge\u00b7malt", "und", "ab\u00b7ge\u00b7spie\u00b7gelt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Gaukelst du im Kreis mit Fabeldingen?", "tokens": ["Gau\u00b7kelst", "du", "im", "Kreis", "mit", "Fa\u00b7bel\u00b7din\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Oder hast du Blut in deinen Schwingen?", "tokens": ["O\u00b7der", "hast", "du", "Blut", "in", "dei\u00b7nen", "Schwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}