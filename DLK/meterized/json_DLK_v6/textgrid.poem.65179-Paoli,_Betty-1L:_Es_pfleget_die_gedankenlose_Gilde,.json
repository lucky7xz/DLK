{"textgrid.poem.65179": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es pfleget die gedankenlose Gilde,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es pfleget die gedankenlose Gilde,", "tokens": ["Es", "pfle\u00b7get", "die", "ge\u00b7dan\u00b7ken\u00b7lo\u00b7se", "Gil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Jubel stets bereit wie zum Verzagen,", "tokens": ["Zum", "Ju\u00b7bel", "stets", "be\u00b7reit", "wie", "zum", "Ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt kalter Grausamkeit dich anzuklagen,", "tokens": ["Jetzt", "kal\u00b7ter", "Grau\u00b7sam\u00b7keit", "dich", "an\u00b7zu\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wieder dann zu preisen deine Milde.", "tokens": ["Und", "wie\u00b7der", "dann", "zu", "prei\u00b7sen", "dei\u00b7ne", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKZU", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie messen dich nach ihrem eig'nen Bilde", "tokens": ["Sie", "mes\u00b7sen", "dich", "nach", "ih\u00b7rem", "eig'\u00b7nen", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00f6nnen sich des Wahnes nicht entschlagen,", "tokens": ["Und", "k\u00f6n\u00b7nen", "sich", "des", "Wah\u00b7nes", "nicht", "ent\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Lieb' und Ha\u00df, wie sie im Herzen tragen,", "tokens": ["Da\u00df", "Lieb'", "und", "Ha\u00df", ",", "wie", "sie", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald segne, bald verw\u00fcste ihr Gefilde.", "tokens": ["Bald", "seg\u00b7ne", ",", "bald", "ver\u00b7w\u00fcs\u00b7te", "ihr", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Thorheit, Strenge, Huld dir anzudichten!", "tokens": ["O", "Thor\u00b7heit", ",", "Stren\u00b7ge", ",", "Huld", "dir", "an\u00b7zu\u00b7dich\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kennst nur der Notwendigkeit Gesetz,", "tokens": ["Du", "kennst", "nur", "der", "Not\u00b7wen\u00b7dig\u00b7keit", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Und bleibst ihm treu beim Schaffen und Vernichten.", "tokens": ["Und", "bleibst", "ihm", "treu", "beim", "Schaf\u00b7fen", "und", "Ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob Heil, ob Fluch in deines Mantels Falten", "tokens": ["Ob", "Heil", ",", "ob", "Fluch", "in", "dei\u00b7nes", "Man\u00b7tels", "Fal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich berge, Ewige! mir bist du stets,", "tokens": ["Sich", "ber\u00b7ge", ",", "E\u00b7wi\u00b7ge", "!", "mir", "bist", "du", "stets", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ADJA", "$.", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was einst das Fatum war den frommen Alten.", "tokens": ["Was", "einst", "das", "Fa\u00b7tum", "war", "den", "from\u00b7men", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}