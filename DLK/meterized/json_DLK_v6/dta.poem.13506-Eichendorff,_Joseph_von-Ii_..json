{"dta.poem.13506": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Herz, in deinen sonnenhellen", "tokens": ["Herz", ",", "in", "dei\u00b7nen", "son\u00b7nen\u00b7hel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tagen halt' nicht karg zur\u00fcck!", "tokens": ["Ta\u00b7gen", "halt'", "nicht", "karg", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allw\u00e4rts fr\u00f6hliche Gesellen", "tokens": ["All\u00b7w\u00e4rts", "fr\u00f6h\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trifft der Frohe und sein Gl\u00fcck.", "tokens": ["Trifft", "der", "Fro\u00b7he", "und", "sein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sinkt der Stern: alleine wandern", "tokens": ["Sinkt", "der", "Stern", ":", "al\u00b7lei\u00b7ne", "wan\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Magst du bis an's End der Welt \u2014", "tokens": ["Magst", "du", "bis", "an's", "End", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bau' du nur auf keinen andern", "tokens": ["Bau'", "du", "nur", "auf", "kei\u00b7nen", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als auf Gott, der Treue h\u00e4lt.", "tokens": ["Als", "auf", "Gott", ",", "der", "Treu\u00b7e", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}