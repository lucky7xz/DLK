{"dta.poem.8835": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiV .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Da bieten Kelch und F\u00e4cher die Bl\u00fcth' und Dolde:", "tokens": ["Da", "bie\u00b7ten", "Kelch", "und", "F\u00e4\u00b7cher", "die", "Bl\u00fcth'", "und", "Dol\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Behalte diesen, f\u00e4chle die feuchte Stirne,", "tokens": ["Be\u00b7hal\u00b7te", "die\u00b7sen", ",", "f\u00e4ch\u00b7le", "die", "feuch\u00b7te", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00fcr Freunde f\u00fclle jenen, f\u00fcr Trunkenbolde!", "tokens": ["F\u00fcr", "Freun\u00b7de", "f\u00fcl\u00b7le", "je\u00b7nen", ",", "f\u00fcr", "Trun\u00b7ken\u00b7bol\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PDS", "$,", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Traubenhyacinthus bewegt die Glocken,", "tokens": ["Der", "Trau\u00b7ben\u00b7hy\u00b7a\u00b7cin\u00b7thus", "be\u00b7wegt", "die", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Da schm\u00fcckt sich wei\u00df die Lilje zum Fest, die holde;", "tokens": ["Da", "schm\u00fcckt", "sich", "wei\u00df", "die", "Lil\u00b7je", "zum", "Fest", ",", "die", "hol\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Licht verschenkt die Farben, wie Band und Orden,", "tokens": ["Das", "Licht", "ver\u00b7schenkt", "die", "Far\u00b7ben", ",", "wie", "Band", "und", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da\u00df Tulpe sich verbr\u00e4me, sich Lack vergolde:", "tokens": ["Da\u00df", "Tul\u00b7pe", "sich", "ver\u00b7br\u00e4\u00b7me", ",", "sich", "Lack", "ver\u00b7gol\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVFIN", "$,", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Damit Natur im Lenze sich selbst genie\u00dfe,", "tokens": ["Da\u00b7mit", "Na\u00b7tur", "im", "Len\u00b7ze", "sich", "selbst", "ge\u00b7nie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "APPRART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ern\u00e4hrt sie einen Dichter in ihrem Solde.", "tokens": ["Er\u00b7n\u00e4hrt", "sie", "ei\u00b7nen", "Dich\u00b7ter", "in", "ih\u00b7rem", "Sol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}}}}}