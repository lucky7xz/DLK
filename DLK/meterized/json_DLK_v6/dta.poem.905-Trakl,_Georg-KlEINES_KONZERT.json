{"dta.poem.905": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "KlEINES KONZERT", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Rot, das traumhaft dich ersch\u00fcttert \u2014", "tokens": ["Ein", "Rot", ",", "das", "traum\u00b7haft", "dich", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch deine H\u00e4nde scheint die Sonne.", "tokens": ["Durch", "dei\u00b7ne", "H\u00e4n\u00b7de", "scheint", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du f\u00fchlst dein Herz verr\u00fcckt vor Wonne", "tokens": ["Du", "f\u00fchlst", "dein", "Herz", "ver\u00b7r\u00fcckt", "vor", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich still zu einer Tat bereiten.", "tokens": ["Sich", "still", "zu", "ei\u00b7ner", "Tat", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Mittag str\u00f6men gelbe Felder.", "tokens": ["In", "Mit\u00b7tag", "str\u00f6\u00b7men", "gel\u00b7be", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum h\u00f6rst du noch der Grillen Singen,", "tokens": ["Kaum", "h\u00f6rst", "du", "noch", "der", "Gril\u00b7len", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der M\u00e4her hartes Sensenschwingen.", "tokens": ["Der", "M\u00e4\u00b7her", "har\u00b7tes", "Sen\u00b7sen\u00b7schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einf\u00e4ltig schweigen goldene W\u00e4lder.", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "schwei\u00b7gen", "gol\u00b7de\u00b7ne", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Im gr\u00fcnen T\u00fcmpel gl\u00fcht Verwesung.", "tokens": ["Im", "gr\u00fc\u00b7nen", "T\u00fcm\u00b7pel", "gl\u00fcht", "Ver\u00b7we\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fische stehen still. Gottes Odem", "tokens": ["Die", "Fi\u00b7sche", "ste\u00b7hen", "still", ".", "Got\u00b7tes", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weckt sacht ein Saitenspiel im Brodem.", "tokens": ["Weckt", "sacht", "ein", "Sai\u00b7ten\u00b7spiel", "im", "Bro\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auss\u00e4tzigen winkt die Flut Genesung.", "tokens": ["Aus\u00b7s\u00e4t\u00b7zi\u00b7gen", "winkt", "die", "Flut", "Ge\u00b7ne\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Geist D\u00e4dals schwebt in blauen Schatten,", "tokens": ["Geist", "D\u00e4\u00b7dals", "schwebt", "in", "blau\u00b7en", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Duft von Milch in Haselzweigen.", "tokens": ["Ein", "Duft", "von", "Milch", "in", "Ha\u00b7sel\u00b7zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man h\u00f6rt noch lang den Lehrer geigen,", "tokens": ["Man", "h\u00f6rt", "noch", "lang", "den", "Leh\u00b7rer", "gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im leeren Hof den Schrei der Ratten.", "tokens": ["Im", "lee\u00b7ren", "Hof", "den", "Schrei", "der", "Rat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Krug an scheu\u00dflichen Tapeten", "tokens": ["Im", "Krug", "an", "scheu\u00df\u00b7li\u00b7chen", "Ta\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fchn k\u00fchlere Violenfarben.", "tokens": ["Bl\u00fchn", "k\u00fch\u00b7le\u00b7re", "Vi\u00b7o\u00b7len\u00b7far\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Hader dunkle Stimmen starben,", "tokens": ["Im", "Ha\u00b7der", "dunk\u00b7le", "Stim\u00b7men", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Narzi\u00df im Endakkord von Fl\u00f6ten.", "tokens": ["Nar\u00b7zi\u00df", "im", "En\u00b7dak\u00b7kord", "von", "Fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}