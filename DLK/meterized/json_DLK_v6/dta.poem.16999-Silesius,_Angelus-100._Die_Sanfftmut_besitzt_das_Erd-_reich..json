{"dta.poem.16999": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "100.  Die Sanfftmut besitzt das Erd-  \n reich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du strebst so embsiglich nach einem Fleklein Erden:", "tokens": ["Du", "strebst", "so", "emb\u00b7sig\u00b7lich", "nach", "ei\u00b7nem", "Fle\u00b7klein", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Sanfftmut k\u00f6ntestu der gantzen Erbherr werden", "tokens": ["Durch", "Sanfft\u00b7mut", "k\u00f6n\u00b7tes\u00b7tu", "der", "gant\u00b7zen", "Er\u00b7bherr", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "ART", "ADJA", "NN", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}