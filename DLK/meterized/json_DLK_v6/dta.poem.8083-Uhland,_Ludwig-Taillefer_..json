{"dta.poem.8083": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Taillefer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Normannenherzog Wilhelm sprach einmal:", "tokens": ["Nor\u00b7man\u00b7nen\u00b7her\u00b7zog", "Wil\u00b7helm", "sprach", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u201ewer singet in meinem Hof und in meinem Saal?", "tokens": ["\u201e", "wer", "sin\u00b7get", "in", "mei\u00b7nem", "Hof", "und", "in", "mei\u00b7nem", "Saal", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wer singet vom Morgen bis in die sp\u00e4te Nacht,", "tokens": ["Wer", "sin\u00b7get", "vom", "Mor\u00b7gen", "bis", "in", "die", "sp\u00e4\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So lieblich, da\u00df mir das Herz im Leibe lacht?\u201c", "tokens": ["So", "lieb\u00b7lich", ",", "da\u00df", "mir", "das", "Herz", "im", "Lei\u00b7be", "lacht", "?", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201edas ist der Taillefer, der so gerne singt,", "tokens": ["\u201e", "das", "ist", "der", "Tail\u00b7le\u00b7fer", ",", "der", "so", "ger\u00b7ne", "singt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Hofe, wann er das Rad am Brunnen schwingt,", "tokens": ["Im", "Ho\u00b7fe", ",", "wann", "er", "das", "Rad", "am", "Brun\u00b7nen", "schwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Saale, wann er das Feuer sch\u00fcret und facht,", "tokens": ["Im", "Saa\u00b7le", ",", "wann", "er", "das", "Feu\u00b7er", "sch\u00fc\u00b7ret", "und", "facht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wann er Abends sich legt und wann er Morgens erwacht.\u201c", "tokens": ["Wann", "er", "A\u00b7bends", "sich", "legt", "und", "wann", "er", "Mor\u00b7gens", "er\u00b7wacht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "NN", "PRF", "VVFIN", "KON", "PWAV", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Herzog sprach: \u201eich hab\u2019 einen guten Knecht,", "tokens": ["Der", "Her\u00b7zog", "sprach", ":", "\u201e", "ich", "hab'", "ei\u00b7nen", "gu\u00b7ten", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Den Taillefer, der dienet mir fromm und recht,", "tokens": ["Den", "Tail\u00b7le\u00b7fer", ",", "der", "die\u00b7net", "mir", "fromm", "und", "recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er treibt mein Rad und sch\u00fcret mein Feuer gut,", "tokens": ["Er", "treibt", "mein", "Rad", "und", "sch\u00fc\u00b7ret", "mein", "Feu\u00b7er", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und singet so hell, das h\u00f6het mir den Muth.\u201c", "tokens": ["Und", "sin\u00b7get", "so", "hell", ",", "das", "h\u00f6\u00b7het", "mir", "den", "Muth", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Da sprach der Taillefer: \u201eund w\u00e4r\u2019 ich frei,", "tokens": ["Da", "sprach", "der", "Tail\u00b7le\u00b7fer", ":", "\u201e", "und", "w\u00e4r'", "ich", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "KON", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel besser wollt\u2019 ich dienen und singen dabei.", "tokens": ["Viel", "bes\u00b7ser", "wollt'", "ich", "die\u00b7nen", "und", "sin\u00b7gen", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie wollt\u2019 ich dienen dem Herzog hoch zu Pferd!", "tokens": ["Wie", "wollt'", "ich", "die\u00b7nen", "dem", "Her\u00b7zog", "hoch", "zu", "Pferd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie wollt\u2019 ich singen und klingen mit Schild und mit Schwerdt!\u201c", "tokens": ["Wie", "wollt'", "ich", "sin\u00b7gen", "und", "klin\u00b7gen", "mit", "Schild", "und", "mit", "Schwerdt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nicht lange, so ritt der Taillefer in\u2019s Gefild,", "tokens": ["Nicht", "lan\u00b7ge", ",", "so", "ritt", "der", "Tail\u00b7le\u00b7fer", "in's", "Ge\u00b7fild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf einem hohen Pferde, mit Schwerdt und mit Schild.", "tokens": ["Auf", "ei\u00b7nem", "ho\u00b7hen", "Pfer\u00b7de", ",", "mit", "Schwerdt", "und", "mit", "Schild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Herzogs Schwester schaute vom Thurm in\u2019s Feld,", "tokens": ["Des", "Her\u00b7zogs", "Schwes\u00b7ter", "schau\u00b7te", "vom", "Thurm", "in's", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie sprach: \u201edort reitet, bei Gott! ein stattlicher Held.\u201c", "tokens": ["Sie", "sprach", ":", "\u201e", "dort", "rei\u00b7tet", ",", "bei", "Gott", "!", "ein", "statt\u00b7li\u00b7cher", "Held", ".", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "$,", "APPR", "NN", "$.", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und als er ritt vor\u00fcber an Fr\u00e4uleins Thurm,", "tokens": ["Und", "als", "er", "ritt", "vor\u00b7\u00fc\u00b7ber", "an", "Fr\u00e4u\u00b7leins", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da sang er bald wie ein L\u00fcftlein, bald wie ein Sturm.", "tokens": ["Da", "sang", "er", "bald", "wie", "ein", "L\u00fcft\u00b7lein", ",", "bald", "wie", "ein", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie sprach: \u201eder singet, das ist eine herrliche Lust!", "tokens": ["Sie", "sprach", ":", "\u201e", "der", "sin\u00b7get", ",", "das", "ist", "ei\u00b7ne", "herr\u00b7li\u00b7che", "Lust", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "VVFIN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Es zittert der Thurm und es zittert mein Herz in der Brust.\u201c", "tokens": ["Es", "zit\u00b7tert", "der", "Thurm", "und", "es", "zit\u00b7tert", "mein", "Herz", "in", "der", "Brust", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.7": {"line.1": {"text": "Der Herzog Wilhelm fuhr wohl \u00fcber das Meer,", "tokens": ["Der", "Her\u00b7zog", "Wil\u00b7helm", "fuhr", "wohl", "\u00fc\u00b7ber", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Er fuhr nach Engelland mit gewaltigem Heer.", "tokens": ["Er", "fuhr", "nach", "En\u00b7gel\u00b7land", "mit", "ge\u00b7wal\u00b7ti\u00b7gem", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er sprang vom Schiffe, da fiel er auf die Hand:", "tokens": ["Er", "sprang", "vom", "Schif\u00b7fe", ",", "da", "fiel", "er", "auf", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201ehei! \u2014 rief er \u2014 ich fass\u2019 und ergreife dich, Engelland!\u201c", "tokens": ["\u201e", "hei", "!", "rief", "er", "ich", "fass'", "und", "er\u00b7grei\u00b7fe", "dich", ",", "En\u00b7gel\u00b7land", "!", "\u201c"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,", "NE", "$.", "$("], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Als nun das Normannenheer zum Sturme schritt,", "tokens": ["Als", "nun", "das", "Nor\u00b7man\u00b7nen\u00b7heer", "zum", "Stur\u00b7me", "schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der edle Taillefer vor den Herzog ritt:", "tokens": ["Der", "ed\u00b7le", "Tail\u00b7le\u00b7fer", "vor", "den", "Her\u00b7zog", "ritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201emanch J\u00e4hrlein hab\u2019 ich gesungen und Feuer gesch\u00fcrt,", "tokens": ["\u201e", "manch", "J\u00e4hr\u00b7lein", "hab'", "ich", "ge\u00b7sun\u00b7gen", "und", "Feu\u00b7er", "ge\u00b7sch\u00fcrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.4": {"text": "WanchManch J&#228;hrlein gesungen und Schwerdt und Lanze ger&#252;hrt.", "tokens": ["Wanch", "Manch", "J", "&#228;", "hrlein", "ge\u00b7sun\u00b7gen", "und", "Schwerdt", "und", "Lan\u00b7ze", "ger", "&#252;", "hrt", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "NE", "NE", "$(", "ADJD", "VVPP", "KON", "NN", "KON", "NN", "ADJD", "$(", "VVFIN", "$."], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Und hab\u2019 ich Euch gedient und gesungen zu Dank,", "tokens": ["Und", "hab'", "ich", "Euch", "ge\u00b7dient", "und", "ge\u00b7sun\u00b7gen", "zu", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "KON", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zuerst als ein Knecht und dann als ein Ritter frank:", "tokens": ["Zu\u00b7erst", "als", "ein", "Knecht", "und", "dann", "als", "ein", "Rit\u00b7ter", "frank", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "KON", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So la\u00dft mich das entgelten am heutigen Tag,", "tokens": ["So", "la\u00dft", "mich", "das", "ent\u00b7gel\u00b7ten", "am", "heu\u00b7ti\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verg\u00f6nnet mir auf die Feinde den ersten Schlag!\u201c", "tokens": ["Ver\u00b7g\u00f6n\u00b7net", "mir", "auf", "die", "Fein\u00b7de", "den", "ers\u00b7ten", "Schlag", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Taillefer ritt vor allem Normannenheer,", "tokens": ["Der", "Tail\u00b7le\u00b7fer", "ritt", "vor", "al\u00b7lem", "Nor\u00b7man\u00b7nen\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf einem hohen Pferde, mit Schwerdt und mit Speer,", "tokens": ["Auf", "ei\u00b7nem", "ho\u00b7hen", "Pfer\u00b7de", ",", "mit", "Schwerdt", "und", "mit", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er sang so herrlich, das klang \u00fcber Hastingsfeld,", "tokens": ["Er", "sang", "so", "herr\u00b7lich", ",", "das", "klang", "\u00fc\u00b7ber", "Has\u00b7tings\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Roland sang er und manchem frommen Held.", "tokens": ["Von", "Ro\u00b7land", "sang", "er", "und", "man\u00b7chem", "from