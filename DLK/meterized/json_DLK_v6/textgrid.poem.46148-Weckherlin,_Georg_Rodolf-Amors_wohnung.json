{"textgrid.poem.46148": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Amors wohnung", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor, der allsigreiche got,", "tokens": ["A\u00b7mor", ",", "der", "all\u00b7si\u00b7grei\u00b7che", "got", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von hochmut einmal \u00fcberwunden,", "tokens": ["von", "hoch\u00b7mut", "ein\u00b7mal", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "den g\u00f6ttern r\u00fchmet sich zu spot,", "tokens": ["den", "g\u00f6t\u00b7tern", "r\u00fch\u00b7met", "sich", "zu", "spot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df ihm allein sie all verbunden.", "tokens": ["da\u00df", "ihm", "al\u00b7lein", "sie", "all", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie endlich, und billich, zumal", "tokens": ["Sie", "end\u00b7lich", ",", "und", "bil\u00b7lich", ",", "zu\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "KON", "ADJD", "$,", "KOUS"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "ab dises kinds frechheit verdrossen,", "tokens": ["ab", "di\u00b7ses", "kinds", "frech\u00b7heit", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "ihn haben aus des himmels saal", "tokens": ["ihn", "ha\u00b7ben", "aus", "des", "him\u00b7mels", "saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "verjaget und ganz ausgeschlossen.", "tokens": ["ver\u00b7ja\u00b7get", "und", "ganz", "aus\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Darauf er, folgend stracks dem glanz,", "tokens": ["Da\u00b7rauf", "er", ",", "fol\u00b7gend", "stracks", "dem", "glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in meiner Myrten augen kommen,", "tokens": ["in", "mei\u00b7ner", "Myr\u00b7ten", "au\u00b7gen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da er aus so bequemer schanz", "tokens": ["da", "er", "aus", "so", "be\u00b7que\u00b7mer", "schanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJD", "ADJD"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "ihm, sich zu r\u00e4chen, f\u00fcrgenommen.", "tokens": ["ihm", ",", "sich", "zu", "r\u00e4\u00b7chen", ",", "f\u00fcr\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PRF", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch dises orts holdseligkeit", "tokens": ["Doch", "di\u00b7ses", "orts", "hold\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hat ihn alsbald so sehr besessen,", "tokens": ["hat", "ihn", "als\u00b7bald", "so", "sehr", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df er bald der rachgirigkeit", "tokens": ["da\u00df", "er", "bald", "der", "rach\u00b7gi\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "und aller g\u00f6tter gar vergessen.", "tokens": ["und", "al\u00b7ler", "g\u00f6t\u00b7ter", "gar", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}