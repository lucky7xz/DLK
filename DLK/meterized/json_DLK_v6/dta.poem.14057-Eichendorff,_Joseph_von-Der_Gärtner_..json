{"dta.poem.14057": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00e4rtner .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "In meinem Garten find' ich", "tokens": ["In", "mei\u00b7nem", "Gar\u00b7ten", "find'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Blumen, sch\u00f6n und fein,", "tokens": ["Viel", "Blu\u00b7men", ",", "sch\u00f6n", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Kr\u00e4nze wohl d'raus wind' ich", "tokens": ["Viel", "Kr\u00e4n\u00b7ze", "wohl", "d'raus", "wind'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "ADV", "VVFIN", "PPER"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und tausend Gedanken bind' ich", "tokens": ["Und", "tau\u00b7send", "Ge\u00b7dan\u00b7ken", "bind'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und Gr\u00fc\u00dfe mit darein.", "tokens": ["Und", "Gr\u00fc\u00b7\u00dfe", "mit", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}