{"textgrid.poem.46842": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn, wie mich, dich Kummer labt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn, wie mich, dich Kummer labt,", "tokens": ["Wenn", ",", "wie", "mich", ",", "dich", "Kum\u00b7mer", "labt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "$,", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm und la\u00df uns laben,", "tokens": ["Komm", "und", "la\u00df", "uns", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Klagen, da\u00df wir sie gehabt,", "tokens": ["Kla\u00b7gen", ",", "da\u00df", "wir", "sie", "ge\u00b7habt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VAPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht mehr sie haben.", "tokens": ["Und", "nicht", "mehr", "sie", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PPER", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "War es nur ein sch\u00f6ner Traum,", "tokens": ["War", "es", "nur", "ein", "sch\u00f6\u00b7ner", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder war's ein Wachen?", "tokens": ["O\u00b7der", "wa\u00b7r's", "ein", "Wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ueberall erf\u00fcllt den Raum", "tokens": ["Ue\u00b7be\u00b7rall", "er\u00b7f\u00fcllt", "den", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ihr Rosenlachen.", "tokens": ["Noch", "ihr", "Ro\u00b7sen\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie sie einst vor uns geschwebt,", "tokens": ["Wie", "sie", "einst", "vor", "uns", "ge\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebt sie vor uns nieder,", "tokens": ["Schwebt", "sie", "vor", "uns", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ja sie lebt, wie sie gelebt,", "tokens": ["Ja", "sie", "lebt", ",", "wie", "sie", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und stirbt nun nicht wieder.", "tokens": ["Und", "stirbt", "nun", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn ich seh' ihr Angesicht,", "tokens": ["Wenn", "ich", "seh'", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcllt mich dunkle Trauer,", "tokens": ["H\u00fcllt", "mich", "dunk\u00b7le", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch es bricht ein sanftes Licht,", "tokens": ["Doch", "es", "bricht", "ein", "sanf\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6stend durch den Schauer:", "tokens": ["Tr\u00f6s\u00b7tend", "durch", "den", "Schau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df einst der nun herbe Schmerz", "tokens": ["Da\u00df", "einst", "der", "nun", "her\u00b7be", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Milde Wonne werde,", "tokens": ["Mil\u00b7de", "Won\u00b7ne", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df in ihr ruht unser Herz,", "tokens": ["Da\u00df", "in", "ihr", "ruht", "un\u00b7ser", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Macht uns sch\u00f6n die Erde.", "tokens": ["Macht", "uns", "sch\u00f6n", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}