{"textgrid.poem.33106": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schweift der Geist zur\u00fcck in jene Tage", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweift der Geist zur\u00fcck in jene Tage", "tokens": ["Schweift", "der", "Geist", "zu\u00b7r\u00fcck", "in", "je\u00b7ne", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alter Zeiten, Sch\u00f6n'res er nicht findet,", "tokens": ["Al\u00b7ter", "Zei\u00b7ten", ",", "Sch\u00f6n'\u00b7res", "er", "nicht", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als das Schicksal, welches eine Sage", "tokens": ["Als", "das", "Schick\u00b7sal", ",", "wel\u00b7ches", "ei\u00b7ne", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Uns von Philemon und Baucis k\u00fcndet.", "tokens": ["Uns", "von", "Phi\u00b7le\u00b7mon", "und", "Bau\u00b7cis", "k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Treuerprobt im Gl\u00fccke und im Leide,", "tokens": ["Treu\u00b7er\u00b7probt", "im", "Gl\u00fc\u00b7cke", "und", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wuchs ihr Leben so in Eins zusammen,", "tokens": ["Wuchs", "ihr", "Le\u00b7ben", "so", "in", "Eins", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df \u2013 damit der Tod sie niemals scheide,", "tokens": ["Da\u00df", "\u2013", "da\u00b7mit", "der", "Tod", "sie", "nie\u00b7mals", "schei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PAV", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Lie\u00df ein Gott aus ihnen B\u00e4ume stammen", "tokens": ["Lie\u00df", "ein", "Gott", "aus", "ih\u00b7nen", "B\u00e4u\u00b7me", "stam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Deren Zweige unaufl\u00f6slich breiten", "tokens": ["De\u00b7ren", "Zwei\u00b7ge", "un\u00b7auf\u00b7l\u00f6s\u00b7lich", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "ADJD", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich zum Schattendache, und ein Tempel", "tokens": ["Sich", "zum", "Schat\u00b7ten\u00b7da\u00b7che", ",", "und", "ein", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Werden vielen Paaren, die voll Freuden", "tokens": ["Wer\u00b7den", "vie\u00b7len", "Paa\u00b7ren", ",", "die", "voll", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nehmen dran ein liebendes Exempel.", "tokens": ["Neh\u00b7men", "dran", "ein", "lie\u00b7ben\u00b7des", "Ex\u00b7em\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und mir d\u00e4ucht, es sei in ", "tokens": ["Und", "mir", "d\u00e4ucht", ",", "es", "sei", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Philemon und Baucis neu erstanden \u2013", "tokens": ["Phi\u00b7le\u00b7mon", "und", "Bau\u00b7cis", "neu", "er\u00b7stan\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Silberhell ergl\u00e4nzen ihre Haare,", "tokens": ["Sil\u00b7ber\u00b7hell", "er\u00b7gl\u00e4n\u00b7zen", "ih\u00b7re", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Golden ihrer Treue feste Banden.", "tokens": ["Gol\u00b7den", "ih\u00b7rer", "Treu\u00b7e", "fes\u00b7te", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Treuerprobt im Gl\u00fccke und im Leide,", "tokens": ["Treu\u00b7er\u00b7probt", "im", "Gl\u00fc\u00b7cke", "und", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wuchs ihr Leben ganz in Eins zusammen,", "tokens": ["Wuchs", "ihr", "Le\u00b7ben", "ganz", "in", "Eins", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch ein mild'rer Gott noch lie\u00df f\u00fcr Beide", "tokens": ["Doch", "ein", "mild'\u00b7rer", "Gott", "noch", "lie\u00df", "f\u00fcr", "Bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6h're Freude, s\u00fc\u00df'res Gl\u00fcck entstammen.", "tokens": ["H\u00f6h'\u00b7re", "Freu\u00b7de", ",", "s\u00fc\u00df'\u00b7res", "Gl\u00fcck", "ent\u00b7stam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Noch im Vollgenu\u00df der Lebensf\u00fclle,", "tokens": ["Noch", "im", "Voll\u00b7ge\u00b7nu\u00df", "der", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Frisch am Geiste, frisch des Herzens Triebe,", "tokens": ["Frisch", "am", "Geis\u00b7te", ",", "frisch", "des", "Her\u00b7zens", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Feiern sie in edler Menschenh\u00fclle", "tokens": ["Fei\u00b7ern", "sie", "in", "ed\u00b7ler", "Men\u00b7schen\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heut' das seltne Jubelfest der Liebe!", "tokens": ["Heut'", "das", "selt\u00b7ne", "Ju\u00b7bel\u00b7fest", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wohl seh' ich zwei B\u00e4ume sich verschlingen,", "tokens": ["Wohl", "seh'", "ich", "zwei", "B\u00e4u\u00b7me", "sich", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch nur als Symbol von jener Sage \u2013", "tokens": ["Doch", "nur", "als", "Sym\u00b7bol", "von", "je\u00b7ner", "Sa\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lass't es euch von treuen Freunden bringen,", "tokens": ["Lass't", "es", "euch", "von", "treu\u00b7en", "Freun\u00b7den", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die sich mit euch freu'n an diesem Tage!", "tokens": ["Die", "sich", "mit", "euch", "freu'n", "an", "die\u00b7sem", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}