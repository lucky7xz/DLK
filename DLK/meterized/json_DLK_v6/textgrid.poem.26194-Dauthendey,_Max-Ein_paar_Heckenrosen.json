{"textgrid.poem.26194": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein paar Heckenrosen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein paar Heckenrosen, die ich gestern f\u00fcr dich im Mondschein gepfl\u00fcckt,", "tokens": ["Ein", "paar", "He\u00b7cken\u00b7ro\u00b7sen", ",", "die", "ich", "ge\u00b7stern", "f\u00fcr", "dich", "im", "Mond\u00b7schein", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Stehen heute rosig im Glas, wie von deiner N\u00e4he entz\u00fcckt.", "tokens": ["Ste\u00b7hen", "heu\u00b7te", "ro\u00b7sig", "im", "Glas", ",", "wie", "von", "dei\u00b7ner", "N\u00e4\u00b7he", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPRART", "NN", "$,", "PWAV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gestern stahl sich kaum ihre Bl\u00e4sse in die bl\u00e4uliche Nacht,", "tokens": ["Ge\u00b7stern", "stahl", "sich", "kaum", "ih\u00b7re", "Bl\u00e4s\u00b7se", "in", "die", "bl\u00e4u\u00b7li\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie waren in D\u00e4mmerdunkel noch nicht f\u00fcr dich erwacht.", "tokens": ["Sie", "wa\u00b7ren", "in", "D\u00e4m\u00b7mer\u00b7dun\u00b7kel", "noch", "nicht", "f\u00fcr", "dich", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sie waren noch Schwestern der Bl\u00e4tter und B\u00fcsche gestern,", "tokens": ["Sie", "wa\u00b7ren", "noch", "Schwes\u00b7tern", "der", "Bl\u00e4t\u00b7ter", "und", "B\u00fc\u00b7sche", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Heute sind sie deines L\u00e4chelns und deiner Wangen Schwestern.", "tokens": ["Heu\u00b7te", "sind", "sie", "dei\u00b7nes", "L\u00e4\u00b7chelns", "und", "dei\u00b7ner", "Wan\u00b7gen", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Die Rosen, wenn in die N\u00e4he verliebter Augen kommen,", "tokens": ["Die", "Ro\u00b7sen", ",", "wenn", "in", "die", "N\u00e4\u00b7he", "ver\u00b7lieb\u00b7ter", "Au\u00b7gen", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-++-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Sind nicht wie V\u00f6gel, denen die Freiheit genommen, und die man gefangen,", "tokens": ["Sind", "nicht", "wie", "V\u00f6\u00b7gel", ",", "de\u00b7nen", "die", "Frei\u00b7heit", "ge\u00b7nom\u00b7men", ",", "und", "die", "man", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "KOKOM", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "KON", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+--+--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Die Rosen werden erst Rosen in der N\u00e4he err\u00f6tender Wangen.", "tokens": ["Die", "Ro\u00b7sen", "wer\u00b7den", "erst", "Ro\u00b7sen", "in", "der", "N\u00e4\u00b7he", "er\u00b7r\u00f6\u00b7ten\u00b7der", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+--+-", "measure": "iambic.septa.relaxed"}}}}}