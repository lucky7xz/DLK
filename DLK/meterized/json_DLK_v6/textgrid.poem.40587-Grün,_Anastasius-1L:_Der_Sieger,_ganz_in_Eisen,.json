{"textgrid.poem.40587": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sieger, ganz in Eisen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sieger, ganz in Eisen,", "tokens": ["Der", "Sie\u00b7ger", ",", "ganz", "in", "Ei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tritt ins ersiegte Land,", "tokens": ["Tritt", "ins", "er\u00b7sieg\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er will noch lang ihm weisen", "tokens": ["Er", "will", "noch", "lang", "ihm", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die harte, eh'rne Hand.", "tokens": ["Die", "har\u00b7te", ",", "eh'r\u00b7ne", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geharnischt ist der Wilde", "tokens": ["Ge\u00b7har\u00b7nischt", "ist", "der", "Wil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis an die Z\u00e4hne schier,", "tokens": ["Bis", "an", "die", "Z\u00e4h\u00b7ne", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schienen, Helm und Schilde,", "tokens": ["Mit", "Schie\u00b7nen", ",", "Helm", "und", "Schil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Panzer und Visir.", "tokens": ["Mit", "Pan\u00b7zer", "und", "Vi\u00b7sir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den breiten scharfen Degen", "tokens": ["Den", "brei\u00b7ten", "schar\u00b7fen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fest um den Leib geschnallt,", "tokens": ["Fest", "um", "den", "Leib", "ge\u00b7schnallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So wallt in Bl\u00fcthengeh\u00e4gen", "tokens": ["So", "wallt", "in", "Bl\u00fc\u00b7then\u00b7ge\u00b7h\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die starre Schreckgestalt.", "tokens": ["Die", "star\u00b7re", "Schreck\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es rasseln die Erzgewande,", "tokens": ["Es", "ras\u00b7seln", "die", "Erz\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo Quell und Lerche singt,", "tokens": ["Wo", "Quell", "und", "Ler\u00b7che", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Eisen bringt er dem Lande,", "tokens": ["Und", "Ei\u00b7sen", "bringt", "er", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das goldnen Segen ihm bringt;", "tokens": ["Das", "gold\u00b7nen", "Se\u00b7gen", "ihm", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Das ihm nun tritt entgegen", "tokens": ["Das", "ihm", "nun", "tritt", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im gr\u00fcnen Fr\u00fchlingskleid,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Fr\u00fch\u00b7lings\u00b7kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das rings auf seinen Wegen", "tokens": ["Das", "rings", "auf", "sei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm Blumen aufgestreut.", "tokens": ["Ihm", "Blu\u00b7men", "auf\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er hebt im Stahlgewande", "tokens": ["Er", "hebt", "im", "Stahl\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Kelch mit Wein gef\u00fcllt,", "tokens": ["Den", "Kelch", "mit", "Wein", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ringsherum im Lande", "tokens": ["Der", "rings\u00b7he\u00b7rum", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von sonn'gen H\u00fcgeln quillt;", "tokens": ["Von", "sonn'\u00b7gen", "H\u00fc\u00b7geln", "quillt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er tr\u00e4nke gern vom reinen,", "tokens": ["Er", "tr\u00e4n\u00b7ke", "gern", "vom", "rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da hemmt ihn sein Visir,", "tokens": ["Da", "hemmt", "ihn", "sein", "Vi\u00b7sir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Mundkorb will's ihm scheinen;", "tokens": ["Ein", "Mund\u00b7korb", "will's", "ihm", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da l\u00f6st er die l\u00e4st'ge Zier.", "tokens": ["Da", "l\u00f6st", "er", "die", "l\u00e4st'\u00b7ge", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Er steht im Kleid von Eisen,", "tokens": ["Er", "steht", "im", "Kleid", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Tanzmusik erklingt", "tokens": ["Wo", "Tanz\u00b7mu\u00b7sik", "er\u00b7klingt"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in des Landes Weisen", "tokens": ["Und", "in", "des", "Lan\u00b7des", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jedwede Sohle beschwingt;", "tokens": ["Jed\u00b7we\u00b7de", "Soh\u00b7le", "be\u00b7schwingt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Auch ihn will's drehn und regen,", "tokens": ["Auch", "ihn", "will's", "drehn", "und", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch zwischen die Beine schl\u00e4gt", "tokens": ["Doch", "zwi\u00b7schen", "die", "Bei\u00b7ne", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihm rasselnd der lange Degen,", "tokens": ["Ihm", "ras\u00b7selnd", "der", "lan\u00b7ge", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis er zur Seit' ihn legt.", "tokens": ["Bis", "er", "zur", "Seit'", "ihn", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er dr\u00fcckt im Stahlgewande", "tokens": ["Er", "dr\u00fcckt", "im", "Stahl\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ans Herz die sch\u00f6nste Maid,", "tokens": ["Ans", "Herz", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie manche hier im Lande", "tokens": ["Wie", "man\u00b7che", "hier", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Rosen und Reben gedeiht;", "tokens": ["Der", "Ro\u00b7sen", "und", "Re\u00b7ben", "ge\u00b7deiht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Er w\u00fcnscht, auch sie empf\u00e4nde", "tokens": ["Er", "w\u00fcnscht", ",", "auch", "sie", "em\u00b7pf\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Herzens Schlag und Brand;", "tokens": ["Des", "Her\u00b7zens", "Schlag", "und", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schnallt er vom Leibe behende", "tokens": ["Da", "schnallt", "er", "vom", "Lei\u00b7be", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJA"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Panzers Scheidewand.", "tokens": ["Des", "Pan\u00b7zers", "Schei\u00b7de\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und zwischen Viol' und Rose", "tokens": ["Und", "zwi\u00b7schen", "Vi\u00b7ol'", "und", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Legt Nachts er sich zur Rast,", "tokens": ["Legt", "Nachts", "er", "sich", "zur", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weich sind des Lagers Moose,", "tokens": ["Weich", "sind", "des", "La\u00b7gers", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Hart seiner R\u00fcstung Last;", "tokens": ["Hart", "sei\u00b7ner", "R\u00fcs\u00b7tung", "Last", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Was ihm an Arm und H\u00fcften", "tokens": ["Was", "ihm", "an", "Arm", "und", "H\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch blieb von Erz zur\u00fcck,", "tokens": ["Noch", "blieb", "von", "Erz", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er will's vom Leib sich l\u00fcften,", "tokens": ["Er", "will's", "vom", "Leib", "sich", "l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er l\u00f6st es St\u00fcck f\u00fcr St\u00fcck.", "tokens": ["Er", "l\u00f6st", "es", "St\u00fcck", "f\u00fcr", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "O Wunder um die Wette,", "tokens": ["O", "Wun\u00b7der", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die drauf der Morgen erhellt:", "tokens": ["Die", "drauf", "der", "Mor\u00b7gen", "er\u00b7hellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den Sieger fesselt die Kette,", "tokens": ["Den", "Sie\u00b7ger", "fes\u00b7selt", "die", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entwaffnet ist der Held!", "tokens": ["Ent\u00b7waff\u00b7net", "ist", "der", "Held", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Da liegt er auf Blumen gebettet,", "tokens": ["Da", "liegt", "er", "auf", "Blu\u00b7men", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Womit das Land sich schm\u00fcckt,", "tokens": ["Wo\u00b7mit", "das", "Land", "sich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Rebguirlanden gekettet,", "tokens": ["Von", "Reb\u00b7gu\u00b7ir\u00b7lan\u00b7den", "ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von Rosenfesseln umstrickt!", "tokens": ["Von", "Ro\u00b7sen\u00b7fes\u00b7seln", "um\u00b7strickt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Und wie durchs Kerkergitter", "tokens": ["Und", "wie", "durchs", "Ker\u00b7ker\u00b7git\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch gr\u00fcnes Astwerk dicht,", "tokens": ["Durch", "gr\u00fc\u00b7nes", "Ast\u00b7werk", "dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Blickt der gefang'ne Ritter", "tokens": ["Blickt", "der", "ge\u00b7fang'\u00b7ne", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Zum Himmel, frei und licht!", "tokens": ["Zum", "Him\u00b7mel", ",", "frei", "und", "licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}