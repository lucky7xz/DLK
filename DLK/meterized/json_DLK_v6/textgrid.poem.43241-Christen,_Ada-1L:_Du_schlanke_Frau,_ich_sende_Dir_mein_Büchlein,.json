{"textgrid.poem.43241": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du schlanke Frau, ich sende Dir mein B\u00fcchlein,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schlanke Frau, ich sende Dir mein B\u00fcchlein,", "tokens": ["Du", "schlan\u00b7ke", "Frau", ",", "ich", "sen\u00b7de", "Dir", "mein", "B\u00fcch\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und mit den Fr\u00fchlingsl\u00fcften kommt vielleicht", "tokens": ["Und", "mit", "den", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7ten", "kommt", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es angeflogen in Dein stilles Dorf,", "tokens": ["Es", "an\u00b7ge\u00b7flo\u00b7gen", "in", "Dein", "stil\u00b7les", "Dorf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Liebesgru\u00df aus ferner, lauter Stadt.", "tokens": ["Ein", "Lie\u00b7bes\u00b7gru\u00df", "aus", "fer\u00b7ner", ",", "lau\u00b7ter", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn in der Mittagsstunde Du alsdann,", "tokens": ["Wenn", "in", "der", "Mit\u00b7tags\u00b7stun\u00b7de", "Du", "als\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die H\u00e4nde leicht gefaltet und gekreuzt die F\u00fc\u00dfchen,", "tokens": ["Die", "H\u00e4n\u00b7de", "leicht", "ge\u00b7fal\u00b7tet", "und", "ge\u00b7kreuzt", "die", "F\u00fc\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "KON", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachsinnend lehnest im Gro\u00dfvaterstuhl,", "tokens": ["Nach\u00b7sin\u00b7nend", "leh\u00b7nest", "im", "Gro\u00df\u00b7va\u00b7ter\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleich der Prinzessin aus dem Ammenm\u00e4rchen,", "tokens": ["Gleich", "der", "Prin\u00b7zes\u00b7sin", "aus", "dem", "Am\u00b7men\u00b7m\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "Bewacht von zwei schneewei\u00dfen gro\u00dfen Katzen,", "tokens": ["Be\u00b7wacht", "von", "zwei", "schnee\u00b7wei\u00b7\u00dfen", "gro\u00b7\u00dfen", "Kat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die emsig spinnend auf der Diele kauern,", "tokens": ["Die", "em\u00b7sig", "spin\u00b7nend", "auf", "der", "Die\u00b7le", "kau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn Fr\u00fchlingsonnenschein durch's Fenster f\u00e4llt,", "tokens": ["Wenn", "Fr\u00fch\u00b7lin\u00b7gson\u00b7nen\u00b7schein", "durch's", "Fens\u00b7ter", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Quer durch die Stube auf Dein blondes Haupt,", "tokens": ["Quer", "durch", "die", "Stu\u00b7be", "auf", "Dein", "blon\u00b7des", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn dann die alte Magd, die schweigsam sa\u00df,", "tokens": ["Wenn", "dann", "die", "al\u00b7te", "Magd", ",", "die", "schweig\u00b7sam", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Halb Deinen Athemz\u00fcgen, halb dem Winde lauschend,", "tokens": ["Halb", "Dei\u00b7nen", "A\u00b7them\u00b7z\u00fc\u00b7gen", ",", "halb", "dem", "Win\u00b7de", "lau\u00b7schend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dich pl\u00f6tzlich fragt in ihrer treuen Art:", "tokens": ["Dich", "pl\u00f6tz\u00b7lich", "fragt", "in", "ih\u00b7rer", "treu\u00b7en", "Art", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u00bbwas schrieb denn die Frau Ada heute ... Frau?!\u00ab", "tokens": ["\u00bb", "was", "schrieb", "denn", "die", "Frau", "A\u00b7da", "heu\u00b7te", "...", "Frau", "?!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "ART", "NN", "NE", "ADV", "$(", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Da wird Dir klar, was Du gedacht, gef\u00fchlt,", "tokens": ["Da", "wird", "Dir", "klar", ",", "was", "Du", "ge\u00b7dacht", ",", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Seit Dir mein B\u00fcchlein aus der Hand gefallen,", "tokens": ["Seit", "Dir", "mein", "B\u00fcch\u00b7lein", "aus", "der", "Hand", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und leise sagst Du dann: Sie kommt bald wieder!", "tokens": ["Und", "lei\u00b7se", "sagst", "Du", "dann", ":", "Sie", "kommt", "bald", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Denn als gesucht Du schweigend, und gebl\u00e4ttert,", "tokens": ["Denn", "als", "ge\u00b7sucht", "Du", "schwei\u00b7gend", ",", "und", "ge\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVPP", "PPER", "ADJD", "$,", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da f\u00fcllten Deine frommen blauen Augen,", "tokens": ["Da", "f\u00fcll\u00b7ten", "Dei\u00b7ne", "from\u00b7men", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die erst mit Kinderneugier niederblickten", "tokens": ["Die", "erst", "mit", "Kin\u00b7der\u00b7neu\u00b7gier", "nie\u00b7der\u00b7blick\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Auf jedes Blatt \u2013 mit Thr\u00e4nen sich allm\u00e4hlig.", "tokens": ["Auf", "je\u00b7des", "Blatt", "\u2013", "mit", "Thr\u00e4\u00b7nen", "sich", "all\u00b7m\u00e4h\u00b7lig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "APPR", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Warum? ... Ich habe niemals Dir erz\u00e4hlt,", "tokens": ["Wa\u00b7rum", "?", "...", "Ich", "ha\u00b7be", "nie\u00b7mals", "Dir", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "$(", "PPER", "VAFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie lichtlos mich das Leben immer d\u00fcnkt,", "tokens": ["Wie", "licht\u00b7los", "mich", "das", "Le\u00b7ben", "im\u00b7mer", "d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie seine R\u00e4thsel allzeit mich gequ\u00e4lt,", "tokens": ["Wie", "sei\u00b7ne", "R\u00e4th\u00b7sel", "all\u00b7zeit", "mich", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und wie ich litt, weil Andere schwerer litten.", "tokens": ["Und", "wie", "ich", "litt", ",", "weil", "An\u00b7de\u00b7re", "schwe\u00b7rer", "lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich habe Dir kein einzigmal gesagt,", "tokens": ["Ich", "ha\u00b7be", "Dir", "kein", "ein\u00b7zig\u00b7mal", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Welch' helles Wunder Du an mir vollbracht:", "tokens": ["Welch'", "hel\u00b7les", "Wun\u00b7der", "Du", "an", "mir", "voll\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Deines schlichten Wesens milder Glanz,", "tokens": ["Wie", "Dei\u00b7nes", "schlich\u00b7ten", "We\u00b7sens", "mil\u00b7der", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Deiner Stimme seltsam-weicher Klang,", "tokens": ["Und", "Dei\u00b7ner", "Stim\u00b7me", "selt\u00b7sam\u00b7wei\u00b7cher", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und Deiner Liebe weiblich-zarte Sorgfalt", "tokens": ["Und", "Dei\u00b7ner", "Lie\u00b7be", "weib\u00b7lich\u00b7zar\u00b7te", "Sorg\u00b7falt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mich selber mild und weich und z\u00e4rtlich machten.", "tokens": ["Mich", "sel\u00b7ber", "mild", "und", "weich", "und", "z\u00e4rt\u00b7lich", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie ich mich freuen lernte, weil Du Dich", "tokens": ["Wie", "ich", "mich", "freu\u00b7en", "lern\u00b7te", ",", "weil", "Du", "Dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Erfreuen konntest voll und wahr an Dingen,", "tokens": ["Er\u00b7freu\u00b7en", "konn\u00b7test", "voll", "und", "wahr", "an", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die unbeachtet ich von jeher lie\u00df.", "tokens": ["Die", "un\u00b7be\u00b7ach\u00b7tet", "ich", "von", "je\u00b7her", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie ich auflachen konnte harmlos-heiter,", "tokens": ["Wie", "ich", "auf\u00b7la\u00b7chen", "konn\u00b7te", "harm\u00b7los\u00b7hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Um \u00fcber dieses Lachen dann gar oft", "tokens": ["Um", "\u00fc\u00b7ber", "die\u00b7ses", "La\u00b7chen", "dann", "gar", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "PDAT", "NN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Verwundert lange selber noch zu l\u00e4cheln.", "tokens": ["Ver\u00b7wun\u00b7dert", "lan\u00b7ge", "sel\u00b7ber", "noch", "zu", "l\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie ich mit einmal singen lernte ...", "tokens": ["Wie", "ich", "mit", "ein\u00b7mal", "sin\u00b7gen", "lern\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und fast erschrack, als meine herbe Stimme", "tokens": ["Und", "fast", "er\u00b7schrack", ",", "als", "mei\u00b7ne", "her\u00b7be", "Stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Durch das Gemach scholl, wo Du horchend", "tokens": ["Durch", "das", "Ge\u00b7mach", "scholl", ",", "wo", "Du", "hor\u00b7chend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "PWAV", "PPER", "VVPP"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Inmitten standest ... und bald ernsthaft mitsangst,", "tokens": ["In\u00b7mit\u00b7ten", "stan\u00b7dest", "...", "und", "bald", "ernst\u00b7haft", "mit\u00b7sangst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "Anschmiegend langsam dich der lust'gen Weise,", "tokens": ["An\u00b7schmie\u00b7gend", "lang\u00b7sam", "dich", "der", "lust'\u00b7gen", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die frohbewegt sich sacht mein Herz ersann.", "tokens": ["Die", "froh\u00b7be\u00b7wegt", "sich", "sacht", "mein", "Herz", "er\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und war es nicht ein wunderliches Bild,", "tokens": ["Und", "war", "es", "nicht", "ein", "wun\u00b7der\u00b7li\u00b7ches", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Zwei Frauen in der Stube ganz allein", "tokens": ["Zwei", "Frau\u00b7en", "in", "der", "Stu\u00b7be", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic