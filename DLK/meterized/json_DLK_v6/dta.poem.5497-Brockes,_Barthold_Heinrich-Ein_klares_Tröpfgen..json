{"dta.poem.5497": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein klares Tr\u00f6pfgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sah ich, da\u00df an meinem Fenster ein kleines kla-", "tokens": ["J\u00fcngst", "sah", "ich", ",", "da\u00df", "an", "mei\u00b7nem", "Fens\u00b7ter", "ein", "klei\u00b7nes", "kla"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "res Tr\u00f6pfgen hieng,", "tokens": ["res", "Tr\u00f6pf\u00b7gen", "hieng", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das von dem hellen Sonnen-Strahle solch einen hellen", "tokens": ["Das", "von", "dem", "hel\u00b7len", "Son\u00b7nen\u00b7Strah\u00b7le", "solch", "ei\u00b7nen", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "PIAT", "ART", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Glantz empfing,", "tokens": ["Glantz", "emp\u00b7fing", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df es mich reitzt\u2019, es zu betrachten; daher ich ihm denn", "tokens": ["Da\u00df", "es", "mich", "reitzt'", ",", "es", "zu", "be\u00b7trach\u00b7ten", ";", "da\u00b7her", "ich", "ihm", "denn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$.", "PAV", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "n\u00e4her ging;", "tokens": ["n\u00e4\u00b7her", "ging", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ich fand, da\u00df es im Zimmer war, und da\u00df durch eines", "tokens": ["Ich", "fand", ",", "da\u00df", "es", "im", "Zim\u00b7mer", "war", ",", "und", "da\u00df", "durch", "ei\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VAFIN", "$,", "KON", "KOUS", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Fensters Ritzen", "tokens": ["Fens\u00b7ters", "Rit\u00b7zen"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Der Strahl so auf-als durch ihn fiel, daher ein kleines", "tokens": ["Der", "Strahl", "so", "auf\u00b7als", "durch", "ihn", "fiel", ",", "da\u00b7her", "ein", "klei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,", "PAV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "helles Blitzen,", "tokens": ["hel\u00b7les", "Blit\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "So man in freyer Luft nicht sieht, im duncklen Zimmer", "tokens": ["So", "man", "in", "frey\u00b7er", "Luft", "nicht", "sieht", ",", "im", "dunck\u00b7len", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "APPR", "ADJA", "NN", "PTKNEG", "VVFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "hell und klar,", "tokens": ["hell", "und", "klar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Und, in viel Millionen Strahlen ein Sonnen-Bild, zu", "tokens": ["Und", ",", "in", "viel", "Mil\u00b7lion\u00b7en", "Strah\u00b7len", "ein", "Son\u00b7nen\u00b7Bild", ",", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "PIAT", "NN", "NN", "ART", "NN", "$,", "APPR"], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "sehen war.", "tokens": ["se\u00b7hen", "war", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Ein recht Geweb\u2019 aus lauter Strahlen, die alle wunder-", "tokens": ["Ein", "recht", "Ge\u00b7web'", "aus", "lau\u00b7ter", "Strah\u00b7len", ",", "die", "al\u00b7le", "wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "PIAT", "NN", "$,", "PRELS", "PIAT", "TRUNC"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "w\u00fcrdig klein", "tokens": ["w\u00fcr\u00b7dig", "klein"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Und die nur durch den duncklen Grund, als eine Fulge,", "tokens": ["Und", "die", "nur", "durch", "den", "dunck\u00b7len", "Grund", ",", "als", "ei\u00b7ne", "Ful\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "sichtbar seyn,", "tokens": ["sicht\u00b7bar", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Umgaben es von allen Seiten, nichts rein- und kleiners,", "tokens": ["Um\u00b7ga\u00b7ben", "es", "von", "al\u00b7len", "Sei\u00b7ten", ",", "nichts", "rein", "und", "klei\u00b7ners", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,", "PIS", "TRUNC", "KON", "ADJA", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "nichts so sch\u00f6n,", "tokens": ["nichts", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Nichts bunt- und hellers, nichts so zart- und nettes kann", "tokens": ["Nichts", "bunt", "und", "hel\u00b7lers", ",", "nichts", "so", "zar\u00b7t", "und", "net\u00b7tes", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "TRUNC", "KON", "ADJA", "$,", "PIS", "ADV", "TRUNC", "KON", "ADJA", "VMFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "das Auge sehn.", "tokens": ["das", "Au\u00b7ge", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Die sch\u00f6ne Kleinheit drang durchs Auge selbst in den Sitz", "tokens": ["Die", "sch\u00f6\u00b7ne", "Klein\u00b7heit", "drang", "durchs", "Au\u00b7ge", "selbst", "in", "den", "Sitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "der Seelen ein;", "tokens": ["der", "See\u00b7len", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Ich dachte wie entsetzlich klein ist dieses Sonnen-Bildchen", "tokens": ["Ich", "dach\u00b7te", "wie", "ent\u00b7setz\u00b7lich", "klein", "ist", "die\u00b7ses", "Son\u00b7nen\u00b7Bild\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "ADJD", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "nicht", "tokens": ["nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Jm Gegenhalt mit seinem Urbild, dem unerme\u00dflich gros-", "tokens": ["Jm", "Ge\u00b7gen\u00b7halt", "mit", "sei\u00b7nem", "Ur\u00b7bild", ",", "dem", "un\u00b7er\u00b7me\u00df\u00b7lich", "gros"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "sen Licht,", "tokens": ["sen", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}}}}}