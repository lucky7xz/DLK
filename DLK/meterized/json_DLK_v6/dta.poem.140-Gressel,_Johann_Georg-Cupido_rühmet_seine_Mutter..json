{"dta.poem.140": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Cupido  r\u00fchmet seine Mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein edler Stamm/ der ist von einer K\u00f6niginnen/", "tokens": ["Mein", "ed\u00b7ler", "Stamm", "/", "der", "ist", "von", "ei\u00b7ner", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wegen ihrer Macht der Liebe wird geehrt.", "tokens": ["Die", "we\u00b7gen", "ih\u00b7rer", "Macht", "der", "Lie\u00b7be", "wird", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sch\u00f6nheit machet sie zu einer Pracht-G\u00f6ttinnen/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ma\u00b7chet", "sie", "zu", "ei\u00b7ner", "Pracht\u00b7G\u00f6t\u00b7tin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gantze Welt hat sie zur Herrscherin erkl\u00e4hrt.", "tokens": ["Die", "gant\u00b7ze", "Welt", "hat", "sie", "zur", "Herr\u00b7sche\u00b7rin", "er\u00b7kl\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie kan ein Demant-Hertz durch einen Blick gewinnen/", "tokens": ["Sie", "kan", "ein", "De\u00b7mant\u00b7Hertz", "durch", "ei\u00b7nen", "Blick", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Marmel-gleichen Sinn ihr holder Strahl verzehrt/", "tokens": ["Den", "Mar\u00b7mel\u00b7glei\u00b7chen", "Sinn", "ihr", "hol\u00b7der", "Strahl", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie werden also bald der s\u00fcssen Liebe innen/", "tokens": ["Sie", "wer\u00b7den", "al\u00b7so", "bald", "der", "s\u00fcs\u00b7sen", "Lie\u00b7be", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn sie mit Blitz und Feur die hellen Blicke nehrt.", "tokens": ["Wenn", "sie", "mit", "Blitz", "und", "Feur", "die", "hel\u00b7len", "Bli\u00b7cke", "nehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich samle von der Brust die sch\u00f6nsten Rosen ein/", "tokens": ["Ich", "sam\u00b7le", "von", "der", "Brust", "die", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein Schwaanen-Bette liegt auf dero zarten Ballen", "tokens": ["Mein", "Schwaa\u00b7nen\u00b7Bet\u00b7te", "liegt", "auf", "de\u00b7ro", "zar\u00b7ten", "Bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mich wieget sanffte ein das auf und niederfallen.", "tokens": ["Mich", "wie\u00b7get", "sanff\u00b7te", "ein", "das", "auf", "und", "nie\u00b7der\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "PDS", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer dieser G\u00f6ttin nun will ungehorsahm seyn/", "tokens": ["Wer", "die\u00b7ser", "G\u00f6t\u00b7tin", "nun", "will", "un\u00b7ge\u00b7hor\u00b7sahm", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ADV", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der wird durch einen Pfeil und Bogen \u00fcberwunden/", "tokens": ["Der", "wird", "durch", "ei\u00b7nen", "Pfeil", "und", "Bo\u00b7gen", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich treffe/ ob mir schon die Augen zugebunden.", "tokens": ["Ich", "tref\u00b7fe", "/", "ob", "mir", "schon", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}