{"textgrid.poem.63160": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Jochen Himmelreich", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Name ist Jochen Himmelreich,", "tokens": ["Mein", "Na\u00b7me", "ist", "Jo\u00b7chen", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich h\u00f6rte den Zapfenstreich", "tokens": ["Ich", "h\u00f6r\u00b7te", "den", "Zap\u00b7fen\u00b7streich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Tsingtau und Windhuk, in Warschau und Lille.", "tokens": ["In", "Tsing\u00b7tau", "und", "Wind\u00b7huk", ",", "in", "Warsc\u00b7hau", "und", "Lil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "--+-+-+++-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kaum sah ich die Sonne \u00fcber Flandern,", "tokens": ["Kaum", "sah", "ich", "die", "Son\u00b7ne", "\u00fc\u00b7ber", "Flan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Musst ich nach Mazedonien wandern,", "tokens": ["Musst", "ich", "nach", "Ma\u00b7ze\u00b7do\u00b7ni\u00b7en", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Tausend Meilen Marsch sind ein Kinderspiel.", "tokens": ["Tau\u00b7send", "Mei\u00b7len", "Marsch", "sind", "ein", "Kin\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NE", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wir sahen die deutsche Fahne strahlen", "tokens": ["Wir", "sa\u00b7hen", "die", "deut\u00b7sche", "Fah\u00b7ne", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In tausend Himmel und H\u00f6llenqualen,", "tokens": ["In", "tau\u00b7send", "Him\u00b7mel", "und", "H\u00f6l\u00b7len\u00b7qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "War immer ein Heiligenschein um sie.", "tokens": ["War", "im\u00b7mer", "ein", "Hei\u00b7li\u00b7gen\u00b7schein", "um", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und blieb uns die Zunge am Gaumen kleben,", "tokens": ["Und", "blieb", "uns", "die", "Zun\u00b7ge", "am", "Gau\u00b7men", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und hiess es des Kaisers Kleider weben,", "tokens": ["Und", "hiess", "es", "des", "Kai\u00b7sers", "Klei\u00b7der", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schimpfte der Offizier uns: Vieh \u2013", "tokens": ["Und", "schimpf\u00b7te", "der", "Of\u00b7fi\u00b7zier", "uns", ":", "Vieh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$.", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Deutschland, Du bist unser Tod und Leben!", "tokens": ["Deutschland", ",", "Du", "bist", "un\u00b7ser", "Tod", "und", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich bin dein Knecht,", "tokens": ["Ich", "bin", "dein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Des Landes Knecht,", "tokens": ["Des", "Lan\u00b7des", "Knecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und stehe auf der Wacht.", "tokens": ["Und", "ste\u00b7he", "auf", "der", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Schwarz ist die Nacht,", "tokens": ["Schwarz", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Weiss ist der Schnee,", "tokens": ["Weiss", "ist", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Weh,", "tokens": ["Weh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Es droht", "tokens": ["Es", "droht"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Der Tod", "tokens": ["Der", "Tod"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Dem morschen Weltgef\u00fcge.", "tokens": ["Dem", "mor\u00b7schen", "Welt\u00b7ge\u00b7f\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Rot fliesst das Blut aus unsrer Brust,", "tokens": ["Rot", "fliesst", "das", "Blut", "aus", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "O Lebensleid, o Lebenslust!", "tokens": ["O", "Le\u00b7bens\u00b7leid", ",", "o", "Le\u00b7bens\u00b7lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Fliege, schwarzweissrote Fahne, fliege ...", "tokens": ["Flie\u00b7ge", ",", "schwarz\u00b7weiss\u00b7ro\u00b7te", "Fah\u00b7ne", ",", "flie\u00b7ge", "..."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Name ist Jochen Himmelreich,", "tokens": ["Mein", "Na\u00b7me", "ist", "Jo\u00b7chen", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anfang und Ende ist alles gleich,", "tokens": ["An\u00b7fang", "und", "En\u00b7de", "ist", "al\u00b7les", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PIS", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "In den Unterst\u00e4nden brennt kein Sonnenlicht.", "tokens": ["In", "den", "Un\u00b7ter\u00b7st\u00e4n\u00b7den", "brennt", "kein", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Drei Jahre schlief ich nicht im Bette,", "tokens": ["Drei", "Jah\u00b7re", "schlief", "ich", "nicht", "im", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schnitt das Brot mit dem Bajonette,", "tokens": ["Ich", "schnitt", "das", "Brot", "mit", "dem", "Ba\u00b7jo\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Oh: die Blutflecken weichen aus meinen Kleidern nicht.", "tokens": ["Oh", ":", "die", "Blut\u00b7fle\u00b7cken", "wei\u00b7chen", "aus", "mei\u00b7nen", "Klei\u00b7dern", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Bruder, wir w\u00e4ren Kameraden geworden,", "tokens": ["Bru\u00b7der", ",", "wir", "w\u00e4\u00b7ren", "Ka\u00b7me\u00b7ra\u00b7den", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "VAPP", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Aber wir m\u00fcssen uns stechen und morden!", "tokens": ["A\u00b7ber", "wir", "m\u00fcs\u00b7sen", "uns", "ste\u00b7chen", "und", "mor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Deinen Blick, sterbender Neger, vergess ich nie.", "tokens": ["Dei\u00b7nen", "Blick", ",", "ster\u00b7ben\u00b7der", "Ne\u00b7ger", ",", "ver\u00b7gess", "ich", "nie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-++--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "L\u00e4ngst ist mir die eigene Sprache fremd.", "tokens": ["L\u00e4ngst", "ist", "mir", "die", "ei\u00b7ge\u00b7ne", "Spra\u00b7che", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich trage eine Franz\u00f6sinnenbluse als Soldatenhemd", "tokens": ["Ich", "tra\u00b7ge", "ei\u00b7ne", "Fran\u00b7z\u00f6\u00b7sin\u00b7nen\u00b7blu\u00b7se", "als", "Sol\u00b7da\u00b7ten\u00b7hemd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Und bin r\u00e4udiger als das r\u00e4udigste Vieh.", "tokens": ["Und", "bin", "r\u00e4u\u00b7di\u00b7ger", "als", "das", "r\u00e4u\u00b7digs\u00b7te", "Vieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Deutschland, die Schande wuchert und schlemmt!", "tokens": ["Deutschland", ",", "die", "Schan\u00b7de", "wu\u00b7chert", "und", "schlemmt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ich bin dein Knecht,", "tokens": ["Ich", "bin", "dein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Des Landes Knecht,", "tokens": ["Des", "Lan\u00b7des", "Knecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und stehe auf der Wacht:", "tokens": ["Und", "ste\u00b7he", "auf", "der", "Wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Schwarz ist die Nacht,", "tokens": ["Schwarz", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Weiss ist der Schnee,", "tokens": ["Weiss", "ist", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Weh,", "tokens": ["Weh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Es droht", "tokens": ["Es", "droht"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Die Not", "tokens": ["Die", "Not"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Dem Kindlein in der Wiege!", "tokens": ["Dem", "Kin\u00b7dlein", "in", "der", "Wie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Rot", "tokens": ["Rot"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Fliesst das Blut aus unsrer Brust,", "tokens": ["Fliesst", "das", "Blut", "aus", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "O Lebensleid, o Lebenslust!", "tokens": ["O", "Le\u00b7bens\u00b7leid", ",", "o", "Le\u00b7bens\u00b7lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Fliege, schwarzweissrote Fahne, fliege ...", "tokens": ["Flie\u00b7ge", ",", "schwarz\u00b7weiss\u00b7ro\u00b7te", "Fah\u00b7ne", ",", "flie\u00b7ge", "..."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Mein Name ist Jochen Himmelreich,", "tokens": ["Mein", "Na\u00b7me", "ist", "Jo\u00b7chen", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Weib ersoff sich im Teich,", "tokens": ["Mein", "Weib", "er\u00b7soff", "sich", "im", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Meine Kinder hungern und schreien durch die", "tokens": ["Mei\u00b7ne", "Kin\u00b7der", "hun\u00b7gern", "und", "schrei\u00b7en", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ART"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nacht nach mir.", "tokens": ["Nacht", "nach", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dieses Sommers Regenstr\u00f6me sind aus Kindertr\u00e4nen,", "tokens": ["Die\u00b7ses", "Som\u00b7mers", "Re\u00b7gen\u00b7str\u00f6\u00b7me", "sind", "aus", "Kin\u00b7der\u00b7tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Meine Arme muss ich in die N\u00e4chte dehnen", "tokens": ["Mei\u00b7ne", "Ar\u00b7me", "muss", "ich", "in", "die", "N\u00e4ch\u00b7te", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Sterne, o ihr Sterne strauchelt nicht wie wir!", "tokens": ["Ster\u00b7ne", ",", "o", "ihr", "Ster\u00b7ne", "strau\u00b7chelt", "nicht", "wie", "wir", "!"], "token_in