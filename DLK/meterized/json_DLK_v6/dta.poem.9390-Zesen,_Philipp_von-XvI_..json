{"dta.poem.9390": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll und mu\u00df ich von Jhr scheiden/", "tokens": ["Soll", "und", "mu\u00df", "ich", "von", "Ihr", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VMFIN", "PPER", "APPR", "PPOSAT", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne/ meine Lust und Zier/", "tokens": ["Sch\u00f6\u00b7ne", "/", "mei\u00b7ne", "Lust", "und", "Zier", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ey so lebe Sie in freuden", "tokens": ["Ey", "so", "le\u00b7be", "Sie", "in", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und verbleibe g\u00fcnstig Mier;", "tokens": ["und", "ver\u00b7blei\u00b7be", "g\u00fcns\u00b7tig", "Mier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weil die sch\u00f6ne Rose bl\u00fchet", "tokens": ["weil", "die", "sch\u00f6\u00b7ne", "Ro\u00b7se", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und die Sonne wasser zihet.", "tokens": ["und", "die", "Son\u00b7ne", "was\u00b7ser", "zi\u00b7het", "."], "token_info": ["word", "word", "word", "word", "wo