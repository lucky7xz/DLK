{"textgrid.poem.36312": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das tragische Schicksal", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles redet in Sentenzen,", "tokens": ["Al\u00b7les", "re\u00b7det", "in", "Sen\u00b7ten\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Auch die Helden werden Zwerge", "tokens": ["Auch", "die", "Hel\u00b7den", "wer\u00b7den", "Zwer\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der tragischen Latwerge,", "tokens": ["In", "der", "tra\u00b7gi\u00b7schen", "Lat\u00b7wer\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "M\u00fcssen idealisch gl\u00e4nzen.", "tokens": ["M\u00fcs\u00b7sen", "i\u00b7dea\u00b7lisch", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da\u00df die Szenen sich erg\u00e4nzen,", "tokens": ["Da\u00df", "die", "Sze\u00b7nen", "sich", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Nichts erhaben prahle,", "tokens": ["Und", "das", "Nichts", "er\u00b7ha\u00b7ben", "prah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles dankt man dem Schicksale.", "tokens": ["Al\u00b7les", "dankt", "man", "dem", "Schick\u00b7sa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie die jungen Katzen pflegen", "tokens": ["Wie", "die", "jun\u00b7gen", "Kat\u00b7zen", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem eignen Schweif zu gehen,", "tokens": ["Nach", "dem", "eig\u00b7nen", "Schweif", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df sich hier im ew'gen Drehen,", "tokens": ["Mu\u00df", "sich", "hier", "im", "ew'\u00b7gen", "Dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zufall und Vernunft bewegen.", "tokens": ["Zu\u00b7fall", "und", "Ver\u00b7nunft", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Herz von kleinen Schl\u00e4gen,", "tokens": ["Und", "das", "Herz", "von", "klei\u00b7nen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ganz empfindlich dem Schicksale,", "tokens": ["Ganz", "emp\u00b7find\u00b7lich", "dem", "Schick\u00b7sa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchlt die Quentchen in der Schale.", "tokens": ["F\u00fchlt", "die", "Quent\u00b7chen", "in", "der", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwischen Pflicht und dem Gef\u00fchle", "tokens": ["Zwi\u00b7schen", "Pflicht", "und", "dem", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df der Mensch verlegen stehn,", "tokens": ["Mu\u00df", "der", "Mensch", "ver\u00b7le\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder schlau durch beide gehn", "tokens": ["O\u00b7der", "schlau", "durch", "bei\u00b7de", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIS", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Tugend Zwickem\u00fchle.", "tokens": ["In", "der", "Tu\u00b7gend", "Zwi\u00b7cke\u00b7m\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahrlich hart auf trag'schem Pf\u00fchle,", "tokens": ["Wahr\u00b7lich", "hart", "auf", "trag'\u00b7schem", "Pf\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruht, wer im Theatersaale", "tokens": ["Ruht", ",", "wer", "im", "The\u00b7a\u00b7ter\u00b7saa\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dichten mu\u00df von dem Schicksale.", "tokens": ["Dich\u00b7ten", "mu\u00df", "von", "dem", "Schick\u00b7sa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}