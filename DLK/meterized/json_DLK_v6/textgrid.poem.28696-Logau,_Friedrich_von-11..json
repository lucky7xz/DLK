{"textgrid.poem.28696": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man h\u00e4lt es f\u00fcr gewi\u00df, da\u00df ietzt Ehbrecherey", "tokens": ["Man", "h\u00e4lt", "es", "f\u00fcr", "ge\u00b7wi\u00df", ",", "da\u00df", "ietzt", "Eh\u00b7bre\u00b7che\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ADV", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey uns so gar gemein und nur ein Possen sey.", "tokens": ["Bey", "uns", "so", "gar", "ge\u00b7mein", "und", "nur", "ein", "Pos\u00b7sen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "ADJD", "KON", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hilffts? Frantz\u00f6sisch ists, da\u00df di\u00df, was war zerst\u00fcckt,", "tokens": ["Was", "hilffts", "?", "Frant\u00b7z\u00f6\u00b7sisch", "ists", ",", "da\u00df", "di\u00df", ",", "was", "war", "zer\u00b7st\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "NN", "VAFIN", "$,", "KOUS", "PDS", "$,", "PWS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleib immer lieber so, als da\u00df man es sehr flickt.", "tokens": ["Bleib", "im\u00b7mer", "lie\u00b7ber", "so", ",", "als", "da\u00df", "man", "es", "sehr", "flickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "wor