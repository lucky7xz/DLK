{"dta.poem.4070": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Aber die Nacht kommt! La\u00df uns eilen, zu feyern", "tokens": ["A\u00b7ber", "die", "Nacht", "kommt", "!", "La\u00df", "uns", "ei\u00b7len", ",", "zu", "fey\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "VVIMP", "PPER", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "das Herbstfest.", "tokens": ["das", "Herbst\u00b7fest", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Heut noch! voll ist das Herz, aber das Leben", "tokens": ["Heut", "noch", "!", "voll", "ist", "das", "Herz", ",", "a\u00b7ber", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "ist kurz,", "tokens": ["ist", "kurz", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und was uns der himmlische Tag zu sagen geboten,", "tokens": ["Und", "was", "uns", "der", "himm\u00b7li\u00b7sche", "Tag", "zu", "sa\u00b7gen", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das zu nennen, mein Schmidt, reichen wir", "tokens": ["Das", "zu", "nen\u00b7nen", ",", "mein", "Schmidt", ",", "rei\u00b7chen", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Beide nicht aus.", "tokens": ["Bei\u00b7de", "nicht", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Trefliche bring' ich dir und das Freudenfeuer wird", "tokens": ["Tref\u00b7li\u00b7che", "bring'", "ich", "dir", "und", "das", "Freu\u00b7den\u00b7feu\u00b7er", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "KON", "ART", "NN", "VAFIN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "hoch auf", "tokens": ["hoch", "auf"], "token_info": ["word", "word"], "pos": ["ADJD", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Schlagen, und heiliger soll sprechen das k\u00fchnere", "tokens": ["Schla\u00b7gen", ",", "und", "hei\u00b7li\u00b7ger", "soll", "spre\u00b7chen", "das", "k\u00fch\u00b7ne\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADJA", "VMFIN", "VVINF", "ART", "ADJA"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Wort.", "tokens": ["Wort", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Siehe! da ist es rein! Und des Gottes freundliche", "tokens": ["Sie\u00b7he", "!", "da", "ist", "es", "rein", "!", "Und", "des", "Got\u00b7tes", "freund\u00b7li\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$.", "KON", "ART", "NN", "ADJA"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Gaben", "tokens": ["Ga\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Die wir theilen, sie sind zwischen den Liebenden", "tokens": ["Die", "wir", "thei\u00b7len", ",", "sie", "sind", "zwi\u00b7schen", "den", "Lie\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.15": {"text": "nur", "tokens": ["nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Anderes nicht \u2014 o kommt, o macht es wahr!", "tokens": ["An\u00b7de\u00b7res", "nicht", "o", "kommt", ",", "o", "macht", "es", "wahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$(", "FM", "VVFIN", "$,", "FM", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "denn allein ja", "tokens": ["denn", "al\u00b7lein", "ja"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Bin ich und Niemand nimmt mir von der Stirne", "tokens": ["Bin", "ich", "und", "Nie\u00b7mand", "nimmt", "mir", "von", "der", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KON", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "den Traum?", "tokens": ["den", "Traum", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Kommt und reicht, ihr Lieben, die Hand! das", "tokens": ["Kommt", "und", "reicht", ",", "ihr", "Lie\u00b7ben", ",", "die", "Hand", "!", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "ADJA", "$,", "ART", "NN", "$.", "ART"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "m\u00f6ge genug seyn,", "tokens": ["m\u00f6\u00b7ge", "ge\u00b7nug", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VAINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Aber die gr\u00f6\u00dfere Luft sparen dem Enkel wir auf.", "tokens": ["A\u00b7ber", "die", "gr\u00f6\u00b7\u00dfe\u00b7re", "Luft", "spa\u00b7ren", "dem", "En\u00b7kel", "wir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}