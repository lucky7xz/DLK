{"textgrid.poem.58933": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "152. Gott wei\u00df, was dir n\u00fctzt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft gibt Gott Licht und S\u00fc\u00dfigkeiten", "tokens": ["Oft", "gibt", "Gott", "Licht", "und", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur St\u00e4rkung in dem Kampf und Leiden,", "tokens": ["Zur", "St\u00e4r\u00b7kung", "in", "dem", "Kampf", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Oft l\u00e4\u00dft er uns in D\u00fcrre leben,", "tokens": ["Oft", "l\u00e4\u00dft", "er", "uns", "in", "D\u00fcr\u00b7re", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir uns lernen rein ergeben.", "tokens": ["Da\u00df", "wir", "uns", "ler\u00b7nen", "rein", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}