{"textgrid.poem.45881": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ich ein Bettelmann w\u00e4r']", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich ein Bettelmann w\u00e4r'", "tokens": ["Wenn", "ich", "ein", "Bet\u00b7tel\u00b7mann", "w\u00e4r'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "K\u00e4m' ich zu Dir,", "tokens": ["K\u00e4m'", "ich", "zu", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "S\u00e4h' Dich gar bittend an", "tokens": ["S\u00e4h'", "Dich", "gar", "bit\u00b7tend", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was g\u00e4bst Du mir? \u2013", "tokens": ["Was", "g\u00e4bst", "Du", "mir", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Pfennig hilft mir nicht", "tokens": ["Der", "Pfen\u00b7nig", "hilft", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nimm ihn zur\u00fcck,", "tokens": ["Nimm", "ihn", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Goldner als golden gl\u00e4nzt", "tokens": ["Gold\u00b7ner", "als", "gol\u00b7den", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOUS", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Allen Dein Blick;", "tokens": ["Al\u00b7len", "Dein", "Blick", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und was Du allen giebst", "tokens": ["Und", "was", "Du", "al\u00b7len", "giebst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gebe nicht mir", "tokens": ["Ge\u00b7be", "nicht", "mir"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKNEG", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur was mein Aug' begehrt", "tokens": ["Nur", "was", "mein", "Aug'", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will ich von Dir.", "tokens": ["Will", "ich", "von", "Dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Bettler wie helf' ich Dir? \u2013", "tokens": ["Bett\u00b7ler", "wie", "hel\u00b7f'", "ich", "Dir", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Spr\u00e4chst Du nur so,", "tokens": ["Spr\u00e4chst", "Du", "nur", "so", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dann w\u00e4r' im Herzen ich", "tokens": ["Dann", "w\u00e4r'", "im", "Her\u00b7zen", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gl\u00fccklich und froh.", "tokens": ["Gl\u00fcck\u00b7lich", "und", "froh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Laufst auf Dein K\u00e4mmerlein", "tokens": ["Laufst", "auf", "Dein", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Holst ein Paar Schuh", "tokens": ["Holst", "ein", "Paar", "Schuh"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die sind mir viel zu klein,", "tokens": ["Die", "sind", "mir", "viel", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sieh einmal zu. \u2013", "tokens": ["Sieh", "ein\u00b7mal", "zu", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Sieh nur wie klein sie sind", "tokens": ["Sieh", "nur", "wie", "klein", "sie", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "KOKOM", "ADJD", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dr\u00fccken mich sehr,", "tokens": ["Dr\u00fc\u00b7cken", "mich", "sehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Jungfrau s\u00fc\u00df l\u00e4chelst Du", "tokens": ["Jung\u00b7frau", "s\u00fc\u00df", "l\u00e4\u00b7chelst", "Du"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "O gieb mir mehr.", "tokens": ["O", "gieb", "mir", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}