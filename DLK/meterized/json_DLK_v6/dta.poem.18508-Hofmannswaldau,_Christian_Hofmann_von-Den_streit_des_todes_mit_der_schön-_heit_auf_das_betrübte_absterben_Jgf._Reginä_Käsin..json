{"dta.poem.18508": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Den streit des todes mit der sch\u00f6n-  \n heit auf das betr\u00fcbte absterben Jgf.  \n Regin\u00e4 K\u00e4sin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje gehts doch immer zu/ da\u00df/ was man sch\u00f6ne nennet/", "tokens": ["Wie", "gehts", "doch", "im\u00b7mer", "zu", "/", "da\u00df", "/", "was", "man", "sch\u00f6\u00b7ne", "nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "PTKZU", "$(", "KOUS", "$(", "PWS", "PIS", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der verg\u00e4nglichkeit den ersten streit bek\u00f6mmt?", "tokens": ["Mit", "der", "ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "den", "ers\u00b7ten", "streit", "be\u00b7k\u00f6mmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht anders/ als weil ihm der neid den ruhm nicht g\u00f6nnet/", "tokens": ["Nicht", "an\u00b7ders", "/", "als", "weil", "ihm", "der", "neid", "den", "ruhm", "nicht", "g\u00f6n\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$(", "KOKOM", "KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seine zierlichkeit/ so viel nur m\u00f6glich/ hemmt!", "tokens": ["Und", "sei\u00b7ne", "zier\u00b7lich\u00b7keit", "/", "so", "viel", "nur", "m\u00f6g\u00b7lich", "/", "hemmt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ADV", "ADV", "ADV", "ADJD", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Am meisten mu\u00df man di\u00df an denen menschen sehen/", "tokens": ["Am", "meis\u00b7ten", "mu\u00df", "man", "di\u00df", "an", "de\u00b7nen", "men\u00b7schen", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VMFIN", "PIS", "PDS", "APPR", "PRELS", "PIS", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So GOtt und die natur vor andern sch\u00f6n gemacht:", "tokens": ["So", "Gott", "und", "die", "na\u00b7tur", "vor", "an\u00b7dern", "sch\u00f6n", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ART", "NN", "APPR", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den morgen findt man offt/ da\u00df es um sie geschehen/", "tokens": ["Den", "mor\u00b7gen", "findt", "man", "offt", "/", "da\u00df", "es", "um", "sie", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PIS", "ADV", "$(", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Obgleich des abends noch die anmuth sie bewacht.", "tokens": ["Ob\u00b7gleich", "des", "a\u00b7bends", "noch", "die", "an\u00b7muth", "sie", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Di\u00df thut der tod/ ein feind von allen lieblichkeiten/", "tokens": ["Di\u00df", "thut", "der", "tod", "/", "ein", "feind", "von", "al\u00b7len", "lieb\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$(", "ART", "NN", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den uns die phantasie des k\u00fcnstlers also zeigt:", "tokens": ["Den", "uns", "die", "phan\u00b7ta\u00b7sie", "des", "k\u00fcnst\u00b7lers", "al\u00b7so", "zeigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie nichts als ein geripp an ihn auff allen seiten/", "tokens": ["Wie", "nichts", "als", "ein", "ge\u00b7ripp", "an", "ihn", "auff", "al\u00b7len", "sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "ART", "ADJD", "APPR", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und er nach lebenden auff d\u00fcrren knochen steigt;", "tokens": ["Und", "er", "nach", "le\u00b7ben\u00b7den", "auff", "d\u00fcr\u00b7ren", "kno\u00b7chen", "steigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Der in der lincken hand den leeren seiger f\u00fchret/", "tokens": ["Der", "in", "der", "lin\u00b7cken", "hand", "den", "lee\u00b7ren", "sei\u00b7ger", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und an der rechten sich mit einer fense tr\u00e4gt/", "tokens": ["Und", "an", "der", "rech\u00b7ten", "sich", "mit", "ei\u00b7ner", "fen\u00b7se", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bey dessen umkrei\u00df man verwelckte blumen sp\u00fcret/", "tokens": ["Bey", "des\u00b7sen", "um\u00b7krei\u00df", "man", "ver\u00b7welck\u00b7te", "blu\u00b7men", "sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PIS", "VVFIN", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Die seine grimmigkeit auff sarg und grab hinlegt.", "tokens": ["Die", "sei\u00b7ne", "grim\u00b7mig\u00b7keit", "auff", "sarg", "und", "grab", "hin\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was mag nun also dem wohl mehr zuwider fallen/", "tokens": ["Was", "mag", "nun", "al\u00b7so", "dem", "wohl", "mehr", "zu\u00b7wi\u00b7der", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als so ein bild/ das auch ein pinsel nicht erreicht?", "tokens": ["Als", "so", "ein", "bild", "/", "das", "auch", "ein", "pin\u00b7sel", "nicht", "er\u00b7reicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$(", "PDS", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auff dessen wangen man nur milch und blut sieht wallen/", "tokens": ["Auff", "des\u00b7sen", "wan\u00b7gen", "man", "nur", "milch", "und", "blut", "sieht", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PIS", "ADV", "ADJD", "KON", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dem weder schwanen-art noch purpur-farbe gleicht:", "tokens": ["Dem", "we\u00b7der", "schwa\u00b7nen\u00b7art", "noch", "pur\u00b7pur\u00b7fa\u00b7rbe", "gleicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "ADJD", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Aus dessen augen-paar zwey kleine sonnen strahlen/", "tokens": ["Aus", "des\u00b7sen", "au\u00b7gen\u00b7paar", "zwey", "klei\u00b7ne", "son\u00b7nen", "strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "CARD", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "An dem die lippen gleich als zucker-rosen bl\u00fchn/", "tokens": ["An", "dem", "die", "lip\u00b7pen", "gleich", "als", "zu\u00b7cke\u00b7rro\u00b7sen", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und dessen weissen hals die lilien bemahlen/", "tokens": ["Und", "des\u00b7sen", "weis\u00b7sen", "hals", "die", "li\u00b7li\u00b7en", "be\u00b7mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Kurtz/ alle glieder nichts denn lieben nach sich ziehn?", "tokens": ["Kurtz", "/", "al\u00b7le", "glie\u00b7der", "nichts", "denn", "lie\u00b7ben", "nach", "sich", "ziehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PIAT", "NN", "PIS", "ADV", "VVFIN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die sucht des todes ha\u00df so gr\u00e4\u00dflich zu masqviren/", "tokens": ["Die", "sucht", "des", "to\u00b7des", "ha\u00df", "so", "gr\u00e4\u00df\u00b7lich", "zu", "mas\u00b7qvi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df in sein ebenbild der gantze mensch verf\u00e4llt.", "tokens": ["Da\u00df", "in", "sein", "e\u00b7ben\u00b7bild", "der", "gant\u00b7ze", "mensch", "ver\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allein di\u00df l\u00e4\u00dft sich nun nicht ohne kampf vollf\u00fchren/", "tokens": ["Al\u00b7lein", "di\u00df", "l\u00e4\u00dft", "sich", "nun", "nicht", "oh\u00b7ne", "kampf", "voll\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PRF", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da denn itzt hier/ itzt dar sich der triumph einstellt.", "tokens": ["Da", "denn", "itzt", "hier", "/", "itzt", "dar", "sich", "der", "tri\u00b7umph", "ein\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "$(", "ADV", "PTKVZ", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Bald mu\u00df der schlaue gast die larve wieder nehmen/", "tokens": ["Bald", "mu\u00df", "der", "schlau\u00b7e", "gast", "die", "lar\u00b7ve", "wie\u00b7der", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "VVFIN", "ART", "ADJA", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn der zu muntre leib erneute kr\u00e4ffte kriegt;", "tokens": ["Wenn", "der", "zu", "mun\u00b7tre", "leib", "er\u00b7neu\u00b7te", "kr\u00e4ff\u00b7te", "kriegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PTKZU", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bald aber soll nach ihr der krancke sich beqvemen/", "tokens": ["Bald", "a\u00b7ber", "soll", "nach", "ihr", "der", "kran\u00b7cke", "sich", "be\u00b7qve\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "APPR", "PPER", "ART", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn sein gesicht erbla\u00dft auf einer baare liegt.", "tokens": ["Wenn", "sein", "ge\u00b7sicht", "er\u00b7bla\u00dft", "auf", "ei\u00b7ner", "baa\u00b7re", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVPP", "VVFIN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Di\u00df will uns leider! itzt ein traurig beyspiel lehren/", "tokens": ["Di\u00df", "will", "uns", "lei\u00b7der", "!", "itzt", "ein", "trau\u00b7rig", "bey\u00b7spiel", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "$.", "ADV", "ART", "ADJD", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da man ein liebstes kind in sterbe-rock einkleidt/", "tokens": ["Da", "man", "ein", "liebs\u00b7tes", "kind", "in", "ster\u00b7be\u00b7rock", "ein\u00b7kleidt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPR", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das durch der sch\u00f6nheit glantz den tod nicht kan beth\u00f6ren/", "tokens": ["Das", "durch", "der", "sch\u00f6n\u00b7heit", "glantz", "den", "tod", "nicht", "kan", "be\u00b7th\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Auff dessen anfall es von seinen eltern scheidt:", "tokens": ["Auff", "des\u00b7sen", "an\u00b7fall", "es", "von", "sei\u00b7nen", "el\u00b7tern", "scheidt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein angenehmer zweig/ aus einem solchem stamme/", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "zweig", "/", "aus", "ei\u00b7nem", "sol\u00b7chem", "stam\u00b7me", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "ART", "PIAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der auch schon anderweit von sch\u00f6ner frucht bekandt/", "tokens": ["Der", "auch", "schon", "an\u00b7der\u00b7weit", "von", "sch\u00f6\u00b7ner", "frucht", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein rauch-altar/ worauff des vaters liebes-flamme", "tokens": ["Ein", "rauch\u00b7al\u00b7tar", "/", "wo\u00b7rauff", "des", "va\u00b7ters", "lie\u00b7bes\u00b7flam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit h\u00f6chster billigkeit in voller glut gebrannt;", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "bil\u00b7lig\u00b7keit", "in", "vol\u00b7ler", "glut", "ge\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": 