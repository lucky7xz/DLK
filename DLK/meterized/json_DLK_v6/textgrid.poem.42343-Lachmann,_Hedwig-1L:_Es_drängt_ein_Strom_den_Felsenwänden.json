{"textgrid.poem.42343": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es dr\u00e4ngt ein Strom den Felsenw\u00e4nden", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es dr\u00e4ngt ein Strom den Felsenw\u00e4nden", "tokens": ["Es", "dr\u00e4ngt", "ein", "Strom", "den", "Fel\u00b7sen\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Erdeninnern zu enteilen,", "tokens": ["Des", "Er\u00b7de\u00b7nin\u00b7nern", "zu", "ent\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In unterirdischen Gel\u00e4nden", "tokens": ["In", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Ge\u00b7l\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fliesst er verborgen viele Meilen.", "tokens": ["Fliesst", "er", "ver\u00b7bor\u00b7gen", "vie\u00b7le", "Mei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PIAT", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit keinem Bruderstrom verb\u00fcndet,", "tokens": ["Mit", "kei\u00b7nem", "Bru\u00b7der\u00b7strom", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4lzt er sich fort in Meeresn\u00e4hen,", "tokens": ["W\u00e4lzt", "er", "sich", "fort", "in", "Mee\u00b7res\u00b7n\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Bis er an seinem Ziele m\u00fcndet,", "tokens": ["Bis", "er", "an", "sei\u00b7nem", "Zie\u00b7le", "m\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An tiefer Stelle ungesehen.", "tokens": ["An", "tie\u00b7fer", "Stel\u00b7le", "un\u00b7ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Trachten geht geheim und stille", "tokens": ["Ein", "Trach\u00b7ten", "geht", "ge\u00b7heim", "und", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach eignem Ziel und eigner Ferne.", "tokens": ["Nach", "eig\u00b7nem", "Ziel", "und", "eig\u00b7ner", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der tiefe dunkle Sehnsuchtswille", "tokens": ["Der", "tie\u00b7fe", "dunk\u00b7le", "Sehn\u00b7suchts\u00b7wil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwacht in N\u00e4chten ohne Sterne.", "tokens": ["Er\u00b7wacht", "in", "N\u00e4ch\u00b7ten", "oh\u00b7ne", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}