{"textgrid.poem.40971": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Erbarmen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl vor dem Fenster im Bauer", "tokens": ["Wohl", "vor", "dem", "Fens\u00b7ter", "im", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sitzt ein V\u00f6glein im Regenschauer,", "tokens": ["Sitzt", "ein", "V\u00f6\u00b7glein", "im", "Re\u00b7gen\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hinaus tat's das M\u00e4gdlein im Sonnenschein,", "tokens": ["Hin\u00b7aus", "tat's", "das", "M\u00e4gd\u00b7lein", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nun st\u00fcrmt's, und sie holt es erbarmend herein.", "tokens": ["Nun", "st\u00fcrmt's", ",", "und", "sie", "holt", "es", "er\u00b7bar\u00b7mend", "her\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Hand Gottes! bet' ich in Trauer \u2013", "tokens": ["Hand", "Got\u00b7tes", "!", "bet'", "ich", "in", "Trau\u00b7er", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "L\u00e4ngst st\u00fcrmt's um mich, fehlt mir der Sonne Schein,", "tokens": ["L\u00e4ngst", "st\u00fcrmt's", "um", "mich", ",", "fehlt", "mir", "der", "Son\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Hand Gottes! hol' mich erbarmend herein!", "tokens": ["Hand", "Got\u00b7tes", "!", "hol'", "mich", "er\u00b7bar\u00b7mend", "her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}