{"textgrid.poem.23973": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Abend", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die grauen Geierfittiche der Nacht", "tokens": ["Die", "grau\u00b7en", "Gei\u00b7er\u00b7fit\u00b7ti\u00b7che", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rauschen \u00fcber den See.", "tokens": ["Rau\u00b7schen", "\u00fc\u00b7ber", "den", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In seinen erzenen F\u00e4ngen h\u00e4lt der Riesenvogel", "tokens": ["In", "sei\u00b7nen", "er\u00b7ze\u00b7nen", "F\u00e4n\u00b7gen", "h\u00e4lt", "der", "Rie\u00b7sen\u00b7vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Leiche des Tages.", "tokens": ["Die", "Lei\u00b7che", "des", "Ta\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein Blutspur hinter ihm her", "tokens": ["Ein", "Blut\u00b7spur", "hin\u00b7ter", "ihm", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "APZR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wellt nach Westen.", "tokens": ["Wellt", "nach", "Wes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die schwarzen Augen des Waldes", "tokens": ["Die", "schwar\u00b7zen", "Au\u00b7gen", "des", "Wal\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Heben die Nadelwimpern", "tokens": ["He\u00b7ben", "die", "Na\u00b7del\u00b7wim\u00b7pern"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Und starren stumm", "tokens": ["Und", "star\u00b7ren", "stumm"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Dem Fluge des R\u00e4ubers nach,", "tokens": ["Dem", "Flu\u00b7ge", "des", "R\u00e4u\u00b7bers", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Dem eine Schaar verdrossener Schatten folgt.", "tokens": ["Dem", "ei\u00b7ne", "Schaar", "ver\u00b7dros\u00b7se\u00b7ner", "Schat\u00b7ten", "folgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Vom Himmel herunter", "tokens": ["Vom", "Him\u00b7mel", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "APZR"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "In frostigen Winden", "tokens": ["In", "fros\u00b7ti\u00b7gen", "Win\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Haucht ein Gedanke:", "tokens": ["Haucht", "ein", "Ge\u00b7dan\u00b7ke", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Auf schwarzen Schwingen", "tokens": ["Auf", "schwar\u00b7zen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Schwebt alles Leben", "tokens": ["Schwebt", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Schweigend", "tokens": ["Schwei\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "In das Thal des Todes.", "tokens": ["In", "das", "Thal", "des", "To\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}