{"dta.poem.10403": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Und als ich so lange, so lange ges\u00e4umt,", "tokens": ["Und", "als", "ich", "so", "lan\u00b7ge", ",", "so", "lan\u00b7ge", "ge\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In fremden Landen geschw\u00e4rmt und getr\u00e4umt;", "tokens": ["In", "frem\u00b7den", "Lan\u00b7den", "ge\u00b7schw\u00e4rmt", "und", "ge\u00b7tr\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da ward meiner Liebsten zu lang die Zeit,", "tokens": ["Da", "ward", "mei\u00b7ner", "Liebs\u00b7ten", "zu", "lang", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und sie n\u00e4hete sich ein Hochzeitkleid,", "tokens": ["Und", "sie", "n\u00e4\u00b7he\u00b7te", "sich", "ein", "Hoch\u00b7zeit\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und hat mit z\u00e4rtlichen Armen umschlungen,", "tokens": ["Und", "hat", "mit", "z\u00e4rt\u00b7li\u00b7chen", "Ar\u00b7men", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Als Br\u00e4ut'gam, den d\u00fcmmsten der dummen Jungen.", "tokens": ["Als", "Br\u00e4ut'\u00b7gam", ",", "den", "d\u00fcmms\u00b7ten", "der", "dum\u00b7men", "Jun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mein Liebchen ist so sch\u00f6n und mild,", "tokens": ["Mein", "Lieb\u00b7chen", "ist", "so", "sch\u00f6n", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch schwebt mir vor ihr s\u00fc\u00dfes Bild;", "tokens": ["Noch", "schwebt", "mir", "vor", "ihr", "s\u00fc\u00b7\u00dfes", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Veilchenaugen, die Rosenw\u00e4nglein,", "tokens": ["Die", "Veil\u00b7chen\u00b7au\u00b7gen", ",", "die", "Ro\u00b7sen\u00b7w\u00e4ng\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die gl\u00fchen und bl\u00fchen, jahraus jahrein.", "tokens": ["Die", "gl\u00fc\u00b7hen", "und", "bl\u00fc\u00b7hen", ",", "ja\u00b7hraus", "ja\u00b7hrein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da\u00df ich von solchem Lieb konnt weichen,", "tokens": ["Da\u00df", "ich", "von", "sol\u00b7chem", "Lieb", "konnt", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "War der d\u00fcmmste von meinen dummen Streichen.", "tokens": ["War", "der", "d\u00fcmms\u00b7te", "von", "mei\u00b7nen", "dum\u00b7men", "Strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}