{"textgrid.poem.33618": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Herbstabend", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die halbentlaubten B\u00e4ume", "tokens": ["Durch", "die", "hal\u00b7bent\u00b7laub\u00b7ten", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glimmt der Mond, und still umflicht", "tokens": ["Glimmt", "der", "Mond", ",", "und", "still", "um\u00b7flicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siegreich die verlassnen R\u00e4ume", "tokens": ["Sieg\u00b7reich", "die", "ver\u00b7lass\u00b7nen", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Trauer falbes Licht.", "tokens": ["Sei\u00b7ner", "Trau\u00b7er", "fal\u00b7bes", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So dem Blick der Schwermut offen", "tokens": ["So", "dem", "Blick", "der", "Schwer\u00b7mut", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt des Menschen Los erhellt,", "tokens": ["Liegt", "des", "Men\u00b7schen", "Los", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn vom ersten Frost betroffen", "tokens": ["Wenn", "vom", "ers\u00b7ten", "Frost", "be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsres Lebens Schmuck zerf\u00e4llt.", "tokens": ["Uns\u00b7res", "Le\u00b7bens", "Schmuck", "zer\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tausende schon l\u00e4ngst Begrabner", "tokens": ["Tau\u00b7sen\u00b7de", "schon", "l\u00e4ngst", "Be\u00b7grab\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "F\u00fchlten, duldeten wie du.", "tokens": ["F\u00fchl\u00b7ten", ",", "dul\u00b7de\u00b7ten", "wie", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denk es, und du gehst erhabner,", "tokens": ["Denk", "es", ",", "und", "du", "gehst", "er\u00b7hab\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Ziel entschlossner zu!", "tokens": ["Dei\u00b7nem", "Ziel", "ent\u00b7schloss\u00b7ner", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flammt doch deine Lebenswelle", "tokens": ["Flammt", "doch", "dei\u00b7ne", "Le\u00b7bens\u00b7wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber der Erloschnen Tag,", "tokens": ["\u00dc\u00b7ber", "der", "Er\u00b7lo\u00b7schnen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie des Fackeltr\u00e4gers Helle", "tokens": ["Wie", "des", "Fa\u00b7ckel\u00b7tr\u00e4\u00b7gers", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber einem Sarkophag.", "tokens": ["\u00dc\u00b7ber", "ei\u00b7nem", "Sar\u00b7ko\u00b7phag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}