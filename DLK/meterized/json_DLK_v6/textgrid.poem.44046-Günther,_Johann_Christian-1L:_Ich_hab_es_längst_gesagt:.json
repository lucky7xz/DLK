{"textgrid.poem.44046": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab es l\u00e4ngst gesagt:", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab es l\u00e4ngst gesagt:", "tokens": ["Ich", "hab", "es", "l\u00e4ngst", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So sehr mich alles plagt,", "tokens": ["So", "sehr", "mich", "al\u00b7les", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wenig f\u00e4llt mein Muth vor Kummer hin;", "tokens": ["So", "we\u00b7nig", "f\u00e4llt", "mein", "Muth", "vor", "Kum\u00b7mer", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Hofnung ist mein Schild,", "tokens": ["Die", "Hof\u00b7nung", "ist", "mein", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die Misgunst billt,", "tokens": ["Und", "wenn", "die", "Mis\u00b7gunst", "billt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So such ich Trost bey mir und bleibe wie ich bin.", "tokens": ["So", "such", "ich", "Trost", "bey", "mir", "und", "blei\u00b7be", "wie", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPER", "KON", "VVFIN", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Gl\u00fccke schl\u00e4ft zwar starck,", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "schl\u00e4ft", "zwar", "starck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Weh fri\u00dft Blut und Marck,", "tokens": ["Das", "Weh", "fri\u00dft", "Blut", "und", "Marck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was mich sieht, das greift mich feindlich an.", "tokens": ["Und", "was", "mich", "sieht", ",", "das", "greift", "mich", "feind\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie, sollt ich \u00e4ngstlich schreyn?", "tokens": ["Wie", ",", "sollt", "ich", "\u00e4ngst\u00b7lich", "schreyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verzweifle nur! O nein,", "tokens": ["Ver\u00b7zweif\u00b7le", "nur", "!", "O", "nein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "NE", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es lebt ja noch die Zeit, die alles \u00e4ndern kan.", "tokens": ["Es", "lebt", "ja", "noch", "die", "Zeit", ",", "die", "al\u00b7les", "\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich hab es oft gesehn,", "tokens": ["Ich", "hab", "es", "oft", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df die, so heute flehn,", "tokens": ["Da\u00df", "die", ",", "so", "heu\u00b7te", "flehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Morgenr\u00f6the Glanz mit Trost erquickt", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Glanz", "mit", "Trost", "er\u00b7quickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mancher, de\u00dfen List", "tokens": ["Und", "man\u00b7cher", ",", "de\u00b7\u00dfen", "List"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELAT", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Den Purpur nechst gek\u00fcst,", "tokens": ["Den", "Pur\u00b7pur", "nechst", "ge\u00b7k\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch einen j\u00e4hen Fall dem Gl\u00fcck im Schoos erstickt.", "tokens": ["Durch", "ei\u00b7nen", "j\u00e4\u00b7hen", "Fall", "dem", "Gl\u00fcck", "im", "Schoos", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr Herzen, derer Noth", "tokens": ["Ihr", "Her\u00b7zen", ",", "de\u00b7rer", "Noth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PDS", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.2": {"text": "Mit langer Marter droht,", "tokens": ["Mit", "lan\u00b7ger", "Mar\u00b7ter", "droht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vertraut doch nur dem Himmel und der Zeit,", "tokens": ["Ver\u00b7traut", "doch", "nur", "dem", "Him\u00b7mel", "und", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bedenckt, da\u00df Last und Weh", "tokens": ["Be\u00b7denckt", ",", "da\u00df", "Last", "und", "Weh"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Gro\u00dfmuth stets erh\u00f6h,", "tokens": ["Die", "Gro\u00df\u00b7muth", "stets", "er\u00b7h\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So wie Gefahr und Kampf den Helden Palmen beuth.", "tokens": ["So", "wie", "Ge\u00b7fahr", "und", "Kampf", "den", "Hel\u00b7den", "Pal\u00b7men", "beuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "KON", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Komm, Hofnung, k\u00fc\u00dfe mich.", "tokens": ["Komm", ",", "Hof\u00b7nung", ",", "k\u00fc\u00b7\u00dfe", "mich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Geist umarmet dich", "tokens": ["Mein", "Geist", "um\u00b7ar\u00b7met", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und trift in dir den Himmel auf der Welt,", "tokens": ["Und", "trift", "in", "dir", "den", "Him\u00b7mel", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du solt mein Leichenstein", "tokens": ["Du", "solt", "mein", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wie mein Brautkleid seyn", "tokens": ["So", "wie", "mein", "Braut\u00b7kleid", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und bleibst mein lezter Trost, wenn alles bricht und f\u00e4llt.", "tokens": ["Und", "bleibst", "mein", "lez\u00b7ter", "Trost", ",", "wenn", "al\u00b7les", "bricht", "und", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}