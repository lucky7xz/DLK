{"dta.poem.665": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was seh' ich dort f\u00fcr schmertzen volle hauffen? ", "tokens": ["Was", "seh'", "ich", "dort", "f\u00fcr", "schmert\u00b7zen", "vol\u00b7le", "hauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie das sie so die br\u00fcste schlagen? ", "tokens": ["Wie", "das", "sie", "so", "die", "br\u00fcs\u00b7te", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PPER", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd mit so rei", "tokens": ["Vnd", "mit", "so", "rei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Vnd jhr verwirrtes haar au\u00dfrauffen?", "tokens": ["Vnd", "jhr", "ver\u00b7wirr\u00b7tes", "haar", "au\u00df\u00b7rauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was stre", "tokens": ["Was", "stre"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Die jhrer zehren regen netzt? ", "tokens": ["Die", "jhrer", "zeh\u00b7ren", "re\u00b7gen", "netzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer mag es seyn der sie verletzt?", "tokens": ["Wer", "mag", "es", "seyn", "der", "sie", "ver\u00b7letzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VAINF", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie das sie so kleinm\u00fcttig werden?", "tokens": ["Wie", "das", "sie", "so", "klein\u00b7m\u00fct\u00b7tig", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}