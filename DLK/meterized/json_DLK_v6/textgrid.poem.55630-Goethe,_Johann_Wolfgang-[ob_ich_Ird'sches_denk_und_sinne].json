{"textgrid.poem.55630": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[ob ich Ird'sches denk und sinne]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich Ird'sches denk und sinne,", "tokens": ["Ob", "ich", "Ird'\u00b7sches", "denk", "und", "sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das gereicht zu h\u00f6herem Gewinne.", "tokens": ["Das", "ge\u00b7reicht", "zu", "h\u00f6\u00b7he\u00b7rem", "Ge\u00b7win\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit dem Staube nicht der Geist zerstoben,", "tokens": ["Mit", "dem", "Stau\u00b7be", "nicht", "der", "Geist", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dringet, in sich selbst gedr\u00e4ngt, nach oben.", "tokens": ["Drin\u00b7get", ",", "in", "sich", "selbst", "ge\u00b7dr\u00e4ngt", ",", "nach", "o\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "APPR", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}