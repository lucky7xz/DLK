{"textgrid.poem.34051": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Schmetterling, der mich in der Gr\u00e4berstra\u00dfe zu Pompeji umflog", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fast in's Angesicht fliegt mir der Schmetterling, immer so scheu sonst;", "tokens": ["Fast", "in's", "An\u00b7ge\u00b7sicht", "fliegt", "mir", "der", "Schmet\u00b7ter\u00b7ling", ",", "im\u00b7mer", "so", "scheu", "sonst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ahnt er, da\u00df hier ein Mensch gar nicht zu t\u00f6dten vermag?", "tokens": ["Ahnt", "er", ",", "da\u00df", "hier", "ein", "Mensch", "gar", "nicht", "zu", "t\u00f6d\u00b7ten", "ver\u00b7mag", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "In der begrabenen Stadt und unter begrabenen Gr\u00e4bern,", "tokens": ["In", "der", "be\u00b7gra\u00b7be\u00b7nen", "Stadt", "und", "un\u00b7ter", "be\u00b7gra\u00b7be\u00b7nen", "Gr\u00e4\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Halt' ich den Odem sogar an, wenn der Gaukler sich naht!", "tokens": ["Halt'", "ich", "den", "O\u00b7dem", "so\u00b7gar", "an", ",", "wenn", "der", "Gauk\u00b7ler", "sich", "naht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}}}}