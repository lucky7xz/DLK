{"textgrid.poem.44880": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ein Ochs ging auf die Wiese]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ochs ging auf die Wiese,", "tokens": ["Ein", "Ochs", "ging", "auf", "die", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo er nach Kr\u00e4ften fra\u00df,", "tokens": ["Wo", "er", "nach", "Kr\u00e4f\u00b7ten", "fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da waren Blumen, Kr\u00e4uter,", "tokens": ["Da", "wa\u00b7ren", "Blu\u00b7men", ",", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es k\u00fcmmert ihn nicht weiter,", "tokens": ["Es", "k\u00fcm\u00b7mert", "ihn", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr ihn war alles Gras.", "tokens": ["F\u00fcr", "ihn", "war", "al\u00b7les", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}