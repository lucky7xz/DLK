{"textgrid.poem.36849": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht von Heroen und Kriegern, von K\u00f6nigen oder Tyrannen,", "tokens": ["Nicht", "von", "He\u00b7roen", "und", "Krie\u00b7gern", ",", "von", "K\u00f6\u00b7ni\u00b7gen", "o\u00b7der", "Ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dion und Dionys und von Timoleon nicht,", "tokens": ["Di\u00b7on", "und", "Di\u00b7o\u00b7nys", "und", "von", "Ti\u00b7mo\u00b7le\u00b7on", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "KON", "APPR", "NE", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht von Roger dem Grafen, von Arabern oder Normannen,", "tokens": ["Nicht", "von", "Ro\u00b7ger", "dem", "Gra\u00b7fen", ",", "von", "A\u00b7ra\u00b7bern", "o\u00b7der", "Nor\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+-+--", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Nicht von Staufen ert\u00f6nt oder von Franken mein Lied.", "tokens": ["Nicht", "von", "Stau\u00b7fen", "er\u00b7t\u00f6nt", "o\u00b7der", "von", "Fran\u00b7ken", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "KON", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Euch, o freundliche Wellen, entrauscht den Saiten der Wohllaut,", "tokens": ["Euch", ",", "o", "freund\u00b7li\u00b7che", "Wel\u00b7len", ",", "ent\u00b7rauscht", "den", "Sai\u00b7ten", "der", "Wohl\u00b7laut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Die ihr purpurnen Scheins lustig den Kahn mir umh\u00fcpft.", "tokens": ["Die", "ihr", "pur\u00b7pur\u00b7nen", "Scheins", "lus\u00b7tig", "den", "Kahn", "mir", "um\u00b7h\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--++-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Noch umwehn mich die D\u00fcfte des fruchtbeladenen Thales,", "tokens": ["Noch", "um\u00b7wehn", "mich", "die", "D\u00fcf\u00b7te", "des", "frucht\u00b7be\u00b7la\u00b7de\u00b7nen", "Tha\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Wo verschwindend Natur Wollust empfindet und weckt.", "tokens": ["Wo", "ver\u00b7schwin\u00b7dend", "Na\u00b7tur", "Wol\u00b7lust", "emp\u00b7fin\u00b7det", "und", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "NN", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und der spiegelnden Fluth entragt der ger\u00f6thete Meerfels,", "tokens": ["Und", "der", "spie\u00b7geln\u00b7den", "Fluth", "en\u00b7tragt", "der", "ge\u00b7r\u00f6\u00b7the\u00b7te", "Meer\u00b7fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Den der Schiffer umf\u00e4hrt, wenn er Panormus erstrebt.", "tokens": ["Den", "der", "Schif\u00b7fer", "um\u00b7f\u00e4hrt", ",", "wenn", "er", "Pa\u00b7nor\u00b7mus", "er\u00b7strebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Lachend rollet der Golf die gl\u00e4nzenden Wogen und ferne", "tokens": ["La\u00b7chend", "rol\u00b7let", "der", "Golf", "die", "gl\u00e4n\u00b7zen\u00b7den", "Wo\u00b7gen", "und", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "KON", "ADJA"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "D\u00e4mmert im Reiche Neptuns Ustica bl\u00e4ulichen Dufts.", "tokens": ["D\u00e4m\u00b7mert", "im", "Rei\u00b7che", "Nep\u00b7tuns", "Us\u00b7ti\u00b7ca", "bl\u00e4u\u00b7li\u00b7chen", "Dufts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NE", "NE", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Und dem felsigen Hang, der niederh\u00e4ngt in die Wasser,", "tokens": ["Und", "dem", "fel\u00b7si\u00b7gen", "Hang", ",", "der", "nie\u00b7der\u00b7h\u00e4ngt", "in", "die", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Rudr' ich entgegen; wie s\u00fc\u00df hier die Erinnerung ist!", "tokens": ["Rudr'", "ich", "ent\u00b7ge\u00b7gen", ";", "wie", "s\u00fc\u00df", "hier", "die", "E\u00b7rin\u00b7ne\u00b7rung", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "ADJD", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Hykkara schwand, es zerst\u00f6rt' es der Grieche; doch immer lebendig", "tokens": ["Hyk\u00b7ka\u00b7ra", "schwand", ",", "es", "zer\u00b7st\u00f6rt'", "es", "der", "Grie\u00b7che", ";", "doch", "im\u00b7mer", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "ADV", "ADJD"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Bleibt dein reizendstes Bild, sch\u00f6nste der Griechinnen, mir.", "tokens": ["Bleibt", "dein", "rei\u00b7zends\u00b7tes", "Bild", ",", "sch\u00f6ns\u00b7te", "der", "Grie\u00b7chin\u00b7nen", ",", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "La\u00efs Heimath zu sein, nicht r\u00fchme sich dessen Carini,", "tokens": ["La\u00efs", "Hei\u00b7math", "zu", "sein", ",", "nicht", "r\u00fch\u00b7me", "sich", "des\u00b7sen", "Ca\u00b7ri\u00b7ni", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VAINF", "$,", "PTKNEG", "VVFIN", "PRF", "PDS", "NE", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Wenn es der Tochter auch ewige Glorie verdankt.", "tokens": ["Wenn", "es", "der", "Toch\u00b7ter", "auch", "e\u00b7wi\u00b7ge", "Glo\u00b7rie", "ver\u00b7dankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Eher glaub' ich, sie stieg vollendet aus goldenen Fluthen,", "tokens": ["E\u00b7her", "glaub'", "ich", ",", "sie", "stieg", "voll\u00b7en\u00b7det", "aus", "gol\u00b7de\u00b7nen", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Um dem entz\u00fcckten Geschlecht sichtbare G\u00f6ttin zu sein.", "tokens": ["Um", "dem", "ent\u00b7z\u00fcck\u00b7ten", "Ge\u00b7schlecht", "sicht\u00b7ba\u00b7re", "G\u00f6t\u00b7tin", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}}}}}