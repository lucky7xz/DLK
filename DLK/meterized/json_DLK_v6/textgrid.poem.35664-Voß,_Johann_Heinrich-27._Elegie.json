{"textgrid.poem.35664": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "27. Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkt mein M\u00e4dchen an mich? Balsamischer duftet der Garten", "tokens": ["Denkt", "mein", "M\u00e4d\u00b7chen", "an", "mich", "?", "Bal\u00b7sa\u00b7mi\u00b7scher", "duf\u00b7tet", "der", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$.", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nach dem Regen, und Glanz tr\u00e4ufelt am gr\u00fcneren Busch.", "tokens": ["Nach", "dem", "Re\u00b7gen", ",", "und", "Glanz", "tr\u00e4u\u00b7felt", "am", "gr\u00fc\u00b7ne\u00b7ren", "Busch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gottes Wolke schaut, nach entlasteter F\u00fclle, geg\u00fcrtet", "tokens": ["Got\u00b7tes", "Wol\u00b7ke", "schaut", ",", "nach", "ent\u00b7las\u00b7te\u00b7ter", "F\u00fcl\u00b7le", ",", "ge\u00b7g\u00fcr\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "VVPP"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Mit dem Bogen der Huld, freundlich zur\u00fcck in das Thal,", "tokens": ["Mit", "dem", "Bo\u00b7gen", "der", "Huld", ",", "freund\u00b7lich", "zu\u00b7r\u00fcck", "in", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wo der gescheuchte Reigen der Kinder aus Weiden hervortanzt,", "tokens": ["Wo", "der", "ge\u00b7scheuch\u00b7te", "Rei\u00b7gen", "der", "Kin\u00b7der", "aus", "Wei\u00b7den", "her\u00b7vor\u00b7tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Und, das Wunder zu sp\u00e4hn, jauchzend den H\u00fcgel erklimmt.", "tokens": ["Und", ",", "das", "Wun\u00b7der", "zu", "sp\u00e4hn", ",", "jauch\u00b7zend", "den", "H\u00fc\u00b7gel", "er\u00b7klimmt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Aber ich hefte den Blick auf die sch\u00e4umenden Wogen der Schleuse,", "tokens": ["A\u00b7ber", "ich", "hef\u00b7te", "den", "Blick", "auf", "die", "sch\u00e4u\u00b7men\u00b7den", "Wo\u00b7gen", "der", "Schleu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Gleite sanft, wie im Traum, gegen die rei\u00dfende Flut,", "tokens": ["Glei\u00b7te", "sanft", ",", "wie", "im", "Traum", ",", "ge\u00b7gen", "die", "rei\u00b7\u00dfen\u00b7de", "Flut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und mein horchendes Ohr h\u00f6rt leise, wie M\u00e4dchenstimmen,", "tokens": ["Und", "mein", "hor\u00b7chen\u00b7des", "Ohr", "h\u00f6rt", "lei\u00b7se", ",", "wie", "M\u00e4d\u00b7chen\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und ein banges Ach, tief in des Stromes Ger\u00e4usch.", "tokens": ["Und", "ein", "ban\u00b7ges", "Ach", ",", "tief", "in", "des", "Stro\u00b7mes", "Ge\u00b7r\u00e4usch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Denkt mein M\u00e4dchen an mich, und schwebt mit dem lieben Gedanken,", "tokens": ["Denkt", "mein", "M\u00e4d\u00b7chen", "an", "mich", ",", "und", "schwebt", "mit", "dem", "lie\u00b7ben", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "In Ged\u00fcften verh\u00fcllt, etwa ihr Engel um mich?", "tokens": ["In", "Ge\u00b7d\u00fcf\u00b7ten", "ver\u00b7h\u00fcllt", ",", "et\u00b7wa", "ihr", "En\u00b7gel", "um", "mich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "O so beschw\u00f6r' ich dich bei des M\u00e4dchens reinster Empfindung,", "tokens": ["O", "so", "be\u00b7schw\u00f6r'", "ich", "dich", "bei", "des", "M\u00e4d\u00b7chens", "reins\u00b7ter", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Die ihr mit Engelwonn' Augen und Wangen verkl\u00e4rt,", "tokens": ["Die", "ihr", "mit", "En\u00b7gel\u00b7wonn'", "Au\u00b7gen", "und", "Wan\u00b7gen", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ihrer heiligsten Thr\u00e4ne, die je in goldener Schale", "tokens": ["Ih\u00b7rer", "hei\u00b7ligs\u00b7ten", "Thr\u00e4\u00b7ne", ",", "die", "je", "in", "gol\u00b7de\u00b7ner", "Scha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Zu Jehovens Altar, freudiger Eile, du trugst:", "tokens": ["Zu", "Je\u00b7ho\u00b7vens", "Al\u00b7tar", ",", "freu\u00b7di\u00b7ger", "Ei\u00b7le", ",", "du", "trugst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Zeig mir die holde Gestalt der Auserw\u00e4hlten, die jetzo,", "tokens": ["Zeig", "mir", "die", "hol\u00b7de", "Ge\u00b7stalt", "der", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ",", "die", "jet\u00b7zo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.18": {"text": "Fern, voll tr\u00fcberes Grams, ihren Geliebten beweint!", "tokens": ["Fern", ",", "voll", "tr\u00fc\u00b7be\u00b7res", "Grams", ",", "ih\u00b7ren", "Ge\u00b7lieb\u00b7ten", "be\u00b7weint", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Irrt sie im bunten Thale, von frohen Gespielen gen\u00f6tigt,", "tokens": ["Irrt", "sie", "im", "bun\u00b7ten", "Tha\u00b7le", ",", "von", "fro\u00b7hen", "Ge\u00b7spie\u00b7len", "ge\u00b7n\u00f6\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Stumm, den gr\u00fcnen Hut \u00fcber die Augen gesenkt?", "tokens": ["Stumm", ",", "den", "gr\u00fc\u00b7nen", "Hut", "\u00fc\u00b7ber", "die", "Au\u00b7gen", "ge\u00b7senkt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "Pfl\u00fcckt ohn' Absicht Blumen, und springt itzt behende zur Bl\u00fcte", "tokens": ["Pfl\u00fcckt", "ohn'", "Ab\u00b7sicht", "Blu\u00b7men", ",", "und", "springt", "itzt", "be\u00b7hen\u00b7de", "zur", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN", "$,", "KON", "VVFIN", "ADV", "ADJA", "APPRART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Jenes Hollunders, der einst unsere K\u00fcsse verbarg;", "tokens": ["Je\u00b7nes", "Hol\u00b7lun\u00b7ders", ",", "der", "einst", "un\u00b7se\u00b7re", "K\u00fcs\u00b7se", "ver\u00b7barg", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Oder zum Quell, den in hohler Hand sie bei Mondenschimmer", "tokens": ["O\u00b7der", "zum", "Quell", ",", "den", "in", "hoh\u00b7ler", "Hand", "sie", "bei", "Mon\u00b7den\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "PPER", "APPR", "NN"], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Einst zu trinken mir bot? Spielet sie z\u00f6gernd im Quell,", "tokens": ["Einst", "zu", "trin\u00b7ken", "mir", "bot", "?", "Spie\u00b7let", "sie", "z\u00f6\u00b7gernd", "im", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Unachtsam des Get\u00e4ndels um sie; und lispelt vergebens", "tokens": ["U\u00b7nacht\u00b7sam", "des", "Ge\u00b7t\u00e4n\u00b7dels", "um", "sie", ";", "und", "lis\u00b7pelt", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "APPR", "PPER", "$.", "KON", "VVFIN", "ADV"], "meter": "+----+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Ihr die Freundin ins Ohr: M\u00e4dchen, du bist ja so still?", "tokens": ["Ihr", "die", "Freun\u00b7din", "ins", "Ohr", ":", "M\u00e4d\u00b7chen", ",", "du", "bist", "ja", "so", "still", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$.", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Oder sitzt sie einsam im gr\u00fcnen Dunkel der Laube,", "tokens": ["O\u00b7der", "sitzt", "sie", "ein\u00b7sam", "im", "gr\u00fc\u00b7nen", "Dun\u00b7kel", "der", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Auf der Stelle, wo einst mir an dem Herzen sie lag?...", "tokens": ["Auf", "der", "Stel\u00b7le", ",", "wo", "einst", "mir", "an", "dem", "Her\u00b7zen", "sie", "lag", "?", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Die ihr heimlich umher von der Sch\u00f6ne des M\u00e4dchens fl\u00fcstert,", "tokens": ["Die", "ihr", "heim\u00b7lich", "um\u00b7her", "von", "der", "Sch\u00f6\u00b7ne", "des", "M\u00e4d\u00b7chens", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.30": {"text": "Weht mir den Rosenbusch, freundliche Weste zur\u00fcck;", "tokens": ["Weht", "mir", "den", "Ro\u00b7sen\u00b7busch", ",", "freund\u00b7li\u00b7che", "Wes\u00b7te", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.31": {"text": "Da\u00df ich das Antlitz schaue der Herrlichen, und in der Sch\u00f6nheit", "tokens": ["Da\u00df", "ich", "das", "Ant\u00b7litz", "schau\u00b7e", "der", "Herr\u00b7li\u00b7chen", ",", "und", "in", "der", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Strahlenmeer sich hinab st\u00fcrze mein schauernder Geist!", "tokens": ["Strah\u00b7len\u00b7meer", "sich", "hin\u00b7ab", "st\u00fcr\u00b7ze", "mein", "schau\u00b7ern\u00b7der", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Ach! sie traurt, die Sch\u00f6nste der M\u00e4dchen, und lehnet die Stirne,", "tokens": ["Ach", "!", "sie", "traurt", ",", "die", "Sch\u00f6ns\u00b7te", "der", "M\u00e4d\u00b7chen", ",", "und", "leh\u00b7net", "die", "Stir\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Hingesenkt auf die Hand, an den gebogenen Ast!", "tokens": ["Hin\u00b7ge\u00b7senkt", "auf", "die", "Hand", ",", "an", "den", "ge\u00b7bo\u00b7ge\u00b7nen", "Ast", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Thr\u00e4nen netzen die Hand und die gl\u00fchende Wange; sie seufzet,", "tokens": ["Thr\u00e4\u00b7nen", "net\u00b7zen", "die", "Hand", "und", "die", "gl\u00fc\u00b7hen\u00b7de", "Wan\u00b7ge", ";", "sie", "seuf\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Nennt mich bei Namen, und schwer zittert ihr Busen empor!", "tokens": ["Nennt", "mich", "bei", "Na\u00b7men", ",", "und", "schwer", "zit\u00b7tert", "ihr", "Bu\u00b7sen", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Selma, Selma, weine nicht so! Du weinest um mich zwar;", "tokens": ["Sel\u00b7ma", ",", "Sel\u00b7ma", ",", "wei\u00b7ne", "nicht", "so", "!", "Du", "wei\u00b7nest", "um", "mich", "zwar", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Aber es bricht mir das Herz, Beste, dich weinen zu sehn!", "tokens": ["A\u00b7ber", "es", "bricht", "mir", "das", "Herz", ",", "Bes\u00b7te", ",", "dich", "wei\u00b7nen", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "PRF", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Der im edenischen Myrtenged\u00fcft einst unsere Seelen,", "tokens": ["Der", "im", "e\u00b7de\u00b7ni\u00b7schen", "Myr\u00b7ten\u00b7ge\u00b7d\u00fcft", "einst", "un\u00b7se\u00b7re", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "--+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Ach so \u00e4hnlich! erschuf, und uns mit segnendem Hauch", "tokens": ["Ach", "so", "\u00e4hn\u00b7lich", "!", "er\u00b7schuf", ",", "und", "uns", "mit", "seg\u00b7nen\u00b7dem", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ADJD", "$.", "VVFIN", "$,", "KON", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Ein zur Liebe weiht', und den z\u00e4rtlichsten Engeln vertraute,", "tokens": ["Ein", "zur", "Lie\u00b7be", "weiht'", ",", "und", "den", "z\u00e4rt\u00b7lichs\u00b7ten", "En\u00b7geln", "ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.42": {"text": "Dann so wunderbar hier beide vereinigte, Gott,", "tokens": ["Dann", "so", "wun\u00b7der\u00b7bar", "hier", "bei\u00b7de", "ver\u00b7ei\u00b7nig\u00b7te", ",", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "PIS", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.43": {"text": "Unser Vater, beschied, nicht z\u00fcrnend, uns Z\u00e4hren der Trennung;", "tokens": ["Un\u00b7ser", "Va\u00b7ter", ",", "be\u00b7schied", ",", "nicht", "z\u00fcr\u00b7nend", ",", "uns", "Z\u00e4h\u00b7ren", "der", "Tren\u00b7nung", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "PTKNEG", "VVPP", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.44": {"text": "Bald vereiniget uns wieder ein ewiger Bund!...", "tokens": ["Bald", "ver\u00b7ei\u00b7ni\u00b7get", "uns", "wie\u00b7der", "ein", "e\u00b7wi\u00b7ger", "Bund", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Still! sie atmet leis'; auf die m\u00fcdegeweinten Wimper", "tokens": ["Still", "!", "sie", "at\u00b7met", "leis'", ";", "auf", "die", "m\u00fc\u00b7de\u00b7ge\u00b7wein\u00b7ten", "Wim\u00b7per"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "PPER", "VVFIN", "ADJD", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.46": {"text": "Gie\u00dft mein Genius ihr duftigen Schlummer herab,", "tokens": ["Gie\u00dft", "mein", "Ge\u00b7nius", "ihr", "duf\u00b7ti\u00b7gen", "Schlum\u00b7mer", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.47": {"text": "Und umleuchtet ihr Haupt mit Tr\u00e4umen vom winkenden Brautkranz,", "tokens": ["Und", "um\u00b7leuch\u00b7tet", "ihr", "Haupt", "mit", "Tr\u00e4u\u00b7men", "vom", "win\u00b7ken\u00b7den", "Braut\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.48": {"text": "Und dem Reigengesang hoffender Br\u00e4ute der Flur.", "tokens": ["Und", "dem", "Rei\u00b7gen\u00b7ge\u00b7sang", "hof\u00b7fen\u00b7der", "Br\u00e4u\u00b7te", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.49": {"text": "Atemlos horcht sie dem Lied' und (o sie f\u00fchlt, da\u00df ich weine!)", "tokens": ["A\u00b7tem\u00b7los", "horcht", "sie", "dem", "Lied'", "und", "(", "o", "sie", "f\u00fchlt", ",", "da\u00df", "ich", "wei\u00b7ne", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "KON", "$(", "FM", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.50": {"text": "Meinem vor Lieb' und Lust trunkenen stammelnden Laut.", "tokens": ["Mei\u00b7nem", "vor", "Lieb'", "und", "Lust", "trun\u00b7ke\u00b7nen", "stam\u00b7meln\u00b7den", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.51": {"text": "Seht! sie bebt, und wie Abendrot auf tr\u00e4ufelnden Rosen,", "tokens": ["Seht", "!", "sie", "bebt", ",", "und", "wie", "A\u00b7ben\u00b7drot", "auf", "tr\u00e4u\u00b7feln\u00b7den", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "$,", "KON", "PWAV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.52": {"text": "Schimmert ein L\u00e4cheln sanft \u00fcber ihr nasses Gesicht.", "tokens": ["Schim\u00b7mert", "ein", "L\u00e4\u00b7cheln", "sanft", "\u00fc\u00b7ber", "ihr", "nas\u00b7ses", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}}}}