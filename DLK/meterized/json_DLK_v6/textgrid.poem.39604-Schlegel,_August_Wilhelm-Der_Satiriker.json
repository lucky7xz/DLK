{"textgrid.poem.39604": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Satiriker", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmt euch in Acht, ihr Freunde des Scherzes und der Satire!", "tokens": ["Nehmt", "euch", "in", "Acht", ",", "ihr", "Freun\u00b7de", "des", "Scher\u00b7zes", "und", "der", "Sa\u00b7ti\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "$,", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+--+-+-+--", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dort die ehrliche Haut ist ein durchtriebener Schalk.", "tokens": ["Dort", "die", "ehr\u00b7li\u00b7che", "Haut", "ist", "ein", "durch\u00b7trie\u00b7be\u00b7ner", "Schalk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Satirisiert er euch nicht die Satir' in seinen Satiren,", "tokens": ["Sa\u00b7ti\u00b7ri\u00b7siert", "er", "euch", "nicht", "die", "Sa\u00b7tir'", "in", "sei\u00b7nen", "Sa\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-+--", "measure": "iambic.septa.invert"}, "line.4": {"text": "Da\u00df noch keiner so weit je sie herunter gebracht?", "tokens": ["Da\u00df", "noch", "kei\u00b7ner", "so", "weit", "je", "sie", "her\u00b7un\u00b7ter", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADV", "ADJD", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}