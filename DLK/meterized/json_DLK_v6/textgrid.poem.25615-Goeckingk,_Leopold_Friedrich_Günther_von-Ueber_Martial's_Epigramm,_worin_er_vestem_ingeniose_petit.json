{"textgrid.poem.25615": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Ueber Martial's Epigramm, worin er vestem ingeniose petit", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Martial vertraut selbst mit dem Kaiser spricht,", "tokens": ["Wenn", "Mar\u00b7ti\u00b7al", "ver\u00b7traut", "selbst", "mit", "dem", "Kai\u00b7ser", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dreisten Spott auf Hoh' und Niedre sch\u00fcttet,", "tokens": ["Und", "dreis\u00b7ten", "Spott", "auf", "Hoh'", "und", "Nied\u00b7re", "sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NE", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6r' ich den R\u00f6mer. Ist es aber nicht,", "tokens": ["H\u00f6r'", "ich", "den", "R\u00f6\u00b7mer", ".", "Ist", "es", "a\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er um Kleidung seinen G\u00f6nner bittet,", "tokens": ["Wenn", "er", "um", "Klei\u00b7dung", "sei\u00b7nen", "G\u00f6n\u00b7ner", "bit\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als wenn ein Franzmann spricht?", "tokens": ["Als", "wenn", "ein", "Franz\u00b7mann", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}