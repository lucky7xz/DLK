{"textgrid.poem.40785": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Glocken hell des Morgens heut zu Weissenburg get\u00f6nt!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Glocken hell des Morgens heut zu Weissenburg get\u00f6nt!", "tokens": ["Wie", "die", "Glo\u00b7cken", "hell", "des", "Mor\u00b7gens", "heut", "zu", "Weis\u00b7sen\u00b7burg", "ge\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "ART", "ADV", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jetzt ist's wieder still geworden, und der K\u00f6nig ist gekr\u00f6nt! \u2013", "tokens": ["Jetzt", "ist's", "wie\u00b7der", "still", "ge\u00b7wor\u00b7den", ",", "und", "der", "K\u00f6\u00b7nig", "ist", "ge\u00b7kr\u00f6nt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "VAPP", "$,", "KON", "ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sieh, nun tritt er aus dem Dome, purpurstrahlend, glanzverkl\u00e4rt,", "tokens": ["Sieh", ",", "nun", "tritt", "er", "aus", "dem", "Do\u00b7me", ",", "pur\u00b7pur\u00b7strah\u00b7lend", ",", "glanz\u00b7ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auf dem Haupt die neue Krone, in der Hand das blanke Schwert.", "tokens": ["Auf", "dem", "Haupt", "die", "neu\u00b7e", "Kro\u00b7ne", ",", "in", "der", "Hand", "das", "blan\u00b7ke", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Englein schmiedeten die Krone, wie die fromme Sage spricht,", "tokens": ["En\u00b7glein", "schmie\u00b7de\u00b7ten", "die", "Kro\u00b7ne", ",", "wie", "die", "from\u00b7me", "Sa\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aus Demanten sonnenhelle, aus Rubinen morgenlicht!", "tokens": ["Aus", "De\u00b7man\u00b7ten", "son\u00b7nen\u00b7hel\u00b7le", ",", "aus", "Ru\u00b7bi\u00b7nen", "mor\u00b7gen\u00b7licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch ein derber Schmied zu Dobschan lie\u00df ergl\u00fch'n am Flammenherd,", "tokens": ["Doch", "ein", "der\u00b7ber", "Schmied", "zu", "Dob\u00b7schan", "lie\u00df", "er\u00b7gl\u00fch'n", "am", "Flam\u00b7men\u00b7herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NE", "VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Schlug mit H\u00e4mmern auf dem Ambo\u00df das gewalt'ge scharfe Schwert.", "tokens": ["Schlug", "mit", "H\u00e4m\u00b7mern", "auf", "dem", "Am\u00b7bo\u00df", "das", "ge\u00b7walt'\u00b7ge", "schar\u00b7fe", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Vor dem Stadtthor ragt ein H\u00fcgel, dessen Pfade Teppich schm\u00fcckt,", "tokens": ["Vor", "dem", "Stadt\u00b7thor", "ragt", "ein", "H\u00fc\u00b7gel", ",", "des\u00b7sen", "Pfa\u00b7de", "Tep\u00b7pich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELAT", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Drein des Landes helle Farben, roth und wei\u00df und gr\u00fcn, gestickt;", "tokens": ["Drein", "des", "Lan\u00b7des", "hel\u00b7le", "Far\u00b7ben", ",", "roth", "und", "wei\u00df", "und", "gr\u00fcn", ",", "ge\u00b7stickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "KON", "ADJD", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Unten harrt der greise Kanzler, h\u00e4lt empor mit stolzem Muth", "tokens": ["Un\u00b7ten", "harrt", "der", "grei\u00b7se", "Kanz\u00b7ler", ",", "h\u00e4lt", "em\u00b7por", "mit", "stol\u00b7zem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Hoch das sammtne Purpurkissen, drauf des Landes Satzung ruht.", "tokens": ["Hoch", "das", "sammt\u00b7ne", "Pur\u00b7pur\u00b7kis\u00b7sen", ",", "drauf", "des", "Lan\u00b7des", "Sat\u00b7zung", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "PAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Rings geschaart in weitem Kreise Ungarns edle V\u00f6lkerkraft!", "tokens": ["Rings", "ge\u00b7schaart", "in", "wei\u00b7tem", "Krei\u00b7se", "Un\u00b7garns", "ed\u00b7le", "V\u00f6l\u00b7ker\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hohe b\u00e4rtige Magnaten mit dem Kern der Ritterschaft,", "tokens": ["Ho\u00b7he", "b\u00e4r\u00b7ti\u00b7ge", "Mag\u00b7na\u00b7ten", "mit", "dem", "Kern", "der", "Rit\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aebt' und Bisch\u00f6f' in den Infuln mit dem Krummstab und Brevier,", "tokens": ["A\u00b7ebt'", "und", "Bi\u00b7sch\u00f6f'", "in", "den", "In\u00b7fuln", "mit", "dem", "Krumm\u00b7stab", "und", "Bre\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und des Reiches Bannertr\u00e4ger mit dem flatternden Panier!", "tokens": ["Und", "des", "Rei\u00b7ches", "Ban\u00b7ner\u00b7tr\u00e4\u00b7ger", "mit", "dem", "flat\u00b7tern\u00b7den", "Pa\u00b7nier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Auf den H\u00fcgel sprengt der K\u00f6nig, jung und bl\u00fchend, hoch zu Pferd,", "tokens": ["Auf", "den", "H\u00fc\u00b7gel", "sprengt", "der", "K\u00f6\u00b7nig", ",", "jung", "und", "bl\u00fc\u00b7hend", ",", "hoch", "zu", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVPP", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nord- und s\u00fcdw\u00e4rts, west- und ostw\u00e4rts, schwingt er flink sein blankes Schwert;", "tokens": ["Nord", "und", "s\u00fcd\u00b7w\u00e4rts", ",", "west", "und", "ost\u00b7w\u00e4rts", ",", "schwingt", "er", "flink", "sein", "blan\u00b7kes", "Schwert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADV", "$,", "TRUNC", "KON", "ADV", "$,", "VVFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dann gleichwie ein goldnes Standbild, steht er ruhig festgebannt,", "tokens": ["Dann", "gleich\u00b7wie", "ein", "gold\u00b7nes", "Stand\u00b7bild", ",", "steht", "er", "ru\u00b7hig", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und empor zum blauen Himmel hebt er feierlich die Hand:", "tokens": ["Und", "em\u00b7por", "zum", "blau\u00b7en", "Him\u00b7mel", "hebt", "er", "fei\u00b7er\u00b7lich", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "\u00bbsei gegr\u00fc\u00dft, mein Volk, und h\u00f6re! Nimm aus meines Kanzlers Hand", "tokens": ["\u00bb", "sei", "ge\u00b7gr\u00fc\u00dft", ",", "mein", "Volk", ",", "und", "h\u00f6\u00b7re", "!", "Nimm", "aus", "mei\u00b7nes", "Kanz\u00b7lers", "Hand"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "NE", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die Geschenke deines K\u00f6nigs, meiner Liebe erstes Pfand!", "tokens": ["Die", "Ge\u00b7schen\u00b7ke", "dei\u00b7nes", "K\u00f6\u00b7nigs", ",", "mei\u00b7ner", "Lie\u00b7be", "ers\u00b7tes", "Pfand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Freien Willens, freien Herzens geb' ich Freiheit dir und Recht,", "tokens": ["Frei\u00b7en", "Wil\u00b7lens", ",", "frei\u00b7en", "Her\u00b7zens", "geb'", "ich", "Frei\u00b7heit", "dir", "und", "Recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "PPER", "NN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dem ich mich der erste beuge huldigend als treuer Knecht!", "tokens": ["Dem", "ich", "mich", "der", "ers\u00b7te", "beu\u00b7ge", "hul\u00b7di\u00b7gend", "als", "treu\u00b7er", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ART", "ADJA", "VVFIN", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Ich beschw\u00f6r's beim ew'gen Himmel, der im Sturm selbst Segen spr\u00fcht,", "tokens": ["Ich", "be\u00b7schw\u00f6r's", "beim", "ew'\u00b7gen", "Him\u00b7mel", ",", "der", "im", "Sturm", "selbst", "Se\u00b7gen", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ich beschw\u00f6r's beim eignen Herzen, das im Zorn selbst Liebe gl\u00fcht,", "tokens": ["Ich", "be\u00b7schw\u00f6r's", "beim", "eig\u00b7nen", "Her\u00b7zen", ",", "das", "im", "Zorn", "selbst", "Lie\u00b7be", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nicht zu herrschen blind nach Willk\u00fcr, nein, nach Recht und Satzung stets!", "tokens": ["Nicht", "zu", "herr\u00b7schen", "blind", "nach", "Will\u00b7k\u00fcr", ",", "nein", ",", "nach", "Recht", "und", "Sat\u00b7zung", "stets", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "ADJD", "APPR", "NE", "$,", "PTKANT", "$,", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "F\u00fcrsten sind nicht immer weise, nie ein Thor ist das Gesetz.", "tokens": ["F\u00fcrs\u00b7ten", "sind", "nicht", "im\u00b7mer", "wei\u00b7se", ",", "nie", "ein", "Thor", "ist", "das", "Ge\u00b7setz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "VVFIN", "$,", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Und, beim Himmel, aufrecht halten will ich's heilig, fest und treu,", "tokens": ["Und", ",", "beim", "Him\u00b7mel", ",", "auf\u00b7recht", "hal\u00b7ten", "will", "ich's", "hei\u00b7lig", ",", "fest", "und", "treu", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "$,", "ADJD", "VVINF", "VMFIN", "PIS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nie nach eignem Hirn es deuteln, nach Gel\u00fcst es modeln neu!", "tokens": ["Nie", "nach", "eig\u00b7nem", "Hirn", "es", "deu\u00b7teln", ",", "nach", "Ge\u00b7l\u00fcst", "es", "mo\u00b7deln", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PPER", "VVINF", "$,", "APPR", "NN", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Will auch nicht in seiner Klammer halten mehr ein einzler Stein,", "tokens": ["Will", "auch", "nicht", "in", "sei\u00b7ner", "Klam\u00b7mer", "hal\u00b7ten", "mehr", "ein", "einz\u00b7ler", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Falle drob doch nicht das ganze wohlgefugte Bauwerk ein!", "tokens": ["Fal\u00b7le", "drob", "doch", "nicht", "das", "gan\u00b7ze", "wohl\u00b7ge\u00b7fug\u00b7te", "Bau\u00b7werk", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Wend' es Gott, da\u00df je ich f\u00fchre in den Kampf f\u00fcrs Unrecht euch,", "tokens": ["Wend'", "es", "Gott", ",", "da\u00df", "je", "ich", "f\u00fch\u00b7re", "in", "den", "Kampf", "f\u00fcrs", "Un\u00b7recht", "euch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "KOUS", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df dem Schild des Bruderm\u00f6rders meines Volks Geschichte gleich,", "tokens": ["Da\u00df", "dem", "Schild", "des", "Bru\u00b7der\u00b7m\u00f6r\u00b7ders", "mei\u00b7nes", "Volks", "Ge\u00b7schich\u00b7te", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Drauf, so prunkvoll auch das Wappen, grausenhaft ein Blutfleck spricht!", "tokens": ["Drauf", ",", "so", "prunk\u00b7voll", "auch", "das", "Wap\u00b7pen", ",", "grau\u00b7sen\u00b7haft", "ein", "Blut\u00b7fleck", "spricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "ADJD", "ADV", "ART", "NN", "$,", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Keine Thr\u00e4ne, keine Quelle w\u00e4scht ihn wieder rein und licht!", "tokens": ["Kei\u00b7ne", "Thr\u00e4\u00b7ne", ",", "kei\u00b7ne", "Quel\u00b7le", "w\u00e4scht", "ihn", "wie\u00b7der", "rein", "und", "licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Ich beschw\u00f6r' es, zu bewahren gl\u00e4nzend meines Landes Ruhm,", "tokens": ["Ich", "be\u00b7schw\u00f6r'", "es", ",", "zu", "be\u00b7wah\u00b7ren", "gl\u00e4n\u00b7zend", "mei\u00b7nes", "Lan\u00b7des", "Ruhm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Blank wie Krieger ihren Panzer, sorgsam, wie ein Heiligthum!", "tokens": ["Blank", "wie", "Krie\u00b7ger", "ih\u00b7ren", "Pan\u00b7zer", ",", "sorg\u00b7sam", ",", "wie", "ein", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "PPOSAT", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Einem garbenreichen Saatfeld ist des Volkes Gl\u00fcck wohl gleich,", "tokens": ["Ei\u00b7nem", "gar\u00b7ben\u00b7rei\u00b7chen", "Saat\u00b7feld", "ist", "des", "Vol\u00b7kes", "Gl\u00fcck", "wohl", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch sein Ruhm dem Aetherdome, glanzerf\u00fcllt und sternenreich!", "tokens": ["Doch", "sein", "Ruhm", "dem", "A\u00b7e\u00b7ther\u00b7do\u00b7me", ",", "glan\u00b7zer\u00b7f\u00fcllt", "und", "ster\u00b7nen\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Ich beschw\u00f6r's, zu treuem Rathe gern mein Ohr und Herz zu leih'n,", "tokens": ["Ich", "be\u00b7schw\u00f6r's", ",", "zu", "treu\u00b7em", "Ra\u00b7the", "gern", "mein", "Ohr", "und", "Herz", "zu", "leih'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nie des Freien Wort zu fesseln, sei er noch so schwach und klein!", "tokens": ["Nie", "des", "Frei\u00b7en", "Wort", "zu", "fes\u00b7seln", ",", "sei", "er", "noch", "so", "schwach", "und", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nicht in reichen F\u00fcrsteng\u00e4rten, wo ihr sie zu finden hofft,", "tokens": ["Nicht", "in", "rei\u00b7chen", "F\u00fcrs\u00b7ten\u00b7g\u00e4r\u00b7ten", ",", "wo", "ihr", "sie", "zu", "fin\u00b7den", "hofft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auf verla\u00dfner, stiller Haide bl\u00fcht die sch\u00f6nste Rose oft.", "tokens": ["Auf", "ver\u00b7la\u00df\u00b7ner", ",", "stil\u00b7ler", "Hai\u00b7de", "bl\u00fcht", "die", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "oft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Ich beschw\u00f6r's, mit eurem Gute hauszuhalten karg und weis',", "tokens": ["Ich", "be\u00b7schw\u00f6r's", ",", "mit", "eu\u00b7rem", "Gu\u00b7te", "haus\u00b7zu\u00b7hal\u00b7ten", "karg", "und", "weis'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dran der Wittwe Thr\u00e4nen kleben, und des armen Landmanns Schwei\u00df!", "tokens": ["Dran", "der", "Witt\u00b7we", "Thr\u00e4\u00b7nen", "kle\u00b7ben", ",", "und", "des", "ar\u00b7men", "Land\u00b7manns", "Schwei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVINF", "$,", "KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie doch k\u00f6nnte jenem munden noch sein s\u00fc\u00dfer goldner Wein,", "tokens": ["Wie", "doch", "k\u00f6nn\u00b7te", "je\u00b7nem", "mun\u00b7den", "noch", "sein", "s\u00fc\u00b7\u00dfer", "gold\u00b7ner", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PDAT", "ADJA", "ADV", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der die sch\u00f6nste seiner Perlen in den Becher warf hinein?", "tokens": ["Der", "die", "sch\u00f6ns\u00b7te", "sei\u00b7ner", "Per\u00b7len", "in", "den", "Be\u00b7cher", "warf", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Ich beschw\u00f6r's, zu sein ein Vater meinem Volke immerdar!", "tokens": ["Ich", "be\u00b7schw\u00f6r's", ",", "zu", "sein", "ein", "Va\u00b7ter", "mei\u00b7nem", "Vol\u00b7ke", "im\u00b7mer\u00b7dar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ART", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Haltet nicht die\u00df Herz zu enge f\u00fcr die gro\u00dfe Kinderschaar!", "tokens": ["Hal\u00b7tet", "nicht", "die\u00df", "Herz", "zu", "en\u00b7ge", "f\u00fcr", "die", "gro\u00b7\u00dfe", "Kin\u00b7der\u00b7schaar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PDS", "NN", "APPR", "ADJA", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Vaterherz ist doch an Liebe doppelt gro\u00df und reich und warm,", "tokens": ["Va\u00b7ter\u00b7herz", "ist", "doch", "an", "Lie\u00b7be", "dop\u00b7pelt", "gro\u00df", "und", "reich", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "ADJD", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Zu umschlingen und zu schirmen reicht um all' ein Vaterarm!\u00ab", "tokens": ["Zu", "um\u00b7schlin\u00b7gen", "und", "zu", "schir\u00b7men", "reicht", "um", "all'", "ein", "Va\u00b7te\u00b7rarm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "VVFIN", "APPR", "PIS", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.14": {"line.1": {"text": "L\u00e4ngst verweht sind schon die L\u00fcfte, die der K\u00f6nigseid durchhallt,", "tokens": ["L\u00e4ngst", "ver\u00b7weht", "sind", "schon", "die", "L\u00fcf\u00b7te", ",", "die", "der", "K\u00f6\u00b7nig\u00b7seid", "durch\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ueber jene gr\u00fcnen Fluren sind Jahrhunderte gewallt,", "tokens": ["Ue\u00b7ber", "je\u00b7ne", "gr\u00fc\u00b7nen", "Flu\u00b7ren", "sind", "Jahr\u00b7hun\u00b7der\u00b7te", "ge\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Jenes Bollwerk von Vasallen, rings als Riesenwand erh\u00f6ht,", "tokens": ["Je\u00b7nes", "Boll\u00b7werk", "von", "Va\u00b7sal\u00b7len", ",", "rings", "als", "Rie\u00b7sen\u00b7wand", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "$,", "ADV", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ist in Asch' und Staub zerfallen und in alle Wind' ges\u00e4t!", "tokens": ["Ist", "in", "Asch'", "und", "Staub", "zer\u00b7fal\u00b7len", "und", "in", "al\u00b7le", "Wind'", "ge\u00b7s\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "KON", "NN", "VVINF", "KON", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.15": {"line.1": {"text": "Doch es wahrt die Burg zu Ofen Stephans Mantel, Kron' und Schwert,", "tokens": ["Doch", "es", "wahrt", "die", "Burg", "zu", "O\u00b7fen", "Ste\u00b7phans", "Man\u00b7tel", ",", "Kron'", "und", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NE", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "W\u00e4chter blank in Waffen, schirmen jener Sch\u00e4tze theuren Werth;", "tokens": ["W\u00e4ch\u00b7ter", "blank", "in", "Waf\u00b7fen", ",", "schir\u00b7men", "je\u00b7ner", "Sch\u00e4t\u00b7ze", "theu\u00b7ren", "Werth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,", "VVFIN", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn sie einen K\u00f6nig kr\u00f6nen, wird er damit angethan.", "tokens": ["Wenn", "sie", "ei\u00b7nen", "K\u00f6\u00b7nig", "kr\u00f6\u00b7nen", ",", "wird", "er", "da\u00b7mit", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ach, da\u00df man doch Stephans Geiste keine W\u00e4chter stellen kann!", "tokens": ["Ach", ",", "da\u00df", "man", "doch", "Ste\u00b7phans", "Geis\u00b7te", "kei\u00b7ne", "W\u00e4ch\u00b7ter", "stel\u00b7len", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PIS", "ADV", "ADJA", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.16": {"line.1": {"text": "Sieht das Volk dann Stephans Mantel, w\u00fcnscht es auch sein Herz hinein!", "tokens": ["Sieht", "das", "Volk", "dann", "Ste\u00b7phans", "Man\u00b7tel", ",", "w\u00fcnscht", "es", "auch", "sein", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sieht sein Schwert es wieder schwingen, \u2013 m\u00f6cht' es doch sein Arm auch sein!", "tokens": ["Sieht", "sein", "Schwert", "es", "wie\u00b7der", "schwin\u00b7gen", ",", "\u2013", "m\u00f6cht'", "es", "doch", "sein", "Arm", "auch", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "