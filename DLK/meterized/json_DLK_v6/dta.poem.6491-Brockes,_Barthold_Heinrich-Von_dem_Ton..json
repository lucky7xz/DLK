{"dta.poem.6491": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die erst bewegte Lufft giebt ihnen Macht zu dr\u00fccken", "tokens": ["Die", "erst", "be\u00b7weg\u00b7te", "Lufft", "giebt", "ih\u00b7nen", "Macht", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "PPER", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die C\u00f6rper, so da um sie seyn;", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "so", "da", "um", "sie", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die W\u00fcrckung aber ist verschiedlich, insgemein.", "tokens": ["Die", "W\u00fcr\u00b7ckung", "a\u00b7ber", "ist", "ver\u00b7schied\u00b7lich", ",", "ins\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Trummel-Schl\u00e4g\u2019 erzittern Fenster-Scheiben,", "tokens": ["Durch", "Trum\u00b7mel\u00b7Schl\u00e4g'", "er\u00b7zit\u00b7tern", "Fens\u00b7ter\u00b7Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die auf gewisse Weit\u2019 entfernet bleiben;", "tokens": ["Die", "auf", "ge\u00b7wis\u00b7se", "Weit'", "ent\u00b7fer\u00b7net", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da sie ein st\u00e4rckrer Ton im mindsten nicht bewegt.", "tokens": ["Da", "sie", "ein", "st\u00e4r\u00b7ck\u00b7rer", "Ton", "im", "minds\u00b7ten", "nicht", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Durch einen Bogen-Strich wird eine Sait\u2019 erregt:", "tokens": ["Durch", "ei\u00b7nen", "Bo\u00b7gen\u00b7Strich", "wird", "ei\u00b7ne", "Sait'", "er\u00b7regt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wodurch man alsobald versp\u00fcrt,", "tokens": ["Wo\u00b7durch", "man", "al\u00b7so\u00b7bald", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df eine andere sich gleichfalls r\u00fchrt.", "tokens": ["Da\u00df", "ei\u00b7ne", "an\u00b7de\u00b7re", "sich", "gleich\u00b7falls", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die denn, wofern man sie in selben Ton gestimmt,", "tokens": ["Die", "denn", ",", "wo\u00b7fern", "man", "sie", "in", "sel\u00b7ben", "Ton", "ge\u00b7stimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PIS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erbebt und unber\u00fchrt auch gleichen Ton annimmt.", "tokens": ["Er\u00b7bebt", "und", "un\u00b7be\u00b7r\u00fchrt", "auch", "glei\u00b7chen", "Ton", "an\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}