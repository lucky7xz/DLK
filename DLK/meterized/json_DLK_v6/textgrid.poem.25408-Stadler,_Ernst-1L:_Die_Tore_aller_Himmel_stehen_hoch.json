{"textgrid.poem.25408": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Tore aller Himmel stehen hoch", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tore aller Himmel stehen hoch", "tokens": ["Die", "To\u00b7re", "al\u00b7ler", "Him\u00b7mel", "ste\u00b7hen", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dem Dunkel offen,", "tokens": ["dem", "Dun\u00b7kel", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das lautlos einstr\u00f6mt,", "tokens": ["Das", "laut\u00b7los", "eins\u00b7tr\u00f6mt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "wie in bodenlosen Trichter", "tokens": ["wie", "in", "bo\u00b7den\u00b7lo\u00b7sen", "Trich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Land niederrei\u00dfend.", "tokens": ["Land", "nie\u00b7der\u00b7rei\u00b7\u00dfend", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Schatten treten dichter", "tokens": ["Schat\u00b7ten", "tre\u00b7ten", "dich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Aus lockren Poren", "tokens": ["Aus", "lock\u00b7ren", "Po\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "nachtgef\u00fcllter Schollen.", "tokens": ["nacht\u00b7ge\u00b7f\u00fcll\u00b7ter", "Schol\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Die Pappeln,", "tokens": ["Die", "Pap\u00b7peln", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "die noch kaum von Sonne troffen,", "tokens": ["die", "noch", "kaum", "von", "Son\u00b7ne", "trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sind stumpf wie schwarze Kreuzesst\u00e4mme", "tokens": ["Sind", "stumpf", "wie", "schwar\u00b7ze", "Kreu\u00b7zes\u00b7st\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "\u00fcbers Land geschlagen.", "tokens": ["\u00fc\u00b7bers", "Land", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Die \u00c4cker wachsen grau und drohend \u2013", "tokens": ["Die", "\u00c4\u00b7cker", "wach\u00b7sen", "grau", "und", "dro\u00b7hend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ebenen tr\u00fcber Schlacke.", "tokens": ["E\u00b7be\u00b7nen", "tr\u00fc\u00b7ber", "Schla\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.15": {"text": "Nacht wirbelt aus den Wolkengruben,", "tokens": ["Nacht", "wir\u00b7belt", "aus", "den", "Wol\u00b7ken\u00b7gru\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "\u00fcber die die St\u00f6\u00dfe rollen", "tokens": ["\u00fc\u00b7ber", "die", "die", "St\u00f6\u00b7\u00dfe", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Schon k\u00fchler Winde,", "tokens": ["Schon", "k\u00fch\u00b7ler", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "und im d\u00e4mmrigen Gezacke", "tokens": ["und", "im", "d\u00e4mm\u00b7ri\u00b7gen", "Ge\u00b7za\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Hellgr\u00fcner Weidenb\u00fcschel,", "tokens": ["Hell\u00b7gr\u00fc\u00b7ner", "Wei\u00b7den\u00b7b\u00fc\u00b7schel", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "drin es rastend sich und r\u00f6chelnd eingeschlagen,", "tokens": ["drin", "es", "ras\u00b7tend", "sich", "und", "r\u00f6\u00b7chelnd", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "PRF", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Verglast das letzte Licht.", "tokens": ["Ver\u00b7glast", "das", "letz\u00b7te", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}