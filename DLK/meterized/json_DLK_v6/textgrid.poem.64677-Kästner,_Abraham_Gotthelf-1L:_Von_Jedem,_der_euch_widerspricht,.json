{"textgrid.poem.64677": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Jedem, der euch widerspricht,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Jedem, der euch widerspricht,", "tokens": ["Von", "Je\u00b7dem", ",", "der", "euch", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt ihr verachtungsvoll: Der Mann versteht uns nicht!", "tokens": ["Sagt", "ihr", "ver\u00b7ach\u00b7tungs\u00b7voll", ":", "Der", "Mann", "ver\u00b7steht", "uns", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00f6nnt ihr nun nicht verst\u00e4ndlich schreiben,", "tokens": ["K\u00f6nnt", "ihr", "nun", "nicht", "ver\u00b7st\u00e4nd\u00b7lich", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So m\u00f6gt ihr ungelesen bleiben.", "tokens": ["So", "m\u00f6gt", "ihr", "un\u00b7ge\u00b7le\u00b7sen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}