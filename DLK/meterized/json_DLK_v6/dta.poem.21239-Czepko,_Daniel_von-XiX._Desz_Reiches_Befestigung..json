{"dta.poem.21239": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.  \n  Desz Reiches Befestigung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "O Deutschland/ nicht der Rein/ noch deiner Tonaw Feld/", "tokens": ["O", "Deutschland", "/", "nicht", "der", "Rein", "/", "noch", "dei\u00b7ner", "To\u00b7naw", "Feld", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PTKNEG", "ART", "NN", "$(", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vnd andrer Fl\u00fcsse Volck mit jhren starcken Wercken/", "tokens": ["Vnd", "an\u00b7drer", "Fl\u00fcs\u00b7se", "Volck", "mit", "jhren", "star\u00b7cken", "Wer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Besch\u00fctzen deinen Thron; Das was Jhn kan verstercken/", "tokens": ["Be\u00b7sch\u00fct\u00b7zen", "dei\u00b7nen", "Thron", ";", "Das", "was", "Jhn", "kan", "ver\u00b7ster\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "PDS", "PRELS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist was de\u00df ", "tokens": ["Ist", "was", "de\u00df"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "O Crone von Madritt/ Sie lassen es geschehn/", "tokens": ["O", "Cro\u00b7ne", "von", "Mad\u00b7ritt", "/", "Sie", "las\u00b7sen", "es", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$(", "PPER", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Zimmer mach ich auff/ ", "tokens": ["Das", "Zim\u00b7mer", "mach", "ich", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo solche ", "tokens": ["Wo", "sol\u00b7che"], "token_info": ["word", "word"], "pos": ["PWAV", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die Welt steht mit mir da/ vnd wil ", "tokens": ["Die", "Welt", "steht", "mit", "mir", "da", "/", "vnd", "wil"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADV", "$(", "KON", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Kanser vnd O Herr/ Sie sind auch vnsre Krafft;", "tokens": ["O", "Kan\u00b7ser", "vnd", "O", "Herr", "/", "Sie", "sind", "auch", "vns\u00b7re", "Krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NE", "NN", "$(", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch hie von ", "tokens": ["Auch", "hie", "von"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Solln ", "tokens": ["Solln"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der auch durch Seinen Sohn den Menschen Frieden\nschafft.", "tokens": ["Der", "auch", "durch", "Sei\u00b7nen", "Sohn", "den", "Men\u00b7schen", "Frie\u00b7den", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}