{"dta.poem.21288": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Doris und der Wein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oanblick, der mich fr\u00f6hlich macht!", "tokens": ["O\u00b7an\u00b7blick", ",", "der", "mich", "fr\u00f6h\u00b7lich", "macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Weinstock reift und Doris lacht,", "tokens": ["Mein", "Wein\u00b7stock", "reift", "und", "Do\u00b7ris", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, mir zur Anmuth, wachsen beyde.", "tokens": ["Und", ",", "mir", "zur", "An\u00b7muth", ",", "wach\u00b7sen", "bey\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "APPRART", "NN", "$,", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ergetzt der Wein ein menschlich Herz,", "tokens": ["Er\u00b7getzt", "der", "Wein", "ein", "menschlich", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So ist auch seltner Sch\u00f6nen Scherz", "tokens": ["So", "ist", "auch", "selt\u00b7ner", "Sch\u00f6\u00b7nen", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wahren Menschlichkeit ein Grund vollkom\u0303ner Freude.", "tokens": ["Der", "wah\u00b7ren", "Menschlich\u00b7keit", "ein", "Grund", "voll\u00b7kom\u0303\u00b7ner", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Was die Empfindung sch\u00e4rft und \u00fcbt,", "tokens": ["Was", "die", "Emp\u00b7fin\u00b7dung", "sch\u00e4rft", "und", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Seelen neue Kr\u00e4fte giebt,", "tokens": ["Was", "See\u00b7len", "neu\u00b7e", "Kr\u00e4f\u00b7te", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird unsre heisse Sehnsucht stillen.", "tokens": ["Wird", "uns\u00b7re", "heis\u00b7se", "Sehn\u00b7sucht", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie reichlich will die mildre Zeit,", "tokens": ["Wie", "reich\u00b7lich", "will", "die", "mild\u00b7re", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sonst so sparsam uns erfreut,", "tokens": ["Die", "sonst", "so", "spar\u00b7sam", "uns", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den tiefsten Kelch der Lust f\u00fcr unsre Lippen f\u00fcllen.", "tokens": ["Den", "tiefs\u00b7ten", "Kelch", "der", "Lust", "f\u00fcr", "uns\u00b7re", "Lip\u00b7pen", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Wein, des Kummers Gegengift,", "tokens": ["Der", "Wein", ",", "des", "Kum\u00b7mers", "Ge\u00b7gen\u00b7gift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe, die ihn \u00fcbertrifft,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "ihn", "\u00fc\u00b7bert\u00b7rifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die werden zwischen uns sich theilen.", "tokens": ["Die", "wer\u00b7den", "zwi\u00b7schen", "uns", "sich", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer mir der Weine Tropfen z\u00e4hlt,", "tokens": ["Wer", "mir", "der", "Wei\u00b7ne", "Trop\u00b7fen", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur der berechnet unverfehlt", "tokens": ["Nur", "der", "be\u00b7rech\u00b7net", "un\u00b7ver\u00b7fehlt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die K\u00fcsse, die geh\u00e4uft zu dir, o Doris! eilen.", "tokens": ["Die", "K\u00fcs\u00b7se", ",", "die", "ge\u00b7h\u00e4uft", "zu", "dir", ",", "o", "Do\u00b7ris", "!", "ei\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPER", "$,", "FM", "FM", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weil deine Jugend lernen mu\u00df,", "tokens": ["Weil", "dei\u00b7ne", "Ju\u00b7gend", "ler\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df dich meinen \u00f6ftern Ku\u00df", "tokens": ["So", "la\u00df", "dich", "mei\u00b7nen", "\u00f6f\u00b7tern", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Menge deiner Sch\u00e4tze lehren.", "tokens": ["Die", "Men\u00b7ge", "dei\u00b7ner", "Sch\u00e4t\u00b7ze", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib seinem treuen Unbestand", "tokens": ["Gib", "sei\u00b7nem", "treu\u00b7en", "Un\u00b7be\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stirn, Augen, Wangen, Mund und Hand,", "tokens": ["Stirn", ",", "Au\u00b7gen", ",", "Wan\u00b7gen", ",", "Mund", "und", "Hand", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und la\u00df ihn ieden Reiz, der dich erhebt, verehren!", "tokens": ["Und", "la\u00df", "ihn", "ie\u00b7den", "Reiz", ",", "der", "dich", "er\u00b7hebt", ",", "ver\u00b7eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Uns klopft ein Vorwitz in der Brust,", "tokens": ["Uns", "klopft", "ein", "Vor\u00b7witz", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stumme Rath ererbter Lust,", "tokens": ["Der", "stum\u00b7me", "Rath", "er\u00b7erb\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Liebe Leidenschaft zu kennen.", "tokens": ["Der", "Lie\u00b7be", "Lei\u00b7den\u00b7schaft", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O lerne meine Holdinn seyn!", "tokens": ["O", "ler\u00b7ne", "mei\u00b7ne", "Hol\u00b7dinn", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schw\u00f6re dir, bey Most und Wein,", "tokens": ["Ich", "schw\u00f6\u00b7re", "dir", ",", "bey", "Most", "und", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich soll auch Most und Wein von keiner Do ris trennen.", "tokens": ["Mich", "soll", "auch", "Most", "und", "Wein", "von", "kei\u00b7ner", "Do", "ris", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "KON", "NN", "APPR", "PIAT", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es m\u00f6gen k\u00fcnftig Wein und Most", "tokens": ["Es", "m\u00f6\u00b7gen", "k\u00fcnf\u00b7tig", "Wein", "und", "Most"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des tr\u00e4gen Alters Ernst und Frost", "tokens": ["Des", "tr\u00e4\u00b7gen", "Al\u00b7ters", "Ernst", "und", "Frost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch feuerreiche Kraft verdringen!", "tokens": ["Durch", "feu\u00b7er\u00b7rei\u00b7che", "Kraft", "ver\u00b7drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alsdann ert\u00f6nt f\u00fcr sie mein Lied;", "tokens": ["Als\u00b7dann", "er\u00b7t\u00f6nt", "f\u00fcr", "sie", "mein", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jtzt, da die Jugend noch verzieht,", "tokens": ["Jtzt", ",", "da", "die", "Ju\u00b7gend", "noch", "ver\u00b7zieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich allein von dir, auch in der Lese, singen.", "tokens": ["Will", "ich", "al\u00b7lein", "von", "dir", ",", "auch", "in", "der", "Le\u00b7se", ",", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}