{"textgrid.poem.58865": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "84. Ein eng'lisches Werk", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt eben alle Cherubinen", "tokens": ["Jetzt", "e\u00b7ben", "al\u00b7le", "Che\u00b7ru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geb\u00fcckt der ew'gen Gottheit dienen;", "tokens": ["Ge\u00b7b\u00fcckt", "der", "ew'\u00b7gen", "Got\u00b7theit", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gott ist dir nahe, wie du wei\u00dft,", "tokens": ["Gott", "ist", "dir", "na\u00b7he", ",", "wie", "du", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So beuge dich denn mit im Geist!", "tokens": ["So", "beu\u00b7ge", "dich", "denn", "mit", "im", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}