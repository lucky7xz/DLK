{"dta.poem.8238": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtsabend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und schaut in des Zimmers Raum:", "tokens": ["Und", "schaut", "in", "des", "Zim\u00b7mers", "Raum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Kinder alle tanzten und sangen", "tokens": ["Die", "Kin\u00b7der", "al\u00b7le", "tanz\u00b7ten", "und", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um den brennenden Weihnachtsbaum.", "tokens": ["Um", "den", "bren\u00b7nen\u00b7den", "Weih\u00b7nachts\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Da pocht ihm das Herz, da\u00df es will zerspringen;", "tokens": ["Da", "pocht", "ihm", "das", "Herz", ",", "da\u00df", "es", "will", "zer\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "O, ruft er, la\u00dft mich hinein,", "tokens": ["O", ",", "ruft", "er", ",", "la\u00dft", "mich", "hin\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was Frommes, was Fr\u00f6hliches will ich euch singen", "tokens": ["Was", "From\u00b7mes", ",", "was", "Fr\u00f6h\u00b7li\u00b7ches", "will", "ich", "euch", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "$,", "PWS", "NN", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu dem hellen Kerzenschein.", "tokens": ["Zu", "dem", "hel\u00b7len", "Ker\u00b7zen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Kinder kommen, die Kinder ziehen", "tokens": ["Und", "die", "Kin\u00b7der", "kom\u00b7men", ",", "die", "Kin\u00b7der", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Schwelle den n\u00e4chtlichen Gast;", "tokens": ["Zur", "Schwel\u00b7le", "den", "n\u00e4cht\u00b7li\u00b7chen", "Gast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Still gr\u00fc\u00dfen die Alten, die Jungen umknieen", "tokens": ["Still", "gr\u00fc\u00b7\u00dfen", "die", "Al\u00b7ten", ",", "die", "Jun\u00b7gen", "um\u00b7kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+--+--+--+--", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Ihn scheu in gesch\u00e4ftiger Hast.", "tokens": ["Ihn", "scheu", "in", "ge\u00b7sch\u00e4f\u00b7ti\u00b7ger", "Hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und er singt: \u201eWeit gl\u00e4nzen da drau\u00dfen die Lande,", "tokens": ["Und", "er", "singt", ":", "\u201e", "Weit", "gl\u00e4n\u00b7zen", "da", "drau\u00b7\u00dfen", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "ADJD", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und locken den Knaben hinaus;", "tokens": ["Und", "lo\u00b7cken", "den", "Kna\u00b7ben", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit klopfender Brust, im Reisegewande", "tokens": ["Mit", "klop\u00b7fen\u00b7der", "Brust", ",", "im", "Rei\u00b7se\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verl\u00e4\u00dft er das Vaterhaus.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "er", "das", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da tr\u00e4gt ihn des Lebens breitere Welle \u2014", "tokens": ["Da", "tr\u00e4gt", "ihn", "des", "Le\u00b7bens", "brei\u00b7te\u00b7re", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie war so weit die Welt!", "tokens": ["Wie", "war", "so", "weit", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und es findet sich mancher gute Geselle,", "tokens": ["Und", "es", "fin\u00b7det", "sich", "man\u00b7cher", "gu\u00b7te", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PIAT", "ADJA", "NN", "$,"], "meter": "--+----+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der's treulich mit ihm h\u00e4lt.", "tokens": ["Der's", "treu\u00b7lich", "mit", "ihm", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Tief br\u00e4unt ihm die Sonne die Bl\u00fcthe der Wangen", "tokens": ["Tief", "br\u00e4unt", "ihm", "die", "Son\u00b7ne", "die", "Bl\u00fc\u00b7the", "der", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und der Bart umsprosset das Kinn;", "tokens": ["Und", "der", "Bart", "um\u00b7spros\u00b7set", "das", "Kinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den Knaben, der blond in die Welt gegangen,", "tokens": ["Den", "Kna\u00b7ben", ",", "der", "blond", "in", "die", "Welt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wohl nimmer erkennet ihr ihn.", "tokens": ["Wohl", "nim\u00b7mer", "er\u00b7ken\u00b7net", "ihr", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Aus goldnen und aus blauen Reben", "tokens": ["Aus", "gold\u00b7nen", "und", "aus", "blau\u00b7en", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es mundet ihm jeder Wein;", "tokens": ["Es", "mun\u00b7det", "ihm", "je\u00b7der", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und dreister greift er in das Leben", "tokens": ["Und", "dreis\u00b7ter", "greift", "er", "in", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in die Saiten ein.", "tokens": ["Und", "in", "die", "Sai\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und f\u00fcr manche Dirne mit schwarzen Locken", "tokens": ["Und", "f\u00fcr", "man\u00b7che", "Dir\u00b7ne", "mit", "schwar\u00b7zen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Im Herzen findet er Raum; \u2014", "tokens": ["Im", "Her\u00b7zen", "fin\u00b7det", "er", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da klingen durch das Land die Glocken,", "tokens": ["Da", "klin\u00b7gen", "durch", "das", "Land", "die", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm war's wie ein alter Traum.", "tokens": ["Ihm", "wa\u00b7r's", "wie", "ein", "al\u00b7ter", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wohin er kam, die Kinder sangen,", "tokens": ["Wo\u00b7hin", "er", "kam", ",", "die", "Kin\u00b7der", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kinder weit und breit,", "tokens": ["Die", "Kin\u00b7der", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kerzen brannten, die Stimmlein klangen,", "tokens": ["Die", "Ker\u00b7zen", "brann\u00b7ten", ",", "die", "Stimm\u00b7lein", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das war die Weihnachtszeit.", "tokens": ["Das", "war", "die", "Weih\u00b7nachts\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da f\u00fchlte er, da\u00df er ein Mann geworden;", "tokens": ["Da", "f\u00fchl\u00b7te", "er", ",", "da\u00df", "er", "ein", "Mann", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier geh\u00f6rte er nicht dazu.", "tokens": ["Hier", "ge\u00b7h\u00f6r\u00b7te", "er", "nicht", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hinter den blauen Bergen im Norden", "tokens": ["Hin\u00b7ter", "den", "blau\u00b7en", "Ber\u00b7gen", "im", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Lie\u00df ihm die Heimath nicht Ruh.", "tokens": ["Lie\u00df", "ihm", "die", "Hei\u00b7math", "nicht", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "An die hellen Fenster kam er gegangen", "tokens": ["An", "die", "hel\u00b7len", "Fens\u00b7ter", "kam", "er", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und schaut' in des Zimmers Raum;", "tokens": ["Und", "schaut'", "in", "des", "Zim\u00b7mers", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Schwestern und Br\u00fcder tanzten und sangen", "tokens": ["Die", "Schwes\u00b7tern", "und", "Br\u00fc\u00b7der", "tanz\u00b7ten", "und", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Christlied am Taxusbaum.\u201c \u2014", "tokens": ["Ein", "Christ\u00b7lied", "am", "Ta\u00b7xus\u00b7baum", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Da war es, als w\u00fcrden lebendig die Lieder", "tokens": ["Da", "war", "es", ",", "als", "w\u00fcr\u00b7den", "le\u00b7ben\u00b7dig", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOKOM", "VAFIN", "ADJD", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und nahe, der eben noch fern;", "tokens": ["Und", "na\u00b7he", ",", "der", "e\u00b7ben", "noch", "fern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PRELS", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Um den Taxus tanzten Schwester und Br\u00fcder", "tokens": ["Um", "den", "Ta\u00b7xus", "tanz\u00b7ten", "Schwes\u00b7ter", "und", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sangen ein Lied vom Herrn.", "tokens": ["Und", "san\u00b7gen", "ein", "Lied", "vom", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Da kann er nicht l\u00e4nger das Herz bezwingen,", "tokens": ["Da", "kann", "er", "nicht", "l\u00e4n\u00b7ger", "das", "Herz", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er breitet die Arme aus:", "tokens": ["Er", "brei\u00b7tet", "die", "Ar\u00b7me", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eo schlie\u00dfet mich ein in das Preisen und Singen,", "tokens": ["\u201e", "o", "schlie\u00b7\u00dfet", "mich", "ein", "in", "das", "Prei\u00b7sen", "und", "Sin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "ART", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich bin ja der Sohn vom Haus.\u201c", "tokens": ["Ich", "bin", "ja", "der", "Sohn", "vom", "Haus", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}