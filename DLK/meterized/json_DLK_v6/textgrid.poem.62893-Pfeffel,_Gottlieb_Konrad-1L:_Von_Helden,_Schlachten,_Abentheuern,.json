{"textgrid.poem.62893": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Helden, Schlachten, Abentheuern,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Helden, Schlachten, Abentheuern,", "tokens": ["Von", "Hel\u00b7den", ",", "Schlach\u00b7ten", ",", "A\u00b7bent\u00b7heu\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00dft du so viel uns vorzuleyern,", "tokens": ["Wei\u00dft", "du", "so", "viel", "uns", "vor\u00b7zu\u00b7ley\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warst du denn immer vorne dran?", "tokens": ["Warst", "du", "denn", "im\u00b7mer", "vor\u00b7ne", "dran", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So redete mit heisrem Bl\u00f6cken", "tokens": ["So", "re\u00b7de\u00b7te", "mit", "heis\u00b7rem", "Bl\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Am Hof des Ritters Hadrian", "tokens": ["Am", "Hof", "des", "Rit\u00b7ters", "Had\u00b7ri\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "(es war zur Zeit der armen Gecken)", "tokens": ["(", "es", "war", "zur", "Zeit", "der", "ar\u00b7men", "Ge\u00b7cken", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Stallbock einen K\u00e4fer an,", "tokens": ["Der", "Stall\u00b7bock", "ei\u00b7nen", "K\u00e4\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem die Natur die Citadelle", "tokens": ["Dem", "die", "Na\u00b7tur", "die", "Ci\u00b7ta\u00b7del\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Des Pferdes, die der Schwanz bedeckt,", "tokens": ["Des", "Pfer\u00b7des", ",", "die", "der", "Schwanz", "be\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu seinem Wohnsitz ausgesteckt.", "tokens": ["Zu", "sei\u00b7nem", "Wohn\u00b7sitz", "aus\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich, sprach er, war der Spie\u00dfgeselle", "tokens": ["Ich", ",", "sprach", "er", ",", "war", "der", "Spie\u00df\u00b7ge\u00b7sel\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Von manchem hochber\u00fchmten Held:", "tokens": ["Von", "man\u00b7chem", "hoch\u00b7be\u00b7r\u00fchm\u00b7ten", "Held", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er trug mich hinter seinem Rosse", "tokens": ["Er", "trug", "mich", "hin\u00b7ter", "sei\u00b7nem", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Incognito durch alle Welt.", "tokens": ["In\u00b7co\u00b7gni\u00b7to", "durch", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die\u00df h\u00f6rte der Poet vom Schlosse;", "tokens": ["Die\u00df", "h\u00f6r\u00b7te", "der", "Po\u00b7et", "vom", "Schlos\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Er schleichet sich zum Pegasus,", "tokens": ["Er", "schlei\u00b7chet", "sich", "zum", "Pe\u00b7ga\u00b7sus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Den eben itzt in seinem Glanze", "tokens": ["Den", "e\u00b7ben", "itzt", "in", "sei\u00b7nem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein \u00e4chter Sohn des Latous", "tokens": ["Ein", "\u00e4ch\u00b7ter", "Sohn", "des", "La\u00b7tous"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Bestieg; er fasset ihn beym Schwanze,", "tokens": ["Be\u00b7stieg", ";", "er", "fas\u00b7set", "ihn", "beym", "Schwan\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Flog baumelnd mit ihm auf und schrie:", "tokens": ["Flog", "bau\u00b7melnd", "mit", "ihm", "auf", "und", "schrie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Triumph! auch ich bin ein Genie.", "tokens": ["Tri\u00b7umph", "!", "auch", "ich", "bin", "ein", "Ge\u00b7nie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}