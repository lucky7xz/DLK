{"textgrid.poem.54488": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie offt/ mein Licht/ wie offt vmbringt mich Angst vnd Weh!", "tokens": ["Wie", "offt", "/", "mein", "Licht", "/", "wie", "offt", "vm\u00b7bringt", "mich", "Angst", "vnd", "Weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$(", "PPOSAT", "NN", "$(", "KOKOM", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie offtmals wil mir Trost/ vnd Glaub/ vnd Muth zerrinnen?", "tokens": ["Wie", "offt\u00b7mals", "wil", "mir", "Trost", "/", "vnd", "Glaub", "/", "vnd", "Muth", "zer\u00b7rin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "NN", "$(", "KON", "NN", "$(", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie offtmals kan ich mich vor Schmertzen kaum besinnen.", "tokens": ["Wie", "offt\u00b7mals", "kan", "ich", "mich", "vor", "Schmert\u00b7zen", "kaum", "be\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie offtmals ruff ich schon: mein Leben nun Ade.", "tokens": ["Wie", "offt\u00b7mals", "ruff", "ich", "schon", ":", "mein", "Le\u00b7ben", "nun", "A\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Doch wenn mich d\u00fcnckt/ da\u00df ich im Elend itzt vergeh/", "tokens": ["Doch", "wenn", "mich", "d\u00fcnckt", "/", "da\u00df", "ich", "im", "E\u00b7lend", "itzt", "ver\u00b7geh", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd meine da\u00df vor mich kein Mittel zu gewinnen/", "tokens": ["Vnd", "mei\u00b7ne", "da\u00df", "vor", "mich", "kein", "Mit\u00b7tel", "zu", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "KOUS", "APPR", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So werd ich deiner H\u00fclff vnd gegenwart recht innen/", "tokens": ["So", "werd", "ich", "dei\u00b7ner", "H\u00fclff", "vnd", "ge\u00b7gen\u00b7wart", "recht", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd da\u00df in deine Hand ich eingeschrieben steh:", "tokens": ["Vnd", "da\u00df", "in", "dei\u00b7ne", "Hand", "ich", "ein\u00b7ge\u00b7schrie\u00b7ben", "steh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn sagstu wie der Zorn de\u00df H\u00f6chsten abgelehnet", "tokens": ["Denn", "sags\u00b7tu", "wie", "der", "Zorn", "de\u00df", "H\u00f6chs\u00b7ten", "ab\u00b7ge\u00b7leh\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie Gott mit mir zu fried'/ vnd wie du mich vers\u00f6hnet.", "tokens": ["Wie", "Gott", "mit", "mir", "zu", "fried'", "/", "vnd", "wie", "du", "mich", "ver\u00b7s\u00f6h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "APPR", "NN", "$(", "KON", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn lern ich/ da\u00df ich Fleisch/ gleich deinem Fleische sey.", "tokens": ["Denn", "lern", "ich", "/", "da\u00df", "ich", "Fleisch", "/", "gleich", "dei\u00b7nem", "Flei\u00b7sche", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "NN", "$(", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df/ wenn ich nichts mehr schau/ mich deine Wunden schauen", "tokens": ["La\u00df", "/", "wenn", "ich", "nichts", "mehr", "schau", "/", "mich", "dei\u00b7ne", "Wun\u00b7den", "schau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "PIS", "ADV", "PTKVZ", "$(", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd wenn dem bl\u00f6den Geist wird vor dem Tode grauen/", "tokens": ["Vnd", "wenn", "dem", "bl\u00f6\u00b7den", "Geist", "wird", "vor", "dem", "To\u00b7de", "grau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So steh! O h\u00f6chster Trost der schwachen Seelen bey.", "tokens": ["So", "steh", "!", "O", "h\u00f6chs\u00b7ter", "Trost", "der", "schwa\u00b7chen", "See\u00b7len", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NE", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}