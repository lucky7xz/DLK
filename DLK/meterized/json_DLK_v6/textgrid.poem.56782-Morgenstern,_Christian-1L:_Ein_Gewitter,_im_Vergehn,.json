{"textgrid.poem.56782": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Gewitter, im Vergehn,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gewitter, im Vergehn,", "tokens": ["Ein", "Ge\u00b7wit\u00b7ter", ",", "im", "Ver\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "lie\u00df einst einen Donner stehn.", "tokens": ["lie\u00df", "einst", "ei\u00b7nen", "Don\u00b7ner", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwarz in einer Felsenscharte", "tokens": ["Schwarz", "in", "ei\u00b7ner", "Fel\u00b7sen\u00b7schar\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "stand der Donner da und harrte \u2013", "tokens": ["stand", "der", "Don\u00b7ner", "da", "und", "harr\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "scharrte dumpf mit Hals und Hufe,", "tokens": ["scharr\u00b7te", "dumpf", "mit", "Hals", "und", "Hu\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df man ihn nach Hause rufe.", "tokens": ["da\u00df", "man", "ihn", "nach", "Hau\u00b7se", "ru\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch das dunkle Donnerfohlen \u2013", "tokens": ["Doch", "das", "dunk\u00b7le", "Don\u00b7ner\u00b7foh\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "niemand kams nach Hause holen.", "tokens": ["nie\u00b7mand", "kams", "nach", "Hau\u00b7se", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Gew\u00f6lk, im Arm des Windes,", "tokens": ["Sein", "Ge\u00b7w\u00f6lk", ",", "im", "Arm", "des", "Win\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dachte nimmer seines Kindes \u2013", "tokens": ["dach\u00b7te", "nim\u00b7mer", "sei\u00b7nes", "Kin\u00b7des", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "flog dahin zum Erdensaum", "tokens": ["flog", "da\u00b7hin", "zum", "Er\u00b7den\u00b7saum"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und verschwand dort wie ein Traum.", "tokens": ["und", "ver\u00b7schwand", "dort", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Grollend und ins Herz getroffen", "tokens": ["Grol\u00b7lend", "und", "ins", "Herz", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "l\u00e4\u00dft der Donner Wunsch und Hoffen,", "tokens": ["l\u00e4\u00dft", "der", "Don\u00b7ner", "Wunsch", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "richtet sich im Felsgestein,", "tokens": ["rich\u00b7tet", "sich", "im", "Fels\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wie ein Bergzentaure ein.", "tokens": ["wie", "ein", "Berg\u00b7zen\u00b7tau\u00b7re", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Als die n\u00e4chste Fr\u00fche blaut,", "tokens": ["Als", "die", "n\u00e4chs\u00b7te", "Fr\u00fc\u00b7he", "blaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ist sein pechschwarz Fell ergraut.", "tokens": ["ist", "sein", "pech\u00b7schwarz", "Fell", "er\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Traurig sieht er sich im See", "tokens": ["Trau\u00b7rig", "sieht", "er", "sich", "im", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "fahl, wie alten Gletscherschnee.", "tokens": ["fahl", ",", "wie", "al\u00b7ten", "Glet\u00b7scher\u00b7schnee", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Stumm verkriecht er sich, verh\u00e4rmt;", "tokens": ["Stumm", "ver\u00b7kriecht", "er", "sich", ",", "ver\u00b7h\u00e4rmt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "nur wenn Menschheit kommt und l\u00e4rmt,", "tokens": ["nur", "wenn", "Menschheit", "kommt", "und", "l\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "\u00e4fft er schaurig ihren Schall,", "tokens": ["\u00e4fft", "er", "schau\u00b7rig", "ih\u00b7ren", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bringt Ger\u00f6ll und Schutt zu Fall ...", "tokens": ["bringt", "Ge\u00b7r\u00f6ll", "und", "Schutt", "zu", "Fall", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Mancher Hirt und mancher Hund", "tokens": ["Man\u00b7cher", "Hirt", "und", "man\u00b7cher", "Hund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schl\u00e4ft zu F\u00fc\u00dfen ihm im Schrund.", "tokens": ["schl\u00e4ft", "zu", "F\u00fc\u00b7\u00dfen", "ihm", "im", "Schrund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}