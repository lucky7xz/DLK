{"textgrid.poem.27743": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weistu, was in dieser Welt", "tokens": ["Weis\u00b7tu", ",", "was", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir am meisten wolgef\u00e4llt?", "tokens": ["Mir", "am", "meis\u00b7ten", "wol\u00b7ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "PIS", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Zeit sich selbst verzehret,", "tokens": ["Da\u00df", "die", "Zeit", "sich", "selbst", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Welt nicht ewig w\u00e4hret.", "tokens": ["Und", "die", "Welt", "nicht", "e\u00b7wig", "w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}