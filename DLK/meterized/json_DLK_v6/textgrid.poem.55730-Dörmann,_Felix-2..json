{"textgrid.poem.55730": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal steigt", "tokens": ["Noch", "ein\u00b7mal", "steigt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das alte, s\u00fc\u00dfe Wahngebilde", "tokens": ["Das", "al\u00b7te", ",", "s\u00fc\u00b7\u00dfe", "Wahn\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schmeichelnd empor;", "tokens": ["Schmei\u00b7chelnd", "em\u00b7por", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Laute, \u2013 lang entbehrte, milde, \u2013", "tokens": ["Lau\u00b7te", ",", "\u2013", "lang", "ent\u00b7behr\u00b7te", ",", "mil\u00b7de", ",", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "$(", "ADJD", "VVFIN", "$,", "ADJD", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klingen zaghaft s\u00fc\u00df ans Ohr.", "tokens": ["Klin\u00b7gen", "zag\u00b7haft", "s\u00fc\u00df", "ans", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "O f\u00fchret mich nicht in Versuchung! \u2013", "tokens": ["O", "f\u00fch\u00b7ret", "mich", "nicht", "in", "Ver\u00b7su\u00b7chung", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist schwach,", "tokens": ["Mein", "Herz", "ist", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Alle die T\u00e4uschungen,", "tokens": ["Al\u00b7le", "die", "T\u00e4u\u00b7schun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "All der bacchantische Taumel, ach,", "tokens": ["All", "der", "bac\u00b7chan\u00b7ti\u00b7sche", "Tau\u00b7mel", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ITJ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nichts kann ert\u00f6ten", "tokens": ["Nichts", "kann", "er\u00b7t\u00f6\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ewig aufsteigender Liebessonnen", "tokens": ["E\u00b7wig", "auf\u00b7stei\u00b7gen\u00b7der", "Lie\u00b7bes\u00b7son\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Morgenr\u00f6ten.", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}