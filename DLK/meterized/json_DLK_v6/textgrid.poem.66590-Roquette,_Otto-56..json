{"textgrid.poem.66590": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was euch Erholung beut,", "tokens": ["Was", "euch", "Er\u00b7ho\u00b7lung", "beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4r mir zur Last;", "tokens": ["W\u00e4r", "mir", "zur", "Last", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was mich als Mu\u00dfe freut,", "tokens": ["Was", "mich", "als", "Mu\u00b7\u00dfe", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist euch verha\u00dft.", "tokens": ["Ist", "euch", "ver\u00b7ha\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Des Tages Schaffen weiht", "tokens": ["Des", "Ta\u00b7ges", "Schaf\u00b7fen", "weiht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mir auch die Rast,", "tokens": ["Mir", "auch", "die", "Rast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und bin ich nie befreit,", "tokens": ["Und", "bin", "ich", "nie", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ist s\u00fc\u00df die Last!", "tokens": ["Ist", "s\u00fc\u00df", "die", "Last", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}