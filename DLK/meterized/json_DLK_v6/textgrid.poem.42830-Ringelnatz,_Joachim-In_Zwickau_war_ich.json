{"textgrid.poem.42830": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "In Zwickau war ich", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich Geld h\u00e4tte", "tokens": ["Wenn", "ich", "Geld", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In unerme\u00dflichen Haufen,", "tokens": ["In", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7chen", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "W\u00fcrde ich die beiden St\u00e4dte", "tokens": ["W\u00fcr\u00b7de", "ich", "die", "bei\u00b7den", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Paris und Zwickau mir kaufen.", "tokens": ["Pa\u00b7ris", "und", "Zwi\u00b7ckau", "mir", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich wei\u00df, auch in Zwickau wohnen", "tokens": ["Ich", "wei\u00df", ",", "auch", "in", "Zwi\u00b7ckau", "woh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Entz\u00fcckende Personen.", "tokens": ["Ent\u00b7z\u00fc\u00b7cken\u00b7de", "Per\u00b7so\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Die w\u00fcrde ich verschonen.", "tokens": ["Die", "w\u00fcr\u00b7de", "ich", "ver\u00b7scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die verkaufen sowieso sich nicht,", "tokens": ["Die", "ver\u00b7kau\u00b7fen", "so\u00b7wi\u00b7e\u00b7so", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PRF", "PTKNEG", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und sie haben auch kein Pariauer,", "tokens": ["Und", "sie", "ha\u00b7ben", "auch", "kein", "Pa\u00b7ri\u00b7au\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Kein Zwickser Gesicht.", "tokens": ["Kein", "Zwick\u00b7ser", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Alles andre w\u00fcrde ich erwerben,", "tokens": ["Al\u00b7les", "and\u00b7re", "w\u00fcr\u00b7de", "ich", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Paris aber gleich zur\u00fcckgeben,", "tokens": ["Pa\u00b7ris", "a\u00b7ber", "gleich", "zu\u00b7r\u00fcck\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Nur darin leben,", "tokens": ["Nur", "da\u00b7rin", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Dann in Zwickau sterben", "tokens": ["Dann", "in", "Zwi\u00b7ckau", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "An Zwergrattengift", "tokens": ["An", "Zwer\u00b7grat\u00b7ten\u00b7gift"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Mit dem Ausruf: \u00bbWas Zwickau betrifft,", "tokens": ["Mit", "dem", "Aus\u00b7ruf", ":", "\u00bb", "Was", "Zwi\u00b7ckau", "be\u00b7tr\u00b7ifft", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "PWS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "O du sch\u00f6nes Ludwigslust in Mecklenburg!\u00ab", "tokens": ["O", "du", "sch\u00f6\u00b7nes", "Lud\u00b7wigs\u00b7lust", "in", "Meck\u00b7len\u00b7burg", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "APPR", "NE", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Lokomotivenrauch trug unsere Blues", "tokens": ["Lo\u00b7ko\u00b7mo\u00b7ti\u00b7ven\u00b7rauch", "trug", "un\u00b7se\u00b7re", "Blu\u00b7es"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+----+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ins alte Erzgebirge und verstreute", "tokens": ["Ins", "al\u00b7te", "Erz\u00b7ge\u00b7bir\u00b7ge", "und", "ver\u00b7streu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00e4\u00dflichen Ru\u00df", "tokens": ["H\u00e4\u00df\u00b7li\u00b7chen", "Ru\u00df"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "An Holz und Stein und arme Leute,", "tokens": ["An", "Holz", "und", "Stein", "und", "ar\u00b7me", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Unsern Passantengru\u00df.", "tokens": ["Un\u00b7sern", "Pas\u00b7san\u00b7ten\u00b7gru\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}