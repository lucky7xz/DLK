{"textgrid.poem.23894": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der lustige Ehemann", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ringelringelrosenkranz,", "tokens": ["Rin\u00b7gel\u00b7rin\u00b7gel\u00b7ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich tanz mit meiner Frau,", "tokens": ["Ich", "tanz", "mit", "mei\u00b7ner", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir tanzen um den Rosenbusch,", "tokens": ["Wir", "tan\u00b7zen", "um", "den", "Ro\u00b7sen\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klingklanggloribusch,", "tokens": ["Kling\u00b7klang\u00b7glo\u00b7ri\u00b7busch", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich dreh mich wie ein Pfau.", "tokens": ["Ich", "dreh", "mich", "wie", "ein", "Pfau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar hab ich kein so sch\u00f6nes Rad,", "tokens": ["Zwar", "hab", "ich", "kein", "so", "sch\u00f6\u00b7nes", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch bin ich sehr verliebt", "tokens": ["Doch", "bin", "ich", "sehr", "ver\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und springe wie ein Firlefink,", "tokens": ["Und", "sprin\u00b7ge", "wie", "ein", "Fir\u00b7le\u00b7fink", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil es gar kein lieber Ding", "tokens": ["Die\u00b7weil", "es", "gar", "kein", "lie\u00b7ber", "Ding"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wie die Meine giebt.", "tokens": ["Als", "wie", "die", "Mei\u00b7ne", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Welt, die ist da drau\u00dfen wo,", "tokens": ["Die", "Welt", ",", "die", "ist", "da", "drau\u00b7\u00dfen", "wo", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADV", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag auf dem Kopf sie stehn!", "tokens": ["Mag", "auf", "dem", "Kopf", "sie", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie intressiert uns gar nicht sehr,", "tokens": ["Sie", "in\u00b7tres\u00b7siert", "uns", "gar", "nicht", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn sie nicht vorhanden w\u00e4r", "tokens": ["Und", "wenn", "sie", "nicht", "vor\u00b7han\u00b7den", "w\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00fcrd's auch noch weiter gehn:", "tokens": ["W\u00fcrd's", "auch", "noch", "wei\u00b7ter", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ringelringelrosenkranz,", "tokens": ["Rin\u00b7gel\u00b7rin\u00b7gel\u00b7ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich tanz mit meiner Frau,", "tokens": ["Ich", "tanz", "mit", "mei\u00b7ner", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir tanzen um den Rosenbusch,", "tokens": ["Wir", "tan\u00b7zen", "um", "den", "Ro\u00b7sen\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klingklanggloribusch,", "tokens": ["Kling\u00b7klang\u00b7glo\u00b7ri\u00b7busch", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich dreh mich wie ein Pfau.", "tokens": ["Ich", "dreh", "mich", "wie", "ein", "Pfau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}