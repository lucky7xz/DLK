{"textgrid.poem.66228": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Patriot und der Weltmann", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie lieb' ich dich, mein Vaterland,", "tokens": ["\u00bb", "wie", "lieb'", "ich", "dich", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich den ersten Odem zog,", "tokens": ["Wo", "ich", "den", "ers\u00b7ten", "O\u00b7dem", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und frische L\u00fcfte athmete;", "tokens": ["Und", "fri\u00b7sche", "L\u00fcf\u00b7te", "ath\u00b7me\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lieb' ich dich! wie lieb' ich dich!\u00ab", "tokens": ["Wie", "lieb'", "ich", "dich", "!", "wie", "lieb'", "ich", "dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sprach ein deutscher Biedermann,", "tokens": ["So", "sprach", "ein", "deut\u00b7scher", "Bie\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Thr\u00e4nen flossen vom Gesicht.", "tokens": ["Und", "Thr\u00e4\u00b7nen", "flos\u00b7sen", "vom", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "(oft weint' ich in der Mitternacht", "tokens": ["(", "oft", "weint'", "ich", "in", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch solche Thr\u00e4nen \u2013 Gott, du wei\u00dft's!)", "tokens": ["Auch", "sol\u00b7che", "Thr\u00e4\u00b7nen", "\u2013", "Gott", ",", "du", "wei\u00dft's", "!", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "NN", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihn h\u00f6rt' ein Weltmann, kalt wie Schnee,", "tokens": ["Ihn", "h\u00f6rt'", "ein", "Welt\u00b7mann", ",", "kalt", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nahm Schnupftabak und l\u00e4chelte.", "tokens": ["Nahm", "Schnupf\u00b7ta\u00b7bak", "und", "l\u00e4\u00b7chel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was Vaterland? Haha, ha, ha!", "tokens": ["Was", "Va\u00b7ter\u00b7land", "?", "Ha\u00b7ha", ",", "ha", ",", "ha", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "$.", "NE", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Mir ist, weil ich erfahrner bin,", "tokens": ["Mir", "ist", ",", "weil", "ich", "er\u00b7fahr\u00b7ner", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die ganze Welt mein Vaterland.", "tokens": ["Die", "gan\u00b7ze", "Welt", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wo f\u00fcr mich Brod und Ehre ist,", "tokens": ["Wo", "f\u00fcr", "mich", "Brod", "und", "Eh\u00b7re", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da ist mein Vaterland! \u2013 Der Deutsche", "tokens": ["Da", "ist", "mein", "Va\u00b7ter\u00b7land", "!", "\u2013", "Der", "Deut\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sprach biederm\u00e4nnisch, keck und kalt:", "tokens": ["Sprach", "bie\u00b7der\u00b7m\u00e4n\u00b7nisch", ",", "keck", "und", "kalt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So schl\u00e4gst du mit geballter Faust", "tokens": ["So", "schl\u00e4gst", "du", "mit", "ge\u00b7ball\u00b7ter", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die eigne Mutter, die dich tr\u00e4nkte,", "tokens": ["Die", "eig\u00b7ne", "Mut\u00b7ter", ",", "die", "dich", "tr\u00e4nk\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ins Angesicht? Undankbarer!", "tokens": ["Ins", "An\u00b7ge\u00b7sicht", "?", "Un\u00b7dank\u00b7ba\u00b7rer", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.20": {"text": "Hat jene Dirne dich ges\u00e4ugt,", "tokens": ["Hat", "je\u00b7ne", "Dir\u00b7ne", "dich", "ge\u00b7s\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der du die geilen Lippen k\u00fcssest?", "tokens": ["Der", "du", "die", "gei\u00b7len", "Lip\u00b7pen", "k\u00fcs\u00b7sest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Fleuch hin zur Krippe, draus du fri\u00dfst,", "tokens": ["Fleuch", "hin", "zur", "Krip\u00b7pe", ",", "draus", "du", "fri\u00df\u00b7st", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und nenne sie dein Vaterland.", "tokens": ["Und", "nen\u00b7ne", "sie", "dein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}