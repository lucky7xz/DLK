{"dta.poem.8437": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Klage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Eichengr\u00fcn,", "tokens": ["Durch", "Ei\u00b7chen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das K\u00fchlung gab,", "tokens": ["Das", "K\u00fch\u00b7lung", "gab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf mich herab,", "tokens": ["Auf", "mich", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Mond, und lachte Ruh", "tokens": ["O", "Mond", ",", "und", "lach\u00b7te", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KON", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir frohen Knaben zu.", "tokens": ["Mir", "fro\u00b7hen", "Kna\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn izt dein Licht", "tokens": ["Wenn", "izt", "dein", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Durchs Fenster bricht,", "tokens": ["Durchs", "Fens\u00b7ter", "bricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Lachts keine Ruh", "tokens": ["Lachts", "kei\u00b7ne", "Ruh"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mir J\u00fcngling zu,", "tokens": ["Mir", "J\u00fcng\u00b7ling", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Siehts meine Wange blass,", "tokens": ["Siehts", "mei\u00b7ne", "Wan\u00b7ge", "blass", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein Auge thr\u00e4nennass.", "tokens": ["Mein", "Au\u00b7ge", "thr\u00e4\u00b7nen\u00b7nass", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald, lieber Freund,", "tokens": ["Bald", ",", "lie\u00b7ber", "Freund", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ach bald bescheint", "tokens": ["Ach", "bald", "be\u00b7scheint"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein Silberschein", "tokens": ["Dein", "Sil\u00b7ber\u00b7schein"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den Leichenstein,", "tokens": ["Den", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der meine Asche birgt,", "tokens": ["Der", "mei\u00b7ne", "A\u00b7sche", "birgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des J\u00fcnglings Asche birgt!", "tokens": ["Des", "J\u00fcng\u00b7lings", "A\u00b7sche", "birgt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}