{"dta.poem.11971": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du hier Gutes thust, das ist dort angelegt", "tokens": ["Was", "du", "hier", "Gu\u00b7tes", "thust", ",", "das", "ist", "dort", "an\u00b7ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "NN", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Kapital, das hier dir nur die Zinsen tr\u00e4gt.", "tokens": ["Als", "Ka\u00b7pi\u00b7tal", ",", "das", "hier", "dir", "nur", "die", "Zin\u00b7sen", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sollt' es Zinsen dir in einer Zeit nicht tragen,", "tokens": ["Und", "sollt'", "es", "Zin\u00b7sen", "dir", "in", "ei\u00b7ner", "Zeit", "nicht", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werden sie dir nur zum Kapital geschlagen.", "tokens": ["So", "wer\u00b7den", "sie", "dir", "nur", "zum", "Ka\u00b7pi\u00b7tal", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-