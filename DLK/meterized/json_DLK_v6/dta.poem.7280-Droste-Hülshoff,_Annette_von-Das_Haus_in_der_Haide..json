{"dta.poem.7280": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Haus in der Haide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lauscht, vom Abendschein umzuckt,", "tokens": ["Wie", "lauscht", ",", "vom", "A\u00b7bend\u00b7schein", "um\u00b7zuckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die strohgedeckte H\u00fctte,", "tokens": ["Die", "stroh\u00b7ge\u00b7deck\u00b7te", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u2014 Recht wie im Nest der Vogel duckt, \u2014", "tokens": ["Recht", "wie", "im", "Nest", "der", "Vo\u00b7gel", "duckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "KOKOM", "APPRART", "NN", "ART", "NE", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dunkler F\u00f6hren Mitte.", "tokens": ["Aus", "dunk\u00b7ler", "F\u00f6h\u00b7ren", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Fensterloche streckt das Haupt", "tokens": ["Am", "Fens\u00b7ter\u00b7lo\u00b7che", "streckt", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wei\u00dfgestirnte St\u00e4rke,", "tokens": ["Die", "wei\u00df\u00b7ges\u00b7tirn\u00b7te", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bl\u00e4\u00dft in den Abendduft und schnaubt", "tokens": ["Bl\u00e4\u00dft", "in", "den", "A\u00b7bend\u00b7duft", "und", "schnaubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und st\u00f6\u00dft an's Holzgewerke.", "tokens": ["Und", "st\u00f6\u00dft", "an's", "Holz\u00b7ge\u00b7wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seitab ein G\u00e4rtchen, dornumhegt,", "tokens": ["Seit\u00b7ab", "ein", "G\u00e4rt\u00b7chen", ",", "dor\u00b7num\u00b7hegt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit reinlichem Gel\u00e4nde,", "tokens": ["Mit", "rein\u00b7li\u00b7chem", "Ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo matt ihr Haupt die Glocke tr\u00e4gt,", "tokens": ["Wo", "matt", "ihr", "Haupt", "die", "Glo\u00b7cke", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufrecht die Sonnenwende.", "tokens": ["Auf\u00b7recht", "die", "Son\u00b7nen\u00b7wen\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und drinnen kniet ein stilles Kind,", "tokens": ["Und", "drin\u00b7nen", "kniet", "ein", "stil\u00b7les", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das scheint den Grund zu j\u00e4ten,", "tokens": ["Das", "scheint", "den", "Grund", "zu", "j\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun pfl\u00fcckt sie eine Lilie lind", "tokens": ["Nun", "pfl\u00fcckt", "sie", "ei\u00b7ne", "Li\u00b7lie", "lind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandelt l\u00e4ngs den Beeten.", "tokens": ["Und", "wan\u00b7delt", "l\u00e4ngs", "den", "Bee\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Am Horizonte Hirten, die", "tokens": ["Am", "Ho\u00b7ri\u00b7zon\u00b7te", "Hir\u00b7ten", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Haidekraut sich strecken,", "tokens": ["Im", "Hai\u00b7de\u00b7kraut", "sich", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit des Aves Melodie", "tokens": ["Und", "mit", "des", "A\u00b7ves", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4umende L\u00fcfte wecken.", "tokens": ["Tr\u00e4u\u00b7men\u00b7de", "L\u00fcf\u00b7te", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und von der Tenne ab und an", "tokens": ["Und", "von", "der", "Ten\u00b7ne", "ab", "und", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schallt es wie Hammerschl\u00e4ge,", "tokens": ["Schallt", "es", "wie", "Ham\u00b7mer\u00b7schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Hobel rauscht, es f\u00e4llt der Span,", "tokens": ["Der", "Ho\u00b7bel", "rauscht", ",", "es", "f\u00e4llt", "der", "Span", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und langsam knarrt die S\u00e4ge.", "tokens": ["Und", "lang\u00b7sam", "knarrt", "die", "S\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da hebt der Abendstern gemach", "tokens": ["Da", "hebt", "der", "A\u00b7bends\u00b7tern", "ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-----+", "measure": "dactylic.init"}, "line.2": {"text": "Sich aus den F\u00f6hrenzweigen,", "tokens": ["Sich", "aus", "den", "F\u00f6h\u00b7ren\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und grade ob der H\u00fctte Dach", "tokens": ["Und", "gra\u00b7de", "ob", "der", "H\u00fct\u00b7te", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint er sich mild zu neigen.", "tokens": ["Scheint", "er", "sich", "mild", "zu", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es ist ein Bild, wie still und hei\u00df", "tokens": ["Es", "ist", "ein", "Bild", ",", "wie", "still", "und", "hei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es alte Meister hegten,", "tokens": ["Es", "al\u00b7te", "Meis\u00b7ter", "heg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kunstvolle M\u00f6nche, und mit Flei\u00df", "tokens": ["Kunst\u00b7vol\u00b7le", "M\u00f6n\u00b7che", ",", "und", "mit", "Flei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Es auf den Goldgrund legten.", "tokens": ["Es", "auf", "den", "Gold\u00b7grund", "leg\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Zimmermann \u2014 die Hirten gleich", "tokens": ["Der", "Zim\u00b7mer\u00b7mann", "die", "Hir\u00b7ten", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit ihrem frommen Liede \u2014", "tokens": ["Mit", "ih\u00b7rem", "from\u00b7men", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfrau mit dem Lilienzweig \u2014", "tokens": ["Die", "Jung\u00b7frau", "mit", "dem", "Li\u00b7li\u00b7en\u00b7zweig"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-++--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und rings der Gottesfriede.", "tokens": ["Und", "rings", "der", "Got\u00b7tes\u00b7frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Des Sternes wunderlich Geleucht", "tokens": ["Des", "Ster\u00b7nes", "wun\u00b7der\u00b7lich", "Ge\u00b7leucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus zarten Wolkenfloren \u2014", "tokens": ["Aus", "zar\u00b7ten", "Wol\u00b7ken\u00b7flo\u00b7ren"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist etwa hier im Stall vielleicht", "tokens": ["Ist", "et\u00b7wa", "hier", "im", "Stall", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Christkindlein heut geboren?", "tokens": ["Christ\u00b7kin\u00b7dlein", "heut", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}}}}