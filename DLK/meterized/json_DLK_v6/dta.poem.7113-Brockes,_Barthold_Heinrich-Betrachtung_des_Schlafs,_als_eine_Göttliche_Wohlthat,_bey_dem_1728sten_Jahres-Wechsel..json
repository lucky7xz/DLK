{"dta.poem.7113": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! lasst uns in der Zeit und ihren Wechsel mercken", "tokens": ["Auf", "!", "lasst", "uns", "in", "der", "Zeit", "und", "ih\u00b7ren", "Wech\u00b7sel", "mer\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf unsre halbe Zeit. Es ist die dunckle Nacht,", "tokens": ["Auf", "uns\u00b7re", "hal\u00b7be", "Zeit", ".", "Es", "ist", "die", "dunck\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist der s\u00fcsse Schlaf, der uns des Sch\u00f6pffers Macht,", "tokens": ["Es", "ist", "der", "s\u00fcs\u00b7se", "Schlaf", ",", "der", "uns", "des", "Sch\u00f6pf\u00b7fers", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Huld, Weisheit, Lieb\u2019 und Gr\u00f6ss\u2019, in Seinen Wunderwercken,", "tokens": ["Huld", ",", "Weis\u00b7heit", ",", "Lieb'", "und", "Gr\u00f6ss'", ",", "in", "Sei\u00b7nen", "Wun\u00b7der\u00b7wer\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aufs allerdeutlichste recht \u00fcberzeuglich weisst.", "tokens": ["Aufs", "al\u00b7ler\u00b7deut\u00b7lichs\u00b7te", "recht", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "weisst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie, da\u00df man Jhn denn nicht erkennt und Jhn nicht preis\u2019t!", "tokens": ["Wie", ",", "da\u00df", "man", "Jhn", "denn", "nicht", "er\u00b7kennt", "und", "Jhn", "nicht", "preis't", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PIS", "PPER", "ADV", "PTKNEG", "VVFIN", "KON", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}