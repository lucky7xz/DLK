{"textgrid.poem.42539": {"metadata": {"author": {"name": "Cronegk, Johann Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du, der Musen Geschenk, Gef\u00e4hrtin der fr\u00f6hlichen Jugend,", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, der Musen Geschenk, Gef\u00e4hrtin der fr\u00f6hlichen Jugend,", "tokens": ["O", "du", ",", "der", "Mu\u00b7sen", "Ge\u00b7schenk", ",", "Ge\u00b7f\u00e4hr\u00b7tin", "der", "fr\u00f6h\u00b7li\u00b7chen", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ert\u00f6ne mir, tr\u00f6stende Leier, wie sonst", "tokens": ["Er\u00b7t\u00f6\u00b7ne", "mir", ",", "tr\u00f6s\u00b7ten\u00b7de", "Lei\u00b7er", ",", "wie", "sonst"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "PWAV", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und treibe mit m\u00e4chtigem Klang die Herde der st\u00fcrmischen Sorgen", "tokens": ["Und", "trei\u00b7be", "mit", "m\u00e4ch\u00b7ti\u00b7gem", "Klang", "die", "Her\u00b7de", "der", "st\u00fcr\u00b7mi\u00b7schen", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Aus meiner ver\u00f6deten Seele hinaus!", "tokens": ["Aus", "mei\u00b7ner", "ver\u00b7\u00f6\u00b7de\u00b7ten", "See\u00b7le", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die finstere Schwermut umh\u00fcllt die Stirne des traurigen J\u00fcnglings,", "tokens": ["Die", "fins\u00b7te\u00b7re", "Schwer\u00b7mut", "um\u00b7h\u00fcllt", "die", "Stir\u00b7ne", "des", "trau\u00b7ri\u00b7gen", "J\u00fcng\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der sonst, Kam\u00f6nen! euch singend gefiel.", "tokens": ["Der", "sonst", ",", "Ka\u00b7m\u00f6\u00b7nen", "!", "euch", "sin\u00b7gend", "ge\u00b7fiel", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "NN", "$.", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Bl\u00fcte der Jugend verwelkt: so sinket die sterbende Rose,", "tokens": ["Die", "Bl\u00fc\u00b7te", "der", "Ju\u00b7gend", "ver\u00b7welkt", ":", "so", "sin\u00b7ket", "die", "ster\u00b7ben\u00b7de", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Um welche sonst Zephir sanft lispelnd gescherzt.", "tokens": ["Um", "wel\u00b7che", "sonst", "Ze\u00b7phir", "sanft", "lis\u00b7pelnd", "ge\u00b7scherzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "In traurigen Schlummer versenkt, verkennt sich die zweifelnde Seele;", "tokens": ["In", "trau\u00b7ri\u00b7gen", "Schlum\u00b7mer", "ver\u00b7senkt", ",", "ver\u00b7kennt", "sich", "die", "zwei\u00b7feln\u00b7de", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie f\u00fchlet die Triebe der Freuden nicht mehr:", "tokens": ["Sie", "f\u00fch\u00b7let", "die", "Trie\u00b7be", "der", "Freu\u00b7den", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie f\u00fchlet nicht einmal den Schmerz; oft ist es ein Trost, ihn zu f\u00fchlen:", "tokens": ["Sie", "f\u00fch\u00b7let", "nicht", "ein\u00b7mal", "den", "Schmerz", ";", "oft", "ist", "es", "ein", "Trost", ",", "ihn", "zu", "f\u00fch\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Es lindern ihn z\u00e4rtliche Tr\u00e4nen alsdann.", "tokens": ["Es", "lin\u00b7dern", "ihn", "z\u00e4rt\u00b7li\u00b7che", "Tr\u00e4\u00b7nen", "als\u00b7dann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Auch dieser schwerm\u00fctige Trost ist itzt meinem Kummer versaget;", "tokens": ["Auch", "die\u00b7ser", "schwer\u00b7m\u00fc\u00b7ti\u00b7ge", "Trost", "ist", "itzt", "mei\u00b7nem", "Kum\u00b7mer", "ver\u00b7sa\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-+--+-", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Ich bin, ach! ich bin euer Thyrsis nicht mehr.", "tokens": ["Ich", "bin", ",", "ach", "!", "ich", "bin", "eu\u00b7er", "Thyr\u00b7sis", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ITJ", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr Freund', beklagt ihn! Er starb! Und was ihr noch itzt f\u00fcr ihn haltet,", "tokens": ["Ihr", "Freund'", ",", "be\u00b7klagt", "ihn", "!", "Er", "starb", "!", "Und", "was", "ihr", "noch", "itzt", "f\u00fcr", "ihn", "hal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$.", "KON", "PWS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ist blo\u00df nur sein Schatten, ist Thyrsis nicht mehr.", "tokens": ["Ist", "blo\u00df", "nur", "sein", "Schat\u00b7ten", ",", "ist", "Thyr\u00b7sis", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "VAFIN", "NE", "PTKNEG", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Mich rufet der Nachtigall Lied nicht mehr in die dichtrischen Haine,", "tokens": ["Mich", "ru\u00b7fet", "der", "Nach\u00b7ti\u00b7gall", "Lied", "nicht", "mehr", "in", "die", "dich\u00b7tri\u00b7schen", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo sonst mich sanft rauschende Nymphen belauscht.", "tokens": ["Wo", "sonst", "mich", "sanft", "rau\u00b7schen\u00b7de", "Nym\u00b7phen", "be\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun bl\u00fcht mehr kein Fr\u00fchling f\u00fcr mich! nun winkt kein gef\u00e4lliges M\u00e4gdchen", "tokens": ["Nun", "bl\u00fcht", "mehr", "kein", "Fr\u00fch\u00b7ling", "f\u00fcr", "mich", "!", "nun", "winkt", "kein", "ge\u00b7f\u00e4l\u00b7li\u00b7ges", "M\u00e4gd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PPER", "$.", "ADV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und l\u00e4chelt mir Wollust und Heiterkeit zu.", "tokens": ["Und", "l\u00e4\u00b7chelt", "mir", "Wol\u00b7lust", "und", "Hei\u00b7ter\u00b7keit", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+-++", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Es ruft mir Ly\u00e4us umsonst, der B\u00e4ndiger sterblicher Sorgen;", "tokens": ["Es", "ruft", "mir", "Ly\u00b7\u00e4us", "um\u00b7sonst", ",", "der", "B\u00e4n\u00b7di\u00b7ger", "sterb\u00b7li\u00b7cher", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Wein ist f\u00fcr freudige Herzen gemacht.", "tokens": ["Der", "Wein", "ist", "f\u00fcr", "freu\u00b7di\u00b7ge", "Her\u00b7zen", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Verla\u00dft mich, ihr Freunde, verla\u00dft den nicht mehr geselligen Thyrsis,", "tokens": ["Ver\u00b7la\u00dft", "mich", ",", "ihr", "Freun\u00b7de", ",", "ver\u00b7la\u00dft", "den", "nicht", "mehr", "ge\u00b7sel\u00b7li\u00b7gen", "Thyr\u00b7sis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "PTKNEG", "ADV", "VVPP", "NE", "$,"], "meter": "-+--+--+--+-+-+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Verla\u00dft ihn im schwermutsvoll einsamen Hain!", "tokens": ["Ver\u00b7la\u00dft", "ihn", "im", "schwer\u00b7muts\u00b7voll", "ein\u00b7sa\u00b7men", "Hain", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Wohin sind die Stunden nunmehr, die scherzenden Stunden der Jugend,", "tokens": ["Wo\u00b7hin", "sind", "die", "Stun\u00b7den", "nun\u00b7mehr", ",", "die", "scher\u00b7zen\u00b7den", "Stun\u00b7den", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In denen ich nichts als nur Freude gef\u00fchlt?", "tokens": ["In", "de\u00b7nen", "ich", "nichts", "als", "nur", "Freu\u00b7de", "ge\u00b7f\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "KOKOM", "ADV", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wohin sind die Rosen nunmehr, mit denen ich stolz mich bekr\u00f6nte,", "tokens": ["Wo\u00b7hin", "sind", "die", "Ro\u00b7sen", "nun\u00b7mehr", ",", "mit", "de\u00b7nen", "ich", "stolz", "mich", "be\u00b7kr\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weil Doris mir diese Rosen gepfl\u00fcckt?", "tokens": ["Weil", "Do\u00b7ris", "mir", "die\u00b7se", "Ro\u00b7sen", "ge\u00b7pfl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich fand in der l\u00e4chelnden Flur die Spuren der ewigen Vorsicht;", "tokens": ["Ich", "fand", "in", "der", "l\u00e4\u00b7cheln\u00b7den", "Flur", "die", "Spu\u00b7ren", "der", "e\u00b7wi\u00b7gen", "Vor\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit freudigen Tr\u00e4nen besang ich ihr Lob.", "tokens": ["Mit", "freu\u00b7di\u00b7gen", "Tr\u00e4\u00b7nen", "be\u00b7sang", "ich", "ihr", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich suchte die Weisheit nicht erst; sie kam und besuchte mich selbsten:", "tokens": ["Ich", "such\u00b7te", "die", "Weis\u00b7heit", "nicht", "erst", ";", "sie", "kam", "und", "be\u00b7such\u00b7te", "mich", "selbs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Itzt such' ich sie sehnlich; sie fliehet vor mir.", "tokens": ["Itzt", "such'", "ich", "sie", "sehn\u00b7lich", ";", "sie", "flie\u00b7het", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Sie liebet den einsamen Hain, sie liebet unschuldige Herzen,", "tokens": ["Sie", "lie\u00b7bet", "den", "ein\u00b7sa\u00b7men", "Hain", ",", "sie", "lie\u00b7bet", "un\u00b7schul\u00b7di\u00b7ge", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die keine Begierde noch st\u00fcrmisch bewegt:", "tokens": ["Die", "kei\u00b7ne", "Be\u00b7gier\u00b7de", "noch", "st\u00fcr\u00b7misch", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie flieht vor der l\u00e4rmenden Pracht, sie flieht vor der flei\u00dfigen Ehrsucht,", "tokens": ["Sie", "flieht", "vor", "der", "l\u00e4r\u00b7men\u00b7den", "Pracht", ",", "sie", "flieht", "vor", "der", "flei\u00b7\u00dfi\u00b7gen", "Ehr\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--++--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die nach ihr bei n\u00e4chtlichen Lampen oft strebt.", "tokens": ["Die", "nach", "ihr", "bei", "n\u00e4cht\u00b7li\u00b7chen", "Lam\u00b7pen", "oft", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "O Ruhe! mein voriges Gl\u00fcck, Gespielin der l\u00e4chelnden Jahre,", "tokens": ["O", "Ru\u00b7he", "!", "mein", "vo\u00b7ri\u00b7ges", "Gl\u00fcck", ",", "Ge\u00b7spie\u00b7lin", "der", "l\u00e4\u00b7cheln\u00b7den", "Jah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die sonst von der Muse vers\u00fc\u00dft mir entflohn!", "tokens": ["Die", "sonst", "von", "der", "Mu\u