{"textgrid.poem.33628": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Nebeltag", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun weicht er nicht mehr von der Erde,", "tokens": ["Nun", "weicht", "er", "nicht", "mehr", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der graue Nebel, unbewegt;", "tokens": ["Der", "grau\u00b7e", "Ne\u00b7bel", ",", "un\u00b7be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er deckt das Feld und deckt die Herde,", "tokens": ["Er", "deckt", "das", "Feld", "und", "deckt", "die", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Wald und was im Wald sich regt.", "tokens": ["Den", "Wald", "und", "was", "im", "Wald", "sich", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "KON", "PWS", "APPRART", "NN", "PRF", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er f\u00e4llt des Nachts in schweren Tropfen", "tokens": ["Er", "f\u00e4llt", "des", "Nachts", "in", "schwe\u00b7ren", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs welke Laub von Baum zu Baum,", "tokens": ["Durchs", "wel\u00b7ke", "Laub", "von", "Baum", "zu", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wollten Elfengeister klopfen", "tokens": ["Als", "woll\u00b7ten", "El\u00b7fen\u00b7geis\u00b7ter", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sommer wach aus seinem Traum.", "tokens": ["Den", "Som\u00b7mer", "wach", "aus", "sei\u00b7nem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der aber schl\u00e4ft, von k\u00fchlen Schauern", "tokens": ["Der", "a\u00b7ber", "schl\u00e4ft", ",", "von", "k\u00fch\u00b7len", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tief eingeh\u00fcllt, im Totenkleid.", "tokens": ["Tief", "ein\u00b7ge\u00b7h\u00fcllt", ",", "im", "To\u00b7ten\u00b7kleid", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O welch ein stilles, sanftes Trauern", "tokens": ["O", "welch", "ein", "stil\u00b7les", ",", "sanf\u00b7tes", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschleicht das Herz in dieser Zeit!", "tokens": ["Be\u00b7schleicht", "das", "Herz", "in", "die\u00b7ser", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Grund der Seele winkt es leise,", "tokens": ["Im", "Grund", "der", "See\u00b7le", "winkt", "es", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und vom dahingeschwundnen Gl\u00fcck", "tokens": ["Und", "vom", "da\u00b7hin\u00b7ge\u00b7schwund\u00b7nen", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beschw\u00f6rt in ihrem Zauberkreise", "tokens": ["Be\u00b7schw\u00f6rt", "in", "ih\u00b7rem", "Zau\u00b7ber\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erinnrung uns den Traum zur\u00fcck.", "tokens": ["E\u00b7rinn\u00b7rung", "uns", "den", "Traum", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}