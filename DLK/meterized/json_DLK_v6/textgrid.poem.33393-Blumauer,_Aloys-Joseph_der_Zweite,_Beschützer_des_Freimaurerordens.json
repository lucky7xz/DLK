{"textgrid.poem.33393": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Joseph der Zweite, Besch\u00fctzer des Freimaurerordens", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, in Joseph's grossen, weiten Staaten,", "tokens": ["Seht", ",", "in", "Jo\u00b7se\u00b7ph's", "gros\u00b7sen", ",", "wei\u00b7ten", "Staa\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wo, verm\u00e4hlet durch der Weisheit Hand,", "tokens": ["Wo", ",", "ver\u00b7m\u00e4h\u00b7let", "durch", "der", "Weis\u00b7heit", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Duldung sich und edle Freiheit gatten,", "tokens": ["Dul\u00b7dung", "sich", "und", "ed\u00b7le", "Frei\u00b7heit", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Nacht der Vorurtheile schwand,", "tokens": ["Und", "die", "Nacht", "der", "Vor\u00b7urt\u00b7hei\u00b7le", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hebt in heller, nun entschlei'rter Klarheit", "tokens": ["Hebt", "in", "hel\u00b7ler", ",", "nun", "ent\u00b7schlei'r\u00b7ter", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eine Br\u00fcderschaft ihr Haupt empor,", "tokens": ["Ei\u00b7ne", "Br\u00fc\u00b7der\u00b7schaft", "ihr", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die im Stillen Wohlthun nur und Wahrheit", "tokens": ["Die", "im", "Stil\u00b7len", "Wohl\u00b7thun", "nur", "und", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sich zu ihrer Arbeit Zweck erkohr.", "tokens": ["Sich", "zu", "ih\u00b7rer", "Ar\u00b7beit", "Zweck", "er\u00b7kohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Joseph, dem in seinem Herrscherkreise", "tokens": ["Jo\u00b7se\u00b7ph", ",", "dem", "in", "sei\u00b7nem", "Herr\u00b7scher\u00b7krei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Nichts zu gro\u00df ist, das sein Geist nicht fa\u00dft,", "tokens": ["Nichts", "zu", "gro\u00df", "ist", ",", "das", "sein", "Geist", "nicht", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKA", "ADJD", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nichts zu klein, das er nicht minder weise", "tokens": ["Nichts", "zu", "klein", ",", "das", "er", "nicht", "min\u00b7der", "wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "PTKA", "ADJD", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ordnet, und in seine Plane pa\u00dft;", "tokens": ["Ord\u00b7net", ",", "und", "in", "sei\u00b7ne", "Pla\u00b7ne", "pa\u00dft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Joseph, der so eben von den Horden", "tokens": ["Jo\u00b7se\u00b7ph", ",", "der", "so", "e\u00b7ben", "von", "den", "Hor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tr\u00e4ger M\u00f6nche seinen Staat befreit,", "tokens": ["Tr\u00e4\u00b7ger", "M\u00f6n\u00b7che", "sei\u00b7nen", "Staat", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sch\u00e4tzt und sch\u00fctzt daf\u00fcr nun einen Orden,", "tokens": ["Sch\u00e4tzt", "und", "sch\u00fctzt", "da\u00b7f\u00fcr", "nun", "ei\u00b7nen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PAV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der sich ganz dem Wohl der Menschheit weiht:", "tokens": ["Der", "sich", "ganz", "dem", "Wohl", "der", "Menschheit", "weiht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Einen Orden, den man oft verkannte,", "tokens": ["Ei\u00b7nen", "Or\u00b7den", ",", "den", "man", "oft", "ver\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weil er in geheim sein Gutes \u00fcbt,", "tokens": ["Weil", "er", "in", "ge\u00b7heim", "sein", "Gu\u00b7tes", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und erst j\u00fcngst aus einem Staat verbannte,", "tokens": ["Und", "erst", "j\u00fcngst", "aus", "ei\u00b7nem", "Staat", "ver\u00b7bann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo ein Exm\u00f6nch nun Gesetze gibt;", "tokens": ["Wo", "ein", "Ex\u00b7m\u00f6nch", "nun", "Ge\u00b7set\u00b7ze", "gibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Einen Orden, dem der Arme Segen,", "tokens": ["Ei\u00b7nen", "Or\u00b7den", ",", "dem", "der", "Ar\u00b7me", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fluch der Fr\u00f6mmler, Hohn der Laye spricht,", "tokens": ["Fluch", "der", "Fr\u00f6mm\u00b7ler", ",", "Hohn", "der", "La\u00b7ye", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der inde\u00df im Stillen sich dagegen", "tokens": ["Der", "in\u00b7de\u00df", "im", "Stil\u00b7len", "sich", "da\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "PRF", "PAV"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Einen Kranz von edlen Thaten flicht;", "tokens": ["Ei\u00b7nen", "Kranz", "von", "ed\u00b7len", "Tha\u00b7ten", "flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Einen Orden, den der M\u00f6nch zu schm\u00e4hen", "tokens": ["Ei\u00b7nen", "Or\u00b7den", ",", "den", "der", "M\u00f6nch", "zu", "schm\u00e4\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oder zu verdammen nie vergi\u00dft,", "tokens": ["O\u00b7der", "zu", "ver\u00b7dam\u00b7men", "nie", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weil sein Zweck nicht m\u00fcssig betteln gehen,", "tokens": ["Weil", "sein", "Zweck", "nicht", "m\u00fcs\u00b7sig", "bet\u00b7teln", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sondern Th\u00e4tigkeit und Wohlthun ist;", "tokens": ["Son\u00b7dern", "Th\u00e4\u00b7tig\u00b7keit", "und", "Wohl\u00b7thun", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Einen Orden, den der Heuchler scheuet,", "tokens": ["Ei\u00b7nen", "Or\u00b7den", ",", "den", "der", "Heuch\u00b7ler", "scheu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weil er ihm die schwarze Seel' entbl\u00f6\u00dft,", "tokens": ["Weil", "er", "ihm", "die", "schwar\u00b7ze", "Seel'", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wider den der Schurke tobt und schreiet,", "tokens": ["Wi\u00b7der", "den", "der", "Schur\u00b7ke", "tobt", "und", "schrei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil er ihn von sich zur\u00fccke st\u00f6\u00dft;", "tokens": ["Weil", "er", "ihn", "von", "sich", "zu\u00b7r\u00fc\u00b7cke", "st\u00f6\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Einen Orden, den als Staatsverr\u00e4ther", "tokens": ["Ei\u00b7nen", "Or\u00b7den", ",", "den", "als", "Staats\u00b7ver\u00b7r\u00e4\u00b7ther"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Verf\u00fchrer man schon oft bestraft;", "tokens": ["Und", "Ver\u00b7f\u00fch\u00b7rer", "man", "schon", "oft", "be\u00b7straft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIS", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hrend er der Unschuld treue Retter,", "tokens": ["W\u00e4h\u00b7rend", "er", "der", "Un\u00b7schuld", "treu\u00b7e", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dem Staate gute B\u00fcrger schafft.", "tokens": ["Und", "dem", "Staa\u00b7te", "gu\u00b7te", "B\u00fcr\u00b7ger", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Dieser Orden ist's, den, frei vom Wahne,", "tokens": ["Die\u00b7ser", "Or\u00b7den", "ist's", ",", "den", ",", "frei", "vom", "Wah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "$,", "ART", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Joseph seines Schutzes w\u00fcrdig fand,", "tokens": ["Jo\u00b7se\u00b7ph", "sei\u00b7nes", "Schut\u00b7zes", "w\u00fcr\u00b7dig", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und zu seinem weisen Herrscherplane,", "tokens": ["Und", "zu", "sei\u00b7nem", "wei\u00b7sen", "Herr\u00b7scher\u00b7pla\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ein Glied zur Kette, mit verband;", "tokens": ["Wie", "ein", "Glied", "zur", "Ket\u00b7te", ",", "mit", "ver\u00b7band", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,", "APPR", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Weil mit ihm der Orden, festen Blickes,", "tokens": ["Weil", "mit", "ihm", "der", "Or\u00b7den", ",", "fes\u00b7ten", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und von einem gleichen Geist belebt,", "tokens": ["Und", "von", "ei\u00b7nem", "glei\u00b7chen", "Geist", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zu dem grossen Zweck des Menschengl\u00fcckes,", "tokens": ["Zu", "dem", "gros\u00b7sen", "Zweck", "des", "Men\u00b7schen\u00b7gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hand in Hand hinan zu dringen strebt.", "tokens": ["Hand", "in", "Hand", "hi\u00b7nan", "zu", "drin\u00b7gen", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APZR", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "D'rum, ihr Br\u00fcder, lasset uns im Stillen", "tokens": ["D'\u00b7rum", ",", "ihr", "Br\u00fc\u00b7der", ",", "las\u00b7set", "uns", "im", "Stil\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht durch Worte, sondern auch durch That,", "tokens": ["Nicht", "durch", "Wor\u00b7te", ",", "son\u00b7dern", "auch", "durch", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "KON", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "All' die grossen Hoffnungen erf\u00fcllen,", "tokens": ["All'", "die", "gros\u00b7sen", "Hoff\u00b7nun\u00b7gen", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die von uns der grosse Weise hat!", "tokens": ["Die", "von", "uns", "der", "gros\u00b7se", "Wei\u00b7se", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "La\u00dft uns dankbar unsern Sch\u00fctzer preisen,", "tokens": ["La\u00dft", "uns", "dank\u00b7bar", "un\u00b7sern", "Sch\u00fct\u00b7zer", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ihm zeigen, da\u00df die Maurerrei", "tokens": ["Und", "ihm", "zei\u00b7gen", ",", "da\u00df", "die", "Mau\u00b7rer\u00b7rei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Werth der Achtung eines jeden Weisen,", "tokens": ["Werth", "der", "Ach\u00b7tung", "ei\u00b7nes", "je\u00b7den", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Werth des Schutzes eines Joseph's sei!", "tokens": ["Werth", "des", "Schut\u00b7zes", "ei\u00b7nes", "Jo\u00b7se\u00b7ph's", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NE", "VAFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}