{"textgrid.poem.63873": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' in strengem Musendienst mich redlich m\u00fcd' und hei\u00df gelebt", "tokens": ["Ich", "hab'", "in", "stren\u00b7gem", "Mu\u00b7sen\u00b7dienst", "mich", "red\u00b7lich", "m\u00fcd'", "und", "hei\u00df", "ge\u00b7lebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und andachtsvoll mein Leben lang in hoher Meister Kreis gelebt.", "tokens": ["Und", "an\u00b7dachts\u00b7voll", "mein", "Le\u00b7ben", "lang", "in", "ho\u00b7her", "Meis\u00b7ter", "Kreis", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "ADJD", "APPR", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Dem Wettlauf, dem banausischen, nach Gold und Ehren blieb ich fern", "tokens": ["Dem", "Wett\u00b7lauf", ",", "dem", "ba\u00b7nau\u00b7si\u00b7schen", ",", "nach", "Gold", "und", "Eh\u00b7ren", "blieb", "ich", "fern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und hab' im Stillen, lauschend auf des Genius Gehei\u00df, gelebt.", "tokens": ["Und", "hab'", "im", "Stil\u00b7len", ",", "lau\u00b7schend", "auf", "des", "Ge\u00b7nius", "Ge\u00b7hei\u00df", ",", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Der Sch\u00f6pfung tausendstimm'gen Chor, in meiner Klause h\u00f6rt' ich ihn", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "tau\u00b7send\u00b7stim\u00b7m'\u00b7gen", "Chor", ",", "in", "mei\u00b7ner", "Klau\u00b7se", "h\u00f6rt'", "ich", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Und glaubte, wenn ich niederschrieb, was in mir klang, so sei's gelebt,", "tokens": ["Und", "glaub\u00b7te", ",", "wenn", "ich", "nie\u00b7der\u00b7schrieb", ",", "was", "in", "mir", "klang", ",", "so", "sei's", "ge\u00b7lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Doch jetzt, am Ziel der langen Bahn, beschleicht mich Sorge: nicht genug", "tokens": ["Doch", "jetzt", ",", "am", "Ziel", "der", "lan\u00b7gen", "Bahn", ",", "be\u00b7schleicht", "mich", "Sor\u00b7ge", ":", "nicht", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "NN", "$.", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "H\u00e4tt' ich in freier Gotteswelt, zu viel nur schwarz auf wei\u00df gelebt.", "tokens": ["H\u00e4tt'", "ich", "in", "frei\u00b7er", "Got\u00b7tes\u00b7welt", ",", "zu", "viel", "nur", "schwarz", "auf", "wei\u00df", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "PTKA", "PIS", "ADV", "ADJD", "APPR", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}