{"textgrid.poem.54187": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man soll nichts tun, was einem nicht gem\u00e4\u00df ist.", "tokens": ["Man", "soll", "nichts", "tun", ",", "was", "ei\u00b7nem", "nicht", "ge\u00b7m\u00e4\u00df", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "VVINF", "$,", "PRELS", "ART", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Film . . . Was kann das schon sein, wenn es die Zensur erlaubt hat!", "tokens": ["Ein", "Film", ".", ".", ".", "Was", "kann", "das", "schon", "sein", ",", "wenn", "es", "die", "Zen\u00b7sur", "er\u00b7laubt", "hat", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$.", "$.", "PWS", "VMFIN", "PDS", "ADV", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Die meisten Hotels verkaufen etwas, was sie gar nicht haben: Ruhe.", "tokens": ["Die", "meis\u00b7ten", "Ho\u00b7tels", "ver\u00b7kau\u00b7fen", "et\u00b7was", ",", "was", "sie", "gar", "nicht", "ha\u00b7ben", ":", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PIS", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VAINF", "$.", "NN", "$."], "meter": "-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Dieser Schriftsteller schreibt einen l\u00e4ufigen Stil.", "tokens": ["Die\u00b7ser", "Schrift\u00b7stel\u00b7ler", "schreibt", "ei\u00b7nen", "l\u00e4u\u00b7fi\u00b7gen", "Stil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Den Menschen aus der Seele zu schreiben \u2013: das k\u00f6nnte eine Aufgabe sein.", "tokens": ["Den", "Men\u00b7schen", "aus", "der", "See\u00b7le", "zu", "schrei\u00b7ben", "\u2013", ":", "das", "k\u00f6nn\u00b7te", "ei\u00b7ne", "Auf\u00b7ga\u00b7be", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$(", "$.", "PDS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+--+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}}}}}