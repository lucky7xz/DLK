{"textgrid.poem.42906": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Freunde, die wir nie erlebten", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die nie ich sah,", "tokens": ["Ihr", ",", "die", "nie", "ich", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nimmer menschlich sehe,", "tokens": ["Nim\u00b7mer", "menschlich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Seid mir nun so nah,", "tokens": ["Seid", "mir", "nun", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn ich einsam gehe.", "tokens": ["Wenn", "ich", "ein\u00b7sam", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Was ich wei\u00df, nicht wu\u00dfte", "tokens": ["Was", "ich", "wei\u00df", ",", "nicht", "wu\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PTKNEG", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u00dcber euch, hab ich's vers\u00e4umt?", "tokens": ["\u00dc\u00b7ber", "euch", ",", "hab", "ich's", "ver\u00b7s\u00e4umt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VAFIN", "PIS", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ich's verfehlt? \u2013", "tokens": ["Ich's", "ver\u00b7fehlt", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "VVPP", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Oder mu\u00dfte", "tokens": ["O\u00b7der", "mu\u00df\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VMFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Fern vergehn, was ich ertr\u00e4umt? \u2013", "tokens": ["Fern", "ver\u00b7gehn", ",", "was", "ich", "er\u00b7tr\u00e4umt", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "$,", "PWS", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schenkte Gott die Kunst, das Wort", "tokens": ["Schenk\u00b7te", "Gott", "die", "Kunst", ",", "das", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferner, Toter nachzulesen.", "tokens": ["Fer\u00b7ner", ",", "To\u00b7ter", "nach\u00b7zu\u00b7le\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach wie hei\u00df mich das beschlich:", "tokens": ["Ach", "wie", "hei\u00df", "mich", "das", "be\u00b7schlich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "PPER", "ART", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann und dann und da und dort", "tokens": ["Dann", "und", "dann", "und", "da", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "KON", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ein Herz wie meins gewesen,", "tokens": ["Ist", "ein", "Herz", "wie", "meins", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "NE", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still f\u00fcr sich.", "tokens": ["Still", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Tr\u00f6stliches Gef\u00fchl: Es d\u00e4chte", "tokens": ["Tr\u00f6st\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", ":", "Es", "d\u00e4ch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00e4ter wer so \u00fcber mich. \u2013", "tokens": ["Sp\u00e4\u00b7ter", "wer", "so", "\u00fc\u00b7ber", "mich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PWS", "ADV", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine aller Erdenm\u00e4chte,", "tokens": ["Kei\u00b7ne", "al\u00b7ler", "Er\u00b7den\u00b7m\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r sie noch so \u00fcberm\u00fctig,", "tokens": ["W\u00e4r", "sie", "noch", "so", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann uns trennen,", "tokens": ["Kann", "uns", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Die wir Gleiche sind zu nennen.", "tokens": ["Die", "wir", "Glei\u00b7che", "sind", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn wir waren nie gesellt,", "tokens": ["Denn", "wir", "wa\u00b7ren", "nie", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil der Gott uns weise, g\u00fctig", "tokens": ["Weil", "der", "Gott", "uns", "wei\u00b7se", ",", "g\u00fc\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Fern vonander aufgestellt,", "tokens": ["Fern", "vo\u00b7nan\u00b7der", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wissend um die Welt.", "tokens": ["Wis\u00b7send", "um", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}