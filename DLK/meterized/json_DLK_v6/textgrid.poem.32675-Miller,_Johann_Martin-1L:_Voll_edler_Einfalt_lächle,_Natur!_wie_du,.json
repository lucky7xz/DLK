{"textgrid.poem.32675": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Voll edler Einfalt l\u00e4chle, Natur! wie du,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll edler Einfalt l\u00e4chle, Natur! wie du,", "tokens": ["Voll", "ed\u00b7ler", "Ein\u00b7falt", "l\u00e4ch\u00b7le", ",", "Na\u00b7tur", "!", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$,", "NN", "$.", "PWAV", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir einst das M\u00e4dchen, das sich mein Herz erkiest:", "tokens": ["Mir", "einst", "das", "M\u00e4d\u00b7chen", ",", "das", "sich", "mein", "Herz", "er\u00b7kiest", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sanft sei ihr himmelblaues Auge,", "tokens": ["Sanft", "sei", "ihr", "him\u00b7mel\u00b7blau\u00b7es", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sittsamkeit wohne darin, und Unschuld.", "tokens": ["Sitt\u00b7sam\u00b7keit", "woh\u00b7ne", "da\u00b7rin", ",", "und", "Un\u00b7schuld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$,", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nicht Flittergold und Puppentand liebe sie,", "tokens": ["Nicht", "Flit\u00b7ter\u00b7gold", "und", "Pup\u00b7pen\u00b7tand", "lie\u00b7be", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den, mit dem Keim des Lasters, Lutetien", "tokens": ["Den", ",", "mit", "dem", "Keim", "des", "Las\u00b7ters", ",", "Lu\u00b7te\u00b7ti\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "NE"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zuerst dem deutschen M\u00e4dchen sandte,", "tokens": ["Zu\u00b7erst", "dem", "deut\u00b7schen", "M\u00e4d\u00b7chen", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eh es der Unschuld Gewand verschm\u00e4hte!", "tokens": ["Eh", "es", "der", "Un\u00b7schuld", "Ge\u00b7wand", "ver\u00b7schm\u00e4h\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Zu gro\u00df, dem schn\u00f6den Schmeichler ihr Ohr zu leihn,", "tokens": ["Zu", "gro\u00df", ",", "dem", "schn\u00f6\u00b7den", "Schmeich\u00b7ler", "ihr", "Ohr", "zu", "leihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gefall' ihr mehr des J\u00fcnglings beredter Blick,", "tokens": ["Ge\u00b7fall'", "ihr", "mehr", "des", "J\u00fcng\u00b7lings", "be\u00b7red\u00b7ter", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der ihr in herzensvoller Sprache", "tokens": ["Der", "ihr", "in", "her\u00b7zens\u00b7vol\u00b7ler", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liebe gesteht, und um Liebe schmachtet.", "tokens": ["Lie\u00b7be", "ge\u00b7steht", ",", "und", "um", "Lie\u00b7be", "schmach\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Um schalen Scherz, und gl\u00fchenden Wechseltanz", "tokens": ["Um", "scha\u00b7len", "Scherz", ",", "und", "gl\u00fc\u00b7hen\u00b7den", "Wech\u00b7sel\u00b7tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verlasse niemals sie den belebten Hain,", "tokens": ["Ver\u00b7las\u00b7se", "nie\u00b7mals", "sie", "den", "be\u00b7leb\u00b7ten", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wo Nachtigallen im Geb\u00fcsche", "tokens": ["Wo", "Nach\u00b7ti\u00b7gal\u00b7len", "im", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott und den d\u00fcftenden Fr\u00fchling preisen.", "tokens": ["Gott", "und", "den", "d\u00fcf\u00b7ten\u00b7den", "Fr\u00fch\u00b7ling", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Im stillen \u00fcbe, wenigen nur bekannt,", "tokens": ["Im", "stil\u00b7len", "\u00fc\u00b7be", ",", "we\u00b7ni\u00b7gen", "nur", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sich in jeder weiblichen Tugend, dann", "tokens": ["Sie", "sich", "in", "je\u00b7der", "weib\u00b7li\u00b7chen", "Tu\u00b7gend", ",", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PRF", "APPR", "PIAT", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erschall' ein keusches Lied am Abend", "tokens": ["Er\u00b7schall'", "ein", "keu\u00b7sches", "Lied", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lieblich ins Silberget\u00f6n der Laute!", "tokens": ["Lieb\u00b7lich", "ins", "Sil\u00b7ber\u00b7ge\u00b7t\u00f6n", "der", "Lau\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ist dies, o Daphne, nicht dein geliebtes Bild?", "tokens": ["Ist", "dies", ",", "o", "Daph\u00b7ne", ",", "nicht", "dein", "ge\u00b7lieb\u00b7tes", "Bild", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "FM", "NN", "$,", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ja, dieses sagt mein klopfender Busen mir.", "tokens": ["Ja", ",", "die\u00b7ses", "sagt", "mein", "klop\u00b7fen\u00b7der", "Bu\u00b7sen", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du aber siehst mich an, und sinkest", "tokens": ["Du", "a\u00b7ber", "siehst", "mich", "an", ",", "und", "sin\u00b7kest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin an die Brust des geliebtern J\u00fcnglings!", "tokens": ["Hin", "an", "die", "Brust", "des", "ge\u00b7lieb\u00b7tern", "J\u00fcng\u00b7lings", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}