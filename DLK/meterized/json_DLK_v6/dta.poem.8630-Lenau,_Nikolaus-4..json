{"dta.poem.8630": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.57", "id:0.42"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Sonnenuntergang;", "tokens": ["Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Schwarze Wolken zieh'n,", "tokens": ["Schwar\u00b7ze", "Wol\u00b7ken", "zieh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "O wie schw\u00fcl und bang", "tokens": ["O", "wie", "schw\u00fcl", "und", "bang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Alle Winde flieh'n!", "tokens": ["Al\u00b7le", "Win\u00b7de", "flieh'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Durch den Himmel wild", "tokens": ["Durch", "den", "Him\u00b7mel", "wild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Jagen Blitze, bleich;", "tokens": ["Ja\u00b7gen", "Blit\u00b7ze", ",", "bleich", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihr verg\u00e4nglich Bild", "tokens": ["Ihr", "ver\u00b7g\u00e4ng\u00b7lich", "Bild"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wandelt durch den Teich.", "tokens": ["Wan\u00b7delt", "durch", "den", "Teich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie gewitterklar", "tokens": ["Wie", "ge\u00b7wit\u00b7ter\u00b7klar"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mein' ich dich zu seh'n,", "tokens": ["Mein'", "ich", "dich", "zu", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und dein langes Haar", "tokens": ["Und", "dein", "lan\u00b7ges", "Haar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Frei im Sturme weh'n!", "tokens": ["Frei", "im", "Stur\u00b7me", "weh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}