{"dta.poem.8824": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Doch wer verliebt ist und berauscht, der h\u00e4lt f\u00fcr einen", "tokens": ["Doch", "wer", "ver\u00b7liebt", "ist", "und", "be\u00b7rauscht", ",", "der", "h\u00e4lt", "f\u00fcr", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVPP", "VAFIN", "KON", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur da\u00df ich alt're, f\u00fchl' ich nun, da mich ein kalter", "tokens": ["Nur", "da\u00df", "ich", "alt'\u00b7re", ",", "f\u00fchl'", "ich", "nun", ",", "da", "mich", "ein", "kal\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADJA", "$,", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es machte sonst ein solcher Blick nur muth'ger und er-", "tokens": ["Es", "mach\u00b7te", "sonst", "ein", "sol\u00b7cher", "Blick", "nur", "muth'\u00b7ger", "und", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIAT", "NN", "ADV", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch senken alte W\u00fcnsche sich, so steigen neue wieder", "tokens": ["Doch", "sen\u00b7ken", "al\u00b7te", "W\u00fcn\u00b7sche", "sich", ",", "so", "stei\u00b7gen", "neu\u00b7e", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PRF", "$,", "ADV", "ADJA", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Verfolgen, wie ein Fliegenschwarm im Sommer, immer", "tokens": ["Ver\u00b7fol\u00b7gen", ",", "wie", "ein", "Flie\u00b7gen\u00b7schwarm", "im", "Som\u00b7mer", ",", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verm\u00f6cht' ich zu vertrau'n die Qual, die seufzend nun", "tokens": ["Ver\u00b7m\u00f6cht'", "ich", "zu", "ver\u00b7trau'n", "die", "Qual", ",", "die", "seuf\u00b7zend", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So tr\u00f6stete vielleicht ein Freund, ein redlicher und", "tokens": ["So", "tr\u00f6s\u00b7te\u00b7te", "viel\u00b7leicht", "ein", "Freund", ",", "ein", "red\u00b7li\u00b7cher", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "KON"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Die Guten lieb' ich allgesammt, und horche gern der", "tokens": ["Die", "Gu\u00b7ten", "lieb'", "ich", "all\u00b7ge\u00b7sammt", ",", "und", "hor\u00b7che", "gern", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch halt' ich freylich lieber stets zu luftigem Gelichter", "tokens": ["Doch", "halt'", "ich", "frey\u00b7lich", "lie\u00b7ber", "stets", "zu", "luf\u00b7ti\u00b7gem", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}