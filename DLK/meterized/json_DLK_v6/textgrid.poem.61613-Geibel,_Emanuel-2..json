{"textgrid.poem.61613": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen S\u00e4uleng\u00e4ngen,", "tokens": ["In", "die\u00b7sen", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo um vermorscht Gestein", "tokens": ["Wo", "um", "ver\u00b7morscht", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich tausend Bl\u00fcten dr\u00e4ngen,", "tokens": ["Sich", "tau\u00b7send", "Bl\u00fc\u00b7ten", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie tr\u00e4um' ich gern allein!", "tokens": ["Wie", "tr\u00e4um'", "ich", "gern", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit r\u00e4tselhaften Schauern", "tokens": ["Mit", "r\u00e4t\u00b7sel\u00b7haf\u00b7ten", "Schau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beklemmen hier die Brust", "tokens": ["Be\u00b7klem\u00b7men", "hier", "die", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erinnrungsvolles Trauern", "tokens": ["E\u00b7rinn\u00b7rungs\u00b7vol\u00b7les", "Trau\u00b7ern"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und reichste Jugendlust.", "tokens": ["Und", "reichs\u00b7te", "Ju\u00b7gend\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl klagt das Herz bek\u00fcmmert", "tokens": ["Wohl", "klagt", "das", "Herz", "be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um diese sch\u00f6ne Welt,", "tokens": ["Um", "die\u00b7se", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die rettungslos zertr\u00fcmmert", "tokens": ["Die", "ret\u00b7tungs\u00b7los", "zer\u00b7tr\u00fcm\u00b7mert"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gemach in Staub zerf\u00e4llt;", "tokens": ["Ge\u00b7mach", "in", "Staub", "zer\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch sp\u00fcr ich, von den D\u00fcften", "tokens": ["Doch", "sp\u00fcr", "ich", ",", "von", "den", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des jungen Tags umgl\u00fcht,", "tokens": ["Des", "jun\u00b7gen", "Tags", "um\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df auch auf G\u00f6ttergr\u00fcften", "tokens": ["Da\u00df", "auch", "auf", "G\u00f6t\u00b7ter\u00b7gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fr\u00fchling wieder bl\u00fcht.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wie\u00b7der", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Granaten bringt und Reben", "tokens": ["Gra\u00b7na\u00b7ten", "bringt", "und", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vers\u00f6hnend jedes Jahr,", "tokens": ["Ver\u00b7s\u00f6h\u00b7nend", "je\u00b7des", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00fc\u00df ist heut das Leben,", "tokens": ["Und", "s\u00fc\u00df", "ist", "heut", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wie's den Alten war.", "tokens": ["So", "wie's", "den", "Al\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach, w\u00e4re jener Sonnen", "tokens": ["Ach", ",", "w\u00e4\u00b7re", "je\u00b7ner", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erlauchtes Rosenlicht", "tokens": ["Er\u00b7lauch\u00b7tes", "Ro\u00b7sen\u00b7licht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht auch in Nacht zerronnen,", "tokens": ["Nicht", "auch", "in", "Nacht", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So liebt' ich heute nicht.", "tokens": ["So", "liebt'", "ich", "heu\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}