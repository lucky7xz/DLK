{"textgrid.poem.62722": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stamm-Buch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist ein sch\u00f6nes Frauenzimmer?", "tokens": ["Was", "ist", "ein", "sch\u00f6\u00b7nes", "Frau\u00b7en\u00b7zim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Antwort f\u00e4llt mir jetzo ein:", "tokens": ["Die", "Ant\u00b7wort", "f\u00e4llt", "mir", "jet\u00b7zo", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wird der Augen Himmel seyn,", "tokens": ["Es", "wird", "der", "Au\u00b7gen", "Him\u00b7mel", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zugleich des Beutels Fegefeuer,", "tokens": ["Zu\u00b7gleich", "des", "Beu\u00b7tels", "Fe\u00b7ge\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie auch der Seelen H\u00f6lle.", "tokens": ["Wie", "auch", "der", "See\u00b7len", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer tadelt nun ein solches Frauenzimmer?", "tokens": ["Wer", "ta\u00b7delt", "nun", "ein", "sol\u00b7ches", "Frau\u00b7en\u00b7zim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie? ist es nicht ein sch\u00f6nes Ungeheuer?", "tokens": ["Wie", "?", "ist", "es", "nicht", "ein", "sch\u00f6\u00b7nes", "Un\u00b7ge\u00b7heu\u00b7er", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}