{"textgrid.poem.27841": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist Feind? Der mir nicht g\u00fcnnet,", "tokens": ["Wer", "ist", "Feind", "?", "Der", "mir", "nicht", "g\u00fcn\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "ART", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir Gott und Gl\u00fccke schenckt,", "tokens": ["Was", "mir", "Gott", "und", "Gl\u00fc\u00b7cke", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der bey Tag und Nachte sinnet,", "tokens": ["Der", "bey", "Tag", "und", "Nach\u00b7te", "sin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er Hertz und Sinn mir kr\u00e4nckt,", "tokens": ["Wie", "er", "Hertz", "und", "Sinn", "mir", "kr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der nach meinem Leben trachtet,", "tokens": ["Der", "nach", "mei\u00b7nem", "Le\u00b7ben", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der nach meiner Wolfahrt strebt.", "tokens": ["Der", "nach", "mei\u00b7ner", "Wol\u00b7fahrt", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O, wird der f\u00fcr Feind geachtet,", "tokens": ["O", ",", "wird", "der", "f\u00fcr", "Feind", "ge\u00b7ach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ART", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So ist Feind, wer immer lebt.", "tokens": ["So", "ist", "Feind", ",", "wer", "im\u00b7mer", "lebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "PWS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}