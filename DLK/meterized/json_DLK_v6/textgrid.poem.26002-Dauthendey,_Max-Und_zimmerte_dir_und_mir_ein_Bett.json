{"textgrid.poem.26002": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und zimmerte dir und mir ein Bett", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schlug vom Weltenbaum ein Brett", "tokens": ["Ich", "schlug", "vom", "Wel\u00b7ten\u00b7baum", "ein", "Brett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zimmerte dir und mir ein Bett.", "tokens": ["Und", "zim\u00b7mer\u00b7te", "dir", "und", "mir", "ein", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Betten wuchsen gl\u00fchend zusammen,", "tokens": ["Die", "Bet\u00b7ten", "wuch\u00b7sen", "gl\u00fc\u00b7hend", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und drinnen wiegen sich lauter Flammen.", "tokens": ["Und", "drin\u00b7nen", "wie\u00b7gen", "sich", "lau\u00b7ter", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nicht Eisen, nicht Zeit kann die Betten je trennen,", "tokens": ["Nicht", "Ei\u00b7sen", ",", "nicht", "Zeit", "kann", "die", "Bet\u00b7ten", "je", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie werden hell durch die Ewigkeit brennen.", "tokens": ["Sie", "wer\u00b7den", "hell", "durch", "die", "E\u00b7wig\u00b7keit", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}