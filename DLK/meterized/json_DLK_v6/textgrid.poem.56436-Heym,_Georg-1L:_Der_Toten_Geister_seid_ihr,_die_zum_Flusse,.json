{"textgrid.poem.56436": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Toten Geister seid ihr, die zum Flusse,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Toten Geister seid ihr, die zum Flusse,", "tokens": ["Der", "To\u00b7ten", "Geis\u00b7ter", "seid", "ihr", ",", "die", "zum", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum \u00fcberladnen Kahn der Wesenlosen", "tokens": ["Zum", "\u00fc\u00b7berl\u00b7ad\u00b7nen", "Kahn", "der", "We\u00b7sen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Bote f\u00fchrt. Euer Rufen hallt im Tosen", "tokens": ["Der", "Bo\u00b7te", "f\u00fchrt", ".", "Eu\u00b7er", "Ru\u00b7fen", "hallt", "im", "To\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Sturms und in des Regens wildem Gusse.", "tokens": ["Des", "Sturms", "und", "in", "des", "Re\u00b7gens", "wil\u00b7dem", "Gus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Todes Banner wird im Zug getragen.", "tokens": ["Des", "To\u00b7des", "Ban\u00b7ner", "wird", "im", "Zug", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Heers carroccio f\u00fchrt die Wappentiere.", "tokens": ["Des", "Heers", "car\u00b7ro\u00b7ccio", "f\u00fchrt", "die", "Wap\u00b7pen\u00b7tie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und graunhaft wei\u00df ergl\u00e4nzen die Paniere,", "tokens": ["Und", "graun\u00b7haft", "wei\u00df", "er\u00b7gl\u00e4n\u00b7zen", "die", "Pa\u00b7nie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mit dem Saum die Horizonte schlagen.", "tokens": ["Die", "mit", "dem", "Saum", "die", "Ho\u00b7ri\u00b7zon\u00b7te", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es nahen M\u00f6nche, die in H\u00e4nden bergen", "tokens": ["Es", "na\u00b7hen", "M\u00f6n\u00b7che", ",", "die", "in", "H\u00e4n\u00b7den", "ber\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Totenlichter in den Prozessionen.", "tokens": ["Die", "To\u00b7ten\u00b7lich\u00b7ter", "in", "den", "Pro\u00b7zes\u00b7si\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf Toter Schultern morsche S\u00e4rge thronen.", "tokens": ["Auf", "To\u00b7ter", "Schul\u00b7tern", "mor\u00b7sche", "S\u00e4r\u00b7ge", "thro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Tote sitzen aufrecht in den S\u00e4rgen.", "tokens": ["Und", "To\u00b7te", "sit\u00b7zen", "auf\u00b7recht", "in", "den", "S\u00e4r\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ertrunkene kommen. Ungeborner Leichen.", "tokens": ["Er\u00b7trun\u00b7ke\u00b7ne", "kom\u00b7men", ".", "Un\u00b7ge\u00b7bor\u00b7ner", "Lei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gehenkte blaugeschn\u00fcrt. Die Hungers starben", "tokens": ["Ge\u00b7henk\u00b7te", "blau\u00b7ge\u00b7schn\u00fcrt", ".", "Die", "Hun\u00b7gers", "star\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Meeres fernen Inseln. Denen Narben", "tokens": ["Auf", "Mee\u00b7res", "fer\u00b7nen", "In\u00b7seln", ".", "De\u00b7nen", "Nar\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des schwarzen Todes umkr\u00e4nzen rings die Weichen.", "tokens": ["Des", "schwar\u00b7zen", "To\u00b7des", "um\u00b7kr\u00e4n\u00b7zen", "rings", "die", "Wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Es kommen Kinder in dem Zug der Toten,", "tokens": ["Es", "kom\u00b7men", "Kin\u00b7der", "in", "dem", "Zug", "der", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die eilend fliehn. Gel\u00e4hmte vorw\u00e4rts hasten.", "tokens": ["Die", "ei\u00b7lend", "fliehn", ".", "Ge\u00b7l\u00e4hm\u00b7te", "vor\u00b7w\u00e4rts", "has\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$.", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Blinden St\u00e4be nach dem Pfade tasten.", "tokens": ["Der", "Blin\u00b7den", "St\u00e4\u00b7be", "nach", "dem", "Pfa\u00b7de", "tas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schatten folgen schreiend dem stummen Boten.", "tokens": ["Die", "Schat\u00b7ten", "fol\u00b7gen", "schrei\u00b7end", "dem", "stum\u00b7men", "Bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Wie sich in Windes Maul des Laubes Tanz", "tokens": ["Wie", "sich", "in", "Win\u00b7des", "Maul", "des", "Lau\u00b7bes", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hindreht, wie Eulen auf dem schwarzen Flug,", "tokens": ["Hind\u00b7reht", ",", "wie", "Eu\u00b7len", "auf", "dem", "schwar\u00b7zen", "Flug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00e4lzt sich schnell der ungeheure Zug,", "tokens": ["So", "w\u00e4lzt", "sich", "schnell", "der", "un\u00b7ge\u00b7heu\u00b7re", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rot \u00fcberstrahlt von gro\u00dfer Fackeln Glanz.", "tokens": ["Rot", "\u00fc\u00b7bers\u00b7trahlt", "von", "gro\u00b7\u00dfer", "Fa\u00b7ckeln", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Auf Sch\u00e4deln trommeln laut die Musikanten,", "tokens": ["Auf", "Sch\u00e4\u00b7deln", "trom\u00b7meln", "laut", "die", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie die wei\u00dfen Segel bl\u00e4hn und knattern,", "tokens": ["Und", "wie", "die", "wei\u00b7\u00dfen", "Se\u00b7gel", "bl\u00e4hn", "und", "knat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bl\u00e4hn der Spieler Hemden sich und flattern.", "tokens": ["So", "bl\u00e4hn", "der", "Spie\u00b7ler", "Hem\u00b7den", "sich", "und", "flat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PRF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es fallen ein im Chore die Verbannten.", "tokens": ["Es", "fal\u00b7len", "ein", "im", "Cho\u00b7re", "die", "Ver\u00b7bann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das Lied braust machtvoll hin in seiner Qual,", "tokens": ["Das", "Lied", "braust", "macht\u00b7voll", "hin", "in", "sei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor der die Herzen durch die Rippen glimmen.", "tokens": ["Vor", "der", "die", "Her\u00b7zen", "durch", "die", "Rip\u00b7pen", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da kommt ein Haufe mit verwesten Stimmen,", "tokens": ["Da", "kommt", "ein", "Hau\u00b7fe", "mit", "ver\u00b7wes\u00b7ten", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Draus ragt ein hohes Kreuz zum Himmel fahl.", "tokens": ["Draus", "ragt", "ein", "ho\u00b7hes", "Kreuz", "zum", "Him\u00b7mel", "fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Kruzifixus ward einhergetragen.", "tokens": ["Der", "Kru\u00b7zi\u00b7fi\u00b7xus", "ward", "ein\u00b7her\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da hob der Sturm sich in der Toten Volke.", "tokens": ["Da", "hob", "der", "Sturm", "sich", "in", "der", "To\u00b7ten", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Meere scholl und aus dem Scho\u00df der Wolke", "tokens": ["Vom", "Mee\u00b7re", "scholl", "und", "aus", "dem", "Scho\u00df", "der", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein nimmer endend grauenvolles Klagen.", "tokens": ["Ein", "nim\u00b7mer", "en\u00b7dend", "grau\u00b7en\u00b7vol\u00b7les", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Es wurde dunkel in den grauen L\u00fcften.", "tokens": ["Es", "wur\u00b7de", "dun\u00b7kel", "in", "den", "grau\u00b7en", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es kam der Tod mit ungeheuren Schwingen.", "tokens": ["Es", "kam", "der", "Tod", "mit", "un\u00b7ge\u00b7heu\u00b7ren", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wurde Nacht, da noch die Wolken gingen", "tokens": ["Es", "wur\u00b7de", "Nacht", ",", "da", "noch", "die", "Wol\u00b7ken", "gin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Orkus zu, den ungeheuren Gr\u00fcften.", "tokens": ["Dem", "Or\u00b7kus", "zu", ",", "den", "un\u00b7ge\u00b7heu\u00b7ren", "Gr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}