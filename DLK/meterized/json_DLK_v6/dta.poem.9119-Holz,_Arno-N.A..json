{"dta.poem.9119": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ueberm Bett, eingerahmt, h\u00e4ngt der Myrthenkranz.", "tokens": ["Ue\u00b7berm", "Bett", ",", "ein\u00b7ge\u00b7rahmt", ",", "h\u00e4ngt", "der", "Myr\u00b7then\u00b7kranz", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vor Jahren", "tokens": ["Vor", "Jah\u00b7ren"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "stand am Fenster mal die N\u00e4hmaschine;", "tokens": ["stand", "am", "Fens\u00b7ter", "mal", "die", "N\u00e4h\u00b7ma\u00b7schi\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ein Kanarienvogel sang.", "tokens": ["ein", "Ka\u00b7na\u00b7ri\u00b7en\u00b7vo\u00b7gel", "sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Jetzt", "tokens": ["Jetzt"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "ist das alles anders!", "tokens": ["ist", "das", "al\u00b7les", "an\u00b7ders", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Abends,", "tokens": ["A\u00b7bends", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "wenn die rote Lampe brennt,", "tokens": ["wenn", "die", "ro\u00b7te", "Lam\u00b7pe", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "kommen fremde Herren in das St\u00fcbchen;", "tokens": ["kom\u00b7men", "frem\u00b7de", "Her\u00b7ren", "in", "das", "St\u00fcb\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "alte, junge, wies grad trifft.", "tokens": ["al\u00b7te", ",", "jun\u00b7ge", ",", "wies", "grad", "trifft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du lieber Gott \u2014 das Leben!", "tokens": ["Du", "lie\u00b7ber", "Gott", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$(", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nur manchmal,", "tokens": ["Nur", "manch\u00b7mal", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "wenn der Regen draussen auf die D\u00e4cher peitscht,", "tokens": ["wenn", "der", "Re\u00b7gen", "draus\u00b7sen", "auf", "die", "D\u00e4\u00b7cher", "peitscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "nachts,", "tokens": ["nachts", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "kein Mensch ist mehr wach,", "tokens": ["kein", "Mensch", "ist", "mehr", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "sitzt das Weib und weint . . .", "tokens": ["sitzt", "das", "Weib", "und", "weint", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Der tote Mann! Die armen Kinder!", "tokens": ["Der", "to\u00b7te", "Mann", "!", "Die", "ar\u00b7men", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}