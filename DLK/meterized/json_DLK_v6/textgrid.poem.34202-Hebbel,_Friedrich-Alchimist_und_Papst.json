{"textgrid.poem.34202": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Alchimist und Papst", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich hat er's entdeckt, das rings ersehnte Geheimni\u00df,", "tokens": ["End\u00b7lich", "hat", "er's", "ent\u00b7deckt", ",", "das", "rings", "er\u00b7sehn\u00b7te", "Ge\u00b7heim\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Gold zu machen, dem Papst bringt er das krause Recept.", "tokens": ["Gold", "zu", "ma\u00b7chen", ",", "dem", "Papst", "bringt", "er", "das", "krau\u00b7se", "Re\u00b7cept", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch der bed\u00e4chtige Greis l\u00f6s't l\u00e4chelnd den S\u00e4ckel vom G\u00fcrtel,", "tokens": ["Doch", "der", "be\u00b7d\u00e4ch\u00b7ti\u00b7ge", "Greis", "l\u00f6s't", "l\u00e4\u00b7chelnd", "den", "S\u00e4\u00b7ckel", "vom", "G\u00fcr\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Leert ihn und reicht ihn und spricht: nimm dir denn, was dir noch fehlt!", "tokens": ["Leert", "ihn", "und", "reicht", "ihn", "und", "spricht", ":", "nimm", "dir", "denn", ",", "was", "dir", "noch", "fehlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "KON", "VVFIN", "$.", "VVIMP", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}