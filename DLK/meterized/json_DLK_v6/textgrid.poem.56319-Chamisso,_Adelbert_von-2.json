{"textgrid.poem.56319": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbbald legt sich der Alte zur letzten Ruh", "tokens": ["\u00bb", "bald", "legt", "sich", "der", "Al\u00b7te", "zur", "letz\u00b7ten", "Ruh"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und f\u00e4llt sein brechendes Aug erst zu, \u2013", "tokens": ["Und", "f\u00e4llt", "sein", "bre\u00b7chen\u00b7des", "Aug", "erst", "zu", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "PTKVZ", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf welcher Seite sei das Recht, \u2013", "tokens": ["Auf", "wel\u00b7cher", "Sei\u00b7te", "sei", "das", "Recht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bin ich der Herr, so bist du der Knecht.\u00ab \u2013", "tokens": ["So", "bin", "ich", "der", "Herr", ",", "so", "bist", "du", "der", "Knecht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbdu, Doppelg\u00e4nger, bist mir fast,", "tokens": ["\u00bb", "du", ",", "Dop\u00b7pel\u00b7g\u00e4n\u00b7ger", ",", "bist", "mir", "fast", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wie ich dir, in der Seele verha\u00dft;", "tokens": ["So", "wie", "ich", "dir", ",", "in", "der", "See\u00b7le", "ver\u00b7ha\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schl\u00e4ft er... ich frage nach keinem Recht,", "tokens": ["Und", "schl\u00e4ft", "er", "...", "ich", "fra\u00b7ge", "nach", "kei\u00b7nem", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So bin ich der Herr, so bist du der Knecht.\u00ab \u2013", "tokens": ["So", "bin", "ich", "der", "Herr", ",", "so", "bist", "du", "der", "Knecht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbich bin der Graf, wer widersagt", "tokens": ["\u00bb", "ich", "bin", "der", "Graf", ",", "wer", "wi\u00b7der\u00b7sagt"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem hochgeborenen Herren? wer wagt", "tokens": ["Dem", "hoch\u00b7ge\u00b7bo\u00b7re\u00b7nen", "Her\u00b7ren", "?", "wer", "wagt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verblendet gegen mich den Raub?", "tokens": ["Ver\u00b7blen\u00b7det", "ge\u00b7gen", "mich", "den", "Raub", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor mir, Leibeigener, in den Staub!\u00ab \u2013", "tokens": ["Vor", "mir", ",", "Leib\u00b7ei\u00b7ge\u00b7ner", ",", "in", "den", "Staub", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbich bin der Graf und dulde hier", "tokens": ["\u00bb", "ich", "bin", "der", "Graf", "und", "dul\u00b7de", "hier"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein blasses Bild nicht neben mir;", "tokens": ["Dein", "blas\u00b7ses", "Bild", "nicht", "ne\u00b7ben", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werfe dich in den tiefsten Turm;", "tokens": ["Ich", "wer\u00b7fe", "dich", "in", "den", "tiefs\u00b7ten", "Turm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu meinen F\u00fc\u00dfen kreuch, du Wurm!\u00ab", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "kreuch", ",", "du", "Wurm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwenn schm\u00e4hen deine Zunge darf,", "tokens": ["\u00bb", "wenn", "schm\u00e4\u00b7hen", "dei\u00b7ne", "Zun\u00b7ge", "darf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADJA", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist doch dein Schwert viel minder scharf,", "tokens": ["Ist", "doch", "dein", "Schwert", "viel", "min\u00b7der", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst m\u00fc\u00dfte bald entschieden sein", "tokens": ["Sonst", "m\u00fc\u00df\u00b7te", "bald", "ent\u00b7schie\u00b7den", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl zwischen uns das Mein und Dein.\u00ab \u2013", "tokens": ["Wohl", "zwi\u00b7schen", "uns", "das", "Mein", "und", "Dein", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "KON", "PPOSAT", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbwas warten wir, da\u00df sein Auge bricht?", "tokens": ["\u00bb", "was", "war\u00b7ten", "wir", ",", "da\u00df", "sein", "Au\u00b7ge", "bricht", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich f\u00e4lle dich gleich, du B\u00f6sewicht!\u00ab \u2013", "tokens": ["Ich", "f\u00e4l\u00b7le", "dich", "gleich", ",", "du", "B\u00f6\u00b7se\u00b7wicht", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwas warten wir? das sprachst du gut;", "tokens": ["\u00bb", "was", "war\u00b7ten", "wir", "?", "das", "sprachst", "du", "gut", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich d\u00fcnge mein Land dein schwarzes Blut!\u00ab", "tokens": ["Gleich", "d\u00fcn\u00b7ge", "mein", "Land", "dein", "schwar\u00b7zes", "Blut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Vernahmst du, Graf, der Waffen Klang", "tokens": ["Ver\u00b7nahmst", "du", ",", "Graf", ",", "der", "Waf\u00b7fen", "Klang"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Hag her\u00fcber die Halle entlang?", "tokens": ["Vom", "Hag", "her\u00b7\u00fc\u00b7ber", "die", "Hal\u00b7le", "ent\u00b7lang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was tr\u00e4gt dein schwankender Fu\u00df dich dahin?", "tokens": ["Was", "tr\u00e4gt", "dein", "schwan\u00b7ken\u00b7der", "Fu\u00df", "dich", "da\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach! Unheil ahndet dein finstrer Sinn.", "tokens": ["Ach", "!", "Un\u00b7heil", "ahn\u00b7det", "dein", "finst\u00b7rer", "Sinn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Und \u00fcber zwei Leichen auf blutigem Grund,", "tokens": ["Und", "\u00fc\u00b7ber", "zwei", "Lei\u00b7chen", "auf", "blu\u00b7ti\u00b7gem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da ringt er verwaist die H\u00e4nde wund,", "tokens": ["Da", "ringt", "er", "ver\u00b7waist", "die", "H\u00e4n\u00b7de", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und weint die alten Augen blind,", "tokens": ["Und", "weint", "die", "al\u00b7ten", "Au\u00b7gen", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00fcttelt sein greises Haar in dem Wind.", "tokens": ["Und", "sch\u00fct\u00b7telt", "sein", "grei\u00b7ses", "Haar", "in", "dem", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}