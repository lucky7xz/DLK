{"textgrid.poem.43102": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es beugt das stolze Haupt sich schwer,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es beugt das stolze Haupt sich schwer,", "tokens": ["Es", "beugt", "das", "stol\u00b7ze", "Haupt", "sich", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwer der starre Sinn,", "tokens": ["Und", "schwer", "der", "star\u00b7re", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dennoch f\u00fchl' ich, da\u00df ich l\u00e4ngst", "tokens": ["Und", "den\u00b7noch", "f\u00fchl'", "ich", ",", "da\u00df", "ich", "l\u00e4ngst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr ich selber bin.", "tokens": ["Nicht", "mehr", "ich", "sel\u00b7ber", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, was noch kommen wird,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "noch", "kom\u00b7men", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist mir oft so bang;", "tokens": ["Doch", "ist", "mir", "oft", "so", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oft rei\u00dft mich dir zu F\u00fc\u00dfen hin", "tokens": ["Oft", "rei\u00dft", "mich", "dir", "zu", "F\u00fc\u00b7\u00dfen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein m\u00e4cht'ger Seelendrang.", "tokens": ["Ein", "m\u00e4cht'\u00b7ger", "See\u00b7len\u00b7drang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein Aug' ist treu; du siehst mich an", "tokens": ["Dein", "Aug'", "ist", "treu", ";", "du", "siehst", "mich", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So innig und so tief \u2013", "tokens": ["So", "in\u00b7nig", "und", "so", "tief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dennoch ist's, als ob hinweg", "tokens": ["Und", "den\u00b7noch", "ist's", ",", "als", "ob", "hin\u00b7weg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "$,", "KOKOM", "KOUS", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von dir mich mein Engel rief'.", "tokens": ["Von", "dir", "mich", "mein", "En\u00b7gel", "rie\u00b7f'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}