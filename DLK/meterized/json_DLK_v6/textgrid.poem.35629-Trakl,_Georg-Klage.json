{"textgrid.poem.35629": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Klage", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf und Tod, die d\u00fcstern Adler", "tokens": ["Schlaf", "und", "Tod", ",", "die", "d\u00fcs\u00b7tern", "Ad\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Umrauschen nachtlang dieses Haupt:", "tokens": ["Um\u00b7rau\u00b7schen", "nacht\u00b7lang", "die\u00b7ses", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Menschen goldnes Bildnis", "tokens": ["Des", "Men\u00b7schen", "gold\u00b7nes", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschl\u00e4nge die eisige Woge", "tokens": ["Ver\u00b7schl\u00e4n\u00b7ge", "die", "ei\u00b7si\u00b7ge", "Wo\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Ewigkeit. An schaurigen Riffen", "tokens": ["Der", "E\u00b7wig\u00b7keit", ".", "An", "schau\u00b7ri\u00b7gen", "Rif\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zerschellt der purpurne Leib", "tokens": ["Zer\u00b7schellt", "der", "pur\u00b7pur\u00b7ne", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und es klagt die dunkle Stimme", "tokens": ["Und", "es", "klagt", "die", "dunk\u00b7le", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber dem Meer.", "tokens": ["\u00dc\u00b7ber", "dem", "Meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Schwester st\u00fcrmischer Schwermut", "tokens": ["Schwes\u00b7ter", "st\u00fcr\u00b7mi\u00b7scher", "Schwer\u00b7mut"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Sieh ein \u00e4ngstlicher Kahn versinkt", "tokens": ["Sieh", "ein", "\u00e4ngst\u00b7li\u00b7cher", "Kahn", "ver\u00b7sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Unter Sternen,", "tokens": ["Un\u00b7ter", "Ster\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Dem schweigenden Antlitz der Nacht.", "tokens": ["Dem", "schwei\u00b7gen\u00b7den", "Ant\u00b7litz", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}