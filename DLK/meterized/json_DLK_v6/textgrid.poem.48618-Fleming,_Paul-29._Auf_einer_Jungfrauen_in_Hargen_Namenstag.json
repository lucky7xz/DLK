{"textgrid.poem.48618": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "29. Auf einer Jungfrauen in Hargen Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Stunden, s\u00fc\u00dfes Licht,", "tokens": ["Lie\u00b7be", "Stun\u00b7den", ",", "s\u00fc\u00b7\u00dfes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "das uns sch\u00f6ne Lust verspricht,", "tokens": ["das", "uns", "sch\u00f6\u00b7ne", "Lust", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "edler Tag, gew\u00fcndschter Schein,", "tokens": ["ed\u00b7ler", "Tag", ",", "ge\u00b7w\u00fcnd\u00b7schter", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "du solst mir willkommen sein!", "tokens": ["du", "solst", "mir", "will\u00b7kom\u00b7men", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sein solstu willkommen mir", "tokens": ["Sein", "sols\u00b7tu", "will\u00b7kom\u00b7men", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "PTKA", "ADJD", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fcr die angen\u00e4me Zier,", "tokens": ["f\u00fcr", "die", "an\u00b7ge\u00b7n\u00e4\u00b7me", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "f\u00fcr die Freude, die du giebst,", "tokens": ["f\u00fcr", "die", "Freu\u00b7de", ",", "die", "du", "giebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "weil du, was ich liebe, liebst!", "tokens": ["weil", "du", ",", "was", "ich", "lie\u00b7be", ",", "liebst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diese Jungfrau, der du scheinst,", "tokens": ["Die\u00b7se", "Jung\u00b7frau", ",", "der", "du", "scheinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "meinet dich, wie du sie meinst;", "tokens": ["mei\u00b7net", "dich", ",", "wie", "du", "sie", "meinst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sie will mit dir, wie du tust,", "tokens": ["sie", "will", "mit", "dir", ",", "wie", "du", "tust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "froh sein \u00fcber deiner Lust.", "tokens": ["froh", "sein", "\u00fc\u00b7ber", "dei\u00b7ner", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Scheine, sch\u00f6ner Sonnenschein,", "tokens": ["Schei\u00b7ne", ",", "sch\u00f6\u00b7ner", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "tausentmal so klar und rein,", "tokens": ["tau\u00b7sent\u00b7mal", "so", "klar", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "bleibe sch\u00f6ne wie du bist,", "tokens": ["blei\u00b7be", "sch\u00f6\u00b7ne", "wie", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bis da\u00df ganz kein Tag mehr ist!", "tokens": ["bis", "da\u00df", "ganz", "kein", "Tag", "mehr", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ADV", "PIAT", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So wird deiner Zierde Zier", "tokens": ["So", "wird", "dei\u00b7ner", "Zier\u00b7de", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "recht gepriesen sein in ihr.", "tokens": ["recht", "ge\u00b7prie\u00b7sen", "sein", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pflege sie, die Werte, die!", "tokens": ["Pfle\u00b7ge", "sie", ",", "die", "Wer\u00b7te", ",", "die", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "$,", "PRELS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Glanzes Glanz ist sie,", "tokens": ["Dei\u00b7nes", "Glan\u00b7zes", "Glanz", "ist", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}