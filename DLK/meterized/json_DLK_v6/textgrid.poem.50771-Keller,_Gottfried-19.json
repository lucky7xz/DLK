{"textgrid.poem.50771": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "19", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unverhofft nach tr\u00fcben Tagen", "tokens": ["Un\u00b7ver\u00b7hofft", "nach", "tr\u00fc\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der heitre Lenz erschienen,", "tokens": ["Ist", "der", "heit\u00b7re", "Lenz", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die aufgewachte Erde", "tokens": ["Und", "die", "auf\u00b7ge\u00b7wach\u00b7te", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberhaucht ein zartes Gr\u00fcnen;", "tokens": ["\u00dc\u00b7ber\u00b7haucht", "ein", "zar\u00b7tes", "Gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit bunten Sonnenschirmen", "tokens": ["Und", "mit", "bun\u00b7ten", "Son\u00b7nen\u00b7schir\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00e4dchen in den G\u00e4rten gehen,", "tokens": ["M\u00e4d\u00b7chen", "in", "den", "G\u00e4r\u00b7ten", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wanderer vor\u00fcberziehend", "tokens": ["Wan\u00b7de\u00b7rer", "vor\u00b7\u00fc\u00b7ber\u00b7zie\u00b7hend"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach den sch\u00f6nen Blumen sp\u00e4hen.", "tokens": ["Nach", "den", "sch\u00f6\u00b7nen", "Blu\u00b7men", "sp\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter all den hellen Fenstern,", "tokens": ["Un\u00b7ter", "all", "den", "hel\u00b7len", "Fens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Sonne offen stehen,", "tokens": ["Die", "der", "Son\u00b7ne", "of\u00b7fen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ein einziges verschlossen", "tokens": ["Ist", "ein", "ein\u00b7zi\u00b7ges", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vor dem lauen Fr\u00fchlingswehen.", "tokens": ["Vor", "dem", "lau\u00b7en", "Fr\u00fch\u00b7lings\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Hyazinthe duftet", "tokens": ["Ei\u00b7ne", "Hy\u00b7a\u00b7zin\u00b7the", "duf\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor den blendenden Gardinen;", "tokens": ["Vor", "den", "blen\u00b7den\u00b7den", "Gar\u00b7di\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber eine kranke Jungfrau", "tokens": ["A\u00b7ber", "ei\u00b7ne", "kran\u00b7ke", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.8": {"text": "Atmet bange hinter ihnen.", "tokens": ["At\u00b7met", "ban\u00b7ge", "hin\u00b7ter", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr zu H\u00e4upten sitzt die Mutter", "tokens": ["Ihr", "zu", "H\u00e4up\u00b7ten", "sitzt", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Schwester ihr zu F\u00fc\u00dfen,", "tokens": ["Und", "die", "Schwes\u00b7ter", "ihr", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So, verhaltend bittre Tr\u00e4nen,", "tokens": ["So", ",", "ver\u00b7hal\u00b7tend", "bitt\u00b7re", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Dritten leis sie gr\u00fc\u00dfen.", "tokens": ["Ei\u00b7nen", "Drit\u00b7ten", "leis", "sie", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in ihren Blicken liest er,", "tokens": ["Und", "in", "ih\u00b7ren", "Bli\u00b7cken", "liest", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df der Herbst hat wahr gesprochen,", "tokens": ["Da\u00df", "der", "Herbst", "hat", "wahr", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die Hoffnung ist vernichtet", "tokens": ["Da\u00df", "die", "Hoff\u00b7nung", "ist", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Lilie gebrochen! \u2013", "tokens": ["Und", "die", "Li\u00b7lie", "ge\u00b7bro\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "So den stillen Tod zu sehen", "tokens": ["So", "den", "stil\u00b7len", "Tod", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den lichten, himmelblauen", "tokens": ["In", "den", "lich\u00b7ten", ",", "him\u00b7mel\u00b7blau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Augen eines kranken Liebchens:", "tokens": ["Au\u00b7gen", "ei\u00b7nes", "kran\u00b7ken", "Lieb\u00b7chens", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahrlich, 's ist ein seltsam Schauen,", "tokens": ["Wahr\u00b7lich", ",", "'s", "ist", "ein", "selt\u00b7sam", "Schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wenn die wei\u00dfen Todesrosen", "tokens": ["Wenn", "die", "wei\u00b7\u00dfen", "To\u00b7des\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar so stolz und sieghaft prangen", "tokens": ["Gar", "so", "stolz", "und", "sieg\u00b7haft", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf der Liebsten ausgegl\u00fchten,", "tokens": ["Auf", "der", "Liebs\u00b7ten", "aus\u00b7ge\u00b7gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleichen, bleichen Marmorwangen!", "tokens": ["Blei\u00b7chen", ",", "blei\u00b7chen", "Mar\u00b7mor\u00b7wan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bl\u00fche, milde Grabesblume!", "tokens": ["Bl\u00fc\u00b7he", ",", "mil\u00b7de", "Gra\u00b7bes\u00b7blu\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fche und verbl\u00fche selig!", "tokens": ["Bl\u00fc\u00b7he", "und", "ver\u00b7bl\u00fc\u00b7he", "se\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein kurzer, hei\u00dfer Sommer,", "tokens": ["Noch", "ein", "kur\u00b7zer", ",", "hei\u00b7\u00dfer", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auch ich bin \u00fcberz\u00e4hlig!", "tokens": ["Und", "auch", "ich", "bin", "\u00fc\u00b7berz\u00b7\u00e4h\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "ADJD", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Wie die linden Maienl\u00fcfte", "tokens": ["Wie", "die", "lin\u00b7den", "Mai\u00b7en\u00b7l\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Bl\u00fcte sanft entbl\u00e4ttern,", "tokens": ["Dei\u00b7ne", "Bl\u00fc\u00b7te", "sanft", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So wird ", "tokens": ["So", "wird"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "In des Herbstes rauhen Wettern!", "tokens": ["In", "des", "Herbs\u00b7tes", "rau\u00b7hen", "Wet\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}