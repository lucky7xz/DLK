{"dta.poem.9909": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob ich schon viel v\u00e4ter habe/ nimt der tod mich doch geschwind:", "tokens": ["Ob", "ich", "schon", "viel", "v\u00e4\u00b7ter", "ha\u00b7be", "/", "nimt", "der", "tod", "mich", "doch", "ge\u00b7schwind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$(", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Denn in einem augen-blicke lebend mich und tod man findt.", "tokens": ["Denn", "in", "ei\u00b7nem", "au\u00b7gen\u00b7bli\u00b7cke", "le\u00b7bend", "mich", "und", "tod", "man", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PPER", "KON", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Feuer thut mir an den tod/ feuer giebet mir das leben/", "tokens": ["Feu\u00b7er", "thut", "mir", "an", "den", "tod", "/", "feu\u00b7er", "gie\u00b7bet", "mir", "das", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "NN", "VVFIN", "PPER", "PDS", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und was wunderns w\u00fcrdig ist: mu\u00df die luft ein grab mir ge-", "tokens": ["Und", "was", "wun\u00b7derns", "w\u00fcr\u00b7dig", "ist", ":", "mu\u00df", "die", "luft", "ein", "grab", "mir", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "NN", "ADJD", "VAFIN", "$.", "VMFIN", "ART", "NN", "ART", "NN", "PPER", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "(ben.", "tokens": ["(", "ben", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVPP", "$."], "meter": "-", "measure": "single.down"}}}}}