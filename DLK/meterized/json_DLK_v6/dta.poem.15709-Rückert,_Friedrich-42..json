{"dta.poem.15709": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wandrer, wenn er geht gesellt mit einem andern,", "tokens": ["Ein", "Wand\u00b7rer", ",", "wenn", "er", "geht", "ge\u00b7sellt", "mit", "ei\u00b7nem", "an\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "VVPP", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird gut thun, Schritt mit ihm zu halten unterm Wandern.", "tokens": ["Wird", "gut", "thun", ",", "Schritt", "mit", "ihm", "zu", "hal\u00b7ten", "un\u00b7term", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$,", "NN", "APPR", "PPER", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vorwerts vergn\u00fcglicher geht es im gleichen Takt,", "tokens": ["Vor\u00b7werts", "ver\u00b7gn\u00fcg\u00b7li\u00b7cher", "geht", "es", "im", "glei\u00b7chen", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Als wenn entgegen stets ein Schritt dem andern hackt.", "tokens": ["Als", "wenn", "ent\u00b7ge\u00b7gen", "stets", "ein", "Schritt", "dem", "an\u00b7dern", "hackt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ADV", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So auch, wenn du ein Buch zum Lesen wirst entfalten,", "tokens": ["So", "auch", ",", "wenn", "du", "ein", "Buch", "zum", "Le\u00b7sen", "wirst", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Such' immer dich mit ihm in gleichem Zug zu halten.", "tokens": ["Such'", "im\u00b7mer", "dich", "mit", "ihm", "in", "glei\u00b7chem", "Zug", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denk' \u00fcberein mit mir, solang du mich begleitest!", "tokens": ["Denk'", "\u00fc\u00b7be\u00b7re\u00b7in", "mit", "mir", ",", "so\u00b7lang", "du", "mich", "be\u00b7glei\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vom Lehrer lernst du nichts, wenn du mit ihm nur streitest.", "tokens": ["Vom", "Leh\u00b7rer", "lernst", "du", "nichts", ",", "wenn", "du", "mit", "ihm", "nur", "strei\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}