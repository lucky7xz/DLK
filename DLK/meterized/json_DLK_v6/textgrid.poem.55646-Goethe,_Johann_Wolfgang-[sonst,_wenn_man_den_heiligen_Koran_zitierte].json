{"textgrid.poem.55646": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[sonst, wenn man den heiligen Koran zitierte]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonst, wenn man den heiligen Koran zitierte,", "tokens": ["Sonst", ",", "wenn", "man", "den", "hei\u00b7li\u00b7gen", "Ko\u00b7ran", "zi\u00b7tier\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nannte man die Sure, den Vers dazu,", "tokens": ["Nann\u00b7te", "man", "die", "Su\u00b7re", ",", "den", "Vers", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "ART", "NN", "PAV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und jeder Moslem, wie sich's geb\u00fchrte,", "tokens": ["Und", "je\u00b7der", "Mos\u00b7lem", ",", "wie", "sich's", "ge\u00b7b\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fchlte sein Gewissen in Respekt und Ruh.", "tokens": ["F\u00fchl\u00b7te", "sein", "Ge\u00b7wis\u00b7sen", "in", "Res\u00b7pekt", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Die neuen Derwische wissen's nicht besser,", "tokens": ["Die", "neu\u00b7en", "Der\u00b7wi\u00b7sche", "wis\u00b7sen's", "nicht", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sie schwatzen das Alte, das Neue dazu.", "tokens": ["Sie", "schwat\u00b7zen", "das", "Al\u00b7te", ",", "das", "Neu\u00b7e", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Verwirrung wird t\u00e4glich gr\u00f6\u00dfer,", "tokens": ["Die", "Ver\u00b7wir\u00b7rung", "wird", "t\u00e4g\u00b7lich", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "O heiliger Koran! O ewige Ruh!", "tokens": ["O", "hei\u00b7li\u00b7ger", "Ko\u00b7ran", "!", "O", "e\u00b7wi\u00b7ge", "Ruh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}