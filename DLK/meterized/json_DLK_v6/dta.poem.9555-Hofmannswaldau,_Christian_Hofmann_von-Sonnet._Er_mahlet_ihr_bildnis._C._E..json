{"dta.poem.9555": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Er mahlet ihr bildnis.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein antlitz/ welches nur dem himmel \u00e4hnlich stehet/", "tokens": ["Ein", "ant\u00b7litz", "/", "wel\u00b7ches", "nur", "dem", "him\u00b7mel", "\u00e4hn\u00b7lich", "ste\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "ADV", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das angebohrne pracht vor tausenden erh\u00f6ht/", "tokens": ["Das", "an\u00b7ge\u00b7bohr\u00b7ne", "pracht", "vor", "tau\u00b7sen\u00b7den", "er\u00b7h\u00f6ht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und das kein pinsel sich zu mahlen untersteht/", "tokens": ["Und", "das", "kein", "pin\u00b7sel", "sich", "zu", "mah\u00b7len", "un\u00b7ter\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist meine feder hier zu bilden h\u00f6chst bem\u00fchet.", "tokens": ["Ist", "mei\u00b7ne", "fe\u00b7der", "hier", "zu", "bil\u00b7den", "h\u00f6chst", "be\u00b7m\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der wangen blumen-krey\u00df/ der voller rosen bl\u00fchet/", "tokens": ["Der", "wan\u00b7gen", "blu\u00b7men\u00b7krey\u00df", "/", "der", "vol\u00b7ler", "ro\u00b7sen", "bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der augen himmel-blau/ des mundes purpur-r\u00f6th/", "tokens": ["Der", "au\u00b7gen", "him\u00b7mel\u00b7blau", "/", "des", "mun\u00b7des", "pur\u00b7pur\u00b7r\u00f6\u00b7th", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der stirne helffenbein/ die \u00fcber marmel geht/", "tokens": ["Der", "stir\u00b7ne", "helf\u00b7fen\u00b7bein", "/", "die", "\u00fc\u00b7ber", "mar\u00b7mel", "geht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die brust/ die auch dem schnee den silber-glantz entziehet/", "tokens": ["Die", "brust", "/", "die", "auch", "dem", "schnee", "den", "sil\u00b7ber\u00b7glantz", "ent\u00b7zie\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des geistes sittsamkeit/ den englischen verstand/", "tokens": ["Des", "geis\u00b7tes", "sitt\u00b7sam\u00b7keit", "/", "den", "eng\u00b7li\u00b7schen", "ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so viel tugenden/ die aller welt bekant/", "tokens": ["Und", "so", "viel", "tu\u00b7gen\u00b7den", "/", "die", "al\u00b7ler", "welt", "be\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$(", "ART", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darff man in ewigkeit von keinem mahler hoffen;", "tokens": ["Darff", "man", "in", "e\u00b7wig\u00b7keit", "von", "kei\u00b7nem", "mah\u00b7ler", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die kunst ist ihm zu hoch: ich sag\u2019 es ohne scheu.", "tokens": ["Die", "kunst", "ist", "ihm", "zu", "hoch", ":", "ich", "sag'", "es", "oh\u00b7ne", "scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$.", "PPER", "VVFIN", "PPER", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer aber siehet nicht/ da\u00df di\u00df Libore sey/", "tokens": ["Wer", "a\u00b7ber", "sie\u00b7het", "nicht", "/", "da\u00df", "di\u00df", "Li\u00b7bo\u00b7re", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PTKNEG", "$(", "KOUS", "PDS", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df mein feder-kiel sie gantz genau getroffen?", "tokens": ["Und", "da\u00df", "mein", "fe\u00b7der\u00b7kiel", "sie", "gantz", "ge\u00b7nau", "ge\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}