word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und da dein G \u2013 \u2013 nun den Stab will weiter setzen,", "tokens": ["Und", "da", "dein", "G", "\u2013", "\u2013", "nun", "den", "Stab", "will", "wei\u00b7ter", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NE", "$(", "$(", "ADV", "ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.56": {"text": "So denke selbst, was mir das Schicksal auferlegt.", "tokens": ["So", "den\u00b7ke", "selbst", ",", "was", "mir", "das", "Schick\u00b7sal", "auf\u00b7er\u00b7legt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Dergleichen schnelle Flucht l\u00e4\u00dft sich nicht leicht verschmerzen.", "tokens": ["Derg\u00b7lei\u00b7chen", "schnel\u00b7le", "Flucht", "l\u00e4\u00dft", "sich", "nicht", "leicht", "ver\u00b7schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "VVFIN", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Doch glaube, dieses wird stets meine Losung seyn,", "tokens": ["Doch", "glau\u00b7be", ",", "die\u00b7ses", "wird", "stets", "mei\u00b7ne", "Lo\u00b7sung", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Je weiter aus dem Aug ie n\u00e4her bey dem Herzen;", "tokens": ["Je", "wei\u00b7ter", "aus", "dem", "Aug", "ie", "n\u00e4\u00b7her", "bey", "dem", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Dies r\u00e4um ich dir gewi\u00df zum Angedenken ein,", "tokens": ["Dies", "r\u00e4um", "ich", "dir", "ge\u00b7wi\u00df", "zum", "An\u00b7ge\u00b7den\u00b7ken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Zeug hin und la\u00df mich auch, so weit du dich entfernest,", "tokens": ["Zeug", "hin", "und", "la\u00df", "mich", "auch", ",", "so", "weit", "du", "dich", "ent\u00b7fer\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVIMP", "PPER", "ADV", "$,", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Aus deinen Briefen sehn, was du mir sonst geg\u00f6nnt,", "tokens": ["Aus", "dei\u00b7nen", "Brie\u00b7fen", "sehn", ",", "was", "du", "mir", "sonst", "ge\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "PWS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Da\u00df du diejenige noch nicht vergessen lernest,", "tokens": ["Da\u00df", "du", "die\u00b7je\u00b7ni\u00b7ge", "noch", "nicht", "ver\u00b7ges\u00b7sen", "ler\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "PTKNEG", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Die dich die Zierde nur von ihren Freunden nennt.", "tokens": ["Die", "dich", "die", "Zier\u00b7de", "nur", "von", "ih\u00b7ren", "Freun\u00b7den", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}