rd", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VAFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da sich anitzt dein Fu\u00df aus ihren Grenzen rei\u00dft,", "tokens": ["Da", "sich", "a\u00b7nitzt", "dein", "Fu\u00df", "aus", "ih\u00b7ren", "Gren\u00b7zen", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und nach Pannonien so unverhofft will lenken,", "tokens": ["Und", "nach", "Pan\u00b7no\u00b7ni\u00b7en", "so", "un\u00b7ver\u00b7hofft", "will", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wohin Mars den Gemal, und dich ihm folgen hei\u00dft!", "tokens": ["Wo\u00b7hin", "Mars", "den", "Ge\u00b7mal", ",", "und", "dich", "ihm", "fol\u00b7gen", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,", "KON", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.49": {"text": "Ach Freundin! willst du dich uns denn so gar entziehen?", "tokens": ["Ach", "Freun\u00b7din", "!", "willst", "du", "dich", "uns", "denn", "so", "gar", "ent\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VMFIN", "PPER", "PRF", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "lin