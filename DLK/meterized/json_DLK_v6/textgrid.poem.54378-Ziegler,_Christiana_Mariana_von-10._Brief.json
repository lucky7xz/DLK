{"textgrid.poem.54378": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "10. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein nett gesetztes Blat, das mir dein Wohlergehen,", "tokens": ["Dein", "nett", "ge\u00b7setz\u00b7tes", "Blat", ",", "das", "mir", "dein", "Woh\u00b7ler\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hochwohlgebohrne Frau, vergn\u00fcgt zu lesen gab,", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ne", "Frau", ",", "ver\u00b7gn\u00fcgt", "zu", "le\u00b7sen", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erweckte mir viel Lust da ich es durch gesehen;", "tokens": ["Er\u00b7weck\u00b7te", "mir", "viel", "Lust", "da", "ich", "es", "durch", "ge\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "KOUS", "PPER", "PPER", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn du dr\u00fcckst deinen Geist in allen Zeilen ab.", "tokens": ["Denn", "du", "dr\u00fcckst", "dei\u00b7nen", "Geist", "in", "al\u00b7len", "Zei\u00b7len", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mir war, als h\u00f6rt ich dich mit mir pers\u00f6nlich sprechen,", "tokens": ["Mir", "war", ",", "als", "h\u00f6rt", "ich", "dich", "mit", "mir", "per\u00b7s\u00f6n\u00b7lich", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PRF", "APPR", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn es kann deiner Schrift, die deinen Geist entdeckt,", "tokens": ["Denn", "es", "kann", "dei\u00b7ner", "Schrift", ",", "die", "dei\u00b7nen", "Geist", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wenig als dem Mund, an Artigkeit gebrechen,", "tokens": ["So", "we\u00b7nig", "als", "dem", "Mund", ",", "an", "Ar\u00b7tig\u00b7keit", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du sprichst und schreibst zugleich, geschickt und aufgeweckt.", "tokens": ["Du", "sprichst", "und", "schreibst", "zu\u00b7gleich", ",", "ge\u00b7schickt", "und", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Muse hilft dir zwar die Feder niedlich sch\u00e4rfen,", "tokens": ["Die", "Mu\u00b7se", "hilft", "dir", "zwar", "die", "Fe\u00b7der", "nied\u00b7lich", "sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch hat die Schmeicheley dabey die Hand gef\u00fchrt,", "tokens": ["Doch", "hat", "die", "Schmei\u00b7che\u00b7ley", "da\u00b7bey", "die", "Hand", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die mein so schlechter Dienst mich billig hei\u00dft verwerfen,", "tokens": ["Die", "mein", "so", "schlech\u00b7ter", "Dienst", "mich", "bil\u00b7lig", "hei\u00dft", "ver\u00b7wer\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADV", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil dir dergleichen Lob, mir aber nicht geb\u00fchrt.", "tokens": ["Weil", "dir", "derg\u00b7lei\u00b7chen", "Lob", ",", "mir", "a\u00b7ber", "nicht", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "$,", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie magst du doch von mir ein solches R\u00fchmen machen,", "tokens": ["Wie", "magst", "du", "doch", "von", "mir", "ein", "sol\u00b7ches", "R\u00fch\u00b7men", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als w\u00e4re Clio mir vor andern zugethan?", "tokens": ["Als", "w\u00e4\u00b7re", "Clio", "mir", "vor", "an\u00b7dern", "zu\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NE", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Besch\u00e4me mich doch nicht; sie wird selbst dr\u00fcber lachen,", "tokens": ["Be\u00b7sch\u00e4\u00b7me", "mich", "doch", "nicht", ";", "sie", "wird", "selbst", "dr\u00fc\u00b7ber", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie sieht dich ja gewi\u00df mit holdern Augen an.", "tokens": ["Sie", "sieht", "dich", "ja", "ge\u00b7wi\u00df", "mit", "hol\u00b7dern", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ists m\u00f6glich da\u00df du wohl mein Seitenspiel kannst preisen?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "du", "wohl", "mein", "Sei\u00b7ten\u00b7spiel", "kannst", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "O Freundinn, denke doch an deiner Lieder Klang,", "tokens": ["O", "Freun\u00b7dinn", ",", "den\u00b7ke", "doch", "an", "dei\u00b7ner", "Lie\u00b7der", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was die f\u00fcr Majestet, f\u00fcr Geist und Feuer weisen;", "tokens": ["Was", "die", "f\u00fcr", "Ma\u00b7jes\u00b7tet", ",", "f\u00fcr", "Geist", "und", "Feu\u00b7er", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "APPR", "NE", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Im Musenchor hast du gewi\u00df den ersten Rang.", "tokens": ["Im", "Mu\u00b7sen\u00b7chor", "hast", "du", "ge\u00b7wi\u00df", "den", "ers\u00b7ten", "Rang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So oft mein Auge was von deiner Hand erblicket,", "tokens": ["So", "oft", "mein", "Au\u00b7ge", "was", "von", "dei\u00b7ner", "Hand", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das die Calliope durch dich zum Vorschein bringt,", "tokens": ["Das", "die", "Cal\u00b7li\u00b7o\u00b7pe", "durch", "dich", "zum", "Vor\u00b7schein", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "So viel mals werd ich auch, ich schwer es dir, entz\u00fccket,", "tokens": ["So", "viel", "mals", "werd", "ich", "auch", ",", "ich", "schwer", "es", "dir", ",", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "ADV", "$,", "PPER", "ADJD", "PPER", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil alles angenehm und mehr als lieblich klingt.", "tokens": ["Weil", "al\u00b7les", "an\u00b7ge\u00b7nehm", "und", "mehr", "als", "lieb\u00b7lich", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "PIAT", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch dieses kann mich gar nicht in Verwundrung setzen,", "tokens": ["Doch", "die\u00b7ses", "kann", "mich", "gar", "nicht", "in", "Ver\u00b7wund\u00b7rung", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df deine Poesie dir so viel Ruhm erwirbt,", "tokens": ["Da\u00df", "dei\u00b7ne", "Poe\u00b7sie", "dir", "so", "viel", "Ruhm", "er\u00b7wirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und die gelehrte Welt dich vor geschickt mu\u00df sch\u00e4tzen,", "tokens": ["Und", "die", "ge\u00b7lehr\u00b7te", "Welt", "dich", "vor", "ge\u00b7schickt", "mu\u00df", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "APPR", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn andern Kraft gebricht, und Geist und Glut erstirbt.", "tokens": ["Wenn", "an\u00b7dern", "Kraft", "ge\u00b7bricht", ",", "und", "Geist", "und", "Glut", "er\u00b7stirbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du wohnst an einem Ort, den sich die Pierinnen", "tokens": ["Du", "wohnst", "an", "ei\u00b7nem", "Ort", ",", "den", "sich", "die", "Pie\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Mit ihrem Dichterprinz zum Leibgeding erkiest;", "tokens": ["Mit", "ih\u00b7rem", "Dich\u00b7ter\u00b7prinz", "zum", "Leib\u00b7ge\u00b7ding", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo man die Hippocren sieht ungetr\u00fcbet rinnen,", "tokens": ["Wo", "man", "die", "Hip\u00b7po\u00b7cren", "sieht", "un\u00b7ge\u00b7tr\u00fc\u00b7bet", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und wo der Sammelplatz recht guter Dichter ist.", "tokens": ["Und", "wo", "der", "Sam\u00b7mel\u00b7platz", "recht", "gu\u00b7ter", "Dich\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dergleichen edle Kunst ist ihnen eigenth\u00fcmlich,", "tokens": ["Derg\u00b7lei\u00b7chen", "ed\u00b7le", "Kunst", "ist", "ih\u00b7nen", "ei\u00b7gent\u00b7h\u00fcm\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "An statt der Muttermilch n\u00e4hrt sie der Musen Brust.", "tokens": ["An", "statt", "der", "Mut\u00b7ter\u00b7milch", "n\u00e4hrt", "sie", "der", "Mu\u00b7sen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wer singt wie sie, so sch\u00f6n, so lebhaft und so r\u00fchmlich?", "tokens": ["Wer", "singt", "wie", "sie", ",", "so", "sch\u00f6n", ",", "so", "leb\u00b7haft", "und", "so", "r\u00fchm\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wer h\u00f6rt ihr Singen nicht mit stets verneuter Lust?", "tokens": ["Wer", "h\u00f6rt", "ihr", "Sin\u00b7gen", "nicht", "mit", "stets", "ver\u00b7neu\u00b7ter", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Auch ihrer T\u00f6chter Geist erhebet sich im Dichten,", "tokens": ["Auch", "ih\u00b7rer", "T\u00f6ch\u00b7ter", "Geist", "er\u00b7he\u00b7bet", "sich", "im", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.38": {"text": "F\u00e4llt liebste Freundinn, dir nicht die von Bre\u00dfler ein?", "tokens": ["F\u00e4llt", "liebs\u00b7te", "Freun\u00b7dinn", ",", "dir", "nicht", "die", "von", "Bre\u00df\u00b7ler", "ein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "PPER", "PTKNEG", "ART", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Schlesien mit Schmerz sah nach den Sternen fl\u00fcchten,", "tokens": ["Die", "Schle\u00b7si\u00b7en", "mit", "Schmerz", "sah", "nach", "den", "Ster\u00b7nen", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Auf deren Asche noch die Musen Weyrauch streun;", "tokens": ["Auf", "de\u00b7ren", "A\u00b7sche", "noch", "die", "Mu\u00b7sen", "Wey\u00b7rauch", "streun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measur