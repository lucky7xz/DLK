{"textgrid.poem.67640": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wind und Sonne machten Wette,", "tokens": ["Wind", "und", "Son\u00b7ne", "mach\u00b7ten", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer die meisten Kr\u00e4fte h\u00e4tte,", "tokens": ["Wer", "die", "meis\u00b7ten", "Kr\u00e4f\u00b7te", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen armen Wandersmann", "tokens": ["Ei\u00b7nen", "ar\u00b7men", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Kleider zu berauben.", "tokens": ["Sei\u00b7ner", "Klei\u00b7der", "zu", "be\u00b7rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wind begann;", "tokens": ["Wind", "be\u00b7gann", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Doch sein Schnauben", "tokens": ["Doch", "sein", "Schnau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "That ihm nichts; der Wandersmann", "tokens": ["That", "ihm", "nichts", ";", "der", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "PIS", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog den Mantel dichter an.", "tokens": ["Zog", "den", "Man\u00b7tel", "dich\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wind verzweifelt nun und ruht;", "tokens": ["Wind", "ver\u00b7zwei\u00b7felt", "nun", "und", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein lieber Sonnenschein", "tokens": ["Und", "ein", "lie\u00b7ber", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcllt mit holder, sanfter Gluth", "tokens": ["F\u00fcllt", "mit", "hol\u00b7der", ",", "sanf\u00b7ter", "Gluth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wanderers Gebein.", "tokens": ["Wan\u00b7de\u00b7rers", "Ge\u00b7bein", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00fcllt er nun sich tiefer ein?", "tokens": ["H\u00fcllt", "er", "nun", "sich", "tie\u00b7fer", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein!", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ab wirft er nun sein Gewand,", "tokens": ["Ab", "wirft", "er", "nun", "sein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.4": {"text": "Und die Sonne \u00fcberwand.", "tokens": ["Und", "die", "Son\u00b7ne", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Uebermacht, Vernunftgewalt", "tokens": ["Ue\u00b7ber\u00b7macht", ",", "Ver\u00b7nunft\u00b7ge\u00b7walt"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht und l\u00e4\u00dft uns kalt;", "tokens": ["Macht", "und", "l\u00e4\u00dft", "uns", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Warme Christusliebe \u2013", "tokens": ["War\u00b7me", "Chris\u00b7tus\u00b7lie\u00b7be", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wer, der kalt ihr bliebe?", "tokens": ["Wer", ",", "der", "kalt", "ihr", "blie\u00b7be", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}