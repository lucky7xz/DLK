{"textgrid.poem.64029": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Waldnacht", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist das eine r\u00fcstige Nacht.", "tokens": ["Ist", "das", "ei\u00b7ne", "r\u00fcs\u00b7ti\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da f\u00fchlt man sich.", "tokens": ["Da", "f\u00fchlt", "man", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie meine Schritte treffen!", "tokens": ["Wie", "mei\u00b7ne", "Schrit\u00b7te", "tref\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und allen Boden wecken wollen.", "tokens": ["Und", "al\u00b7len", "Bo\u00b7den", "we\u00b7cken", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und er gibt Antwort.", "tokens": ["Und", "er", "gibt", "Ant\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$."], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.6": {"text": "So weit.", "tokens": ["So", "weit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "So klar.", "tokens": ["So", "klar", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Man meint: es ist Wort.", "tokens": ["Man", "meint", ":", "es", "ist", "Wort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "So grau und fein und deutlich.", "tokens": ["So", "grau", "und", "fein", "und", "deut\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und riecht wie ein Kristall.", "tokens": ["Und", "riecht", "wie", "ein", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die graue Perle der feinen, r\u00fcstigen Nacht.", "tokens": ["Die", "grau\u00b7e", "Per\u00b7le", "der", "fei\u00b7nen", ",", "r\u00fcs\u00b7ti\u00b7gen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die nichts gibt, nichts nimmt, sein l\u00e4\u00dft.", "tokens": ["Die", "nichts", "gibt", ",", "nichts", "nimmt", ",", "sein", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und sehe ganz deutlich meines Atems, meines Lebens Baum", "tokens": ["Und", "se\u00b7he", "ganz", "deut\u00b7lich", "mei\u00b7nes", "A\u00b7tems", ",", "mei\u00b7nes", "Le\u00b7bens", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Und sto\u00dfe ihn vor mir aus.", "tokens": ["Und", "sto\u00b7\u00dfe", "ihn", "vor", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Ja, das tut wohl.", "tokens": ["Ja", ",", "das", "tut", "wohl", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "ADV", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.16": {"text": "Da k\u00f6nnte man immer sein.", "tokens": ["Da", "k\u00f6nn\u00b7te", "man", "im\u00b7mer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Immer gehn.", "tokens": ["Im\u00b7mer", "gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Immer Leib haben.", "tokens": ["Im\u00b7mer", "Leib", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.19": {"text": "Als k\u00f6nne der nicht von uns lassen.", "tokens": ["Als", "k\u00f6n\u00b7ne", "der", "nicht", "von", "uns", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Licht ist nicht zu sehen.", "tokens": ["Licht", "ist", "nicht", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Nicht oben.", "tokens": ["Nicht", "o\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Nicht unten.", "tokens": ["Nicht", "un\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Das machen meine Augen, meine klaren gesunden Augen.", "tokens": ["Das", "ma\u00b7chen", "mei\u00b7ne", "Au\u00b7gen", ",", "mei\u00b7ne", "kla\u00b7ren", "ge\u00b7sun\u00b7den", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Juhu!", "tokens": ["Ju\u00b7hu", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Und habe mich je im Gr\u00fcbeln gekr\u00fcmmt?", "tokens": ["Und", "ha\u00b7be", "mich", "je", "im", "Gr\u00fc\u00b7beln", "ge\u00b7kr\u00fcmmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Komme ja hin.", "tokens": ["Kom\u00b7me", "ja", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Komme \u00fcberall hin.", "tokens": ["Kom\u00b7me", "\u00fc\u00b7be\u00b7rall", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Es wird w\u00e4rmer.", "tokens": ["Es", "wird", "w\u00e4r\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Wohl nur von mir aus.", "tokens": ["Wohl", "nur", "von", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.30": {"text": "Ich bin ja alles hier.", "tokens": ["Ich", "bin", "ja", "al\u00b7les", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Und wie eigen, warm vor Leibhaftigkeit die gro\u00dfe, wei\u00dfe Wolke leuchtet.", "tokens": ["Und", "wie", "ei\u00b7gen", ",", "warm", "vor", "Leib\u00b7haf\u00b7tig\u00b7keit", "die", "gro\u00b7\u00dfe", ",", "wei\u00b7\u00dfe", "Wol\u00b7ke", "leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$,", "ADJD", "APPR", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.32": {"text": "Wo kommt sie her?", "tokens": ["Wo", "kommt", "sie", "her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Was scheint sie an?", "tokens": ["Was", "scheint", "sie", "an", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "Ist ja nirgends Licht zu sehen.", "tokens": ["Ist", "ja", "nir\u00b7gends", "Licht", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Nirgends Licht, nirgends!", "tokens": ["Nir\u00b7gends", "Licht", ",", "nir\u00b7gends", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.36": {"text": "Auch eigen?", "tokens": ["Auch", "ei\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Wie ich.", "tokens": ["Wie", "ich", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.38": {"text": "Und lockt so stark, so woll\u00fcstig wie sonst des", "tokens": ["Und", "lockt", "so", "stark", ",", "so", "wol\u00b7l\u00fcs\u00b7tig", "wie", "sonst", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "ADV", "ART"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Weibes schwellend uns emp\u00f6render Frieden.", "tokens": ["Wei\u00b7bes", "schwel\u00b7lend", "uns", "em\u00b7p\u00f6\u00b7ren\u00b7der", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "Und so keusch wie nur die weite Welt.", "tokens": ["Und", "so", "keusch", "wie", "nur", "die", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KOKOM", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "Das ganz Durchdrungene.", "tokens": ["Das", "ganz", "Durch\u00b7drun\u00b7ge\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.42": {"text": "Ich lese mich zur\u00fcck, lese mich weiter, lese mich", "tokens": ["Ich", "le\u00b7se", "mich", "zu\u00b7r\u00fcck", ",", "le\u00b7se", "mich", "wei\u00b7ter", ",", "le\u00b7se", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.43": {"text": "aus allen nahenden, beflissen farbigen Mantelgestalten des Haines.", "tokens": ["aus", "al\u00b7len", "na\u00b7hen\u00b7den", ",", "be\u00b7flis\u00b7sen", "far\u00b7bi\u00b7gen", "Man\u00b7tel\u00b7ge\u00b7stal\u00b7ten", "des", "Hai\u00b7nes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "$,", "VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.44": {"text": "Kein Lied f\u00e4llt nieder.", "tokens": ["Kein", "Lied", "f\u00e4llt", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "Kein Vogeltraum.", "tokens": ["Kein", "Vo\u00b7gel\u00b7traum", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Wir selbst sind Leben.", "tokens": ["Wir", "selbst", "sind", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.47": {"text": "Eigenes Leben.", "tokens": ["Ei\u00b7ge\u00b7nes", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Und einen Rausch habe ich.", "tokens": ["Und", "ei\u00b7nen", "Rausch", "ha\u00b7be", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "H\u00f6her als der von bl\u00f6dem Gegorenen.", "tokens": ["H\u00f6\u00b7her", "als", "der", "von", "bl\u00f6\u00b7dem", "Ge\u00b7go\u00b7re\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}}}}}