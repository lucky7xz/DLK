{"textgrid.poem.34569": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[scheltet das nicht Abenteuer]", "genre": "verse", "period": "N.A.", "pub_year": 1903, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scheltet das nicht Abenteuer,", "tokens": ["Schel\u00b7tet", "das", "nicht", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was, aus Fr\u00fchlingsnacht geboren,", "tokens": ["was", ",", "aus", "Fr\u00fch\u00b7lings\u00b7nacht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehnsucht sich geformt zu neuer", "tokens": ["Sehn\u00b7sucht", "sich", "ge\u00b7formt", "zu", "neu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "VVPP", "PTKZU", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcnsche Bild \u2013 nie mehr verloren. \u2013", "tokens": ["W\u00fcn\u00b7sche", "Bild", "\u2013", "nie", "mehr", "ver\u00b7lo\u00b7ren", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob ich mich ans Ende wage,", "tokens": ["Ob", "ich", "mich", "ans", "En\u00b7de", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bin ich dennoch zu beneiden,", "tokens": ["bin", "ich", "den\u00b7noch", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "denn ich lernte solcher Tage", "tokens": ["denn", "ich", "lern\u00b7te", "sol\u00b7cher", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "diese neue Lust zu leiden. \u2013", "tokens": ["die\u00b7se", "neu\u00b7e", "Lust", "zu", "lei\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Neid und Liebe sind nur L\u00fcgen", "tokens": ["Neid", "und", "Lie\u00b7be", "sind", "nur", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fcr ein allesf\u00fchlend Herz \u2013", "tokens": ["f\u00fcr", "ein", "al\u00b7les\u00b7f\u00fch\u00b7lend", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sich gen\u00fcgen \u2013 selbst gen\u00fcgen \u2013", "tokens": ["sich", "ge\u00b7n\u00fc\u00b7gen", "\u2013", "selbst", "ge\u00b7n\u00fc\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$(", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "endlos \u2013 niegeb\u00fcsster Schmerz.", "tokens": ["end\u00b7los", "\u2013", "nie\u00b7ge\u00b7b\u00fcs\u00b7ster", "Schmerz", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}