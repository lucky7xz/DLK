{"textgrid.poem.63115": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sommerabende, ihr lauen,", "tokens": ["Som\u00b7mer\u00b7a\u00b7ben\u00b7de", ",", "ihr", "lau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bettet mich auf eure Kissen,", "tokens": ["Bet\u00b7tet", "mich", "auf", "eu\u00b7re", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft in Fernen, dunkelblauen,", "tokens": ["La\u00dft", "in", "Fer\u00b7nen", ",", "dun\u00b7kel\u00b7blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "APPR", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Tr\u00e4ume Wimpel hissen.", "tokens": ["Mei\u00b7ner", "Tr\u00e4u\u00b7me", "Wim\u00b7pel", "his\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stunden, die am Tag sich placken,", "tokens": ["Stun\u00b7den", ",", "die", "am", "Tag", "sich", "pla\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feiern n\u00e4chtlich froh verwegen,", "tokens": ["Fei\u00b7ern", "n\u00e4cht\u00b7lich", "froh", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich f\u00fchl um meinen Nacken", "tokens": ["Und", "ich", "f\u00fchl", "um", "mei\u00b7nen", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlich sich zwei Arme legen.", "tokens": ["Z\u00e4rt\u00b7lich", "sich", "zwei", "Ar\u00b7me", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist die Seele liebeswund?", "tokens": ["Ist", "die", "See\u00b7le", "lie\u00b7bes\u00b7wund", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfren Atem haucht der Flieder,", "tokens": ["Hei\u00df\u00b7ren", "A\u00b7tem", "haucht", "der", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der rote Himmelsmund", "tokens": ["Und", "der", "ro\u00b7te", "Him\u00b7mels\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Neigt sich \u00fcppig zu mir nieder.", "tokens": ["Neigt", "sich", "\u00fcp\u00b7pig", "zu", "mir", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}