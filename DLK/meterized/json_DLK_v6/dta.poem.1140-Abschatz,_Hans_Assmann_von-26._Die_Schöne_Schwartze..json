{"dta.poem.1140": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "26. Die Sch\u00f6ne Schwartze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Schwartz bistu/ aber sch\u00f6n; und wer dich anders nennt/ ", "tokens": ["Schwartz", "bis\u00b7tu", "/", "a\u00b7ber", "sch\u00f6n", ";", "und", "wer", "dich", "an\u00b7ders", "nennt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ADV", "ADJD", "$.", "KON", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betrachte bey der Nacht des d\u00fcstern Himmels-Zelt/", "tokens": ["Be\u00b7trach\u00b7te", "bey", "der", "Nacht", "des", "d\u00fcs\u00b7tern", "Him\u00b7mels\u00b7Zelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwey Sternen f\u00fcllen hier mit Brand und Licht die Welt/", "tokens": ["Zwey", "Ster\u00b7nen", "f\u00fcl\u00b7len", "hier", "mit", "Brand", "und", "Licht", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man sieht/ wie jener kaum mit tausend Augen brennt.", "tokens": ["Man", "sieht", "/", "wie", "je\u00b7ner", "kaum", "mit", "tau\u00b7send", "Au\u00b7gen", "brennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOKOM", "PDS", "ADV", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der reinen Liebe Glantz/ so ich auff dich gewendt/", "tokens": ["Der", "rei\u00b7nen", "Lie\u00b7be", "Glantz", "/", "so", "ich", "auff", "dich", "ge\u00b7wendt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ADV", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wird durch dein helles Schwartz ans klare Licht gestellt.", "tokens": ["Wird", "durch", "dein", "hel\u00b7les", "Schwartz", "ans", "kla\u00b7re", "Licht", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn sonst ein tunckler Stein dem Erzt ein Urtheil f\u00e4llt/", "tokens": ["Wenn", "sonst", "ein", "tunck\u00b7ler", "Stein", "dem", "Erzt", "ein", "Ur\u00b7theil", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird meine Treu an dir dem Golde gleich erkennt.", "tokens": ["Wird", "mei\u00b7ne", "Treu", "an", "dir", "dem", "Gol\u00b7de", "gleich", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O Kohle s\u00fcsser Glutt/ die dich so schwartz gemacht/", "tokens": ["O", "Koh\u00b7le", "s\u00fcs\u00b7ser", "Glutt", "/", "die", "dich", "so", "schwartz", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was hat dein sch\u00f6nes Feur vor Hertzen machen schwitzen/", "tokens": ["Was", "hat", "dein", "sch\u00f6\u00b7nes", "Feur", "vor", "Hert\u00b7zen", "ma\u00b7chen", "schwit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Eh dich ihr Rauch und Ru\u00df zu solcher Br\u00e4une bracht!", "tokens": ["Eh", "dich", "ihr", "Rauch", "und", "Ru\u00df", "zu", "sol\u00b7cher", "Br\u00e4u\u00b7ne", "bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Am hellsten siehet manns aus tuncklen Wolcken blitzen/", "tokens": ["Am", "hells\u00b7ten", "sie\u00b7het", "manns", "aus", "tunck\u00b7len", "Wol\u00b7cken", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In Thetis brauner Scho\u00df verbirgt sich Ph\u00f6bus Zier/", "tokens": ["In", "The\u00b7tis", "brau\u00b7ner", "Scho\u00df", "ver\u00b7birgt", "sich", "Ph\u00f6\u00b7bus", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVFIN", "PRF", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schwartz endlich ist die Nacht/ und bringt den Tag herf\u00fcr.", "tokens": ["Schwartz", "end\u00b7lich", "ist", "die", "Nacht", "/", "und", "bringt", "den", "Tag", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}