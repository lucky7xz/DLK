{"textgrid.poem.25338": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Finale", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Siegesjauchzen ist es, das", "tokens": ["Wie", "Sie\u00b7ges\u00b7jauch\u00b7zen", "ist", "es", ",", "das"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "VAFIN", "PPER", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jahrtausende verrauschend weiterwerfen.", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "ver\u00b7rau\u00b7schend", "wei\u00b7ter\u00b7wer\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Berge donnern.", "tokens": ["Die", "Ber\u00b7ge", "don\u00b7nern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "An den Eisenklammern", "tokens": ["An", "den", "Ei\u00b7sen\u00b7klam\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "R\u00fcttelt Prometheus,", "tokens": ["R\u00fct\u00b7telt", "Pro\u00b7me\u00b7theus", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "R\u00fcttelt, r\u00fcttelt,", "tokens": ["R\u00fct\u00b7telt", ",", "r\u00fct\u00b7telt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sie rucken \u2013 springen \u2013", "tokens": ["Sie", "ru\u00b7cken", "\u2013", "sprin\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein Erl\u00f6sungsschrei", "tokens": ["Ein", "Er\u00b7l\u00f6\u00b7sungs\u00b7schrei"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Gellt \u00fcber die Welten hin.", "tokens": ["Gellt", "\u00fc\u00b7ber", "die", "Wel\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Doch er", "tokens": ["Doch", "er"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Sch\u00fcttelt die Glieder, die lang entw\u00f6hnten,", "tokens": ["Sch\u00fct\u00b7telt", "die", "Glie\u00b7der", ",", "die", "lang", "ent\u00b7w\u00f6hn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Reckt die Arme, die lang gel\u00e4hmten,", "tokens": ["Reckt", "die", "Ar\u00b7me", ",", "die", "lang", "ge\u00b7l\u00e4hm\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Schreitet hinab,", "tokens": ["Schrei\u00b7tet", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Ein Sturzbach, den der Tauwind losgek\u00fc\u00dft \u2013", "tokens": ["Ein", "Sturz\u00b7bach", ",", "den", "der", "Tau\u00b7wind", "los\u00b7ge\u00b7k\u00fc\u00dft", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Vom Kaukasus hinab zu seinen Menschen.", "tokens": ["Vom", "Kau\u00b7ka\u00b7sus", "hin\u00b7ab", "zu", "sei\u00b7nen", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Festglocken dr\u00f6hnen,", "tokens": ["Fest\u00b7glo\u00b7cken", "dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.17": {"text": "Sonnentrunkne Reigen", "tokens": ["Son\u00b7nen\u00b7trunk\u00b7ne", "Rei\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Flattern um ihn, dionysisch verschlungen,", "tokens": ["Flat\u00b7tern", "um", "ihn", ",", "di\u00b7o\u00b7ny\u00b7sisch", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.19": {"text": "Weinlaubumkr\u00e4nzt:", "tokens": ["Wein\u00b7lau\u00b7bum\u00b7kr\u00e4nzt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "\u00bbprometheus!", "tokens": ["\u00bb", "pro\u00b7me\u00b7theus", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NE", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.21": {"text": "Gott des Lichts!", "tokens": ["Gott", "des", "Lichts", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Heil dem Erl\u00f6sten, der die Welt erl\u00f6st!\u00ab", "tokens": ["Heil", "dem", "Er\u00b7l\u00f6s\u00b7ten", ",", "der", "die", "Welt", "er\u00b7l\u00f6st", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.23": {"text": "An seinen Armen gl\u00fchn die Eisenstriemen", "tokens": ["An", "sei\u00b7nen", "Ar\u00b7men", "gl\u00fchn", "die", "Ei\u00b7sen\u00b7strie\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "In roten Flammen auf.", "tokens": ["In", "ro\u00b7ten", "Flam\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Und dann \u2013", "tokens": ["Und", "dann", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Pl\u00f6tzlich ist er's nicht mehr:", "tokens": ["Pl\u00f6tz\u00b7lich", "ist", "er's", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "PTKNEG", "ADV", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.27": {"text": "An den H\u00e4nden klaffen", "tokens": ["An", "den", "H\u00e4n\u00b7den", "klaf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.28": {"text": "Braune Wundenmale wie von Kreuzesn\u00e4geln,", "tokens": ["Brau\u00b7ne", "Wun\u00b7den\u00b7ma\u00b7le", "wie", "von", "Kreu\u00b7zes\u00b7n\u00e4\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.29": {"text": "Blutige Schmerzensmale. \u2013", "tokens": ["Blu\u00b7ti\u00b7ge", "Schmer\u00b7zens\u00b7ma\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.30": {"text": "Doch aus den Augen bricht ein goldner Strom", "tokens": ["Doch", "aus", "den", "Au\u00b7gen", "bricht", "ein", "gold\u00b7ner", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Von Morgenlicht,", "tokens": ["Von", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Aus tiefen Balduraugen ...", "tokens": ["Aus", "tie\u00b7fen", "Bal\u00b7du\u00b7rau\u00b7gen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Baldur-Prometheus-Christus \u2013", "tokens": ["Bal\u00b7dur\u00b7Pro\u00b7me\u00b7theus\u00b7Chris\u00b7tus", "\u2013"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Heiliges Leben", "tokens": ["Hei\u00b7li\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In Licht, in Sch\u00f6nheit,", "tokens": ["In", "Licht", ",", "in", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nie sterbender G\u00f6tterrausch", "tokens": ["Nie", "ster\u00b7ben\u00b7der", "G\u00f6t\u00b7ter\u00b7rausch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gl\u00fchendster Trunkenheit! ...", "tokens": ["Gl\u00fc\u00b7hends\u00b7ter", "Trun\u00b7ken\u00b7heit", "!", "..."], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur f\u00fchlen, atmen, schwelgen. Seligstes", "tokens": ["Nur", "f\u00fch\u00b7len", ",", "at\u00b7men", ",", "schwel\u00b7gen", ".", "Se\u00b7ligs\u00b7tes"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$.", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nirwana und", "tokens": ["Nir\u00b7wa\u00b7na", "und"], "token_info": ["word", "word"], "pos": ["NE", "KON"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Aus tausend Himmeln tausend Morgensonnen.", "tokens": ["Aus", "tau\u00b7send", "Him\u00b7meln", "tau\u00b7send", "Mor\u00b7gen\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}