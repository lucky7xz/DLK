{"textgrid.poem.45717": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In nieder\u00f6streichischer Mundart", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In D\u00e4nemark trug man", "tokens": ["In", "D\u00e4\u00b7ne\u00b7mark", "trug", "man"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PIS"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Einem gewiegten Staatsmann", "tokens": ["Ei\u00b7nem", "ge\u00b7wieg\u00b7ten", "Staats\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Finanzministerstelle an.", "tokens": ["Die", "Fi\u00b7nanz\u00b7mi\u00b7nis\u00b7ter\u00b7stel\u00b7le", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Er aber wollte sich nicht bequemen", "tokens": ["Er", "a\u00b7ber", "woll\u00b7te", "sich", "nicht", "be\u00b7que\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PRF", "PTKNEG", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und sprach: Solche Finanzen zu \u00fcbernehmen", "tokens": ["Und", "sprach", ":", "Sol\u00b7che", "Fi\u00b7nan\u00b7zen", "zu", "\u00fc\u00b7ber\u00b7neh\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "-++--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mu\u00df man ein Genie sein oder \u00bbnarisch\u00ab.", "tokens": ["Mu\u00df", "man", "ein", "Ge\u00b7nie", "sein", "o\u00b7der", "\u00bb", "na\u00b7risch", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VAINF", "KON", "$(", "ADJD", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Welches von beiden ist nun Graf Larisch?", "tokens": ["Wel\u00b7ches", "von", "bei\u00b7den", "ist", "nun", "Graf", "La\u00b7risch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIS", "VAFIN", "ADV", "NE", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}