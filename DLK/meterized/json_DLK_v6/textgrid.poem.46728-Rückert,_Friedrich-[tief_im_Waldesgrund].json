{"textgrid.poem.46728": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[tief im Waldesgrund]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Waldesgrund", "tokens": ["Tief", "im", "Wal\u00b7des\u00b7grund"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und im Felsenthal", "tokens": ["Und", "im", "Fel\u00b7sen\u00b7thal"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Laut mit Herz und Mund", "tokens": ["Laut", "mit", "Herz", "und", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ruf' ich tausendmal:", "tokens": ["Ruf'", "ich", "tau\u00b7send\u00b7mal", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kinder, seid ihr da?", "tokens": ["Kin\u00b7der", ",", "seid", "ihr", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "\u00bbda!\u00ab", "tokens": ["\u00bb", "da", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Wo denn da? \u00bbDa! da!\u00ab", "tokens": ["Wo", "denn", "da", "?", "\u00bb", "Da", "!", "da", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "ADV", "$.", "$(", "ADV", "$.", "ADV", "$.", "$("], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Dunkles Waldgestr\u00e4uch", "tokens": ["Dunk\u00b7les", "Wald\u00b7ges\u00b7tr\u00e4uch"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Trennet euch von mir,", "tokens": ["Tren\u00b7net", "euch", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht erblick' ich euch,", "tokens": ["Nicht", "er\u00b7blick'", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aber sagt, ob ihr", "tokens": ["A\u00b7ber", "sagt", ",", "ob", "ihr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Fern seid oder nah?", "tokens": ["Fern", "seid", "o\u00b7der", "nah", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.6": {"text": "\u00bbnah!\u00ab", "tokens": ["\u00bb", "nah", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Wie denn nah? \u00bbNah, nah!\u00ab", "tokens": ["Wie", "denn", "nah", "?", "\u00bb", "Nah", ",", "nah", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$.", "$(", "NN", "$,", "ADJD", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wollt ihr nah mir sein,", "tokens": ["Wollt", "ihr", "nah", "mir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wo ihr fern auch seid?", "tokens": ["Wo", "ihr", "fern", "auch", "seid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Immer bleiben mein", "tokens": ["Im\u00b7mer", "blei\u00b7ben", "mein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eine Lust im Leid?", "tokens": ["Ei\u00b7ne", "Lust", "im", "Leid", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mein? Nein oder Ja?", "tokens": ["Mein", "?", "Nein", "o\u00b7der", "Ja", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PTKANT", "KON", "PTKANT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "\u00bbja!\u00ab", "tokens": ["\u00bb", "ja", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$.", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Immer, ja? \u00bbJa, ja!\u00ab", "tokens": ["Im\u00b7mer", ",", "ja", "?", "\u00bb", "Ja", ",", "ja", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "$(", "PTKANT", "$,", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}