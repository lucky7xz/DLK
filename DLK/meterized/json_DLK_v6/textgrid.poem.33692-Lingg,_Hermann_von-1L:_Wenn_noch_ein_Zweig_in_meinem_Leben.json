{"textgrid.poem.33692": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn noch ein Zweig in meinem Leben", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn noch ein Zweig in meinem Leben", "tokens": ["Wenn", "noch", "ein", "Zweig", "in", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch auf ein Bl\u00fchen hoffen l\u00e4\u00dft,", "tokens": ["Noch", "auf", "ein", "Bl\u00fc\u00b7hen", "hof\u00b7fen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So soll er seine Zierde geben,", "tokens": ["So", "soll", "er", "sei\u00b7ne", "Zier\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie zu verweben", "tokens": ["Sie", "zu", "ver\u00b7we\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu dieses Tages Fest.", "tokens": ["Zu", "die\u00b7ses", "Ta\u00b7ges", "Fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie freundlich war't ihr mir, ihr frohen Stunden!", "tokens": ["Wie", "freund\u00b7lich", "wa\u00b7r't", "ihr", "mir", ",", "ihr", "fro\u00b7hen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ihr wi\u00dft es, wo ich Trost und Huld gefunden.", "tokens": ["Ihr", "wi\u00dft", "es", ",", "wo", "ich", "Trost", "und", "Huld", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vergessen lernt' ich manche Schranken,", "tokens": ["Ver\u00b7ges\u00b7sen", "lernt'", "ich", "man\u00b7che", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergessen manche Sorgenflut.", "tokens": ["Ver\u00b7ges\u00b7sen", "man\u00b7che", "Sor\u00b7gen\u00b7flut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fand Asyl f\u00fcr den Gedanken,", "tokens": ["Ich", "fand", "A\u00b7syl", "f\u00fcr", "den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele durfte ranken,", "tokens": ["Die", "See\u00b7le", "durf\u00b7te", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Freundschaft gab ihr Mut.", "tokens": ["Die", "Freund\u00b7schaft", "gab", "ihr", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Entrissen dem verh\u00e4ngten Los der Schatten,", "tokens": ["Ent\u00b7ris\u00b7sen", "dem", "ver\u00b7h\u00e4ng\u00b7ten", "Los", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wem dank' ich's? Dir und deinem edlen Gatten!", "tokens": ["Wem", "dank'", "ich's", "?", "Dir", "und", "dei\u00b7nem", "ed\u00b7len", "Gat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "$.", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Begl\u00fcckt ist, wer es noch empfinden", "tokens": ["Be\u00b7gl\u00fcckt", "ist", ",", "wer", "es", "noch", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wer es noch bekennen kann,", "tokens": ["Und", "wer", "es", "noch", "be\u00b7ken\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, wenn uns alle Sterne schwinden,", "tokens": ["Da\u00df", ",", "wenn", "uns", "al\u00b7le", "Ster\u00b7ne", "schwin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn uns mit ihrem blinden,", "tokens": ["Wenn", "uns", "mit", "ih\u00b7rem", "blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Unselig d\u00fcstern Bann", "tokens": ["Un\u00b7se\u00b7lig", "d\u00fcs\u00b7tern", "Bann"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Nacht umf\u00e4ngt, da\u00df dann noch Menschen leben,", "tokens": ["Die", "Nacht", "um\u00b7f\u00e4ngt", ",", "da\u00df", "dann", "noch", "Men\u00b7schen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die rettend uns die Hand mit W\u00e4rme geben. \u2013", "tokens": ["Die", "ret\u00b7tend", "uns", "die", "Hand", "mit", "W\u00e4r\u00b7me", "ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seid mir gesegnet! Euch erbl\u00fche,", "tokens": ["Seid", "mir", "ge\u00b7seg\u00b7net", "!", "Euch", "er\u00b7bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ihr mir wiedergabt, das Gl\u00fcck", "tokens": ["Das", "ihr", "mir", "wie\u00b7der\u00b7gabt", ",", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erneuter Jugend, sp\u00e4t wie fr\u00fche", "tokens": ["Er\u00b7neu\u00b7ter", "Ju\u00b7gend", ",", "sp\u00e4t", "wie", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "KOKOM", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Lohn der Erdenm\u00fche", "tokens": ["Ein", "Lohn", "der", "Er\u00b7den\u00b7m\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Strahl's hell auf euch zur\u00fcck", "tokens": ["Strahl's", "hell", "auf", "euch", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von euren Kindern, die mit Freudekr\u00e4nzen", "tokens": ["Von", "eu\u00b7ren", "Kin\u00b7dern", ",", "die", "mit", "Freu\u00b7de\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie dort am Himmel Zwillingssterne gl\u00e4nzen!", "tokens": ["Wie", "dort", "am", "Him\u00b7mel", "Zwil\u00b7lings\u00b7ster\u00b7ne", "gl\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}