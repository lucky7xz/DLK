{"textgrid.poem.39781": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dr\u00fcben auf den gr\u00fcnen Feldern,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00fcben auf den gr\u00fcnen Feldern,", "tokens": ["Dr\u00fc\u00b7ben", "auf", "den", "gr\u00fc\u00b7nen", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcben zwischen dunkeln W\u00e4ldern", "tokens": ["Dr\u00fc\u00b7ben", "zwi\u00b7schen", "dun\u00b7keln", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wogt das bunte Sch\u00e4ferspiel;", "tokens": ["Wogt", "das", "bun\u00b7te", "Sch\u00e4\u00b7fer\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Muntre Knaben aus dem St\u00e4dtchen,", "tokens": ["Mun\u00b7tre", "Kna\u00b7ben", "aus", "dem", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rasche M\u00e4dchen", "tokens": ["Ra\u00b7sche", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sputen sich zum frohen Ziel.", "tokens": ["Spu\u00b7ten", "sich", "zum", "fro\u00b7hen", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jahre sind's, da stand ich dr\u00fcben,", "tokens": ["Jah\u00b7re", "sin\u00b7d's", ",", "da", "stand", "ich", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Lie\u00df sie tummeln sich und \u00fcben,", "tokens": ["Lie\u00df", "sie", "tum\u00b7meln", "sich", "und", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PRF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah mich nicht am Spiele blind.", "tokens": ["Sah", "mich", "nicht", "am", "Spie\u00b7le", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neben mir im Glanz der Auen,", "tokens": ["Ne\u00b7ben", "mir", "im", "Glanz", "der", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zuzuschauen,", "tokens": ["Zu\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Stand das allersch\u00f6nste Kind.", "tokens": ["Stand", "das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als sie so zu schaun sich m\u00fchte,", "tokens": ["Als", "sie", "so", "zu", "schaun", "sich", "m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reines Bild in Engelsg\u00fcte,", "tokens": ["Rei\u00b7nes", "Bild", "in", "En\u00b7gels\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bot ich schirmend ihr die Hand.", "tokens": ["Bot", "ich", "schir\u00b7mend", "ihr", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihr's aus den Aeuglein blinkte,", "tokens": ["Wie", "ih\u00b7r's", "aus", "den", "A\u00b7e\u00b7u\u00b7glein", "blink\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dankend winkte,", "tokens": ["Dan\u00b7kend", "wink\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wie der Gru\u00df ihr lieblich stand!", "tokens": ["Wie", "der", "Gru\u00df", "ihr", "lieb\u00b7lich", "stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drunten war ein Laufen, Scherzen,", "tokens": ["Drun\u00b7ten", "war", "ein", "Lau\u00b7fen", ",", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber mir im innern Herzen", "tokens": ["A\u00b7ber", "mir", "im", "in\u00b7nern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Regte sich geheimes Leid.", "tokens": ["Reg\u00b7te", "sich", "ge\u00b7hei\u00b7mes", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbk\u00f6nnt' ich, ach, zum s\u00fc\u00dfen Minnen", "tokens": ["\u00bb", "k\u00f6nnt'", "ich", ",", "ach", ",", "zum", "s\u00fc\u00b7\u00dfen", "Min\u00b7nen"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "$,", "ITJ", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich gewinnen,", "tokens": ["Dich", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sch\u00e4ferin im sch\u00f6nsten Kleid!\u00ab", "tokens": ["Sch\u00e4\u00b7fe\u00b7rin", "im", "sch\u00f6ns\u00b7ten", "Kleid", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also ging ich von dem Feste,", "tokens": ["Al\u00b7so", "ging", "ich", "von", "dem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatte wohl erschaut das Beste,", "tokens": ["Hat\u00b7te", "wohl", "er\u00b7schaut", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtend stieg das Bild mir auf.", "tokens": ["Leuch\u00b7tend", "stieg", "das", "Bild", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so ward mein ganzes Leben,", "tokens": ["Und", "so", "ward", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr' ergeben,", "tokens": ["Ih\u00b7r'", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nur ein hei\u00dfer Sch\u00e4ferlauf.", "tokens": ["Nur", "ein", "hei\u00b7\u00dfer", "Sch\u00e4\u00b7fer\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wieder sind sie heut beim Spiele,", "tokens": ["Wie\u00b7der", "sind", "sie", "heut", "beim", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagen nach dem sch\u00f6nen Ziele,", "tokens": ["Ja\u00b7gen", "nach", "dem", "sch\u00f6\u00b7nen", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie und mich sieht Keiner dort!", "tokens": ["Sie", "und", "mich", "sieht", "Kei\u00b7ner", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! wie anders ist es heute,", "tokens": ["Ach", "!", "wie", "an\u00b7ders", "ist", "es", "heu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fremde Leute", "tokens": ["Frem\u00b7de", "Leu\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Stehen an dem liebsten Ort.", "tokens": ["Ste\u00b7hen", "an", "dem", "liebs\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "K\u00fcmmert's mich, wer heute sieget,", "tokens": ["K\u00fcm\u00b7mert's", "mich", ",", "wer", "heu\u00b7te", "sie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PWS", "ADV", "VVFIN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wer am Ziele sich vergn\u00fcget,", "tokens": ["Wer", "am", "Zie\u00b7le", "sich", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer das Kleinod an sich zieht?", "tokens": ["Wer", "das", "Klei\u00b7nod", "an", "sich", "zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fraget nicht, ob ich's errungen \u2013", "tokens": ["Fra\u00b7get", "nicht", ",", "ob", "ich's", "er\u00b7run\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ausgesungen,", "tokens": ["Aus\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ausgesungen sei mein Lied!", "tokens": ["Aus\u00b7ge\u00b7sun\u00b7gen", "sei", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}