{"textgrid.poem.36012": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[es schl\u00e4gt dir unser Herz so froh entgegen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schl\u00e4gt dir unser Herz so froh entgegen,", "tokens": ["Es", "schl\u00e4gt", "dir", "un\u00b7ser", "Herz", "so", "froh", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ehrw\u00fcrdiger Hirt! sei uns willkommen hier.", "tokens": ["Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "Hirt", "!", "sei", "uns", "will\u00b7kom\u00b7men", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du kommst, zu spenden uns des Himmels Segen,", "tokens": ["Du", "kommst", ",", "zu", "spen\u00b7den", "uns", "des", "Him\u00b7mels", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und freudig blicken wir hinauf zu dir.", "tokens": ["Und", "freu\u00b7dig", "bli\u00b7cken", "wir", "hin\u00b7auf", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O, k\u00f6nnten dir es unsre Lippen sagen,", "tokens": ["O", ",", "k\u00f6nn\u00b7ten", "dir", "es", "uns\u00b7re", "Lip\u00b7pen", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie gern wir dich in unserm Kreise sehn,", "tokens": ["Wie", "gern", "wir", "dich", "in", "un\u00b7serm", "Krei\u00b7se", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie kindlich treu dir unsre Herzen schlagen", "tokens": ["Wie", "kind\u00b7lich", "treu", "dir", "uns\u00b7re", "Her\u00b7zen", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Gottes Segen deinem Tun erflehn.", "tokens": ["Und", "Got\u00b7tes", "Se\u00b7gen", "dei\u00b7nem", "Tun", "er\u00b7flehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}