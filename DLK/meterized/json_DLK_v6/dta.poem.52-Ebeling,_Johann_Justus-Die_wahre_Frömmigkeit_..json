{"dta.poem.52": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  wahre Fr\u00f6mmigkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Herzen allen Schein verfluchen,", "tokens": ["Von", "Her\u00b7zen", "al\u00b7len", "Schein", "ver\u00b7flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Den N\u00e4chsten lieben jederzeit;", "tokens": ["Den", "N\u00e4chs\u00b7ten", "lie\u00b7ben", "je\u00b7der\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Demuht und Gelassenheit,", "tokens": ["In", "De\u00b7muht", "und", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Des Sch\u00f6pfers Ordnung Wink und Willen", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Ord\u00b7nung", "Wink", "und", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit wahrer Lieb und Lust erf\u00fcllen", "tokens": ["Mit", "wah\u00b7rer", "Lieb", "und", "Lust", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und leiden was er auferlegt:", "tokens": ["Und", "lei\u00b7den", "was", "er", "auf\u00b7er\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer diese Eigenschaften hegt,", "tokens": ["Wer", "die\u00b7se", "Ei\u00b7gen\u00b7schaf\u00b7ten", "hegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der kan mit recht ein Frommer heissen", "tokens": ["Der", "kan", "mit", "recht", "ein", "From\u00b7mer", "heis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und wer sich dabei wird befleissen", "tokens": ["Und", "wer", "sich", "da\u00b7bei", "wird", "be\u00b7fleis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "PAV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das Herz von Dingen dieser Welt,", "tokens": ["Das", "Herz", "von", "Din\u00b7gen", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und was dem H\u00f6chsten misgef\u00e4llt,", "tokens": ["Und", "was", "dem", "H\u00f6chs\u00b7ten", "mis\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie sichs geb\u00fchret, abzuziehen;", "tokens": ["Wie", "sichs", "ge\u00b7b\u00fch\u00b7ret", ",", "ab\u00b7zu\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der wird durch redliches Bem\u00fchen", "tokens": ["Der", "wird", "durch", "red\u00b7li\u00b7ches", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "In seiner Kraft durch ihn bestehn,", "tokens": ["In", "sei\u00b7ner", "Kraft", "durch", "ihn", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und in das Himmelreich eingehn.", "tokens": ["Und", "in", "das", "Him\u00b7mel\u00b7reich", "ein\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}