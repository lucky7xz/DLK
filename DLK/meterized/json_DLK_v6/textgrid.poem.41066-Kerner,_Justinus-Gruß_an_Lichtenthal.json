{"textgrid.poem.41066": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gru\u00df an Lichtenthal", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier bin ich wieder! sei gegr\u00fc\u00dfet", "tokens": ["Hier", "bin", "ich", "wie\u00b7der", "!", "sei", "ge\u00b7gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum zweitenmal, mein lichtes Tal,", "tokens": ["Zum", "zwei\u00b7ten\u00b7mal", ",", "mein", "lich\u00b7tes", "Tal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sieh! deine Klarheit, deinen Frieden", "tokens": ["Sieh", "!", "dei\u00b7ne", "Klar\u00b7heit", ",", "dei\u00b7nen", "Frie\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verglich ich einst dem Mondenstrahl.", "tokens": ["Ver\u00b7glich", "ich", "einst", "dem", "Mon\u00b7dens\u00b7trahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo aus der Waldnacht rauscht die Quelle", "tokens": ["Wo", "aus", "der", "Wald\u00b7nacht", "rauscht", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blitzend und k\u00fchl das Tal entlang,", "tokens": ["Blit\u00b7zend", "und", "k\u00fchl", "das", "Tal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hell niedert\u00f6nt ins Quellenrauschen", "tokens": ["Hell", "nie\u00b7der\u00b7t\u00f6nt", "ins", "Quel\u00b7len\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus L\u00fcften blau der V\u00f6gel Sang.", "tokens": ["Aus", "L\u00fcf\u00b7ten", "blau", "der", "V\u00f6\u00b7gel", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warst mir doch lieber als die Sonne,", "tokens": ["Warst", "mir", "doch", "lie\u00b7ber", "als", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Baden, sch\u00f6n wohl anzusehn,", "tokens": ["Dein", "Ba\u00b7den", ",", "sch\u00f6n", "wohl", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dort Britannias schlanke T\u00f6chter", "tokens": ["Wenn", "dort", "Bri\u00b7tan\u00b7ni\u00b7as", "schlan\u00b7ke", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durchsichtig wei\u00df wie Lilien gehn.", "tokens": ["Durch\u00b7sich\u00b7tig", "wei\u00df", "wie", "Li\u00b7li\u00b7en", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Mein Tal! es lag ein harter Winter", "tokens": ["Mein", "Tal", "!", "es", "lag", "ein", "har\u00b7ter", "Win\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Indessen schwer auf mir und dir.", "tokens": ["In\u00b7des\u00b7sen", "schwer", "auf", "mir", "und", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lenz hat ihn von dir genommen,", "tokens": ["Der", "Lenz", "hat", "ihn", "von", "dir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ach! er nahm ihn nicht von mir.", "tokens": ["Doch", "ach", "!", "er", "nahm", "ihn", "nicht", "von", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich sink', o Tal! in deinem Frieden", "tokens": ["Ich", "sink'", ",", "o", "Tal", "!", "in", "dei\u00b7nem", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ans Herze der Natur und fleh':", "tokens": ["Ans", "Her\u00b7ze", "der", "Na\u00b7tur", "und", "fleh'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm mir vom Aug' den tr\u00fcben Schleier!", "tokens": ["Nimm", "mir", "vom", "Aug'", "den", "tr\u00fc\u00b7ben", "Schlei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm mir vom m\u00fcden Haupt den Schnee!", "tokens": ["Nimm", "mir", "vom", "m\u00fc\u00b7den", "Haupt", "den", "Schnee", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gib Helle mir wie deinen Quellen,", "tokens": ["Gib", "Hel\u00b7le", "mir", "wie", "dei\u00b7nen", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie deinen V\u00f6geln mir ein Lied!", "tokens": ["Wie", "dei\u00b7nen", "V\u00f6\u00b7geln", "mir", "ein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df neu mich bl\u00fchen, neu mich singen,", "tokens": ["La\u00df", "neu", "mich", "bl\u00fc\u00b7hen", ",", "neu", "mich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "VVFIN", "$,", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier, wo es ringsum singt und bl\u00fcht.", "tokens": ["Hier", ",", "wo", "es", "ring\u00b7sum", "singt", "und", "bl\u00fcht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Soll's nimmer sein, so fl\u00f6\u00df mir Ruhe", "tokens": ["Soll's", "nim\u00b7mer", "sein", ",", "so", "fl\u00f6\u00df", "mir", "Ru\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAINF", "$,", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Herz, zu tragen still mein Los!", "tokens": ["Ins", "Herz", ",", "zu", "tra\u00b7gen", "still", "mein", "Los", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKZU", "VVINF", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lerche, hat sie ausgesungen,", "tokens": ["Die", "Ler\u00b7che", ",", "hat", "sie", "aus\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Senkt sich zur Erde klagelos.", "tokens": ["Senkt", "sich", "zur", "Er\u00b7de", "kla\u00b7ge\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}