{"textgrid.poem.44044": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mensch ist nicht von Stahl, und Fleisch und Blut mu\u00df sincken,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist nicht von Stahl, und Fleisch und Blut mu\u00df sincken,", "tokens": ["Der", "Mensch", "ist", "nicht", "von", "Stahl", ",", "und", "Fleisch", "und", "Blut", "mu\u00df", "sin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$,", "KON", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Unruh und Gefahr uns in die L\u00e4nge st\u00e4upt:", "tokens": ["Wenn", "Un\u00b7ruh", "und", "Ge\u00b7fahr", "uns", "in", "die", "L\u00e4n\u00b7ge", "st\u00e4upt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich seh die Ungedult auf allen Seiten wincken,", "tokens": ["Ich", "seh", "die", "Un\u00b7ge\u00b7dult", "auf", "al\u00b7len", "Sei\u00b7ten", "win\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich mercke, da\u00df der Trost auf ewig au\u00dfen bleibt.", "tokens": ["Ich", "mer\u00b7cke", ",", "da\u00df", "der", "Trost", "auf", "e\u00b7wig", "au\u00b7\u00dfen", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Seufzer macht vergebens", "tokens": ["Ihr", "Seuf\u00b7zer", "macht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mund, Herz und Glieder matt,", "tokens": ["Mund", ",", "Herz", "und", "Glie\u00b7der", "matt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich bin des armen Lebens", "tokens": ["Ich", "bin", "des", "ar\u00b7men", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wie der W\u00fcntsche satt.", "tokens": ["So", "wie", "der", "W\u00fcnt\u00b7sche", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und was erwarth ich hier? Fast st\u00fcndlich neue Sorgen.", "tokens": ["Und", "was", "er\u00b7warth", "ich", "hier", "?", "Fast", "st\u00fcnd\u00b7lich", "neu\u00b7e", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "$.", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn redlich gilt nicht mehr, die Welt ist schlimm und roh.", "tokens": ["Denn", "red\u00b7lich", "gilt", "nicht", "mehr", ",", "die", "Welt", "ist", "schlimm", "und", "roh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Reiche schwelgt und trozt, der Arme schwizt vom Morgen", "tokens": ["Der", "Rei\u00b7che", "schwelgt", "und", "trozt", ",", "der", "Ar\u00b7me", "schwizt", "vom", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis in die sp\u00e4te Nacht und wird es doch nicht froh.", "tokens": ["Bis", "in", "die", "sp\u00e4\u00b7te", "Nacht", "und", "wird", "es", "doch", "nicht", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "KON", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel wuchern mit den S\u00fcnden", "tokens": ["Viel", "wu\u00b7chern", "mit", "den", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und haben Lohn davon;", "tokens": ["Und", "ha\u00b7ben", "Lohn", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich mag mich drehn und winden,", "tokens": ["Ich", "mag", "mich", "drehn", "und", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Undanck bleibt mein Lohn.", "tokens": ["Der", "Un\u00b7danck", "bleibt", "mein", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch halt, besorgtes Herz, den feigen Fluch zur\u00fccke,", "tokens": ["Doch", "halt", ",", "be\u00b7sorg\u00b7tes", "Herz", ",", "den", "fei\u00b7gen", "Fluch", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Kleinmuth stellt dir nur das Ungl\u00fcck gr\u00f6\u00dfer vor:", "tokens": ["Die", "Klein\u00b7muth", "stellt", "dir", "nur", "das", "Un\u00b7gl\u00fcck", "gr\u00f6\u00b7\u00dfer", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es hat ein jeder Mensch sein eignes Creuz und Gl\u00fccke,", "tokens": ["Es", "hat", "ein", "je\u00b7der", "Mensch", "sein", "eig\u00b7nes", "Creuz", "und", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer seins am gr\u00f6sten sch\u00e4zt, der handelt als ein Thor.", "tokens": ["Wer", "seins", "am", "gr\u00f6s\u00b7ten", "sch\u00e4zt", ",", "der", "han\u00b7delt", "als", "ein", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "ADJA", "VVFIN", "$,", "PRELS", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Vorsicht weises F\u00fcgen", "tokens": ["Der", "Vor\u00b7sicht", "wei\u00b7ses", "F\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Betr\u00fcbet und erg\u00f6zt;", "tokens": ["Be\u00b7tr\u00fc\u00b7bet", "und", "er\u00b7g\u00f6zt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wohl dem, der sein Vergn\u00fcgen", "tokens": ["Wohl", "dem", ",", "der", "sein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ihren Rathschlu\u00df sezt.", "tokens": ["In", "ih\u00b7ren", "Rath\u00b7schlu\u00df", "sezt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So lang ein Puls noch schl\u00e4gt, ist Hofnung zum Genesen,", "tokens": ["So", "lang", "ein", "Puls", "noch", "schl\u00e4gt", ",", "ist", "Hof\u00b7nung", "zum", "Ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer niedertr\u00e4chtig weint, ist keiner H\u00fclfe werth;", "tokens": ["Wer", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "weint", ",", "ist", "kei\u00b7ner", "H\u00fcl\u00b7fe", "werth", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gedult l\u00e4st mit der Zeit von Dornen Feigen lesen", "tokens": ["Ge\u00b7dult", "l\u00e4st", "mit", "der", "Zeit", "von", "Dor\u00b7nen", "Fei\u00b7gen", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mindert nach und nach, was Seel und Leib beschwert.", "tokens": ["Und", "min\u00b7dert", "nach", "und", "nach", ",", "was", "Seel", "und", "Leib", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "APPR", "$,", "PWS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Abend aller Tage", "tokens": ["Der", "A\u00b7bend", "al\u00b7ler", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bricht wohl noch nicht herein;", "tokens": ["Bricht", "wohl", "noch", "nicht", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jezt rase Sturm und Plage,", "tokens": ["Jezt", "ra\u00b7se", "Sturm", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es wird nicht stets so seyn.", "tokens": ["Es", "wird", "nicht", "stets", "so", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du Himmel, kennst mein Herz, es liebt dich auch im Strafen.", "tokens": ["Du", "Him\u00b7mel", ",", "kennst", "mein", "Herz", ",", "es", "liebt", "dich", "auch", "im", "Stra\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlag, nimm mir alles weg und wirf mich hin und her!", "tokens": ["Schlag", ",", "nimm", "mir", "al\u00b7les", "weg", "und", "wirf", "mich", "hin", "und", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PIS", "PTKVZ", "KON", "VVIMP", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach Arbeit, M\u00fch und Schmerz erfolgt ein s\u00fc\u00dfes Schlafen,", "tokens": ["Nach", "Ar\u00b7beit", ",", "M\u00fch", "und", "Schmerz", "er\u00b7folgt", "ein", "s\u00fc\u00b7\u00dfes", "Schla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wenn es eher nicht als in dem Sarge w\u00e4r.", "tokens": ["Und", "wenn", "es", "e\u00b7her", "nicht", "als", "in", "dem", "Sar\u00b7ge", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "KOKOM", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Wille ist mein Gl\u00fccke,", "tokens": ["Dein", "Wil\u00b7le", "ist", "mein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Hofnung meine Ruh;", "tokens": ["Die", "Hof\u00b7nung", "mei\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Erdkrei\u00df brech in St\u00fccke,", "tokens": ["Der", "Erd\u00b7krei\u00df", "brech", "in", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich seh mit Gro\u00dfmuth zu.", "tokens": ["Ich", "seh", "mit", "Gro\u00df\u00b7muth", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}