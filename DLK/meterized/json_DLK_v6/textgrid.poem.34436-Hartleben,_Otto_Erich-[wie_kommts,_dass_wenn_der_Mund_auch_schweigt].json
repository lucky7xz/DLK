{"textgrid.poem.34436": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[wie kommts, dass wenn der Mund auch schweigt]", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie kommts, dass wenn der Mund auch schweigt,", "tokens": ["\u00bb", "wie", "kommts", ",", "dass", "wenn", "der", "Mund", "auch", "schweigt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "$,", "KOUS", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Herz nur um so lauter spricht \u2013", "tokens": ["das", "Herz", "nur", "um", "so", "lau\u00b7ter", "spricht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie kommts, dass unser Auge sich", "tokens": ["wie", "kommts", ",", "dass", "un\u00b7ser", "Au\u00b7ge", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Einverst\u00e4ndnis trifft?", "tokens": ["im", "Ein\u00b7ver\u00b7st\u00e4nd\u00b7nis", "trifft", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie kommt es, dass dein Arm so oft", "tokens": ["Wie", "kommt", "es", ",", "dass", "dein", "Arm", "so", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mir eine St\u00fctze scheint,", "tokens": ["mir", "ei\u00b7ne", "St\u00fct\u00b7ze", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dass in Gedanken meine Stirn", "tokens": ["dass", "in", "Ge\u00b7dan\u00b7ken", "mei\u00b7ne", "Stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "daran zu ruhen meint?\u00ab", "tokens": ["da\u00b7ran", "zu", "ru\u00b7hen", "meint", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sehe, wie in deiner Brust", "tokens": ["Ich", "se\u00b7he", ",", "wie", "in", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Bl\u00fcthe schwillt und sich erschliesst \u2013", "tokens": ["die", "Bl\u00fc\u00b7the", "schwillt", "und", "sich", "er\u00b7schliesst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich bin der Selige, den du liebst,", "tokens": ["ich", "bin", "der", "Se\u00b7li\u00b7ge", ",", "den", "du", "liebst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "der ihren Duft geniesst.", "tokens": ["der", "ih\u00b7ren", "Duft", "ge\u00b7niesst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von allem, was auf Erden bl\u00fcht,", "tokens": ["Von", "al\u00b7lem", ",", "was", "auf", "Er\u00b7den", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ist diese Bl\u00fcthe zart und jung \u2013", "tokens": ["ist", "die\u00b7se", "Bl\u00fc\u00b7the", "zart", "und", "jung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich k\u00fcsse deine weisse Hand,", "tokens": ["ich", "k\u00fcs\u00b7se", "dei\u00b7ne", "weis\u00b7se", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich k\u00fcsse deinen Mund ...", "tokens": ["ich", "k\u00fcs\u00b7se", "dei\u00b7nen", "Mund", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}