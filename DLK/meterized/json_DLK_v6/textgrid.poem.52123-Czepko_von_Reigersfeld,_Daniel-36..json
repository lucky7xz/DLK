{"textgrid.poem.52123": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein schn\u00f6der Mensch, und sprichst: Ich bin ein Gott,", "tokens": ["Du", "bist", "ein", "schn\u00f6\u00b7der", "Mensch", ",", "und", "sprichst", ":", "Ich", "bin", "ein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe Stadt und Land erl\u00f6st aus aller Not:", "tokens": ["Ich", "ha\u00b7be", "Stadt", "und", "Land", "er\u00b7l\u00f6st", "aus", "al\u00b7ler", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du sch\u00f6ner Heyland du. Erl\u00f6sung zu erwerben,", "tokens": ["Du", "sch\u00f6\u00b7ner", "Hey\u00b7land", "du", ".", "Er\u00b7l\u00f6\u00b7sung", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$.", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So muste Christus Gott und Mensch am Creutze sterben.", "tokens": ["So", "mus\u00b7te", "Chris\u00b7tus", "Gott", "und", "Mensch", "am", "Creut\u00b7ze", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun ihn gehst du nichts an: Rom hielt zwar den Gebrauch,", "tokens": ["Nun", "ihn", "gehst", "du", "nichts", "an", ":", "Rom", "hielt", "zwar", "den", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "NE", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da\u00df sie nach ihrer Zeit die B\u00fcrgermeister auch,", "tokens": ["Da\u00df", "sie", "nach", "ih\u00b7rer", "Zeit", "die", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hie\u00df G\u00f6tter auff dem Marckt und allen Stra\u00dfen nennen,", "tokens": ["Hie\u00df", "G\u00f6t\u00b7ter", "auff", "dem", "Marckt", "und", "al\u00b7len", "Stra\u00b7\u00dfen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jedoch, es muste vor ein Holtz Sto\u00df sie verbrennen.", "tokens": ["Je\u00b7doch", ",", "es", "mus\u00b7te", "vor", "ein", "Holtz", "Sto\u00df", "sie", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "APPR", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Wann dich vor einen Gott dein Volck anbeten sol,", "tokens": ["Wann", "dich", "vor", "ei\u00b7nen", "Gott", "dein", "Volck", "an\u00b7be\u00b7ten", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Must du zu\u00e4schert auch (es sol ihm gehen wol),", "tokens": ["Must", "du", "zu\u00b7\u00e4sc\u00b7hert", "auch", "(", "es", "sol", "ihm", "ge\u00b7hen", "wol", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "PPER", "VVFIN", "ADV", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu\u00e4schert oder ja drey Ellen von der Erden", "tokens": ["Zu\u00b7\u00e4sc\u00b7hert", "o\u00b7der", "ja", "drey", "El\u00b7len", "von", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADV", "CARD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff deutschen Brauch geschn\u00fcrt ans Griech'sche \u0393 werden.", "tokens": ["Auff", "deut\u00b7schen", "Brauch", "ge\u00b7schn\u00fcrt", "ans", "Griech'\u00b7sche", "\u0393", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPRART", "NN", "$(", "VAINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}