{"textgrid.poem.53421": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[erkennest du, da\u00df Noth]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erkennest du, da\u00df Noth,", "tokens": ["Er\u00b7ken\u00b7nest", "du", ",", "da\u00df", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verhangn\u00fc\u00df, Fall und Todt", "tokens": ["Ver\u00b7hang\u00b7n\u00fc\u00df", ",", "Fall", "und", "Todt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich wieder dich verbinden", "tokens": ["Sich", "wie\u00b7der", "dich", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00e4ndern deinen Muth,", "tokens": ["Und", "\u00e4n\u00b7dern", "dei\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df oftmals Fleisch und Blut", "tokens": ["Da\u00df", "oft\u00b7mals", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich nicht darin kan finden:", "tokens": ["Sich", "nicht", "da\u00b7rin", "kan", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "PAV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bleib ein behertzter Mann", "tokens": ["Bleib", "ein", "be\u00b7hertz\u00b7ter", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und nimm es tapfer an.", "tokens": ["Und", "nimm", "es", "tap\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was niemand wei\u00df zu meiden,", "tokens": ["Was", "nie\u00b7mand", "wei\u00df", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Natur dar\u00fcber h\u00e4lt,", "tokens": ["Na\u00b7tur", "da\u00b7r\u00fc\u00b7ber", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es tr\u00e4gt es alle Welt,", "tokens": ["Es", "tr\u00e4gt", "es", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das trag auch du bescheiden.", "tokens": ["Das", "trag", "auch", "du", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Uns bindet dieser Eyd,", "tokens": ["Uns", "bin\u00b7det", "die\u00b7ser", "Eyd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geduldig, was die Zeit", "tokens": ["Ge\u00b7dul\u00b7dig", ",", "was", "die", "Zeit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sterblich ist, ertragen,", "tokens": ["Und", "sterb\u00b7lich", "ist", ",", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was bald f\u00fcr sich geht", "tokens": ["Und", "was", "bald", "f\u00fcr", "sich", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und nicht zu wenden steht,", "tokens": ["Und", "nicht", "zu", "wen\u00b7den", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr diesem nicht verzagen.", "tokens": ["F\u00fcr", "die\u00b7sem", "nicht", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir kommen alle gleich", "tokens": ["Wir", "kom\u00b7men", "al\u00b7le", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gebohren in ein Reich,", "tokens": ["Ge\u00b7boh\u00b7ren", "in", "ein", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da niemand sich mu\u00df sperren.", "tokens": ["Da", "nie\u00b7mand", "sich", "mu\u00df", "sper\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch Gott gehorsam seyn,", "tokens": ["Doch", "Gott", "ge\u00b7hor\u00b7sam", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dies machet uns allein", "tokens": ["Dies", "ma\u00b7chet", "uns", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu unsern eignen Herren.", "tokens": ["Zu", "un\u00b7sern", "eig\u00b7nen", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}