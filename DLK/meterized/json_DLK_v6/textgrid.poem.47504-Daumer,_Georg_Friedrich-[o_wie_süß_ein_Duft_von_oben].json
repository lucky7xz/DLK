{"textgrid.poem.47504": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o wie s\u00fc\u00df ein Duft von oben]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie s\u00fc\u00df ein Duft von oben", "tokens": ["O", "wie", "s\u00fc\u00df", "ein", "Duft", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "ART", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Geist umwittert!", "tokens": ["Mei\u00b7nen", "Geist", "um\u00b7wit\u00b7tert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein Blick in jene Kl\u00e4re", "tokens": ["Wie", "ein", "Blick", "in", "je\u00b7ne", "Kl\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir das Hier verbittert!", "tokens": ["Mir", "das", "Hier", "ver\u00b7bit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Breit', o Seele, deine Fl\u00fcgel,", "tokens": ["Breit'", ",", "o", "See\u00b7le", ",", "dei\u00b7ne", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwinge dich nach Eden! \u2013", "tokens": ["Schwin\u00b7ge", "dich", "nach", "E\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wehe, wehe, da\u00df du ringsum", "tokens": ["We\u00b7he", ",", "we\u00b7he", ",", "da\u00df", "du", "ring\u00b7sum"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Schm\u00e4hlich eingegittert! \u2013", "tokens": ["Schm\u00e4h\u00b7lich", "ein\u00b7ge\u00b7git\u00b7tert", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber nein, ich will nicht klagen,", "tokens": ["A\u00b7ber", "nein", ",", "ich", "will", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, ich will nicht fliegen,", "tokens": ["Nein", ",", "ich", "will", "nicht", "flie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob mir auch, in's Freie winkend,", "tokens": ["Ob", "mir", "auch", ",", "in's", "Frei\u00b7e", "win\u00b7kend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Schranke splittert.", "tokens": ["Je\u00b7de", "Schran\u00b7ke", "split\u00b7tert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Alles Sch\u00f6nste, Liebste, Beste", "tokens": ["Al\u00b7les", "Sch\u00f6ns\u00b7te", ",", "Liebs\u00b7te", ",", "Bes\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchet auf der Erde,", "tokens": ["Bl\u00fc\u00b7het", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und es ist ein hohler Flitter,", "tokens": ["Und", "es", "ist", "ein", "hoh\u00b7ler", "Flit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dort oben flittert.", "tokens": ["Der", "dort", "o\u00b7ben", "flit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nur ein Schatten ird'scher Wonne,", "tokens": ["Nur", "ein", "Schat\u00b7ten", "ird'\u00b7scher", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der H\u00f6he spiegelnd,", "tokens": ["In", "der", "H\u00f6\u00b7he", "spie\u00b7gelnd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Macht da\u00df unsre Brust so sehnlich", "tokens": ["Macht", "da\u00df", "uns\u00b7re", "Brust", "so", "sehn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr entgegenzittert.", "tokens": ["Ihr", "ent\u00b7ge\u00b7gen\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Um das Heil, das uns von dorther", "tokens": ["Um", "das", "Heil", ",", "das", "uns", "von", "dor\u00b7ther"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Leere ladet,", "tokens": ["In", "die", "Lee\u00b7re", "la\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sei auf Erden nicht ein einzig", "tokens": ["Sei", "auf", "Er\u00b7den", "nicht", "ein", "ein\u00b7zig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "ART", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosenblatt zerknittert!", "tokens": ["Ro\u00b7sen\u00b7blatt", "zer\u00b7knit\u00b7tert", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}