{"dta.poem.6721": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entstehet nun in unsern Hirn ein Schein,", "tokens": ["Ent\u00b7ste\u00b7het", "nun", "in", "un\u00b7sern", "Hirn", "ein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ohn\u2019 da\u00df von aussen uns was in die Augen f\u00e4llt;", "tokens": ["Ohn'", "da\u00df", "von", "aus\u00b7sen", "uns", "was", "in", "die", "Au\u00b7gen", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So werden Farben auch darinn erzeuget seyn,", "tokens": ["So", "wer\u00b7den", "Far\u00b7ben", "auch", "da\u00b7rinn", "er\u00b7zeu\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "PAV", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn\u2019 da\u00df man sie am C\u00f6rper auswerts stellt.", "tokens": ["Ohn'", "da\u00df", "man", "sie", "am", "C\u00f6r\u00b7per", "aus\u00b7werts", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wann fr\u00fch\u2019 am Horizont sich alles \u00fcberg\u00fcldet,", "tokens": ["Wann", "fr\u00fch'", "am", "Ho\u00b7ri\u00b7zont", "sich", "al\u00b7les", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "PRF", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und durch der Sonnen Strahl sich f\u00e4rbet, gl\u00e4ntzt und bildet;", "tokens": ["Und", "durch", "der", "Son\u00b7nen", "Strahl", "sich", "f\u00e4r\u00b7bet", ",", "gl\u00e4ntzt", "und", "bil\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So dencke man nur frey,", "tokens": ["So", "den\u00b7cke", "man", "nur", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df in den Gegenden, wie gr\u00fcn sie gleich, wie sch\u00f6n,", "tokens": ["Da\u00df", "in", "den", "Ge\u00b7gen\u00b7den", ",", "wie", "gr\u00fcn", "sie", "gleich", ",", "wie", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dennoch der gr\u00fcne Schmeltz, in welchen wir sie sehn,", "tokens": ["Den\u00b7noch", "der", "gr\u00fc\u00b7ne", "Sch\u00b7meltz", ",", "in", "wel\u00b7chen", "wir", "sie", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "PWAT", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Nicht anzutreffen sey.", "tokens": ["Nicht", "an\u00b7zu\u00b7tref\u00b7fen", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}