{"textgrid.poem.39001": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weithin \u00f6ffnet sich die Gegend,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weithin \u00f6ffnet sich die Gegend,", "tokens": ["Weit\u00b7hin", "\u00f6ff\u00b7net", "sich", "die", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unten gl\u00e4nzt ein blauer See.", "tokens": ["Un\u00b7ten", "gl\u00e4nzt", "ein", "blau\u00b7er", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcmmer einer alten Burg", "tokens": ["Tr\u00fcm\u00b7mer", "ei\u00b7ner", "al\u00b7ten", "Burg"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blicken aus dem dunkeln dichten Eppich.", "tokens": ["Bli\u00b7cken", "aus", "dem", "dun\u00b7keln", "dich\u00b7ten", "Ep\u00b7pich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Wie der Weg sich senkt,", "tokens": ["Wie", "der", "Weg", "sich", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Steigen Inseln, Felsen aus dem Wasser,", "tokens": ["Stei\u00b7gen", "In\u00b7seln", ",", "Fel\u00b7sen", "aus", "dem", "Was\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sanft verschmolzen,", "tokens": ["Sanft", "ver\u00b7schmol\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Lieblich erhellt,", "tokens": ["Lieb\u00b7lich", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Als wenn der violblaue Duft", "tokens": ["Als", "wenn", "der", "vi\u00b7olb\u00b7lau\u00b7e", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "See und Insel und Fels", "tokens": ["See", "und", "In\u00b7sel", "und", "Fels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "L\u00f6ste in lieblichen Traum.", "tokens": ["L\u00f6s\u00b7te", "in", "lieb\u00b7li\u00b7chen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, dies sind die lichten Formen,", "tokens": ["Ja", ",", "dies", "sind", "die", "lich\u00b7ten", "For\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die warmen, heitern T\u00f6ne,", "tokens": ["Die", "war\u00b7men", ",", "hei\u00b7tern", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die der Zaubrer aus Lothring", "tokens": ["Die", "der", "Zaub\u00b7rer", "aus", "Loth\u00b7ring"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So wundervoll schafft.", "tokens": ["So", "wun\u00b7der\u00b7voll", "schafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der die Natur,", "tokens": ["Der", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wie ein scherzendes muntres Kind", "tokens": ["Wie", "ein", "scher\u00b7zen\u00b7des", "mun\u00b7tres", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "In das Wollustbad des Lichtes taucht,", "tokens": ["In", "das", "Wol\u00b7lust\u00b7bad", "des", "Lich\u00b7tes", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df Wies' und Wald", "tokens": ["Da\u00df", "Wies'", "und", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und Fels und Strom,", "tokens": ["Und", "Fels", "und", "Strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Meer und Luft", "tokens": ["Meer", "und", "Luft"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Nur Eine Lust und Freude sind.", "tokens": ["Nur", "Ei\u00b7ne", "Lust", "und", "Freu\u00b7de", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und deiner dacht' ich", "tokens": ["Und", "dei\u00b7ner", "dacht'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Brittischer Freund,", "tokens": ["Brit\u00b7ti\u00b7scher", "Freund", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der mich nie verl\u00e4\u00dft,", "tokens": ["Der", "mich", "nie", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Durch dessen Augen", "tokens": ["Durch", "des\u00b7sen", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ich Welt und Menschen sehe,", "tokens": ["Ich", "Welt", "und", "Men\u00b7schen", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und dein blaues helles Gedicht", "tokens": ["Und", "dein", "blau\u00b7es", "hel\u00b7les", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "In dem sich lustberauscht", "tokens": ["In", "dem", "sich", "lust\u00b7be\u00b7rauscht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Alle Gestalten", "tokens": ["Al\u00b7le", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Im hellen Azur", "tokens": ["Im", "hel\u00b7len", "A\u00b7zur"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Scherzend bewegen.", "tokens": ["Scher\u00b7zend", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}