{"textgrid.poem.46699": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein Winter war's und keiner]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Winter war's und keiner,", "tokens": ["Ein", "Win\u00b7ter", "wa\u00b7r's", "und", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn es hat nicht geschneit,", "tokens": ["Denn", "es", "hat", "nicht", "ge\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "O Schnee, du gl\u00e4nzend reiner,", "tokens": ["O", "Schnee", ",", "du", "gl\u00e4n\u00b7zend", "rei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Machest die Winterzeit.", "tokens": ["Ma\u00b7chest", "die", "Win\u00b7ter\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Und nun ist ohne Stocken", "tokens": ["Und", "nun", "ist", "oh\u00b7ne", "Sto\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Schneegeweb im Gang,", "tokens": ["Das", "Schnee\u00b7ge\u00b7web", "im", "Gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Worauf, ihr lieben Flocken,", "tokens": ["Wo\u00b7rauf", ",", "ihr", "lie\u00b7ben", "Flo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Habt ihr gewartet so lang?", "tokens": ["Habt", "ihr", "ge\u00b7war\u00b7tet", "so", "lang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ihr wartetet, bis man getragen", "tokens": ["Ihr", "war\u00b7te\u00b7tet", ",", "bis", "man", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein todtes Kind aus dem Haus;", "tokens": ["Mein", "tod\u00b7tes", "Kind", "aus", "dem", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nun breitet ihr mit Behagen", "tokens": ["Nun", "brei\u00b7tet", "ihr", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Decke dar\u00fcber aus.", "tokens": ["Die", "De\u00b7cke", "da\u00b7r\u00fc\u00b7ber", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}