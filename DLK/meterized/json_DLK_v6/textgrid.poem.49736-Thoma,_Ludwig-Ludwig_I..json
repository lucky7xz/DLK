{"textgrid.poem.49736": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ludwig I.", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lola, auf den Knien vor dir liegend,", "tokens": ["Lo\u00b7la", ",", "auf", "den", "Kni\u00b7en", "vor", "dir", "lie\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lebend in den h\u00f6chsten Wonnen hin", "tokens": ["Le\u00b7bend", "in", "den", "h\u00f6chs\u00b7ten", "Won\u00b7nen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mein Haupt an deine Reize schmiegend,", "tokens": ["Und", "mein", "Haupt", "an", "dei\u00b7ne", "Rei\u00b7ze", "schmie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "War ich selig, Andalusierin!", "tokens": ["War", "ich", "se\u00b7lig", ",", "An\u00b7da\u00b7lu\u00b7sie\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Teutsche Kraft in alten Gliedern f\u00fchlend", "tokens": ["Teut\u00b7sche", "Kraft", "in", "al\u00b7ten", "Glie\u00b7dern", "f\u00fch\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und besiegend die Bedenklichkeit,", "tokens": ["Und", "be\u00b7sie\u00b7gend", "die", "Be\u00b7denk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Niemals die Begierde g\u00e4nzlich k\u00fchlend,", "tokens": ["Nie\u00b7mals", "die", "Be\u00b7gier\u00b7de", "g\u00e4nz\u00b7lich", "k\u00fch\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hab' ich meine Reste dir geweiht.", "tokens": ["Hab'", "ich", "mei\u00b7ne", "Res\u00b7te", "dir", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber w\u00fctend, mein Idol bespeiend,", "tokens": ["A\u00b7ber", "w\u00fc\u00b7tend", ",", "mein", "I\u00b7dol", "be\u00b7spei\u00b7end", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Glaubete mein Volk sich heldenhaft;", "tokens": ["Glau\u00b7be\u00b7te", "mein", "Volk", "sich", "hel\u00b7den\u00b7haft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Immer L\u00fcmmel nur gewesen seiend,", "tokens": ["Im\u00b7mer", "L\u00fcm\u00b7mel", "nur", "ge\u00b7we\u00b7sen", "sei\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VAPP", "VVPP", "$,"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Spottet' es der Liebe Zauberkraft.", "tokens": ["Spot\u00b7tet'", "es", "der", "Lie\u00b7be", "Zau\u00b7ber\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}}}}