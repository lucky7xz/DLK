{"textgrid.poem.62530": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wag' ich endlich auszusprechen,", "tokens": ["Wag'", "ich", "end\u00b7lich", "aus\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sich l\u00e4ngst in mir bewegt?", "tokens": ["Was", "sich", "l\u00e4ngst", "in", "mir", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darf ich nun das Schweigen brechen,", "tokens": ["Darf", "ich", "nun", "das", "Schwei\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das du selbst mir aufgelegt?", "tokens": ["Das", "du", "selbst", "mir", "auf\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ave dir in deiner Krone,", "tokens": ["A\u00b7ve", "dir", "in", "dei\u00b7ner", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00fcngste Heil'ge, sei gegr\u00fc\u00dft!", "tokens": ["J\u00fcngs\u00b7te", "Heil'\u00b7ge", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nste Braut am K\u00f6nigsthrone,", "tokens": ["Sch\u00f6ns\u00b7te", "Braut", "am", "K\u00f6\u00b7nigs\u00b7thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Sternenblume sprie\u00dft.", "tokens": ["Wo", "die", "Ster\u00b7nen\u00b7blu\u00b7me", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vor den Menschen will ich preisen", "tokens": ["Vor", "den", "Men\u00b7schen", "will", "ich", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich und deine Seligkeit,", "tokens": ["Dich", "und", "dei\u00b7ne", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Hymnen, mit den Weisen", "tokens": ["Mit", "den", "Hym\u00b7nen", ",", "mit", "den", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der glaubenvollen Zeit.", "tokens": ["Aus", "der", "glau\u00b7ben\u00b7vol\u00b7len", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ach, ich kann nicht singen,", "tokens": ["A\u00b7ber", "ach", ",", "ich", "kann", "nicht", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich gerne will und soll,", "tokens": ["Wie", "ich", "ger\u00b7ne", "will", "und", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "KON", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Weinen mu\u00df ich ringen,", "tokens": ["Mit", "dem", "Wei\u00b7nen", "mu\u00df", "ich", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Herz ist kummervoll.", "tokens": ["Und", "mein", "Herz", "ist", "kum\u00b7mer\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh mich an in meiner Demuth,", "tokens": ["Sieh", "mich", "an", "in", "mei\u00b7ner", "De\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Deinen J\u00fcnger, deinen Freund,", "tokens": ["Dei\u00b7nen", "J\u00fcn\u00b7ger", ",", "dei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lindre mir die tiefe Wehmuth,", "tokens": ["Lind\u00b7re", "mir", "die", "tie\u00b7fe", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heil'ge, die mit Gott vereint.", "tokens": ["Heil'\u00b7ge", ",", "die", "mit", "Gott", "ver\u00b7eint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}