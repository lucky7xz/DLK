{"dta.poem.2936": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "135.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist zu sehr geneigt, andre nach dir zu richten,", "tokens": ["Du", "bist", "zu", "sehr", "ge\u00b7neigt", ",", "and\u00b7re", "nach", "dir", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADV", "VVPP", "$,", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jedwedem dein Gef\u00fchl im Busen anzudichten.", "tokens": ["Jed\u00b7we\u00b7dem", "dein", "Ge\u00b7f\u00fchl", "im", "Bu\u00b7sen", "an\u00b7zu\u00b7dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Danach benennest du den einen hochbegl\u00fcckt,", "tokens": ["Da\u00b7nach", "be\u00b7nen\u00b7nest", "du", "den", "ei\u00b7nen", "hoch\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen andern tief in Noth hinabgedr\u00fcckt.", "tokens": ["Und", "ei\u00b7nen", "an\u00b7dern", "tief", "in", "Noth", "hin\u00b7ab\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du setzest nur voraus, da\u00df sie in ihren Lagen", "tokens": ["Du", "set\u00b7zest", "nur", "vo\u00b7raus", ",", "da\u00df", "sie", "in", "ih\u00b7ren", "La\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich f\u00fchlen m\u00fc\u00dften so wie du sie w\u00fcrdest tragen.", "tokens": ["Sich", "f\u00fch\u00b7len", "m\u00fc\u00df\u00b7ten", "so", "wie", "du", "sie", "w\u00fcr\u00b7dest", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "ADV", "KOKOM", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bedenke: jeder lebt in seinem Element,", "tokens": ["Be\u00b7den\u00b7ke", ":", "je\u00b7der", "lebt", "in", "sei\u00b7nem", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob dumpf ob licht es sei, wie wer kein andres kennt.", "tokens": ["Ob", "dumpf", "ob", "licht", "es", "sei", ",", "wie", "wer", "kein", "and\u00b7res", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KOUS", "NN", "PPER", "VAFIN", "$,", "PWAV", "PWS", "PIAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr Leben f\u00fchlen sie in angemessner Lage", "tokens": ["Ihr", "Le\u00b7ben", "f\u00fch\u00b7len", "sie", "in", "an\u00b7ge\u00b7mess\u00b7ner", "La\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht als besondre Lust, noch als besondre Plage.", "tokens": ["Nicht", "als", "be\u00b7sond\u00b7re", "Lust", ",", "noch", "als", "be\u00b7sond\u00b7re", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ADJA", "NN", "$,", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In dem Gef\u00fchle sollst du sie durch deins nicht st\u00f6ren,", "tokens": ["In", "dem", "Ge\u00b7f\u00fch\u00b7le", "sollst", "du", "sie", "durch", "deins", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "APPR", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht das deinige durch Tr\u00e4umerei beth\u00f6ren.", "tokens": ["Und", "nicht", "das", "dei\u00b7ni\u00b7ge", "durch", "Tr\u00e4u\u00b7me\u00b7rei", "be\u00b7th\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "PPOSS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}