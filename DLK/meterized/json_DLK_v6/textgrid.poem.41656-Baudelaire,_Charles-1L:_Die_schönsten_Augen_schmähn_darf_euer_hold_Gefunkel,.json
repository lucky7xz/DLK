{"textgrid.poem.41656": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die sch\u00f6nsten Augen schm\u00e4hn darf euer hold Gefunkel,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nsten Augen schm\u00e4hn darf euer hold Gefunkel,", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Au\u00b7gen", "schm\u00e4hn", "darf", "eu\u00b7er", "hold", "Ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Augen meines Kinds, drin lieblich ruht und wacht", "tokens": ["Ihr", "Au\u00b7gen", "mei\u00b7nes", "Kinds", ",", "drin", "lieb\u00b7lich", "ruht", "und", "wacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Etwas, das so gut und selig wie die Nacht!", "tokens": ["Ein", "Et\u00b7was", ",", "das", "so", "gut", "und", "se\u00b7lig", "wie", "die", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Augen, \u00fcber mich gie\u00dft euer s\u00fc\u00dfes Dunkel!", "tokens": ["Ihr", "Au\u00b7gen", ",", "\u00fc\u00b7ber", "mich", "gie\u00dft", "eu\u00b7er", "s\u00fc\u00b7\u00dfes", "Dun\u00b7kel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr Kinderaugen weit, geliebt und r\u00e4tselhaft,", "tokens": ["Ihr", "Kin\u00b7der\u00b7au\u00b7gen", "weit", ",", "ge\u00b7liebt", "und", "r\u00e4t\u00b7sel\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr gleicht dem Zaubertraum verborgner Grottenhallen,", "tokens": ["Ihr", "gleicht", "dem", "Zau\u00b7ber\u00b7traum", "ver\u00b7borg\u00b7ner", "Grot\u00b7ten\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo durch die D\u00e4mmerung der Schatten, die dort wallen,", "tokens": ["Wo", "durch", "die", "D\u00e4m\u00b7me\u00b7rung", "der", "Schat\u00b7ten", ",", "die", "dort", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein magisch Flimmern rinnt von seltner Sch\u00e4tze Kraft.", "tokens": ["Ein", "ma\u00b7gisch", "Flim\u00b7mern", "rinnt", "von", "selt\u00b7ner", "Sch\u00e4t\u00b7ze", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Kind hat Augen, die tiefdunkel und verkl\u00e4rend,", "tokens": ["Mein", "Kind", "hat", "Au\u00b7gen", ",", "die", "tief\u00b7dun\u00b7kel", "und", "ver\u00b7kl\u00e4\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PRELS", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie du, gewaltge Nacht, erhellt, wie du zu schaun!", "tokens": ["Wie", "du", ",", "ge\u00b7walt\u00b7ge", "Nacht", ",", "er\u00b7hellt", ",", "wie", "du", "zu", "schaun", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ihrem Feuer eint sich Liebe und Vertraun,", "tokens": ["In", "ih\u00b7rem", "Feu\u00b7er", "eint", "sich", "Lie\u00b7be", "und", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die scheinen tief im Grund, bald sch\u00fcchtern bald begehrend.", "tokens": ["Die", "schei\u00b7nen", "tief", "im", "Grund", ",", "bald", "sch\u00fcch\u00b7tern", "bald", "be\u00b7geh\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPRART", "NN", "$,", "ADV", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}