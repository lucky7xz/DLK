{"textgrid.poem.43147": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist ein Kichern, ein Jubeln und Lachen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein Kichern, ein Jubeln und Lachen,", "tokens": ["Das", "ist", "ein", "Ki\u00b7chern", ",", "ein", "Ju\u00b7beln", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So kindlich heiter und kindlich warm,", "tokens": ["So", "kind\u00b7lich", "hei\u00b7ter", "und", "kind\u00b7lich", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es sch\u00e4kert dr\u00fcben am Fenster die Mutter,", "tokens": ["Es", "sch\u00e4\u00b7kert", "dr\u00fc\u00b7ben", "am", "Fens\u00b7ter", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr jauchzendes Kindlein im wiegenden Arm.", "tokens": ["Ihr", "jauch\u00b7zen\u00b7des", "Kin\u00b7dlein", "im", "wie\u00b7gen\u00b7den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie sie so t\u00e4nzelt und singend scherzet,", "tokens": ["Und", "wie", "sie", "so", "t\u00e4n\u00b7zelt", "und", "sin\u00b7gend", "scher\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das kleine Wesen so innig k\u00fc\u00dft,", "tokens": ["Das", "klei\u00b7ne", "We\u00b7sen", "so", "in\u00b7nig", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da f\u00fchlt sie, es ist ihr Eines und Alles,", "tokens": ["Da", "f\u00fchlt", "sie", ",", "es", "ist", "ihr", "Ei\u00b7nes", "und", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "PIS", "KON", "PIS", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In dem sie das Gl\u00fcck und die Zukunft begr\u00fc\u00dft.", "tokens": ["In", "dem", "sie", "das", "Gl\u00fcck", "und", "die", "Zu\u00b7kunft", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "O, gl\u00fcckliche Mutter! \u2013 Vor Noth und Schmerzen", "tokens": ["O", ",", "gl\u00fcck\u00b7li\u00b7che", "Mut\u00b7ter", "!", "\u2013", "Vor", "Noth", "und", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "$(", "APPR", "NN", "KON", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beh\u00fcte Dein Kindlein treu und lind \u2013", "tokens": ["Be\u00b7h\u00fc\u00b7te", "Dein", "Kin\u00b7dlein", "treu", "und", "lind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es giebt auf der Erde manch' einsame Mutter", "tokens": ["Es", "giebt", "auf", "der", "Er\u00b7de", "man\u00b7ch'", "ein\u00b7sa\u00b7me", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und unter der Erde \u2013 manch' liebes Kind!", "tokens": ["Und", "un\u00b7ter", "der", "Er\u00b7de", "\u2013", "man\u00b7ch'", "lie\u00b7bes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}