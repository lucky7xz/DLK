{"dta.poem.751": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.  An seinen H. Bruder/ au\u00df den  \n worten de\u00df H. Cypriani.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Entschlaffen/ nicht erw\u00fcrgt/ verschickt/ doch nicht verlohr&#7869;. ", "tokens": ["Ent\u00b7schlaf\u00b7fen", "/", "nicht", "er\u00b7w\u00fcrgt", "/", "ver\u00b7schickt", "/", "doch", "nicht", "ver\u00b7lohr", "&#7869;", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "XML_entity", "punct"], "pos": ["NN", "$(", "PTKNEG", "VVPP", "$(", "VVPP", "$(", "ADV", "PTKNEG", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verreis\u2019t/ doch nur voran/ sind diese so der welt", "tokens": ["Ver\u00b7reis't", "/", "doch", "nur", "vo\u00b7ran", "/", "sind", "die\u00b7se", "so", "der", "welt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "ADV", "ADV", "PTKVZ", "$(", "VAFIN", "PDS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun gute Nacht gesagt/ vnd in dem wolekenzelt", "tokens": ["Nun", "gu\u00b7te", "Nacht", "ge\u00b7sagt", "/", "vnd", "in", "dem", "wo\u00b7le\u00b7ken\u00b7zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$(", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da keine drangsall mehr jhn jhren sitz erkohren ", "tokens": ["Da", "kei\u00b7ne", "drangs\u00b7all", "mehr", "jhn", "jhren", "sitz", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier waren sie recht todt/ dort sindt sie newgebohren.", "tokens": ["Hier", "wa\u00b7ren", "sie", "recht", "todt", "/", "dort", "sindt", "sie", "new\u00b7ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$(", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie/ da\u00df jhr hintritt vns denn so bes", "tokens": ["Wie", "/", "da\u00df", "jhr", "hin\u00b7tritt", "vns", "denn", "so", "bes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$(", "KOUS", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie da\u00df man solch geschrey vnd solche klag' anstelt? ", "tokens": ["Wie", "da\u00df", "man", "solch", "ge\u00b7schrey", "vnd", "sol\u00b7che", "klag'", "an\u00b7stelt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "PIAT", "NN", "KON", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(verlangen geht noch hin!) wir f\u00fcllen Hertz vnd Ohren", "tokens": ["(", "ver\u00b7lan\u00b7gen", "geht", "noch", "hin", "!", ")", "wir", "f\u00fcl\u00b7len", "Hertz", "vnd", "Oh\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "VVFIN", "ADV", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit ach/ vnd heulen an! ", "tokens": ["Mit", "ach", "/", "vnd", "heu\u00b7len", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "XY", "$(", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir machen alles schwartz. Es scheint in vnserm leydt", "tokens": ["Wir", "ma\u00b7chen", "al\u00b7les", "schwartz", ".", "Es", "scheint", "in", "vn\u00b7serm", "leydt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als wenn der glaub\u2019 in vns erloschen vnd gestorben.", "tokens": ["Als", "wenn", "der", "glaub'", "in", "vns", "er\u00b7lo\u00b7schen", "vnd", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "VVFIN", "APPR", "PPER", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wir reden mit dem ", "tokens": ["Wir", "re\u00b7den", "mit", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dis laugnet vnser thun/ in dem wir jhren Todt", "tokens": ["Dis", "laug\u00b7net", "vn\u00b7ser", "thun", "/", "in", "dem", "wir", "jhren", "Todt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "$(", "APPR", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So klagen als wenn Leib vnd Seele gantz verdorben.", "tokens": ["So", "kla\u00b7gen", "als", "wenn", "Leib", "vnd", "See\u00b7le", "gantz", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "KOUS", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}