{"textgrid.poem.63361": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Skat", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hocken, ihre Socken schwei\u00dfgetr\u00e4nkt,", "tokens": ["Sie", "ho\u00b7cken", ",", "ih\u00b7re", "So\u00b7cken", "schwei\u00df\u00b7ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Leib bedeckt mit braven J\u00e4gerhemden.", "tokens": ["Den", "Leib", "be\u00b7deckt", "mit", "bra\u00b7ven", "J\u00e4\u00b7ger\u00b7hem\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie dulden keinen zugereisten Fremden,", "tokens": ["Sie", "dul\u00b7den", "kei\u00b7nen", "zu\u00b7ge\u00b7reis\u00b7ten", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Groschen wird versch\u00e4mt gesenkt.", "tokens": ["Und", "je\u00b7der", "Gro\u00b7schen", "wird", "ver\u00b7sch\u00e4mt", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Blick am Blatt steil wie am Galgen h\u00e4ngt.", "tokens": ["Der", "Blick", "am", "Blatt", "steil", "wie", "am", "Gal\u00b7gen", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "KOKOM", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man teilt. Ein scheuer Jude fl\u00fcstert: \u00bbWemm denn?\u00ab", "tokens": ["Man", "teilt", ".", "Ein", "scheu\u00b7er", "Ju\u00b7de", "fl\u00fcs\u00b7tert", ":", "\u00bb", "Wemm", "denn", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "$.", "$(", "PWS", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Turnvereinler preist den Kreuzer Emden,", "tokens": ["Ein", "Turn\u00b7ver\u00b7ein\u00b7ler", "preist", "den", "Kreu\u00b7zer", "Em\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indem er feurig seine R\u00f6llchen schwenkt.", "tokens": ["In\u00b7dem", "er", "feu\u00b7rig", "sei\u00b7ne", "R\u00f6ll\u00b7chen", "schwenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwei Herrn erbleichen, weil sie stark verlieren", "tokens": ["Zwei", "Herrn", "er\u00b7blei\u00b7chen", ",", "weil", "sie", "stark", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(so zwei Mark achtzig, wenn ich richtig sah.", "tokens": ["(", "so", "zwei", "Mark", "acht\u00b7zig", ",", "wenn", "ich", "rich\u00b7tig", "sah", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "CARD", "NN", "CARD", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir geht das Spiel betr\u00e4chtlich an die Nieren,", "tokens": ["Mir", "geht", "das", "Spiel", "be\u00b7tr\u00e4cht\u00b7lich", "an", "die", "Nie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beziehungsweise die es spielen...) \u00bbTja\u00ab,", "tokens": ["Be\u00b7zie\u00b7hungs\u00b7wei\u00b7se", "die", "es", "spie\u00b7len", "...", ")", "\u00bb", "Tja", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["NN", "ART", "PPER", "VVINF", "$(", "$(", "$(", "NE", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Strahlt der Herr Apotheker \u00bbGrand mit Vieren\u00ab", "tokens": ["Strahlt", "der", "Herr", "A\u00b7pot\u00b7he\u00b7ker", "\u00bb", "Grand", "mit", "Vie\u00b7ren", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$(", "NN", "APPR", "NN", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Und f\u00fchlt als Sohn sich der Germania.", "tokens": ["Und", "f\u00fchlt", "als", "Sohn", "sich", "der", "Ger\u00b7ma\u00b7nia", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}