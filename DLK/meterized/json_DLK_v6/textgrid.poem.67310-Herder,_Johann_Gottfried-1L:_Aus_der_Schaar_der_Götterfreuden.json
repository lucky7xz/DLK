{"textgrid.poem.67310": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus der Schaar der G\u00f6tterfreuden", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der Schaar der G\u00f6tterfreuden", "tokens": ["Aus", "der", "Schaar", "der", "G\u00f6t\u00b7ter\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stahl die j\u00fcngste Freude sich,", "tokens": ["Stahl", "die", "j\u00fcngs\u00b7te", "Freu\u00b7de", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Flei\u00df, ein Sohn der Leiden,", "tokens": ["Und", "der", "Flei\u00df", ",", "ein", "Sohn", "der", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nahte zu ihr jugendlich.", "tokens": ["Nah\u00b7te", "zu", "ihr", "ju\u00b7gend\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unschuld war in ihren Mienen,", "tokens": ["Un\u00b7schuld", "war", "in", "ih\u00b7ren", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treue war in seinem Blick,", "tokens": ["Treu\u00b7e", "war", "in", "sei\u00b7nem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Liebe zwischen ihnen", "tokens": ["Und", "die", "Lie\u00b7be", "zwi\u00b7schen", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stiftete der Beiden Gl\u00fcck.", "tokens": ["Stif\u00b7te\u00b7te", "der", "Bei\u00b7den", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbich ermatte,\u00ab sprach die Sch\u00f6ne;", "tokens": ["\u00bb", "ich", "er\u00b7mat\u00b7te", ",", "\u00ab", "sprach", "die", "Sch\u00f6\u00b7ne", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbgieb mir Deine sichre Hand!\u00ab", "tokens": ["\u00bb", "gieb", "mir", "Dei\u00b7ne", "sich\u00b7re", "Hand", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbnimm sie!\u00ab sprach er; Eintracht kr\u00f6ne", "tokens": ["\u00bb", "nimm", "sie", "!", "\u00ab", "sprach", "er", ";", "Ein\u00b7tracht", "kr\u00f6\u00b7ne"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$.", "$(", "VVFIN", "PPER", "$.", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Beider treues Band.\u00ab", "tokens": ["Un\u00b7ser", "Bei\u00b7der", "treu\u00b7es", "Band", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also wohnten sie im Schatten,", "tokens": ["Al\u00b7so", "wohn\u00b7ten", "sie", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter aller G\u00f6tter Gunst,", "tokens": ["Un\u00b7ter", "al\u00b7ler", "G\u00f6t\u00b7ter", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und das Kind, das Beide hatten,", "tokens": ["Und", "das", "Kind", ",", "das", "Bei\u00b7de", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War ein sch\u00f6nes Kind: die Kunst.", "tokens": ["War", "ein", "sch\u00f6\u00b7nes", "Kind", ":", "die", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von der Mutter lebte F\u00fclle,", "tokens": ["Von", "der", "Mut\u00b7ter", "leb\u00b7te", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6tterf\u00fcll' in ihrer Brust,", "tokens": ["G\u00f6t\u00b7ter\u00b7f\u00fcll'", "in", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Vater gab ihr Stille,", "tokens": ["Und", "der", "Va\u00b7ter", "gab", "ihr", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flei\u00df und Emsigkeit zur Lust.", "tokens": ["Flei\u00df", "und", "Em\u00b7sig\u00b7keit", "zur", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sorgsam hat er sie erzogen,", "tokens": ["Sorg\u00b7sam", "hat", "er", "sie", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00e4rtlich hat sie sie ges\u00e4ugt;", "tokens": ["Z\u00e4rt\u00b7lich", "hat", "sie", "sie", "ge\u00b7s\u00e4ugt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "G\u00f6tter waren ihr gewogen,", "tokens": ["G\u00f6t\u00b7ter", "wa\u00b7ren", "ihr", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Menschen waren ihr geneigt.", "tokens": ["Men\u00b7schen", "wa\u00b7ren", "ihr", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber als, sie zu verm\u00e4hlen,", "tokens": ["A\u00b7ber", "als", ",", "sie", "zu", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun die frohe Zeit erschien,", "tokens": ["Nun", "die", "fro\u00b7he", "Zeit", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer der G\u00f6tter wird sie w\u00e4hlen?", "tokens": ["Wer", "der", "G\u00f6t\u00b7ter", "wird", "sie", "w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wem der Menschen wird sie bl\u00fchn?", "tokens": ["Wem", "der", "Men\u00b7schen", "wird", "sie", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen Erd' und Himmel schwebet", "tokens": ["Zwi\u00b7schen", "Erd'", "und", "Him\u00b7mel", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie, der Einsamkeit geweiht;", "tokens": ["Sie", ",", "der", "Ein\u00b7sam\u00b7keit", "ge\u00b7weiht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn der Mutter Gottheit lebet", "tokens": ["Denn", "der", "Mut\u00b7ter", "Got\u00b7theit", "le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Vaters Sterblichkeit.", "tokens": ["In", "des", "Va\u00b7ters", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Verlorne zu begl\u00fccken,", "tokens": ["Die", "Ver\u00b7lor\u00b7ne", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaute Jupiter hinab.", "tokens": ["Schau\u00b7te", "Ju\u00b7pi\u00b7ter", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbunsern Himmel soll sie schm\u00fccken,", "tokens": ["\u00bb", "un\u00b7sern", "Him\u00b7mel", "soll", "sie", "schm\u00fc\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie, die nur der Himmel gab!", "tokens": ["Sie", ",", "die", "nur", "der", "Him\u00b7mel", "gab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Aus dem Chor der G\u00f6tterjugend", "tokens": ["Aus", "dem", "Chor", "der", "G\u00f6t\u00b7ter\u00b7ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4re Thalia verbannt?", "tokens": ["W\u00e4\u00b7re", "Tha\u00b7lia", "ver\u00b7bannt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Unschuld und Du, frohe Tugend,", "tokens": ["Un\u00b7schuld", "und", "Du", ",", "fro\u00b7he", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Holet sie in unser Land!\u00ab", "tokens": ["Ho\u00b7let", "sie", "in", "un\u00b7ser", "Land", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unschuld und die Tugend stiegen", "tokens": ["Un\u00b7schuld", "und", "die", "Tu\u00b7gend", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Schwester Einsamkeit,", "tokens": ["In", "der", "Schwes\u00b7ter", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus ihrer Beider Z\u00fcgen", "tokens": ["Und", "aus", "ih\u00b7rer", "Bei\u00b7der", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuf sie selbst sich G\u00f6ttlichkeit.", "tokens": ["Schuf", "sie", "selbst", "sich", "G\u00f6tt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unabtrennlich stets von Beiden,", "tokens": ["Un\u00b7ab\u00b7trenn\u00b7lich", "stets", "von", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PIS", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Ward sie wie die Anmuth sch\u00f6n,", "tokens": ["Ward", "sie", "wie", "die", "An\u00b7muth", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Chor der G\u00f6tterfreuden", "tokens": ["Und", "im", "Chor", "der", "G\u00f6t\u00b7ter\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tanzen jetzt ", "tokens": ["Tan\u00b7zen", "jetzt"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}}}}