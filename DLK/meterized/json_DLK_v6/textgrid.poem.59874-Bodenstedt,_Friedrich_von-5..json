{"textgrid.poem.59874": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlag die Tschadra zur\u00fcck! Was verh\u00fcllst du dich?", "tokens": ["Schlag", "die", "Tschad\u00b7ra", "zu\u00b7r\u00fcck", "!", "Was", "ver\u00b7h\u00fcllst", "du", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Verh\u00fcllt auch die Blume des Gartens sich?", "tokens": ["Ver\u00b7h\u00fcllt", "auch", "die", "Blu\u00b7me", "des", "Gar\u00b7tens", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "PRF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hat dich nicht Gott, wie der Blume Pracht,", "tokens": ["Und", "hat", "dich", "nicht", "Gott", ",", "wie", "der", "Blu\u00b7me", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "NN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Erde zur Zierde, zur Sch\u00f6nheit gemacht?", "tokens": ["Der", "Er\u00b7de", "zur", "Zier\u00b7de", ",", "zur", "Sch\u00f6n\u00b7heit", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Schuf er all diesen Glanz, diese Herrlichkeit,", "tokens": ["Schuf", "er", "all", "die\u00b7sen", "Glanz", ",", "die\u00b7se", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Zu verbl\u00fchn in dumpfer Verborgenheit?", "tokens": ["Zu", "ver\u00b7bl\u00fchn", "in", "dum\u00b7pfer", "Ver\u00b7bor\u00b7gen\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "--+-++-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Schlag die Tschadra zur\u00fcck! La\u00df alle Welt sehn,", "tokens": ["Schlag", "die", "Tschad\u00b7ra", "zu\u00b7r\u00fcck", "!", "La\u00df", "al\u00b7le", "Welt", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "PTKVZ", "$.", "VVIMP", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da\u00df auf Erden wie du, Kind, kein M\u00e4dchen so sch\u00f6n!", "tokens": ["Da\u00df", "auf", "Er\u00b7den", "wie", "du", ",", "Kind", ",", "kein", "M\u00e4d\u00b7chen", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KOKOM", "PPER", "$,", "NN", "$,", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "La\u00df die Augen herzz\u00fcndende Funken spr\u00fchn,", "tokens": ["La\u00df", "die", "Au\u00b7gen", "herz\u00b7z\u00fcn\u00b7den\u00b7de", "Fun\u00b7ken", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "La\u00df die Lippen in rosigem L\u00e4cheln gl\u00fchn,", "tokens": ["La\u00df", "die", "Lip\u00b7pen", "in", "ro\u00b7si\u00b7gem", "L\u00e4\u00b7cheln", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Da\u00df dich, Holde, kein anderer Schleier umschwebt,", "tokens": ["Da\u00df", "dich", ",", "Hol\u00b7de", ",", "kein", "an\u00b7de\u00b7rer", "Schlei\u00b7er", "um\u00b7schwebt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Als mit dem dich das Dunkel der N\u00e4chte umwebt!", "tokens": ["Als", "mit", "dem", "dich", "das", "Dun\u00b7kel", "der", "N\u00e4ch\u00b7te", "um\u00b7webt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRELS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schlag die Tschadra zur\u00fcck! Solch ein Antlitz sah", "tokens": ["Schlag", "die", "Tschad\u00b7ra", "zu\u00b7r\u00fcck", "!", "Solch", "ein", "Ant\u00b7litz", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NE", "PTKVZ", "$.", "PIS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nie zu Stambul der Harem des Padischah \u2013", "tokens": ["Nie", "zu", "Stam\u00b7bul", "der", "Ha\u00b7rem", "des", "Pa\u00b7di\u00b7schah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nie s\u00e4umte zwei Augen so gro\u00df und klar", "tokens": ["Nie", "s\u00e4um\u00b7te", "zwei", "Au\u00b7gen", "so", "gro\u00df", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der langen Wimpern seid'nes Haar \u2013", "tokens": ["Der", "lan\u00b7gen", "Wim\u00b7pern", "sei\u00b7d'\u00b7nes", "Haar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Drum erhebe den Blick, schlag die Tschadra zur\u00fcck!", "tokens": ["Drum", "er\u00b7he\u00b7be", "den", "Blick", ",", "schlag", "die", "Tschad\u00b7ra", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NE", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Dir selbst zum Triumphe, den Menschen zum Gl\u00fcck!", "tokens": ["Dir", "selbst", "zum", "Tri\u00b7um\u00b7phe", ",", "den", "Men\u00b7schen", "zum", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}