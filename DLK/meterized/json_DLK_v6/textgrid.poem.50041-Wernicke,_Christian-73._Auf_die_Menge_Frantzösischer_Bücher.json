{"textgrid.poem.50041": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "73. Auf die Menge Frantz\u00f6sischer B\u00fccher", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass ein ", "tokens": ["Dass", "ein"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Manch ", "tokens": ["Manch"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "In Holland uns beschert;", "tokens": ["In", "Hol\u00b7land", "uns", "be\u00b7schert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dass er uns nicht sein ", "tokens": ["Dass", "er", "uns", "nicht", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VAINF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dass Jahr durch ", "tokens": ["Dass", "Jahr", "durch"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Ist nicht Verwundernswehrt;", "tokens": ["Ist", "nicht", "Ver\u00b7wun\u00b7derns\u00b7wehrt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er macht kaum seine Feder nass,", "tokens": ["Er", "macht", "kaum", "sei\u00b7ne", "Fe\u00b7der", "nass", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und k\u00fcnstelt ohne M\u00fch':", "tokens": ["Und", "k\u00fcns\u00b7telt", "oh\u00b7ne", "M\u00fch'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wahr ist's, ", "tokens": ["Wahr", "ist's", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-", "measure": "trochaic.single"}}}}}