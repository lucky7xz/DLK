{"dta.poem.2916": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "115.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein tugendhafter Mann denkt nie, weil es vergebens", "tokens": ["Ein", "tu\u00b7gend\u00b7haf\u00b7ter", "Mann", "denkt", "nie", ",", "weil", "es", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu denken ist, des Tods, er denkt allein des Lebens.", "tokens": ["Zu", "den\u00b7ken", "ist", ",", "des", "Tods", ",", "er", "denkt", "al\u00b7lein", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Todes nie, weil nie der Tod ihm schaden kann;", "tokens": ["Des", "To\u00b7des", "nie", ",", "weil", "nie", "der", "Tod", "ihm", "scha\u00b7den", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lebens nur, weil nur im Leben wirkt der Mann.", "tokens": ["Des", "Le\u00b7bens", "nur", ",", "weil", "nur", "im", "Le\u00b7ben", "wirkt", "der", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So denkt ein Tapfrer nicht, weil er zuvor bedacht", "tokens": ["So", "denkt", "ein", "Tapf\u00b7rer", "nicht", ",", "weil", "er", "zu\u00b7vor", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn ein f\u00fcr allemal, des Todes in der Schlacht.", "tokens": ["Ihn", "ein", "f\u00fcr", "al\u00b7le\u00b7mal", ",", "des", "To\u00b7des", "in", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und also in der Schlacht des Lebens, die wir k\u00e4mpfen,", "tokens": ["Und", "al\u00b7so", "in", "der", "Schlacht", "des", "Le\u00b7bens", ",", "die", "wir", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df nie des Todes Furcht die R\u00fcstigkeit dir d\u00e4mpfen.", "tokens": ["La\u00df", "nie", "des", "To\u00b7des", "Furcht", "die", "R\u00fcs\u00b7tig\u00b7keit", "dir", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wenn des dunklen du gedenken sollst, so thu", "tokens": ["Und", "wenn", "des", "dunk\u00b7len", "du", "ge\u00b7den\u00b7ken", "sollst", ",", "so", "thu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es so wie wer gedenkt am hei\u00dfen Tag der Ruh;", "tokens": ["Es", "so", "wie", "wer", "ge\u00b7denkt", "am", "hei\u00b7\u00dfen", "Tag", "der", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOKOM", "PWS", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Den der Gedanke st\u00e4rkt, da\u00df er die Nacht soll ruhn,", "tokens": ["Den", "der", "Ge\u00b7dan\u00b7ke", "st\u00e4rkt", ",", "da\u00df", "er", "die", "Nacht", "soll", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fr\u00fch erwachen, neu gest\u00e4rkt sein Werk zu thun.", "tokens": ["Und", "fr\u00fch", "er\u00b7wa\u00b7chen", ",", "neu", "ge\u00b7st\u00e4rkt", "sein", "Werk", "zu", "thun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "ADJD", "VVPP", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}