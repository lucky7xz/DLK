{"textgrid.poem.47883": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer jener Weisen Reis'/ aus weit entlegnem Ort/", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer jener Weisen Reis'/ aus weit entlegnem Ort/", "tokens": ["Wer", "je\u00b7ner", "Wei\u00b7sen", "Reis'", "/", "aus", "weit", "ent\u00b7leg\u00b7nem", "Ort", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "NE", "$(", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu sehen tr\u00e4get Lust/ wie sie gen Salem fort", "tokens": ["zu", "se\u00b7hen", "tr\u00e4\u00b7get", "Lust", "/", "wie", "sie", "gen", "Sa\u00b7lem", "fort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "NN", "$(", "PWAV", "PPER", "APPR", "NE", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gefolget Fl\u00fcgelschnell dem neuen Wunderstern/", "tokens": ["ge\u00b7fol\u00b7get", "Fl\u00fc\u00b7gel\u00b7schnell", "dem", "neu\u00b7en", "Wun\u00b7ders\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "der ihnen Weg und Steg bedeutet aus der Fern/", "tokens": ["der", "ih\u00b7nen", "Weg", "und", "Steg", "be\u00b7deu\u00b7tet", "aus", "der", "Fern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der komm'/ und h\u00f6re zu/ wie Klajus solches weist.", "tokens": ["der", "komm'", "/", "und", "h\u00f6\u00b7re", "zu", "/", "wie", "Kla\u00b7jus", "sol\u00b7ches", "weist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$(", "KON", "VVFIN", "PTKZU", "$(", "KOKOM", "NE", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das grosse Himmelsliecht/ das ewig wird gepreist/", "tokens": ["Das", "gros\u00b7se", "Him\u00b7mels\u00b7liecht", "/", "das", "e\u00b7wig", "wird", "ge\u00b7preist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "f\u00fcr dem das Sonnenrad mit seinem Glantz erbleicht/", "tokens": ["f\u00fcr", "dem", "das", "Son\u00b7nen\u00b7rad", "mit", "sei\u00b7nem", "Glantz", "er\u00b7bleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "besagt/ wie Gottes Sohn sich unserm Fleische gleicht.", "tokens": ["be\u00b7sagt", "/", "wie", "Got\u00b7tes", "Sohn", "sich", "un\u00b7serm", "Flei\u00b7sche", "gleicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KOKOM", "NN", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wem anzuschauen liebt das trotzig' Angesicht", "tokens": ["Wem", "an\u00b7zu\u00b7schau\u00b7en", "liebt", "das", "trot\u00b7zig'", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVIZU", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herodes/ welcher schnaufft nach Greuelhalsgericht/", "tokens": ["He\u00b7ro\u00b7des", "/", "wel\u00b7cher", "schnaufft", "nach", "Greu\u00b7el\u00b7hals\u00b7ge\u00b7richt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWS", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der auf viel tausend Weis' unschuldig Blut vergeust/", "tokens": ["der", "auf", "viel", "tau\u00b7send", "Weis'", "un\u00b7schul\u00b7dig", "Blut", "ver\u00b7geust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "CARD", "NN", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und den/ da\u00df er nicht all in Grund zernicht/ verdreust;", "tokens": ["und", "den", "/", "da\u00df", "er", "nicht", "all", "in", "Grund", "zer\u00b7nicht", "/", "ver\u00b7dreust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "$(", "KOUS", "PPER", "PTKNEG", "PIAT", "APPR", "NN", "VVPP", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wie manchen zarten Sohn/ den noch die Mutter kust'/", "tokens": ["wie", "man\u00b7chen", "zar\u00b7ten", "Sohn", "/", "den", "noch", "die", "Mut\u00b7ter", "kust'", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "hab dieses Rasers Schwert erw\u00fcrget an der Brust/", "tokens": ["hab", "die\u00b7ses", "Ra\u00b7sers", "Schwert", "er\u00b7w\u00fcr\u00b7get", "an", "der", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NE", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "zerschmissen/ angespist/ geviertelt und zerst\u00fckkt:", "tokens": ["zer\u00b7schmis\u00b7sen", "/", "an\u00b7ges\u00b7pist", "/", "ge\u00b7vier\u00b7telt", "und", "zer\u00b7st\u00fckkt", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "VVPP", "$(", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der komm' und h\u00f6r es an/ von Klajus ausgedr\u00fckkt.", "tokens": ["der", "komm'", "und", "h\u00f6r", "es", "an", "/", "von", "Kla\u00b7jus", "aus\u00b7ge\u00b7dr\u00fckkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$(", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wem zu betrachten liebt des Teutschlands Trauerfall/", "tokens": ["Wem", "zu", "be\u00b7trach\u00b7ten", "liebt", "des", "Teutschlands", "Trau\u00b7er\u00b7fall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "das/ gleich dem Mutterhertz/ mit Threnen sonder Zahl/", "tokens": ["das", "/", "gleich", "dem", "Mut\u00b7ter\u00b7hertz", "/", "mit", "Thre\u00b7nen", "son\u00b7der", "Zahl", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "ART", "NN", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "betr\u00fcbet/ ungestalt/ erschwartzet und verwildt/", "tokens": ["be\u00b7tr\u00fc\u00b7bet", "/", "un\u00b7ge\u00b7stalt", "/", "er\u00b7schwart\u00b7zet", "und", "ver\u00b7wildt", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJD", "$(", "VVFIN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "schreit ob Herodis Grimm von neuem angebildt:", "tokens": ["schreit", "ob", "He\u00b7ro\u00b7dis", "Grimm", "von", "neu\u00b7em", "an\u00b7ge\u00b7bildt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NE", "NE", "APPR", "ADJA", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "der komm und h\u00f6re zu/ wie Klajus dieses sagt/", "tokens": ["der", "komm", "und", "h\u00f6\u00b7re", "zu", "/", "wie", "Kla\u00b7jus", "die\u00b7ses", "sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "KON", "VVFIN", "PTKZU", "$(", "KOKOM", "NE", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und mit Kunstteutschem Vers die Grausamkeit beklagt.", "tokens": ["und", "mit", "Kunst\u00b7teut\u00b7schem", "Vers", "die", "Grau\u00b7sam\u00b7keit", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Komt dann/ wann morgen fr\u00fc ist Chor und Predigt aus", "tokens": ["Komt", "dann", "/", "wann", "mor\u00b7gen", "fr\u00fc", "ist", "Chor", "und", "Pre\u00b7digt", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "PWAV", "ADV", "ADJD", "VAFIN", "NN", "KON", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und alles Christenvolk sich wieder f\u00fcgt nach Haus.", "tokens": ["und", "al\u00b7les", "Chris\u00b7ten\u00b7volk", "sich", "wie\u00b7der", "f\u00fcgt", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}