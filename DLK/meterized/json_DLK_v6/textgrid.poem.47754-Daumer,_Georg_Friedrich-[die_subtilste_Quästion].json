{"textgrid.poem.47754": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die subtilste Qu\u00e4stion]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die subtilste Qu\u00e4stion,", "tokens": ["Die", "sub\u00b7tils\u00b7te", "Qu\u00e4s\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Problem, so schwer, wie keines,", "tokens": ["Ein", "Prob\u00b7lem", ",", "so", "schwer", ",", "wie", "kei\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,", "PWAV", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist Suleima's Taille mir.", "tokens": ["Ist", "Su\u00b7lei\u00b7ma's", "Tail\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie ist so wunderfein,", "tokens": ["Denn", "sie", "ist", "so", "wun\u00b7der\u00b7fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie als ein Nichts erscheinet,", "tokens": ["Da\u00df", "sie", "als", "ein", "Nichts", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und doch ist sie auch ein Etwas;", "tokens": ["Und", "doch", "ist", "sie", "auch", "ein", "Et\u00b7was", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sein und Nichts, die Gegens\u00e4tze,", "tokens": ["Sein", "und", "Nichts", ",", "die", "Ge\u00b7gen\u00b7s\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PIS", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bilden hier ein einfach Eines \u2013", "tokens": ["Bil\u00b7den", "hier", "ein", "ein\u00b7fach", "Ei\u00b7nes", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADV", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mein Verstand, was sagst du hier?", "tokens": ["Mein", "Ver\u00b7stand", ",", "was", "sagst", "du", "hier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}