{"dta.poem.12675": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Heyraths-gl\u00fccke.  \n J. G. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mjr steht ein gro\u00dfes gl\u00fccke vor,", "tokens": ["Mjr", "steht", "ein", "gro\u00b7\u00dfes", "gl\u00fc\u00b7cke", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kan mit leichter m\u00fch zum ehe-mann itzt werden.", "tokens": ["Ich", "kan", "mit", "leich\u00b7ter", "m\u00fch", "zum", "e\u00b7he\u00b7mann", "itzt", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJD", "ADJD", "APPRART", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein \u2012 \u2012 \u2012 \u2012 \u2012 beut seine tochter aus,", "tokens": ["Ein", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "beut", "sei\u00b7ne", "toch\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "$(", "$(", "$(", "$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur mitaifft will er ihr zweytausend thaler geben.", "tokens": ["Zur", "mi\u00b7taifft", "will", "er", "ihr", "zweyt\u00b7au\u00b7send", "tha\u00b7ler", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPER", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und welchem sie gef\u00e4llt,", "tokens": ["Und", "wel\u00b7chem", "sie", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dem giebt er \u00fcberdi\u00df auch geld,", "tokens": ["Dem", "giebt", "er", "\u00fc\u00b7ber\u00b7di\u00df", "auch", "geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er noch l\u00e4nger kan auf hohen schulen leben.", "tokens": ["Da\u00df", "er", "noch", "l\u00e4n\u00b7ger", "kan", "auf", "ho\u00b7hen", "schu\u00b7len", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VMFIN", "APPR", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das m\u00e4gdgen ist auch gut genug,", "tokens": ["Das", "m\u00e4gd\u00b7gen", "ist", "auch", "gut", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wei\u00df sich artig auffzuf\u00fchren;", "tokens": ["Und", "wei\u00df", "sich", "ar\u00b7tig", "auff\u00b7zu\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie sie der m\u00e4nner hertz soll r\u00fchren,", "tokens": ["Wie", "sie", "der", "m\u00e4n\u00b7ner", "hertz", "soll", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dazu ist sie auch ziemlich klug.", "tokens": ["Da\u00b7zu", "ist", "sie", "auch", "ziem\u00b7lich", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Allein bey allen diesen gaben,", "tokens": ["Al\u00b7lein", "bey", "al\u00b7len", "die\u00b7sen", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mag ich gleichwol die kuh nicht mit dem kalbe haben:", "tokens": ["Mag", "ich", "gleich\u00b7wol", "die", "kuh", "nicht", "mit", "dem", "kal\u00b7be", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "APPR", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein schon gedrucktes pferd", "tokens": ["Ein", "schon", "ge\u00b7druck\u00b7tes", "pferd"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Ist keines frischen reuters werth.", "tokens": ["Ist", "kei\u00b7nes", "fri\u00b7schen", "reu\u00b7ters", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}