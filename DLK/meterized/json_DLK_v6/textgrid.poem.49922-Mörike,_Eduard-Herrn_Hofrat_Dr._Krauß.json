{"textgrid.poem.49922": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Herrn Hofrat Dr. Krau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der j\u00fcngsten in dem weitgepriesnen Schwesternchor", "tokens": ["Der", "j\u00fcng\u00b7sten", "in", "dem", "weit\u00b7ge\u00b7pri\u00b7es\u00b7nen", "Schwes\u00b7tern\u00b7chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Heilkr\u00e4ftger Nymphen unsres lieben Vaterlands,", "tokens": ["Heil\u00b7kr\u00e4ft\u00b7ger", "Nym\u00b7phen", "uns\u00b7res", "lie\u00b7ben", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wundert\u00e4tig im bescheidnen Tempel wohnt,", "tokens": ["Die", "wun\u00b7der\u00b7t\u00e4\u00b7tig", "im", "be\u00b7scheid\u00b7nen", "Tem\u00b7pel", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich selber still weissagend einen herrlichern;", "tokens": ["Sich", "sel\u00b7ber", "still", "weis\u00b7sa\u00b7gend", "ei\u00b7nen", "herr\u00b7li\u00b7chern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In deren schon verlorne Gunst du leise mich", "tokens": ["In", "de\u00b7ren", "schon", "ver\u00b7lor\u00b7ne", "Gunst", "du", "lei\u00b7se", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "ADV", "ADJA", "NN", "PPER", "ADJD", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An deiner priesterlichen Hand zur\u00fcckgef\u00fchrt:", "tokens": ["An", "dei\u00b7ner", "pries\u00b7ter\u00b7li\u00b7chen", "Hand", "zu\u00b7r\u00fcck\u00b7ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Heut in der fr\u00fchsten Morgenstunde go\u00df ich ihr", "tokens": ["Heut", "in", "der", "fr\u00fchs\u00b7ten", "Mor\u00b7gen\u00b7stun\u00b7de", "go\u00df", "ich", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Opfermilch, die reine, an der Schwelle aus,", "tokens": ["Die", "Op\u00b7fer\u00b7milch", ",", "die", "rei\u00b7ne", ",", "an", "der", "Schwel\u00b7le", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und schenkte dankbar ein kristallen Weihgef\u00e4\u00df.", "tokens": ["Und", "schenk\u00b7te", "dank\u00b7bar", "ein", "kris\u00b7tal\u00b7len", "Weih\u00b7ge\u00b7f\u00e4\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie aber, rauschend in der Tiefe, sprach dies Wort:", "tokens": ["Sie", "a\u00b7ber", ",", "rau\u00b7schend", "in", "der", "Tie\u00b7fe", ",", "sprach", "dies", "Wort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "APPR", "ART", "NN", "$,", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u00bbbring meinem Diener, deinem Freunde, den Pokal,", "tokens": ["\u00bb", "bring", "mei\u00b7nem", "Die\u00b7ner", ",", "dei\u00b7nem", "Freun\u00b7de", ",", "den", "Po\u00b7kal", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit jenes Gottes Feuergabe voll gef\u00fcllt,", "tokens": ["Mit", "je\u00b7nes", "Got\u00b7tes", "Feu\u00b7er\u00b7ga\u00b7be", "voll", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der meinen Berg mit seinen heiligen Ranken schm\u00fcckt,", "tokens": ["Der", "mei\u00b7nen", "Berg", "mit", "sei\u00b7nen", "hei\u00b7li\u00b7gen", "Ran\u00b7ken", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Obwohl er meine Lippen zu ber\u00fchren scheut.\u00ab", "tokens": ["Ob\u00b7wohl", "er", "mei\u00b7ne", "Lip\u00b7pen", "zu", "be\u00b7r\u00fch\u00b7ren", "scheut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}