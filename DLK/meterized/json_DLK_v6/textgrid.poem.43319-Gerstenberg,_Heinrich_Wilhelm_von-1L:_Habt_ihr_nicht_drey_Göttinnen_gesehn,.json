{"textgrid.poem.43319": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Habt ihr nicht drey G\u00f6ttinnen gesehn,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr nicht drey G\u00f6ttinnen gesehn,", "tokens": ["Habt", "ihr", "nicht", "drey", "G\u00f6t\u00b7tin\u00b7nen", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "CARD", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Naiv, und jung, und, wie die Liebe, sch\u00f6n?", "tokens": ["Naiv", ",", "und", "jung", ",", "und", ",", "wie", "die", "Lie\u00b7be", ",", "sch\u00f6n", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJD", "$,", "KON", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich sah sie dort im Garten gehn,", "tokens": ["Ich", "sah", "sie", "dort", "im", "Gar\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lief hieher, sie noch einmal zu sehn.", "tokens": ["Und", "lief", "hie\u00b7her", ",", "sie", "noch", "ein\u00b7mal", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sieh da! seyd Ihrs? \u2013 \u2013 So nah bey euch zu stehn,", "tokens": ["Sieh", "da", "!", "seyd", "Ihrs", "?", "\u2013", "\u2013", "So", "nah", "bey", "euch", "zu", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VAFIN", "PIS", "$.", "$(", "$(", "ADV", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und! \u2013 \u2013 Dummkopf! kann ich denn nicht sehn?", "tokens": ["Und", "!", "\u2013", "\u2013", "Dumm\u00b7kopf", "!", "kann", "ich", "denn", "nicht", "sehn", "?"], "token_info": ["word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "$(", "$(", "NN", "$.", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie reizend seyd Ihr, ach! \u2013 Ach Nymphen, kann es seyn,", "tokens": ["Wie", "rei\u00b7zend", "seyd", "Ihr", ",", "ach", "!", "\u2013", "Ach", "Nym\u00b7phen", ",", "kann", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "$,", "ITJ", "$.", "$(", "ITJ", "NN", "$,", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So g\u00f6nnt, vor andern T\u00e4ndeleyn,", "tokens": ["So", "g\u00f6nnt", ",", "vor", "an\u00b7dern", "T\u00e4n\u00b7de\u00b7leyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir Knaben das Verdienst, euch manchmal zu erfreun?", "tokens": ["Mir", "Kna\u00b7ben", "das", "Ver\u00b7dienst", ",", "euch", "manch\u00b7mal", "zu", "er\u00b7freun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich mag nun nicht zur\u00fcck nach Paphos. Nein!", "tokens": ["Ich", "mag", "nun", "nicht", "zu\u00b7r\u00fcck", "nach", "Pa\u00b7phos", ".", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "PTKVZ", "APPR", "NE", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr sollt mir mehr, als Paphos, seyn.", "tokens": ["Ihr", "sollt", "mir", "mehr", ",", "als", "Pa\u00b7phos", ",", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "KOUS", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}