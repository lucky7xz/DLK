{"textgrid.poem.61133": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2. Winter", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun weht auf der Heide der scharfe Nordost,", "tokens": ["Nun", "weht", "auf", "der", "Hei\u00b7de", "der", "schar\u00b7fe", "Nord\u00b7ost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am Vordach hangt der Zapfen von Eis,", "tokens": ["Am", "Vor\u00b7dach", "hangt", "der", "Zap\u00b7fen", "von", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Tannen sch\u00fctteln sich rings vor Frost,", "tokens": ["Die", "Tan\u00b7nen", "sch\u00fct\u00b7teln", "sich", "rings", "vor", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Feld und Kirchhof sind silberwei\u00df.", "tokens": ["Und", "Feld", "und", "Kirch\u00b7hof", "sind", "sil\u00b7ber\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Im Dorf verschneit liegt jeglicher Pfad,", "tokens": ["Im", "Dorf", "ver\u00b7schneit", "liegt", "jeg\u00b7li\u00b7cher", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ein Weg nur f\u00fchret zur Schenke allein,", "tokens": ["Ein", "Weg", "nur", "f\u00fch\u00b7ret", "zur", "Schen\u00b7ke", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und geh' ich dort grade des Abends spat,", "tokens": ["Und", "geh'", "ich", "dort", "gra\u00b7de", "des", "A\u00b7bends", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So tret' ich hinein;", "tokens": ["So", "tret'", "ich", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "O mein K\u00e4thchen, mein M\u00e4dchen, nun bringe mir Wein!", "tokens": ["O", "mein", "K\u00e4th\u00b7chen", ",", "mein", "M\u00e4d\u00b7chen", ",", "nun", "brin\u00b7ge", "mir", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "O liebes K\u00e4thchen, nun sing' mir ein Lied", "tokens": ["O", "lie\u00b7bes", "K\u00e4th\u00b7chen", ",", "nun", "sing'", "mir", "ein", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von der sonnigen, wonnigen Fr\u00fchlingszeit!", "tokens": ["Von", "der", "son\u00b7ni\u00b7gen", ",", "won\u00b7ni\u00b7gen", "Fr\u00fch\u00b7lings\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und wenn erst wieder die Schwalbe zieht,", "tokens": ["Und", "wenn", "erst", "wie\u00b7der", "die", "Schwal\u00b7be", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann sollst du schauen, wie hold sich's freit.", "tokens": ["Dann", "sollst", "du", "schau\u00b7en", ",", "wie", "hold", "sich's", "freit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wenn aufs neu der Winter sich naht,", "tokens": ["Und", "wenn", "aufs", "neu", "der", "Win\u00b7ter", "sich", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da schiert kein Wind uns von Ost und von West;", "tokens": ["Da", "schiert", "kein", "Wind", "uns", "von", "Ost", "und", "von", "West", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Am lodernden Herde sitzen wir spat", "tokens": ["Am", "lo\u00b7dern\u00b7den", "Her\u00b7de", "sit\u00b7zen", "wir", "spat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Im traulichen Nest", "tokens": ["Im", "trau\u00b7li\u00b7chen", "Nest"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und k\u00fcssen uns warm und umschlingen uns fest.", "tokens": ["Und", "k\u00fcs\u00b7sen", "uns", "warm", "und", "um\u00b7schlin\u00b7gen", "uns", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}