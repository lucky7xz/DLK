{"textgrid.poem.27945": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thraso preiste seine Wunden,", "tokens": ["Thra\u00b7so", "preis\u00b7te", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er im Gesicht empfunden,", "tokens": ["Die", "er", "im", "Ge\u00b7sicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Da er n\u00e4mlich wie ein Held", "tokens": ["Da", "er", "n\u00e4m\u00b7lich", "wie", "ein", "Held"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich f\u00fcr seinen Feind gestellt.", "tokens": ["Sich", "f\u00fcr", "sei\u00b7nen", "Feind", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey, sagt einer, da\u00df dir nicht", "tokens": ["Ey", ",", "sagt", "ei\u00b7ner", ",", "da\u00df", "dir", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses mehr schimpfft dein Gesicht,", "tokens": ["Die\u00b7ses", "mehr", "schimpfft", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "So enthalt dich, ob du fliehest,", "tokens": ["So", "ent\u00b7halt", "dich", ",", "ob", "du", "flie\u00b7hest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df du nicht zur\u00fccke sihest.", "tokens": ["Da\u00df", "du", "nicht", "zu\u00b7r\u00fc\u00b7cke", "si\u00b7hest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}