{"dta.poem.7105": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dies ein Mensch mit Ernst bedencket,", "tokens": ["Wenn", "dies", "ein", "Mensch", "mit", "Ernst", "be\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wird er ja mit Recht nicht zweifflen k\u00f6nnen,", "tokens": ["So", "wird", "er", "ja", "mit", "Recht", "nicht", "zweiff\u00b7len", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nicht derselbe GOTT, der ihm so viel geschencket,", "tokens": ["Da\u00df", "nicht", "der\u00b7sel\u00b7be", "GoTT", ",", "der", "ihm", "so", "viel", "ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch gut, auch m\u00e4chtig gnung, ein mehrers ihm zu g\u00f6nnen,", "tokens": ["Auch", "gut", ",", "auch", "m\u00e4ch\u00b7tig", "gnung", ",", "ein", "meh\u00b7rers", "ihm", "zu", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "$,", "ART", "PIS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Gutes immer noch zu mehren, zu vergr\u00f6ssern,", "tokens": ["Sein", "Gu\u00b7tes", "im\u00b7mer", "noch", "zu", "meh\u00b7ren", ",", "zu", "ver\u00b7gr\u00f6s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja ins unendliche sein Wesen zu verbessern.", "tokens": ["Ja", "ins", "un\u00b7end\u00b7li\u00b7che", "sein", "We\u00b7sen", "zu", "ver\u00b7bes\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPRART", "ADJA", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}