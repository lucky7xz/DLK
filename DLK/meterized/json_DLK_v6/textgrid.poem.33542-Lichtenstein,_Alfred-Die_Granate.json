{"textgrid.poem.33542": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Die Granate", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuerst ein heller knapper Paukenschlag,", "tokens": ["Zu\u00b7erst", "ein", "hel\u00b7ler", "knap\u00b7per", "Pau\u00b7ken\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Knall und Platzen in den blauen Tag.", "tokens": ["Ein", "Knall", "und", "Plat\u00b7zen", "in", "den", "blau\u00b7en", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann ein Ger\u00e4usch, wie wenn Raketen steigen.", "tokens": ["Dann", "ein", "Ge\u00b7r\u00e4usch", ",", "wie", "wenn", "Ra\u00b7ke\u00b7ten", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOKOM", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Auf Eisenschienen. Angst und langes Schweigen.", "tokens": ["Auf", "Ei\u00b7sen\u00b7schie\u00b7nen", ".", "Angst", "und", "lan\u00b7ges", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich in der Ferne Rauch und Fall,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "in", "der", "Fer\u00b7ne", "Rauch", "und", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein seltsam harter dunkler Widerhall.", "tokens": ["Ein", "selt\u00b7sam", "har\u00b7ter", "dunk\u00b7ler", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}