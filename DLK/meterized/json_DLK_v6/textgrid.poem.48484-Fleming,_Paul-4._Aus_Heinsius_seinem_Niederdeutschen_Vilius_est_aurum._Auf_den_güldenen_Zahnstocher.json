{"textgrid.poem.48484": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Aus Heinsius seinem Niederdeutschen Vilius est aurum. Auf den g\u00fcldenen Zahnstocher", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebes, k\u00f6stlichs Pfand, genommen aus dem Munde", "tokens": ["Du", "lie\u00b7bes", ",", "k\u00f6st\u00b7lichs", "Pfand", ",", "ge\u00b7nom\u00b7men", "aus", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der himmlischen Gestalt, k\u00f6mst zu mir diese Stunde", "tokens": ["der", "himm\u00b7li\u00b7schen", "Ge\u00b7stalt", ",", "k\u00f6mst", "zu", "mir", "die\u00b7se", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit s\u00fc\u00dfer Dieberei, zu mindern meine Qual,", "tokens": ["mit", "s\u00fc\u00b7\u00dfer", "Die\u00b7be\u00b7rei", ",", "zu", "min\u00b7dern", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil sie zuerste mir mein krankes Herze stahl,", "tokens": ["weil", "sie", "zu\u00b7ers\u00b7te", "mir", "mein", "kran\u00b7kes", "Her\u00b7ze", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "mein Herze, das sie mir verkn\u00fcpft mit s\u00fc\u00dfen Banden,", "tokens": ["mein", "Her\u00b7ze", ",", "das", "sie", "mir", "ver\u00b7kn\u00fcpft", "mit", "s\u00fc\u00b7\u00dfen", "Ban\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit dem sie spielt und das sie tr\u00e4gt in ihren Handen.", "tokens": ["mit", "dem", "sie", "spielt", "und", "das", "sie", "tr\u00e4gt", "in", "ih\u00b7ren", "Han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KON", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun bin ich ohne mich. Doch um zu sein bei ihr,", "tokens": ["Nun", "bin", "ich", "oh\u00b7ne", "mich", ".", "Doch", "um", "zu", "sein", "bei", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$.", "KON", "KOUI", "PTKZU", "VAINF", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da ich doch nicht sein kan, so hab' ich dich bei mir.", "tokens": ["da", "ich", "doch", "nicht", "sein", "kan", ",", "so", "hab'", "ich", "dich", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VAINF", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Itzt nehm' ich dich herf\u00fcr, itzt leg' ich dich denn nieder;", "tokens": ["Itzt", "nehm'", "ich", "dich", "her\u00b7f\u00fcr", ",", "itzt", "leg'", "ich", "dich", "denn", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ich schaue dich bald an, bald nehm' ich dich hinwieder.", "tokens": ["ich", "schau\u00b7e", "dich", "bald", "an", ",", "bald", "nehm'", "ich", "dich", "hin\u00b7wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O w\u00fcrdigs, k\u00f6stlichs Pfand, dem ich bin herzlich hold,", "tokens": ["O", "w\u00fcr\u00b7digs", ",", "k\u00f6st\u00b7lichs", "Pfand", ",", "dem", "ich", "bin", "herz\u00b7lich", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "an dir befind' ich Nichts, das schlechter ist als Gold.", "tokens": ["an", "dir", "be\u00b7find'", "ich", "Nichts", ",", "das", "schlech\u00b7ter", "ist", "als", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}