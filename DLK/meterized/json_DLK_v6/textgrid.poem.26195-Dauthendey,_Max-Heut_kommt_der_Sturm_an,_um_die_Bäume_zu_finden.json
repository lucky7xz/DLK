{"textgrid.poem.26195": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Heut kommt der Sturm an, um die B\u00e4ume zu finden", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut kommt der Sturm an, um die B\u00e4ume zu finden.", "tokens": ["Heut", "kommt", "der", "Sturm", "an", ",", "um", "die", "B\u00e4u\u00b7me", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Seit jeder Baum belaubt und ein gr\u00fcner Turm,", "tokens": ["Seit", "je\u00b7der", "Baum", "be\u00b7laubt", "und", "ein", "gr\u00fc\u00b7ner", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hat der Sturm noch kein Blatt geraubt.", "tokens": ["Hat", "der", "Sturm", "noch", "kein", "Blatt", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber heut lie\u00df er sich nicht mehr binden.", "tokens": ["A\u00b7ber", "heut", "lie\u00df", "er", "sich", "nicht", "mehr", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wirbelnd und sich w\u00e4lzend wie ein Wurm,", "tokens": ["Wir\u00b7belnd", "und", "sich", "w\u00e4l\u00b7zend", "wie", "ein", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PRF", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Siehst du ihn sich auf der Landstra\u00dfe winden.", "tokens": ["Siehst", "du", "ihn", "sich", "auf", "der", "Land\u00b7stra\u00b7\u00dfe", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Er rennt in das Laub und zerrei\u00dft die Rinden", "tokens": ["Er", "rennt", "in", "das", "Laub", "und", "zer\u00b7rei\u00dft", "die", "Rin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und aufrecht jagt er den dem\u00fctigen Staub,", "tokens": ["Und", "auf\u00b7recht", "jagt", "er", "den", "de\u00b7m\u00fc\u00b7ti\u00b7gen", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Und er tobt, als bleibt ihm die Geliebte taub.", "tokens": ["Und", "er", "tobt", ",", "als", "bleibt", "ihm", "die", "Ge\u00b7lieb\u00b7te", "taub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}