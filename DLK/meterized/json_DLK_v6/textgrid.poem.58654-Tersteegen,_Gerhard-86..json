{"textgrid.poem.58654": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Herren priesterlich Geschlecht soll ewig nicht vergehen,", "tokens": ["Des", "Her\u00b7ren", "pries\u00b7ter\u00b7lich", "Ge\u00b7schlecht", "soll", "e\u00b7wig", "nicht", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VMFIN", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Es wird vor seinem Angesicht zu allen Zeiten stehen;", "tokens": ["Es", "wird", "vor", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", "zu", "al\u00b7len", "Zei\u00b7ten", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wie sehr Vernunft auch widerspricht, rei\u00dft der Verfall schon ein,", "tokens": ["Wie", "sehr", "Ver\u00b7nunft", "auch", "wi\u00b7der\u00b7spricht", ",", "rei\u00dft", "der", "Ver\u00b7fall", "schon", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "ADV", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Doch sollen inn're Seelen stets auf Erden \u00fcbrig sein.", "tokens": ["Doch", "sol\u00b7len", "inn'\u00b7re", "See\u00b7len", "stets", "auf", "Er\u00b7den", "\u00fcb\u00b7rig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ADV", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "O auserw\u00e4hlte Seelen, die nichts auf der Welt begehren", "tokens": ["O", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "See\u00b7len", ",", "die", "nichts", "auf", "der", "Welt", "be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Als Leib und Seel' und alles gar dem Herren zu verzehren,", "tokens": ["Als", "Leib", "und", "Seel'", "und", "al\u00b7les", "gar", "dem", "Her\u00b7ren", "zu", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "PIS", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "In Liebe, Lob und Dankbarkeit sie brennen Tag und Nacht,", "tokens": ["In", "Lie\u00b7be", ",", "Lob", "und", "Dank\u00b7bar\u00b7keit", "sie", "bren\u00b7nen", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Ihr Opfer auch f\u00fcr alle wird im Geist vor Gott gebracht!", "tokens": ["Ihr", "Op\u00b7fer", "auch", "f\u00fcr", "al\u00b7le", "wird", "im", "Geist", "vor", "Gott", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PIS", "VAFIN", "APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}