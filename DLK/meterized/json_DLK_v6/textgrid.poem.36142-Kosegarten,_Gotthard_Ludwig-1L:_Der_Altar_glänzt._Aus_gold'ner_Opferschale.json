{"textgrid.poem.36142": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Altar gl\u00e4nzt. Aus gold'ner Opferschale", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Altar gl\u00e4nzt. Aus gold'ner Opferschale", "tokens": ["Der", "Al\u00b7tar", "gl\u00e4nzt", ".", "Aus", "gold'\u00b7ner", "Op\u00b7fer\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gl\u00fcht Blut des Mittlers purpurroth \u2013", "tokens": ["Gl\u00fcht", "Blut", "des", "Mitt\u00b7lers", "pur\u00b7pur\u00b7roth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da tritt die Herrliche einher zum Hochzeitmahle,", "tokens": ["Da", "tritt", "die", "Herr\u00b7li\u00b7che", "ein\u00b7her", "zum", "Hoch\u00b7zeit\u00b7mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Inbrunstschauer wechselnd bleich und roth.", "tokens": ["Von", "In\u00b7brunst\u00b7schau\u00b7er", "wech\u00b7selnd", "bleich", "und", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft in deinem Brautgewande,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "in", "dei\u00b7nem", "Braut\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im heil'gen Schmuck der Mitternacht!", "tokens": ["Im", "heil'\u00b7gen", "Schmuck", "der", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist so sch\u00f6n, du strahlst aus deinem Ernstgewande,", "tokens": ["Du", "bist", "so", "sch\u00f6n", ",", "du", "strahlst", "aus", "dei\u00b7nem", "Ernst\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wie ein Stern aus dunkler Wolkennacht.", "tokens": ["So", "wie", "ein", "Stern", "aus", "dunk\u00b7ler", "Wol\u00b7ken\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was aber bebst du, Heilige, was wanken", "tokens": ["Was", "a\u00b7ber", "bebst", "du", ",", "Hei\u00b7li\u00b7ge", ",", "was", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$,", "ADJA", "$,", "PWS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schritte dir! Du vielgeliebte Braut", "tokens": ["Die", "Schrit\u00b7te", "dir", "!", "Du", "viel\u00b7ge\u00b7lieb\u00b7te", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Herrn \u2013 und meine Braut!! \u2013 O, bei dem Wonngedanken", "tokens": ["Des", "Herrn", "\u2013", "und", "mei\u00b7ne", "Braut", "!!", "\u2013", "O", ",", "bei", "dem", "Wonn\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "PPOSAT", "NN", "$.", "$(", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schl\u00e4gt hoch mein Herz empor und wonnelaut.", "tokens": ["Schl\u00e4gt", "hoch", "mein", "Herz", "em\u00b7por", "und", "won\u00b7ne\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erbebe nicht. Aus gold'ner Opferschale", "tokens": ["Er\u00b7be\u00b7be", "nicht", ".", "Aus", "gold'\u00b7ner", "Op\u00b7fer\u00b7scha\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$.", "APPR", "ADJA", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Winkt dir des Mittlers Bundesblut,", "tokens": ["Winkt", "dir", "des", "Mitt\u00b7lers", "Bun\u00b7des\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und str\u00f6mt herab auf dich vom hohen Todespfahle,", "tokens": ["Und", "str\u00f6mt", "her\u00b7ab", "auf", "dich", "vom", "ho\u00b7hen", "To\u00b7des\u00b7pfah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vers\u00f6hnung und unsterblich's Guts.", "tokens": ["Ver\u00b7s\u00f6h\u00b7nung", "und", "un\u00b7sterb\u00b7lich's", "Guts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Erbebe nicht. Tritt hin zum Stuhl der Gnaden,", "tokens": ["Er\u00b7be\u00b7be", "nicht", ".", "Tritt", "hin", "zum", "Stuhl", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fei'r' den neuen Seelenbund.", "tokens": ["Und", "fei'\u00b7r'", "den", "neu\u00b7en", "See\u00b7len\u00b7bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dein Engel fei'rt ihn auch, und macht den Myriaden", "tokens": ["Dein", "En\u00b7gel", "fei'rt", "ihn", "auch", ",", "und", "macht", "den", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Am Thron die neue Schwester kund. \u2013 \u2013", "tokens": ["Am", "Thron", "die", "neu\u00b7e", "Schwes\u00b7ter", "kund", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Altar gl\u00e4nzt. Des Engels Strahlenschwinge", "tokens": ["Der", "Al\u00b7tar", "gl\u00e4nzt", ".", "Des", "En\u00b7gels", "Strah\u00b7len\u00b7schwin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umrauscht den Altar um und um.", "tokens": ["Um\u00b7rauscht", "den", "Al\u00b7tar", "um", "und", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "KON", "KOUI", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm gl\u00fcht vom Prophezei'n sein Antlitz. Hohe Dinge", "tokens": ["Ihm", "gl\u00fcht", "vom", "Pro\u00b7phe\u00b7zei'n", "sein", "Ant\u00b7litz", ".", "Ho\u00b7he", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verk\u00fcnd't er dir und Evangelium.", "tokens": ["Ver\u00b7k\u00fcnd't", "er", "dir", "und", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Von Gottes Stuhl, von Christi Todesh\u00f6he,", "tokens": ["Von", "Got\u00b7tes", "Stuhl", ",", "von", "Chris\u00b7ti", "To\u00b7des\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von des Geist Krystallstrom, Fried' und Heil", "tokens": ["Und", "von", "des", "Geist", "Krys\u00b7tall\u00b7strom", ",", "Fried'", "und", "Heil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Herab auf dich! Getilgt sei S\u00fcnd' und S\u00fcnderwehe,", "tokens": ["Her\u00b7ab", "auf", "dich", "!", "Ge\u00b7tilgt", "sei", "S\u00fcnd'", "und", "S\u00fcn\u00b7der\u00b7we\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$.", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Gottes Gnade sei dein Theil!", "tokens": ["Und", "Got\u00b7tes", "Gna\u00b7de", "sei", "dein", "Theil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Heil dir! du bist dem Herrn und Sions Lamme", "tokens": ["Heil", "dir", "!", "du", "bist", "dem", "Herrn", "und", "Si\u00b7ons", "Lam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "KON", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Blut und Thr\u00e4nen angetraut.", "tokens": ["Mit", "Blut", "und", "Thr\u00e4\u00b7nen", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Beten stieg hinauf vor Gott wie Weihrauch-Flamme,", "tokens": ["Dein", "Be\u00b7ten", "stieg", "hin\u00b7auf", "vor", "Gott", "wie", "Weih\u00b7rauch\u00b7Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Christus gr\u00fc\u00dfete dich, Braut!!", "tokens": ["Und", "Chris\u00b7tus", "gr\u00fc\u00b7\u00dfe\u00b7te", "dich", ",", "Braut", "!!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So halt ihm denn, was du ihm heut' geschworen,", "tokens": ["So", "halt", "ihm", "denn", ",", "was", "du", "ihm", "heut'", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und freue dich der Seligkeit", "tokens": ["Und", "freu\u00b7e", "dich", "der", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Arm des J\u00fcnglinges, den du dir auserkoren,", "tokens": ["Im", "Arm", "des", "J\u00fcng\u00b7lin\u00b7ges", ",", "den", "du", "dir", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVIZU", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und den ich, dich zu lieben, eingeweiht.", "tokens": ["Und", "den", "ich", ",", "dich", "zu", "lie\u00b7ben", ",", "ein\u00b7ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Mit ihm geh, Hand mit Hand, und Geist mit Geist, verbunden,", "tokens": ["Mit", "ihm", "geh", ",", "Hand", "mit", "Hand", ",", "und", "Geist", "mit", "Geist", ",", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Leben durch. Und k\u00e4mpft und glaubt!", "tokens": ["Dein", "Le\u00b7ben", "durch", ".", "Und", "k\u00e4mpft", "und", "glaubt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Eden ist euch schon ein frischer Kranz gewunden,", "tokens": ["In", "E\u00b7den", "ist", "euch", "schon", "ein", "fri\u00b7scher", "Kranz", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn euch den irdischen der Todesengel raubt!", "tokens": ["Wenn", "euch", "den", "ir\u00b7di\u00b7schen", "der", "To\u00b7de\u00b7sen\u00b7gel", "raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}