{"textgrid.poem.43728": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mond", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond zieht durch die Wolken,", "tokens": ["Der", "Mond", "zieht", "durch", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er kommt so hell heran.", "tokens": ["Er", "kommt", "so", "hell", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Kinder, eilt ins Freie!", "tokens": ["Ihr", "Kin\u00b7der", ",", "eilt", "ins", "Frei\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O seht den Mond euch an!", "tokens": ["O", "seht", "den", "Mond", "euch", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da streckt das kleinste Kn\u00e4bchen", "tokens": ["Da", "streckt", "das", "kleins\u00b7te", "Kn\u00e4b\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Arm' hinaus gar weit,", "tokens": ["Die", "Arm'", "hin\u00b7aus", "gar", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Mond, den Mond will's haben,", "tokens": ["Den", "Mond", ",", "den", "Mond", "will's", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach ihm es weint und schreit.", "tokens": ["Nach", "ihm", "es", "weint", "und", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich kann ihn dir nicht geben,", "tokens": ["Ich", "kann", "ihn", "dir", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch wenn du gr\u00f6\u00dfer bist,", "tokens": ["Auch", "wenn", "du", "gr\u00f6\u00b7\u00dfer", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann ich kein Gl\u00fcck dir geben,", "tokens": ["Kann", "ich", "kein", "Gl\u00fcck", "dir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nicht auf Erden ist. \u2013", "tokens": ["Das", "nicht", "auf", "Er\u00b7den", "ist", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denk' bei dem goldnen Monde,", "tokens": ["Denk'", "bei", "dem", "gold\u00b7nen", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hoch am Himmel schwebt,", "tokens": ["Der", "hoch", "am", "Him\u00b7mel", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Niemand hier auf Erden", "tokens": ["Da\u00df", "Nie\u00b7mand", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unm\u00f6gliches erstrebt.", "tokens": ["Un\u00b7m\u00f6g\u00b7li\u00b7ches", "er\u00b7strebt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}