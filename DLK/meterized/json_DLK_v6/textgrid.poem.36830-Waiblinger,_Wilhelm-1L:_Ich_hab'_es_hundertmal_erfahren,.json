{"textgrid.poem.36830": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab' es hundertmal erfahren,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' es hundertmal erfahren,", "tokens": ["Ich", "hab'", "es", "hun\u00b7dert\u00b7mal", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mir die reinsten Herzensfreuden", "tokens": ["Da\u00df", "mir", "die", "reins\u00b7ten", "Her\u00b7zens\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein blut'ger Quell von Schmerzen waren.", "tokens": ["Ein", "blut'\u00b7ger", "Quell", "von", "Schmer\u00b7zen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit Herz und Leib, mit Geist und Sinnen,", "tokens": ["Mit", "Herz", "und", "Leib", ",", "mit", "Geist", "und", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Sch\u00f6nheit und Genu\u00df versuchte", "tokens": ["Als", "Sch\u00f6n\u00b7heit", "und", "Ge\u00b7nu\u00df", "ver\u00b7such\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Blick mir Liebe zu umspinnen,", "tokens": ["Den", "Blick", "mir", "Lie\u00b7be", "zu", "um\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als h\u00f6chste Kraft und Gluth im Leben,", "tokens": ["Als", "h\u00f6chs\u00b7te", "Kraft", "und", "Gluth", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Drang nach That und Ruhm und Ehre", "tokens": ["Als", "Drang", "nach", "That", "und", "Ruhm", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freundschaft meinen Muth zu heben.", "tokens": ["Die", "Freund\u00b7schaft", "mei\u00b7nen", "Muth", "zu", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich schlang mit gl\u00fchendem Vertrauen", "tokens": ["Ich", "schlang", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Arm um manchen sch\u00f6nen Nacken,", "tokens": ["Den", "Arm", "um", "man\u00b7chen", "sch\u00f6\u00b7nen", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sah manches Aug' in Thr\u00e4nen thauen.", "tokens": ["Sah", "man\u00b7ches", "Aug'", "in", "Thr\u00e4\u00b7nen", "thau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit mir zu streben und zu handeln", "tokens": ["Mit", "mir", "zu", "stre\u00b7ben", "und", "zu", "han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schwur manches Heldenherz, und manches", "tokens": ["Schwur", "man\u00b7ches", "Hel\u00b7den\u00b7herz", ",", "und", "man\u00b7ches"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den rauhen Pfad des Ruhms zu wandeln.", "tokens": ["Den", "rau\u00b7hen", "Pfad", "des", "Ruhms", "zu", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch weil ich hier auf unsrer Erden", "tokens": ["Doch", "weil", "ich", "hier", "auf", "uns\u00b7rer", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Heil'ger bin und kein Apostel", "tokens": ["Kein", "Heil'\u00b7ger", "bin", "und", "kein", "A\u00b7pos\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und erst im Himmel m\u00f6cht' es werden,", "tokens": ["Und", "erst", "im", "Him\u00b7mel", "m\u00f6cht'", "es", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So war es leicht mich zu beth\u00f6ren,", "tokens": ["So", "war", "es", "leicht", "mich", "zu", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn aus dem Kelch, den sie mir reichten,", "tokens": ["Denn", "aus", "dem", "Kelch", ",", "den", "sie", "mir", "reich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Konnt' ich den Satan nicht beschw\u00f6ren.", "tokens": ["Konnt'", "ich", "den", "Sa\u00b7tan", "nicht", "be\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So schl\u00fcrft' ich denn, ein trunk'ner Zecher,", "tokens": ["So", "schl\u00fcrft'", "ich", "denn", ",", "ein", "trunk'\u00b7ner", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Freund und M\u00e4dchen s\u00fc\u00df umlispelt,", "tokens": ["Von", "Freund", "und", "M\u00e4d\u00b7chen", "s\u00fc\u00df", "um\u00b7lis\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der H\u00f6lle Gift aus vollem Becher.", "tokens": ["Der", "H\u00f6l\u00b7le", "Gift", "aus", "vol\u00b7lem", "Be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drum mu\u00df ich jetzt alleine bleiben,", "tokens": ["Drum", "mu\u00df", "ich", "jetzt", "al\u00b7lei\u00b7ne", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne Freund, und ohne Liebchen", "tokens": ["Und", "oh\u00b7ne", "Freund", ",", "und", "oh\u00b7ne", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im \u00f6den Strom des Lebens treiben.", "tokens": ["Im", "\u00f6\u00b7den", "Strom", "des", "Le\u00b7bens", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und siehst du einst noch halb erschlossen", "tokens": ["Und", "siehst", "du", "einst", "noch", "halb", "er\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus gift'gem Boden manches Veilchen", "tokens": ["Aus", "gift'\u00b7gem", "Bo\u00b7den", "man\u00b7ches", "Veil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In meines Lorbeers Schatten sprossen,", "tokens": ["In", "mei\u00b7nes", "Lor\u00b7beers", "Schat\u00b7ten", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So sei dir eben nicht verhehlet,", "tokens": ["So", "sei", "dir", "e\u00b7ben", "nicht", "ver\u00b7heh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df jenen Blumen ihre Seele,", "tokens": ["Da\u00df", "je\u00b7nen", "Blu\u00b7men", "ih\u00b7re", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der sch\u00f6ne Duft, der Glaube fehlet.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Duft", ",", "der", "Glau\u00b7be", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}