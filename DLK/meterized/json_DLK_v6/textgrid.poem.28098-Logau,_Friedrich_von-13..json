{"textgrid.poem.28098": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der, den das theure Blut de\u00df Lammes hat besprenget,", "tokens": ["Der", ",", "den", "das", "theu\u00b7re", "Blut", "de\u00df", "Lam\u00b7mes", "hat", "be\u00b7spren\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird von den W\u00f6lffen zwar ge\u00e4ngstet und bedr\u00e4nget;", "tokens": ["Wird", "von", "den", "W\u00f6lf\u00b7fen", "zwar", "ge\u00b7\u00e4ngs\u00b7tet", "und", "be\u00b7dr\u00e4n\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch herrscht er mit dem Lamm in immer s\u00fcssen Freuden", "tokens": ["Doch", "herrscht", "er", "mit", "dem", "Lamm", "in", "im\u00b7mer", "s\u00fcs\u00b7sen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schauet seine W\u00f6lff in ewig-heissem Leiden.", "tokens": ["Und", "schau\u00b7et", "sei\u00b7ne", "W\u00f6lff", "in", "e\u00b7wig\u00b7heis\u00b7sem", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}