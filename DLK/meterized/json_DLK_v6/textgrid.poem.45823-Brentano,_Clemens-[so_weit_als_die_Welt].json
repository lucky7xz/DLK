{"textgrid.poem.45823": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[so weit als die Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So weit als die Welt,", "tokens": ["So", "weit", "als", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So m\u00e4chtig der Sinn,", "tokens": ["So", "m\u00e4ch\u00b7tig", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So viel Fremde er umfangen h\u00e4lt,", "tokens": ["So", "viel", "Frem\u00b7de", "er", "um\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "So viel Heimat ist ihm Gewinn.", "tokens": ["So", "viel", "Hei\u00b7mat", "ist", "ihm", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}