{"dta.poem.1931": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Gutt sprost von gutter Art: der Hengst weist sein Gest\u00fctte/", "tokens": ["Gutt", "sprost", "von", "gut\u00b7ter", "Art", ":", "der", "Hengst", "weist", "sein", "Ge\u00b7st\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein k\u00fchner Adler heckt die Taube sonder Galle/", "tokens": ["Kein", "k\u00fch\u00b7ner", "Ad\u00b7ler", "heckt", "die", "Tau\u00b7be", "son\u00b7der", "Gal\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch Unterweisung weckt den Zunder im Gebl\u00fctte/", "tokens": ["Doch", "Un\u00b7ter\u00b7wei\u00b7sung", "weckt", "den", "Zun\u00b7der", "im", "Ge\u00b7bl\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wehrt/ da\u00df der rohe Geist nicht aus dem Mittel falle;", "tokens": ["Wehrt", "/", "da\u00df", "der", "ro\u00b7he", "Geist", "nicht", "aus", "dem", "Mit\u00b7tel", "fal\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schl\u00e4gt aber Bo\u00dfheit bey/ sinckst du in Laster ein/", "tokens": ["Schl\u00e4gt", "a\u00b7ber", "Bo\u00df\u00b7heit", "bey", "/", "sinckst", "du", "in", "Las\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "$(", "VVFIN", "PPER", "APPR", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mehret sich dein Schimpff durch Wohlgebohren-seyn.", "tokens": ["So", "meh\u00b7ret", "sich", "dein", "Schimpff", "durch", "Wohl\u00b7ge\u00b7boh\u00b7ren\u00b7seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}