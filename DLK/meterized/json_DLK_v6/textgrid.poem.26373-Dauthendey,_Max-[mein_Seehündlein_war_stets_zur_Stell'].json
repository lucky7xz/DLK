{"textgrid.poem.26373": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[mein Seeh\u00fcndlein war stets zur Stell']", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Seeh\u00fcndlein war stets zur Stell',", "tokens": ["Mein", "See\u00b7h\u00fcnd\u00b7lein", "war", "stets", "zur", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und freudig gl\u00e4nzte ihm das Fell,", "tokens": ["Und", "freu\u00b7dig", "gl\u00e4nz\u00b7te", "ihm", "das", "Fell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schwamm mir lustig nebenher,", "tokens": ["Es", "schwamm", "mir", "lus\u00b7tig", "ne\u00b7ben\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn es ganz mein Scho\u00dfhund w\u00e4r'.", "tokens": ["Als", "wenn", "es", "ganz", "mein", "Scho\u00df\u00b7hund", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Man rudert so am hellen Tag,", "tokens": ["Man", "ru\u00b7dert", "so", "am", "hel\u00b7len", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis man am Abend nicht mehr mag.", "tokens": ["Bis", "man", "am", "A\u00b7bend", "nicht", "mehr", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PIS", "APPRART", "NN", "PTKNEG", "ADV", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Da tat es einen Leuchtturm geben", "tokens": ["Da", "tat", "es", "ei\u00b7nen", "Leucht\u00b7turm", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bei dem Turm ein Witwenleben.", "tokens": ["Und", "bei", "dem", "Turm", "ein", "Wit\u00b7wen\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie f\u00fchrte ein beschaulich Sein", "tokens": ["Sie", "f\u00fchr\u00b7te", "ein", "be\u00b7schau\u00b7lich", "Sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus einem kleinen Inselstein.", "tokens": ["Aus", "ei\u00b7nem", "klei\u00b7nen", "In\u00b7sel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Haus, ein Turm, ein Baum, ein Grab,", "tokens": ["Ein", "Haus", ",", "ein", "Turm", ",", "ein", "Baum", ",", "ein", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war der Witwe ganze Hab.", "tokens": ["Das", "war", "der", "Wit\u00b7we", "gan\u00b7ze", "Hab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Witwe sie war Menschenkenner:", "tokens": ["Die", "Wit\u00b7we", "sie", "war", "Men\u00b7schen\u00b7ken\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Grabe lagen ihr vier M\u00e4nner.", "tokens": ["Im", "Gra\u00b7be", "la\u00b7gen", "ihr", "vier", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich hielt sie erst f\u00fcr einen Mann,", "tokens": ["Ich", "hielt", "sie", "erst", "f\u00fcr", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Dame hatte Hosen an,", "tokens": ["Die", "Da\u00b7me", "hat\u00b7te", "Ho\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Teerhosen, und S\u00fcdwester auf;", "tokens": ["Teer\u00b7ho\u00b7sen", ",", "und", "S\u00fcd\u00b7wes\u00b7ter", "auf", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Zog mich am Seil zum Fels hinauf.", "tokens": ["Zog", "mich", "am", "Seil", "zum", "Fels", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dort oben staunte ich noch mehr:", "tokens": ["Dort", "o\u00b7ben", "staun\u00b7te", "ich", "noch", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Blumen rannten rot umher.", "tokens": ["Viel", "Blu\u00b7men", "rann\u00b7ten", "rot", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Pl\u00f6tzlich blieb eine Blume stehn;", "tokens": ["Pl\u00f6tz\u00b7lich", "blieb", "ei\u00b7ne", "Blu\u00b7me", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Blume, sie begann zu kr\u00e4hn.", "tokens": ["Die", "Blu\u00b7me", ",", "sie", "be\u00b7gann", "zu", "kr\u00e4hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sofort sah ich den Zauber ein:", "tokens": ["So\u00b7fort", "sah", "ich", "den", "Zau\u00b7ber", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es waren lauter H\u00fchnerlein,", "tokens": ["Es", "wa\u00b7ren", "lau\u00b7ter", "H\u00fch\u00b7ner\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "H\u00fchner wie Steine gelb und grau,", "tokens": ["H\u00fch\u00b7ner", "wie", "Stei\u00b7ne", "gelb", "und", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und sie geh\u00f6rten jener Frau.", "tokens": ["Und", "sie", "ge\u00b7h\u00f6r\u00b7ten", "je\u00b7ner", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Vor Steinen konnt' man nichts erkennen", "tokens": ["Vor", "Stei\u00b7nen", "konnt'", "man", "nichts", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PIS", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah nur rote K\u00e4mme rennen.", "tokens": ["Und", "sah", "nur", "ro\u00b7te", "K\u00e4m\u00b7me", "ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Die Witwe rief die Magd, den W\u00e4chter,", "tokens": ["Die", "Wit\u00b7we", "rief", "die", "Magd", ",", "den", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und man empfing mich mit Gel\u00e4chter,", "tokens": ["Und", "man", "emp\u00b7fing", "mich", "mit", "Ge\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Denn ganz verdummt waren die Drei", "tokens": ["Denn", "ganz", "ver\u00b7dummt", "wa\u00b7ren", "die", "Drei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ART", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom steinernen Meereinerlei.", "tokens": ["Vom", "stei\u00b7ner\u00b7nen", "Meer\u00b7ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.17": {"line.1": {"text": "Sie sa\u00dfen auf dem Felsenriff,", "tokens": ["Sie", "sa\u00b7\u00dfen", "auf", "dem", "Fel\u00b7sen\u00b7riff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einmal kam j\u00e4hrlich nur ein Schiff", "tokens": ["Ein\u00b7mal", "kam", "j\u00e4hr\u00b7lich", "nur", "ein", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Mit Proviant f\u00fcrs ganze Jahr,", "tokens": ["Mit", "Pro\u00b7vi\u00b7ant", "f\u00fcrs", "gan\u00b7ze", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So da\u00df ich ein Meerwunder war.", "tokens": ["So", "da\u00df", "ich", "ein", "Meer\u00b7wun\u00b7der", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Gew\u00f6hnlich fischten sie nur Leichen,", "tokens": ["Ge\u00b7w\u00f6hn\u00b7lich", "fischten", "sie", "nur", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich hatte alle Lebenszeichen.", "tokens": ["Ich", "hat\u00b7te", "al\u00b7le", "Le\u00b7bens\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Deshalb sie wie die Wilden lachten", "tokens": ["Des\u00b7halb", "sie", "wie", "die", "Wil\u00b7den", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und tausende Grimassen machten.", "tokens": ["Und", "tau\u00b7sen\u00b7de", "Gri\u00b7mas\u00b7sen", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.21": {"line.1": {"text": "Zw\u00f6lf Fische brachte man zum Essen,", "tokens": ["Zw\u00f6lf", "Fi\u00b7sche", "brach\u00b7te", "man", "zum", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir schien, als sollt' ich das Meer auffressen,", "tokens": ["Mir", "schien", ",", "als", "sollt'", "ich", "das", "Meer", "auf\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "Fische in allen Lebenslagen,", "tokens": ["Fi\u00b7sche", "in", "al\u00b7len", "Le\u00b7bens\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein Fischbassin war bald mein Magen,", "tokens": ["Ein", "Fischbas\u00b7sin", "war", "bald", "mein", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.23": {"line.1": {"text": "Zum Morgen-, Mittag-, Abendtisch,", "tokens": ["Zum", "Mor\u00b7gen", ",", "Mit\u00b7tag", ",", "A\u00b7bend\u00b7tisch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "TRUNC", "$,", "TRUNC", "$,", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Immer und nachts im Traum noch Fisch.", "tokens": ["Im\u00b7mer", "und", "nachts", "im", "Traum", "noch", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "APPRART", "NN", "ADV", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.24": {"line.1": {"text": "Die Dreie gingen um mich her,", "tokens": ["Die", "Drei\u00b7e", "gin\u00b7gen", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn ich ganz ihr S\u00e4ugling w\u00e4r'.", "tokens": ["Als", "wenn", "ich", "ganz", "ihr", "S\u00e4ug\u00b7ling", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Erfurchtsvoll sa\u00df niemand bei Tisch,", "tokens": ["Er\u00b7furchts\u00b7voll", "sa\u00df", "nie\u00b7mand", "bei", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Nur ich allein sa\u00df und a\u00df Fisch.", "tokens": ["Nur", "ich", "al\u00b7lein", "sa\u00df", "und", "a\u00df", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.26": {"line.1": {"text": "Ich war f\u00fcr sie nicht nur ein Mann,", "tokens": ["Ich", "war", "f\u00fcr", "sie", "nicht", "nur", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich war gleich eine Karawan',", "tokens": ["Ich", "war", "gleich", "ei\u00b7ne", "Ka\u00b7ra\u00b7wan'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Mein Schuh, mein Hemd, mein Hut, mein Kragen", "tokens": ["Mein", "Schuh", ",", "mein", "Hemd", ",", "mein", "Hut", ",", "mein", "Kra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wurden Personen sozusagen,", "tokens": ["Wur