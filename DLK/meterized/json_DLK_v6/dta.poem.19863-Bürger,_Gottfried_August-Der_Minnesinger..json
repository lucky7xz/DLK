{"dta.poem.19863": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  Minnesinger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wil das Herz mein Leben lang,               ", "tokens": ["Ich", "wil", "das", "Herz", "mein", "Le\u00b7ben", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der holden Minne weihen,", "tokens": ["Der", "hol\u00b7den", "Min\u00b7ne", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meinen leichten Volksgesang", "tokens": ["Und", "mei\u00b7nen", "leich\u00b7ten", "Volks\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdienten Schmeicheleien.", "tokens": ["Ver\u00b7dien\u00b7ten", "Schmei\u00b7che\u00b7lei\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn warlich keines Lobes Ton,", "tokens": ["Denn", "war\u00b7lich", "kei\u00b7nes", "Lo\u00b7bes", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf keiner Flur, gew\u00e4ret", "tokens": ["Auf", "kei\u00b7ner", "Flur", ",", "ge\u00b7w\u00e4\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem S\u00e4nger einen f\u00fcssern Lohn,", "tokens": ["Dem", "S\u00e4n\u00b7ger", "ei\u00b7nen", "f\u00fcs\u00b7sern", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn er Sch\u00f6nheit ehret.", "tokens": ["Als", "wenn", "er", "Sch\u00f6n\u00b7heit", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wolan, o Laute, werde dann", "tokens": ["Wo\u00b7lan", ",", "o", "Lau\u00b7te", ",", "wer\u00b7de", "dann"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6nen, die gesellig", "tokens": ["Der", "Sch\u00f6\u00b7nen", ",", "die", "ge\u00b7sel\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und freundlich ist, und minnen kan,", "tokens": ["Und", "freund\u00b7lich", "ist", ",", "und", "min\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Lied und Lob gef\u00e4llig!", "tokens": ["Durch", "Lied", "und", "Lob", "ge\u00b7f\u00e4l\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein Schmeicheln mildert die Natur.", "tokens": ["Dein", "Schmei\u00b7cheln", "mil\u00b7dert", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon lassen Sch\u00e4ferinnen", "tokens": ["Schon", "las\u00b7sen", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVINF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich hie und da, auf teutscher Flur,", "tokens": ["Sich", "hie", "und", "da", ",", "auf", "teut\u00b7scher", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Lied und Lob gewinnen.", "tokens": ["Durch", "Lied", "und", "Lob", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du solst noch manche Sommernacht,", "tokens": ["Du", "solst", "noch", "man\u00b7che", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um stille Sch\u00e4ferh\u00fctten,", "tokens": ["Um", "stil\u00b7le", "Sch\u00e4\u00b7fer\u00b7h\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das M\u00e4dchen, das im Bette wacht,", "tokens": ["Das", "M\u00e4d\u00b7chen", ",", "das", "im", "Bet\u00b7te", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von mir zu tr\u00e4umen bitten.", "tokens": ["Von", "mir", "zu", "tr\u00e4u\u00b7men", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mir danket dann ihr Morgengrus,", "tokens": ["Mir", "dan\u00b7ket", "dann", "ihr", "Mor\u00b7gen\u00b7grus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr liebevolles Nicken,", "tokens": ["Ihr", "lie\u00b7be\u00b7vol\u00b7les", "Ni\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wonniglicher, warmer Kus,", "tokens": ["Ihr", "won\u00b7nig\u00b7li\u00b7cher", ",", "war\u00b7mer", "Kus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sanftes H\u00e4ndedr\u00fccken.", "tokens": ["Ihr", "sanf\u00b7tes", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Erwerben werd\u2019 ich reiches Gut", "tokens": ["Er\u00b7wer\u00b7ben", "werd'", "ich", "rei\u00b7ches", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An kleinen Minnepf\u00e4ndern;", "tokens": ["An", "klei\u00b7nen", "Min\u00b7ne\u00b7pf\u00e4n\u00b7dern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und prangen wird mein Stab und Hut", "tokens": ["Und", "pran\u00b7gen", "wird", "mein", "Stab", "und", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Rosen und mit B\u00e4ndern.", "tokens": ["Mit", "Ro\u00b7sen", "und", "mit", "B\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Beim Spiel und Tanze werden mir", "tokens": ["Beim", "Spiel", "und", "Tan\u00b7ze", "wer\u00b7den", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00f6nsten immer winken;", "tokens": ["Die", "Sch\u00f6ns\u00b7ten", "im\u00b7mer", "win\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, die ich fodre, werden schier", "tokens": ["Und", ",", "die", "ich", "fod\u00b7re", ",", "wer\u00b7den", "schier"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mehr als Andre d\u00fcnken.", "tokens": ["Sich", "mehr", "als", "And\u00b7re", "d\u00fcn\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KOUS", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Geliebt, geehrt, bis an mein Ziel,", "tokens": ["Ge\u00b7liebt", ",", "ge\u00b7ehrt", ",", "bis", "an", "mein", "Ziel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einer Flur zur andern,", "tokens": ["Von", "ei\u00b7ner", "Flur", "zur", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Werd\u2019 ich mit meinem Minnespiel,", "tokens": ["Werd'", "ich", "mit", "mei\u00b7nem", "Min\u00b7ne\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herbeigerufen, wandern.", "tokens": ["Her\u00b7bei\u00b7ge\u00b7ru\u00b7fen", ",", "wan\u00b7dern", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und, wenn ich l\u00e4ngst gestorben bin,", "tokens": ["Und", ",", "wenn", "ich", "l\u00e4ngst", "ge\u00b7stor\u00b7ben", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unter Ulmen schlafe,", "tokens": ["Und", "un\u00b7ter", "Ul\u00b7men", "schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So weidet noch die Sch\u00e4ferin", "tokens": ["So", "wei\u00b7det", "noch", "die", "Sch\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um meine Gruft die Schafe;", "tokens": ["Um", "mei\u00b7ne", "Gruft", "die", "Scha\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und lehnet sich auf ihren Stab,", "tokens": ["Und", "leh\u00b7net", "sich", "auf", "ih\u00b7ren", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und senkt vol heller Thr\u00e4nen,", "tokens": ["Und", "senkt", "vol", "hel\u00b7ler", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den sanften Blik zu mir herab,", "tokens": ["Den", "sanf\u00b7ten", "Blik", "zu", "mir", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klagt in weichen T\u00f6nen:", "tokens": ["Und", "klagt", "in", "wei\u00b7chen", "T\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u201edu, der so f\u00fcsse Lieder schuf,", "tokens": ["\u201e", "du", ",", "der", "so", "f\u00fcs\u00b7se", "Lie\u00b7der", "schuf", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So minnigliche Lieder!", "tokens": ["So", "min\u00b7nig\u00b7li\u00b7che", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wekte dich mein lauter Ruf", "tokens": ["O", "wek\u00b7te", "dich", "mein", "lau\u00b7ter", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus deinem Grabe wieder!", "tokens": ["Aus", "dei\u00b7nem", "Gra\u00b7be", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Du w\u00fcrdest mich, nach deinem Brauch,", "tokens": ["Du", "w\u00fcr\u00b7dest", "mich", ",", "nach", "dei\u00b7nem", "Brauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewis ein wenig preisen.", "tokens": ["Ge\u00b7wis", "ein", "we\u00b7nig", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann h\u00e4tt\u2019 ich doch bei Schwestern auch", "tokens": ["Dann", "h\u00e4tt'", "ich", "doch", "bei", "Schwes\u00b7tern", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Liedchen aufzuweisen.", "tokens": ["Ein", "Lied\u00b7chen", "auf\u00b7zu\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dein Minneliedchen s\u00e4ng\u2019 ich dann,", "tokens": ["Dein", "Min\u00b7ne\u00b7lied\u00b7chen", "s\u00e4ng'", "ich", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solt\u2019 auch die Mutter schelten.", "tokens": ["Solt'", "auch", "die", "Mut\u00b7ter", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O lieber, lieber Leierman,", "tokens": ["O", "lie\u00b7ber", ",", "lie\u00b7ber", "Lei\u00b7er\u00b7man", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wolt\u2019 ich\u2019s dir vergelten!\u201e", "tokens": ["Wie", "wolt'", "ich's", "dir", "ver\u00b7gel\u00b7ten", "!", "\u201e"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Dann wird mein Geist, wie Sommerluft,", "tokens": ["Dann", "wird", "mein", "Geist", ",", "wie", "Som\u00b7mer\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus seiner Ulme Zweigen,", "tokens": ["Aus", "sei\u00b7ner", "Ul\u00b7me", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu ihr herunter auf die Gruft,", "tokens": ["Zu", "ihr", "her\u00b7un\u00b7ter", "auf", "die", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie anzuwehen, steigen;", "tokens": ["Sie", "an\u00b7zu\u00b7we\u00b7hen", ",", "stei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVIZU", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Wird durch des Wiesenbaches Rohr,", "tokens": ["Wird", "durch", "des", "Wie\u00b7sen\u00b7ba\u00b7ches", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Bl\u00e4tter, die sich kr\u00e4useln,", "tokens": ["Und", "Bl\u00e4t\u00b7ter", ",", "die", "sich", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Liedchen in ihr horchend Ohr,", "tokens": ["Ein", "Lied\u00b7chen", "in", "ihr", "hor\u00b7chend", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihrem Lobe, s\u00e4useln.", "tokens": ["Zu", "ih\u00b7rem", "Lo\u00b7be", ",", "s\u00e4u\u00b7seln", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}