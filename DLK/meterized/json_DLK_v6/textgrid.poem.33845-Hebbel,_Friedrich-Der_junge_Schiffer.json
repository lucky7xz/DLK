{"textgrid.poem.33845": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der junge Schiffer", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort bl\u00e4ht ein Schiff die Segel,", "tokens": ["Dort", "bl\u00e4ht", "ein", "Schiff", "die", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frisch saus't hinein der Wind;", "tokens": ["Frisch", "saus't", "hin\u00b7ein", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Anker wird gelichtet,", "tokens": ["Der", "An\u00b7ker", "wird", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Steuer flugs gerichtet,", "tokens": ["Das", "Steu\u00b7er", "flugs", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun fliegt's hinaus geschwind.", "tokens": ["Nun", "fliegt's", "hin\u00b7aus", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APZR", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein k\u00fchner Wasservogel", "tokens": ["Ein", "k\u00fch\u00b7ner", "Was\u00b7ser\u00b7vo\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kreis't gr\u00fc\u00dfend um den Mast,", "tokens": ["Kreis't", "gr\u00fc\u00b7\u00dfend", "um", "den", "Mast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne brennt herunter,", "tokens": ["Die", "Son\u00b7ne", "brennt", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch Fischlein, blank und munter,", "tokens": ["Manch", "Fisc\u00b7hlein", ",", "blank", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umgaukelt keck den Gast.", "tokens": ["Um\u00b7gau\u00b7kelt", "keck", "den", "Gast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "W\u00e4r' gern hinein gesprungen,", "tokens": ["W\u00e4r'", "gern", "hin\u00b7ein", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APZR", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da drau\u00dfen ist mein Reich!", "tokens": ["Da", "drau\u00b7\u00dfen", "ist", "mein", "Reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ja jung von Jahren,", "tokens": ["Ich", "bin", "ja", "jung", "von", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ist's mir nur um's Fahren,", "tokens": ["Da", "ist's", "mir", "nur", "um's", "Fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohin? Das gilt mir gleich!", "tokens": ["Wo\u00b7hin", "?", "Das", "gilt", "mir", "gleich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}