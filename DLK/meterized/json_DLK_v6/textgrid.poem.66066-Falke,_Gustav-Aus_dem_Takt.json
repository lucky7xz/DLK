{"textgrid.poem.66066": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Takt", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Weib und all mein holder Kreis,", "tokens": ["Mein", "Weib", "und", "all", "mein", "hol\u00b7der", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kind und all mein lachend Gl\u00fcck.", "tokens": ["Mein", "Kind", "und", "all", "mein", "la\u00b7chend", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PIAT", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich r\u00fchre an die Saite leis,", "tokens": ["Ich", "r\u00fch\u00b7re", "an", "die", "Sai\u00b7te", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hell klingt es zur\u00fcck.", "tokens": ["Wie", "hell", "klingt", "es", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur manchmal, wenn von ferne ich", "tokens": ["Nur", "manch\u00b7mal", ",", "wenn", "von", "fer\u00b7ne", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPR", "ADJA", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gro\u00dfen Str\u00f6me rauschen h\u00f6re,", "tokens": ["Die", "gro\u00b7\u00dfen", "Str\u00f6\u00b7me", "rau\u00b7schen", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sich der vollern Lebensch\u00f6re", "tokens": ["Wenn", "sich", "der", "vol\u00b7lern", "Le\u00b7ben\u00b7sch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ton in meine Stille schlich,", "tokens": ["Ein", "Ton", "in", "mei\u00b7ne", "Stil\u00b7le", "schlich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schrei laut ich auf und hebe Klag:", "tokens": ["Schrei", "laut", "ich", "auf", "und", "he\u00b7be", "Klag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mehr Licht, mehr Licht, nur einen Tag!", "tokens": ["Mehr", "Licht", ",", "mehr", "Licht", ",", "nur", "ei\u00b7nen", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und blutend leg ich, abgewandt,", "tokens": ["Und", "blu\u00b7tend", "leg", "ich", ",", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz in eure Liebeshand,", "tokens": ["Mein", "Herz", "in", "eu\u00b7re", "Lie\u00b7bes\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis es von aller Angst entbunden", "tokens": ["Bis", "es", "von", "al\u00b7ler", "Angst", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder seinen Takt gefunden,", "tokens": ["Und", "wie\u00b7der", "sei\u00b7nen", "Takt", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Gleichtakt zwischen Wunsch und Pflicht.", "tokens": ["Den", "Gleich\u00b7takt", "zwi\u00b7schen", "Wunsch", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herdd\u00e4mmergl\u00fcck, Herdd\u00e4mmerlicht.", "tokens": ["Herd\u00b7d\u00e4m\u00b7mer\u00b7gl\u00fcck", ",", "Herd\u00b7d\u00e4m\u00b7mer\u00b7licht", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}