{"textgrid.poem.47038": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe dir in hei\u00dfer Ernte Tagen", "tokens": ["Ich", "ha\u00b7be", "dir", "in", "hei\u00b7\u00dfer", "Ern\u00b7te", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(sahst du den Schwei\u00df, der deinethalb mich n\u00e4\u00dfte?)", "tokens": ["(", "sahst", "du", "den", "Schwei\u00df", ",", "der", "dei\u00b7net\u00b7halb", "mich", "n\u00e4\u00df\u00b7te", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Frucht gesch\u00fcttelt deiner reichen \u00c4ste,", "tokens": ["Die", "Frucht", "ge\u00b7sch\u00fct\u00b7telt", "dei\u00b7ner", "rei\u00b7chen", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch keine Fr\u00fcchte hat es mir getragen.", "tokens": ["Doch", "kei\u00b7ne", "Fr\u00fcch\u00b7te", "hat", "es", "mir", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich habe dir des Flachses duft'ge Lagen", "tokens": ["Ich", "ha\u00b7be", "dir", "des", "Flach\u00b7ses", "duft'\u00b7ge", "La\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gereicht, als deine Hand sie bosselnd pre\u00dfte,", "tokens": ["Ge\u00b7reicht", ",", "als", "dei\u00b7ne", "Hand", "sie", "bos\u00b7selnd", "pre\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wird kein Weber draus zum Fest der Feste", "tokens": ["Doch", "wird", "kein", "We\u00b7ber", "draus", "zum", "Fest", "der", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Hochzeitkleid f\u00fcr mich zusammenschlagen.", "tokens": ["Das", "Hoch\u00b7zeit\u00b7kleid", "f\u00fcr", "mich", "zu\u00b7sam\u00b7men\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich habe mich gegeben dir zum Knechte,", "tokens": ["Ich", "ha\u00b7be", "mich", "ge\u00b7ge\u00b7ben", "dir", "zum", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin f\u00fcr dich, zum Trotz den Stundenzeigern,", "tokens": ["Ich", "bin", "f\u00fcr", "dich", ",", "zum", "Trotz", "den", "Stun\u00b7den\u00b7zei\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Tages und des Nachts gerannt, gesprungen.", "tokens": ["Des", "Ta\u00b7ges", "und", "des", "Nachts", "ge\u00b7rannt", ",", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADV", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl einen Lohn h\u00e4tt' ich verdient, ich d\u00e4chte,", "tokens": ["Wohl", "ei\u00b7nen", "Lohn", "h\u00e4tt'", "ich", "ver\u00b7dient", ",", "ich", "d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch kannst du freilich mir den Lohn auch weigern,", "tokens": ["Doch", "kannst", "du", "frei\u00b7lich", "mir", "den", "Lohn", "auch", "wei\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn (kannst du fragen): wer hat dich gedungen?", "tokens": ["Denn", "(", "kannst", "du", "fra\u00b7gen", ")", ":", "wer", "hat", "dich", "ge\u00b7dun\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VMFIN", "PPER", "VVINF", "$(", "$.", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}