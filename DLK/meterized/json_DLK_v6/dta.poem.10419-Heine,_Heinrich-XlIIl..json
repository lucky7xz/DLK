{"dta.poem.10419": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlIIl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebchen, wir sa\u00dfen beisammen,", "tokens": ["Mein", "Lieb\u00b7chen", ",", "wir", "sa\u00b7\u00dfen", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Traulich im leichten Kahn.", "tokens": ["Trau\u00b7lich", "im", "leich\u00b7ten", "Kahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Nacht war still, und wir schwammen", "tokens": ["Die", "Nacht", "war", "still", ",", "und", "wir", "schwam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf weiter Wasserbahn.", "tokens": ["Auf", "wei\u00b7ter", "Was\u00b7ser\u00b7bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Geisterinsel, die sch\u00f6ne,", "tokens": ["Die", "Geis\u00b7te\u00b7rin\u00b7sel", ",", "die", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lag d\u00e4mm'rig im Mondenglanz;", "tokens": ["Lag", "d\u00e4m\u00b7m'\u00b7rig", "im", "Mon\u00b7den\u00b7glanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort klangen liebe T\u00f6ne,", "tokens": ["Dort", "klan\u00b7gen", "lie\u00b7be", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wogte der Nebeltanz.", "tokens": ["Und", "wog\u00b7te", "der", "Ne\u00b7bel\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort klang es lieb und lieber,", "tokens": ["Dort", "klang", "es", "lieb", "und", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wogt' es hin und her;", "tokens": ["Und", "wogt'", "es", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir aber schwammen vor\u00fcber,", "tokens": ["Wir", "a\u00b7ber", "schwam\u00b7men", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Trostlos auf weitem Meer.", "tokens": ["Trost\u00b7los", "auf", "wei\u00b7tem", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}