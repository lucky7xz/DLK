{"textgrid.poem.62956": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Derwisch Aladin lag in Buchara krank:", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Derwisch Aladin lag in Buchara krank:", "tokens": ["Der", "Der\u00b7wisch", "A\u00b7la\u00b7din", "lag", "in", "Buc\u00b7ha\u00b7ra", "krank", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NE", "ADJD", "$."], "meter": "--+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sein Fu\u00df ber\u00fchrte schon des Grabes j\u00e4he Stufen.", "tokens": ["Sein", "Fu\u00df", "be\u00b7r\u00fchr\u00b7te", "schon", "des", "Gra\u00b7bes", "j\u00e4\u00b7he", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man lie\u00df den Avicenna rufen.", "tokens": ["Man", "lie\u00df", "den", "A\u00b7vi\u00b7cen\u00b7na", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er kam. Du mu\u00dft in deinem Trank", "tokens": ["Er", "kam", ".", "Du", "mu\u00dft", "in", "dei\u00b7nem", "Trank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von diesem Elixir, sprach er nach reifen Schl\u00fcssen,", "tokens": ["Von", "die\u00b7sem", "E\u00b7li\u00b7xir", ",", "sprach", "er", "nach", "rei\u00b7fen", "Schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Tags drey L\u00f6ffel voll genie\u00dfen;", "tokens": ["Des", "Tags", "drey", "L\u00f6f\u00b7fel", "voll", "ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es st\u00e4rkt das Haupt und heilt die Brust.", "tokens": ["Es", "st\u00e4rkt", "das", "Haupt", "und", "heilt", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Patient nahm es mit Lust", "tokens": ["Der", "Pa\u00b7ti\u00b7ent", "nahm", "es", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Und fieng schon an die Wunderkraft zu sp\u00fcren.", "tokens": ["Und", "fi\u00b7eng", "schon", "an", "die", "Wun\u00b7der\u00b7kraft", "zu", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Gut! denkt er bey sich selbst, nehm ich den Balsam pur", "tokens": ["Gut", "!", "denkt", "er", "bey", "sich", "selbst", ",", "nehm", "ich", "den", "Bal\u00b7sam", "pur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "VVFIN", "PPER", "APPR", "PRF", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und recht nach Appetit, so wird das meine Cur", "tokens": ["Und", "recht", "nach", "Ap\u00b7pe\u00b7tit", ",", "so", "wird", "das", "mei\u00b7ne", "Cur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NE", "$,", "ADV", "VAFIN", "PDS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weit eher noch zum frohen Ziele f\u00fchren.", "tokens": ["Weit", "e\u00b7her", "noch", "zum", "fro\u00b7hen", "Zie\u00b7le", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Gesagt, gethan. Er leeret die Tinctur", "tokens": ["Ge\u00b7sagt", ",", "ge\u00b7than", ".", "Er", "lee\u00b7ret", "die", "Tinc\u00b7tur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mit einem Zug bis auf den letzten Tropfen.", "tokens": ["Mit", "ei\u00b7nem", "Zug", "bis", "auf", "den", "letz\u00b7ten", "Trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Sie f\u00e4hrt ihm wie ein Blitz durch Adern und Gebein:", "tokens": ["Sie", "f\u00e4hrt", "ihm", "wie", "ein", "Blitz", "durch", "A\u00b7dern", "und", "Ge\u00b7bein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Schwindel dreht sein Haupt, das Herz f\u00e4ngt an zu klopfen", "tokens": ["Der", "Schwin\u00b7del", "dreht", "sein", "Haupt", ",", "das", "Herz", "f\u00e4ngt", "an", "zu", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und bald verkalkt es sich zum Kieselstein.", "tokens": ["Und", "bald", "ver\u00b7kalkt", "es", "sich", "zum", "Kie\u00b7sel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Er taumelt durch die Stadt, steigt auf die Minareen,", "tokens": ["Er", "tau\u00b7melt", "durch", "die", "Stadt", ",", "steigt", "auf", "die", "Mi\u00b7na\u00b7reen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ruft alles Volk mit bacchischem Gebr\u00fcll", "tokens": ["Ruft", "al\u00b7les", "Volk", "mit", "bac\u00b7chi\u00b7schem", "Ge\u00b7br\u00fcll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zum Beiramstanz, und wer nicht tanzen will,", "tokens": ["Zum", "Bei\u00b7rams\u00b7tanz", ",", "und", "wer", "nicht", "tan\u00b7zen", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PWS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Den schleppet er in die Moscheen", "tokens": ["Den", "schlep\u00b7pet", "er", "in", "die", "Mo\u00b7scheen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.22": {"text": "Und st\u00f6\u00dft ihm einen Dolch ins Herz.", "tokens": ["Und", "st\u00f6\u00dft", "ihm", "ei\u00b7nen", "Dolch", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Man lief, den Arzt um Rath zu fragen.", "tokens": ["Man", "lief", ",", "den", "Arzt", "um", "Rath", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Er lie\u00df nicht ohne Furcht ihm ein Paar Adern schlagen;", "tokens": ["Er", "lie\u00df", "nicht", "oh\u00b7ne", "Furcht", "ihm", "ein", "Paar", "A\u00b7dern", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch er gestand mit edelm Scherz,", "tokens": ["Doch", "er", "ge\u00b7stand", "mit", "e\u00b7delm", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Er werde schwerlich ganz genesen.", "tokens": ["Er", "wer\u00b7de", "schwer\u00b7lich", "ganz", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Was meinst du, Freund, gleicht die Religion", "tokens": ["Was", "meinst", "du", ",", "Freund", ",", "gleicht", "die", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Nicht diesem Elixir? Braucht sie der Erdensohn", "tokens": ["Nicht", "die\u00b7sem", "E\u00b7li\u00b7xir", "?", "Braucht", "sie", "der", "Er\u00b7den\u00b7sohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PDAT", "NN", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie grobe Kost und als ein fremdes Wesen;", "tokens": ["Wie", "gro\u00b7be", "Kost", "und", "als", "ein", "frem\u00b7des", "We\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "So macht sie krank, erzeuget Schw\u00e4rmerey", "tokens": ["So", "macht", "sie", "krank", ",", "er\u00b7zeu\u00b7get", "Schw\u00e4r\u00b7me\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und Pharis\u00e4erstolz; doch mischt er als Arzney", "tokens": ["Und", "Pha\u00b7ri\u00b7s\u00e4er\u00b7stolz", ";", "doch", "mischt", "er", "als", "Arz\u00b7ney"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "ADV", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Von ihrem Geist, von ihrem Freuden\u00f6le,", "tokens": ["Von", "ih\u00b7rem", "Geist", ",", "von", "ih\u00b7rem", "Freu\u00b7den\u00b7\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "In jede Nahrung seiner Seele;", "tokens": ["In", "je\u00b7de", "Nah\u00b7rung", "sei\u00b7ner", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "So mehrt es ihren Lebenssaft", "tokens": ["So", "mehrt", "es", "ih\u00b7ren", "Le\u00b7bens\u00b7saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und f\u00fcllet sie mit Gotteskraft.", "tokens": ["Und", "f\u00fcl\u00b7let", "sie", "mit", "Got\u00b7tes\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}