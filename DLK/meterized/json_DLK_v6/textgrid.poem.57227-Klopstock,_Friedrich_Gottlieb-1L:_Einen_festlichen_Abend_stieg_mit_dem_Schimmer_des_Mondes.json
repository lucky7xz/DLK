{"textgrid.poem.57227": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einen festlichen Abend stieg mit dem Schimmer des Mondes", "genre": "verse", "period": "N.A.", "pub_year": 1748, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen festlichen Abend stieg mit dem Schimmer des Mondes", "tokens": ["Ei\u00b7nen", "fest\u00b7li\u00b7chen", "A\u00b7bend", "stieg", "mit", "dem", "Schim\u00b7mer", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Salem, der Engel der Lieb' und mein Schutzgeist,", "tokens": ["Sa\u00b7lem", ",", "der", "En\u00b7gel", "der", "Lieb'", "und", "mein", "Schutz\u00b7geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Vom Olympus herab; ich sah den G\u00f6ttlichen wandeln,", "tokens": ["Vom", "O\u00b7lym\u00b7pus", "her\u00b7ab", ";", "ich", "sah", "den", "G\u00f6tt\u00b7li\u00b7chen", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ihn gegen mich l\u00e4chelnd einhergehn.", "tokens": ["Und", "ihn", "ge\u00b7gen", "mich", "l\u00e4\u00b7chelnd", "ein\u00b7her\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ewigbl\u00fchende Rosen umkr\u00e4nzten sein fliessendes Haupthaar,", "tokens": ["E\u00b7wig\u00b7bl\u00fc\u00b7hen\u00b7de", "Ro\u00b7sen", "um\u00b7kr\u00e4nz\u00b7ten", "sein", "flies\u00b7sen\u00b7des", "Haupt\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Himlische Rosen, von Thr\u00e4nen erzogen,", "tokens": ["Him\u00b7li\u00b7sche", "Ro\u00b7sen", ",", "von", "Thr\u00e4\u00b7nen", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Die bey dem Wiedersehn einander Liebende weinten,", "tokens": ["Die", "bey", "dem", "Wie\u00b7der\u00b7sehn", "ein\u00b7an\u00b7der", "Lie\u00b7ben\u00b7de", "wein\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Als sie kein Tod mehr trennt' und kein Schicksal.", "tokens": ["Als", "sie", "kein", "Tod", "mehr", "trennt'", "und", "kein", "Schick\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und ein wolkiger Hauch geathmeter Weihrauchsd\u00fcfte", "tokens": ["Und", "ein", "wol\u00b7ki\u00b7ger", "Hauch", "ge\u00b7ath\u00b7me\u00b7ter", "Weih\u00b7rauchs\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Floss von dem Haupt des Unsterblichen nieder;", "tokens": ["Floss", "von", "dem", "Haupt", "des", "U\u00b7nsterb\u00b7li\u00b7chen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Opferd\u00fcfte, wie Gott sie, bey s\u00fcssen dankenden Liedern,", "tokens": ["Op\u00b7fer\u00b7d\u00fcf\u00b7te", ",", "wie", "Gott", "sie", ",", "bey", "s\u00fcs\u00b7sen", "dan\u00b7ken\u00b7den", "Lie\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "PPER", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Nach dem Tode die Liebenden opfern,", "tokens": ["Nach", "dem", "To\u00b7de", "die", "Lie\u00b7ben\u00b7den", "op\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Dass er sie ewig erschuf, und sie, f\u00fcr einander geschaffen,", "tokens": ["Dass", "er", "sie", "e\u00b7wig", "er\u00b7schuf", ",", "und", "sie", ",", "f\u00fcr", "ein\u00b7an\u00b7der", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "KON", "PPER", "$,", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Auf der Erde sich fanden und liebten,", "tokens": ["Auf", "der", "Er\u00b7de", "sich", "fan\u00b7den", "und", "lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Sie kein Schicksal trennte; dass sie nun ewig sich lieben,", "tokens": ["Sie", "kein", "Schick\u00b7sal", "trenn\u00b7te", ";", "dass", "sie", "nun", "e\u00b7wig", "sich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$.", "KOUS", "PPER", "ADV", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Weil sie auf Erden sich fanden und liebten.", "tokens": ["Weil", "sie", "auf", "Er\u00b7den", "sich", "fan\u00b7den", "und", "lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Also n\u00e4herte Salem sich mir, und tief in mein Herz hin", "tokens": ["Al\u00b7so", "n\u00e4\u00b7her\u00b7te", "Sa\u00b7lem", "sich", "mir", ",", "und", "tief", "in", "mein", "Herz", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "PRF", "PPER", "$,", "KON", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Drang ein Schauer wallender Freuden,", "tokens": ["Drang", "ein", "Schau\u00b7er", "wal\u00b7len\u00b7der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Wie ich mich freue, wenn ich ein Kind der Unschuld erblicke,", "tokens": ["Wie", "ich", "mich", "freu\u00b7e", ",", "wenn", "ich", "ein", "Kind", "der", "Un\u00b7schuld", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Und an Adams Unsterblichkeit denke.", "tokens": ["Und", "an", "A\u00b7dams", "U\u00b7nsterb\u00b7lich\u00b7keit", "den\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Sieh, ein silberner Ton floss von der Lippe des Seraphs,", "tokens": ["Sieh", ",", "ein", "sil\u00b7ber\u00b7ner", "Ton", "floss", "von", "der", "Lip\u00b7pe", "des", "Se\u00b7raphs", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Und er blickte sanfter, und sagte:", "tokens": ["Und", "er", "blick\u00b7te", "sanf\u00b7ter", ",", "und", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "\u00bbich bin Salem, der Liebenden Engel, die edler sich lieben,", "tokens": ["\u00bb", "ich", "bin", "Sa\u00b7lem", ",", "der", "Lie\u00b7ben\u00b7den", "En\u00b7gel", ",", "die", "ed\u00b7ler", "sich", "lie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NE", "$,", "ART", "NN", "NE", "$,", "ART", "ADJA", "PRF", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "G\u00f6ttlicher, als sich Sterbliche lieben.", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", ",", "als", "sich", "Sterb\u00b7li\u00b7che", "lie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PRF", "NN", "VVINF", "$."], "meter": "+-++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Wenn es die ersten Empfindungen schl\u00e4gt, in den stammelnden Jahren,", "tokens": ["Wenn", "es", "die", "ers\u00b7ten", "Emp\u00b7fin\u00b7dun\u00b7gen", "schl\u00e4gt", ",", "in", "den", "stam\u00b7meln\u00b7den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Bild' ich das Herz der jungen Geliebten.", "tokens": ["Bild'", "ich", "das", "Herz", "der", "jun\u00b7gen", "Ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Lehre dann in Thr\u00e4nen des Knaben Auge zerfliessen,", "tokens": ["Leh\u00b7re", "dann", "in", "Thr\u00e4\u00b7nen", "des", "Kna\u00b7ben", "Au\u00b7ge", "zer\u00b7flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Die er unwissend der Sterblichen weinet,", "tokens": ["Die", "er", "un\u00b7wis\u00b7send", "der", "Sterb\u00b7li\u00b7chen", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.29": {"text": "Die er lieben soll. S\u00e4he den Knaben die Sterbliche weinen,", "tokens": ["Die", "er", "lie\u00b7ben", "soll", ".", "S\u00e4\u00b7he", "den", "Kna\u00b7ben", "die", "Sterb\u00b7li\u00b7che", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$.", "NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "O sie w\u00fcrd' ihn da schon umarmen,", "tokens": ["O", "sie", "w\u00fcrd'", "ihn", "da", "schon", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und ihn lieben, und w\u00fcsst' es doch nicht, dass es Liebe w\u00e4re,", "tokens": ["Und", "ihn", "lie\u00b7ben", ",", "und", "w\u00fcsst'", "es", "doch", "nicht", ",", "dass", "es", "Lie\u00b7be", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Was sie in seiner Umarmung empf\u00e4nde.", "tokens": ["Was", "sie", "in", "sei\u00b7ner", "Um\u00b7ar\u00b7mung", "em\u00b7pf\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Wenn die Sterbliche nun, wie an den B\u00e4chen des Himmels", "tokens": ["Wenn", "die", "Sterb\u00b7li\u00b7che", "nun", ",", "wie", "an", "den", "B\u00e4\u00b7chen", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Eine Rose der Seraphim, aufbl\u00fcht,", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "der", "Se\u00b7ra\u00b7phim", ",", "auf\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Und den J\u00fcngling erblickt, der seiner Einsamkeit Tage", "tokens": ["Und", "den", "J\u00fcng\u00b7ling", "er\u00b7blickt", ",", "der", "sei\u00b7ner", "Ein\u00b7sam\u00b7keit", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "F\u00fchlt, und seufzend ihr Ende verlanget,", "tokens": ["F\u00fchlt", ",", "und", "seuf\u00b7zend", "ihr", "En\u00b7de", "ver\u00b7lan\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.37": {"text": "L\u00e4sst sie der Thr\u00e4nen viel ihn weinen, Thr\u00e4nen der Wehmuth,", "tokens": ["L\u00e4sst", "sie", "der", "Thr\u00e4\u00b7nen", "viel", "ihn", "wei\u00b7nen", ",", "Thr\u00e4\u00b7nen", "der", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PPER", "VVINF", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.38": {"text": "Und der unaussprechlichen Liebe.", "tokens": ["Und", "der", "un\u00b7aus\u00b7sprech\u00b7li\u00b7chen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Denn sie f\u00fchlet noch nicht f\u00fcr ihn, was f\u00fcr sie er empfindet,", "tokens": ["Denn", "sie", "f\u00fch\u00b7let", "noch", "nicht", "f\u00fcr", "ihn", ",", "was", "f\u00fcr", "sie", "er", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$,", "PRELS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-++--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.40": {"text": "Kennet nicht den z\u00e4rtlichen Kummer", "tokens": ["Ken\u00b7net", "nicht", "den", "z\u00e4rt\u00b7li\u00b7chen", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "Seiner Seele, den thr\u00e4nenden Blick nicht des wachenden Auges", "tokens": ["Sei\u00b7ner", "See\u00b7le", ",", "den", "thr\u00e4\u00b7nen\u00b7den", "Blick", "nicht", "des", "wa\u00b7chen\u00b7den", "Au\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.42": {"text": "Durch die mittern\u00e4chtlichen Stunden,", "tokens": ["Durch", "die", "mit\u00b7ter\u00b7n\u00e4cht\u00b7li\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "l