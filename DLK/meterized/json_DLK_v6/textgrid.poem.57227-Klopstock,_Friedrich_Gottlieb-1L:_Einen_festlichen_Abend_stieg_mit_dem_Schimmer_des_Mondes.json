, "line.62": {"text": "Als er Seelen schuf zu der Liebe.", "tokens": ["Als", "er", "See\u00b7len", "schuf", "zu", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.63": {"text": "Und wie stolz, mit welcher Empfindung bring' ich die Seelen,", "tokens": ["Und", "wie", "stolz", ",", "mit", "wel\u00b7cher", "Emp\u00b7fin\u00b7dung", "bring'", "ich", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$,", "APPR", "PWAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.64": {"text": "Nach dem Tode, zur ewigen Ruhe,", "tokens": ["Nach", "dem", "To\u00b7de", ",", "zur", "e\u00b7wi\u00b7gen", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.65": {"text": "Zu den Schaaren der Liebenden alle, die ewig sich lieben,", "tokens": ["Zu", "den", "Schaa\u00b7ren", "der", "Lie\u00b7ben\u00b7den", "al\u00b7le", ",", "die", "e\u00b7wig", "sich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PIS", "$,", "PRELS", "ADJD", "PRF", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.66": {"text": "Weil sie auf Erden sich fanden und liebten!\u00ab", "tokens": ["Weil", "sie", "auf", "Er\u00b7den", "sich", "fan\u00b7den", "und", "lieb\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER",