{"dta.poem.3052": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Andacht lis, und dich wird jedes Buch erbauen;", "tokens": ["Mit", "An\u00b7dacht", "lis", ",", "und", "dich", "wird", "je\u00b7des", "Buch", "er\u00b7bau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$,", "KON", "PPER", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Andacht schau, und du wirst lauter Wunder schauen.", "tokens": ["Mit", "An\u00b7dacht", "schau", ",", "und", "du", "wirst", "lau\u00b7ter", "Wun\u00b7der", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KON", "PPER", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Andacht sprich nur, und man h\u00f6rt dir zu and\u00e4chtig;", "tokens": ["Mit", "An\u00b7dacht", "sprich", "nur", ",", "und", "man", "h\u00f6rt", "dir", "zu", "an\u00b7d\u00e4ch\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "$,", "KON", "PIS", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Andacht bist du stark, und ohn' Andacht ohnm\u00e4chtig.", "tokens": ["Mit", "An\u00b7dacht", "bist", "du", "stark", ",", "und", "ohn'", "An\u00b7dacht", "ohn\u00b7m\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "pun