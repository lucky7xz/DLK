{"textgrid.poem.28016": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann zwischen Menschen Hertz und zwischen Gottes Liebe", "tokens": ["Wann", "zwi\u00b7schen", "Men\u00b7schen", "Hertz", "und", "zwi\u00b7schen", "Got\u00b7tes", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NN", "KON", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erde Schatten f\u00e4llt, so wird es sch\u00e4dlich tr\u00fcbe;", "tokens": ["Der", "Er\u00b7de", "Schat\u00b7ten", "f\u00e4llt", ",", "so", "wird", "es", "sch\u00e4d\u00b7lich", "tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann Gottes Trost vergeht, der doch allein erfreut,", "tokens": ["Dann", "Got\u00b7tes", "Trost", "ver\u00b7geht", ",", "der", "doch", "al\u00b7lein", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum bleibt dem Hertzen nichts, als Welt, das ist: nur Leid.", "tokens": ["Drum", "bleibt", "dem", "Hert\u00b7zen", "nichts", ",", "als", "Welt", ",", "das", "ist", ":", "nur", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PIS", "$,", "KOUS", "NN", "$,", "PDS", "VAFIN", "$.", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}