{"dta.poem.1674": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist die deutsche Welt in Neuigkeit ersoffen! ", "tokens": ["Wie", "ist", "die", "deut\u00b7sche", "Welt", "in", "Neu\u00b7ig\u00b7keit", "er\u00b7sof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man deckt und kleidet sich/ man schreibet/ singt und", "tokens": ["Man", "deckt", "und", "klei\u00b7det", "sich", "/", "man", "schrei\u00b7bet", "/", "singt", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PRF", "$(", "PIS", "VVFIN", "$(", "VVFIN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man reiset/ schl\u00e4fft und isst/ man reitet/ tanzt und ficht", "tokens": ["Man", "rei\u00b7set", "/", "schl\u00e4fft", "und", "isst", "/", "man", "rei\u00b7tet", "/", "tanzt", "und", "ficht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$(", "PIS", "VVFIN", "$(", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach neu-erw\u00e4hlter Art: Wer Gl\u00fcck und Gunst will hoffen/", "tokens": ["Nach", "neu\u00b7er\u00b7w\u00e4hl\u00b7ter", "Art", ":", "Wer", "Gl\u00fcck", "und", "Gunst", "will", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PWS", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mu\u00df sich in allem Thun der Neuigkeit bequemen/", "tokens": ["Mu\u00df", "sich", "in", "al\u00b7lem", "Thun", "der", "Neu\u00b7ig\u00b7keit", "be\u00b7que\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PIS", "NN", "ART", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sonst wird ihn Uberwitz mit Hohn und Spott besch\u00e4men.", "tokens": ["Sonst", "wird", "ihn", "U\u00b7ber\u00b7witz", "mit", "Hohn", "und", "Spott", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es bleibet nicht darbey: Man \u00e4ndert auch die Sitten/", "tokens": ["Es", "blei\u00b7bet", "nicht", "dar\u00b7bey", ":", "Man", "\u00e4n\u00b7dert", "auch", "die", "Sit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$.", "PIS", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der K\u00fcttel alter Treu und deutscher Redligkeit", "tokens": ["Der", "K\u00fct\u00b7tel", "al\u00b7ter", "Treu", "und", "deut\u00b7scher", "Red\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist unsrer Mode-Welt ein viel zu schlechtes Kleid:", "tokens": ["Ist", "uns\u00b7rer", "Mo\u00b7de\u00b7Welt", "ein", "viel", "zu", "schlech\u00b7tes", "Kleid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die junge Neuigkeit will \u00fcberall gebitten.", "tokens": ["Die", "jun\u00b7ge", "Neu\u00b7ig\u00b7keit", "will", "\u00fc\u00b7be\u00b7rall", "ge\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was Wunder/ wenn nun auch in manchem deutschen Lande", "tokens": ["Was", "Wun\u00b7der", "/", "wenn", "nun", "auch", "in", "man\u00b7chem", "deut\u00b7schen", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$(", "KOUS", "ADV", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der neue vor will gehn dem alten Adel-Stande?", "tokens": ["Der", "neu\u00b7e", "vor", "will", "gehn", "dem", "al\u00b7ten", "A\u00b7del\u00b7Stan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Alter wird veracht/ das doch so viel begehren:", "tokens": ["Das", "Al\u00b7ter", "wird", "ver\u00b7acht", "/", "das", "doch", "so", "viel", "be\u00b7geh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "PDS", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch will ich lieber Alt- als Jung-gebohren seyn;", "tokens": ["Doch", "will", "ich", "lie\u00b7ber", "Al\u00b7t", "als", "Jung\u00b7ge\u00b7boh\u00b7ren", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "TRUNC", "KON", "NN", "VAINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit Auffgeld tauschet man die alten M\u00fcntzen ein:", "tokens": ["Mit", "Auff\u00b7geld", "tau\u00b7schet", "man", "die", "al\u00b7ten", "M\u00fcnt\u00b7zen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Firne-Wein gilt mehr/ als der noch soll verj\u00e4hren.", "tokens": ["Der", "Fir\u00b7ne\u00b7Wein", "gilt", "mehr", "/", "als", "der", "noch", "soll", "ver\u00b7j\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "KOUS", "ART", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sieht die Aloe nach hundert Jahren bl\u00fchen/", "tokens": ["Man", "sieht", "die", "A\u00b7loe", "nach", "hun\u00b7dert", "Jah\u00b7ren", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der jungen Tulpe Pracht in kurtzer Zeit entfliehen.", "tokens": ["Der", "jun\u00b7gen", "Tul\u00b7pe", "Pracht", "in", "kurt\u00b7zer", "Zeit", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}