{"text": "vor\u00fcber mein Boot ...", "tokens": ["vor\u00b7\u00fc\u00b7ber", "mein", "Boot", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein Schrei aus der Ferne \u2013", "tokens": ["Ein", "Schrei", "aus", "der", "Fer\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "dann still wie zuvor.", "tokens": ["dann", "still", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie weit sich von Menschen", "tokens": ["Wie", "weit", "sich", "von", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "mein Leben verlor! ...", "tokens": ["mein", "Le\u00b7ben", "ver\u00b7lor", "!", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Eine Welle l\u00e4uft leise", "tokens": ["Ei\u00b7ne", "Wel\u00b7le", "l\u00e4uft", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "schon lang nebenher,", "tokens": ["schon", "lang", "ne\u00b7ben\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.7": {"text": "sie denkt wohl, ich reise", "tokens": ["sie", "denkt", "wohl", ",", "ich", "rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "hinunter zum Meer ...", "tokens": ["hin\u00b7un\u00b7ter", "zum", "Meer", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ja, ich reise, ich reise,", "tokens": ["Ja", ",", "ich", "rei\u00b7se", ",", "ich", "rei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "wei\u00df selbst nicht, wohin.", "tokens": ["wei\u00df", "selbst", "nicht", ",", "wo\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Immer weiter und weiter", "tokens": ["Im\u00b7mer", "wei\u00b7ter", "und", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "verlockt mich mein Sinn.", "tokens": ["ver\u00b7lockt", "mich", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Schon k\u00fcndet ein Schimmer", "tokens": ["Schon", "k\u00fcn\u00b7det", "ein", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "vom morgenden Rot, \u2013", "tokens": ["vom", "mor\u00b7gen\u00b7den", "Rot", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und ich treibe noch immer", "tokens": ["und", "ich", "trei\u00b7be", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "im fl\u00fcsternden Boot.", "tokens": ["im", "fl\u00fcs\u00b7tern\u00b7den", "Boot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}