{"textgrid.poem.66890": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie kommen aus kahlen Kasernen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie kommen aus kahlen Kasernen,", "tokens": ["Sie", "kom\u00b7men", "aus", "kah\u00b7len", "Ka\u00b7ser\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu trinken das himmlische Licht,", "tokens": ["Zu", "trin\u00b7ken", "das", "himm\u00b7li\u00b7sche", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Aus ihren Augensternen", "tokens": ["Aus", "ih\u00b7ren", "Au\u00b7gens\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schimmer der Sehnsucht bricht.", "tokens": ["Ein", "Schim\u00b7mer", "der", "Sehn\u00b7sucht", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie kommen aus dumpfigen Gassen", "tokens": ["Sie", "kom\u00b7men", "aus", "dump\u00b7fi\u00b7gen", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und scharen sich machtvoll zuhauf,", "tokens": ["Und", "scha\u00b7ren", "sich", "macht\u00b7voll", "zu\u00b7hauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Erde zu umfassen", "tokens": ["Die", "Er\u00b7de", "zu", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ihrem sonnigen Lauf.", "tokens": ["In", "ih\u00b7rem", "son\u00b7ni\u00b7gen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie haben die Stimme vernommen,", "tokens": ["Sie", "ha\u00b7ben", "die", "Stim\u00b7me", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Pfade der Freiheit erw\u00e4hlt,", "tokens": ["Die", "Pfa\u00b7de", "der", "Frei\u00b7heit", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie kommen zuhauf, sie kommen,", "tokens": ["Sie", "kom\u00b7men", "zu\u00b7hauf", ",", "sie", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Feuern des Kampfes gest\u00e4hlt.", "tokens": ["In", "Feu\u00b7ern", "des", "Kamp\u00b7fes", "ge\u00b7st\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Sie kommen, von Wahrheit durchdrungen,", "tokens": ["Sie", "kom\u00b7men", ",", "von", "Wahr\u00b7heit", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Erkauft mit Wunden und Blut,", "tokens": ["Er\u00b7kauft", "mit", "Wun\u00b7den", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Umlodert von flammenden Zungen", "tokens": ["Um\u00b7lo\u00b7dert", "von", "flam\u00b7men\u00b7den", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Der furchtbaren Zornesglut.", "tokens": ["Der", "furcht\u00b7ba\u00b7ren", "Zor\u00b7nes\u00b7glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ihr Ha\u00df, ihr Ha\u00df gilt der H\u00f6lle", "tokens": ["Ihr", "Ha\u00df", ",", "ihr", "Ha\u00df", "gilt", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der goldgeknechteten Welt \u2013", "tokens": ["Der", "gold\u00b7ge\u00b7knech\u00b7te\u00b7ten", "Welt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn dieser Ha\u00df noch schw\u00f6lle,", "tokens": ["Wenn", "die\u00b7ser", "Ha\u00df", "noch", "schw\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schl\u00f6sser w\u00fcrden zerschellt,", "tokens": ["Die", "Schl\u00f6s\u00b7ser", "w\u00fcr\u00b7den", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Throne, sie st\u00fcrzten zusammen,", "tokens": ["Die", "Thro\u00b7ne", ",", "sie", "st\u00fcrz\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Heere verschl\u00e4nge der Schlund,", "tokens": ["Die", "Hee\u00b7re", "ver\u00b7schl\u00e4n\u00b7ge", "der", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und in wilden, vulkanischen Flammen", "tokens": ["Und", "in", "wil\u00b7den", ",", "vul\u00b7ka\u00b7ni\u00b7schen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ginge alles Geschaffne zugrund.", "tokens": ["Gin\u00b7ge", "al\u00b7les", "Ge\u00b7schaff\u00b7ne", "zu\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch nein! Sie wollen es wagen,", "tokens": ["Doch", "nein", "!", "Sie", "wol\u00b7len", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr Wirken der Menschheit weihn,", "tokens": ["Ihr", "Wir\u00b7ken", "der", "Menschheit", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und sie zeugen in Maientagen,", "tokens": ["Und", "sie", "zeu\u00b7gen", "in", "Mai\u00b7en\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo lieblich die Bl\u00fcten schnein,", "tokens": ["Wo", "lieb\u00b7lich", "die", "Bl\u00fc\u00b7ten", "schnein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ja, sie zeugen vom fruchtbaren Leben,", "tokens": ["Ja", ",", "sie", "zeu\u00b7gen", "vom", "frucht\u00b7ba\u00b7ren", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Das schwellend und wachsend sich baut,", "tokens": ["Das", "schwel\u00b7lend", "und", "wach\u00b7send", "sich", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und wollen zur Sonne heben", "tokens": ["Und", "wol\u00b7len", "zur", "Son\u00b7ne", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ihr Haupt, das sich selber vertraut.", "tokens": ["Ihr", "Haupt", ",", "das", "sich", "sel\u00b7ber", "ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und sie schauen die k\u00fcnftigen Helden,", "tokens": ["Und", "sie", "schau\u00b7en", "die", "k\u00fcnf\u00b7ti\u00b7gen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Die Siege der Freiheit voraus,", "tokens": ["Die", "Sie\u00b7ge", "der", "Frei\u00b7heit", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihre Dichter sollen es melden", "tokens": ["Ih\u00b7re", "Dich\u00b7ter", "sol\u00b7len", "es", "mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie rauschenden Meeres Gebraus ...", "tokens": ["Wie", "rau\u00b7schen\u00b7den", "Mee\u00b7res", "Ge\u00b7braus", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Drum ziehn sie aus dumpfigen Gassen", "tokens": ["Drum", "ziehn", "sie", "aus", "dump\u00b7fi\u00b7gen", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sammeln sich machtvoll zuhauf,", "tokens": ["Und", "sam\u00b7meln", "sich", "macht\u00b7voll", "zu\u00b7hauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Die Erde k\u00fchn zu umfassen", "tokens": ["Die", "Er\u00b7de", "k\u00fchn", "zu", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In ihrem sonnigen Lauf.", "tokens": ["In", "ih\u00b7rem", "son\u00b7ni\u00b7gen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}