{"textgrid.poem.43358": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die junge Dirne kehrt, sobald die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die junge Dirne kehrt, sobald die Sonne", "tokens": ["Die", "jun\u00b7ge", "Dir\u00b7ne", "kehrt", ",", "so\u00b7bald", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich neigt, vom Feld nach Haus,", "tokens": ["Sich", "neigt", ",", "vom", "Feld", "nach", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr B\u00fcndel Gras zu H\u00e4upten, in der Hand", "tokens": ["Ihr", "B\u00fcn\u00b7del", "Gras", "zu", "H\u00e4up\u00b7ten", ",", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Rosen und Violen einen Strau\u00df,", "tokens": ["Von", "Ro\u00b7sen", "und", "Vi\u00b7o\u00b7len", "ei\u00b7nen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und freut sich schon, daraus", "tokens": ["Und", "freut", "sich", "schon", ",", "da\u00b7raus"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$,", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Morgen am Sonntag wieder", "tokens": ["Mor\u00b7gen", "am", "Sonn\u00b7tag", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Den Schmuck f\u00fcr Haar und Mieder zu gewinnen.", "tokens": ["Den", "Schmuck", "f\u00fcr", "Haar", "und", "Mie\u00b7der", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit ihren Nachbarinnen", "tokens": ["Mit", "ih\u00b7ren", "Nach\u00b7ba\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sitzt vor der Th\u00fcr das M\u00fctterchen und spinnt", "tokens": ["Sitzt", "vor", "der", "Th\u00fcr", "das", "M\u00fct\u00b7ter\u00b7chen", "und", "spinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und schaut gen Abend, wo der Tag vergl\u00fcht,", "tokens": ["Und", "schaut", "gen", "A\u00b7bend", ",", "wo", "der", "Tag", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und plaudert von den eignen jungen Tagen,", "tokens": ["Und", "plau\u00b7dert", "von", "den", "eig\u00b7nen", "jun\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wo sie am Feiertag sich auch geputzt hat", "tokens": ["Wo", "sie", "am", "Fei\u00b7er\u00b7tag", "sich", "auch", "ge\u00b7putzt", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PRF", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und schlank noch und geschwind", "tokens": ["Und", "schlank", "noch", "und", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Am Abend dann zu tanzen pflag mit Denen,", "tokens": ["Am", "A\u00b7bend", "dann", "zu", "tan\u00b7zen", "pflag", "mit", "De\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "APPR", "PDS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die ihrer sch\u00f6nsten Zeit Gef\u00e4hrten waren.", "tokens": ["Die", "ih\u00b7rer", "sch\u00f6ns\u00b7ten", "Zeit", "Ge\u00b7f\u00e4hr\u00b7ten", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Schon aus der H\u00f6he sinkt", "tokens": ["Schon", "aus", "der", "H\u00f6\u00b7he", "sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Tiefblaue D\u00e4mmrung, und die Schatten fallen", "tokens": ["Tief\u00b7blau\u00b7e", "D\u00e4mm\u00b7rung", ",", "und", "die", "Schat\u00b7ten", "fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Von D\u00e4chern und von H\u00fcgeln,", "tokens": ["Von", "D\u00e4\u00b7chern", "und", "von", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Da silbern jetzt der neue Mond erblinkt.", "tokens": ["Da", "sil\u00b7bern", "jetzt", "der", "neu\u00b7e", "Mond", "er\u00b7blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und nun beginnt die Glocke", "tokens": ["Und", "nun", "be\u00b7ginnt", "die", "Glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Den Festtag einzul\u00e4uten,", "tokens": ["Den", "Fest\u00b7tag", "ein\u00b7zu\u00b7l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und bei dem Klange zieht es", "tokens": ["Und", "bei", "dem", "Klan\u00b7ge", "zieht", "es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Wie Trost in alle Seelen.", "tokens": ["Wie", "Trost", "in", "al\u00b7le", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Die Knaben, die in Haufen", "tokens": ["Die", "Kna\u00b7ben", ",", "die", "in", "Hau\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Dort auf dem Platze jauchzen", "tokens": ["Dort", "auf", "dem", "Plat\u00b7ze", "jauch\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Und hier- und dorthin laufen,", "tokens": ["Und", "hier", "und", "dor\u00b7thin", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Wie lachen sie und l\u00e4rmen!", "tokens": ["Wie", "la\u00b7chen", "sie", "und", "l\u00e4r\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Indessen kehrt zu seinem d\u00fcrft'gen Tisch", "tokens": ["In\u00b7des\u00b7sen", "kehrt", "zu", "sei\u00b7nem", "d\u00fcrft'\u00b7gen", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Pfl\u00fcger pfeifend heim", "tokens": ["Der", "Pfl\u00fc\u00b7ger", "pfei\u00b7fend", "heim"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Und denkt bei sich an seinen Ruhetag.", "tokens": ["Und", "denkt", "bei", "sich", "an", "sei\u00b7nen", "Ru\u00b7he\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann, wenn erloschen jedes Licht ringsum", "tokens": ["Dann", ",", "wenn", "er\u00b7lo\u00b7schen", "je\u00b7des", "Licht", "ring\u00b7sum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADJA", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und alles Andre stumm,", "tokens": ["Und", "al\u00b7les", "And\u00b7re", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rst du den Hammer klopfen, h\u00f6rst die S\u00e4ge", "tokens": ["H\u00f6rst", "du", "den", "Ham\u00b7mer", "klop\u00b7fen", ",", "h\u00f6rst", "die", "S\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Zimmermanns, der wacht", "tokens": ["Des", "Zim\u00b7mer\u00b7manns", ",", "der", "wacht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In der verschlossnen Werkstatt und beim L\u00e4mpchen", "tokens": ["In", "der", "ver\u00b7schloss\u00b7nen", "Werk\u00b7statt", "und", "beim", "L\u00e4mp\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich sputet, da\u00df die Arbeit", "tokens": ["Sich", "spu\u00b7tet", ",", "da\u00df", "die", "Ar\u00b7beit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch fertig werde, eh' der Tag sich r\u00f6thet.", "tokens": ["Noch", "fer\u00b7tig", "wer\u00b7de", ",", "eh'", "der", "Tag", "sich", "r\u00f6\u00b7thet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dies ist der liebste von den sieben Tagen,", "tokens": ["Dies", "ist", "der", "liebs\u00b7te", "von", "den", "sie\u00b7ben", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "APPR", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Hoffnung, voller Wonne.", "tokens": ["Voll", "Hoff\u00b7nung", ",", "vol\u00b7ler", "Won\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es bringt die neue Sonne", "tokens": ["Es", "bringt", "die", "neu\u00b7e", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tr\u00fcbsinn und Langweil; Jeder denkt im Stillen,", "tokens": ["Tr\u00fcb\u00b7sinn", "und", "Lang\u00b7weil", ";", "Je\u00b7der", "denkt", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Da\u00df wieder sich erneu'n die alten Plagen.", "tokens": ["Da\u00df", "wie\u00b7der", "sich", "er\u00b7neu'n", "die", "al\u00b7ten", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du muntrer Knabe, dies", "tokens": ["Du", "mun\u00b7trer", "Kna\u00b7be", ",", "dies"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PDS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein Bl\u00fctenalter gleicht", "tokens": ["Dein", "Bl\u00fc\u00b7ten\u00b7al\u00b7ter", "gleicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Solch einem heitren Tag, so klar und froh,", "tokens": ["Solch", "ei\u00b7nem", "hei\u00b7tren", "Tag", ",", "so", "klar", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn er dann entfloh,", "tokens": ["Und", "wenn", "er", "dann", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hast deines Lebens Sonntag du erreicht.", "tokens": ["Hast", "dei\u00b7nes", "Le\u00b7bens", "Sonn\u00b7tag", "du", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Genie\u00df ihn, Kind; gar s\u00fc\u00df ist diese Zeit,", "tokens": ["Ge\u00b7nie\u00df", "ihn", ",", "Kind", ";", "gar", "s\u00fc\u00df", "ist", "die\u00b7se", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "ADV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Jeder lebt sie gerne.", "tokens": ["Und", "Je\u00b7der", "lebt", "sie", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr will ich dir nicht sagen. Doch da\u00df ferne", "tokens": ["Mehr", "will", "ich", "dir", "nicht", "sa\u00b7gen", ".", "Doch", "da\u00df", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$.", "KON", "KOUS", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir noch dein Sonntag, sei es dir nicht leid!", "tokens": ["Dir", "noch", "dein", "Sonn\u00b7tag", ",", "sei", "es", "dir", "nicht", "leid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}