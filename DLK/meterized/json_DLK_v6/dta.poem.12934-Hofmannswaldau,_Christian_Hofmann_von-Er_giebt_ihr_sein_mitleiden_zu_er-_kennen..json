{"dta.poem.12934": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er giebt ihr sein mitleiden zu er-  \n kennen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So weinest du? ach ja, du weinst, mein Leben!", "tokens": ["So", "wei\u00b7nest", "du", "?", "ach", "ja", ",", "du", "weinst", ",", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "XY", "PTKANT", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ich, ich selber wein\u2019, und mu\u00df in \u00e4ngsten schweben;", "tokens": ["Und", "ich", ",", "ich", "sel\u00b7ber", "wein'", ",", "und", "mu\u00df", "in", "\u00e4ngs\u00b7ten", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "ADV", "PTKVZ", "$,", "KON", "VMFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn deine wehmuth dringt in meiner adern blut:", "tokens": ["Denn", "dei\u00b7ne", "weh\u00b7muth", "dringt", "in", "mei\u00b7ner", "a\u00b7dern", "blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiewol sie redet nicht; Doch wilst du sie verstehn?", "tokens": ["Wie\u00b7wol", "sie", "re\u00b7det", "nicht", ";", "Doch", "wilst", "du", "sie", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKNEG", "$.", "KON", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So la\u00df nur einen blick in meine sterne gehn,", "tokens": ["So", "la\u00df", "nur", "ei\u00b7nen", "blick", "in", "mei\u00b7ne", "ster\u00b7ne", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann wirst du sie schon zu erkennen wissen,", "tokens": ["Dann", "wirst", "du", "sie", "schon", "zu", "er\u00b7ken\u00b7nen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und durch den trieb erbarmnis-voller glut", "tokens": ["Und", "durch", "den", "trieb", "er\u00b7bar\u00b7mnis\u00b7vol\u00b7ler", "glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mein eigen hertze sehn in deinen thr\u00e4nen flie\u00dfen.", "tokens": ["Mein", "ei\u00b7gen", "hert\u00b7ze", "sehn", "in", "dei\u00b7nen", "thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}