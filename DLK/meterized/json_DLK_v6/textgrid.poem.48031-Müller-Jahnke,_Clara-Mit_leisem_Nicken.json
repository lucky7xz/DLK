{"textgrid.poem.48031": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Mit leisem Nicken", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem fernen, fremden Ort", "tokens": ["An", "ei\u00b7nem", "fer\u00b7nen", ",", "frem\u00b7den", "Ort"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "war's, wo ich all mein Gl\u00fcck verloren;", "tokens": ["wa\u00b7r's", ",", "wo", "ich", "all", "mein", "Gl\u00fcck", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "ich ging, dich suchend, fort und fort", "tokens": ["ich", "ging", ",", "dich", "su\u00b7chend", ",", "fort", "und", "fort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVPP", "$,", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vorbei an festverschlossenen Toren.", "tokens": ["vor\u00b7bei", "an", "fest\u00b7ver\u00b7schlos\u00b7se\u00b7nen", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-----+--+-", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Am fernen Horizont erblich", "tokens": ["Am", "fer\u00b7nen", "Ho\u00b7ri\u00b7zont", "er\u00b7blich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Abendr\u00f6te letzter Schimmer \u2013", "tokens": ["der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "letz\u00b7ter", "Schim\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit blutendem Herzen sucht ich dich", "tokens": ["mit", "blu\u00b7ten\u00b7dem", "Her\u00b7zen", "sucht", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und suchte dich und fand dich nimmer.", "tokens": ["und", "such\u00b7te", "dich", "und", "fand", "dich", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann war's nach Jahren, als sich grau", "tokens": ["Dann", "wa\u00b7r's", "nach", "Jah\u00b7ren", ",", "als", "sich", "grau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "$,", "KOUS", "PRF", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das Haar um meine Schl\u00e4fe schmiegte,", "tokens": ["das", "Haar", "um", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "schmieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "als auf der bl\u00fctenleeren Au", "tokens": ["als", "auf", "der", "bl\u00fc\u00b7ten\u00b7lee\u00b7ren", "Au"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der letzte Halm im Wind sich wiegte,", "tokens": ["der", "letz\u00b7te", "Halm", "im", "Wind", "sich", "wieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df wir uns trafen \u2013 da\u00df du mir", "tokens": ["Da\u00df", "wir", "uns", "tra\u00b7fen", "\u2013", "da\u00df", "du", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$(", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von fern gewinkt mit leisem Nicken . . . . . . .", "tokens": ["von", "fern", "ge\u00b7winkt", "mit", "lei\u00b7sem", "Ni\u00b7cken", ".", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$.", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Gru\u00df von dir \u2013 ein Laut von dir \u2013", "tokens": ["Ein", "Gru\u00df", "von", "dir", "\u2013", "ein", "Laut", "von", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$(", "ART", "APPR", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Widerschein aus feuchten Blicken!", "tokens": ["ein", "Wi\u00b7der\u00b7schein", "aus", "feuch\u00b7ten", "Bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und eh ich noch die liebe Hand", "tokens": ["Und", "eh", "ich", "noch", "die", "lie\u00b7be", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit z\u00e4rtlich festem Druck umfangen,", "tokens": ["mit", "z\u00e4rt\u00b7lich", "fes\u00b7tem", "Druck", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "war schon dein Bild am Himmelsrand", "tokens": ["war", "schon", "dein", "Bild", "am", "Him\u00b7mels\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie Sp\u00e4trotschein dahingegangen.", "tokens": ["wie", "Sp\u00e4t\u00b7rot\u00b7schein", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da wacht ich auf. \u2013 Vor Sehnsucht bla\u00df", "tokens": ["Da", "wacht", "ich", "auf", ".", "\u2013", "Vor", "Sehn\u00b7sucht", "bla\u00df"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sah Morgend\u00e4mmrung in mein Zimmer;", "tokens": ["sah", "Mor\u00b7gen\u00b7d\u00e4mm\u00b7rung", "in", "mein", "Zim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mein Herz schlug laut, mein Aug war na\u00df \u2013 \u2013 \u2013", "tokens": ["mein", "Herz", "schlug", "laut", ",", "mein", "Aug", "war", "na\u00df", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich f\u00fchl's: ich seh dich nun und nimmer.", "tokens": ["ich", "f\u00fchl's", ":", "ich", "seh", "dich", "nun", "und", "nim\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}