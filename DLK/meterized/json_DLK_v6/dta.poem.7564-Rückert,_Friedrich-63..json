{"dta.poem.7564": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie au\u00dfer Athem, wem der Kopf brennt, kommt gelaufen", "tokens": ["Wie", "au\u00b7\u00dfer", "A\u00b7them", ",", "wem", "der", "Kopf", "brennt", ",", "kommt", "ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um zu l\u00f6schen sich st\u00fcrzt in den Wasserhaufen;", "tokens": ["Und", "um", "zu", "l\u00f6\u00b7schen", "sich", "st\u00fcrzt", "in", "den", "Was\u00b7ser\u00b7hau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PTKZU", "VVINF", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So komm' ein Lernender, de\u00df Hirn Weltwirbelglut", "tokens": ["So", "komm'", "ein", "Ler\u00b7nen\u00b7der", ",", "de\u00df", "Hirn", "Welt\u00b7wir\u00b7belg\u00b7lut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umschwindelt, auch gerannt zum Brunnen k\u00fchler Flut,", "tokens": ["Um\u00b7schwin\u00b7delt", ",", "auch", "ge\u00b7rannt", "zum", "Brun\u00b7nen", "k\u00fch\u00b7ler", "Flut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVPP", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum weisen Lehrer, der ihn tief ins Wissen taucht,", "tokens": ["Zum", "wei\u00b7sen", "Leh\u00b7rer", ",", "der", "ihn", "tief", "ins", "Wis\u00b7sen", "taucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch das auf ewig ihm der Brand der Welt verraucht.", "tokens": ["Durch", "das", "auf", "e\u00b7wig", "ihm", "der", "Brand", "der", "Welt", "ver\u00b7raucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ADJD", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Der Meister mitleidsvoll hilft treu den Kampf ihm k\u00e4mpfen;", "tokens": ["Der", "Meis\u00b7ter", "mit\u00b7le\u00b7ids\u00b7voll", "hilft", "treu", "den", "Kampf", "ihm", "k\u00e4mp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADJD", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn stets begierig ist das Wasser Glut zu d\u00e4mpfen.", "tokens": ["Denn", "stets", "be\u00b7gie\u00b7rig", "ist", "das", "Was\u00b7ser", "Glut", "zu", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}