{"textgrid.poem.47505": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Weisen mit ihrem sauern Rath]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Weisen mit ihrem sauern Rath,", "tokens": ["Die", "Wei\u00b7sen", "mit", "ih\u00b7rem", "sau\u00b7ern", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie machen uns das Herz so schwer;", "tokens": ["Sie", "ma\u00b7chen", "uns", "das", "Herz", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4re nicht mein alter Wirth,", "tokens": ["Und", "w\u00e4\u00b7re", "nicht", "mein", "al\u00b7ter", "Wirth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lebte wohl schon lang nicht mehr.", "tokens": ["Ich", "leb\u00b7te", "wohl", "schon", "lang", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}