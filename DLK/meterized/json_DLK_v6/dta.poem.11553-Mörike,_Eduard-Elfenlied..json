{"dta.poem.11553": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Elfenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Bei Nacht im Dorf der W\u00e4chter rief:", "tokens": ["Bei", "Nacht", "im", "Dorf", "der", "W\u00e4ch\u00b7ter", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Elfe!", "tokens": ["El\u00b7fe", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein ganz kleines Elfchen im Walde schlief,", "tokens": ["Ein", "ganz", "klei\u00b7nes", "Elf\u00b7chen", "im", "Wal\u00b7de", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wohl um die Elfe;", "tokens": ["Wohl", "um", "die", "El\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und meint, es rief ihm aus dem Thal", "tokens": ["Und", "meint", ",", "es", "rief", "ihm", "aus", "dem", "Thal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei seinem Namen die Nachtigall,", "tokens": ["Bei", "sei\u00b7nem", "Na\u00b7men", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Oder Silpelit h\u00e4tt' ihn gerufen.", "tokens": ["O\u00b7der", "Sil\u00b7pe\u00b7lit", "h\u00e4tt'", "ihn", "ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Reibt sich der Elf die Augen aus,", "tokens": ["Reibt", "sich", "der", "Elf", "die", "Au\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Begibt sich vor sein Schneckenhaus,", "tokens": ["Be\u00b7gibt", "sich", "vor", "sein", "Schne\u00b7cken\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ist als wie ein trunken Mann,", "tokens": ["Und", "ist", "als", "wie", "ein", "trun\u00b7ken", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sein Schl\u00e4flein war nicht voll gethan,", "tokens": ["Sein", "Schl\u00e4f\u00b7lein", "war", "nicht", "voll", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und humpelt also tippe tapp", "tokens": ["Und", "hum\u00b7pelt", "al\u00b7so", "tip\u00b7pe", "tapp"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Durch's Haselholz in's Thal hinab,", "tokens": ["Durch's", "Ha\u00b7sel\u00b7holz", "in's", "Thal", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Schlupft an der Weinbergmauer hin,", "tokens": ["Schlupft", "an", "der", "Wein\u00b7berg\u00b7mau\u00b7er", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Daran viel Feuerw\u00fcrmchen gl\u00fchn:", "tokens": ["Da\u00b7ran", "viel", "Feu\u00b7er\u00b7w\u00fcrm\u00b7chen", "gl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u201ewas sind das helle Fensterlein?", "tokens": ["\u201e", "was", "sind", "das", "hel\u00b7le", "Fens\u00b7ter\u00b7lein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da drin wird eine Hochzeit seyn;", "tokens": ["Da", "drin", "wird", "ei\u00b7ne", "Hoch\u00b7zeit", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Kleinen sitzen beim Mahle,", "tokens": ["Die", "Klei\u00b7nen", "sit\u00b7zen", "beim", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Und treiben's in dem Saale,", "tokens": ["Und", "trei\u00b7ben's", "in", "dem", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Da guck' ich wohl ein wenig 'nein!\u201c", "tokens": ["Da", "guck'", "ich", "wohl", "ein", "we\u00b7nig", "'n\u00b7ein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "\u2014 Pfui, st\u00f6\u00dft den Kopf an harten Stein!", "tokens": ["Pfui", ",", "st\u00f6\u00dft", "den", "Kopf", "an", "har\u00b7ten", "Stein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Elfe, gelt, du hast genug?", "tokens": ["El\u00b7fe", ",", "gelt", ",", "du", "hast", "ge\u00b7nug", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Gukuk! Gukuk!", "tokens": ["Gu\u00b7kuk", "!", "Gu\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}