{"dta.poem.545": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das freye M\u00e4dgen.  \n Aus dem Frantz\u00f6sis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mit einer Jungferschafft war ich vor dem begabt/", "tokens": ["Mit", "ei\u00b7ner", "Jung\u00b7fer\u00b7schafft", "war", "ich", "vor", "dem", "be\u00b7gabt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "APPR", "ART", "ADJD", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und dieses war der Schatz von meiner Eltern Sparen", "tokens": ["Und", "die\u00b7ses", "war", "der", "Schatz", "von", "mei\u00b7ner", "El\u00b7tern", "Spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein jetzt hab ich nichts von dem was ich gehabt", "tokens": ["Al\u00b7lein", "jetzt", "hab", "ich", "nichts", "von", "dem", "was", "ich", "ge\u00b7habt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIS", "APPR", "ART", "PWS", "PPER", "VAPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein kleines Erbtheil hat die Endschafft schon erfahren.", "tokens": ["Mein", "klei\u00b7nes", "E\u00b7rbtheil", "hat", "die", "End\u00b7schafft", "schon", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Damit der M\u00e4nner Sinn wenn wir von funffzehn Jahren", "tokens": ["Da\u00b7mit", "der", "M\u00e4n\u00b7ner", "Sinn", "wenn", "wir", "von", "funff\u00b7zehn", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN", "KOUS", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn dieses M\u00e4dgen fort wie eine N\u00e4rrinn trabt/", "tokens": ["Denn", "die\u00b7ses", "M\u00e4d\u00b7gen", "fort", "wie", "ei\u00b7ne", "N\u00e4r\u00b7rinn", "trabt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PTKVZ", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df seine Jungferschafft noch l\u00e4nger will verwahren.", "tokens": ["Da\u00df", "sei\u00b7ne", "Jung\u00b7fer\u00b7schafft", "noch", "l\u00e4n\u00b7ger", "will", "ver\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}