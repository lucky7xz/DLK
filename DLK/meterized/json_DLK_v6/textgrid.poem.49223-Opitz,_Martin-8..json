{"textgrid.poem.49223": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du unmenschlichs Mensch, was Leidens Pein und Schmerzen", "tokens": ["O", "du", "un\u00b7menschlichs", "Mensch", ",", "was", "Lei\u00b7dens", "Pein", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "PWS", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Gleicht deiner schn\u00f6den That! Bleibt unverweicht, ihr Herzen,", "tokens": ["Gleicht", "dei\u00b7ner", "schn\u00f6\u00b7den", "That", "!", "Bleibt", "un\u00b7ver\u00b7weicht", ",", "ihr", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wofern ihr gute Frucht des Vaterlandes seid;", "tokens": ["Wo\u00b7fern", "ihr", "gu\u00b7te", "Frucht", "des", "Va\u00b7ter\u00b7lan\u00b7des", "seid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann hier barmherzig sein, ist Unbarmherzigkeit.", "tokens": ["Dann", "hier", "barm\u00b7her\u00b7zig", "sein", ",", "ist", "Un\u00b7barm\u00b7her\u00b7zig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}