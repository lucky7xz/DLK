{"dta.poem.19064": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlV.  Uber das Grab der Gro\u00df-Frau  \n Mutter.   Jm Mertz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Solls seyn, Hochw\u00fcrdigs H\u00e4upt! so neig' auf JEsus\nWincken", "tokens": ["Solls", "seyn", ",", "Hoch\u00b7w\u00fcr\u00b7digs", "H\u00e4upt", "!", "so", "neig'", "auf", "Je\u00b7sus", "Win\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "$,", "ADJA", "NN", "$.", "ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jhm deine von der Last erdr\u00fcckte Scheitel zu:", "tokens": ["Jhm", "dei\u00b7ne", "von", "der", "Last", "er\u00b7dr\u00fcck\u00b7te", "Schei\u00b7tel", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entweiche ", "tokens": ["Ent\u00b7wei\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und la\u00df den ", "tokens": ["Und", "la\u00df", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVIMP", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Zwar trinckt den neuen Wein ins Vaters Reiche nicht,", "tokens": ["Zwar", "trinckt", "den", "neu\u00b7en", "Wein", "ins", "Va\u00b7ters", "Rei\u00b7che", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "NE", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer nicht den bittern Kelch vorher noch angesetzet:", "tokens": ["Wer", "nicht", "den", "bit\u00b7tern", "Kelch", "vor\u00b7her", "noch", "an\u00b7ge\u00b7set\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch, wenn der Myrrhen-Tranck dir kaum die Lippen netzet,", "tokens": ["Doch", ",", "wenn", "der", "Myr\u00b7rhen\u00b7Tranck", "dir", "kaum", "die", "Lip\u00b7pen", "net\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So sieht ein jeder leicht, warum das dir geschicht.", "tokens": ["So", "sieht", "ein", "je\u00b7der", "leicht", ",", "wa\u00b7rum", "das", "dir", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "ADJD", "$,", "PWAV", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es hatte dir die Welt zum \u00f6ftern Tranck und Speise", "tokens": ["Es", "hat\u00b7te", "dir", "die", "Welt", "zum", "\u00f6f\u00b7tern", "Tranck", "und", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Staub der K\u00fcmmerni\u00df, und Thr\u00e4nen-Saltz vermischt;", "tokens": ["Mit", "Staub", "der", "K\u00fcm\u00b7mer\u00b7ni\u00df", ",", "und", "Thr\u00e4\u00b7nen\u00b7Saltz", "ver\u00b7mischt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Arbeit lie\u00df nicht nach, wenn man dir ", "tokens": ["Die", "Ar\u00b7beit", "lie\u00df", "nicht", "nach", ",", "wenn", "man", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PIS", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und deine ", "tokens": ["Und", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So wuste denn ", "tokens": ["So", "wus\u00b7te", "denn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit aufgebotner Macht die Crone zu erk\u00e4mpfen,", "tokens": ["Mit", "auf\u00b7ge\u00b7bot\u00b7ner", "Macht", "die", "Cro\u00b7ne", "zu", "er\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wohl der Feinde Wuth, als deinen Streit zu d\u00e4mpfen,", "tokens": ["So", "wohl", "der", "Fein\u00b7de", "Wuth", ",", "als", "dei\u00b7nen", "Streit", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Indem er aus den Sturm dich bald bey Seite nahm.", "tokens": ["In\u00b7dem", "er", "aus", "den", "Sturm", "dich", "bald", "bey", "Sei\u00b7te", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sonst macht ein k\u00fchner Held bey vortheilhafter Lage", "tokens": ["Sonst", "macht", "ein", "k\u00fch\u00b7ner", "Held", "bey", "vor\u00b7theil\u00b7haf\u00b7ter", "La\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Zugeordneten die gr\u00f6ste Sorgen-Noth:", "tokens": ["Den", "Zu\u00b7ge\u00b7ord\u00b7ne\u00b7ten", "die", "gr\u00f6s\u00b7te", "Sor\u00b7gen\u00b7Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn sie vermuthen schon, da\u00df er, Trotz Blut und Tod,", "tokens": ["Denn", "sie", "ver\u00b7mu\u00b7then", "schon", ",", "da\u00df", "er", ",", "Trotz", "Blut", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-++-++-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Den eingedrungnen Feind zur\u00fcck zu treiben wage.", "tokens": ["Den", "ein\u00b7ge\u00b7drung\u00b7nen", "Feind", "zu\u00b7r\u00fcck", "zu", "trei\u00b7ben", "wa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Bau, den Sturm und Schlag noch eins so starck gemacht,", "tokens": ["Ein", "Bau", ",", "den", "Sturm", "und", "Schlag", "noch", "eins", "so", "starck", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "ADV", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So, da\u00df er nun davon nicht leicht zu zittern pfleget,", "tokens": ["So", ",", "da\u00df", "er", "nun", "da\u00b7von", "nicht", "leicht", "zu", "zit\u00b7tern", "pfle\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PAV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In welchem sich der Geist noch immer lebhaft reget,", "tokens": ["In", "wel\u00b7chem", "sich", "der", "Geist", "noch", "im\u00b7mer", "leb\u00b7haft", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Will ", "tokens": ["Will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Drum h\u00f6ret man das Blut in allen Adern kochen,", "tokens": ["Drum", "h\u00f6\u00b7ret", "man", "das", "Blut", "in", "al\u00b7len", "A\u00b7dern", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Tod und Leben noch aus allen Kr\u00e4ften ringt,", "tokens": ["Weil", "Tod", "und", "Le\u00b7ben", "noch", "aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df der gefangne Geist sich aus den Banden schwingt,", "tokens": ["Bi\u00df", "der", "ge\u00b7fang\u00b7ne", "Geist", "sich", "aus", "den", "Ban\u00b7den", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nachdem er durch das Hertz, und Marck und Bein gebrochen.", "tokens": ["Nach\u00b7dem", "er", "durch", "das", "Hertz", ",", "und", "Marck", "und", "Bein", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da der Seelen-Feind den Erden-Crei\u00df durchrei\u00dft,", "tokens": ["Und", "da", "der", "See\u00b7len\u00b7Feind", "den", "Er\u00b7den\u00b7Crei\u00df", "durc\u00b7hrei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um schon in dieser Zeit die Seelen zu verschlingen;", "tokens": ["Um", "schon", "in", "die\u00b7ser", "Zeit", "die", "See\u00b7len", "zu", "ver\u00b7schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "PDAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vermeynt man, wenn sie itzt aus ihrer H\u00fctte dringen,", "tokens": ["Ver\u00b7meynt", "man", ",", "wenn", "sie", "itzt", "aus", "ih\u00b7rer", "H\u00fct\u00b7te", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df der Erboste nicht noch allen Ernst beweist?", "tokens": ["Da\u00df", "der", "Er\u00b7bos\u00b7te", "nicht", "noch", "al\u00b7len", "Ernst", "be\u00b7weist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dein Geist war nicht gewohnt best\u00fcrtzt herum zu schweiffen,", "tokens": ["Dein", "Geist", "war", "nicht", "ge\u00b7wohnt", "be\u00b7st\u00fcrtzt", "he\u00b7rum", "zu", "schwei\u00b7ffen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "VVPP", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dein unerschrockner Muth ertrotzte manchen Sieg:", "tokens": ["Dein", "un\u00b7er\u00b7schrock\u00b7ner", "Muth", "er\u00b7trotz\u00b7te", "man\u00b7chen", "Sieg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alsdann vermuthet man gar einen schweren Krieg,", "tokens": ["Als\u00b7dann", "ver\u00b7mu\u00b7thet", "man", "gar", "ei\u00b7nen", "schwe\u00b7ren", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Weil diese J\u00fcngerin mir immer treu geblieben,", "tokens": ["Weil", "die\u00b7se", "J\u00fcn\u00b7ge\u00b7rin", "mir", "im\u00b7mer", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihre meiste Zeit in meinem Dienst vertrieben,", "tokens": ["Und", "ih\u00b7re", "meis\u00b7te", "Zeit", "in", "mei\u00b7nem", "Dienst", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So soll sie auch davor mit Ruhe schlaffen gehn.", "tokens": ["So", "soll", "sie", "auch", "da\u00b7vor", "mit", "Ru\u00b7he", "schlaf\u00b7fen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PAV", "APPR", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie hat sich ofte gnug um Cron und Lohn gestritten,", "tokens": ["Sie", "hat", "sich", "of\u00b7te", "gnug", "um", "Cron", "und", "Lohn", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihr das Meer der Angst bi\u00df an den G\u00fcrtel gieng:", "tokens": ["Wenn", "ihr", "das", "Meer", "der", "Angst", "bi\u00df", "an", "den", "G\u00fcr\u00b7tel", "gieng", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob manche Centner-Last auf ihrer Schulter hieng.", "tokens": ["Ob", "man\u00b7che", "Cent\u00b7ner\u00b7Last", "auf", "ih\u00b7rer", "Schul\u00b7ter", "hieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist ihr Fu\u00df doch nicht aus meiner Bahn geglitten.", "tokens": ["So", "ist", "ihr", "Fu\u00df", "doch", "nicht", "aus", "mei\u00b7ner", "Bahn", "ge\u00b7glit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf einen tapfern Kampf folgt iedesmal Triumph,", "tokens": ["Auf", "ei\u00b7nen", "tap\u00b7fern", "Kampf", "folgt", "ie\u00b7des\u00b7mal", "Tri\u00b7umph", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "p