{"textgrid.poem.43402": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn an des Himmels Bl\u00fcthengaben", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn an des Himmels Bl\u00fcthengaben", "tokens": ["Wenn", "an", "des", "Him\u00b7mels", "Bl\u00fc\u00b7then\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Schmetterling' und Bienen laben,", "tokens": ["Sich", "Schmet\u00b7ter\u00b7ling'", "und", "Bie\u00b7nen", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Essen wir Kartoffeln;", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn V\u00f6gel ihre Mahlzeit picken,", "tokens": ["Wenn", "V\u00f6\u00b7gel", "ih\u00b7re", "Mahl\u00b7zeit", "pi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sobald die goldnen \u00c4hren nicken,", "tokens": ["So\u00b7bald", "die", "gold\u00b7nen", "\u00c4h\u00b7ren", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Essen wir Kartoffeln.", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn wir in dem Sande schwitzen,", "tokens": ["Und", "wenn", "wir", "in", "dem", "San\u00b7de", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn wir hinter'm Ofen sitzen,", "tokens": ["Und", "wenn", "wir", "hin\u00b7ter'm", "O\u00b7fen", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Essen wir Kartoffeln;", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und wenn wir zu des Tages Werken", "tokens": ["Und", "wenn", "wir", "zu", "des", "Ta\u00b7ges", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns Morgens oder Mittags st\u00e4rken,", "tokens": ["Uns", "Mor\u00b7gens", "o\u00b7der", "Mit\u00b7tags", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Essen wir Kartoffeln.", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn wir Morgens fr\u00fch aufstehen,", "tokens": ["Und", "wenn", "wir", "Mor\u00b7gens", "fr\u00fch", "auf\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn wir sp\u00e4t zu Bette gehen,", "tokens": ["Und", "wenn", "wir", "sp\u00e4t", "zu", "Bet\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Essen wir Kartoffeln;", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und wenn ein Siechthum bei uns weilet,", "tokens": ["Und", "wenn", "ein", "Siech\u00b7thum", "bei", "uns", "wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn der Tod uns \u00fcbereilet,", "tokens": ["Und", "wenn", "der", "Tod", "uns", "\u00fc\u00b7be\u00b7rei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Essen wir Kartoffeln.", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und wollen wir den Vorschmack haben", "tokens": ["Und", "wol\u00b7len", "wir", "den", "Vor\u00b7schmack", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von jenes Lebens h\u00f6chsten Gaben,", "tokens": ["Von", "je\u00b7nes", "Le\u00b7bens", "h\u00f6chs\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Essen wir Kartoffeln;", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und wenn wir werden auferstehen", "tokens": ["Und", "wenn", "wir", "wer\u00b7den", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ein ins ew'ge Leben gehen,", "tokens": ["Und", "ein", "ins", "ew'\u00b7ge", "Le\u00b7ben", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Essen wir Kartoffeln.", "tokens": ["Es\u00b7sen", "wir", "Kar\u00b7tof\u00b7feln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}