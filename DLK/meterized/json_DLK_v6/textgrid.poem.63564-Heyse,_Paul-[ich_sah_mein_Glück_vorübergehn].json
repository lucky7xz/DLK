{"textgrid.poem.63564": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[ich sah mein Gl\u00fcck vor\u00fcbergehn]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah mein Gl\u00fcck vor\u00fcbergehn,", "tokens": ["Ich", "sah", "mein", "Gl\u00fcck", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich konnt' es am Stirnhaar fassen", "tokens": ["Ich", "konnt'", "es", "am", "Stirn\u00b7haar", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und blieb wie ein t\u00f6richter Tr\u00e4umer stehn", "tokens": ["Und", "blieb", "wie", "ein", "t\u00f6\u00b7rich\u00b7ter", "Tr\u00e4u\u00b7mer", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hab' es vorbeigelassen.", "tokens": ["Und", "hab'", "es", "vor\u00b7bei\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+----+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ich sah mein Gl\u00fcck auf der Wiese ruhn,", "tokens": ["Ich", "sah", "mein", "Gl\u00fcck", "auf", "der", "Wie\u00b7se", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich konnt's auf die Lippen k\u00fcssen", "tokens": ["Ich", "konnt's", "auf", "die", "Lip\u00b7pen", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und starrt' es nur an vom Hut zu den Schuh'n", "tokens": ["Und", "starrt'", "es", "nur", "an", "vom", "Hut", "zu", "den", "Schuh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und habe mich losgerissen.", "tokens": ["Und", "ha\u00b7be", "mich", "los\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich harrte, ob es mit holdem Blick", "tokens": ["Ich", "harr\u00b7te", ",", "ob", "es", "mit", "hol\u00b7dem", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht selbst sich meiner erbarme.", "tokens": ["Nicht", "selbst", "sich", "mei\u00b7ner", "er\u00b7bar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PRF", "PPOSAT", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich dachte: ist es ein rechtes Gl\u00fcck,", "tokens": ["Ich", "dach\u00b7te", ":", "ist", "es", "ein", "rech\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So l\u00e4uft dir's frei in die Arme.", "tokens": ["So", "l\u00e4uft", "dir's", "frei", "in", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und sieh, wie am Abend ich sa\u00df zu Haus", "tokens": ["Und", "sieh", ",", "wie", "am", "A\u00b7bend", "ich", "sa\u00df", "zu", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPRART", "NN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und an nichts Fr\u00f6hliches dachte,", "tokens": ["Und", "an", "nichts", "Fr\u00f6h\u00b7li\u00b7ches", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADJA", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da pocht's, da stand's an der Schwelle drau\u00df", "tokens": ["Da", "pocht's", ",", "da", "stan\u00b7d's", "an", "der", "Schwel\u00b7le", "drau\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$,", "KOUS", "NE", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und flog mir ans Herz und lachte.", "tokens": ["Und", "flog", "mir", "ans", "Herz", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}