{"dta.poem.11022": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00f6licher Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! in diesem sch\u00f6nen Garten, kann ich mit Vergn\u00fcgen\ngehn,", "tokens": ["Herr", "!", "in", "die\u00b7sem", "sch\u00f6\u00b7nen", "Gar\u00b7ten", ",", "kann", "ich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PDAT", "ADJA", "NN", "$,", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und, durch meiner Augen Wunder, deiner Wunder Menge sehn,", "tokens": ["Und", ",", "durch", "mei\u00b7ner", "Au\u00b7gen", "Wun\u00b7der", ",", "dei\u00b7ner", "Wun\u00b7der", "Men\u00b7ge", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "NN", "$,", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00f6cht es doch, ohn ernsten Vorsatz deinen Namen zu erh\u00f6hn,", "tokens": ["M\u00f6cht", "es", "doch", ",", "ohn", "erns\u00b7ten", "Vor\u00b7satz", "dei\u00b7nen", "Na\u00b7men", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KOUI", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dich in deinem Werk zu preisen und zu lieben, nie geschehn!", "tokens": ["Dich", "in", "dei\u00b7nem", "Werk", "zu", "prei\u00b7sen", "und", "zu", "lie\u00b7ben", ",", "nie", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}