{"textgrid.poem.60157": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schnell, wie ein Sturmwind sich erhebt,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnell, wie ein Sturmwind sich erhebt,", "tokens": ["Schnell", ",", "wie", "ein", "Sturm\u00b7wind", "sich", "er\u00b7hebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie aus den Wolken gehn des Donnerschlages Bothen,", "tokens": ["Wie", "aus", "den", "Wol\u00b7ken", "gehn", "des", "Don\u00b7ner\u00b7schla\u00b7ges", "Bo\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So flog er hin, und schlug. Das Erzgeb\u00fcrge bebt,", "tokens": ["So", "flog", "er", "hin", ",", "und", "schlug", ".", "Das", "Erz\u00b7ge\u00b7b\u00fcr\u00b7ge", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Feind st\u00fcrzt \u00fcber seine Todten,", "tokens": ["Der", "Feind", "st\u00fcrzt", "\u00fc\u00b7ber", "sei\u00b7ne", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Flieht, wendet nicht die Stirn zur\u00fcck,", "tokens": ["Flieht", ",", "wen\u00b7det", "nicht", "die", "Stirn", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat auf der Flucht nicht mehr im Herzen Lust zu siegen:", "tokens": ["Hat", "auf", "der", "Flucht", "nicht", "mehr", "im", "Her\u00b7zen", "Lust", "zu", "sie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKNEG", "ADV", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Scham hat er auf der Stirn, und Furchtsamkeit im Blick,", "tokens": ["Scham", "hat", "er", "auf", "der", "Stirn", ",", "und", "Furcht\u00b7sam\u00b7keit", "im", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als w\u00e4r ein Gott herabgestiegen,", "tokens": ["Als", "w\u00e4r", "ein", "Gott", "her\u00b7ab\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und h\u00e4tt' aus seiner Hand den Strahl", "tokens": ["Und", "h\u00e4tt'", "aus", "sei\u00b7ner", "Hand", "den", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Zornes unter die gewaltig hingeschossen", "tokens": ["Des", "Zor\u00b7nes", "un\u00b7ter", "die", "ge\u00b7wal\u00b7tig", "hin\u00b7ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die oft sich z\u00e4hleten, und, stolz auf ihre Zahl,", "tokens": ["Die", "oft", "sich", "z\u00e4h\u00b7le\u00b7ten", ",", "und", ",", "stolz", "auf", "ih\u00b7re", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "$,", "KON", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des kleinen Haufen Tod beschlossen.", "tokens": ["Des", "klei\u00b7nen", "Hau\u00b7fen", "Tod", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Spott murmelt nach der Moldau Strom", "tokens": ["Spott", "mur\u00b7melt", "nach", "der", "Mol\u00b7dau", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem fortgeschlagnen Feind; Uns komts die Elbe sagen.", "tokens": ["Dem", "fort\u00b7ge\u00b7schlag\u00b7nen", "Feind", ";", "Uns", "komts", "die", "El\u00b7be", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir singen Heinrichs That. So sang das Volk zu Rom", "tokens": ["Wir", "sin\u00b7gen", "Hein\u00b7richs", "That", ".", "So", "sang", "das", "Volk", "zu", "Rom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des stolzen Feindes Niederlagen,", "tokens": ["Des", "stol\u00b7zen", "Fein\u00b7des", "Nie\u00b7der\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn es zuvor die G\u00f6tter bat,", "tokens": ["Wenn", "es", "zu\u00b7vor", "die", "G\u00f6t\u00b7ter", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Opfer zu verschm\u00e4hn von ungerechten H\u00e4nden", "tokens": ["Das", "Op\u00b7fer", "zu", "ver\u00b7schm\u00e4hn", "von", "un\u00b7ge\u00b7rech\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Jubellieder sang, wenn fl\u00fcchtig Mithridat", "tokens": ["Und", "Ju\u00b7bel\u00b7lie\u00b7der", "sang", ",", "wenn", "fl\u00fcch\u00b7tig", "Mith\u00b7ri\u00b7dat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "KOUS", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verga\u00df den Nakken umzuwenden.", "tokens": ["Ver\u00b7ga\u00df", "den", "Nak\u00b7ken", "um\u00b7zu\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}