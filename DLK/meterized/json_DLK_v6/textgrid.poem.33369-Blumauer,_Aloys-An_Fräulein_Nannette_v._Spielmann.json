{"textgrid.poem.33369": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An Fr\u00e4ulein Nannette v. Spielmann", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Liebensw\u00fcrdigsten der M\u00e4dchen", "tokens": ["Dem", "Lie\u00b7bens\u00b7w\u00fcr\u00b7digs\u00b7ten", "der", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Schickt dies Beh\u00e4ltni\u00df gold'ner F\u00e4dchen", "tokens": ["Schickt", "dies", "Be\u00b7h\u00e4lt\u00b7ni\u00df", "gold'\u00b7ner", "F\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Freund zum neuen Jahr;", "tokens": ["Ein", "Freund", "zum", "neu\u00b7en", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Allein, statt Gold hineinzulegen,", "tokens": ["Al\u00b7lein", ",", "statt", "Gold", "hin\u00b7ein\u00b7zu\u00b7le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bringt er darin, wie Dichter pflegen,", "tokens": ["Bringt", "er", "da\u00b7rin", ",", "wie", "Dich\u00b7ter", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur gute W\u00fcnsche dar.", "tokens": ["Nur", "gu\u00b7te", "W\u00fcn\u00b7sche", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er w\u00fcnscht, da\u00df, gleich den gold'nen F\u00e4den,", "tokens": ["Er", "w\u00fcnscht", ",", "da\u00df", ",", "gleich", "den", "gold'\u00b7nen", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An deiner Lebenstage jeden", "tokens": ["An", "dei\u00b7ner", "Le\u00b7bens\u00b7ta\u00b7ge", "je\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich Gl\u00fcck und Segen reih';", "tokens": ["Sich", "Gl\u00fcck", "und", "Se\u00b7gen", "reih'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und keiner deiner M\u00fchetage", "tokens": ["Und", "kei\u00b7ner", "dei\u00b7ner", "M\u00fc\u00b7he\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dir jemals eine gr\u00f6\u00df're Plage,", "tokens": ["Dir", "je\u00b7mals", "ei\u00b7ne", "gr\u00f6\u00df'\u00b7re", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als \u2013 Gold zu zupfen sei.", "tokens": ["Als", "\u2013", "Gold", "zu", "zup\u00b7fen", "sei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Leicht, wie das Gold sich l\u00f6st von Seide,", "tokens": ["Leicht", ",", "wie", "das", "Gold", "sich", "l\u00f6st", "von", "Sei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entwickle zu der Eltern Freude", "tokens": ["Ent\u00b7wick\u00b7le", "zu", "der", "El\u00b7tern", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich auch dein fr\u00fcher Geist,", "tokens": ["Sich", "auch", "dein", "fr\u00fc\u00b7her", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und zeige dann in jedem Falle", "tokens": ["Und", "zei\u00b7ge", "dann", "in", "je\u00b7dem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich gleich dem edelsten Metalle,", "tokens": ["Sich", "gleich", "dem", "e\u00b7dels\u00b7ten", "Me\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das gl\u00e4nzt und niemals glei\u00dft.", "tokens": ["Das", "gl\u00e4nzt", "und", "nie\u00b7mals", "glei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und so verbinde dann auf immer,", "tokens": ["Und", "so", "ver\u00b7bin\u00b7de", "dann", "auf", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O M\u00e4dchen, mit dem Jugendschimmer", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "mit", "dem", "Ju\u00b7gend\u00b7schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der \u00e4usseren Gestalt,", "tokens": ["Der", "\u00e4us\u00b7se\u00b7ren", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem Golde gleich, das vor dir lieget,", "tokens": ["Dem", "Gol\u00b7de", "gleich", ",", "das", "vor", "dir", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn man's pr\u00fcfet, gl\u00e4nzt und wieget,", "tokens": ["Und", "wenn", "man's", "pr\u00fc\u00b7fet", ",", "gl\u00e4nzt", "und", "wie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch inneren Gehalt.", "tokens": ["Auch", "in\u00b7ne\u00b7ren", "Ge\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}