{"textgrid.poem.35681": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Nachtgedanken eines J\u00fcnglings", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00dft euch endlich, Augenlieder!", "tokens": ["Schlie\u00dft", "euch", "end\u00b7lich", ",", "Au\u00b7gen\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voll Gedanken w\u00e4lz' ich mich,", "tokens": ["Voll", "Ge\u00b7dan\u00b7ken", "w\u00e4lz'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mond sinkt schon hernieder;", "tokens": ["Und", "der", "Mond", "sinkt", "schon", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "\u00d6stlich wieder", "tokens": ["\u00d6st\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zeigt die fr\u00fche R\u00f6te sich!", "tokens": ["Zeigt", "die", "fr\u00fc\u00b7he", "R\u00f6\u00b7te", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Still, du klopfend Herz! Sie wehrte", "tokens": ["Still", ",", "du", "klop\u00b7fend", "Herz", "!", "Sie", "wehr\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "PPER", "ADJD", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drohend ihren Mund mir ja! \u2013", "tokens": ["Dro\u00b7hend", "ih\u00b7ren", "Mund", "mir", "ja", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch als mich ihr Drohn nicht st\u00f6rte,", "tokens": ["Doch", "als", "mich", "ihr", "Drohn", "nicht", "st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, was lehrte", "tokens": ["Ach", ",", "was", "lehr\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mich ihr schamrot L\u00e4cheln da!", "tokens": ["Mich", "ihr", "scham\u00b7rot", "L\u00e4\u00b7cheln", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}