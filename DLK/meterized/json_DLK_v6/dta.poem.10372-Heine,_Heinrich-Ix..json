{"dta.poem.10372": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte weinen, doch ich kann es nicht;", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wei\u00b7nen", ",", "doch", "ich", "kann", "es", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KON", "PPER", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich m\u00f6cht' mich r\u00fcstig in die H\u00f6he heben,", "tokens": ["Ich", "m\u00f6cht'", "mich", "r\u00fcs\u00b7tig", "in", "die", "H\u00f6\u00b7he", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch kann ich's nicht; am Boden mu\u00df ich kleben,", "tokens": ["Doch", "kann", "ich's", "nicht", ";", "am", "Bo\u00b7den", "mu\u00df", "ich", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PTKNEG", "$.", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umkr\u00e4chzt, umzischt von ekelm Wurmgez\u00fccht.", "tokens": ["Um\u00b7kr\u00e4chzt", ",", "um\u00b7zischt", "von", "e\u00b7kelm", "Wurm\u00b7ge\u00b7z\u00fccht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich m\u00f6chte gern mein heitres Lebenslicht,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "gern", "mein", "heit\u00b7res", "Le\u00b7bens\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein sch\u00f6nes Lieb, all\u00fcberall umschweben,", "tokens": ["Mein", "sch\u00f6\u00b7nes", "Lieb", ",", "al\u00b7l\u00fc\u00b7be\u00b7rall", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In ihrem selig s\u00fc\u00dfen Hauche leben, \u2014", "tokens": ["In", "ih\u00b7rem", "se\u00b7lig", "s\u00fc\u00b7\u00dfen", "Hau\u00b7che", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch kann ich's nicht, mein krankes Herze bricht.", "tokens": ["Doch", "kann", "ich's", "nicht", ",", "mein", "kran\u00b7kes", "Her\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aus dem gebrochnen Herzen f\u00fchl' ich flie\u00dfen", "tokens": ["Aus", "dem", "ge\u00b7broch\u00b7nen", "Her\u00b7zen", "f\u00fchl'", "ich", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mein hei\u00dfes Blut, ich f\u00fchle mich ermatten,", "tokens": ["Mein", "hei\u00b7\u00dfes", "Blut", ",", "ich", "f\u00fch\u00b7le", "mich", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und vor den Augen wird's mir tr\u00fcb und tr\u00fcber.", "tokens": ["Und", "vor", "den", "Au\u00b7gen", "wird's", "mir", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und heimlich schauernd sehn' ich mich hin\u00fcber", "tokens": ["Und", "heim\u00b7lich", "schau\u00b7ernd", "sehn'", "ich", "mich", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nach jenem Nebelreich, wo stille Schatten", "tokens": ["Nach", "je\u00b7nem", "Ne\u00b7bel\u00b7reich", ",", "wo", "stil\u00b7le", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mit weichen Armen liebend mich umschlie\u00dfen.", "tokens": ["Mit", "wei\u00b7chen", "Ar\u00b7men", "lie\u00b7bend", "mich", "um\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}