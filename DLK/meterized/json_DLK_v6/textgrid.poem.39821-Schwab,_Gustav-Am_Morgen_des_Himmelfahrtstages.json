{"textgrid.poem.39821": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Am Morgen des Himmelfahrtstages", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich nicht den Fr\u00fchling t\u00e4uschen,", "tokens": ["La\u00df", "dich", "nicht", "den", "Fr\u00fch\u00b7ling", "t\u00e4u\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, der dich mit Lust umringt,", "tokens": ["Herz", ",", "der", "dich", "mit", "Lust", "um\u00b7ringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mit wonnigen Ger\u00e4uschen", "tokens": ["Wo", "mit", "won\u00b7ni\u00b7gen", "Ge\u00b7r\u00e4u\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wald und Flur von Leben klingt;", "tokens": ["Wald", "und", "Flur", "von", "Le\u00b7ben", "klingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sich auf den Aesten wiegen", "tokens": ["Wo", "sich", "auf", "den", "A\u00b7es\u00b7ten", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Kehlen, voll von ew'gem Klang,", "tokens": ["Keh\u00b7len", ",", "voll", "von", "ew'\u00b7gem", "Klang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo, als g\u00e4b' es kein Versiegen,", "tokens": ["Wo", ",", "als", "g\u00e4b'", "es", "kein", "Ver\u00b7sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fl\u00fcsse brausen ihren Gang.", "tokens": ["Fl\u00fcs\u00b7se", "brau\u00b7sen", "ih\u00b7ren", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von den B\u00e4umen, aus den B\u00e4chen,", "tokens": ["Von", "den", "B\u00e4u\u00b7men", ",", "aus", "den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem hellen Morgenrot", "tokens": ["Aus", "dem", "hel\u00b7len", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint ein tr\u00f6stlich Wort zu sprechen \u2013", "tokens": ["Scheint", "ein", "tr\u00f6st\u00b7lich", "Wort", "zu", "spre\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschest du, so ist's der Tod.", "tokens": ["Lau\u00b7schest", "du", ",", "so", "ist's", "der", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese Welt, sie mu\u00df vergehen;", "tokens": ["Die\u00b7se", "Welt", ",", "sie", "mu\u00df", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fcher noch der L\u00fcfte Raub", "tokens": ["Fr\u00fc\u00b7her", "noch", "der", "L\u00fcf\u00b7te", "Raub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wirst als Asche du verwehen,", "tokens": ["Wirst", "als", "A\u00b7sche", "du", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Herz, wie fl\u00fccht'ger Blumenstaub.", "tokens": ["Herz", ",", "wie", "fl\u00fccht'\u00b7ger", "Blu\u00b7men\u00b7staub", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst du bis zum Wesen dringen,", "tokens": ["Willst", "du", "bis", "zum", "We\u00b7sen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wende vom Erschaffnen dich,", "tokens": ["Wen\u00b7de", "vom", "Er\u00b7schaff\u00b7nen", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du dich in's Leben schwingen \u2013", "tokens": ["Willst", "du", "dich", "in's", "Le\u00b7ben", "schwin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer zeigt als F\u00fchrer sich:", "tokens": ["Ei\u00b7ner", "zeigt", "als", "F\u00fch\u00b7rer", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der an solchem Fr\u00fchlingsmorgen", "tokens": ["Der", "an", "sol\u00b7chem", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinter sich lie\u00df die Natur,", "tokens": ["Hin\u00b7ter", "sich", "lie\u00df", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und, dem ird'schen Blick verborgen,", "tokens": ["Und", ",", "dem", "ird'\u00b7schen", "Blick", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Himmel Himmel fuhr.", "tokens": ["In", "der", "Him\u00b7mel", "Him\u00b7mel", "fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was die J\u00fcnger dort empfanden,", "tokens": ["Was", "die", "J\u00fcn\u00b7ger", "dort", "emp\u00b7fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ihr Auge flog empor,", "tokens": ["Als", "ihr", "Au\u00b7ge", "flog", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchl' es, Herz, und aus den Banden", "tokens": ["F\u00fchl'", "es", ",", "Herz", ",", "und", "aus", "den", "Ban\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcchte durch des Glaubens Thor.", "tokens": ["Fl\u00fcch\u00b7te", "durch", "des", "Glau\u00b7bens", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den Ewigkeits-Gedanken", "tokens": ["Mit", "den", "E\u00b7wig\u00b7keits\u00b7Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bist du doch von Erde nur,", "tokens": ["Bist", "du", "doch", "von", "Er\u00b7de", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchrt nicht Er dich aus den Schranken", "tokens": ["F\u00fchrt", "nicht", "Er", "dich", "aus", "den", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Ueber alle Kreatur.", "tokens": ["Ue\u00b7ber", "al\u00b7le", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was auf Erden ihn umgeben,", "tokens": ["Was", "auf", "Er\u00b7den", "ihn", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War ihm Bild und Ahnung blos,", "tokens": ["War", "ihm", "Bild", "und", "Ah\u00b7nung", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er athmete sein Leben", "tokens": ["Und", "er", "ath\u00b7me\u00b7te", "sein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Stets nur in des Vaters Schoos.", "tokens": ["Stets", "nur", "in", "des", "Va\u00b7ters", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh auch du im Glanz der Erde", "tokens": ["Sieh", "auch", "du", "im", "Glanz", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur vom Himmel einen Traum;", "tokens": ["Nur", "vom", "Him\u00b7mel", "ei\u00b7nen", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gleichni\u00df dir des H\u00f6chsten werde", "tokens": ["Gleich\u00b7ni\u00df", "dir", "des", "H\u00f6chs\u00b7ten", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heerde, Haus und Blum' und Baum.", "tokens": ["Heer\u00b7de", ",", "Haus", "und", "Blum'", "und", "Baum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn auf's Leben du verzichtet,", "tokens": ["Wenn", "auf's", "Le\u00b7ben", "du", "ver\u00b7zich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann beginnt dein Lebenslauf;", "tokens": ["Dann", "be\u00b7ginnt", "dein", "Le\u00b7bens\u00b7lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du dich als Staub vernichtet,", "tokens": ["Wenn", "du", "dich", "als", "Staub", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehst du erst als Wesen auf.", "tokens": ["Stehst", "du", "erst", "als", "We\u00b7sen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines innern Lebens Schwingen", "tokens": ["Dei\u00b7nes", "in\u00b7nern", "Le\u00b7bens", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wachsen aus dem Erdentod;", "tokens": ["Wach\u00b7sen", "aus", "dem", "Er\u00b7den\u00b7tod", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eh' er konnt' ins Leben dringen,", "tokens": ["Eh'", "er", "konnt'", "ins", "Le\u00b7ben", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat auch Ihm das Grab gedroht.", "tokens": ["Hat", "auch", "Ihm", "das", "Grab", "ge\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Blick' hinauf zum Himmelsbogen,", "tokens": ["Blick'", "hin\u00b7auf", "zum", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehest du den Wiederschein", "tokens": ["Sie\u00b7hest", "du", "den", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Bahn, die er geflogen?", "tokens": ["Von", "der", "Bahn", ",", "die", "er", "ge\u00b7flo\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4dt dich nicht ein Schimmer ein?", "tokens": ["L\u00e4dt", "dich", "nicht", "ein", "Schim\u00b7mer", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will das Himmelslicht ermatten?", "tokens": ["Will", "das", "Him\u00b7mels\u00b7licht", "er\u00b7mat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ringen Zweifel um den Sieg?", "tokens": ["Rin\u00b7gen", "Zwei\u00b7fel", "um", "den", "Sieg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es ist nur der Wolke Schatten,", "tokens": ["Es", "ist", "nur", "der", "Wol\u00b7ke", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Hinter der er aufw\u00e4rts stieg.", "tokens": ["Hin\u00b7ter", "der", "er", "auf\u00b7w\u00e4rts", "stieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}