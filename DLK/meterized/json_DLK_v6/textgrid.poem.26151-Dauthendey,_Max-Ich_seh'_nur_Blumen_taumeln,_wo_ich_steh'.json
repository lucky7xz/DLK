{"textgrid.poem.26151": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ich seh' nur Blumen taumeln, wo ich steh'", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Flieder streut sich auf die Erde blau,", "tokens": ["Der", "Flie\u00b7der", "streut", "sich", "auf", "die", "Er\u00b7de", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wei\u00dfdorn sch\u00fcttet seinen warmen Schnee,", "tokens": ["Der", "Wei\u00df\u00b7dorn", "sch\u00fct\u00b7tet", "sei\u00b7nen", "war\u00b7men", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Ahornbl\u00fcte regnet \u00fcber Weg und Au,", "tokens": ["Die", "A\u00b7horn\u00b7bl\u00fc\u00b7te", "reg\u00b7net", "\u00fc\u00b7ber", "Weg", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich seh' nur Blumen taumeln, wo ich steh':", "tokens": ["Ich", "seh'", "nur", "Blu\u00b7men", "tau\u00b7meln", ",", "wo", "ich", "steh'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schneeballen, welche keinen schmerzen,", "tokens": ["Schnee\u00b7bal\u00b7len", ",", "wel\u00b7che", "kei\u00b7nen", "schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Goldregenbaum, dem helle Ketten fallen,", "tokens": ["Gold\u00b7re\u00b7gen\u00b7baum", ",", "dem", "hel\u00b7le", "Ket\u00b7ten", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und feuerblaue Iris hingestellt zum Gartensee.", "tokens": ["Und", "feu\u00b7er\u00b7blau\u00b7e", "I\u00b7ris", "hin\u00b7ge\u00b7stellt", "zum", "Gar\u00b7ten\u00b7see", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Doch ohne dich, Geliebte, ich an allen", "tokens": ["Doch", "oh\u00b7ne", "dich", ",", "Ge\u00b7lieb\u00b7te", ",", "ich", "an", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "$,", "NN", "$,", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Stumm wie ein Winterstumpf vor\u00fcbergeh'.", "tokens": ["Stumm", "wie", "ein", "Win\u00b7ter\u00b7stumpf", "vor\u00b7\u00fcber\u00b7geh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}