{"textgrid.poem.41663": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Landschaft.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich zart und rein zu weihn der Dichtung der Eklogen,", "tokens": ["Mich", "zart", "und", "rein", "zu", "weihn", "der", "Dich\u00b7tung", "der", "Ek\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will ich dem Himmel nah ruhn wie die Astrologen,", "tokens": ["Will", "ich", "dem", "Him\u00b7mel", "nah", "ruhn", "wie", "die", "Ast\u00b7ro\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Glocken nah, da\u00df mir im Traum, der mich umspinnt,", "tokens": ["Den", "Glo\u00b7cken", "nah", ",", "da\u00df", "mir", "im", "Traum", ",", "der", "mich", "um\u00b7spinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Feiersang ert\u00f6nt, dahingeweht vom Wind.", "tokens": ["Ihr", "Fei\u00b7er\u00b7sang", "er\u00b7t\u00f6nt", ",", "da\u00b7hin\u00b7ge\u00b7weht", "vom", "Wind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann aufgest\u00fctzten Haupts ich aus der Kammer sp\u00e4he,", "tokens": ["Wann", "auf\u00b7ge\u00b7st\u00fctz\u00b7ten", "Haupts", "ich", "aus", "der", "Kam\u00b7mer", "sp\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schau ich der Werkstatt Flei\u00df und L\u00e4rm in meiner N\u00e4he,", "tokens": ["Schau", "ich", "der", "Werk\u00b7statt", "Flei\u00df", "und", "L\u00e4rm", "in", "mei\u00b7ner", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Rauchf\u00e4nge, T\u00fcrme rings, wie Masten, und dann weit", "tokens": ["Rauch\u00b7f\u00e4n\u00b7ge", ",", "T\u00fcr\u00b7me", "rings", ",", "wie", "Mas\u00b7ten", ",", "und", "dann", "weit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ADV", "$,", "PWAV", "NN", "$,", "KON", "ADV", "ADJD"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Den Himmel, der uns gie\u00dft den Traum der Ewigkeit.", "tokens": ["Den", "Him\u00b7mel", ",", "der", "uns", "gie\u00dft", "den", "Traum", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie s\u00fc\u00df ist es zu schaun, vom Nebelflor umfeuchtet,", "tokens": ["Wie", "s\u00fc\u00df", "ist", "es", "zu", "schaun", ",", "vom", "Ne\u00b7bel\u00b7flor", "um\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann im Azur der Stern, das Licht am Fenster leuchtet,", "tokens": ["Wann", "im", "A\u00b7zur", "der", "Stern", ",", "das", "Licht", "am", "Fens\u00b7ter", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wie auf zum Firmament der Strom des Rauches flie\u00dft", "tokens": ["Wie", "auf", "zum", "Fir\u00b7ma\u00b7ment", "der", "Strom", "des", "Rau\u00b7ches", "flie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie das Mondlicht bleich Verz\u00fcckung niedergie\u00dft.", "tokens": ["Und", "wie", "das", "Mond\u00b7licht", "bleich", "Ver\u00b7z\u00fc\u00b7ckung", "nie\u00b7der\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich werde Fr\u00fchlingszeit und Herbst und Sommer sehen,", "tokens": ["Ich", "wer\u00b7de", "Fr\u00fch\u00b7lings\u00b7zeit", "und", "Herbst", "und", "Som\u00b7mer", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wann der Winter kommt mit Schnee und eisgem Wehen,", "tokens": ["Und", "wann", "der", "Win\u00b7ter", "kommt", "mit", "Schnee", "und", "eis\u00b7gem", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So schlie\u00dfe Laden ich und T\u00fcr, um in der Nacht", "tokens": ["So", "schlie\u00b7\u00dfe", "La\u00b7den", "ich", "und", "T\u00fcr", ",", "um", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "KON", "NN", "$,", "KOUI", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu bauen herrlich der Pal\u00e4ste Feeenpracht.", "tokens": ["Zu", "bau\u00b7en", "herr\u00b7lich", "der", "Pa\u00b7l\u00e4s\u00b7te", "Fe\u00b7e\u00b7en\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Dann werde tr\u00e4umen ich von blauer Fluren Sehnen,", "tokens": ["Dann", "wer\u00b7de", "tr\u00e4u\u00b7men", "ich", "von", "blau\u00b7er", "Flu\u00b7ren", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von G\u00e4rten, Marmorglanz, von weinenden Font\u00e4nen,", "tokens": ["Von", "G\u00e4r\u00b7ten", ",", "Mar\u00b7mor\u00b7glanz", ",", "von", "wei\u00b7nen\u00b7den", "Fon\u00b7t\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von K\u00fcssen, V\u00f6geln, die uns singen fr\u00fch und sp\u00e4t,", "tokens": ["Von", "K\u00fcs\u00b7sen", ",", "V\u00f6\u00b7geln", ",", "die", "uns", "sin\u00b7gen", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von all der Kindlichkeit, die durch Idylle weht.", "tokens": ["Von", "all", "der", "Kind\u00b7lich\u00b7keit", ",", "die", "durch", "I\u00b7dyl\u00b7le", "weht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Aufruhr l\u00e4\u00dft umsonst im Sturm die Scheiben beben,", "tokens": ["Der", "Auf\u00b7ruhr", "l\u00e4\u00dft", "um\u00b7sonst", "im", "Sturm", "die", "Schei\u00b7ben", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich werde nimmermehr vom Pult mein Haupt erheben,", "tokens": ["Ich", "wer\u00b7de", "nim\u00b7mer\u00b7mehr", "vom", "Pult", "mein", "Haupt", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil dann die Freude ganz mein Herz umfangen h\u00e4lt,", "tokens": ["Weil", "dann", "die", "Freu\u00b7de", "ganz", "mein", "Herz", "um\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zu schaffen eigner Kraft des Fr\u00fchlings junge Welt,", "tokens": ["Zu", "schaf\u00b7fen", "eig\u00b7ner", "Kraft", "des", "Fr\u00fch\u00b7lings", "jun\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Sonne aus der Brust zu ziehn und herzul\u00e4cheln", "tokens": ["Die", "Son\u00b7ne", "aus", "der", "Brust", "zu", "ziehn", "und", "her\u00b7zu\u00b7l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Aus der Gef\u00fchle Gl\u00fchn der milden L\u00fcfte F\u00e4cheln.", "tokens": ["Aus", "der", "Ge\u00b7f\u00fch\u00b7le", "Gl\u00fchn", "der", "mil\u00b7den", "L\u00fcf\u00b7te", "F\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}