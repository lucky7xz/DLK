{"dta.poem.3366": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Miltons Hymne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses sind deine herrlichen Werke, Vater", "tokens": ["Die\u00b7ses", "sind", "dei\u00b7ne", "herr\u00b7li\u00b7chen", "Wer\u00b7ke", ",", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "des Guten!", "tokens": ["des", "Gu\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Allgewaltiger, dein ist dieser strahlende Weltbau,", "tokens": ["All\u00b7ge\u00b7wal\u00b7ti\u00b7ger", ",", "dein", "ist", "die\u00b7ser", "strah\u00b7len\u00b7de", "Welt\u00b7bau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wie so sch\u00f6n geschaffen von dir, o Wunder-", "tokens": ["Wie", "so", "sch\u00f6n", "ge\u00b7schaf\u00b7fen", "von", "dir", ",", "o", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VVPP", "APPR", "PPER", "$,", "FM", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "sch\u00f6ner!", "tokens": ["sch\u00f6\u00b7ner", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Unaussprechlicher, zwar du wohnst hoch \u00fcber", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", ",", "zwar", "du", "wohnst", "hoch", "\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PPER", "ADV", "ADJD", "APPR"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "den Himmeln!", "tokens": ["den", "Him\u00b7meln", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Unsre Augen erreichen dich nicht. Dort d\u00e4mmert", "tokens": ["Uns\u00b7re", "Au\u00b7gen", "er\u00b7rei\u00b7chen", "dich", "nicht", ".", "Dort", "d\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "VVFIN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "der Abglanz", "tokens": ["der", "Ab\u00b7glanz"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Deiner Herrlichkeit uns in deinen niedrigsten Wer-", "tokens": ["Dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "uns", "in", "dei\u00b7nen", "nied\u00b7rigs\u00b7ten", "Wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "ken \u2014", "tokens": ["ken"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Niedrig, doch preisen die niedrigen auch mit tau-", "tokens": ["Nied\u00b7rig", ",", "doch", "prei\u00b7sen", "die", "nied\u00b7ri\u00b7gen", "auch", "mit", "tau"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "ADV", "APPR", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "send Stimmen", "tokens": ["send", "Stim\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Deine unendliche Kraft und unergr\u00fcndliche Milde.", "tokens": ["Dei\u00b7ne", "un\u00b7end\u00b7li\u00b7che", "Kraft", "und", "un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7che", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}}}}}