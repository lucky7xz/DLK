{"textgrid.poem.60143": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freundin dessen, der die Welt regieret,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundin dessen, der die Welt regieret,", "tokens": ["Freun\u00b7din", "des\u00b7sen", ",", "der", "die", "Welt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der an diamantnen Ketten f\u00fchret", "tokens": ["Der", "an", "di\u00b7a\u00b7mant\u00b7nen", "Ket\u00b7ten", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jene Sonnen \u00fcber unserm Haupt!", "tokens": ["Je\u00b7ne", "Son\u00b7nen", "\u00fc\u00b7ber", "un\u00b7serm", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sieh'! an seiner Ordnung goldnen Seilen", "tokens": ["Sieh'", "!", "an", "sei\u00b7ner", "Ord\u00b7nung", "gold\u00b7nen", "Sei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mu\u00df der Fr\u00fchling neu herunter eilen", "tokens": ["Mu\u00df", "der", "Fr\u00fch\u00b7ling", "neu", "her\u00b7un\u00b7ter", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJD", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mit dem Schmuck, den ihm der Herbst geraubt.", "tokens": ["Mit", "dem", "Schmuck", ",", "den", "ihm", "der", "Herbst", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Siehe! wie befl\u00fcgelt er gekommen", "tokens": ["Sie\u00b7he", "!", "wie", "be\u00b7fl\u00fc\u00b7gelt", "er", "ge\u00b7kom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PWAV", "VVFIN", "PPER", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die Trauer der Natur benommen.", "tokens": ["Und", "die", "Trau\u00b7er", "der", "Na\u00b7tur", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie er sie schon jugendlich geschm\u00fcckt,", "tokens": ["Wie", "er", "sie", "schon", "ju\u00b7gend\u00b7lich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00e4dchen, die den Lenz im Antlitz haben,", "tokens": ["M\u00e4d\u00b7chen", ",", "die", "den", "Lenz", "im", "Ant\u00b7litz", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "M\u00e4nner, J\u00fcnglinge und kleine Knaben", "tokens": ["M\u00e4n\u00b7ner", ",", "J\u00fcng\u00b7lin\u00b7ge", "und", "klei\u00b7ne", "Kna\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und der Grei\u00df, der sich am Stabe b\u00fcckt;", "tokens": ["Und", "der", "Grei\u00df", ",", "der", "sich", "am", "Sta\u00b7be", "b\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Alles geht, gereizt von den Ger\u00fcchen", "tokens": ["Al\u00b7les", "geht", ",", "ge\u00b7reizt", "von", "den", "Ge\u00b7r\u00fc\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Junger Veilchen, die so niedrig kriechen", "tokens": ["Jun\u00b7ger", "Veil\u00b7chen", ",", "die", "so", "nied\u00b7rig", "krie\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und doch edler, als die Tulpen sind!", "tokens": ["Und", "doch", "ed\u00b7ler", ",", "als", "die", "Tul\u00b7pen", "sind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "$,", "KOUS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Hyacinthen ofne Glocken", "tokens": ["Und", "der", "Hya\u00b7cin\u00b7then", "of\u00b7ne", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Duften Balsam, den um seine Locken", "tokens": ["Duf\u00b7ten", "Bal\u00b7sam", ",", "den", "um", "sei\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dir entgegen tr\u00e4gt der Fr\u00fchlingswind.", "tokens": ["Dir", "ent\u00b7ge\u00b7gen", "tr\u00e4gt", "der", "Fr\u00fch\u00b7lings\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Blat und Frucht, die in der Knospe lagen,", "tokens": ["Blat", "und", "Frucht", ",", "die", "in", "der", "Knos\u00b7pe", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dringen sich des Sch\u00f6pfers Lob zu sagen,", "tokens": ["Drin\u00b7gen", "sich", "des", "Sch\u00f6p\u00b7fers", "Lob", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus der H\u00fclle nun mit Macht hervor.", "tokens": ["Aus", "der", "H\u00fcl\u00b7le", "nun", "mit", "Macht", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn die stummen Redner pr\u00e4chtig bl\u00fchen,", "tokens": ["Wenn", "die", "stum\u00b7men", "Red\u00b7ner", "pr\u00e4ch\u00b7tig", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Steigt, in regellosen Symphonien,", "tokens": ["Steigt", ",", "in", "re\u00b7gel\u00b7lo\u00b7sen", "Sym\u00b7pho\u00b7ni\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aus den Zweigen ein Gesang empor!", "tokens": ["Aus", "den", "Zwei\u00b7gen", "ein", "Ge\u00b7sang", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ohne Muse, ohne Kunst und Schriften", "tokens": ["Oh\u00b7ne", "Mu\u00b7se", ",", "oh\u00b7ne", "Kunst", "und", "Schrif\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Singt die Lerche, schwebend in den L\u00fcften,", "tokens": ["Singt", "die", "Ler\u00b7che", ",", "schwe\u00b7bend", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unaufh\u00f6rlich ihr pindarisch Lied!", "tokens": ["Un\u00b7auf\u00b7h\u00f6r\u00b7lich", "ihr", "pin\u00b7da\u00b7risch", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Unter ihr, in fr\u00fcher Tagesstunde,", "tokens": ["Un\u00b7ter", "ihr", ",", "in", "fr\u00fc\u00b7her", "Ta\u00b7gess\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Singt mit b\u00e4urisch vollgenommnem Munde", "tokens": ["Singt", "mit", "b\u00e4u\u00b7risch", "voll\u00b7ge\u00b7nomm\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auch die Einfalt, welche Furchen zieht!", "tokens": ["Auch", "die", "Ein\u00b7falt", ",", "wel\u00b7che", "Fur\u00b7chen", "zieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "L\u00e4mmer, die noch an den M\u00fcttern saugen,", "tokens": ["L\u00e4m\u00b7mer", ",", "die", "noch", "an", "den", "M\u00fct\u00b7tern", "sau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bl\u00f6ken dem zum Lobe, dessen Augen", "tokens": ["Bl\u00f6\u00b7ken", "dem", "zum", "Lo\u00b7be", ",", "des\u00b7sen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "APPRART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das Insekt im Staube kriechen sehn.", "tokens": ["Das", "In\u00b7sekt", "im", "Stau\u00b7be", "krie\u00b7chen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Ihn mu\u00df so der Wurm im Grase preisen,", "tokens": ["Ihn", "mu\u00df", "so", "der", "Wurm", "im", "Gra\u00b7se", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als das Herz mit ihm bekannter Weisen,", "tokens": ["Als", "das", "Herz", "mit", "ihm", "be\u00b7kann\u00b7ter", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Als die R\u00e4der, die den Weltbau drehn.", "tokens": ["Als", "die", "R\u00e4\u00b7der", ",", "die", "den", "Welt\u00b7bau", "drehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "O du Tochter seiner Lieb und G\u00fcte,", "tokens": ["O", "du", "Toch\u00b7ter", "sei\u00b7ner", "Lieb", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der in jedem Lenz die junge Bl\u00fcthe,", "tokens": ["Der", "in", "je\u00b7dem", "Lenz", "die", "jun\u00b7ge", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die gr\u00fcne Saat sein Lob beschreibt.", "tokens": ["Und", "die", "gr\u00fc\u00b7ne", "Saat", "sein", "Lob", "be\u00b7schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6her, als der Dichtgeist in dem Fluge", "tokens": ["H\u00f6\u00b7her", ",", "als", "der", "Dicht\u00b7geist", "in", "dem", "Flu\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Preisest du mit jedem Athemzuge", "tokens": ["Prei\u00b7sest", "du", "mit", "je\u00b7dem", "A\u00b7them\u00b7zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Einen Gott, der deine Freude bleibt!", "tokens": ["Ei\u00b7nen", "Gott", ",", "der", "dei\u00b7ne", "Freu\u00b7de", "bleibt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Alles singt ihm. \u2013 Seine Nachtigallen", "tokens": ["Al\u00b7les", "singt", "ihm", ".", "\u2013", "Sei\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$.", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oft behorchend, will ich Lieder lallen", "tokens": ["Oft", "be\u00b7hor\u00b7chend", ",", "will", "ich", "Lie\u00b7der", "lal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "VMFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Voll vom Lobe dessen, der mich schuf;", "tokens": ["Voll", "vom", "Lo\u00b7be", "des\u00b7sen", ",", "der", "mich", "schuf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bienen, die auf Lindenwipfeln summen,", "tokens": ["Bie\u00b7nen", ",", "die", "auf", "Lin\u00b7den\u00b7wip\u00b7feln", "sum\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und des Fleisses Lehrer, jene Stummen", "tokens": ["Und", "des", "Fleis\u00b7ses", "Leh\u00b7rer", ",", "je\u00b7ne", "Stum\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Im Erdhaufen, werden mir ein Ruf!", "tokens": ["Im", "Erd\u00b7hau\u00b7fen", ",", "wer\u00b7den", "mir", "ein", "Ruf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}}}}}