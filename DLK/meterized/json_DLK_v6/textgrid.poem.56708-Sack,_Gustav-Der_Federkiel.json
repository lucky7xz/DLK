{"textgrid.poem.56708": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Federkiel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich einem umgest\u00fclpten Glas,", "tokens": ["Gleich", "ei\u00b7nem", "um\u00b7ge\u00b7st\u00fclp\u00b7ten", "Glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das von der Lerchen Liederschlag", "tokens": ["das", "von", "der", "Ler\u00b7chen", "Lie\u00b7der\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in endlosem Bibrieren t\u00f6nt,", "tokens": ["in", "end\u00b7lo\u00b7sem", "Bib\u00b7rie\u00b7ren", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "h\u00e4ngt nun der Himmel jeden Tag", "tokens": ["h\u00e4ngt", "nun", "der", "Him\u00b7mel", "je\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00fcber dem strotzend fetten Gras.", "tokens": ["\u00fc\u00b7ber", "dem", "strot\u00b7zend", "fet\u00b7ten", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Doch mir ward diese blaue Welt", "tokens": ["Doch", "mir", "ward", "die\u00b7se", "blau\u00b7e", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "feindlich zu einem Satyrspiel,", "tokens": ["feind\u00b7lich", "zu", "ei\u00b7nem", "Sa\u00b7tyr\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "das meiner N\u00f6te l\u00e4chelnd h\u00f6hnt", "tokens": ["das", "mei\u00b7ner", "N\u00f6\u00b7te", "l\u00e4\u00b7chelnd", "h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und mich gleich einem Federkiel", "tokens": ["und", "mich", "gleich", "ei\u00b7nem", "Fe\u00b7der\u00b7kiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "in seinen losen H\u00e4nden h\u00e4lt.", "tokens": ["in", "sei\u00b7nen", "lo\u00b7sen", "H\u00e4n\u00b7den", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}