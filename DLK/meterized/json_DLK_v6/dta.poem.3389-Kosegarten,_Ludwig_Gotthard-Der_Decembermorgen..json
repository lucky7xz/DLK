{"dta.poem.3389": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Decembermorgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du wunderherrliche Natur,", "tokens": ["Du", "wun\u00b7der\u00b7herr\u00b7li\u00b7che", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl mir, dass ich dir Treue schwur!", "tokens": ["Wohl", "mir", ",", "dass", "ich", "dir", "Treu\u00b7e", "schwur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl mir, dass du, so lieb und traut,", "tokens": ["Wohl", "mir", ",", "dass", "du", ",", "so", "lieb", "und", "traut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich liebst, als Schwester und als Braut!", "tokens": ["Mich", "liebst", ",", "als", "Schwes\u00b7ter", "und", "als", "Braut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "KON", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist du doch immer wunderbar,", "tokens": ["Bist", "du", "doch", "im\u00b7mer", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer herzlich, treu und wahr,", "tokens": ["Und", "im\u00b7mer", "herz\u00b7lich", ",", "treu", "und", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer gross und sch\u00f6n und hold", "tokens": ["Und", "im\u00b7mer", "gross", "und", "sch\u00f6n", "und", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Morgen- und im Abendgold.", "tokens": ["Im", "Mor\u00b7gen", "und", "im", "A\u00b7bend\u00b7gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bist in der Fr\u00fchlingswinde Wehn", "tokens": ["Bist", "in", "der", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", "Wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Garten Gottes lieb und sch\u00f6n;", "tokens": ["Ein", "Gar\u00b7ten", "Got\u00b7tes", "lieb", "und", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wirft der Herbst dein Laub herab,", "tokens": ["Und", "wirft", "der", "Herbst", "dein", "Laub", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hehr, wie eines Heilgen Grab.", "tokens": ["So", "hehr", ",", "wie", "ei\u00b7nes", "Heil\u00b7gen", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und in des Winters ernster Pracht,", "tokens": ["Und", "in", "des", "Win\u00b7ters", "erns\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seines Schneees Leichentracht,", "tokens": ["In", "sei\u00b7nes", "Schne\u00b7e\u00b7es", "Lei\u00b7chen\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gemahnst du mich, o Herrscherin,", "tokens": ["Ge\u00b7mahnst", "du", "mich", ",", "o", "Herr\u00b7sche\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine Wittwe-K\u00f6nigin.", "tokens": ["Wie", "ei\u00b7ne", "Witt\u00b7we\u00b7K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gegr\u00fcsst, gegr\u00fcsst sey mir auch heut", "tokens": ["Ge\u00b7gr\u00fcs\u00b7st", ",", "ge\u00b7gr\u00fcs\u00b7st", "sey", "mir", "auch", "heut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVPP", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In deiner milden Herrlichkeit,", "tokens": ["In", "dei\u00b7ner", "mil\u00b7den", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im ersten jungfr\u00e4ulichen Schnee,", "tokens": ["Im", "ers\u00b7ten", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der sanft dich h\u00fcllet, Traurende.", "tokens": ["Der", "sanft", "dich", "h\u00fcl\u00b7let", ",", "Trau\u00b7ren\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Wie frisch, wie rasch, wie freudig wehn", "tokens": ["Wie", "frisch", ",", "wie", "rasch", ",", "wie", "freu\u00b7dig", "wehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Winterd\u00fcfte! Ach, wie stehn", "tokens": ["Die", "Win\u00b7ter\u00b7d\u00fcf\u00b7te", "!", "Ach", ",", "wie", "stehn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ITJ", "$,", "PWAV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nackten B\u00e4ume zart und klar,", "tokens": ["Die", "nack\u00b7ten", "B\u00e4u\u00b7me", "zart", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich Greisen mit bereiftem Haar!", "tokens": ["Gleich", "Grei\u00b7sen", "mit", "be\u00b7reif\u00b7tem", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Aurorens Rosenkelch erbl\u00fcht,", "tokens": ["Au\u00b7ro\u00b7rens", "Ro\u00b7sen\u00b7kelch", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die besch\u00e4mte Luna flieht.", "tokens": ["Und", "die", "be\u00b7sch\u00e4m\u00b7te", "Lu\u00b7na", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So raubet ", "tokens": ["So", "rau\u00b7bet"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Geringrer Sch\u00f6nheit Preis und Kranz.", "tokens": ["Ge\u00b7ring\u00b7rer", "Sch\u00f6n\u00b7heit", "Preis", "und", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Den lasurblauen Himmel kr\u00e4nzt", "tokens": ["Den", "la\u00b7sur\u00b7blau\u00b7en", "Him\u00b7mel", "kr\u00e4nzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aurorens Rosensaum. Wie gl\u00e4nzt,", "tokens": ["Au\u00b7ro\u00b7rens", "Ro\u00b7sen\u00b7saum", ".", "Wie", "gl\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie funkelt die beschneite Flur \u2014", "tokens": ["Wie", "fun\u00b7kelt", "die", "be\u00b7schnei\u00b7te", "Flur"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Gottes Dienerin, Natur!", "tokens": ["O", "Got\u00b7tes", "Die\u00b7ne\u00b7rin", ",", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O heilige Verk\u00fcnderin", "tokens": ["O", "hei\u00b7li\u00b7ge", "Ver\u00b7k\u00fcn\u00b7de\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Unbekannten, Herz und Sinn,", "tokens": ["Des", "Un\u00b7be\u00b7kann\u00b7ten", ",", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Tichten, Trachten weih' ich dir,", "tokens": ["Und", "Tich\u00b7ten", ",", "Trach\u00b7ten", "weih'", "ich", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will dich lieben f\u00fcr und f\u00fcr.", "tokens": ["Und", "will", "dich", "lie\u00b7ben", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mag seyn, dass diese bl\u00f6de Welt", "tokens": ["Mag", "seyn", ",", "dass", "die\u00b7se", "bl\u00f6\u00b7de", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von mir manch schielend Urtheil f\u00e4llt.", "tokens": ["Von", "mir", "manch", "schie\u00b7lend", "Ur\u00b7theil", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Natur gibt Zeugniss dir, mein Geist,", "tokens": ["Na\u00b7tur", "gibt", "Zeug\u00b7niss", "dir", ",", "mein", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dass du der Letzten keiner seyst.", "tokens": ["Dass", "du", "der", "Letz\u00b7ten", "kei\u00b7ner", "seyst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mag seyn, dass mich des Schicksals Schluss", "tokens": ["Mag", "seyn", ",", "dass", "mich", "des", "Schick\u00b7sals", "Schluss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betriegt um Braut- und Schwesterkuss.", "tokens": ["Be\u00b7triegt", "um", "Braut", "und", "Schwes\u00b7ter\u00b7kuss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Winkst du mir doch, und sprichst zu mir:", "tokens": ["Winkst", "du", "mir", "doch", ",", "und", "sprichst", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich nicht Braut und Schwester dir!", "tokens": ["Bin", "ich", "nicht", "Braut", "und", "Schwes\u00b7ter", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "O Herrliche, o Freundliche,", "tokens": ["O", "Herr\u00b7li\u00b7che", ",", "o", "Freund\u00b7li\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Von jedem Jammer, jedem Weh,", "tokens": ["Von", "je\u00b7dem", "Jam\u00b7mer", ",", "je\u00b7dem", "Weh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von jedem Taumel frecher Lust", "tokens": ["Von", "je\u00b7dem", "Tau\u00b7mel", "fre\u00b7cher", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geneset sichs an deiner Brust.", "tokens": ["Ge\u00b7ne\u00b7set", "sichs", "an", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.13": {"line.1": {"text": "O Freundliche, o Herrliche,", "tokens": ["O", "Freund\u00b7li\u00b7che", ",", "o", "Herr\u00b7li\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dich zu umfassen je und je,", "tokens": ["Dich", "zu", "um\u00b7fas\u00b7sen", "je", "und", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir treu zu seyn in Freud' und Leid,", "tokens": ["Dir", "treu", "zu", "seyn", "in", "Freud'", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VAINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gew\u00e4hre morgen mir, wie heut!", "tokens": ["Ge\u00b7w\u00e4h\u00b7re", "mor\u00b7gen", "mir", ",", "wie", "heut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}