{"textgrid.poem.47445": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich bin ein armes L\u00e4mpchen nur]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein armes L\u00e4mpchen nur,", "tokens": ["Ich", "bin", "ein", "ar\u00b7mes", "L\u00e4mp\u00b7chen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein d\u00e4mmerndes in dunkler Nacht;", "tokens": ["Ein", "d\u00e4m\u00b7mern\u00b7des", "in", "dunk\u00b7ler", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist die lichte Morgenpracht", "tokens": ["Du", "bist", "die", "lich\u00b7te", "Mor\u00b7gen\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufstrahlend im Azur.", "tokens": ["Auf\u00b7strah\u00b7lend", "im", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Du strahle nur, du prange nur!", "tokens": ["Du", "strah\u00b7le", "nur", ",", "du", "pran\u00b7ge", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wiewohl vor deinem Angesicht", "tokens": ["Wie\u00b7wohl", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des armen L\u00e4mpchens Auge bricht,", "tokens": ["Des", "ar\u00b7men", "L\u00e4mp\u00b7chens", "Au\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich bebe nicht, ich bange nicht;", "tokens": ["Ich", "be\u00b7be", "nicht", ",", "ich", "ban\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du leuchte nur,", "tokens": ["Du", "leuch\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und ich vergehe gern in deinem Licht.", "tokens": ["Und", "ich", "ver\u00b7ge\u00b7he", "gern", "in", "dei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}