{"textgrid.poem.40338": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Occulta", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist das noch die gro\u00dfe Stadt,", "tokens": ["Ist", "das", "noch", "die", "gro\u00b7\u00dfe", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dies Geraune rings im Grauen?", "tokens": ["dies", "Ge\u00b7rau\u00b7ne", "rings", "im", "Grau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "diese M\u00e4nner, diese Frauen,", "tokens": ["die\u00b7se", "M\u00e4n\u00b7ner", ",", "die\u00b7se", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kaum erschienen, schon verschwunden;", "tokens": ["kaum", "er\u00b7schie\u00b7nen", ",", "schon", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und die Sonne steht so matt", "tokens": ["und", "die", "Son\u00b7ne", "steht", "so", "matt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wie ein kleiner, rotgewordner Mond da.", "tokens": ["wie", "ein", "klei\u00b7ner", ",", "rot\u00b7ge\u00b7word\u00b7ner", "Mond", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dr\u00fcck dich dichter an mich an,", "tokens": ["Dr\u00fcck", "dich", "dich\u00b7ter", "an", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wie der Nebel an die Mauern!", "tokens": ["wie", "der", "Ne\u00b7bel", "an", "die", "Mau\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner st\u00f6rt den stillen Bann,", "tokens": ["Kei\u00b7ner", "st\u00f6rt", "den", "stil\u00b7len", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn wir Blick in Blick erschauern.", "tokens": ["wenn", "wir", "Blick", "in", "Blick", "er\u00b7schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh, wir schreiten wie vermummt in Weihrauch;", "tokens": ["Sieh", ",", "wir", "schrei\u00b7ten", "wie", "ver\u00b7mummt", "in", "Weih\u00b7rauch", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "KOKOM", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "jeder wilde Laut wird stumm.", "tokens": ["je\u00b7der", "wil\u00b7de", "Laut", "wird", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hebe deinen dunkeln Schleier,", "tokens": ["He\u00b7be", "dei\u00b7nen", "dun\u00b7keln", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df dein Atem mich erquickt!", "tokens": ["da\u00df", "dein", "A\u00b7tem", "mich", "er\u00b7quickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner st\u00f6rt die stille Feier,", "tokens": ["Kei\u00b7ner", "st\u00f6rt", "die", "stil\u00b7le", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn sich uns in diesem Dunste", "tokens": ["wenn", "sich", "uns", "in", "die\u00b7sem", "Duns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "fester Hand in Hand verstrickt.", "tokens": ["fes\u00b7ter", "Hand", "in", "Hand", "ver\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Stra\u00dfe m\u00fcndet in den Himmel.", "tokens": ["Die\u00b7se", "Stra\u00b7\u00dfe", "m\u00fcn\u00b7det", "in", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Oder wei\u00dft du, wo wir sind?", "tokens": ["O\u00b7der", "wei\u00dft", "du", ",", "wo", "wir", "sind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcsse mir die Augenbrauen,", "tokens": ["K\u00fcs\u00b7se", "mir", "die", "Au\u00b7gen\u00b7brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "k\u00fcsse mir die Seele blind!", "tokens": ["k\u00fcs\u00b7se", "mir", "die", "See\u00b7le", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese tote Stadt ist Babel,", "tokens": ["Die\u00b7se", "to\u00b7te", "Stadt", "ist", "Ba\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "und ihr blasser Dampf umspinnt", "tokens": ["und", "ihr", "blas\u00b7ser", "Dampf", "um\u00b7spinnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "eine tausendj\u00e4hrig tr\u00fcbe Fabel.", "tokens": ["ei\u00b7ne", "tau\u00b7send\u00b7j\u00e4h\u00b7rig", "tr\u00fc\u00b7be", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Alle Farben sind ertrunken.", "tokens": ["Al\u00b7le", "Far\u00b7ben", "sind", "er\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur auf deinem schwarzen Haare", "tokens": ["Nur", "auf", "dei\u00b7nem", "schwar\u00b7zen", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "flimmern noch die Purpurfunken", "tokens": ["flim\u00b7mern", "noch", "die", "Pur\u00b7pur\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deines Hutes aus Paris,", "tokens": ["dei\u00b7nes", "Hu\u00b7tes", "aus", "Pa\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "rot wie unsre Lippenpaare;", "tokens": ["rot", "wie", "uns\u00b7re", "Lip\u00b7pen\u00b7paa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und mein blauer Wettermantel raschelt.", "tokens": ["und", "mein", "blau\u00b7er", "Wet\u00b7ter\u00b7man\u00b7tel", "ra\u00b7schelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Du, was tr\u00e4umst du? Deine Augen", "tokens": ["Du", ",", "was", "tr\u00e4umst", "du", "?", "Dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PWS", "VVFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "waren eben wie zwei Kohlen,", "tokens": ["wa\u00b7ren", "e\u00b7ben", "wie", "zwei", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die sich von der Glut erholen;", "tokens": ["die", "sich", "von", "der", "Glut", "er\u00b7ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ja, du bist Semiramis!", "tokens": ["ja", ",", "du", "bist", "Se\u00b7mi\u00b7ra\u00b7mis", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in seinem dunkelblauen Mantel", "tokens": ["Und", "in", "sei\u00b7nem", "dun\u00b7kel\u00b7blau\u00b7en", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "f\u00fchrt dein Odhin dich ins Paradies.", "tokens": ["f\u00fchrt", "dein", "Od\u00b7hin", "dich", "ins", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Zwar, wir mu\u00dften durch viel dumpfe Gassen,", "tokens": ["Zwar", ",", "wir", "mu\u00df\u00b7ten", "durch", "viel", "dump\u00b7fe", "Gas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "bis der Gott zu seiner G\u00f6ttin kam,", "tokens": ["bis", "der", "Gott", "zu", "sei\u00b7ner", "G\u00f6t\u00b7tin", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und du hast manch braven Mann,", "tokens": ["und", "du", "hast", "manch", "bra\u00b7ven", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ich manch gutes Weib verlassen;", "tokens": ["ich", "manch", "gu\u00b7tes", "Weib", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "aber dies ist unsre letzte Irrfahrt,", "tokens": ["a\u00b7ber", "dies", "ist", "uns\u00b7re", "letz\u00b7te", "Irr\u00b7fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "dr\u00fcck dich dichter an mich an!", "tokens": ["dr\u00fcck", "dich", "dich\u00b7ter", "an", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sag mir \u2013 Nein: horch! was f\u00fcr T\u00f6ne?", "tokens": ["Sag", "mir", "\u2013", "Nein", ":", "horch", "!", "was", "f\u00fcr", "T\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PTKANT", "$(", "VVIMP", "$.", "PWS", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "warum stehn wir so erschrocken?", "tokens": ["wa\u00b7rum", "stehn", "wir", "so", "er\u00b7schro\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies verhaltene Gest\u00f6hne", "tokens": ["Dies", "ver\u00b7hal\u00b7te\u00b7ne", "Ge\u00b7st\u00f6h\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aus den Wolken, dies Gedr\u00f6hne,", "tokens": ["aus", "den", "Wol\u00b7ken", ",", "dies", "Ge\u00b7dr\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "kannst du diesen L\u00e4rm begreifen? \u2013", "tokens": ["kannst", "du", "die\u00b7sen", "L\u00e4rm", "be\u00b7grei\u00b7fen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Komm nach Hause, F\u00fcrstin! das sind Glocken.", "tokens": ["Komm", "nach", "Hau\u00b7se", ",", "F\u00fcrs\u00b7tin", "!", "das", "sind", "Glo\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "$.", "PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Vor verschiednen hundert Jahren", "tokens": ["Vor", "ver\u00b7schied\u00b7nen", "hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "herrschte hier ein Gott der Leiden", "tokens": ["herrschte", "hier", "ein", "Gott", "der", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00fcber traurige Barbaren.", "tokens": ["\u00fc\u00b7ber", "trau\u00b7ri\u00b7ge", "Bar\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm, wir wolln die G\u00f6tter tr\u00f6sten,", "tokens": ["Komm", ",", "wir", "wolln", "die", "G\u00f6t\u00b7ter", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df sie sich in Dunst aufl\u00f6sten,", "tokens": ["da\u00df", "sie", "sich", "in", "Dunst", "auf\u00b7l\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wir zwei seligen, verirrten Heiden.", "tokens": ["wir", "zwei", "se\u00b7li\u00b7gen", ",", "ver\u00b7irr\u00b7ten", "Hei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punc