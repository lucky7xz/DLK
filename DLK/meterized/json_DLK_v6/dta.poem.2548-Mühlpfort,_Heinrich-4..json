{"dta.poem.2548": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gew\u00fcnschte Nacht/ die Margaris sol nun", "tokens": ["Ge\u00b7w\u00fcnschte", "Nacht", "/", "die", "Mar\u00b7ga\u00b7ris", "sol", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "ART", "NE", "VMFIN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hinfort von Bre\u00dflau ziehn/", "tokens": ["Hin\u00b7fort", "von", "Bre\u00df\u00b7lau", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihren Garten fliehn/", "tokens": ["Und", "ih\u00b7ren", "Gar\u00b7ten", "fliehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schau da\u00df sie nicht m\u00f6g\u2019 allzuschmertzlich thun;", "tokens": ["Schau", "da\u00df", "sie", "nicht", "m\u00f6g'", "all\u00b7zu\u00b7schmertz\u00b7lich", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch sind wir zweiffels lo\u00df/", "tokens": ["Doch", "sind", "wir", "zweif\u00b7fels", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie sitzt dem reichen Gl\u00fcck und Liebsten in der Scho\u00df.", "tokens": ["Sie", "sitzt", "dem", "rei\u00b7che