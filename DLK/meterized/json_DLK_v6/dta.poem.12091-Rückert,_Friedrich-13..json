{"dta.poem.12091": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die alte Fabel fiel mir heute wieder bei,", "tokens": ["Die", "al\u00b7te", "Fa\u00b7bel", "fiel", "mir", "heu\u00b7te", "wie\u00b7der", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie st\u00e4rker milder Sinn als ungest\u00fcmer sei;", "tokens": ["Wie", "st\u00e4r\u00b7ker", "mil\u00b7der", "Sinn", "als", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie eine Wette schlo\u00df die Sonne mit dem Winde,", "tokens": ["Wie", "ei\u00b7ne", "Wet\u00b7te", "schlo\u00df", "die", "Son\u00b7ne", "mit", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer einem Wanderer den Mantel ehr entwinde.", "tokens": ["Wer", "ei\u00b7nem", "Wan\u00b7de\u00b7rer", "den", "Man\u00b7tel", "ehr", "ent\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da blies der Wind, da zog sein Kleid der Wandrer straffer;", "tokens": ["Da", "blies", "der", "Wind", ",", "da", "zog", "sein", "Kleid", "der", "Wand\u00b7rer", "straf\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonne schien hierauf, da lie\u00df ers hangen schlaffer.", "tokens": ["Die", "Son\u00b7ne", "schien", "hier\u00b7auf", ",", "da", "lie\u00df", "ers", "han\u00b7gen", "schlaf\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "ADV", "VVFIN", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und als sie lange schien, da zog er's endlich aus,", "tokens": ["Und", "als", "sie", "lan\u00b7ge", "schien", ",", "da", "zog", "er's", "end\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne Mantel kam der Wanderer nach Haus.", "tokens": ["Und", "oh\u00b7ne", "Man\u00b7tel", "kam", "der", "Wan\u00b7de\u00b7rer", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So hat mich unterwegs kein R\u00e4uber ausgezogen,", "tokens": ["So", "hat", "mich", "un\u00b7ter\u00b7wegs", "kein", "R\u00e4u\u00b7ber", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mancher Wirth ums Geld mit Artigkeit betrogen.", "tokens": ["Doch", "man\u00b7cher", "Wirth", "ums", "Geld", "mit", "Ar\u00b7tig\u00b7keit", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}