{"textgrid.poem.51118": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was eilt zu Tal der Schweizerknab", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was eilt zu Tal der Schweizerknab", "tokens": ["Was", "eilt", "zu", "Tal", "der", "Schwei\u00b7zer\u00b7knab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wandert aus den Toren?", "tokens": ["Und", "wan\u00b7dert", "aus", "den", "To\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00e4hrt den Strom und See herab,", "tokens": ["Er", "f\u00e4hrt", "den", "Strom", "und", "See", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was hat er wohl verloren?", "tokens": ["Was", "hat", "er", "wohl", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heiho! heiho! er sucht geschwind", "tokens": ["Hei\u00b7ho", "!", "hei\u00b7ho", "!", "er", "sucht", "ge\u00b7schwind"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "PPER", "VVFIN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und findet seine Br\u00fcder,", "tokens": ["Und", "fin\u00b7det", "sei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis hundert und bis tausend sind", "tokens": ["Bis", "hun\u00b7dert", "und", "bis", "tau\u00b7send", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "KON", "APPR", "CARD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dreimal tausend wieder!", "tokens": ["Und", "drei\u00b7mal", "tau\u00b7send", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hei seht! er schw\u00e4rmt von Haus zu Haus", "tokens": ["Hei", "seht", "!", "er", "schw\u00e4rmt", "von", "Haus", "zu", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will schon Eisen tragen!", "tokens": ["Und", "will", "schon", "Ei\u00b7sen", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie ziehn mit Wehr und Waffen aus", "tokens": ["Sie", "ziehn", "mit", "Wehr", "und", "Waf\u00b7fen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch mit Stuck und Wagen.", "tokens": ["Und", "auch", "mit", "Stuck", "und", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und auf des Herbstes goldner Au", "tokens": ["Und", "auf", "des", "Herbs\u00b7tes", "gold\u00b7ner", "Au"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergl\u00e4nzt in langen Z\u00fcgen", "tokens": ["Er\u00b7gl\u00e4nzt", "in", "lan\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Jugend kecke Heeresschau,", "tokens": ["Der", "Ju\u00b7gend", "ke\u00b7cke", "Hee\u00b7res\u00b7schau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Fahnen fliegen.", "tokens": ["Und", "ih\u00b7re", "Fah\u00b7nen", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Von hundert Trommeln ist der Klang", "tokens": ["Von", "hun\u00b7dert", "Trom\u00b7meln", "ist", "der", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Vorgehn dumpf zu h\u00f6ren;", "tokens": ["Zum", "Vor\u00b7gehn", "dumpf", "zu", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Blachfeld hier und dort entlang", "tokens": ["Das", "Blach\u00b7feld", "hier", "und", "dort", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wallt Rauch aus tausend R\u00f6hren.", "tokens": ["Wallt", "Rauch", "aus", "tau\u00b7send", "R\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Eidgenossen Oberst schl\u00e4gt", "tokens": ["Der", "Eid\u00b7ge\u00b7nos\u00b7sen", "O\u00b7berst", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zufrieden an den Degen;", "tokens": ["Zu\u00b7frie\u00b7den", "an", "den", "De\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ruft, von frohem Mut bewegt:", "tokens": ["Er", "ruft", ",", "von", "fro\u00b7hem", "Mut", "be\u00b7wegt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdie Saat, die steht im Segen!\u00ab", "tokens": ["\u00bb", "die", "Saat", ",", "die", "steht", "im", "Se\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wie im hohen Schweizertann", "tokens": ["Und", "wie", "im", "ho\u00b7hen", "Schwei\u00b7zer\u00b7tann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die alte Brut gesungen,", "tokens": ["Die", "al\u00b7te", "Brut", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So, wehr dich, guter Schweizermann!", "tokens": ["So", ",", "wehr", "dich", ",", "gu\u00b7ter", "Schwei\u00b7zer\u00b7mann", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So pfeifen auch die Jungen!", "tokens": ["So", "pfei\u00b7fen", "auch", "die", "Jun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}