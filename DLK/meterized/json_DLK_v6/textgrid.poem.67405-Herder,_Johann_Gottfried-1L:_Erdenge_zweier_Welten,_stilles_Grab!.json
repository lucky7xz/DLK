{"textgrid.poem.67405": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erdenge zweier Welten, stilles Grab!", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erdenge zweier Welten, stilles Grab!", "tokens": ["Er\u00b7den\u00b7ge", "zwei\u00b7er", "Wel\u00b7ten", ",", "stil\u00b7les", "Grab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie schweiget's um Dich her! Diesseit ist Nacht", "tokens": ["Wie", "schwei\u00b7get's", "um", "Dich", "her", "!", "Dies\u00b7seit", "ist", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "NN", "VAFIN", "NN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Staub, ein H\u00e4ufchen todter Asche nur;", "tokens": ["Und", "Staub", ",", "ein", "H\u00e4uf\u00b7chen", "tod\u00b7ter", "A\u00b7sche", "nur", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jenseit kommt kein Laut zu mir her\u00fcber,", "tokens": ["Und", "jen\u00b7seit", "kommt", "kein", "Laut", "zu", "mir", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PIAT", "APPR", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein sel'ger Geist, der diesen Staub besucht,", "tokens": ["Kein", "sel'\u00b7ger", "Geist", ",", "der", "die\u00b7sen", "Staub", "be\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein Traumgesicht. \u2013 Nacht ist es um mich. Hoch", "tokens": ["Kein", "Traum\u00b7ge\u00b7sicht", ".", "\u2013", "Nacht", "ist", "es", "um", "mich", ".", "Hoch"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "$.", "$(", "NN", "VAFIN", "PPER", "APPR", "PPER", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Daroben funkeln Sterne. Gl\u00e4nzet Ihr,", "tokens": ["Da\u00b7ro\u00b7ben", "fun\u00b7keln", "Ster\u00b7ne", ".", "Gl\u00e4n\u00b7zet", "Ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gestirne droben, dem entschwungnen Geist?", "tokens": ["Ge\u00b7stir\u00b7ne", "dro\u00b7ben", ",", "dem", "ent\u00b7schwung\u00b7nen", "Geist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und bricht dem Todten, wie dem Schlummernden,", "tokens": ["Und", "bricht", "dem", "Tod\u00b7ten", ",", "wie", "dem", "Schlum\u00b7mern\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Ein neuer Morgen an? Was zweifelst Du,", "tokens": ["Ein", "neu\u00b7er", "Mor\u00b7gen", "an", "?", "Was", "zwei\u00b7felst", "Du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In mir, Unsterblicher, der hier am Rath", "tokens": ["In", "mir", ",", "U\u00b7nsterb\u00b7li\u00b7cher", ",", "der", "hier", "am", "Rath"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADJD", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und Werke der Natur schon Antheil nahm?", "tokens": ["Und", "Wer\u00b7ke", "der", "Na\u00b7tur", "schon", "An\u00b7theil", "nahm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Er sann, was sie ersonnen, und er traf's,", "tokens": ["Er", "sann", ",", "was", "sie", "er\u00b7son\u00b7nen", ",", "und", "er", "traf's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Aussprechend ihre Regel, ihr Gesetz,", "tokens": ["Aus\u00b7spre\u00b7chend", "ih\u00b7re", "Re\u00b7gel", ",", "ihr", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und bot unausgeschlagen ihr die Hand", "tokens": ["Und", "bot", "un\u00b7aus\u00b7ge\u00b7schla\u00b7gen", "ihr", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.16": {"text": "Zum edelsten, zu ihrem ew'gen Werk,", "tokens": ["Zum", "e\u00b7dels\u00b7ten", ",", "zu", "ih\u00b7rem", "ew'\u00b7gen", "Werk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.17": {"text": "Dem Ordnen durch Verstand und G\u00fcte. Wenn", "tokens": ["Dem", "Ord\u00b7nen", "durch", "Ver\u00b7stand", "und", "G\u00fc\u00b7te", ".", "Wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$.", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sie fortan Dein zu ihrem Dienst bedarf", "tokens": ["Sie", "for\u00b7tan", "Dein", "zu", "ih\u00b7rem", "Dienst", "be\u00b7darf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPOSAT", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "(und sie verschm\u00e4ht, die reiche Sparerin,", "tokens": ["(", "und", "sie", "ver\u00b7schm\u00e4ht", ",", "die", "rei\u00b7che", "Spa\u00b7re\u00b7rin", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die nichts verthut und mit dem Mindesten", "tokens": ["Die", "nichts", "ver\u00b7thut", "und", "mit", "dem", "Min\u00b7des\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.21": {"text": "Das Gr\u00f6\u00dfeste verrichtet, keine Kraft", "tokens": ["Das", "Gr\u00f6\u00b7\u00dfes\u00b7te", "ver\u00b7rich\u00b7tet", ",", "kei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und Uebung), wenn sie fortan Dein bedarf,", "tokens": ["Und", "Ue\u00b7bung", ")", ",", "wenn", "sie", "for\u00b7tan", "Dein", "be\u00b7darf", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "So sage willig: \u00bbIch bin da! Und web'", "tokens": ["So", "sa\u00b7ge", "wil\u00b7lig", ":", "\u00bb", "Ich", "bin", "da", "!", "Und", "web'"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "$.", "$(", "PPER", "VAFIN", "ADV", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "In meinem kleinen Winkel emsig fort", "tokens": ["In", "mei\u00b7nem", "klei\u00b7nen", "Win\u00b7kel", "em\u00b7sig", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "An jenem Schleier der Penelope-", "tokens": ["An", "je\u00b7nem", "Schlei\u00b7er", "der", "Pe\u00b7ne\u00b7lo\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "TRUNC"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Minerva, der un\u00fcbersehbar dort", "tokens": ["Mi\u00b7ner\u00b7va", ",", "der", "un\u00b7\u00fc\u00b7ber\u00b7seh\u00b7bar", "dort"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADJD", "ADV"], "meter": "+-+----+-+", "measure": "unknown.measure.tetra"}, "line.27": {"text": "In Millionen Sternen pr\u00e4chtig gl\u00e4nzt.\u00ab", "tokens": ["In", "Mil\u00b7lion\u00b7en", "Ster\u00b7nen", "pr\u00e4ch\u00b7tig", "gl\u00e4nzt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}