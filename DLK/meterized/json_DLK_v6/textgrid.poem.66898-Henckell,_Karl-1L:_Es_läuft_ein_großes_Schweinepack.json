{"textgrid.poem.66898": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es l\u00e4uft ein gro\u00dfes Schweinepack", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es l\u00e4uft ein gro\u00dfes Schweinepack", "tokens": ["Es", "l\u00e4uft", "ein", "gro\u00b7\u00dfes", "Schwei\u00b7ne\u00b7pack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dieser Welt herum,", "tokens": ["Auf", "die\u00b7ser", "Welt", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob Sportdre\u00df, Samtflaus, Joppe, Frack,", "tokens": ["Ob", "Sport\u00b7dre\u00df", ",", "Samt\u00b7flaus", ",", "Jop\u00b7pe", ",", "Frack", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob's Schnaps s\u00e4uft oder Mumm.", "tokens": ["Ob's", "Schnaps", "s\u00e4uft", "o\u00b7der", "Mumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Steckt \u00fcberall den R\u00fcssel 'rein,", "tokens": ["Steckt", "\u00fc\u00b7be\u00b7rall", "den", "R\u00fcs\u00b7sel", "'rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo's was zu schn\u00fcffeln gibt,", "tokens": ["Wo's", "was", "zu", "schn\u00fcf\u00b7feln", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bekleckert selbst den klaren Wein,", "tokens": ["Be\u00b7kle\u00b7ckert", "selbst", "den", "kla\u00b7ren", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den man zu trinken liebt.", "tokens": ["Den", "man", "zu", "trin\u00b7ken", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und hat es einem brav verhunzt", "tokens": ["Und", "hat", "es", "ei\u00b7nem", "brav", "ver\u00b7hunzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens Kunst und Preis,", "tokens": ["Des", "Le\u00b7bens", "Kunst", "und", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00e4lzt es sich und quiekt und grunzt", "tokens": ["So", "w\u00e4lzt", "es", "sich", "und", "qui\u00b7ekt", "und", "grunzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und jodelt mit dem Stei\u00df.", "tokens": ["Und", "jo\u00b7delt", "mit", "dem", "Stei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und macht dazu ein s\u00fc\u00df Gesicht,", "tokens": ["Und", "macht", "da\u00b7zu", "ein", "s\u00fc\u00df", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Infam bis dorthinaus,", "tokens": ["In\u00b7fam", "bis", "dor\u00b7thin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schiebt man's weg, es r\u00fchrt sich nicht", "tokens": ["Und", "schiebt", "man's", "weg", ",", "es", "r\u00fchrt", "sich", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "PRF", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dr\u00fcckt sich fest ans Haus.", "tokens": ["Und", "dr\u00fcckt", "sich", "fest", "ans", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es l\u00e4uft ein gro\u00dfes Schweinepack", "tokens": ["Es", "l\u00e4uft", "ein", "gro\u00b7\u00dfes", "Schwei\u00b7ne\u00b7pack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dieser Welt herum,", "tokens": ["Auf", "die\u00b7ser", "Welt", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In L\u00f6cherk\u00e4hnen oder Lack \u2013", "tokens": ["In", "L\u00f6\u00b7cher\u00b7k\u00e4h\u00b7nen", "o\u00b7der", "Lack", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}