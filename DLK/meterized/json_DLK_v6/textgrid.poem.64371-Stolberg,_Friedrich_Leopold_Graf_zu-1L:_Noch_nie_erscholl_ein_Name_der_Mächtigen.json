"word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$.", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht h\u00e4ufend noch auf seine Schulter", "tokens": ["Nicht", "h\u00e4u\u00b7fend", "noch", "auf", "sei\u00b7ne", "Schul\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fluch des zertretnen, zerri\u00dfnen Volkes,", "tokens": ["Fluch", "des", "zer\u00b7tret\u00b7nen", ",", "zer\u00b7ri\u00df\u00b7nen", "Vol\u00b7kes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Uneingedenk der heiligen Lehren, und", "tokens": ["Un\u00b7ein\u00b7ge\u00b7denk", "der", "hei\u00b7li\u00b7gen", "Leh\u00b7ren", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KON"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "F\u00fcr jene Ader f\u00fchllos, die Gottes Hand", "tokens": ["F\u00fcr", "je\u00b7ne", "A\u00b7der", "f\u00fchl\u00b7los", ",", "die", "Got\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Herzen spannte, da\u00df sie klopfend", "tokens": ["Im", "Her\u00b7zen", "spann\u00b7te", ",", "da\u00df", "sie", "klop\u00b7fend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unrecht und Recht und Erbarmen lehre?", "tokens": ["Un\u00b7recht", "und", 