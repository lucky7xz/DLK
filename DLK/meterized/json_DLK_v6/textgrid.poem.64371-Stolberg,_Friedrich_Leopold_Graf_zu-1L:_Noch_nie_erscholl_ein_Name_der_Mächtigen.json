{"textgrid.poem.64371": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch nie erscholl ein Name der M\u00e4chtigen", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch nie erscholl ein Name der M\u00e4chtigen", "tokens": ["Noch", "nie", "er\u00b7scholl", "ein", "Na\u00b7me", "der", "M\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu meiner Leier, J\u00fcngling; ich weihte sie", "tokens": ["Zu", "mei\u00b7ner", "Lei\u00b7er", ",", "J\u00fcng\u00b7ling", ";", "ich", "weih\u00b7te", "sie"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Freunden nur und Gott, und s\u00fc\u00dfem", "tokens": ["Den", "Freun\u00b7den", "nur", "und", "Gott", ",", "und", "s\u00fc\u00b7\u00dfem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4uslichen Gl\u00fcck, und der Liebe Thr\u00e4nen,", "tokens": ["H\u00e4us\u00b7li\u00b7chen", "Gl\u00fcck", ",", "und", "der", "Lie\u00b7be", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und dir, Natur, im Hain und am Meergestad',", "tokens": ["Und", "dir", ",", "Na\u00b7tur", ",", "im", "Hain", "und", "am", "Meer\u00b7ge\u00b7stad'", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und dir, o Freiheit! Freiheit, du Hochgef\u00fchl", "tokens": ["Und", "dir", ",", "o", "Frei\u00b7heit", "!", "Frei\u00b7heit", ",", "du", "Hoch\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$.", "NN", "$,", "PPER", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der reinen Seelen! Deinen Becher", "tokens": ["Der", "rei\u00b7nen", "See\u00b7len", "!", "Dei\u00b7nen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kr\u00e4nzt' ich mit Blumen des k\u00fchnen Liedes!", "tokens": ["Kr\u00e4nzt'", "ich", "mit", "Blu\u00b7men", "des", "k\u00fch\u00b7nen", "Lie\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Und werd' ihn kr\u00e4nzen, weil eine Nerve mir", "tokens": ["Und", "werd'", "ihn", "kr\u00e4n\u00b7zen", ",", "weil", "ei\u00b7ne", "Ner\u00b7ve", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch z\u00fccket! werd' ihn kosten mit zitternder", "tokens": ["Noch", "z\u00fc\u00b7cket", "!", "werd'", "ihn", "kos\u00b7ten", "mit", "zit\u00b7tern\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und blauer Lippe, wenn des Todes", "tokens": ["Und", "blau\u00b7er", "Lip\u00b7pe", ",", "wenn", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hand mir ihn reichet in hehrer Stunde.", "tokens": ["Hand", "mir", "ihn", "rei\u00b7chet", "in", "heh\u00b7rer", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Nun wind' ich junge Blumen im Kranze dir,", "tokens": ["Nun", "wind'", "ich", "jun\u00b7ge", "Blu\u00b7men", "im", "Kran\u00b7ze", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O J\u00fcngling, weil du fr\u00fch es nicht achtetest", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "weil", "du", "fr\u00fch", "es", "nicht", "ach\u00b7te\u00b7test"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADJD", "PPER", "PTKNEG", "XY"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu herrschen \u00fcber Sklaven, weil du", "tokens": ["Zu", "herr\u00b7schen", "\u00fc\u00b7ber", "Skla\u00b7ven", ",", "weil", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Forschetest, h\u00f6rtest, beschlossest, thatest!", "tokens": ["For\u00b7sche\u00b7test", ",", "h\u00f6r\u00b7test", ",", "be\u00b7schlos\u00b7sest", ",", "tha\u00b7test", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Das Joch des Landmanns dr\u00fcckte Jahrhunderte;", "tokens": ["Das", "Joch", "des", "Land\u00b7manns", "dr\u00fcck\u00b7te", "Jahr\u00b7hun\u00b7der\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du brachst es! H\u00f6r' es, heiliger Schatte du", "tokens": ["Du", "brachst", "es", "!", "H\u00f6r'", "es", ",", "hei\u00b7li\u00b7ger", "Schat\u00b7te", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von meinem Vater, der das Beispiel", "tokens": ["Von", "mei\u00b7nem", "Va\u00b7ter", ",", "der", "das", "Bei\u00b7spiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Diesseit der Eider, und dann am Sund gab.", "tokens": ["Dies\u00b7seit", "der", "Ei\u00b7der", ",", "und", "dann", "am", "Sund", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Du brachst es, J\u00fcngling! wandtest err\u00f6tend dich", "tokens": ["Du", "brachst", "es", ",", "J\u00fcng\u00b7ling", "!", "wand\u00b7test", "er\u00b7r\u00f6\u00b7tend", "dich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "ADJD", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Dank des Landes, sahst auf dem Ocean", "tokens": ["Vom", "Dank", "des", "Lan\u00b7des", ",", "sahst", "auf", "dem", "O\u00b7cean"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Handlung Bande, die des Neides", "tokens": ["Der", "Hand\u00b7lung", "Ban\u00b7de", ",", "die", "des", "Nei\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hand und der Habsucht im Finstern kn\u00fcpfte.", "tokens": ["Hand", "und", "der", "Hab\u00b7sucht", "im", "Fins\u00b7tern", "kn\u00fcpf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Zerrissest leicht wie Spinngewebe sie,", "tokens": ["Zer\u00b7ris\u00b7sest", "leicht", "wie", "Spinn\u00b7ge\u00b7we\u00b7be", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df nicht die stolze Fichte des Normanns mehr", "tokens": ["Da\u00df", "nicht", "die", "stol\u00b7ze", "Fich\u00b7te", "des", "Nor\u00b7manns", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Bruderhafen huldigt, eh sie", "tokens": ["Dem", "Bru\u00b7der\u00b7ha\u00b7fen", "hul\u00b7digt", ",", "eh", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Schwellende Segel dem Ostwind \u00f6ffne.", "tokens": ["Schwel\u00b7len\u00b7de", "Se\u00b7gel", "dem", "Ost\u00b7wind", "\u00f6ff\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Nicht gleiche Gaben spendet des Vaters Hand", "tokens": ["Nicht", "glei\u00b7che", "Ga\u00b7ben", "spen\u00b7det", "des", "Va\u00b7ters", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den V\u00f6lkern; Eisen starret im Schachte dort,", "tokens": ["Den", "V\u00f6l\u00b7kern", ";", "Ei\u00b7sen", "star\u00b7ret", "im", "Schach\u00b7te", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier wanken \u00c4hren, unsers Tisches", "tokens": ["Hier", "wan\u00b7ken", "\u00c4h\u00b7ren", ",", "un\u00b7sers", "Ti\u00b7sches"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freude gedeihet auf fernen Bergen.", "tokens": ["Freu\u00b7de", "ge\u00b7dei\u00b7het", "auf", "fer\u00b7nen", "Ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Zum freien Tausche ladet der Vater ein;", "tokens": ["Zum", "frei\u00b7en", "Tau\u00b7sche", "la\u00b7det", "der", "Va\u00b7ter", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch schmiedet, hart und kl\u00fcgelnd, der blinde Mensch", "tokens": ["Doch", "schmie\u00b7det", ",", "hart", "und", "kl\u00fc\u00b7gelnd", ",", "der", "blin\u00b7de", "Mensch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Tausche Zwang; der biedre Normann", "tokens": ["Dem", "Tau\u00b7sche", "Zwang", ";", "der", "bied\u00b7re", "Nor\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kaufte sein Brot auf verengtem Markte.", "tokens": ["Kauf\u00b7te", "sein", "Brot", "auf", "ver\u00b7eng\u00b7tem", "Mark\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Nun reifen fremde Saaten f\u00fcr ihn, wenn fr\u00fch", "tokens": ["Nun", "rei\u00b7fen", "frem\u