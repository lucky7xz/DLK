{"textgrid.poem.67398": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der heuchelnde Sophist", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemand wird sagen: \u00bbIch bin ein Sophist!\u00ab", "tokens": ["Nie\u00b7mand", "wird", "sa\u00b7gen", ":", "\u00bb", "Ich", "bin", "ein", "So\u00b7phist", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "VVINF", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Um desto seiner giebt die Schule Trug", "tokens": ["Um", "des\u00b7to", "sei\u00b7ner", "giebt", "die", "Schu\u00b7le", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "PPOSAT", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Wahrheit, L\u00fcg' und Larve f\u00fcr Verstand", "tokens": ["F\u00fcr", "Wahr\u00b7heit", ",", "L\u00fcg'", "und", "Lar\u00b7ve", "f\u00fcr", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nennt ihr Dogma reines Postulat.", "tokens": ["Und", "nennt", "ihr", "Dog\u00b7ma", "rei\u00b7nes", "Pos\u00b7tu\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dagegen Aretin mit seiner Secte,", "tokens": ["Da\u00b7ge\u00b7gen", "A\u00b7re\u00b7tin", "mit", "sei\u00b7ner", "Sec\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sch\u00e4mete sich des Cynismus nicht,", "tokens": ["Er", "sch\u00e4\u00b7me\u00b7te", "sich", "des", "Cy\u00b7nis\u00b7mus", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gab Dorn und Rosen mit einander, schw\u00e4tzte", "tokens": ["Gab", "Dorn", "und", "Ro\u00b7sen", "mit", "ein\u00b7an\u00b7der", ",", "schw\u00e4tz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "PRF", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hochprahlend Gut- und B\u00f6ses \u2013 Alles nur", "tokens": ["Hoch\u00b7prah\u00b7lend", "Gut", "und", "B\u00f6\u00b7ses", "\u2013", "Al\u00b7les", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "TRUNC", "KON", "NN", "$(", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum Scherz, nicht zum Betruge; denn es sch\u00e4mte", "tokens": ["Zum", "Scherz", ",", "nicht", "zum", "Be\u00b7tru\u00b7ge", ";", "denn", "es", "sch\u00e4m\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich Aretin, da\u00df man ihn minder arg", "tokens": ["Sich", "A\u00b7re\u00b7tin", ",", "da\u00df", "man", "ihn", "min\u00b7der", "arg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "NE", "$,", "KOUS", "PIS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und schamlos achte, als er selbst sein will.", "tokens": ["Und", "scham\u00b7los", "ach\u00b7te", ",", "als", "er", "selbst", "sein", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "$,", "KOUS", "PPER", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jene verdecken sorgsam den Betrug,", "tokens": ["Je\u00b7ne", "ver\u00b7de\u00b7cken", "sorg\u00b7sam", "den", "Be\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Verstopfen jeden Mund, der sie der List", "tokens": ["Ver\u00b7stop\u00b7fen", "je\u00b7den", "Mund", ",", "der", "sie", "der", "List"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und L\u00fcge zeih', verbieten jedes Buch,", "tokens": ["Und", "L\u00fc\u00b7ge", "zeih'", ",", "ver\u00b7bie\u00b7ten", "je\u00b7des", "Buch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ihre Fallstrick' offenbare. Ist", "tokens": ["Das", "ih\u00b7re", "Fall\u00b7strick'", "of\u00b7fen\u00b7ba\u00b7re", ".", "Ist"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$.", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gef\u00e4hrlich Einer, ist es der Sophist.", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "Ei\u00b7ner", ",", "ist", "es", "der", "So\u00b7phist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}