{"textgrid.poem.25947": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Lieb' kennt keine Jahreszeit", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sommer, der so fr\u00f6hlich war,", "tokens": ["Som\u00b7mer", ",", "der", "so", "fr\u00f6h\u00b7lich", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er entl\u00e4\u00dft der V\u00f6gel Schar,", "tokens": ["Er", "ent\u00b7l\u00e4\u00dft", "der", "V\u00f6\u00b7gel", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Stare weiter ziehn,", "tokens": ["Tau\u00b7send", "Sta\u00b7re", "wei\u00b7ter", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Lieder jetzt entfliehn.", "tokens": ["Tau\u00b7send", "Lie\u00b7der", "jetzt", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf der Wiese, die verbl\u00fcht,", "tokens": ["Auf", "der", "Wie\u00b7se", ",", "die", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch der Himmel einsam gl\u00fcht,", "tokens": ["Noch", "der", "Him\u00b7mel", "ein\u00b7sam", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Sehnsucht, die nie stirbt", "tokens": ["Wie", "die", "Sehn\u00b7sucht", ",", "die", "nie", "stirbt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und um neue Lieder wirbt.", "tokens": ["Und", "um", "neu\u00b7e", "Lie\u00b7der", "wirbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sitzt das Herz am rechten Fleck,", "tokens": ["Sitzt", "das", "Herz", "am", "rech\u00b7ten", "Fleck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt's nicht wie ein Herbstblatt weg,", "tokens": ["F\u00e4llt's", "nicht", "wie", "ein", "Herbst\u00b7blatt", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wechselt auch der Baum sein Kleid, \u2013", "tokens": ["Wech\u00b7selt", "auch", "der", "Baum", "sein", "Kleid", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb' kennt keine Jahreszeit.", "tokens": ["Lieb'", "kennt", "kei\u00b7ne", "Jah\u00b7res\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}