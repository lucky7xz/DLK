{"textgrid.poem.36255": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erstes", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Blume gleich, die sich zur Sonne wendet,", "tokens": ["Der", "Blu\u00b7me", "gleich", ",", "die", "sich", "zur", "Son\u00b7ne", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhebt das sch\u00f6ne Haupt, so sanft gebogen,", "tokens": ["Er\u00b7hebt", "das", "sch\u00f6\u00b7ne", "Haupt", ",", "so", "sanft", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von seidner Locken Heil'genglanz umflogen,", "tokens": ["Von", "seid\u00b7ner", "Lo\u00b7cken", "Heil'\u00b7genglanz", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Auge, das zum Himmel Strahlen sendet.", "tokens": ["Das", "Au\u00b7ge", ",", "das", "zum", "Him\u00b7mel", "Strah\u00b7len", "sen\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die edle Nase, die so sinnreich endet,", "tokens": ["Die", "ed\u00b7le", "Na\u00b7se", ",", "die", "so", "sinn\u00b7reich", "en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hohe Mund, der glatten Stirne Bogen,", "tokens": ["Der", "ho\u00b7he", "Mund", ",", "der", "glat\u00b7ten", "Stir\u00b7ne", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wange braun, von R\u00f6te angeflogen,", "tokens": ["Der", "Wan\u00b7ge", "braun", ",", "von", "R\u00f6\u00b7te", "an\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie scheinen ganz zur Harmonie vollendet.", "tokens": ["Sie", "schei\u00b7nen", "ganz", "zur", "Har\u00b7mo\u00b7nie", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer sieht den Wurm an dieser Blume nagen?", "tokens": ["Wer", "sieht", "den", "Wurm", "an", "die\u00b7ser", "Blu\u00b7me", "na\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer ahndet nahen Tod so sch\u00f6ner H\u00fclle,", "tokens": ["Wer", "ahn\u00b7det", "na\u00b7hen", "Tod", "so", "sch\u00f6\u00b7ner", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schmerzen, die des Knaben Herz umwinden?", "tokens": ["Die", "Schmer\u00b7zen", ",", "die", "des", "Kna\u00b7ben", "Herz", "um\u00b7win\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zerrissen in der Harmonien F\u00fclle,", "tokens": ["Zer\u00b7ris\u00b7sen", "in", "der", "Har\u00b7mo\u00b7ni\u00b7en", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheint mitleidsvoll der stille Geist zu sagen:", "tokens": ["Scheint", "mit\u00b7le\u00b7ids\u00b7voll", "der", "stil\u00b7le", "Geist", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Das Sch\u00f6nste mu\u00df, erscheinend euch, verschwinden.", "tokens": ["Das", "Sch\u00f6ns\u00b7te", "mu\u00df", ",", "er\u00b7schei\u00b7nend", "euch", ",", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "VVPP", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}