{"dta.poem.9925": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin gold- und silber-farben/ traure denn zu zeiten wohl/", "tokens": ["Ich", "bin", "gold", "und", "sil\u00b7ber\u00b7fa\u00b7rben", "/", "trau\u00b7re", "denn", "zu", "zei\u00b7ten", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "TRUNC", "KON", "NN", "$(", "VVFIN", "ADV", "PTKZU", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Frau und jungfern suchen mich/ auch die m\u00e4n\u0303er mein begehien/", "tokens": ["Frau", "und", "jung\u00b7fern", "su\u00b7chen", "mich", "/", "auch", "die", "m\u00e4\u00f1er", "mein", "be\u00b7ge\u00b7hi\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "$(", "ADV", "ART", "ADJA", "PPOSAT", "VVFIN", "$("], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ja die gr\u00f6ste f\u00fcrstin kan meiner dienste nicht entbehren/", "tokens": ["Ja", "die", "gr\u00f6s\u00b7te", "f\u00fcrs\u00b7tin", "kan", "mei\u00b7ner", "diens\u00b7te", "nicht", "ent\u00b7beh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "VMFIN", "PPOSAT", "ADJA", "PTKNEG", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Vielmahls so verletz ich diese/ welchen ich doch dienen soll.", "tokens": ["Viel\u00b7mahls", "so", "ver\u00b7letz", "ich", "die\u00b7se", "/", "wel\u00b7chen", "ich", "doch", "die\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PDAT", "$(", "PWAT", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}