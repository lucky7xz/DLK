{"textgrid.poem.63676": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl wird, der Frauenliebe zu entsagen,", "tokens": ["Wohl", "wird", ",", "der", "Frau\u00b7en\u00b7lie\u00b7be", "zu", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Manne schwer, trotz stoischer Grimassen,", "tokens": ["Dem", "Man\u00b7ne", "schwer", ",", "trotz", "sto\u00b7i\u00b7scher", "Gri\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch da\u00df der Muse Gunst uns will verlassen,", "tokens": ["Doch", "da\u00df", "der", "Mu\u00b7se", "Gunst", "uns", "will", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "D\u00fcnkt uns ein Todesurteil, kaum zu tragen.", "tokens": ["D\u00fcnkt", "uns", "ein", "To\u00b7des\u00b7ur\u00b7teil", ",", "kaum", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich durfte ja in tr\u00fcb' und hellen Tagen", "tokens": ["Ich", "durf\u00b7te", "ja", "in", "tr\u00fcb'", "und", "hel\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die warme Hand der hohen Freundin fassen,", "tokens": ["Die", "war\u00b7me", "Hand", "der", "ho\u00b7hen", "Freun\u00b7din", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von ihrem Frohsinn mich befl\u00fcgeln lassen", "tokens": ["Von", "ih\u00b7rem", "Froh\u00b7sinn", "mich", "be\u00b7fl\u00fc\u00b7geln", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, was die Seele niederzog, ihr klagen.", "tokens": ["Und", ",", "was", "die", "See\u00b7le", "nie\u00b7der\u00b7zog", ",", "ihr", "kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur noch zuweilen, wenn im Wald ich schreite,", "tokens": ["Nur", "noch", "zu\u00b7wei\u00b7len", ",", "wenn", "im", "Wald", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl' ich ein wundersam ambrosisch Wehn,", "tokens": ["F\u00fchl'", "ich", "ein", "wun\u00b7der\u00b7sam", "am\u00b7bro\u00b7sisch", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ginge sie mir unsichtbar zur Seite.", "tokens": ["Als", "gin\u00b7ge", "sie", "mir", "un\u00b7sicht\u00b7bar", "zur", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch bleib' ich froh und s\u00fc\u00dfverworren stehn,", "tokens": ["Doch", "bleib'", "ich", "froh", "und", "s\u00fc\u00df\u00b7ver\u00b7wor\u00b7ren", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich ihr Schleierchen schon in der Weite", "tokens": ["Seh'", "ich", "ihr", "Schlei\u00b7er\u00b7chen", "schon", "in", "der", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und die Gestalt wie einen Traum zergehn.", "tokens": ["Und", "die", "Ge\u00b7stalt", "wie", "ei\u00b7nen", "Traum", "zer\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}