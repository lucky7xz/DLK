{"textgrid.poem.63261": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, sch\u00f6ner Knabe, den beim Heuen", "tokens": ["Wer", "bist", "du", ",", "sch\u00f6\u00b7ner", "Kna\u00b7be", ",", "den", "beim", "Heu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Mutter wohl von ihrer Brust verlor?", "tokens": ["Die", "Mut\u00b7ter", "wohl", "von", "ih\u00b7rer", "Brust", "ver\u00b7lor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du schreitest durch der Mittagssonne Tor,", "tokens": ["Du", "schrei\u00b7test", "durch", "der", "Mit\u00b7tags\u00b7son\u00b7ne", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit L\u00e4cheln das Lebendge zu erneuen.", "tokens": ["Mit", "L\u00e4\u00b7cheln", "das", "Le\u00b7bend\u00b7ge", "zu", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Lass Mann und J\u00fcngling sich am Bilde freuen,", "tokens": ["Lass", "Mann", "und", "J\u00fcng\u00b7ling", "sich", "am", "Bil\u00b7de", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das seine starke Hand zum Heil erkor.", "tokens": ["Das", "sei\u00b7ne", "star\u00b7ke", "Hand", "zum", "Heil", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schwinge im Kinderschwarm das Fl\u00f6tenrohr,", "tokens": ["Schwin\u00b7ge", "im", "Kin\u00b7der\u00b7schwarm", "das", "Fl\u00f6\u00b7ten\u00b7rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit Kl\u00e4ngen die Genossen zu betreuen.", "tokens": ["Mit", "Kl\u00e4n\u00b7gen", "die", "Ge\u00b7nos\u00b7sen", "zu", "be\u00b7treu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bin dein Pferd. Du darfst auf meinem R\u00fccken", "tokens": ["Ich", "bin", "dein", "Pferd", ".", "Du", "darfst", "auf", "mei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu der ertr\u00e4umten Nacht der N\u00e4chte reiten,", "tokens": ["Zu", "der", "er\u00b7tr\u00e4um\u00b7ten", "Nacht", "der", "N\u00e4ch\u00b7te", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Flammenglut das schmale Holzschwert z\u00fccken!", "tokens": ["In", "Flam\u00b7men\u00b7glut", "das", "schma\u00b7le", "Holz\u00b7schwert", "z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die ewigen Engel werden dich begleiten,", "tokens": ["Die", "e\u00b7wi\u00b7gen", "En\u00b7gel", "wer\u00b7den", "dich", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den kleinen K\u00e4mpfer fl\u00fcgelnd zu begl\u00fccken,", "tokens": ["Den", "klei\u00b7nen", "K\u00e4mp\u00b7fer", "fl\u00fc\u00b7gelnd", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihn zum Siegesfest der Mannheit leiten.", "tokens": ["Und", "ihn", "zum", "Sie\u00b7ges\u00b7fest", "der", "Mann\u00b7heit", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}