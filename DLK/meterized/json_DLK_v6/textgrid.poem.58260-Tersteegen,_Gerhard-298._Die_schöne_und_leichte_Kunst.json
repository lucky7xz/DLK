{"textgrid.poem.58260": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "298. Die sch\u00f6ne und leichte Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur sanft und stille Gott ansehen,", "tokens": ["Nur", "sanft", "und", "stil\u00b7le", "Gott", "an\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Welt und sich nicht denken mehr,", "tokens": ["An", "Welt", "und", "sich", "nicht", "den\u00b7ken", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PRF", "PTKNEG", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihm als seinem Freund umgehen,", "tokens": ["Mit", "ihm", "als", "sei\u00b7nem", "Freund", "um\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O sch\u00f6ne Kunst, o leichte Lehr!", "tokens": ["O", "sch\u00f6\u00b7ne", "Kunst", ",", "o", "leich\u00b7te", "Lehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}