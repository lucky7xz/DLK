{"textgrid.poem.42767": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Fu\u00dfball", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fu\u00dfballwahn ist eine Krank-", "tokens": ["Der", "Fu\u00df\u00b7ball\u00b7wahn", "ist", "ei\u00b7ne", "Krank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heit, aber selten, Gott sei Dank.", "tokens": ["Heit", ",", "a\u00b7ber", "sel\u00b7ten", ",", "Gott", "sei", "Dank", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$,", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kenne wen, der litt akut", "tokens": ["Ich", "ken\u00b7ne", "wen", ",", "der", "litt", "a\u00b7kut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PRELS", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Fu\u00dfballwahn und Fu\u00dfballwut.", "tokens": ["An", "Fu\u00df\u00b7ball\u00b7wahn", "und", "Fu\u00df\u00b7ball\u00b7wut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sowie er einen Gegenstand", "tokens": ["So\u00b7wie", "er", "ei\u00b7nen", "Ge\u00b7gen\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Kugelform und \u00e4hnlich fand,", "tokens": ["In", "Ku\u00b7gel\u00b7form", "und", "\u00e4hn\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So trat er zu und stie\u00df mit Kraft", "tokens": ["So", "trat", "er", "zu", "und", "stie\u00df", "mit", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn in die bunte Nachbarschaft.", "tokens": ["Ihn", "in", "die", "bun\u00b7te", "Nach\u00b7bar\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ob es ein Schwalbennest, ein Tiegel,", "tokens": ["Ob", "es", "ein", "Schwal\u00b7ben\u00b7nest", ",", "ein", "Tie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein K\u00e4se, Globus oder Igel,", "tokens": ["Ein", "K\u00e4\u00b7se", ",", "Glo\u00b7bus", "o\u00b7der", "I\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Krug, ein Schmuckwerk am Altar,", "tokens": ["Ein", "Krug", ",", "ein", "Schmuck\u00b7werk", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Kegelball, ein Kissen war,", "tokens": ["Ein", "Ke\u00b7gel\u00b7ball", ",", "ein", "Kis\u00b7sen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wem der Gegenstand geh\u00f6rte,", "tokens": ["Und", "wem", "der", "Ge\u00b7gen\u00b7stand", "ge\u00b7h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das war etwas, was ihn nicht st\u00f6rte.", "tokens": ["Das", "war", "et\u00b7was", ",", "was", "ihn", "nicht", "st\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Bald trieb er eine Schweineblase,", "tokens": ["Bald", "trieb", "er", "ei\u00b7ne", "Schwei\u00b7ne\u00b7bla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Bald steife H\u00fcte durch die Stra\u00dfe.", "tokens": ["Bald", "stei\u00b7fe", "H\u00fc\u00b7te", "durch", "die", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dann wieder mit ge\u00fcbtem Schwung", "tokens": ["Dann", "wie\u00b7der", "mit", "ge\u00b7\u00fcb\u00b7tem", "Schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Stie\u00df er den Fu\u00df in Pferdedung.", "tokens": ["Stie\u00df", "er", "den", "Fu\u00df", "in", "Pfer\u00b7de\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.19": {"text": "Mit Schwamm und Seife trieb er Sport.", "tokens": ["Mit", "Schwamm", "und", "Sei\u00b7fe", "trieb", "er", "Sport", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Lampenkuppel brach sofort.", "tokens": ["Die", "Lam\u00b7pen\u00b7kup\u00b7pel", "brach", "so\u00b7fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Das Nachtgeschirr flog zielbewu\u00dft", "tokens": ["Das", "Nacht\u00b7ge\u00b7schirr", "flog", "ziel\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Tante Berta an die Brust.", "tokens": ["Der", "Tan\u00b7te", "Ber\u00b7ta", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Kein Abwehrmittel wollte n\u00fctzen,", "tokens": ["Kein", "Ab\u00b7wehr\u00b7mit\u00b7tel", "woll\u00b7te", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nicht Stacheldraht in Stiefelspitzen,", "tokens": ["Nicht", "Sta\u00b7chel\u00b7draht", "in", "Stie\u00b7fel\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Noch Puffer au\u00dfen angebracht.", "tokens": ["Noch", "Puf\u00b7fer", "au\u00b7\u00dfen", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Er siegte immer, o zu 8.", "tokens": ["Er", "sieg\u00b7te", "im\u00b7mer", ",", "o", "zu", "8."], "token_info": ["word", "word", "word", "punct", "word", "word", "ordinal"], "pos": ["PPER", "VVFIN", "ADV", "$,", "FM", "APPR", "ADJA"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.27": {"text": "Und \u00fcbte weiter frisch, fromm, frei", "tokens": ["Und", "\u00fcb\u00b7te", "wei\u00b7ter", "frisch", ",", "fromm", ",", "frei"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mit Totenkopf und Strau\u00dfenei.", "tokens": ["Mit", "To\u00b7ten\u00b7kopf", "und", "Strau\u00b7\u00dfe\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Erschreckt durch seine wilden St\u00f6\u00dfe,", "tokens": ["Er\u00b7schreckt", "durch", "sei\u00b7ne", "wil\u00b7den", "St\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Gab man ihm nie Kartoffelkl\u00f6\u00dfe.", "tokens": ["Gab", "man", "ihm", "nie", "Kar\u00b7tof\u00b7fel\u00b7kl\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Selbst vor dem Podex und den Br\u00fcsten", "tokens": ["Selbst", "vor", "dem", "Po\u00b7dex", "und", "den", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Der Frau ergriff ihn ein Gel\u00fcsten,", "tokens": ["Der", "Frau", "er\u00b7griff", "ihn", "ein", "Ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Was er jedoch als Mann von Stand", "tokens": ["Was", "er", "je\u00b7doch", "als", "Mann", "von", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "KOUS", "NN", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.34": {"text": "Aus H\u00f6flichkeit meist \u00fcberwand.", "tokens": ["Aus", "H\u00f6f\u00b7lich\u00b7keit", "meist", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dagegen gab ein Schwartenmagen", "tokens": ["Da\u00b7ge\u00b7gen", "gab", "ein", "Schwar\u00b7ten\u00b7ma\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Dem Fleischer Anla\u00df zum Verklagen.", "tokens": ["Dem", "Flei\u00b7scher", "An\u00b7la\u00df", "zum", "Ver\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Was beim Gem\u00fcsemarkt geschah,", "tokens": ["Was", "beim", "Ge\u00b7m\u00fc\u00b7se\u00b7markt", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Kommt einer Schlacht bei Leipzig nah.", "tokens": ["Kommt", "ei\u00b7ner", "Schlacht", "bei", "Leip\u00b7zig", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da schwirrten \u00c4pfel, Apfelsinen", "tokens": ["Da", "schwirr\u00b7ten", "\u00c4p\u00b7fel", ",", "Ap\u00b7fel\u00b7si\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Durch Publikum wie wilde Bienen.", "tokens": ["Durch", "Pub\u00b7li\u00b7kum", "wie", "wil\u00b7de", "Bie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Da sah man Blutorangen, Zwetschen", "tokens": ["Da", "sah", "man", "Blu\u00b7to\u00b7ran\u00b7gen", ",", "Zwet\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "An blassen Wangen sich zerquetschen.", "tokens": ["An", "blas\u00b7sen", "Wan\u00b7gen", "sich", "zer\u00b7quet\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Das Eigelb \u00fcberzog die Leiber,", "tokens": ["Das", "Ei\u00b7gelb", "\u00fc\u00b7ber\u00b7zog", "die", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Ein Fischkorb platzte zwischen Weiber.", "tokens": ["Ein", "Fischkorb", "platz\u00b7te", "zwi\u00b7schen", "Wei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Kartoffeln spritzten und Citronen.", "tokens": ["Kar\u00b7tof\u00b7feln", "spritz\u00b7ten", "und", "Cit\u00b7ro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Man duckte sich vor den Melonen.", "tokens": ["Man", "duck\u00b7te", "sich", "vor", "den", "Me\u00b7lo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.47": {"text": "Dem Krautkopf folgten K\u00fcrbissch\u00fcsse.", "tokens": ["Dem", "Kraut\u00b7kopf", "folg\u00b7ten", "K\u00fcr\u00b7bis\u00b7sch\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Dann donnerten die Kokosn\u00fcsse.", "tokens": ["Dann", "don\u00b7ner\u00b7ten", "die", "Ko\u00b7kos\u00b7n\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Genug! Als alles dies getan,", "tokens": ["Ge\u00b7nug", "!", "Als", "al\u00b7les", "dies", "ge\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PIS", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Griff unser Held zum Gr\u00f6\u00dfenwahn.", "tokens": ["Griff", "un\u00b7ser", "Held", "zum", "Gr\u00f6\u00b7\u00dfen\u00b7wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Schon sch\u00e4kernd mit der U-Bootsmine \u2013", "tokens": ["Schon", "sch\u00e4\u00b7kernd", "mit", "der", "U\u00b7\u00b7Boots\u00b7mi\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Besann er sich auf die Lawine.", "tokens": ["Be\u00b7sann", "er", "sich", "auf", "die", "La\u00b7wi\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.53": {"text": "Doch als pomp\u00f6ser Fu\u00dfballst\u00f6\u00dfer", "tokens": ["Doch", "als", "pom\u00b7p\u00f6\u00b7ser", "Fu\u00df\u00b7ball\u00b7st\u00f6\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Fand er die Erde noch viel gr\u00f6\u00dfer.", "tokens": ["Fand", "er", "die", "Er\u00b7de", "noch", "viel", "gr\u00f6\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.55": {"text": "Er rang mit mancherlei Problemen.", "tokens": ["Er", "rang", "mit", "man\u00b7cher\u00b7lei", "Prob\u00b7le\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Zun\u00e4chst: Wie soll man Anlauf nehmen?", "tokens": ["Zu\u00b7n\u00e4chst", ":", "Wie", "soll", "man", "An\u00b7lauf", "neh\u00b7men", "?"], "token_info": ["word", "punct", "word", "wo