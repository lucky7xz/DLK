{"dta.poem.9808": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff den nahmens-tag Hn. Nicolaus Will-  \n mann/ Churf\u00fcrstl. Brandenburgischen  \n amts- und steuer-raths.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die kluge welt hat noch bi\u00df heute nicht ergr\u00fcndt/", "tokens": ["Die", "klu\u00b7ge", "welt", "hat", "noch", "bi\u00df", "heu\u00b7te", "nicht", "er\u00b7gr\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was Martyr dennoch will vor eine warheit schreiben;", "tokens": ["Was", "Mar\u00b7tyr", "den\u00b7noch", "will", "vor", "ei\u00b7ne", "war\u00b7heit", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob in Longuca sich ein wunder-brunn befindt/", "tokens": ["Ob", "in", "Lon\u00b7gu\u00b7ca", "sich", "ein", "wun\u00b7der\u00b7brunn", "be\u00b7findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "PRF", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der alte leiber kan in junge formen treiben:", "tokens": ["Der", "al\u00b7te", "lei\u00b7ber", "kan", "in", "jun\u00b7ge", "for\u00b7men", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df aber ist gewi\u00df: da\u00df GOttes stille krafft", "tokens": ["Di\u00df", "a\u00b7ber", "ist", "ge\u00b7wi\u00df", ":", "da\u00df", "Got\u00b7tes", "stil\u00b7le", "krafft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ADV", "$.", "KOUS", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verlebten dingen offt das meiste leben schafft.", "tokens": ["Ver\u00b7leb\u00b7ten", "din\u00b7gen", "offt", "das", "meis\u00b7te", "le\u00b7ben", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ART", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie spielt nicht die natur an dem gestirnten bogen?", "tokens": ["Wie", "spielt", "nicht", "die", "na\u00b7tur", "an", "dem", "ge\u00b7stirn\u00b7ten", "bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des monden silber f\u00e4llt und nimmet wieder zu:", "tokens": ["Des", "mon\u00b7den", "sil\u00b7ber", "f\u00e4llt", "und", "nim\u00b7met", "wie\u00b7der", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wolcken werden ab- und wieder auffgezogen:", "tokens": ["Die", "wol\u00b7cken", "wer\u00b7den", "ab", "und", "wie\u00b7der", "auff\u00b7ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "TRUNC", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sonnen-kugel sucht in der bewegung ruh/", "tokens": ["Die", "son\u00b7nen\u00b7ku\u00b7gel", "sucht", "in", "der", "be\u00b7we\u00b7gung", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da wir alle gleich von ihrer flamme zehren/", "tokens": ["Und", "da", "wir", "al\u00b7le", "gleich", "von", "ih\u00b7rer", "flam\u00b7me", "zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sieht man sie t\u00e4glich doch ein neues feur geb\u00e4hren:", "tokens": ["Sieht", "man", "sie", "t\u00e4g\u00b7lich", "doch", "ein", "neu\u00b7es", "feur", "ge\u00b7b\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Di\u00df thut der himmel nur; was nicht die unter-welt?", "tokens": ["Di\u00df", "thut", "der", "him\u00b7mel", "nur", ";", "was", "nicht", "die", "un\u00b7ter\u00b7welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$.", "PWS", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die uns mehr wunder l\u00e4\u00dft als jener sterne schauen.", "tokens": ["Die", "uns", "mehr", "wun\u00b7der", "l\u00e4\u00dft", "als", "je\u00b7ner", "ster\u00b7ne", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "KOKOM", "PDAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wenn die graue zeit den marmel fast verstellt/", "tokens": ["Denn", "wenn", "die", "grau\u00b7e", "zeit", "den", "mar\u00b7mel", "fast", "ver\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df witz und kunst daraus erst grosse tempel bauen/", "tokens": ["Mu\u00df", "witz", "und", "kunst", "da\u00b7raus", "erst", "gros\u00b7se", "tem\u00b7pel", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "VMFIN", "PAV", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und die erfahrung giebt: da\u00df barben und der wein", "tokens": ["Und", "die", "er\u00b7fah\u00b7rung", "giebt", ":", "da\u00df", "bar\u00b7ben", "und", "der", "wein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "KOUS", "ADJA", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm alter k\u00f6stlicher als in der jugend seyn.", "tokens": ["Jm", "al\u00b7ter", "k\u00f6st\u00b7li\u00b7cher", "als", "in", "der", "ju\u00b7gend", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "KOKOM", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein crocodil der w\u00e4chst/ so lang er sich beweget:", "tokens": ["Ein", "cro\u00b7co\u00b7dil", "der", "w\u00e4chst", "/", "so", "lang", "er", "sich", "be\u00b7we\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$(", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die str\u00f6me mehren sich durch ihren weiten lauff.", "tokens": ["Die", "str\u00f6\u00b7me", "meh\u00b7ren", "sich", "durch", "ih\u00b7ren", "wei\u00b7ten", "lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr ein feigen-baum auf erden fr\u00fcchte tr\u00e4get/", "tokens": ["Je", "mehr", "ein", "fei\u00b7gen\u00b7baum", "auf", "er\u00b7den", "fr\u00fcch\u00b7te", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "ADJA", "VVFIN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Je mehr setzt die natur ihm wieder knospen auf.", "tokens": ["Je", "mehr", "setzt", "die", "na\u00b7tur", "ihm", "wie\u00b7der", "knos\u00b7pen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Ein adler kan ihm selbst die jugend wieder bringen/", "tokens": ["Ein", "ad\u00b7ler", "kan", "ihm", "selbst", "die", "ju\u00b7gend", "wie\u00b7der", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein todter ph\u00f6nix sich aus staub und asche schwingen.", "tokens": ["Ein", "tod\u00b7ter", "ph\u00f6\u00b7nix", "sich", "aus", "staub", "und", "asc\u00b7he", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PRF", "APZR", "VVFIN", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der arme mensch allein bricht wie der porcellan/", "tokens": ["Der", "ar\u00b7me", "mensch", "al\u00b7lein", "bricht", "wie", "der", "por\u00b7cel\u00b7lan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "KOKOM", "ART", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und steckt voll ungemach/ wie d\u00fcnste voller regen.", "tokens": ["Und", "steckt", "voll", "un\u00b7ge\u00b7mach", "/", "wie", "d\u00fcns\u00b7te", "vol\u00b7ler", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "$(", "KOKOM", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er weint/ so bald er nur die augen auffgethan/", "tokens": ["Er", "weint", "/", "so", "bald", "er", "nur", "die", "au\u00b7gen", "auff\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADV", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit thr\u00e4nen mu\u00df er sich auch wieder niederlegen/", "tokens": ["Mit", "thr\u00e4\u00b7nen", "mu\u00df", "er", "sich", "auch", "wie\u00b7der", "nie\u00b7der\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und iedem kinde zeigt sein schreyend A und E/", "tokens": ["Und", "ie\u00b7dem", "kin\u00b7de", "zeigt", "sein", "schre\u00b7yend", "A", "und", "E", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "ADJD", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man mit Armuth auf/ mit Elend untergeh.", "tokens": ["Da\u00df", "man", "mit", "Ar\u00b7muth", "auf", "/", "mit", "E\u00b7lend", "un\u00b7ter\u00b7geh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "APPR", "$(", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum rei\u00dft die ungedult zuweilen aus den schrancken/", "tokens": ["Drum", "rei\u00dft", "die", "un\u00b7ge\u00b7dult", "zu\u00b7wei\u00b7len", "aus", "den", "schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fl\u00f6\u00dft den sterblichen die falsche lehren ein:", "tokens": ["Und", "fl\u00f6\u00dft", "den", "sterb\u00b7li\u00b7chen", "die", "fal\u00b7sche", "leh\u00b7ren", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die g\u00f6tter w\u00e4ren nichts als schatten und gedancken;", "tokens": ["Die", "g\u00f6t\u00b7ter", "w\u00e4\u00b7ren", "nichts", "als", "schat\u00b7ten", "und", "ge\u00b7dan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PIS", "KOKOM", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst m\u00fcste ja der mensch mehr wie die thiere seyn.", "tokens": ["Sonst", "m\u00fcs\u00b7te", "ja", "der", "mensch", "mehr", "wie", "die", "thie\u00b7re", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "ADV", "KOKOM", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn diese schlieffen offt auf rosen und ja\u00dfminen/", "tokens": ["Denn", "die\u00b7se", "schlief\u00b7fen", "offt", "auf", "ro\u00b7sen", "und", "ja\u00df\u00b7mi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPR", "ADJA", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn jenem tod und blitz an statt der sonne schienen.", "tokens": ["Wenn", "je\u00b7nem", "tod", "und", "blitz", "an", "statt", "der", "son\u00b7ne", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "KON", "VVIMP", "PTKVZ", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So denckt die blinde welt: doch sonder alles recht.", "tokens": ["So", "denckt", "die", "blin\u00b7de", "welt", ":", "doch", "son\u00b7der", "al\u00b7les", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn fleisch und blut sind nicht die nahrung unsrer seelen.", "tokens": ["Denn", "fleisch", "und", "blut", "sind", "nicht", "die", "nah\u00b7rung", "uns\u00b7rer", "see\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "NN", "VAFIN", "PTKNEG", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr der matte leib sich an begierden schw\u00e4cht/", "tokens": ["Je", "mehr", "der", "mat\u00b7te", "leib", "sich", "an", "be\u00b7gier\u00b7den", "schw\u00e4cht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PRF", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Je weiter r\u00fcckt der geist aus seinen m\u00f6rder-h\u00f6len;", "tokens": ["Je", "wei\u00b7ter", "r\u00fcckt", "der", "geist", "aus", "sei\u00b7nen", "m\u00f6r\u00b7der\u00b7h\u00f6len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil/ wenn die rosen uns am munde niedergehn/", "tokens": ["Weil", "/", "wenn", "die", "ro\u00b7