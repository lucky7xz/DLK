{"dta.poem.8850": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "D'rum, o Freunde, la\u00dft vergebens nicht verrinnen heut", "tokens": ["D'\u00b7rum", ",", "o", "Freun\u00b7de", ",", "la\u00dft", "ver\u00b7ge\u00b7bens", "nicht", "ver\u00b7rin\u00b7nen", "heut"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "FM", "NN", "$,", "VVIMP", "ADV", "PTKNEG", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Heut und morgen ist die Summe dieses allzukurzen", "tokens": ["Heut", "und", "mor\u00b7gen", "ist", "die", "Sum\u00b7me", "die\u00b7ses", "all\u00b7zu\u00b7kur\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "VAFIN", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Und wie schnell, wir wissen's Alle, gehn von hinnen", "tokens": ["Und", "wie", "schnell", ",", "wir", "wis\u00b7sen's", "Al\u00b7le", ",", "gehn", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "$,", "PPER", "VVFIN", "PIS", "$,", "VVFIN", "APPR", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Im topas'nen Kelch der Tulpe schwelgt der Thau als", "tokens": ["Im", "to\u00b7pas'\u00b7nen", "Kelch", "der", "Tul\u00b7pe", "schwelgt", "der", "Thau", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "NN", "KOKOM"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Doch ihn l\u00e4\u00dft das Gold der Sonne nicht darinnen heut", "tokens": ["Doch", "ihn", "l\u00e4\u00dft", "das", "Gold", "der", "Son\u00b7ne", "nicht", "da\u00b7rin\u00b7nen", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Ein'ge Bl\u00e4tter aus den Rosen hat ein Wind davon ge-", "tokens": ["Ein'\u00b7ge", "Bl\u00e4t\u00b7ter", "aus", "den", "Ro\u00b7sen", "hat", "ein", "Wind", "da\u00b7von", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "PAV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Und er wird sie ganz entf\u00fchren, f\u00fcrcht' ich, binnen heut", "tokens": ["Und", "er", "wird", "sie", "ganz", "ent\u00b7f\u00fch\u00b7ren", ",", "f\u00fcrcht'", "ich", ",", "bin\u00b7nen", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "La\u00df den Trank im Becher steigen, denn der Wein des", "tokens": ["La\u00df", "den", "Trank", "im", "Be\u00b7cher", "stei\u00b7gen", ",", "denn", "der", "Wein", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPRART", "NN", "VVINF", "$,", "KON", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Quillt empor bis an der Berge hohe Zinnen heut und", "tokens": ["Quillt", "em\u00b7por", "bis", "an", "der", "Ber\u00b7ge", "ho\u00b7he", "Zin\u00b7nen", "heut", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "APPR", "ART", "NN", "ADJA", "NN", "ADV", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}