{"dta.poem.13806": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder 1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Steig' aufw\u00e4rts, Morgenstunde!", "tokens": ["Steig'", "auf\u00b7w\u00e4rts", ",", "Mor\u00b7gen\u00b7stun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zerrei\u00df' die Nacht, da\u00df ich in meinem Wehe", "tokens": ["Zer\u00b7rei\u00df'", "die", "Nacht", ",", "da\u00df", "ich", "in", "mei\u00b7nem", "We\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Himmel wiedersehe,", "tokens": ["Den", "Him\u00b7mel", "wie\u00b7der\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ew'ger Frieden in dem blauen Grunde!", "tokens": ["Wo", "ew'\u00b7ger", "Frie\u00b7den", "in", "dem", "blau\u00b7en", "Grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Will Licht die Welt erneuen:", "tokens": ["Will", "Licht", "die", "Welt", "er\u00b7neu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mag auch der Schmerz in Thr\u00e4nen sich befreien.", "tokens": ["Mag", "auch", "der", "Schmerz", "in", "Thr\u00e4\u00b7nen", "sich", "be\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}