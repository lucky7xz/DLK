{"dta.poem.23801": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Der 51. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "O Reicher Quell der Langmuth und Gedult/", "tokens": ["O", "Rei\u00b7cher", "Quell", "der", "Lang\u00b7muth", "und", "Ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Getreuer GOtt/ du Zuflucht aller Armen!", "tokens": ["Ge\u00b7treu\u00b7er", "Gott", "/", "du", "Zu\u00b7flucht", "al\u00b7ler", "Ar\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bestr\u00f6hme mich mit g\u00fctigem Erbarmen/", "tokens": ["Be\u00b7str\u00f6h\u00b7me", "mich", "mit", "g\u00fc\u00b7ti\u00b7gem", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sp\u00fchle weg den Unflath meiner Schuld.", "tokens": ["Und", "sp\u00fch\u00b7le", "weg", "den", "Un\u00b7flath", "mei\u00b7ner", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich k age selbst mein b\u00f6ses Leben an/", "tokens": ["Ich", "k", "a\u00b7ge", "selbst", "mein", "b\u00f6\u00b7ses", "Le\u00b7ben", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mich schrecken stets die Larven meiner S\u00fcnden.", "tokens": ["Mich", "schre\u00b7cken", "stets", "die", "Lar\u00b7ven", "mei\u00b7ner", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Drum ist kein Mensch/ der dirs verdencken kan/", "tokens": ["Drum", "ist", "kein", "Mensch", "/", "der", "dirs", "ver\u00b7den\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "$(", "ART", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn du mir nicht wilst Gnade lassen finden.", "tokens": ["Wenn", "du", "mir", "nicht", "wilst", "Gna\u00b7de", "las\u00b7sen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Fehler die mit meiner Eltern Blut/", "tokens": ["Die", "Feh\u00b7ler", "die", "mit", "mei\u00b7ner", "El\u00b7tern", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ich gezeugt/ in meine Adern flossen/", "tokens": ["Da", "ich", "ge\u00b7zeugt", "/", "in", "mei\u00b7ne", "A\u00b7dern", "flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die waren schon genug mich zu verstossen.", "tokens": ["Die", "wa\u00b7ren", "schon", "ge\u00b7nug", "mich", "zu", "ver\u00b7stos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sieh\u2019 aber an dein Kind das Busse thut/", "tokens": ["Sieh'", "a\u00b7ber", "an", "dein", "Kind", "das", "Bus\u00b7se", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und dir noch mehr zu seiner Schmach bekennt:", "tokens": ["Und", "dir", "noch", "mehr", "zu", "sei\u00b7ner", "Schmach", "be\u00b7kennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich wuste wol die Wahrheit deiner Worte/", "tokens": ["Ich", "wus\u00b7te", "wol", "die", "Wahr\u00b7heit", "dei\u00b7ner", "Wor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch hab\u2019 ich mich mit Flei\u00df von dir getrennt/", "tokens": ["Doch", "hab'", "ich", "mich", "mit", "Flei\u00df", "von", "dir", "ge\u00b7trennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "APPR", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und nicht gesucht den Weg zur Himmels-Pforte.", "tokens": ["Und", "nicht", "ge\u00b7sucht", "den", "Weg", "zur", "Him\u00b7mels\u00b7P\u00b7for\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach wasche doch mich von dem Aussatz rein/", "tokens": ["Ach", "wa\u00b7sche", "doch", "mich", "von", "dem", "Aus\u00b7satz", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "PRF", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Treib aus den Gifft/ erfrische mein Gebl\u00fcte;", "tokens": ["Treib", "aus", "den", "Gifft", "/", "er\u00b7fri\u00b7sche", "mein", "Ge\u00b7bl\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Ents\u00fcndge mich mit Isop deiner G\u00fcte/", "tokens": ["Ent\u00b7s\u00fcnd\u00b7ge", "mich", "mit", "I\u00b7sop", "dei\u00b7ner", "G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So werd\u2019 ich wei\u00df wie Schnee in Unschuld seyn.", "tokens": ["So", "werd'", "ich", "wei\u00df", "wie", "Schnee", "in", "Un\u00b7schuld", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "KOKOM", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Herr mein Gebein/ das du zermalmet hast/", "tokens": ["Herr", "mein", "Ge\u00b7bein", "/", "das", "du", "zer\u00b7mal\u00b7met", "hast", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "PRELS", "PPER", "VVPP", "VAFIN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "La\u00df wiederum mit mildem Trost erqvicken/", "tokens": ["La\u00df", "wie\u00b7de\u00b7rum", "mit", "mil\u00b7dem", "Trost", "er\u00b7qvi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und mir einmahl nach dieser schweren Last/", "tokens": ["Und", "mir", "ein\u00b7mahl", "nach", "die\u00b7ser", "schwe\u00b7ren", "Last", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur einen Strahl von deiner Huld erblicken.", "tokens": ["Nur", "ei\u00b7nen", "Strahl", "von", "dei\u00b7ner", "Huld", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja pflantze gar in meiner matten Brust", "tokens": ["Ja", "pflant\u00b7ze", "gar", "in", "mei\u00b7ner", "mat\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein neues Hertz/ das neue Regung f\u00fchlet/", "tokens": ["Ein", "neu\u00b7es", "Hertz", "/", "das", "neu\u00b7e", "Re\u00b7gung", "f\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht im Koth der alten S\u00fcnden w\u00fchlet/", "tokens": ["Und", "nicht", "im", "Koth", "der", "al\u00b7ten", "S\u00fcn\u00b7den", "w\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ausser dir sonst findet keine Lust;", "tokens": ["Das", "aus\u00b7ser", "dir", "sonst", "fin\u00b7det", "kei\u00b7ne", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Hertz das dir best\u00e4ndig treu verbleibt/", "tokens": ["Ein", "Hertz", "das", "dir", "be\u00b7st\u00e4n\u00b7dig", "treu", "ver\u00b7bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das dich in sich/ und sich in dir kan schauen/", "tokens": ["Das", "dich", "in", "sich", "/", "und", "sich", "in", "dir", "kan", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PRF", "$(", "KON", "PRF", "APPR", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Worin dein Geist/ der uns zum guten treibt/", "tokens": ["Wo\u00b7rin", "dein", "Geist", "/", "der", "uns", "zum", "gu\u00b7ten", "treibt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$(", "PRELS", "PPER", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihm ewiglich mag eine Wohnung bauen.", "tokens": ["Ihm", "e\u00b7wig\u00b7lich", "mag", "ei\u00b7ne", "Woh\u00b7nung", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Steh du mir bey itzund und allezeit/", "tokens": ["Steh", "du", "mir", "bey", "it\u00b7zund", "und", "al\u00b7le\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lenck wie du wilst mein Tichten und Beginnen/", "tokens": ["Lenck", "wie", "du", "wilst", "mein", "Tich\u00b7ten", "und", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entz\u00fcnd ein Licht in meinen tr\u00fcben Sinnen/", "tokens": ["Ent\u00b7z\u00fcnd", "ein", "Licht", "in", "mei\u00b7nen", "tr\u00fc\u00b7ben", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das einen Glantz der Freude von sich streut.", "tokens": ["Das", "ei\u00b7nen", "Glantz", "der", "Freu\u00b7de", "von", "sich", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So wil ich denn mit Worten und der That/", "tokens": ["So", "wil", "ich", "denn", "mit", "Wor\u00b7ten", "und", "der", "That", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Herr/ dein Gesetz die rohen S\u00fcnder lehren/", "tokens": ["Herr", "/", "dein", "Ge\u00b7setz", "die", "ro\u00b7hen", "S\u00fcn\u00b7der", "leh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und was dein Arm an mir erwiesen hat/", "tokens": ["Und", "was", "dein", "Arm", "an", "mir", "er\u00b7wie\u00b7sen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wird kr\u00e4fftig seyn viel andre zu bekehren.", "tokens": ["Wird", "kr\u00e4ff\u00b7tig", "seyn", "viel", "and\u00b7re", "zu", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VAINF", "PIAT", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vergi\u00df nur erst die Blut-Schuld/ die mich dr\u00fcckt/", "tokens": ["Ver\u00b7gi\u00df", "nur", "erst", "die", "Blut\u00b7Schuld", "/", "die", "mich", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn soll dein Lob in aller We", "tokens": ["Denn", "soll", "dein", "Lob", "in", "al\u00b7ler", "We"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df ich dir mag reines Opffer bringen/", "tokens": ["Und", "da\u00df", "ich", "dir", "mag", "rei\u00b7nes", "Opf\u00b7fer", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VMFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So mache mich zu deinem Werck geschickt.", "tokens": ["So", "ma\u00b7che", "mich", "zu", "dei\u00b7nem", "Werck", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Halt mich zur\u00fcck von allem eitlen Schein;", "tokens": ["Halt", "mich", "zu\u00b7r\u00fcck", "von", "al\u00b7lem", "eit\u00b7len", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df mich nicht mehr durch schn\u00f6de Lust bewegen/", "tokens": ["La\u00df", "mich", "nicht", "mehr", "durch", "schn\u00f6\u00b7de", "Lust", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7"