{"dta.poem.1299": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jahr-Ged\u00e4chtnisse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Jahr/ O h\u00f6chster GOtt/ ist wieder bracht zu Ende/", "tokens": ["Ein", "Jahr", "/", "O", "h\u00f6chs\u00b7ter", "Gott", "/", "ist", "wie\u00b7der", "bracht", "zu", "En\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "ADJA", "NN", "$(", "VAFIN", "ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wiewohl nicht ohne Creutz und untermengte Pein/", "tokens": ["Wie\u00b7wohl", "nicht", "oh\u00b7ne", "Creutz", "und", "un\u00b7ter\u00b7meng\u00b7te", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch da\u00df ich kan gesund und unverletzet seyn/", "tokens": ["Doch", "da\u00df", "ich", "kan", "ge\u00b7sund", "und", "un\u00b7ver\u00b7let\u00b7zet", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist ein Genaden-Werck/ O Sch\u00f6pffer/ deiner H\u00e4nde/", "tokens": ["Ist", "ein", "Ge\u00b7na\u00b7den\u00b7\u00b7Werck", "/", "O", "Sch\u00f6pf\u00b7fer", "/", "dei\u00b7ner", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$(", "NE", "NE", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Darvor ich Himmel au der Lippen Opffer sende/", "tokens": ["Dar\u00b7vor", "ich", "Him\u00b7mel", "au", "der", "Lip\u00b7pen", "Opf\u00b7fer", "sen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "NE", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und stelle mich bey dir mit neuem Bitten ein:", "tokens": ["Und", "stel\u00b7le", "mich", "bey", "dir", "mit", "neu\u00b7em", "Bit\u00b7ten", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Gieb/ grosser Himmels-F\u00fcrst/ da\u00df dieses Tages Schein/", "tokens": ["Gieb", "/", "gros\u00b7ser", "Him\u00b7mels\u00b7F\u00fcrst", "/", "da\u00df", "die\u00b7ses", "Ta\u00b7ges", "Schein", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ADJA", "NN", "$(", "KOUS", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was n\u00fctzet/ zu mir her/ was schadet/ von mir wende.", "tokens": ["Was", "n\u00fct\u00b7zet", "/", "zu", "mir", "her", "/", "was", "scha\u00b7det", "/", "von", "mir", "wen\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "APPR", "PPER", "PTKVZ", "$(", "PWS", "VVFIN", "$(", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vergieb die schwere Schuld/ vergi\u00df der ernsten Rache/", "tokens": ["Ver\u00b7gieb", "die", "schwe\u00b7re", "Schuld", "/", "ver\u00b7gi\u00df", "der", "erns\u00b7ten", "Ra\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$(", "VVIMP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verleih/ im fall di\u00df Jahr mein Leben schliessen soll/", "tokens": ["Ver\u00b7leih", "/", "im", "fall", "di\u00df", "Jahr", "mein", "Le\u00b7ben", "schlies\u00b7sen", "soll", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPRART", "NN", "PDS", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ich auff dein Verdienst/ O JEsu/ sterbe wohl.", "tokens": ["Da\u00df", "ich", "auff", "dein", "Ver\u00b7dienst", "/", "O", "Je\u00b7su", "/", "ster\u00b7be", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$(", "NE", "NE", "$(", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Im fall mich deine Gnad in treuem Schutz erh\u00e4lt/", "tokens": ["Im", "fall", "mich", "dei\u00b7ne", "Gnad", "in", "treu\u00b7em", "Schutz", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So leb ich wohl begl\u00fcckt in der und jener Welt/", "tokens": ["So", "leb", "ich", "wohl", "be\u00b7gl\u00fcckt", "in", "der", "und", "je\u00b7ner", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "KON", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So schadet mir noch Tod/ noch Welt/ noch H\u00f6llen-Drache.", "tokens": ["So", "scha\u00b7det", "mir", "noch", "Tod", "/", "noch", "Welt", "/", "noch", "H\u00f6l\u00b7len\u00b7Dra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$(", "ADV", "NN", "$(", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}