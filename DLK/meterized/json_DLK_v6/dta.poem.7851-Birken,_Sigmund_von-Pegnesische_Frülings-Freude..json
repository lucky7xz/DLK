{"dta.poem.7851": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcnt/ wie ihr thut/ ihr Bl\u00e4tter!", "tokens": ["Gr\u00fcnt", "/", "wie", "ihr", "thut", "/", "ihr", "Bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "PPER", "VVFIN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zu kr\u00f6nen diese G\u00f6tter.", "tokens": ["zu", "kr\u00f6\u00b7nen", "die\u00b7se", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr hohe: Tugend-Wehrt/", "tokens": ["Ihr", "ho\u00b7he", ":", "Tu\u00b7gen\u00b7dWehrt", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$.", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "sey stets durch euch beehrt.", "tokens": ["sey", "stets", "durch", "euch", "be\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}