{"textgrid.poem.52546": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwebe, Mond, im tiefen Blau", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwebe, Mond, im tiefen Blau", "tokens": ["Schwe\u00b7be", ",", "Mond", ",", "im", "tie\u00b7fen", "Blau"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Berg und H\u00f6hn,", "tokens": ["Ue\u00b7ber", "Berg", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sprudle Wasser, blinke Thau!", "tokens": ["Sprud\u00b7le", "Was\u00b7ser", ",", "blin\u00b7ke", "Thau", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nacht, wie bist du sch\u00f6n!", "tokens": ["Nacht", ",", "wie", "bist", "du", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Spiegle See den reinen Strahl!", "tokens": ["Spieg\u00b7le", "See", "den", "rei\u00b7nen", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Friedeathmend lind", "tokens": ["Frie\u00b7de\u00b7ath\u00b7mend", "lind"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Durch das wiesenhelle Thal", "tokens": ["Durch", "das", "wie\u00b7sen\u00b7hel\u00b7le", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Walle, weicher Wind!", "tokens": ["Wal\u00b7le", ",", "wei\u00b7cher", "Wind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie durch einen Zauberschlag", "tokens": ["Wie", "durch", "ei\u00b7nen", "Zau\u00b7ber\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich umgestimmt", "tokens": ["Bin", "ich", "um\u00b7ge\u00b7stimmt"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Von Gedanken, die der Tag", "tokens": ["Von", "Ge\u00b7dan\u00b7ken", ",", "die", "der", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt und wieder nimmt.", "tokens": ["Bringt", "und", "wie\u00b7der", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df es auch ein Sterben gibt,", "tokens": ["Da\u00df", "es", "auch", "ein", "Ster\u00b7ben", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchl ich ohne Schmerz,", "tokens": ["F\u00fchl", "ich", "oh\u00b7ne", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was ich liebe, was mich liebt,", "tokens": ["Was", "ich", "lie\u00b7be", ",", "was", "mich", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht mir still durchs Herz.", "tokens": ["Geht", "mir", "still", "durchs", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}