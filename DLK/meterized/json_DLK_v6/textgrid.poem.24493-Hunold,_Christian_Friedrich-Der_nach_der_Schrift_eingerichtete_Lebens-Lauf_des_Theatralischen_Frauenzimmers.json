{"textgrid.poem.24493": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der nach der Schrift eingerichtete Lebens-Lauf des Theatralischen Frauenzimmers", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man wie offt geschicht/ bey euch ins Netze f\u00e4llt/", "tokens": ["Wenn", "man", "wie", "offt", "ge\u00b7schicht", "/", "bey", "euch", "ins", "Net\u00b7ze", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "ADV", "VVPP", "$(", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So denckt ihr an das Wort; seyd fruchtbar mehrt die Welt.", "tokens": ["So", "denckt", "ihr", "an", "das", "Wort", ";", "seyd", "frucht\u00b7bar", "mehrt", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "VAFIN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn ihr den K\u00fctzel habt unreiner Lust vertrieben/", "tokens": ["Wenn", "ihr", "den", "K\u00fct\u00b7zel", "habt", "un\u00b7rei\u00b7ner", "Lust", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das heist: den Nechsten so/ als wie sich selber lieben.", "tokens": ["Das", "heist", ":", "den", "Nechs\u00b7ten", "so", "/", "als", "wie", "sich", "sel\u00b7ber", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "ART", "NN", "ADV", "$(", "KOUS", "PWAV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr haltet aller Welt/ so Jung- als Alten still/", "tokens": ["Ihr", "hal\u00b7tet", "al\u00b7ler", "Welt", "/", "so", "Jung", "als", "Al\u00b7ten", "still", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "ADV", "TRUNC", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um andern das zu thun/ was man von ihnen will.", "tokens": ["Um", "an\u00b7dern", "das", "zu", "thun", "/", "was", "man", "von", "ih\u00b7nen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "PDS", "PTKZU", "VVINF", "$(", "PWS", "PIS", "APPR", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr thut es viel um Geld/ da\u00df ihr die Wollust \u00fcbet:", "tokens": ["Ihr", "thut", "es", "viel", "um", "Geld", "/", "da\u00df", "ihr", "die", "Wol\u00b7lust", "\u00fc\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil Juda Bock und Lohn auch seiner Huren giebet.", "tokens": ["Weil", "Ju\u00b7da", "Bock", "und", "Lohn", "auch", "sei\u00b7ner", "Hu\u00b7ren", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "KON", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr schlucket Tag und Nacht die geilsten Pillen ein:", "tokens": ["Ihr", "schluc\u00b7ket", "Tag", "und", "Nacht", "die", "geils\u00b7ten", "Pil\u00b7len", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Frauen Mutter soll ja uners\u00e4ttlich seyn.", "tokens": ["Der", "Frau\u00b7en", "Mut\u00b7ter", "soll", "ja", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn Euer Ehebruch dem Mann ein Horn erwirbet/", "tokens": ["Wenn", "Eu\u00b7er", "E\u00b7heb\u00b7ruch", "dem", "Mann", "ein", "Horn", "er\u00b7wir\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So seyd ihr ", "tokens": ["So", "seyd", "ihr"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Und endlich dencket ihr/ was selbst die Wei\u00dfheit spricht:", "tokens": ["Und", "end\u00b7lich", "den\u00b7cket", "ihr", "/", "was", "selbst", "die", "Wei\u00df\u00b7heit", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$(", "PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Mensch stirbt wie das Vieh und auffersteht auch nicht.", "tokens": ["Der", "Mensch", "stirbt", "wie", "das", "Vieh", "und", "auf\u00b7fer\u00b7steht", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "KON", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}