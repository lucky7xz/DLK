{"dta.poem.12727": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der leichen-stein.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die armen dr\u00fccket man, wenn sie das leben haben;", "tokens": ["Die", "ar\u00b7men", "dr\u00fc\u00b7cket", "man", ",", "wenn", "sie", "das", "le\u00b7ben", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PDS", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die reichen aber erst, nachdem sie schon begraben.", "tokens": ["Die", "rei\u00b7chen", "a\u00b7ber", "erst", ",", "nach\u00b7dem", "sie", "schon", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADV", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}