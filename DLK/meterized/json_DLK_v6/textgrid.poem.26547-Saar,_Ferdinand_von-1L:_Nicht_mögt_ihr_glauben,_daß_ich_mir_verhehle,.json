{"textgrid.poem.26547": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht m\u00f6gt ihr glauben, da\u00df ich mir verhehle,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht m\u00f6gt ihr glauben, da\u00df ich mir verhehle,", "tokens": ["Nicht", "m\u00f6gt", "ihr", "glau\u00b7ben", ",", "da\u00df", "ich", "mir", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft es widerspricht dem Geist der Zeiten;", "tokens": ["Wie", "oft", "es", "wi\u00b7der\u00b7spricht", "dem", "Geist", "der", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon will die Zukunft anders sich bereiten \u2013", "tokens": ["Schon", "will", "die", "Zu\u00b7kunft", "an\u00b7ders", "sich", "be\u00b7rei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und so sind auch die T\u00f6ne, die ich w\u00e4hle.", "tokens": ["Und", "so", "sind", "auch", "die", "T\u00f6\u00b7ne", ",", "die", "ich", "w\u00e4h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was auch der Ruhm der Gegenwart erz\u00e4hle,", "tokens": ["Was", "auch", "der", "Ruhm", "der", "Ge\u00b7gen\u00b7wart", "er\u00b7z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und welche Siege m\u00e4chtig sie begleiten:", "tokens": ["Und", "wel\u00b7che", "Sie\u00b7ge", "m\u00e4ch\u00b7tig", "sie", "be\u00b7glei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den inn'ren Zwiespalt k\u00f6nnt ihr nicht bestreiten \u2013", "tokens": ["Den", "inn'\u00b7ren", "Zwies\u00b7palt", "k\u00f6nnt", "ihr", "nicht", "be\u00b7strei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dieser Bruch, er ging durch meine Seele.", "tokens": ["Und", "die\u00b7ser", "Bruch", ",", "er", "ging", "durch", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch nicht blo\u00df ", "tokens": ["Doch", "nicht", "blo\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Was still und segensreich in euch erglommen:", "tokens": ["Was", "still", "und", "se\u00b7gens\u00b7reich", "in", "euch", "er\u00b7glom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch meine Saiten hat es auch gezittert.", "tokens": ["Durch", "mei\u00b7ne", "Sai\u00b7ten", "hat", "es", "auch", "ge\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Doch sie blieb taub, an Herz und Sinn zersplittert:", "tokens": ["Doch", "sie", "blieb", "taub", ",", "an", "Herz", "und", "Sinn", "zer\u00b7split\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich gab ihr Brod \u2013 sie hat's f\u00fcr Stein genommen.", "tokens": ["Ich", "gab", "ihr", "Brod", "\u2013", "sie", "hat's", "f\u00fcr", "Stein", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}