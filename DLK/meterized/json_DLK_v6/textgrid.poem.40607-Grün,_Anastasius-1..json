{"textgrid.poem.40607": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinen sp\u00e4ten Tagen", "tokens": ["In", "mei\u00b7nen", "sp\u00e4\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was treibst du, altes Herz?", "tokens": ["Was", "treibst", "du", ",", "al\u00b7tes", "Herz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was will dein tolles Schlagen,", "tokens": ["Was", "will", "dein", "tol\u00b7les", "Schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein wonnevoller Schmerz?", "tokens": ["Dein", "won\u00b7ne\u00b7vol\u00b7ler", "Schmerz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Maienthau, die Thr\u00e4nen,", "tokens": ["Der", "Mai\u00b7ent\u00b7hau", ",", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die du ins Aug' mir dr\u00e4ngst?", "tokens": ["Die", "du", "ins", "Aug'", "mir", "dr\u00e4ngst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was will die\u00df Fr\u00fchlingssehnen,", "tokens": ["Was", "will", "die\u00df", "Fr\u00fch\u00b7lings\u00b7seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da Herbst es worden l\u00e4ngst?", "tokens": ["Da", "Herbst", "es", "wor\u00b7den", "l\u00e4ngst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAPP", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verstummt sind alle Lieder,", "tokens": ["Ver\u00b7stummt", "sind", "al\u00b7le", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die W\u00e4lder stehn entlaubt,", "tokens": ["Die", "W\u00e4l\u00b7der", "stehn", "ent\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schneeflocken rieseln nieder", "tokens": ["Schnee\u00b7flo\u00b7cken", "rie\u00b7seln", "nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Aufs Feld und auf mein Haupt.", "tokens": ["Aufs", "Feld", "und", "auf", "mein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gew\u00f6lke schwer und bleiern", "tokens": ["Ge\u00b7w\u00f6l\u00b7ke", "schwer", "und", "blei\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im kalten Luftrevier,", "tokens": ["Im", "kal\u00b7ten", "Luf\u00b7tre\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Thal in Nebelschleiern, \u2013", "tokens": ["Das", "Thal", "in", "Ne\u00b7bel\u00b7schlei\u00b7ern", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz, wie steht's in dir?", "tokens": ["Mein", "Herz", ",", "wie", "steht's", "in", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Sommerf\u00e4den wiegen", "tokens": ["Die", "Som\u00b7mer\u00b7f\u00e4\u00b7den", "wie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zerrissen sich im Raum;", "tokens": ["Zer\u00b7ris\u00b7sen", "sich", "im", "Raum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist als s\u00e4h' ich fliegen", "tokens": ["Mir", "ist", "als", "s\u00e4h'", "ich", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von einst den eignen Traum.", "tokens": ["Von", "einst", "den", "eig\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Schwalben mu\u00dften wandern", "tokens": ["Die", "Schwal\u00b7ben", "mu\u00df\u00b7ten", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und all mein Hoffen auch,", "tokens": ["Und", "all", "mein", "Hof\u00b7fen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbla\u00dft ist mit dem andern", "tokens": ["Ver\u00b7bla\u00dft", "ist", "mit", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Gr\u00fcn im Windeshauch.", "tokens": ["Mein", "Gr\u00fcn", "im", "Win\u00b7des\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}