{"textgrid.poem.57677": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Es singt der Star ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es singt der Star, die Sonne lacht,", "tokens": ["Es", "singt", "der", "Star", ",", "die", "Son\u00b7ne", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Bl\u00fctenschmuck die B\u00e4ume stehn,", "tokens": ["Im", "Bl\u00fc\u00b7ten\u00b7schmuck", "die", "B\u00e4u\u00b7me", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tag ist hin und eine Nacht,", "tokens": ["Ein", "Tag", "ist", "hin", "und", "ei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seitdem ich dich nicht hab' gesehn.", "tokens": ["Seit\u00b7dem", "ich", "dich", "nicht", "hab'", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Himmel ist so hoch und blau,", "tokens": ["Der", "Him\u00b7mel", "ist", "so", "hoch", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde tr\u00e4gt ihr Hochzeitskleid,", "tokens": ["Die", "Er\u00b7de", "tr\u00e4gt", "ihr", "Hoch\u00b7zeits\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe alles grau in grau,", "tokens": ["Ich", "se\u00b7he", "al\u00b7les", "grau", "in", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich friert in meiner Einsamkeit.", "tokens": ["Mich", "friert", "in", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich friert in meiner schwarzen Nacht,", "tokens": ["Mich", "friert", "in", "mei\u00b7ner", "schwar\u00b7zen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe keinen Sonnenschein,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7nen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann geht die Sonne auf voll Pracht,", "tokens": ["Wann", "geht", "die", "Son\u00b7ne", "auf", "voll", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann wirst du wieder bei mir sein?", "tokens": ["Wann", "wirst", "du", "wie\u00b7der", "bei", "mir", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}