{"dta.poem.6973": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie aber, es wird ja die Seele doch ger\u00fchret", "tokens": ["Wie", "a\u00b7ber", ",", "es", "wird", "ja", "die", "See\u00b7le", "doch", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Durch Schmertzen, die der Leib versp\u00fcret!", "tokens": ["Durch", "Schmert\u00b7zen", ",", "die", "der", "Leib", "ver\u00b7sp\u00fc\u00b7ret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wird sie doch mit seiner Schw\u00e4ch\u2019 und Pein,", "tokens": ["Wie", "wird", "sie", "doch", "mit", "sei\u00b7ner", "Schw\u00e4ch'", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verwickelt und mit fortgerissen?", "tokens": ["Ver\u00b7wi\u00b7ckelt", "und", "mit", "fort\u00b7ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man untersucht, man wil die Ursach wissen,", "tokens": ["Man", "un\u00b7ter\u00b7sucht", ",", "man", "wil", "die", "Ur\u00b7sach", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf welche Weise sie so fest verbunden seyn.", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sie", "so", "fest", "ver\u00b7bun\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer w\u00fcrckt die Mischungen, wer wei\u00df auf welche Art", "tokens": ["Wer", "w\u00fcrckt", "die", "Misc\u00b7hun\u00b7gen", ",", "wer", "wei\u00df", "auf", "wel\u00b7che", "Art"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "APPR", "PWAT", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Mit der Materie der Geist verbunden ward?", "tokens": ["Mit", "der", "Ma\u00b7te\u00b7rie", "der", "Geist", "ver\u00b7bun\u00b7den", "ward", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}