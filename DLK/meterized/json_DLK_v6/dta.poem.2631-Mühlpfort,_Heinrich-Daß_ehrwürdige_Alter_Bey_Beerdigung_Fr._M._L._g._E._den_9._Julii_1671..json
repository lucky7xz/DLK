{"dta.poem.2631": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df ehrw\u00fcrdige Alter/  \n Bey Beerdigung Fr. M. L. g. E. den 9.  \n Julii 1671.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das ungemenschte Bolck/ die rauhen Massageten/", "tokens": ["Das", "un\u00b7ge\u00b7menschte", "Bolck", "/", "die", "rau\u00b7hen", "Mas\u00b7sa\u00b7ge\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So theils M\u00f6otis Pful/ theils Tanais umschr\u00e4nckt/", "tokens": ["So", "theils", "M\u00f6o\u00b7tis", "Pful", "/", "theils", "Ta\u00b7nais", "um\u00b7schr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$(", "ADV", "NN", "VVPP", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mag aus ergrimmtem Ha\u00df sein\u2019 alte Greisen t\u00f6dten;", "tokens": ["Mag", "aus", "er\u00b7grimm\u00b7tem", "Ha\u00df", "sein'", "al\u00b7te", "Grei\u00b7sen", "t\u00f6d\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob es die ", "tokens": ["Ob", "es", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df ein verlebter Leib nicht zu Gesch\u00e4fften n\u00fctze/", "tokens": ["Da\u00df", "ein", "ver\u00b7leb\u00b7ter", "Leib", "nicht", "zu", "Ge\u00b7sch\u00e4ff\u00b7ten", "n\u00fct\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ein krafftloser Arm nicht ferner dienen kan/", "tokens": ["Da\u00df", "ein", "krafft\u00b7lo\u00b7ser", "Arm", "nicht", "fer\u00b7ner", "die\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df ein betagter Mann mehr bey dem Ofen sitze/", "tokens": ["Da\u00df", "ein", "be\u00b7tag\u00b7ter", "Mann", "mehr", "bey", "dem", "O\u00b7fen", "sit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als er zu Felde geht/ und legt den Harnisch an.", "tokens": ["Als", "er", "zu", "Fel\u00b7de", "geht", "/", "und", "legt", "den", "Har\u00b7nisch", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So laufft doch der Natur di\u00df Rasen gantz zuwider.", "tokens": ["So", "laufft", "doch", "der", "Na\u00b7tur", "di\u00df", "Ra\u00b7sen", "gantz", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PDS", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ists m\u00f6glich da\u00df ein Mensch den andern Menschen schlacht?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "ein", "Mensch", "den", "an\u00b7dern", "Men\u00b7schen", "schlacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und j\u00e4mmerlich zermetzt sein eigen Fleisch und Glieder?", "tokens": ["Und", "j\u00e4m\u00b7mer\u00b7lich", "zer\u00b7metzt", "sein", "ei\u00b7gen", "Fleisch", "und", "Glie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAINF", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja oft den jenen Leib so ihn zur Welt gebracht?", "tokens": ["Ja", "oft", "den", "je\u00b7nen", "Leib", "so", "ihn", "zur", "Welt", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "PDAT", "NN", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr wilden Scythen geht mit den verdammten Br\u00e4uchen/", "tokens": ["Ihr", "wil\u00b7den", "Scyt\u00b7hen", "geht", "mit", "den", "ver\u00b7damm\u00b7ten", "Br\u00e4u\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Mit dem verfluchten Neid/ der auf das Alter f\u00e4llt.", "tokens": ["Mit", "dem", "ver\u00b7fluch\u00b7ten", "Neid", "/", "der", "auf", "das", "Al\u00b7ter", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein Anblick grauer Haar/ und derer Ehren-Zeichen", "tokens": ["Ein", "An\u00b7blick", "grau\u00b7er", "Haar", "/", "und", "de\u00b7rer", "Eh\u00b7ren\u00b7Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KON", "PDS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hat gar was Herrlichers in unserm Sinn bestellt.", "tokens": ["Hat", "gar", "was", "Herr\u00b7li\u00b7chers", "in", "un\u00b7serm", "Sinn", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Pflantzt ein beschneites Haupt und ernstliches Gesichte", "tokens": ["Pflantzt", "ein", "be\u00b7schnei\u00b7tes", "Haupt", "und", "ernst\u00b7li\u00b7ches", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der rohen Jugend nicht Pflicht und Gehorsam ein?", "tokens": ["Der", "ro\u00b7hen", "Ju\u00b7gend", "nicht", "Pflicht", "und", "Ge\u00b7hor\u00b7sam", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Weist nicht der Jahre Schnee der Wei\u00dfheit reiffe Fruchte?", "tokens": ["Weist", "nicht", "der", "Jah\u00b7re", "Schnee", "der", "Wei\u00df\u00b7heit", "reif\u00b7fe", "Fruch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und bey Betagten mu\u00df auch viel Erfahrung seyn.", "tokens": ["Und", "bey", "Be\u00b7tag\u00b7ten", "mu\u00df", "auch", "viel", "Er\u00b7fah\u00b7rung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des Menschen Seele f\u00e4ngt alsdenn erst an zu leben/", "tokens": ["Des", "Men\u00b7schen", "See\u00b7le", "f\u00e4ngt", "als\u00b7denn", "erst", "an", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ADV", "APPR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Je mehr der m\u00fcrbe Leib sich zu dem Grabe neigt;", "tokens": ["Je", "mehr", "der", "m\u00fcr\u00b7be", "Leib", "sich", "zu", "dem", "Gra\u00b7be", "neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ist von den Reitzungen und L\u00fcsten nicht umbgeben/", "tokens": ["Ist", "von", "den", "Reit\u00b7zun\u00b7gen", "und", "L\u00fcs\u00b7ten", "nicht", "umb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "KON", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Au\u00df derer Keim und Kern nur lauter Unfall steigt.", "tokens": ["Au\u00df", "de\u00b7rer", "Keim", "und", "Kern", "nur", "lau\u00b7ter", "Un\u00b7fall", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "KON", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Da vor der Jahre May vom warmen Blut erhitzet/", "tokens": ["Da", "vor", "der", "Jah\u00b7re", "May", "vom", "war\u00b7men", "Blut", "er\u00b7hit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Hat hier des Alters Frost die Flammen abgek\u00fchlt/", "tokens": ["Hat", "hier", "des", "Al\u00b7ters", "Frost", "die", "Flam\u00b7men", "ab\u00b7ge\u00b7k\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da in Begierden vor das geile Fleisch geschwitzet/", "tokens": ["Da", "in", "Be\u00b7gier\u00b7den", "vor", "das", "gei\u00b7le", "Fleisch", "ge\u00b7schwit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Verzehrt es hier die Zeit da\u00df nichts dergleichen f\u00fchlt.", "tokens": ["Ver\u00b7zehrt", "es", "hier", "die", "Zeit", "da\u00df", "nichts", "derg\u00b7lei\u00b7chen", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "KOUS", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Zu dem verlescht auch nicht des Geistes Feuer-Funcken/", "tokens": ["Zu", "dem", "ver\u00b7lescht", "auch", "nicht", "des", "Geis\u00b7tes", "Feu\u00b7er\u00b7Fun\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Asche grauer Haar/ so sie vielmehr ernehrt.", "tokens": ["Die", "A\u00b7sche", "grau\u00b7er", "Haar", "/", "so", "sie", "viel\u00b7mehr", "er\u00b7nehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Seele/ so vorhin von dem Gebl\u00fcte truncken/", "tokens": ["Die", "See\u00b7le", "/", "so", "vor\u00b7hin", "von", "dem", "Ge\u00b7bl\u00fc\u00b7te", "trun\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wird gleichsam n\u00fcchtern da/ und bleibet unbeschwert.", "tokens": ["Wird", "gleich\u00b7sam", "n\u00fcch\u00b7tern", "da", "/", "und", "blei\u00b7bet", "un\u00b7be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "ADV", "$(", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Verschwind des Leibes Krafft/ so w\u00e4chst der Klugheit St\u00e4rcke/", "tokens": ["Ver\u00b7schwind", "des", "Lei\u00b7bes", "Krafft", "/", "so", "w\u00e4chst", "der", "Klug\u00b7heit", "St\u00e4r\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verbl\u00fchn die Glieder schon/ so gr\u00fcnet der Verstand.", "tokens": ["Ver\u00b7bl\u00fchn", "die", "Glie\u00b7der", "schon", "/", "so", "gr\u00fc\u00b7net", "der", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "That nicht Themistocles im Alter Wunder-Wercke?", "tokens": ["That", "nicht", "The\u00b7mi\u00b7sto\u00b7cles", "im", "Al\u00b7ter", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NE", "APPRART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Lebt er nicht hundert Jahr an Sinnen unverwand?", "tokens": ["Lebt", "er", "nicht", "hun\u00b7dert", "Jahr", "an", "Sin\u00b7nen", "un\u00b7ver\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "CARD", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und alte Leute sind recht himmlische Sibyllen/", "tokens": ["Und", "al\u00b7te", "Leu\u00b7te", "sind", "recht", "himm\u00b7li\u00b7sche", "Si\u00b7byl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die ins Verborgne sehn/ weissagen was ergeht/", "tokens": ["Die", "ins", "Ver\u00b7borg\u00b7ne", "sehn", "/", "weis\u00b7sa\u00b7gen", "was", "er\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$(", "VVFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die den Oraculn gleich die Welt mit Wei\u00dfheit f\u00fcllen/", "tokens": ["Die", "den", "O\u00b7ra\u00b7culn", "gleich", "die", "Welt", "mit", "Wei\u00df\u00b7heit", "f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.40": {"text": "Und lehren was da wol/ und was da \u00fcbel steht.", "tokens": ["Und", "leh\u00b7ren", "was", "da", "wol", "/", "und", "was", "da", "\u00fc\u00b7bel", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PWS", "ADV", "ADV", "$(", "KON", "PWS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Lie\u00df uns das Alterthum nicht Sitten und Gesetze?", "tokens": ["Lie\u00df", "uns", "das", "Al\u00b7ter\u00b7thum", "nicht", "Sit\u00b7ten", "und", "Ge\u00b7set\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Hat der begraute Witz nicht L\u00e4nder auferbaut?", "tokens": ["Hat",