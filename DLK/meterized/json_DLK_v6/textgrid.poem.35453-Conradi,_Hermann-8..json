{"textgrid.poem.35453": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat um mich die Einsamkeit", "tokens": ["Es", "hat", "um", "mich", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebreitet ihrer Schatten F\u00fclle ...", "tokens": ["Ge\u00b7brei\u00b7tet", "ih\u00b7rer", "Schat\u00b7ten", "F\u00fcl\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und doppelt f\u00fchlbar wird mein Leid", "tokens": ["Und", "dop\u00b7pelt", "f\u00fchl\u00b7bar", "wird", "mein", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Inmitten dieser tiefen Stille ...", "tokens": ["In\u00b7mit\u00b7ten", "die\u00b7ser", "tie\u00b7fen", "Stil\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Welt und Menschen abgetrennt,", "tokens": ["Von", "Welt", "und", "Men\u00b7schen", "ab\u00b7ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sp\u00fcr' ich, wie sich mein Schmerz verdichtet \u2013", "tokens": ["Sp\u00fcr'", "ich", ",", "wie", "sich", "mein", "Schmerz", "ver\u00b7dich\u00b7tet", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PRF", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich schlie\u00dft zu ", "tokens": ["Sich", "schlie\u00dft", "zu"], "token_info": ["word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In dem sich ", "tokens": ["In", "dem", "sich"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "PRF"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und um mich sch\u00fcrzt sich's wie ein Netz", "tokens": ["Und", "um", "mich", "sch\u00fcrzt", "sich's", "wie", "ein", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und engt sich immer mehr zusammen:", "tokens": ["Und", "engt", "sich", "im\u00b7mer", "mehr", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}