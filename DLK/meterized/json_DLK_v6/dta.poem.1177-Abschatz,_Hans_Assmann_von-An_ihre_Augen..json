{"dta.poem.1177": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "An ihre Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Augen/ die ich lieb und ehr/", "tokens": ["Ihr", "Au\u00b7gen", "/", "die", "ich", "lieb", "und", "ehr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr meine Lust und s\u00fcsse Pein/", "tokens": ["Ihr", "mei\u00b7ne", "Lust", "und", "s\u00fcs\u00b7se", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was netzet ihr die tr\u00fcben Wangen/", "tokens": ["Was", "net\u00b7zet", "ihr", "die", "tr\u00fc\u00b7ben", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was sagt mir euer blasser Schein?", "tokens": ["Was", "sagt", "mir", "eu\u00b7er", "blas\u00b7ser", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Habt ihr mein Hertze nicht empfangen?", "tokens": ["Habt", "ihr", "mein", "Hert\u00b7ze", "nicht", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was fodert/ was verlangt ihr mehr?", "tokens": ["Was", "fo\u00b7dert", "/", "was", "ver\u00b7langt", "ihr", "mehr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Augen/ die ich lieb und ehr/", "tokens": ["Ihr", "Au\u00b7gen", "/", "die", "ich", "lieb", "und", "ehr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sehet meine Schmertzen an/", "tokens": ["Ihr", "se\u00b7het", "mei\u00b7ne", "Schmert\u00b7zen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kennt die Menge meiner Plagen:", "tokens": ["Und", "kennt", "die", "Men\u00b7ge", "mei\u00b7ner", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wofern ich euch vergn\u00fcgen kan/", "tokens": ["Wo\u00b7fern", "ich", "euch", "ver\u00b7gn\u00fc\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will ich mit Lust den Tod ertragen.", "tokens": ["Will", "ich", "mit", "Lust", "den", "Tod", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$."], "met