{"textgrid.poem.54449": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "An ein\u1ebd seiner Bekanten/ welcher sich in vnzeitige Ehe eingelassen", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du mein ", "tokens": ["Da\u00df", "du", "mein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Durch Weiber List verf\u00fchrt; auch aller Einsamkeit/", "tokens": ["Durch", "Wei\u00b7ber", "List", "ver\u00b7f\u00fchrt", ";", "auch", "al\u00b7ler", "Ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "VVPP", "$.", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd Keuschheit abgesagt/ im Lentzen deiner Zeit", "tokens": ["Vnd", "Keuschheit", "ab\u00b7ge\u00b7sagt", "/", "im", "Lent\u00b7zen", "dei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVPP", "$(", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Vnd dich durch schn\u00f6de Lust/ v\u00f1 Gelt beth\u00f6ren lassen;", "tokens": ["Vnd", "dich", "durch", "schn\u00f6\u00b7de", "Lust", "/", "v\u00f1", "Gelt", "be\u00b7th\u00f6\u00b7ren", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$(", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Meinst Du sey wolgethan/ ja da\u00df auff allen Gassen/", "tokens": ["Meinst", "Du", "sey", "wol\u00b7ge\u00b7than", "/", "ja", "da\u00df", "auff", "al\u00b7len", "Gas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "ADJD", "$(", "ADV", "KOUS", "APPR", "PIAT", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ein jeder nur von dir vnd deinem Gl\u00fccke schreyt;", "tokens": ["Ein", "je\u00b7der", "nur", "von", "dir", "vnd", "dei\u00b7nem", "Gl\u00fc\u00b7cke", "schreyt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schaw da\u00df dirs nicht zu bald von Hertzen werde Leid/", "tokens": ["Schaw", "da\u00df", "dirs", "nicht", "zu", "bald", "von", "Hert\u00b7zen", "wer\u00b7de", "Leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "PTKNEG", "PTKA", "ADV", "APPR", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df dein verblender Geist/ sich jtzt so l\u00e4st anfassen.", "tokens": ["Da\u00df", "dein", "ver\u00b7blen\u00b7der", "Geist", "/", "sich", "jtzt", "so", "l\u00e4st", "an\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$(", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was deucht dich/ wenn hernach dir deine Braut vorsingt/", "tokens": ["Was", "deucht", "dich", "/", "wenn", "her\u00b7nach", "dir", "dei\u00b7ne", "Braut", "vor\u00b7singt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das di\u00df Ihr Teller sey/ der auff dem Tische klingt;", "tokens": ["Das", "di\u00df", "Ihr", "Tel\u00b7ler", "sey", "/", "der", "auff", "dem", "Ti\u00b7sche", "klingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDS", "PPOSAT", "NN", "VAFIN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn nicht ihr Tahler wer/ so must du Hunger sterben/", "tokens": ["Wenn", "nicht", "ihr", "Tah\u00b7ler", "wer", "/", "so", "must", "du", "Hun\u00b7ger", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "PWS", "$(", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie wird dir seyn zu Mut? du meinst wie m\u00f6chts geschehn?", "tokens": ["Wie", "wird", "dir", "seyn", "zu", "Mut", "?", "du", "meinst", "wie", "m\u00f6chts", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "APPR", "NN", "$.", "PPER", "ADV", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie liebt mich viel zu sehr/ sonst pflegets so zu gehn/", "tokens": ["Sie", "liebt", "mich", "viel", "zu", "sehr", "/", "sonst", "pfle\u00b7gets", "so", "zu", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$(", "ADV", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn nicht ein Ehman kan sein eigen Brod erwerben.", "tokens": ["Wenn", "nicht", "ein", "Eh\u00b7man", "kan", "sein", "ei\u00b7gen", "Brod", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VMFIN", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}