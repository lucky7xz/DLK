{"textgrid.poem.48058": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber den brennenden Meeressand", "tokens": ["Ue\u00b7ber", "den", "bren\u00b7nen\u00b7den", "Mee\u00b7res\u00b7sand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "sind wir beide geschritten,", "tokens": ["sind", "wir", "bei\u00b7de", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "als mir dein trotziger Mund gestand,", "tokens": ["als", "mir", "dein", "trot\u00b7zi\u00b7ger", "Mund", "ge\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "was du erlebt und erlitten.", "tokens": ["was", "du", "er\u00b7lebt", "und", "er\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Weithin lachender Sonnenschein,", "tokens": ["Weit\u00b7hin", "la\u00b7chen\u00b7der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "duftverschleierte Fluren . . .", "tokens": ["duft\u00b7ver\u00b7schlei\u00b7er\u00b7te", "Flu\u00b7ren", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$.", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "sahen denn unsere Augen allein", "tokens": ["sa\u00b7hen", "denn", "un\u00b7se\u00b7re", "Au\u00b7gen", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "PPOSAT", "NN", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "rings des Verderbens Spuren?", "tokens": ["rings", "des", "Ver\u00b7der\u00b7bens", "Spu\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Welk am Stengel, duftlos und matt,", "tokens": ["Welk", "am", "Sten\u00b7gel", ",", "duft\u00b7los", "und", "matt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "hing die Rose, die s\u00fc\u00dfe;", "tokens": ["hing", "die", "Ro\u00b7se", ",", "die", "s\u00fc\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "ein vertrocknetes Lindenblatt", "tokens": ["ein", "ver\u00b7trock\u00b7ne\u00b7tes", "Lin\u00b7den\u00b7blatt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "warf uns der Wind vor die F\u00fc\u00dfe.", "tokens": ["warf", "uns", "der", "Wind", "vor", "die", "F\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Glanzumflossen und ohne Schmerz", "tokens": ["Glan\u00b7zum\u00b7flos\u00b7sen", "und", "oh\u00b7ne", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "nahte des Sommers Scheiden \u2013", "tokens": ["nah\u00b7te", "des", "Som\u00b7mers", "Schei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "du nur, zuckendes Menschenherz,", "tokens": ["du", "nur", ",", "zu\u00b7cken\u00b7des", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "du mu\u00dft leiden, mu\u00dft leiden! \u2013", "tokens": ["du", "mu\u00dft", "lei\u00b7den", ",", "mu\u00dft", "lei\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Seine Bl\u00fcte hat jeder Strauch,", "tokens": ["Sei\u00b7ne", "Bl\u00fc\u00b7te", "hat", "je\u00b7der", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Fr\u00fcchte der Baum getragen \u2013", "tokens": ["Fr\u00fcch\u00b7te", "der", "Baum", "ge\u00b7tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "du nur, ringender Menschengeist,", "tokens": ["du", "nur", ",", "rin\u00b7gen\u00b7der", "Men\u00b7schen\u00b7geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "mu\u00dft entsagen \u2013 entsagen!", "tokens": ["mu\u00dft", "ent\u00b7sa\u00b7gen", "\u2013", "ent\u00b7sa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVINF", "$(", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Ueber den brennenden Meeressand", "tokens": ["Ue\u00b7ber", "den", "bren\u00b7nen\u00b7den", "Mee\u00b7res\u00b7sand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "sind wir beide geschritten,", "tokens": ["sind", "wir", "bei\u00b7de", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "als mir dein trotziger Mund gestand,", "tokens": ["als", "mir", "dein", "trot\u00b7zi\u00b7ger", "Mund", "ge\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "wie du geliebt und gelitten.", "tokens": ["wie", "du", "ge\u00b7liebt", "und", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}