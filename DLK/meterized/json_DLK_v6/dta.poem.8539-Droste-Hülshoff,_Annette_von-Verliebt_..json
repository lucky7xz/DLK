{"dta.poem.8539": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Verliebt .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mutter, l\u00f6se die Spangen mir!", "tokens": ["Mut\u00b7ter", ",", "l\u00f6\u00b7se", "die", "Span\u00b7gen", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mich hat ein Fieber befallen,", "tokens": ["Mich", "hat", "ein", "Fie\u00b7ber", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn das Fenster lie\u00dfest du auf,", "tokens": ["Denn", "das", "Fens\u00b7ter", "lie\u00b7\u00dfest", "du", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das immer sorglich verh\u00e4ngte;", "tokens": ["Das", "im\u00b7mer", "sorg\u00b7lich", "ver\u00b7h\u00e4ng\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und im Garten ich M\u00e4dchen sah,", "tokens": ["Und", "im", "Gar\u00b7ten", "ich", "M\u00e4d\u00b7chen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die warfen Ringe im Kreise,", "tokens": ["Die", "war\u00b7fen", "Rin\u00b7ge", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Flatternd selber, ein Bl\u00fctenschnee,", "tokens": ["Flat\u00b7ternd", "sel\u00b7ber", ",", "ein", "Bl\u00fc\u00b7ten\u00b7schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Vom leichten Winde getragen.", "tokens": ["Vom", "leich\u00b7ten", "Win\u00b7de", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Immer fl\u00f6ten nur Stimmen mir,", "tokens": ["Im\u00b7mer", "fl\u00f6\u00b7ten", "nur", "Stim\u00b7men", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Und immer Spiegel mir flirren,", "tokens": ["Und", "im\u00b7mer", "Spie\u00b7gel", "mir", "flir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Blind geworden bin ich schon ganz,", "tokens": ["Blind", "ge\u00b7wor\u00b7den", "bin", "ich", "schon", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAPP", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Taub werd\u2019 ich n\u00e4chstens werden,", "tokens": ["Taub", "werd'", "ich", "n\u00e4chs\u00b7tens", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mutter, l\u00f6se die Spangen mir!", "tokens": ["Mut\u00b7ter", ",", "l\u00f6\u00b7se", "die", "Span\u00b7gen", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Mich hat ein Fieber befallen.", "tokens": ["Mich", "hat", "ein", "Fie\u00b7ber", "be\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}