{"textgrid.poem.39037": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Rom", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch f\u00fchl' ich der theuren Schwester brennende Thr\u00e4ne,", "tokens": ["Noch", "f\u00fchl'", "ich", "der", "theu\u00b7ren", "Schwes\u00b7ter", "bren\u00b7nen\u00b7de", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und den liebenden Druck des z\u00e4rtlichen Bruders,", "tokens": ["Und", "den", "lie\u00b7ben\u00b7den", "Druck", "des", "z\u00e4rt\u00b7li\u00b7chen", "Bru\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nacht umf\u00e4ngt mich und bringt den Freunden", "tokens": ["Nacht", "um\u00b7f\u00e4ngt", "mich", "und", "bringt", "den", "Freun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die tiefe Bewegung meiner Brust.", "tokens": ["Die", "tie\u00b7fe", "Be\u00b7we\u00b7gung", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So war denn auch diese Lebens-Aera geschlossen?", "tokens": ["So", "war", "denn", "auch", "die\u00b7se", "Le\u00b7bens\u00b7Ae\u00b7ra", "ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Brause nur Rom, mit deinen Brunnen,", "tokens": ["Brau\u00b7se", "nur", "Rom", ",", "mit", "dei\u00b7nen", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wie Schluchzen klingt es mir her\u00fcber,", "tokens": ["Wie", "Schluch\u00b7zen", "klingt", "es", "mir", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da vernehm' ich selbst das Donnern", "tokens": ["Da", "ver\u00b7nehm'", "ich", "selbst", "das", "Don\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Der gro\u00dfen Fontana di Trevi,", "tokens": ["Der", "gro\u00b7\u00dfen", "Fon\u00b7ta\u00b7na", "di", "Tre\u00b7vi", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bei der ich oft in N\u00e4chten verweilt,", "tokens": ["Bei", "der", "ich", "oft", "in", "N\u00e4ch\u00b7ten", "ver\u00b7weilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Der ich mich viel am Tage gefreut,", "tokens": ["Der", "ich", "mich", "viel", "am", "Ta\u00b7ge", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Lebt wohl, ihr Pl\u00e4tze, ihr S\u00e4ulen,", "tokens": ["Lebt", "wohl", ",", "ihr", "Pl\u00e4t\u00b7ze", ",", "ihr", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Du gro\u00dfes verst\u00f6rtes Haus, jetzt Heiligthum,", "tokens": ["Du", "gro\u00b7\u00dfes", "ver\u00b7st\u00f6r\u00b7tes", "Haus", ",", "jetzt", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Du Coliseum, das ich noch j\u00fcngst", "tokens": ["Du", "Co\u00b7li\u00b7se\u00b7um", ",", "das", "ich", "noch", "j\u00fcngst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.11": {"text": "Beim Glanz des Vollmonds durchschritten,", "tokens": ["Beim", "Glanz", "des", "Voll\u00b7monds", "durch\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Deine Gew\u00f6lbe besucht, als die Freunde", "tokens": ["Dei\u00b7ne", "Ge\u00b7w\u00f6l\u00b7be", "be\u00b7sucht", ",", "als", "die", "Freun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.13": {"text": "Ueberkletternd den Eremiten weckten.", "tokens": ["Ue\u00b7ber\u00b7klet\u00b7ternd", "den", "E\u00b7re\u00b7mi\u00b7ten", "weck\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du Sankt Peter, nie seh' ich dich wieder,", "tokens": ["Du", "Sankt", "Pe\u00b7ter", ",", "nie", "seh'", "ich", "dich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Edler stets, und gr\u00f6\u00dfer, majest\u00e4tischer und heiliger,", "tokens": ["Ed\u00b7ler", "stets", ",", "und", "gr\u00f6\u00b7\u00dfer", ",", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7scher", "und", "hei\u00b7li\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KON", "ADJD", "$,", "ADJA", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber auch erfreulicher, behaglicher, umf\u00e4ngst du den Wandrer,", "tokens": ["A\u00b7ber", "auch", "er\u00b7freu\u00b7li\u00b7cher", ",", "be\u00b7hag\u00b7li\u00b7cher", ",", "um\u00b7f\u00e4ngst", "du", "den", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+---+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Je \u00f6fter er deinen k\u00f6niglichen Raum besucht.", "tokens": ["Je", "\u00f6f\u00b7ter", "er", "dei\u00b7nen", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Raum", "be\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Schon sind wir durch das Thor. \u2013", "tokens": ["Schon", "sind", "wir", "durch", "das", "Thor", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da denk' ich des Vatikans", "tokens": ["Da", "denk'", "ich", "des", "Va\u00b7ti\u00b7kans"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und der g\u00f6ttlichen Dichtungen Rafaels,", "tokens": ["Und", "der", "g\u00f6tt\u00b7li\u00b7chen", "Dich\u00b7tun\u00b7gen", "Ra\u00b7faels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der erhabnen Sistina,", "tokens": ["Der", "er\u00b7hab\u00b7nen", "Sis\u00b7ti\u00b7na", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und auch des heimlichen St\u00fcbchens oben,", "tokens": ["Und", "auch", "des", "heim\u00b7li\u00b7chen", "St\u00fcb\u00b7chens", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wo in der stillen Einsamkeit", "tokens": ["Wo", "in", "der", "stil\u00b7len", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich die Pergamente las und in Lust mir vieles schrieb,", "tokens": ["Ich", "die", "Per\u00b7ga\u00b7men\u00b7te", "las", "und", "in", "Lust", "mir", "vie\u00b7les", "schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "KON", "APPR", "NN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Inde\u00df durch die hei\u00dfe ruhende Luft", "tokens": ["In\u00b7de\u00df", "durch", "die", "hei\u00b7\u00dfe", "ru\u00b7hen\u00b7de", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Ein ferner Ambos und Hammer lieblich erklang.", "tokens": ["Ein", "fer\u00b7ner", "Am\u00b7bos", "und", "Ham\u00b7mer", "lieb\u00b7lich", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie oft sah ich dann r\u00fcckkehrend die G\u00f6tterbilder,", "tokens": ["Wie", "oft", "sah", "ich", "dann", "r\u00fcck\u00b7keh\u00b7rend", "die", "G\u00f6t\u00b7ter\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und die freundlichen Logen. \u2013", "tokens": ["Und", "die", "freund\u00b7li\u00b7chen", "Lo\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Alles versinkt jetzt hinter mir:", "tokens": ["Al\u00b7les", "ver\u00b7sinkt", "jetzt", "hin\u00b7ter", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Noch gl\u00e4nzt im innern Auge das farnesische Gartenhaus,", "tokens": ["Noch", "gl\u00e4nzt", "im", "in\u00b7nern", "Au\u00b7ge", "das", "far\u00b7ne\u00b7si\u00b7sche", "Gar\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die Blumendichtung von Amor und Psyche,", "tokens": ["Die", "Blu\u00b7men\u00b7dich\u00b7tung", "von", "A\u00b7mor", "und", "Psy\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die trunkne Galathea;", "tokens": ["Und", "die", "trunk\u00b7ne", "Ga\u00b7la\u00b7thea", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wen hier nicht Lebenslust anlacht,", "tokens": ["Wen", "hier", "nicht", "Le\u00b7bens\u00b7lust", "an\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Heiterkeit und Muthwill gr\u00fc\u00dfen,", "tokens": ["Hei\u00b7ter\u00b7keit", "und", "Mut\u00b7hwill", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der entsage der Kunst und Farbe.", "tokens": ["Der", "ent\u00b7sa\u00b7ge", "der", "Kunst", "und", "Far\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Aber auch Lebewohl dir,", "tokens": ["A\u00b7ber", "auch", "Le\u00b7be\u00b7wohl", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Pallast Farnese,", "tokens": ["Pal\u00b7last", "Far\u00b7ne\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Wo ich gelernt in herrlichen Bildern", "tokens": ["Wo", "ich", "ge\u00b7lernt", "in", "herr\u00b7li\u00b7chen", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Des Carracci Dichtung bewundern;", "tokens": ["Des", "Car\u00b7rac\u00b7ci", "Dich\u00b7tung", "be\u00b7wun\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "O was nenn' ich, was verschweig' ich,", "tokens": ["O", "was", "nenn'", "ich", ",", "was", "ver\u00b7schweig'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Das Ged\u00e4chtni\u00df erm\u00fcdet,", "tokens": ["Das", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "er\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Alle die Wunder, die gro\u00dfen Erinnerungen,", "tokens": ["Al\u00b7le", "die", "Wun\u00b7der", ",", "die", "gro\u00b7\u00dfen", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Aller der Steine und Tafeln Pracht,", "tokens": ["Al\u00b7ler", "der", "Stei\u00b7ne", "und", "Ta\u00b7feln", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Des Erzes Bildwerk wiederzusagen.", "tokens": ["Des", "Er\u00b7zes", "Bild\u00b7werk", "wie\u00b7der\u00b7zu\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Mit kindlicher R\u00fchrung pilgerte ich auch zu dir,", "tokens": ["Mit", "kind\u00b7li\u00b7cher", "R\u00fch\u00b7rung", "pil\u00b7ger\u00b7te", "ich", "auch", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Grabmal der C\u00e4cilia Metalla,", "tokens": ["Grab\u00b7mal", "der", "C\u00e4\u00b7ci\u00b7lia", "Me\u00b7tal\u00b7la", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das ich mit seinen wilden Ruinen umher", "tokens": ["Das", "ich", "mit", "sei\u00b7nen", "wil\u00b7den", "Ru