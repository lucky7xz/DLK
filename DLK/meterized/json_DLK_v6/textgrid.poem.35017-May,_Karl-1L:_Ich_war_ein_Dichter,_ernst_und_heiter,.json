{"textgrid.poem.35017": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war ein Dichter, ernst und heiter,", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Dichter, ernst und heiter,", "tokens": ["Ich", "war", "ein", "Dich\u00b7ter", ",", "ernst", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schicksal spielte mit mir frech;", "tokens": ["Das", "Schick\u00b7sal", "spiel\u00b7te", "mit", "mir", "frech", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein ganzes Leben war nichts weiter,", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "war", "nichts", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als nur ein gro\u00dfer \u2013 Klumpen Pech!", "tokens": ["Als", "nur", "ein", "gro\u00b7\u00dfer", "\u2013", "Klum\u00b7pen", "Pech", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "$(", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}