{"textgrid.poem.26706": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du, Freund, die erhabensten H\u00f6hn der Weisheit erfliegen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du, Freund, die erhabensten H\u00f6hn der Weisheit erfliegen,", "tokens": ["Willst", "du", ",", "Freund", ",", "die", "er\u00b7ha\u00b7bens\u00b7ten", "H\u00f6hn", "der", "Weis\u00b7heit", "er\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--++-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wag es auf die Gefahr, da\u00df dich die Klugheit verlacht.", "tokens": ["Wag", "es", "auf", "die", "Ge\u00b7fahr", ",", "da\u00df", "dich", "die", "Klug\u00b7heit", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die kurzsichtige sieht nur das Ufer, das dir zur\u00fcckflieht,", "tokens": ["Die", "kurz\u00b7sich\u00b7ti\u00b7ge", "sieht", "nur", "das", "U\u00b7fer", ",", "das", "dir", "zu\u00b7r\u00fcck\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Jenes nicht, wo dereinst landet dein mutiger Flug.", "tokens": ["Je\u00b7nes", "nicht", ",", "wo", "de\u00b7reinst", "lan\u00b7det", "dein", "mu\u00b7ti\u00b7ger", "Flug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "$,", "PWAV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}