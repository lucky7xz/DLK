{"textgrid.poem.59335": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Leid und Lust", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch Wolken beneid ich", "tokens": ["Euch", "Wol\u00b7ken", "be\u00b7neid", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In blauer Luft,", "tokens": ["In", "blau\u00b7er", "Luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie schwingt ihr euch freudig", "tokens": ["Wie", "schwingt", "ihr", "euch", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "\u00dcber Berg und Kluft!", "tokens": ["\u00dc\u00b7ber", "Berg", "und", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Liebchen wohl seht ihr", "tokens": ["Mein", "Lieb\u00b7chen", "wohl", "seht", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Garten gehn,", "tokens": ["Im", "Gar\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Am Springbrunnen steht sie", "tokens": ["Am", "Spring\u00b7brun\u00b7nen", "steht", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "So morgensch\u00f6n.", "tokens": ["So", "mor\u00b7gen\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und w\u00e4scht an der Quelle", "tokens": ["Und", "w\u00e4scht", "an", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ihr goldenes Haar,", "tokens": ["Ihr", "gol\u00b7de\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die \u00c4ugelein helle,", "tokens": ["Die", "\u00c4u\u00b7ge\u00b7lein", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und blickt so klar.", "tokens": ["Und", "blickt", "so", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und Busen und Wangen", "tokens": ["Und", "Bu\u00b7sen", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "D\u00fcrft ihr da sehn. \u2013", "tokens": ["D\u00fcrft", "ihr", "da", "sehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich brenn vor Verlangen,", "tokens": ["Ich", "brenn", "vor", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und mu\u00df hier stehn!", "tokens": ["Und", "mu\u00df", "hier", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Euch Wolken bedaur ich", "tokens": ["Euch", "Wol\u00b7ken", "be\u00b7daur", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bei stiller Nacht;", "tokens": ["Bei", "stil\u00b7ler", "Nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Erde bebt schaurig,", "tokens": ["Die", "Er\u00b7de", "bebt", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Mond erwacht:", "tokens": ["Der", "Mond", "er\u00b7wacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Da f\u00fchrt mich ein B\u00fcbchen", "tokens": ["Da", "f\u00fchrt", "mich", "ein", "B\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit Fl\u00fcgelein fein,", "tokens": ["Mit", "Fl\u00fc\u00b7ge\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durchs Dunkel zum Liebchen,", "tokens": ["Durchs", "Dun\u00b7kel", "zum", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie l\u00e4\u00dft mich ein.", "tokens": ["Sie", "l\u00e4\u00dft", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Wohl schaut ihr die Sterne", "tokens": ["Wohl", "schaut", "ihr", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Weit, ohne Zahl,", "tokens": ["Weit", ",", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch bleiben sie ferne", "tokens": ["Doch", "blei\u00b7ben", "sie", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Euch allzumal.", "tokens": ["Euch", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Mir leuchten zwei Sterne", "tokens": ["Mir", "leuch\u00b7ten", "zwei", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit s\u00fc\u00dfem Strahl,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die k\u00fc\u00df ich so gerne", "tokens": ["Die", "k\u00fc\u00df", "ich", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vieltausendmal.", "tokens": ["Viel\u00b7tau\u00b7send\u00b7mal", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Euch gr\u00fc\u00dft mit Gefunkel", "tokens": ["Euch", "gr\u00fc\u00dft", "mit", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Wasserfall,", "tokens": ["Der", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und tief aus dem Dunkel", "tokens": ["Und", "tief", "aus", "dem", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Nachtigall.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Doch s\u00fc\u00dfer es gr\u00fc\u00dfet", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", "es", "gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Als Wellentanz,", "tokens": ["Als", "Wel\u00b7len\u00b7tanz", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wenn Liebchen hold fl\u00fcstert:", "tokens": ["Wenn", "Lieb\u00b7chen", "hold", "fl\u00fcs\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "\u00bbdein bin ich ganz.\u00ab", "tokens": ["\u00bb", "dein", "bin", "ich", "ganz", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "So segelt denn traurig", "tokens": ["So", "se\u00b7gelt", "denn", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In \u00f6der Pracht!", "tokens": ["In", "\u00f6\u00b7der", "Pracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Euch Wolken bedaur ich", "tokens": ["Euch", "Wol\u00b7ken", "be\u00b7daur", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bei s\u00fc\u00dfer Nacht.", "tokens": ["Bei", "s\u00fc\u00b7\u00dfer", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}