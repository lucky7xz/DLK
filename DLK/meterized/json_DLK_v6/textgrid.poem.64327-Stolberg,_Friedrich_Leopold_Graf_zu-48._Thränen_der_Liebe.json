{"textgrid.poem.64327": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "48. Thr\u00e4nen der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4ufle, mein s\u00fc\u00dfes M\u00e4dchen, diese Thr\u00e4nen", "tokens": ["Tr\u00e4uf\u00b7le", ",", "mein", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ",", "die\u00b7se", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PDAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf die silberne Leier deines Stolberg!", "tokens": ["Auf", "die", "sil\u00b7ber\u00b7ne", "Lei\u00b7er", "dei\u00b7nes", "Stol\u00b7berg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sitz auf meinen Knien, und la\u00df die Thr\u00e4ne", "tokens": ["Sitz", "auf", "mei\u00b7nen", "Kni\u00b7en", ",", "und", "la\u00df", "die", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVIMP", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "\u00dcber die Wange", "tokens": ["\u00dc\u00b7ber", "die", "Wan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Deines Geliebten rinnen auf die Saiten,", "tokens": ["Dei\u00b7nes", "Ge\u00b7lieb\u00b7ten", "rin\u00b7nen", "auf", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df sie beben, wie deine Busenb\u00e4nder,", "tokens": ["Da\u00df", "sie", "be\u00b7ben", ",", "wie", "dei\u00b7ne", "Bu\u00b7sen\u00b7b\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df meine Thr\u00e4ne mit deiner Thr\u00e4ne", "tokens": ["Und", "da\u00df", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "mit", "dei\u00b7ner", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "T\u00f6nend sich mische!", "tokens": ["T\u00f6\u00b7nend", "sich", "mi\u00b7sche", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Thr\u00e4ne der Liebe, ach! der stummen Wonne", "tokens": ["Thr\u00e4\u00b7ne", "der", "Lie\u00b7be", ",", "ach", "!", "der", "stum\u00b7men", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ITJ", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Thr\u00e4ne! k\u00f6nnt' ich sie fassen und verwahren!", "tokens": ["Thr\u00e4\u00b7ne", "!", "k\u00f6nnt'", "ich", "sie", "fas\u00b7sen", "und", "ver\u00b7wah\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und mit ihr den ersten der K\u00fcsse, da du", "tokens": ["Und", "mit", "ihr", "den", "ers\u00b7ten", "der", "K\u00fcs\u00b7se", ",", "da", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ART", "ADJA", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sch\u00fcchtern dich umsahst,", "tokens": ["Sch\u00fcch\u00b7tern", "dich", "um\u00b7sahst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dann um den Hals mir fielst, und sanfterr\u00f6tend", "tokens": ["Dann", "um", "den", "Hals", "mir", "fielst", ",", "und", "sanf\u00b7ter\u00b7r\u00f6\u00b7tend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Deine Lippen an meine Lippen dr\u00fccktest:", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", "an", "mei\u00b7ne", "Lip\u00b7pen", "dr\u00fcck\u00b7test", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Unsre Seelen huben sich auf der Liebe", "tokens": ["Uns\u00b7re", "See\u00b7len", "hu\u00b7ben", "sich", "auf", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Seufzer, und schwebten", "tokens": ["Seuf\u00b7zer", ",", "und", "schweb\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wonneberauschet auf des Kusses Fl\u00fcgeln,", "tokens": ["Won\u00b7ne\u00b7be\u00b7rau\u00b7schet", "auf", "des", "Kus\u00b7ses", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie auf Hauchen des Lenzes s\u00fc\u00dfe D\u00fcfte", "tokens": ["Wie", "auf", "Hau\u00b7chen", "des", "Len\u00b7zes", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Um die Wangen r\u00f6tlicher, taubenetzter", "tokens": ["Um", "die", "Wan\u00b7gen", "r\u00f6t\u00b7li\u00b7cher", ",", "tau\u00b7be\u00b7netz\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "ART", "NN", "ADJD", "$,", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bl\u00fcten des Apfels!", "tokens": ["Bl\u00fc\u00b7ten", "des", "Ap\u00b7fels", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}