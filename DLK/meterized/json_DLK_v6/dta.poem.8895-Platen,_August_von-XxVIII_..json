{"dta.poem.8895": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Denn euch zu missen k\u00f6nnt' ich nicht ertragen,", "tokens": ["Denn", "euch", "zu", "mis\u00b7sen", "k\u00f6nnt'", "ich", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch dem Genu\u00df auf ew'ge Zeit entsagen,", "tokens": ["Noch", "dem", "Ge\u00b7nu\u00df", "auf", "ew'\u00b7ge", "Zeit", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach eurer Herrlichkeit emporzustreben!", "tokens": ["Nach", "eu\u00b7rer", "Herr\u00b7lich\u00b7keit", "em\u00b7por\u00b7zu\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Um Gottes eigne Glorie zu schweben", "tokens": ["Um", "Got\u00b7tes", "eig\u00b7ne", "Glo\u00b7rie", "zu", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vermag die Kunst allein und darf es wagen,", "tokens": ["Ver\u00b7mag", "die", "Kunst", "al\u00b7lein", "und", "darf", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wessen Herz Vollendetem geschlagen,", "tokens": ["Und", "wes\u00b7sen", "Herz", "Voll\u00b7en\u00b7de\u00b7tem", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Dem hat der Himmel weiter nichts zu geben!", "tokens": ["Dem", "hat", "der", "Him\u00b7mel", "wei\u00b7ter", "nichts", "zu", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer wollte nicht den Glauben aller Zeiten,", "tokens": ["Wer", "woll\u00b7te", "nicht", "den", "Glau\u00b7ben", "al\u00b7ler", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch alle L\u00e4nder, alle Kirchensprengel", "tokens": ["Durch", "al\u00b7le", "L\u00e4n\u00b7der", ",", "al\u00b7le", "Kir\u00b7chen\u00b7spren\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Sch\u00f6nen Evangelium verbreiten:", "tokens": ["Des", "Sch\u00f6\u00b7nen", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "ver\u00b7brei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn Palma's Heil'ge mit dem Palmenstengel,", "tokens": ["Wenn", "Pal\u00b7ma's", "Heil'\u00b7ge", "mit", "dem", "Pal\u00b7mens\u00b7ten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und Paolo's Alexander ihn begleiten,", "tokens": ["Und", "Pao\u00b7lo's", "A\u00b7lex\u00b7an\u00b7der", "ihn", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Tizians Tobias mit dem Engel?", "tokens": ["Und", "Ti\u00b7zi\u00b7ans", "To\u00b7bias", "mit", "dem", "En\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}