{"textgrid.poem.47469": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich bin so fromm, ich bin so rein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so fromm, ich bin so rein;", "tokens": ["Ich", "bin", "so", "fromm", ",", "ich", "bin", "so", "rein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald ist das h\u00f6chste Ziel erklettert. \u2013", "tokens": ["Bald", "ist", "das", "h\u00f6chs\u00b7te", "Ziel", "er\u00b7klet\u00b7tert", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch o wie sch\u00f6n, ein Mensch zu sein!", "tokens": ["Doch", "o", "wie", "sch\u00f6n", ",", "ein", "Mensch", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "KOKOM", "ADJD", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum, Freunde, sagt, wo ist der Wein,", "tokens": ["Drum", ",", "Freun\u00b7de", ",", "sagt", ",", "wo", "ist", "der", "Wein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "$,", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der fromme Seelen niederschmettert?", "tokens": ["Der", "from\u00b7me", "See\u00b7len", "nie\u00b7der\u00b7schmet\u00b7tert", "?"], "token_inf