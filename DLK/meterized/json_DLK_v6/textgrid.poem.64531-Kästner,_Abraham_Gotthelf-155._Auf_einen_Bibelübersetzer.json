{"textgrid.poem.64531": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "155. Auf einen Bibel\u00fcbersetzer", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn er von den, die ", "tokens": ["Wenn", "er", "von", "den", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "$,", "PRELS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Von Hagars Enkeln, lernt, was Jacobs Gott gesagt,", "tokens": ["Von", "Ha\u00b7gars", "En\u00b7keln", ",", "lernt", ",", "was", "Ja\u00b7cobs", "Gott", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "VVFIN", "$,", "PRELS", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00fcrde Luther selbst ihn lehrbegierig h\u00f6ren;", "tokens": ["So", "w\u00fcr\u00b7de", "Lu\u00b7ther", "selbst", "ihn", "lehr\u00b7be\u00b7gie\u00b7rig", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch richtig, edel Deutsch, das m\u00fc\u00dft' ihn ", "tokens": ["Doch", "rich\u00b7tig", ",", "e\u00b7del", "Deutsch", ",", "das", "m\u00fc\u00dft'", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "NN", "$,", "PDS", "VMFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}