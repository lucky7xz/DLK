{"textgrid.poem.42893": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Gewisse junge Burschen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltsam schauen diese Jungen ins Leben,", "tokens": ["Selt\u00b7sam", "schau\u00b7en", "die\u00b7se", "Jun\u00b7gen", "ins", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Davon sie gar nichts begreifen,", "tokens": ["Da\u00b7von", "sie", "gar", "nichts", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In einer Zeit, da sie gar nichts erleben", "tokens": ["In", "ei\u00b7ner", "Zeit", ",", "da", "sie", "gar", "nichts", "er\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eben deshalb so gesund reifen.", "tokens": ["Und", "e\u00b7ben", "des\u00b7halb", "so", "ge\u00b7sund", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dr\u00fcckt kein Gewehr sie, auch kein Ranzen.", "tokens": ["Dr\u00fcckt", "kein", "Ge\u00b7wehr", "sie", ",", "auch", "kein", "Ran\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ohne zu ahnen, wissen sie.", "tokens": ["Oh\u00b7ne", "zu", "ah\u00b7nen", ",", "wis\u00b7sen", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ohne zu fragen, beherrschen und tanzen", "tokens": ["Oh\u00b7ne", "zu", "fra\u00b7gen", ",", "be\u00b7herr\u00b7schen", "und", "tan\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "KON", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Sie sicher jede Zurzeit-Melodie.", "tokens": ["Sie", "si\u00b7cher", "je\u00b7de", "Zur\u00b7zeit\u00b7Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie lange wird's w\u00e4hren?", "tokens": ["Wie", "lan\u00b7ge", "wird's", "w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wer ist der erste Rohling, der spricht,", "tokens": ["Wer", "ist", "der", "ers\u00b7te", "Roh\u00b7ling", ",", "der", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Um sie aufzukl\u00e4ren?", "tokens": ["Um", "sie", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ich wagte es nicht.", "tokens": ["Ich", "wag\u00b7te", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und ihre M\u00e4dchen, vom gleichen Jahr,", "tokens": ["Und", "ih\u00b7re", "M\u00e4d\u00b7chen", ",", "vom", "glei\u00b7chen", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Meist j\u00fcnger sogar,", "tokens": ["Meist", "j\u00fcn\u00b7ger", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lassen sich gern scheinbar lenken", "tokens": ["Las\u00b7sen", "sich", "gern", "schein\u00b7bar", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und empfinden wunderbar:", "tokens": ["Und", "emp\u00b7fin\u00b7den", "wun\u00b7der\u00b7bar", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er gibt uns gar nichts zu denken.", "tokens": ["Er", "gibt", "uns", "gar", "nichts", "zu", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "G\u00f6nnt doch den jungen, frischen", "tokens": ["G\u00f6nnt", "doch", "den", "jun\u00b7gen", ",", "fri\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tieren ihr freudiges Weichmaulgefr\u00e4\u00df.", "tokens": ["Tie\u00b7ren", "ihr", "freu\u00b7di\u00b7ges", "Weich\u00b7maul\u00b7ge\u00b7fr\u00e4\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ihrem Zahnarzt entwischen", "tokens": ["Ih\u00b7rem", "Zahn\u00b7arzt", "ent\u00b7wi\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sie doch nicht. Bestimmungsgem\u00e4\u00df.", "tokens": ["Sie", "doch", "nicht", ".", "Be\u00b7stim\u00b7mungs\u00b7ge\u00b7m\u00e4\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "$.", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Neben mir, still, vom Ball abgewandt,", "tokens": ["Ne\u00b7ben", "mir", ",", "still", ",", "vom", "Ball", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Steht so einer dergleichen.", "tokens": ["Steht", "so", "ei\u00b7ner", "derg\u00b7lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIS", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ich m\u00f6chte so gern aus der flachen Hand", "tokens": ["Ich", "m\u00f6ch\u00b7te", "so", "gern", "aus", "der", "fla\u00b7chen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihm ein St\u00fcck Zucker reichen.", "tokens": ["Ihm", "ein", "St\u00fcck", "Zu\u00b7cker", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}