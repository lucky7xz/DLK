{"dta.poem.13668": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Uns beide will noch sch\u00f6n das Leben schm\u00fccken", "tokens": ["Uns", "bei\u00b7de", "will", "noch", "sch\u00f6n", "das", "Le\u00b7ben", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VMFIN", "ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinen reichen, heitern, vollen Kr\u00e4nzen,", "tokens": ["Mit", "sei\u00b7nen", "rei\u00b7chen", ",", "hei\u00b7tern", ",", "vol\u00b7len", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Morgenwind w\u00fchlt um den off'nen Busen!", "tokens": ["Der", "Mor\u00b7gen\u00b7wind", "w\u00fchlt", "um", "den", "off'\u00b7nen", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}