{"dta.poem.18377": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An seinen guten freund/ welchem>  \n von seiner liebsten Agnes der puls mit  \n einer nadel gestochen  \n worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was z\u00fcrnet ihr/ mein freund/ umb einen nadelstich?", "tokens": ["Was", "z\u00fcr\u00b7net", "ihr", "/", "mein", "freund", "/", "umb", "ei\u00b7nen", "na\u00b7del\u00b7stich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ey tr\u00e4f\u2019 er meine hand/ wie gl\u00fccklich sch\u00e4tzt\u2019 ich mich.", "tokens": ["Ey", "tr\u00e4f'", "er", "mei\u00b7ne", "hand", "/", "wie", "gl\u00fcck\u00b7lich", "sch\u00e4tzt'", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df rosenbild ist nicht so grimmig sich zu r\u00e4chen/", "tokens": ["Di\u00df", "ro\u00b7sen\u00b7bild", "ist", "nicht", "so", "grim\u00b7mig", "sich", "zu", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "PTKNEG", "ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was kan sie davor/ wenn ihre nadeln stechen?", "tokens": ["Und", "was", "kan", "sie", "da\u00b7vor", "/", "wenn", "ih\u00b7re", "na\u00b7deln", "ste\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "PAV", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo sch\u00f6ne rosen stehn/ da ist ihr stachel auch/", "tokens": ["Wo", "sch\u00f6\u00b7ne", "ro\u00b7sen", "stehn", "/", "da", "ist", "ihr", "sta\u00b7chel", "auch", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$(", "ADV", "VAFIN", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und stechen sie gleich nicht/ so sticht dennoch der strauch.", "tokens": ["Und", "ste\u00b7chen", "sie", "gleich", "nicht", "/", "so", "sticht", "den\u00b7noch", "der", "strauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Seht/ dieser puls hat euch den puls der hand geritzet;", "tokens": ["Seht", "/", "die\u00b7ser", "puls", "hat", "euch", "den", "puls", "der", "hand", "ge\u00b7rit\u00b7zet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PDAT", "NE", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es ist ein liebes-stich/ der euch das blut erhitzet/", "tokens": ["Es", "ist", "ein", "lie\u00b7bes\u00b7stich", "/", "der", "euch", "das", "blut", "er\u00b7hit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und der durch euren puls das hertze selbst bewegt/", "tokens": ["Und", "der", "durch", "eu\u00b7ren", "puls", "das", "hert\u00b7ze", "selbst", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn wie der puls sich r\u00fchrt/ so wird das hertz geregt.", "tokens": ["Denn", "wie", "der", "puls", "sich", "r\u00fchrt", "/", "so", "wird", "das", "hertz", "ge\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "PRF", "VVFIN", "$(", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Agnet\u2019 ist ein Magnet; wen ihre nadel r\u00fchret/", "tokens": ["A\u00b7gnet'", "ist", "ein", "Mag\u00b7net", ";", "wen", "ih\u00b7re", "na\u00b7del", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dem wird von ihrem geist auch etwas zugef\u00fchret.", "tokens": ["Dem", "wird", "von", "ih\u00b7rem", "geist", "auch", "et\u00b7was", "zu\u00b7ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein linder hauch von ihr begeistert diesen stahl/", "tokens": ["Ein", "lin\u00b7der", "hauch", "von", "ihr", "be\u00b7geis\u00b7tert", "die\u00b7sen", "stahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es lauscht ein heimlich feu\u2019r in dem verborgnen strahl.", "tokens": ["Es", "lauscht", "ein", "heim\u00b7lich", "feu'r", "in", "dem", "ver\u00b7borg\u00b7nen", "strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und warumb m\u00f6get ihr umb solche wunde sorgen?", "tokens": ["Und", "wa\u00b7rumb", "m\u00f6\u00b7get", "ihr", "umb", "sol\u00b7che", "wun\u00b7de", "sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Last stechen auf den puls/ der finger ist geborgen/", "tokens": ["Last", "ste\u00b7chen", "auf", "den", "puls", "/", "der", "fin\u00b7ger", "ist", "ge\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NE", "$(", "ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.17": {"text": "Den nun ihr fingerhut bedecket und besch\u00fctzt;", "tokens": ["Den", "nun", "ihr", "fin\u00b7ger\u00b7hut", "be\u00b7de\u00b7cket", "und", "be\u00b7sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie heilet selber auch/ was ihre seder ritzt.", "tokens": ["Sie", "hei\u00b7let", "sel\u00b7ber", "auch", "/", "was", "ih\u00b7re", "se\u00b7der", "ritzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die biene/ welche sticht/ die kan auch honig geben;", "tokens": ["Die", "bie\u00b7ne", "/", "wel\u00b7che", "sticht", "/", "die", "kan", "auch", "ho\u00b7nig", "ge\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PWS", "VVFIN", "$(", "PDS", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jhr lippen-honig gibt den todten selbst das leben.", "tokens": ["Ihr", "lip\u00b7pen\u00b7ho\u00b7nig", "gibt", "den", "tod\u00b7ten", "selbst", "das", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "ADV", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gebraucht den honig nur/ so wird in kurtzer weil/", "tokens": ["Ge\u00b7braucht", "den", "ho\u00b7nig", "nur", "/", "so", "wird", "in", "kurt\u00b7zer", "weil", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$(", "ADV", "VAFIN", "APPR", "ADJA", "KOUS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und w\u00e4r sie noch so tief/ die wunde wieder heil.", "tokens": ["Und", "w\u00e4r", "sie", "noch", "so", "tief", "/", "die", "wun\u00b7de", "wie\u00b7der", "heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$(", "ART", "ADJA", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die gegenwehr dient nicht/ die waffen sind geringer/", "tokens": ["Die", "ge\u00b7gen\u00b7wehr", "dient", "nicht", "/", "die", "waf\u00b7fen", "sind", "ge\u00b7rin\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie hat der nadel schutz/ ihr aber nur den finger.", "tokens": ["Sie", "hat", "der", "na\u00b7del", "schutz", "/", "ihr", "a\u00b7ber", "nur", "den", "fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PPER", "ADV", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Drum nehmet nur/ mein freund/ die stiche willig an/", "tokens": ["Drum", "neh\u00b7met", "nur", "/", "mein", "freund", "/", "die", "sti\u00b7che", "wil\u00b7lig", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "$(", "ART", "ADJA", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich wei\u00df noch mehr an ihr/ was seelen ritzen kan/", "tokens": ["Ich", "wei\u00df", "noch", "mehr", "an", "ihr", "/", "was", "see\u00b7len", "rit\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "$(", "PWS", "VVINF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn wenn die strahlen ihr aus ihren augen brechen/", "tokens": ["Denn", "wenn", "die", "strah\u00b7len", "ihr", "aus", "ih\u00b7ren", "au\u00b7gen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So stechen sie weit mehr/ als tausend nadeln stechen.", "tokens": ["So", "ste\u00b7chen", "sie", "weit", "mehr", "/", "als", "tau\u00b7send", "na\u00b7deln", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$(", "KOKOM", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}