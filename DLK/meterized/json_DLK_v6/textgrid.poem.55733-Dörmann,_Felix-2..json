{"textgrid.poem.55733": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df mich knien vor Dir", "tokens": ["So", "la\u00df", "mich", "kni\u00b7en", "vor", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Tief im Staube,", "tokens": ["Tief", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "La\u00df mich Deine schmalen H\u00e4nde", "tokens": ["La\u00df", "mich", "Dei\u00b7ne", "schma\u00b7len", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise k\u00fcssen;", "tokens": ["Lei\u00b7se", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "In diesen tr\u00e4nenfeuchten,", "tokens": ["In", "die\u00b7sen", "tr\u00e4\u00b7nen\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dunklen Augen", "tokens": ["Dunk\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wohnt das Mitleid,", "tokens": ["Wohnt", "das", "Mit\u00b7leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wohnt die Alles vergebende,", "tokens": ["Wohnt", "die", "Al\u00b7les", "ver\u00b7ge\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.9": {"text": "G\u00f6ttliche Liebe.", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "O la\u00df mich knien vor Dir", "tokens": ["O", "la\u00df", "mich", "kni\u00b7en", "vor", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Tief im Staube. \u2013", "tokens": ["Tief", "im", "Stau\u00b7be", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}}}}