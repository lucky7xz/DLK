{"dta.poem.3393": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Rugard.  \n  Drittes Lied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sich, sieh den k\u00f6niglichen Berg", "tokens": ["Sich", ",", "sieh", "den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Berg"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "$,", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedeckt mit glimmerndem Kristall.", "tokens": ["Ge\u00b7deckt", "mit", "glim\u00b7mern\u00b7dem", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sieh, der Gewaltige", "tokens": ["Sieh", ",", "der", "Ge\u00b7wal\u00b7ti\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Streckt die beeis'te Felsenstirn,", "tokens": ["Streckt", "die", "be\u00b7eis'\u00b7te", "Fel\u00b7sen\u00b7stirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich einem hellgeschliffnen Schild,", "tokens": ["Gleich", "ei\u00b7nem", "hell\u00b7ge\u00b7schliff\u00b7nen", "Schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gigantisch himmelan.", "tokens": ["Gi\u00b7gan\u00b7tisch", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Rings um die graue Scheitel ballt", "tokens": ["Rings", "um", "die", "grau\u00b7e", "Schei\u00b7tel", "ballt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wolken d\u00fcstre F\u00fclle sich.", "tokens": ["Der", "Wol\u00b7ken", "d\u00fcst\u00b7re", "F\u00fcl\u00b7le", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ihrem Fittig schwebt", "tokens": ["Auf", "ih\u00b7rem", "Fit\u00b7tig", "schwebt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des Frostes Genius daher,", "tokens": ["Des", "Fros\u00b7tes", "Ge\u00b7nius", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In Eis gepanzert, Bart und Haar", "tokens": ["In", "Eis", "ge\u00b7pan\u00b7zert", ",", "Bart", "und", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Braue sch\u00f6nbereift.", "tokens": ["Und", "Brau\u00b7e", "sch\u00f6n\u00b7be\u00b7reift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sein Antlitz ist kometenroth.", "tokens": ["Sein", "Ant\u00b7litz", "ist", "ko\u00b7me\u00b7ten\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Scheitel kahle Glatze gl\u00e4nzt", "tokens": ["Der", "Schei\u00b7tel", "kah\u00b7le", "Glat\u00b7ze", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im blassen Sonnenstrahl.", "tokens": ["Im", "blas\u00b7sen", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es gl\u00e4nzt im blassen Sonnenstrahl", "tokens": ["Es", "gl\u00e4nzt", "im", "blas\u00b7sen", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Diadem, sein Panzerhemd", "tokens": ["Sein", "Di\u00b7a\u00b7dem", ",", "sein", "Pan\u00b7zer\u00b7hemd"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus gr\u00fcnem Gletschereis.", "tokens": ["Aus", "gr\u00fc\u00b7nem", "Glet\u00b7sche\u00b7reis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Winter, Winter, Tode schlosst", "tokens": ["O", "Win\u00b7ter", ",", "Win\u00b7ter", ",", "To\u00b7de", "schlosst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Odem. Deines Mundes Hauch", "tokens": ["Dein", "O\u00b7dem", ".", "Dei\u00b7nes", "Mun\u00b7des", "Hauch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versteinert Land und Meer.", "tokens": ["Ver\u00b7stei\u00b7nert", "Land", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vor deinem Dr\u00e4uen stirbt der Wald;", "tokens": ["Vor", "dei\u00b7nem", "Dr\u00e4u\u00b7en", "stirbt", "der", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor deinem Schelten schweigt das Meer,", "tokens": ["Vor", "dei\u00b7nem", "Schel\u00b7ten", "schweigt", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Donner ruhn.", "tokens": ["Und", "sei\u00b7ne", "Don\u00b7ner", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Sonne schauet grossgeaugt", "tokens": ["Die", "Son\u00b7ne", "schau\u00b7et", "gross\u00b7ge\u00b7augt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leichenblass aus schwarzer Nacht,", "tokens": ["Und", "lei\u00b7chen\u00b7blass", "aus", "schwar\u00b7zer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schauert bang zur\u00fcck.", "tokens": ["Und", "schau\u00b7ert", "bang", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So blickt noch sterbend aufs Gefild", "tokens": ["So", "blickt", "noch", "ster\u00b7bend", "aufs", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voll Schlacht und Tod und Graun ein Held,", "tokens": ["Voll", "Schlacht", "und", "Tod", "und", "Graun", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schliesst sein Aug' und stirbt.", "tokens": ["Und", "schliesst", "sein", "Aug'", "und", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie still ist es, wie leichenstill,", "tokens": ["Wie", "still", "ist", "es", ",", "wie", "lei\u00b7chen\u00b7still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O hoher Rugard, rings um dich!", "tokens": ["O", "ho\u00b7her", "Ru\u00b7gard", ",", "rings", "um", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NE", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im ausgestorbnen Hayn,", "tokens": ["Im", "aus\u00b7ge\u00b7storb\u00b7nen", "Hayn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am hohlen Ufer schwirrt kein Laut;", "tokens": ["Am", "hoh\u00b7len", "U\u00b7fer", "schwirrt", "kein", "Laut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Vogel streift; es schweift kein Wild;", "tokens": ["Kein", "Vo\u00b7gel", "streift", ";", "es", "schweift", "kein", "Wild", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Leben Pulsschlag starrt.", "tokens": ["Der", "Le\u00b7ben", "Puls\u00b7schlag", "starrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch pl\u00f6tzlich dumpf erdonnernd kracht", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "dumpf", "er\u00b7don\u00b7nernd", "kracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das meilenlang gespaltne Meer.", "tokens": ["Das", "mei\u00b7len\u00b7lang", "ge\u00b7spalt\u00b7ne", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dickem Brodem brau'n", "tokens": ["In", "di\u00b7ckem", "Bro\u00b7dem", "brau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich schwere Wetter. Horch, wie g\u00e4hrt,", "tokens": ["Sich", "schwe\u00b7re", "Wet\u00b7ter", ".", "Horch", ",", "wie", "g\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "$.", "NE", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie saust es in dem schwarzen Schwall!", "tokens": ["Wie", "saust", "es", "in", "dem", "schwar\u00b7zen", "Schwall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Sturmes Kraft erwacht.", "tokens": ["Des", "Stur\u00b7mes", "Kraft", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das Wetter w\u00e4lzt sich schwer daher.", "tokens": ["Das", "Wet\u00b7ter", "w\u00e4lzt", "sich", "schwer", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wildprasselnd f\u00e4llt es; prasselnd braust", "tokens": ["Wild\u00b7pras\u00b7selnd", "f\u00e4llt", "es", ";", "pras\u00b7selnd", "braust"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "VVPP", "VVFIN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schlossen Schwall herab.", "tokens": ["Der", "Schlos\u00b7sen", "Schwall", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es schwinden Erde, Luft und Meer.", "tokens": ["Es", "schwin\u00b7den", "Er\u00b7de", ",", "Luft", "und", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Weltall schwindet; alte Nacht", "tokens": ["Das", "Wel\u00b7tall", "schwin\u00b7det", ";", "al\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00f6des Chaos herrscht.", "tokens": ["Und", "\u00f6\u00b7des", "Chaos", "herrscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}