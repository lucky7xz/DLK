{"textgrid.poem.64938": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie hoch auch mancher Berg ragt in der Wolken Reich,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hoch auch mancher Berg ragt in der Wolken Reich,", "tokens": ["Wie", "hoch", "auch", "man\u00b7cher", "Berg", "ragt", "in", "der", "Wol\u00b7ken", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An H\u00f6h' ist keiner doch dem Grabesh\u00fcgel gleich.", "tokens": ["An", "H\u00f6h'", "ist", "kei\u00b7ner", "doch", "dem", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn stehest sinnend du auf seinen ernsten H\u00f6hen,", "tokens": ["Denn", "ste\u00b7hest", "sin\u00b7nend", "du", "auf", "sei\u00b7nen", "erns\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du das ganze Sein in dunkler Tiefe sehen.", "tokens": ["Wirst", "du", "das", "gan\u00b7ze", "Sein", "in", "dunk\u00b7ler", "Tie\u00b7fe", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und \u00fcberschau'n wirst du mit einem Blick die Welt,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7schau'n", "wirst", "du", "mit", "ei\u00b7nem", "Blick", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Alles, was sie fa\u00dft und was ihr Kern enth\u00e4lt.", "tokens": ["Und", "Al\u00b7les", ",", "was", "sie", "fa\u00dft", "und", "was", "ihr", "Kern", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "KON", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}