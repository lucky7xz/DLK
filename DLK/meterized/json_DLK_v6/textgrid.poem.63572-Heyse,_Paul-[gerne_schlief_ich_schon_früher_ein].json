{"textgrid.poem.63572": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[gerne schlief ich schon fr\u00fcher ein]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne schlief ich schon fr\u00fcher ein,", "tokens": ["Ger\u00b7ne", "schlief", "ich", "schon", "fr\u00fc\u00b7her", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Doch mein Herz, vor lauter Frohlocken,", "tokens": ["Doch", "mein", "Herz", ",", "vor", "lau\u00b7ter", "Froh\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Da\u00df die holde Geliebte mein,", "tokens": ["Da\u00df", "die", "hol\u00b7de", "Ge\u00b7lieb\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "L\u00e4utete Sturm mit allen Glocken.", "tokens": ["L\u00e4u\u00b7te\u00b7te", "Sturm", "mit", "al\u00b7len", "Glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Gerne h\u00e4tt' ich noch l\u00e4nger geruht,", "tokens": ["Ger\u00b7ne", "h\u00e4tt'", "ich", "noch", "l\u00e4n\u00b7ger", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Aber im Kopfe begann zu l\u00e4rmen", "tokens": ["A\u00b7ber", "im", "Kop\u00b7fe", "be\u00b7gann", "zu", "l\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Eine tolle Gedankenbrut,", "tokens": ["Ei\u00b7ne", "tol\u00b7le", "Ge\u00b7dan\u00b7ken\u00b7brut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Fr\u00fch wie Bienen hinauszuschw\u00e4rmen.", "tokens": ["Fr\u00fch", "wie", "Bie\u00b7nen", "hin\u00b7aus\u00b7zu\u00b7schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nichts als Verse und sehnendes Leid", "tokens": ["Nichts", "als", "Ver\u00b7se", "und", "seh\u00b7nen\u00b7des", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schafft dies einsam n\u00e4chtliche Wachen.", "tokens": ["Schafft", "dies", "ein\u00b7sam", "n\u00e4cht\u00b7li\u00b7che", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "O, es ist Zeit, es ist hohe Zeit,", "tokens": ["O", ",", "es", "ist", "Zeit", ",", "es", "ist", "ho\u00b7he", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "NN", "$,", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Endlich ein End' und Hochzeit zu machen!", "tokens": ["End\u00b7lich", "ein", "End'", "und", "Hoch\u00b7zeit", "zu", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}