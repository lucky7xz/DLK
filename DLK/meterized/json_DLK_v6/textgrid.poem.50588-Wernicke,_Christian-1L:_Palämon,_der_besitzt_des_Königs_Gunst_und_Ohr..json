{"textgrid.poem.50588": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pal\u00e4mon, der besitzt des K\u00f6nigs Gunst und Ohr.", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pal\u00e4mon, der besitzt des K\u00f6nigs Gunst und Ohr.", "tokens": ["Pa\u00b7l\u00e4\u00b7mon", ",", "der", "be\u00b7sitzt", "des", "K\u00f6\u00b7nigs", "Gunst", "und", "Ohr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich folg' ihm durch den Saal bis an des Palasts Thor;", "tokens": ["Ich", "fol\u00b7g'", "ihm", "durch", "den", "Saal", "bis", "an", "des", "Pa\u00b7lasts", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich hust', er sieht sich um; ich neige mich, er lacht;", "tokens": ["Ich", "hust'", ",", "er", "sieht", "sich", "um", ";", "ich", "nei\u00b7ge", "mich", ",", "er", "lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00bbich habe gestern noch\u00ab, sagt er, \u00bban Ihn gedacht.\u00ab", "tokens": ["\u00bb", "ich", "ha\u00b7be", "ge\u00b7stern", "noch", "\u00ab", ",", "sagt", "er", ",", "\u00bb", "an", "Ihn", "ge\u00b7dacht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich glaub' es ohne sein Vermessen", "tokens": ["Ich", "glaub'", "es", "oh\u00b7ne", "sein", "Ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und find' es in der That,", "tokens": ["Und", "find'", "es", "in", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er sich meiner hat", "tokens": ["Da\u00df", "er", "sich", "mei\u00b7ner", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}