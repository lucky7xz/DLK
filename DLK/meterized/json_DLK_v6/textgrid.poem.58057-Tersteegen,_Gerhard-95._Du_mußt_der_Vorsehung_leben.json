{"textgrid.poem.58057": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "95. Du mu\u00dft der Vorsehung leben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seele, die sich Gott in Wahrheit hat ergeben", "tokens": ["Die", "See\u00b7le", ",", "die", "sich", "Gott", "in", "Wahr\u00b7heit", "hat", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "NN", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ein unm\u00fcndig's Kind, mu\u00df der Vorsehung leben.", "tokens": ["Wie", "ein", "un\u00b7m\u00fcn\u00b7dig's", "Kind", ",", "mu\u00df", "der", "Vor\u00b7se\u00b7hung", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Wart augenblicklich nur, was Gott gibt, nimmt und tut", "tokens": ["Wart", "au\u00b7gen\u00b7blick\u00b7lich", "nur", ",", "was", "Gott", "gibt", ",", "nimmt", "und", "tut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "PRELS", "NN", "VVFIN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Leib und auch nach Seel', er wei\u00df ja, was dir gut!", "tokens": ["Nach", "Leib", "und", "auch", "nach", "Seel'", ",", "er", "wei\u00df", "ja", ",", "was", "dir", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"],