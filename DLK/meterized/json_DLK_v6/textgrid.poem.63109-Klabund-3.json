{"textgrid.poem.63109": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und schick noch einmal deine Raben,", "tokens": ["Und", "schick", "noch", "ein\u00b7mal", "dei\u00b7ne", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Raben, die Elias speisten.", "tokens": ["Die", "Ra\u00b7ben", ",", "die", "E\u00b7lias", "speis\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir haben nichts mehr, was wir haben.", "tokens": ["Wir", "ha\u00b7ben", "nichts", "mehr", ",", "was", "wir", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Dr\u00fcsen faulen in den Leisten,", "tokens": ["Die", "Dr\u00fc\u00b7sen", "fau\u00b7len", "in", "den", "Leis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Abfall fra\u00dfen l\u00e4ngst die Schaben.", "tokens": ["Den", "Ab\u00b7fall", "fra\u00b7\u00dfen", "l\u00e4ngst", "die", "Scha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Fl\u00f6he springen vom Skelette,", "tokens": ["Die", "Fl\u00f6\u00b7he", "sprin\u00b7gen", "vom", "Ske\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Glocken schweigen in den Str\u00e4ngen.", "tokens": ["Die", "Glo\u00b7cken", "schwei\u00b7gen", "in", "den", "Str\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wanzen wandern aus dem Bette.", "tokens": ["Die", "Wan\u00b7zen", "wan\u00b7dern", "aus", "dem", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts bleibt uns als die Sch\u00e4delst\u00e4tte", "tokens": ["Nichts", "bleibt", "uns", "als", "die", "Sch\u00e4\u00b7del\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und als ein Kreuz, uns dran zu h\u00e4ngen.", "tokens": ["Und", "als", "ein", "Kreuz", ",", "uns", "dran", "zu", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PPER", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}