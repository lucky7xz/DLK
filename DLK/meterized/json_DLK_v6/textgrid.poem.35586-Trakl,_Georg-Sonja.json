{"textgrid.poem.35586": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sonja", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abend kehrt in alten Garten;", "tokens": ["A\u00b7bend", "kehrt", "in", "al\u00b7ten", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonjas Leben, blaue Stille.", "tokens": ["Son\u00b7jas", "Le\u00b7ben", ",", "blau\u00b7e", "Stil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilder V\u00f6gel Wanderfahrten;", "tokens": ["Wil\u00b7der", "V\u00f6\u00b7gel", "Wan\u00b7der\u00b7fahr\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kahler Baum in Herbst und Stille.", "tokens": ["Kah\u00b7ler", "Baum", "in", "Herbst", "und", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonnenblume, sanftgeneigte", "tokens": ["Son\u00b7nen\u00b7blu\u00b7me", ",", "sanft\u00b7ge\u00b7neig\u00b7te"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Sonjas wei\u00dfes Leben.", "tokens": ["\u00dc\u00b7ber", "Son\u00b7jas", "wei\u00b7\u00dfes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wunde, rote, niegezeigte", "tokens": ["Wun\u00b7de", ",", "ro\u00b7te", ",", "nie\u00b7ge\u00b7zeig\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft in dunklen Zimmern leben,", "tokens": ["L\u00e4\u00dft", "in", "dunk\u00b7len", "Zim\u00b7mern", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo die blauen Glocken l\u00e4uten;", "tokens": ["Wo", "die", "blau\u00b7en", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonjas Schritt und sanfte Stille.", "tokens": ["Son\u00b7jas", "Schritt", "und", "sanf\u00b7te", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterbend Tier gr\u00fc\u00dft im Entgleiten,", "tokens": ["Ster\u00b7bend", "Tier", "gr\u00fc\u00dft", "im", "Ent\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kahler Baum in Herbst und Stille.", "tokens": ["Kah\u00b7ler", "Baum", "in", "Herbst", "und", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonne alter Tage leuchtet", "tokens": ["Son\u00b7ne", "al\u00b7ter", "Ta\u00b7ge", "leuch\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Sonjas wei\u00dfe Brauen,", "tokens": ["\u00dc\u00b7ber", "Son\u00b7jas", "wei\u00b7\u00dfe", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schnee, der ihre Wangen feuchtet,", "tokens": ["Schnee", ",", "der", "ih\u00b7re", "Wan\u00b7gen", "feuch\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Wildnis ihrer Brauen.", "tokens": ["Und", "die", "Wild\u00b7nis", "ih\u00b7rer", "Brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}