{"textgrid.poem.32677": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Einladung in die Laube", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu kurz ist dieses Leben, um zu klagen,", "tokens": ["Zu", "kurz", "ist", "die\u00b7ses", "Le\u00b7ben", ",", "um", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PDAT", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und viel der Freuden sind noch ungef\u00fchlt;", "tokens": ["Und", "viel", "der", "Freu\u00b7den", "sind", "noch", "un\u00b7ge\u00b7f\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum la\u00df uns, Damon, jeden Gram verjagen,", "tokens": ["Drum", "la\u00df", "uns", ",", "Da\u00b7mon", ",", "je\u00b7den", "Gram", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NE", "$,", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der in der Seele w\u00fchlt!", "tokens": ["Der", "in", "der", "See\u00b7le", "w\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zur Freude schuf uns die Natur den Lenzen,", "tokens": ["Zur", "Freu\u00b7de", "schuf", "uns", "die", "Na\u00b7tur", "den", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tausend bunte Fr\u00fchlingsblumen bl\u00e4hn", "tokens": ["Und", "tau\u00b7send", "bun\u00b7te", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", "bl\u00e4hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Garten sich, sie allesamt zu Kr\u00e4nzen", "tokens": ["Im", "Gar\u00b7ten", "sich", ",", "sie", "al\u00b7le\u00b7samt", "zu", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PRF", "$,", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um unser Haupt zu drehn.", "tokens": ["Um", "un\u00b7ser", "Haupt", "zu", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Komm in die k\u00fchle Nacht der Sommerlaube,", "tokens": ["Komm", "in", "die", "k\u00fch\u00b7le", "Nacht", "der", "Som\u00b7mer\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo lieblicher Jesmin bei Rosen bl\u00fcht,", "tokens": ["Wo", "lieb\u00b7li\u00b7cher", "Jes\u00b7min", "bei", "Ro\u00b7sen", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und feuriger der Saft der rhein'schen Traube", "tokens": ["Und", "feu\u00b7ri\u00b7ger", "der", "Saft", "der", "rhein'\u00b7schen", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Deckelglase gl\u00fcht!", "tokens": ["Im", "De\u00b7ckel\u00b7gla\u00b7se", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Manch Rosenbl\u00e4ttchen schwimmt, herabgerissen,", "tokens": ["Manch", "Ro\u00b7sen\u00b7bl\u00e4tt\u00b7chen", "schwimmt", ",", "her\u00b7ab\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im edeln Wein, und ruft uns warnend zu:", "tokens": ["Im", "e\u00b7deln", "Wein", ",", "und", "ruft", "uns", "war\u00b7nend", "zu", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbeil J\u00fcngling, deine Tage zu genie\u00dfen!", "tokens": ["\u00bb", "eil", "J\u00fcng\u00b7ling", ",", "dei\u00b7ne", "Ta\u00b7ge", "zu", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn sterblich bist auch du.\u00ab", "tokens": ["Denn", "sterb\u00b7lich", "bist", "auch", "du", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Um Chloen girrst du, Damon, wie die Taube,", "tokens": ["Um", "Chloen", "girrst", "du", ",", "Da\u00b7mon", ",", "wie", "die", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "VVFIN", "PPER", "$,", "NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sendest tausend Seufzer hin zu ihr;", "tokens": ["Und", "sen\u00b7dest", "tau\u00b7send", "Seuf\u00b7zer", "hin", "zu", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "ADV", "APPR", "PPOSAT", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie aber trinkt indes in meiner Laube", "tokens": ["Sie", "a\u00b7ber", "trinkt", "in\u00b7des", "in", "mei\u00b7ner", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Daphnen, und mit mir.", "tokens": ["Mit", "Daph\u00b7nen", ",", "und", "mit", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O komm, verzagter J\u00fcngling, deine Klagen,", "tokens": ["O", "komm", ",", "ver\u00b7zag\u00b7ter", "J\u00fcng\u00b7ling", ",", "dei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die im Herzen tiefverborgne Pein,", "tokens": ["Und", "die", "im", "Her\u00b7zen", "tief\u00b7ver\u00b7borg\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit offner Brust, ihr freier vorzusagen:", "tokens": ["Mit", "off\u00b7ner", "Brust", ",", "ihr", "frei\u00b7er", "vor\u00b7zu\u00b7sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn k\u00fchner macht der Wein;", "tokens": ["Denn", "k\u00fch\u00b7ner", "macht", "der", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und milder auch! Der Liebe sanftes Feuer", "tokens": ["Und", "mil\u00b7der", "auch", "!", "Der", "Lie\u00b7be", "sanf\u00b7tes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Strahlt schon aus ihrem blauen Aug'; es lacht", "tokens": ["Strahlt", "schon", "aus", "ih\u00b7rem", "blau\u00b7en", "Aug'", ";", "es", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So freundlich, wie Diana, ohne Schleier,", "tokens": ["So", "freund\u00b7lich", ",", "wie", "Di\u00b7a\u00b7na", ",", "oh\u00b7ne", "Schlei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NE", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einer Maiennacht.", "tokens": ["In", "ei\u00b7ner", "Mai\u00b7en\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Komm, was sie lange barg, anjetzt zu sehen,", "tokens": ["Komm", ",", "was", "sie", "lan\u00b7ge", "barg", ",", "an\u00b7jetzt", "zu", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Herz, das deinem Herzen zugeh\u00f6rt:", "tokens": ["Ein", "Herz", ",", "das", "dei\u00b7nem", "Her\u00b7zen", "zu\u00b7ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn seine tiefsten Winkel auszusp\u00e4hen,", "tokens": ["Denn", "sei\u00b7ne", "tiefs\u00b7ten", "Win\u00b7kel", "aus\u00b7zu\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat Bacchus mich gelehrt. \u2013", "tokens": ["Hat", "Bac\u00b7chus", "mich", "ge\u00b7lehrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NE", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O, wer beherrscht die Herzen allgemeiner,", "tokens": ["O", ",", "wer", "be\u00b7herrscht", "die", "Her\u00b7zen", "all\u00b7ge\u00b7mei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Vater Bacchus! Solche Blicke thut,", "tokens": ["Als", "Va\u00b7ter", "Bac\u00b7chus", "!", "Sol\u00b7che", "Bli\u00b7cke", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis tief ins Herz hinab, der Weisen keiner,", "tokens": ["Bis", "tief", "ins", "Herz", "hin\u00b7ab", ",", "der", "Wei\u00b7sen", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ich beim Traubenblut.", "tokens": ["Wie", "ich", "beim", "Trau\u00b7ben\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da winden sich Gedanken an Gedanken", "tokens": ["Da", "win\u00b7den", "sich", "Ge\u00b7dan\u00b7ken", "an", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewaltig aus dem engen Kerker los;", "tokens": ["Ge\u00b7wal\u00b7tig", "aus", "dem", "en\u00b7gen", "Ker\u00b7ker", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geheimnisse durchbrechen ihre Schranken,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7se", "durch\u00b7bre\u00b7chen", "ih\u00b7re", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fliehn in Freundes Scho\u00df.", "tokens": ["Und", "fliehn", "in", "Freun\u00b7des", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}