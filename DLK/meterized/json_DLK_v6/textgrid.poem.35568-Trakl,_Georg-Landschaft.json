{"textgrid.poem.35568": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Landschaft", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Septemberabend; traurig t\u00f6nen die dunklen Rufe der Hirten", "tokens": ["Sep\u00b7tem\u00b7ber\u00b7a\u00b7bend", ";", "trau\u00b7rig", "t\u00f6\u00b7nen", "die", "dunk\u00b7len", "Ru\u00b7fe", "der", "Hir\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+--+-+--+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Durch das d\u00e4mmernde Dorf; Feuer spr\u00fcht in der Schmiede.", "tokens": ["Durch", "das", "d\u00e4m\u00b7mern\u00b7de", "Dorf", ";", "Feu\u00b7er", "spr\u00fcht", "in", "der", "Schmie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Gewaltig b\u00e4umt sich ein schwarzes Pferd; die hyazinthenen Locken der Magd", "tokens": ["Ge\u00b7wal\u00b7tig", "b\u00e4umt", "sich", "ein", "schwar\u00b7zes", "Pferd", ";", "die", "hy\u00b7a\u00b7zin\u00b7the\u00b7nen", "Lo\u00b7cken", "der", "Magd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Haschen nach der Inbrunst seiner purpurnen N\u00fcstern.", "tokens": ["Ha\u00b7schen", "nach", "der", "In\u00b7brunst", "sei\u00b7ner", "pur\u00b7pur\u00b7nen", "N\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Leise erstarrt am Saum des Waldes der Schrei der Hirschkuh", "tokens": ["Lei\u00b7se", "er\u00b7starrt", "am", "Saum", "des", "Wal\u00b7des", "der", "Schrei", "der", "Hirschkuh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Und die gelben Blumen des Herbstes", "tokens": ["Und", "die", "gel\u00b7ben", "Blu\u00b7men", "des", "Herbs\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Neigen sich sprachlos \u00fcber das blaue Antlitz des Teichs.", "tokens": ["Nei\u00b7gen", "sich", "sprach\u00b7los", "\u00fc\u00b7ber", "das", "blau\u00b7e", "Ant\u00b7litz", "des", "Teichs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "In roter Flamme verbrannte ein Baum; aufflattern mit dunklen Gesichtern die Flederm\u00e4use.", "tokens": ["In", "ro\u00b7ter", "Flam\u00b7me", "ver\u00b7brann\u00b7te", "ein", "Baum", ";", "auf\u00b7flat\u00b7tern", "mit", "dunk\u00b7len", "Ge\u00b7sich\u00b7tern", "die", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+--+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}