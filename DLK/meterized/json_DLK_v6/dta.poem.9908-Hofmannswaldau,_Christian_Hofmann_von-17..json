{"dta.poem.9908": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo es pr\u00e4chtig gehet zu/ kan man melner nicht entbehren:", "tokens": ["Wo", "es", "pr\u00e4ch\u00b7tig", "ge\u00b7het", "zu", "/", "kan", "man", "mel\u00b7ner", "nicht", "ent\u00b7beh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "PTKVZ", "$(", "VMFIN", "PIS", "PIS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "F\u00fcrsten machen mir oft platz/ jeder pflegt mich zu begehren.", "tokens": ["F\u00fcrs\u00b7ten", "ma\u00b7chen", "mir", "oft", "platz", "/", "je\u00b7der", "pflegt", "mich", "zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "$(", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Feuer/ das ist meine seele/ frist den leib mir aber auf/", "tokens": ["Feu\u00b7er", "/", "das", "ist", "mei\u00b7ne", "see\u00b7le", "/", "frist", "den", "leib", "mir", "a\u00b7ber", "auf", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und mit heissen thr\u00e4nen-fl\u00fcssen endigt sich mein lebens-lauff.", "tokens": ["Und", "mit", "heis\u00b7sen", "thr\u00e4\u00b7nen\u00b7fl\u00fcs\u00b7sen", "en\u00b7digt", "sich", "mein", "le\u00b7bens\u00b7lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-++-", "measure": "unknown.measure.octa.plus"}}}}}