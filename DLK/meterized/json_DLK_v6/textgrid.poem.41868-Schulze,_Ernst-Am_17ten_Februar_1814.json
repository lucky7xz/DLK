{"textgrid.poem.41868": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Am 17ten Februar 1814", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hold windet sich der bunte Schmuck zum Kranze,", "tokens": ["Hold", "win\u00b7det", "sich", "der", "bun\u00b7te", "Schmuck", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bl\u00fcmchen mu\u00df des andern Reiz erh\u00f6hn,", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "mu\u00df", "des", "an\u00b7dern", "Reiz", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entfaltet lacht die Bl\u00fcth' im milden Glanze,", "tokens": ["Ent\u00b7fal\u00b7tet", "lacht", "die", "Bl\u00fcth'", "im", "mil\u00b7den", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Knospe strebt, zum Licht emporzusehn,", "tokens": ["Die", "Knos\u00b7pe", "strebt", ",", "zum", "Licht", "em\u00b7por\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch jeder Kelch bewahrt das eigne Wehn,", "tokens": ["Doch", "je\u00b7der", "Kelch", "be\u00b7wahrt", "das", "eig\u00b7ne", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und zierlich ruht der Kranz in heller Schale", "tokens": ["Und", "zier\u00b7lich", "ruht", "der", "Kranz", "in", "hel\u00b7ler", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und l\u00e4chelt, da\u00df er l\u00e4chelnd wiederstrahle.", "tokens": ["Und", "l\u00e4\u00b7chelt", ",", "da\u00df", "er", "l\u00e4\u00b7chelnd", "wie\u00b7der\u00b7strah\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So freundlich hat in deinem zarten Leben", "tokens": ["So", "freund\u00b7lich", "hat", "in", "dei\u00b7nem", "zar\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder Schmuck des Sch\u00f6nen sich enth\u00fcllt.", "tokens": ["Ein", "je\u00b7der", "Schmuck", "des", "Sch\u00f6\u00b7nen", "sich", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vollendung scheint der Reiz dem Reiz zu geben,", "tokens": ["Vol\u00b7len\u00b7dung", "scheint", "der", "Reiz", "dem", "Reiz", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Durch Harmonie des Wechsels Streit gestillt,", "tokens": ["Durch", "Har\u00b7mo\u00b7nie", "des", "Wech\u00b7sels", "Streit", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und tausend Farben seh' ich lieblich schweben,", "tokens": ["Und", "tau\u00b7send", "Far\u00b7ben", "seh'", "ich", "lieb\u00b7lich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus allen taucht ein einz'ges sch\u00f6nes Bild;", "tokens": ["Aus", "al\u00b7len", "taucht", "ein", "einz'\u00b7ges", "sch\u00f6\u00b7nes", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und leuchtend blickt im heiteren Gem\u00fcthe", "tokens": ["Und", "leuch\u00b7tend", "blickt", "im", "hei\u00b7te\u00b7ren", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Wahrheit Glanz selbst aus des Traumes Bl\u00fcthe.", "tokens": ["Der", "Wahr\u00b7heit", "Glanz", "selbst", "aus", "des", "Trau\u00b7mes", "Bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}