{"textgrid.poem.61736": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Einst geschieht's", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst geschieht's, da wird die Schmach", "tokens": ["Einst", "ge\u00b7schieht's", ",", "da", "wird", "die", "Schmach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seines Volks der Herr zerbrechen;", "tokens": ["Sei\u00b7nes", "Volks", "der", "Herr", "zer\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der auf Leipzigs Feldern sprach,", "tokens": ["Der", "auf", "Leip\u00b7zigs", "Fel\u00b7dern", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird im Donner wieder sprechen.", "tokens": ["Wird", "im", "Don\u00b7ner", "wie\u00b7der", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann, o Deutschland, sei getrost!", "tokens": ["Dann", ",", "o", "Deutschland", ",", "sei", "ge\u00b7trost", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NE", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dieses ist das erste Zeichen,", "tokens": ["Die\u00b7ses", "ist", "das", "ers\u00b7te", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn verb\u00fcndet West und Ost", "tokens": ["Wenn", "ver\u00b7b\u00fcn\u00b7det", "West", "und", "Ost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wider dich die Hand sich reichen.", "tokens": ["Wi\u00b7der", "dich", "die", "Hand", "sich", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn verb\u00fcndet Ost und West", "tokens": ["Wenn", "ver\u00b7b\u00fcn\u00b7det", "Ost", "und", "West"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wider dich zum Schwerte fassen,", "tokens": ["Wi\u00b7der", "dich", "zum", "Schwer\u00b7te", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wisse, da\u00df dich Gott nicht l\u00e4\u00dft,", "tokens": ["Wis\u00b7se", ",", "da\u00df", "dich", "Gott", "nicht", "l\u00e4\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So du nicht dich selbst verlassen.", "tokens": ["So", "du", "nicht", "dich", "selbst", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deinen alten Bruderzwist", "tokens": ["Dei\u00b7nen", "al\u00b7ten", "Bru\u00b7der\u00b7zwist"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird das Wetter dann verzehren;", "tokens": ["Wird", "das", "Wet\u00b7ter", "dann", "ver\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Taten wird zu dieser Frist,", "tokens": ["Ta\u00b7ten", "wird", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Helden dir die Not geb\u00e4ren,", "tokens": ["Hel\u00b7den", "dir", "die", "Not", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bis du wieder stark wie sonst,", "tokens": ["Bis", "du", "wie\u00b7der", "stark", "wie", "sonst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Stirn der Herrschaft Zeichen,", "tokens": ["Auf", "der", "Stirn", "der", "Herr\u00b7schaft", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor Europas V\u00f6lkern thronst,", "tokens": ["Vor", "Eu\u00b7ro\u00b7pas", "V\u00f6l\u00b7kern", "thronst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine F\u00fcrstin sondergleichen.", "tokens": ["Ei\u00b7ne", "F\u00fcrs\u00b7tin", "son\u00b7derg\u00b7lei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schlage, schlage denn empor", "tokens": ["Schla\u00b7ge", ",", "schla\u00b7ge", "denn", "em\u00b7por"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4utrungsglut des Weltenbrandes!", "tokens": ["L\u00e4ut\u00b7rungs\u00b7glut", "des", "Wel\u00b7ten\u00b7bran\u00b7des", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steig als Ph\u00f6nix draus hervor,", "tokens": ["Steig", "als", "Ph\u00f6\u00b7nix", "draus", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "PAV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaiseraar des deutschen Landes!", "tokens": ["Kai\u00b7se\u00b7raar", "des", "deut\u00b7schen", "Lan\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}