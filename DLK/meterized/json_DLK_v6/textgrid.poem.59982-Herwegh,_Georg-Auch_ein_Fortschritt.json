{"textgrid.poem.59982": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auch ein Fortschritt", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir zogen von Gotha bis Eisenach", "tokens": ["Wir", "zo\u00b7gen", "von", "Go\u00b7tha", "bis", "Ei\u00b7se\u00b7nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In zehen Jahren, gemach, gemach;", "tokens": ["In", "ze\u00b7hen", "Jah\u00b7ren", ",", "ge\u00b7mach", ",", "ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADV", "$,", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Von Gotha bis Eisenach sind drei Meilen \u2013", "tokens": ["Von", "Go\u00b7tha", "bis", "Ei\u00b7se\u00b7nach", "sind", "drei", "Mei\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "VAFIN", "CARD", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Staatsm\u00e4nner sollen sich nicht \u00fcbereilen.", "tokens": ["Staats\u00b7m\u00e4n\u00b7ner", "sol\u00b7len", "sich", "nicht", "\u00fc\u00b7be\u00b7rei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir zogen von Gotha bis Eisenach", "tokens": ["Wir", "zo\u00b7gen", "von", "Go\u00b7tha", "bis", "Ei\u00b7se\u00b7nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zehn Jahre; \u2013 wir streben den Griechen nach:", "tokens": ["Zehn", "Jah\u00b7re", ";", "\u2013", "wir", "stre\u00b7ben", "den", "Grie\u00b7chen", "nach", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Zehn Jahre mu\u00dften sie Troja belagern \u2013", "tokens": ["Zehn", "Jah\u00b7re", "mu\u00df\u00b7ten", "sie", "Tro\u00b7ja", "be\u00b7la\u00b7gern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "NE", "VVINF", "$("], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie hatten Achill, wir hatten Gagern.", "tokens": ["Sie", "hat\u00b7ten", "A\u00b7chill", ",", "wir", "hat\u00b7ten", "Ga\u00b7gern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "PPER", "VAFIN", "NE", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir zogen von Gotha bis Eisenach \u2013", "tokens": ["Wir", "zo\u00b7gen", "von", "Go\u00b7tha", "bis", "Ei\u00b7se\u00b7nach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Politik, o trauriges Fach!", "tokens": ["O", "Po\u00b7li\u00b7tik", ",", "o", "trau\u00b7ri\u00b7ges", "Fach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es ist sehr schwierig, den Stall zu rein'gen", "tokens": ["Es", "ist", "sehr", "schwie\u00b7rig", ",", "den", "Stall", "zu", "rein'\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sein langwierig Deutschland zu ein'gen.", "tokens": ["Und", "sein", "lang\u00b7wie\u00b7rig", "Deutschland", "zu", "ein'\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir zogen von Gotha bis Eisenach,", "tokens": ["Wir", "zo\u00b7gen", "von", "Go\u00b7tha", "bis", "Ei\u00b7se\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Luther dem Teufel geboten Schach;", "tokens": ["Wo", "Lu\u00b7ther", "dem", "Teu\u00b7fel", "ge\u00b7bo\u00b7ten", "Schach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wir werfen noch immer mit Tintenf\u00e4ssern,", "tokens": ["Wir", "wer\u00b7fen", "noch", "im\u00b7mer", "mit", "Tin\u00b7ten\u00b7f\u00e4s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wir verstehn's, die Tinte zu w\u00e4ssern.", "tokens": ["Doch", "wir", "ver\u00b7stehn's", ",", "die", "Tin\u00b7te", "zu", "w\u00e4s\u00b7sern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir zogen von Gotha bis Eisenach", "tokens": ["Wir", "zo\u00b7gen", "von", "Go\u00b7tha", "bis", "Ei\u00b7se\u00b7nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zehn Jahre \u2013 vertrocknet ist mancher Bach,", "tokens": ["Zehn", "Jah\u00b7re", "\u2013", "ver\u00b7trock\u00b7net", "ist", "man\u00b7cher", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Manch Herz verdorrt wie eine Dattel,", "tokens": ["Manch", "Herz", "ver\u00b7dorrt", "wie", "ei\u00b7ne", "Dat\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch mancher Freund nicht fest mehr im Sattel.", "tokens": ["Auch", "man\u00b7cher", "Freund", "nicht", "fest", "mehr", "im", "Sat\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir zogen von Gotha bis Eisenach \u2013", "tokens": ["Wir", "zo\u00b7gen", "von", "Go\u00b7tha", "bis", "Ei\u00b7se\u00b7nach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Manch eiserner Trutz wie Glas zerbrach;", "tokens": ["Manch", "ei\u00b7ser\u00b7ner", "Trutz", "wie", "Glas", "zer\u00b7brach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem Rausch folgt oft ein greulicher Kater,", "tokens": ["Dem", "Rausch", "folgt", "oft", "ein", "greu\u00b7li\u00b7cher", "Ka\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir singen wieder den \u00bbLandesvater\u00ab.", "tokens": ["Wir", "sin\u00b7gen", "wie\u00b7der", "den", "\u00bb", "Lan\u00b7des\u00b7va\u00b7ter", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "$(", "NN", "$(", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Es schl\u00e4ft sich so s\u00fc\u00df in Eisenach \u2013", "tokens": ["Es", "schl\u00e4ft", "sich", "so", "s\u00fc\u00df", "in", "Ei\u00b7se\u00b7nach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine sch\u00f6ne Gegend, auch nicht zu flach;", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Ge\u00b7gend", ",", "auch", "nicht", "zu", "flach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Ochsen k\u00f6nnen dort stehn am Berge,", "tokens": ["Die", "Och\u00b7sen", "k\u00f6n\u00b7nen", "dort", "stehn", "am", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Th\u00fcringer Wald gibt's viele Zwerge.", "tokens": ["Im", "Th\u00fc\u00b7rin\u00b7ger", "Wald", "gibt's", "vie\u00b7le", "Zwer\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Im Th\u00fcringer Wald bei Eisenach,", "tokens": ["Im", "Th\u00fc\u00b7rin\u00b7ger", "Wald", "bei", "Ei\u00b7se\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl unter germanischer Eichen Dach,", "tokens": ["Wohl", "un\u00b7ter", "ger\u00b7ma\u00b7ni\u00b7scher", "Ei\u00b7chen", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sitzen die Feen, sie sitzen und sinnen \u2013", "tokens": ["Da", "sit\u00b7zen", "die", "Feen", ",", "sie", "sit\u00b7zen", "und", "sin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVINF", "KON", "VVINF", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich m\u00f6chte wohl wissen, was sie jetzt spinnen.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wohl", "wis\u00b7sen", ",", "was", "sie", "jetzt", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Sie sitzen und sinnen um Eisenach \u2013", "tokens": ["Sie", "sit\u00b7zen", "und", "sin\u00b7nen", "um", "Ei\u00b7se\u00b7nach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Besinnen ist eine sch\u00f6ne Sach:", "tokens": ["Be\u00b7sin\u00b7nen", "ist", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Sach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo bleibt der Gagern? und werden wir's bringen", "tokens": ["Wo", "bleibt", "der", "Ga\u00b7gern", "?", "und", "wer\u00b7den", "wir's", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "KON", "VAFIN", "PIS", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Gottes Hilfe noch bis Meiningen?", "tokens": ["Mit", "Got\u00b7tes", "Hil\u00b7fe", "noch", "bis", "Mei\u00b7nin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Man kann auch bis Jena von Eisenach,", "tokens": ["Man", "kann", "auch", "bis", "Je\u00b7na", "von", "Ei\u00b7se\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel schneller als anno Sechse, ach!", "tokens": ["Viel", "schnel\u00b7ler", "als", "an\u00b7no", "Sech\u00b7se", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "FM", "FM", "$,", "ITJ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Eisenbahn ist eine sch\u00f6ne Erfindung,", "tokens": ["Die", "Ei\u00b7sen\u00b7bahn", "ist", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Er\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Deutsche Bund ist eine sch\u00f6ne Verbindung!", "tokens": ["Der", "Deut\u00b7sche", "Bund", "ist", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Ver\u00b7bin\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}