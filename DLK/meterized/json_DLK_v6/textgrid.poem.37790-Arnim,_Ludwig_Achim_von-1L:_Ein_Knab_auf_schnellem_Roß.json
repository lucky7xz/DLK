{"textgrid.poem.37790": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Knab auf schnellem Ro\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Knab auf schnellem Ro\u00df", "tokens": ["Ein", "Knab", "auf", "schnel\u00b7lem", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sprengt auf der Kaisrin Schlo\u00df,", "tokens": ["Sprengt", "auf", "der", "Kais\u00b7rin", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Ro\u00df zur Erd sich neigt,", "tokens": ["Das", "Ro\u00df", "zur", "Erd", "sich", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Knab sich zierlich beugt.", "tokens": ["Der", "Knab", "sich", "zier\u00b7lich", "beugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie lieblich, artig, sch\u00f6n", "tokens": ["Wie", "lieb\u00b7lich", ",", "ar\u00b7tig", ",", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Frauen sich ansehn,", "tokens": ["Die", "Frau\u00b7en", "sich", "an\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Horn trug seine Hand,", "tokens": ["Ein", "Horn", "trug", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Daran vier goldne Band.", "tokens": ["Da\u00b7ran", "vier", "gold\u00b7ne", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gar mancher sch\u00f6ne Stein", "tokens": ["Gar", "man\u00b7cher", "sch\u00f6\u00b7ne", "Stein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gelegt ins Gold hinein,", "tokens": ["Ge\u00b7legt", "ins", "Gold", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Perlen und Rubin", "tokens": ["Viel", "Per\u00b7len", "und", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Augen auf sich ziehn.", "tokens": ["Die", "Au\u00b7gen", "auf", "sich", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Horn vom Elephant,", "tokens": ["Das", "Horn", "vom", "E\u00b7le\u00b7phant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So gros man keinen fand,", "tokens": ["So", "gros", "man", "kei\u00b7nen", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sch\u00f6n man keinen fing", "tokens": ["So", "sch\u00f6n", "man", "kei\u00b7nen", "fing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIS", "PIAT", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und oben dran ein Ring,", "tokens": ["Und", "o\u00b7ben", "dran", "ein", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie Silber blinken kann", "tokens": ["Wie", "Sil\u00b7ber", "blin\u00b7ken", "kann"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und hundert Glocken dran", "tokens": ["Und", "hun\u00b7dert", "Glo\u00b7cken", "dran"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom feinsten Gold gemacht,", "tokens": ["Vom", "feins\u00b7ten", "Gold", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus tiefem Meer gebracht.", "tokens": ["Aus", "tie\u00b7fem", "Meer", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von einer Meerfey Hand", "tokens": ["Von", "ei\u00b7ner", "Meer\u00b7fey", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Kaiserin gesandt,", "tokens": ["Der", "Kai\u00b7se\u00b7rin", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu ihrer Reinheit Preis,", "tokens": ["Zu", "ih\u00b7rer", "Rein\u00b7heit", "Preis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dieweil sie sch\u00f6n und weis'.", "tokens": ["Die\u00b7weil", "sie", "sch\u00f6n", "und", "weis'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der sch\u00f6ne Knab sagt auch:", "tokens": ["Der", "sch\u00f6\u00b7ne", "Knab", "sagt", "auch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdies ist des Horns Gebrauch:", "tokens": ["\u00bb", "dies", "ist", "des", "Horns", "Ge\u00b7brauch", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Druck von Eurem Finger,", "tokens": ["Ein", "Druck", "von", "Eu\u00b7rem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Druck von Eurem Finger", "tokens": ["Ein", "Druck", "von", "Eu\u00b7rem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und diese Glocken all,", "tokens": ["Und", "die\u00b7se", "Glo\u00b7cken", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie geben s\u00fc\u00dfen Schall,", "tokens": ["Sie", "ge\u00b7ben", "s\u00fc\u00b7\u00dfen", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie nie ein Harfenklang", "tokens": ["Wie", "nie", "ein", "Har\u00b7fen\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und keiner Frauen Sang,", "tokens": ["Und", "kei\u00b7ner", "Frau\u00b7en", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Kein Vogel obenher,", "tokens": ["Kein", "Vo\u00b7gel", "o\u00b7ben\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Jungfraun nicht im Meer", "tokens": ["Die", "Jung\u00b7fraun", "nicht", "im", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie so was geben an!\u00ab", "tokens": ["Nie", "so", "was", "ge\u00b7ben", "an", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PWS", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Fort sprengt der Knab bergan,", "tokens": ["Fort", "sprengt", "der", "Knab", "ber\u00b7gan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Lie\u00df in der Kaisrin Hand", "tokens": ["Lie\u00df", "in", "der", "Kais\u00b7rin", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Horn, so weltbekannt;", "tokens": ["Das", "Horn", ",", "so", "welt\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Druck von ihrem Finger,", "tokens": ["Ein", "Druck", "von", "ih\u00b7rem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O s\u00fc\u00dfes hell Geklinge!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "hell", "Ge\u00b7klin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}