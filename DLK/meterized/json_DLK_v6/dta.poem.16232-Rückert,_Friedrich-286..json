{"dta.poem.16232": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "286.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unruhig ist die Welt, unruhig ist das Herz,", "tokens": ["Un\u00b7ru\u00b7hig", "ist", "die", "Welt", ",", "un\u00b7ru\u00b7hig", "ist", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und eins das andre setzt in Unruh allerwerts.", "tokens": ["Und", "eins", "das", "and\u00b7re", "setzt", "in", "Un\u00b7ruh", "al\u00b7ler\u00b7werts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "PIS", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Himmel nur ist Ruh, im Himmel nur ist Frieden;", "tokens": ["Im", "Him\u00b7mel", "nur", "ist", "Ruh", ",", "im", "Him\u00b7mel", "nur", "ist", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "NN", "$,", "APPRART", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O f\u00e4nd' ich Ruh, von mir und von der Welt geschieden!", "tokens": ["O", "f\u00e4nd'", "ich", "Ruh", ",", "von", "mir", "und", "von", "der", "Welt", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "$,", "APPR", "PPER", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Komm, Gottesruh, den Sturm mir aus der Brust zu hauchen!", "tokens": ["Komm", ",", "Got\u00b7tes\u00b7ruh", ",", "den", "Sturm", "mir", "aus", "der", "Brust", "zu", "hau\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich den Krieg der Welt in deinen Frieden tauchen.", "tokens": ["La\u00df", "mich", "den", "Krieg", "der", "Welt", "in", "dei\u00b7nen", "Frie\u00b7den", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}