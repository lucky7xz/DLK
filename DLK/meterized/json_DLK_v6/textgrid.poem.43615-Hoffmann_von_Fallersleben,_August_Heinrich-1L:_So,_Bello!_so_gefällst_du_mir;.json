{"textgrid.poem.43615": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: So, Bello! so gef\u00e4llst du mir;", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So, Bello! so gef\u00e4llst du mir;", "tokens": ["So", ",", "Bel\u00b7lo", "!", "so", "ge\u00b7f\u00e4llst", "du", "mir", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn artig und gehorsam sein", "tokens": ["Denn", "ar\u00b7tig", "und", "ge\u00b7hor\u00b7sam", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist eine Zier", "tokens": ["Ist", "ei\u00b7ne", "Zier"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "F\u00fcr Gro\u00df und Klein.", "tokens": ["F\u00fcr", "Gro\u00df", "und", "Klein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und mit Geschicklichkeit und Kunst", "tokens": ["Und", "mit", "Ge\u00b7schick\u00b7lich\u00b7keit", "und", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwirbst du \u2013 k\u00f6nnt's wohl anders sein! \u2013", "tokens": ["Er\u00b7wirbst", "du", "\u2013", "k\u00f6nnt's", "wohl", "an\u00b7ders", "sein", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$(", "VMFIN", "ADV", "ADV", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir Lieb' und Gunst", "tokens": ["Dir", "Lieb'", "und", "Gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bei Gro\u00df und Klein.", "tokens": ["Bei", "Gro\u00df", "und", "Klein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wer Allen so wie du gef\u00e4llt,", "tokens": ["Wer", "Al\u00b7len", "so", "wie", "du", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der findet \u00fcberall sein Brot,", "tokens": ["Der", "fin\u00b7det", "\u00fc\u00b7be\u00b7rall", "sein", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat auf der Welt", "tokens": ["Hat", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Nie Sorg' und Noth.", "tokens": ["Nie", "Sor\u00b7g'", "und", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So, Bello! hast es brav gemacht!", "tokens": ["So", ",", "Bel\u00b7lo", "!", "hast", "es", "brav", "ge\u00b7macht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$.", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du stehst und bettelst mit Manier.", "tokens": ["Du", "stehst", "und", "bet\u00b7telst", "mit", "Ma\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dank sei gebracht,", "tokens": ["Dank", "sei", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mein H\u00fcndlein, dir!", "tokens": ["Mein", "H\u00fcnd\u00b7lein", ",", "dir", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}