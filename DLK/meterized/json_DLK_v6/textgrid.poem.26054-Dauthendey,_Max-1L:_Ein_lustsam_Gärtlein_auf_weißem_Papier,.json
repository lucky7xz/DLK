{"textgrid.poem.26054": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein lustsam G\u00e4rtlein auf wei\u00dfem Papier,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein lustsam G\u00e4rtlein auf wei\u00dfem Papier,", "tokens": ["Ein", "lust\u00b7sam", "G\u00e4rt\u00b7lein", "auf", "wei\u00b7\u00dfem", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Nie welke drinnen Lied noch Blatt.", "tokens": ["Nie", "wel\u00b7ke", "drin\u00b7nen", "Lied", "noch", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAT", "ADJA", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Buchstaben stehen als Blumen hier,", "tokens": ["Buch\u00b7sta\u00b7ben", "ste\u00b7hen", "als", "Blu\u00b7men", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus Reim und Zeil' es Landschaft hat.", "tokens": ["Aus", "Reim", "und", "Zeil'", "es", "Land\u00b7schaft", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du findest dort den ersten Keim,", "tokens": ["Du", "fin\u00b7dest", "dort", "den", "ers\u00b7ten", "Keim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Fr\u00fchling voller Liebessinn,", "tokens": ["Den", "Fr\u00fch\u00b7ling", "vol\u00b7ler", "Lie\u00b7bes\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis in den Sommer voll Honigseim.", "tokens": ["Bis", "in", "den", "Som\u00b7mer", "voll", "Ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Schick' deine Augen wie Bienen hin,", "tokens": ["Schick'", "dei\u00b7ne", "Au\u00b7gen", "wie", "Bie\u00b7nen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Jed' Lied will lustsam als Laube dienen.", "tokens": ["Jed'", "Lied", "will", "lust\u00b7sam", "als", "Lau\u00b7be", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}