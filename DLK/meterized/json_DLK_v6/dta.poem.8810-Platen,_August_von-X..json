{"dta.poem.8810": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Hat sie doch im Paradiese der Prophete nicht versagt!", "tokens": ["Hat", "sie", "doch", "im", "Pa\u00b7ra\u00b7die\u00b7se", "der", "Pro\u00b7phe\u00b7te", "nicht", "ver\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn er euch den Wein verboten, hat er wohl bedacht,", "tokens": ["Wenn", "er", "euch", "den", "Wein", "ver\u00b7bo\u00b7ten", ",", "hat", "er", "wohl", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Doch ein Thor, wer nach Geboten oder nach Verboten", "tokens": ["Doch", "ein", "Thor", ",", "wer", "nach", "Ge\u00b7bo\u00b7ten", "o\u00b7der", "nach", "Ver\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWS", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "H\u00f6rtet ihr die Rose fragen, ob sie bl\u00fchen darf? Sie", "tokens": ["H\u00f6r\u00b7tet", "ihr", "die", "Ro\u00b7se", "fra\u00b7gen", ",", "ob", "sie", "bl\u00fc\u00b7hen", "darf", "?", "Sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$.", "PPER"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "H\u00f6rtet ihr das Echo fragen, ob es klagen darf? Es", "tokens": ["H\u00f6r\u00b7tet", "ihr", "das", "E\u00b7cho", "fra\u00b7gen", ",", "ob", "es", "kla\u00b7gen", "darf", "?", "Es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$.", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Vom Gebirge f\u00e4llt die Quelle, rinnt als Silberflu\u00df", "tokens": ["Vom", "Ge\u00b7bir\u00b7ge", "f\u00e4llt", "die", "Quel\u00b7le", ",", "rinnt", "als", "Sil\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "KOUS", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Prallt am Felsen ab und spr\u00fctzet bis zum Himmel un-", "tokens": ["Prallt", "am", "Fel\u00b7sen", "ab", "und", "spr\u00fct\u00b7zet", "bis", "zum", "Him\u00b7mel", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Kl\u00e4glich me\u00dft ihr eure Schritte, weil ihr strauchelt jeden", "tokens": ["Kl\u00e4g\u00b7lich", "me\u00dft", "ihr", "eu\u00b7re", "Schrit\u00b7te", ",", "weil", "ihr", "strau\u00b7chelt", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "PIAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Doch es f\u00fcrchtet nicht zu fallen, wer f\u00fcr Alles Alles", "tokens": ["Doch", "es", "f\u00fcrch\u00b7tet", "nicht", "zu", "fal\u00b7len", ",", "wer", "f\u00fcr", "Al\u00b7les", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "PWS", "APPR", "PIS", "PIS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Staunet nicht, wenn unser ", "tokens": ["Stau\u00b7net", "nicht", ",", "wenn", "un\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Da ihr stets des Lebens Sorge, wie der B\u00e4r die Pfote,", "tokens": ["Da", "ihr", "stets", "des", "Le\u00b7bens", "Sor\u00b7ge", ",", "wie", "der", "B\u00e4r", "die", "Pfo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}