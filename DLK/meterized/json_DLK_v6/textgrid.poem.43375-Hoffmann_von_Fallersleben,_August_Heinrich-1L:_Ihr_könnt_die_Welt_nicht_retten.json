{"textgrid.poem.43375": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr k\u00f6nnt die Welt nicht retten", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr k\u00f6nnt die Welt nicht retten", "tokens": ["Ihr", "k\u00f6nnt", "die", "Welt", "nicht", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Hals- und Hochgericht;", "tokens": ["Mit", "Hals", "und", "Hoch\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit des Gefangnen Ketten", "tokens": ["Mit", "des", "Ge\u00b7fang\u00b7nen", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hemmt ihr sein Laster nicht.", "tokens": ["Hemmt", "ihr", "sein", "Las\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Arbeitshaus' erwachet", "tokens": ["Im", "Ar\u00b7beits\u00b7haus'", "er\u00b7wa\u00b7chet"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht Flei\u00df und Arbeitstrieb;", "tokens": ["Nicht", "Flei\u00df", "und", "Ar\u00b7beit\u00b7strieb", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Zwangs- und Zuchthaus machet", "tokens": ["Das", "Zwangs", "und", "Zucht\u00b7haus", "ma\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht tugendhaft den Dieb.", "tokens": ["Nicht", "tu\u00b7gend\u00b7haft", "den", "Dieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bei Brot und Wasser eilet", "tokens": ["Bei", "Brot", "und", "Was\u00b7ser", "ei\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht weg die S\u00fcndenlust,", "tokens": ["Nicht", "weg", "die", "S\u00fcn\u00b7den\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und keine Bibel heilet", "tokens": ["Und", "kei\u00b7ne", "Bi\u00b7bel", "hei\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die frevelvolle Brust.", "tokens": ["Die", "fre\u00b7vel\u00b7vol\u00b7le", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wollt ihr Genesung bringen", "tokens": ["Wollt", "ihr", "Ge\u00b7ne\u00b7sung", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Der armen kranken Zeit,", "tokens": ["Der", "ar\u00b7men", "kran\u00b7ken", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lernt selbst vor allen Dingen", "tokens": ["Lernt", "selbst", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Recht und Gerechtigkeit.", "tokens": ["Recht", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}