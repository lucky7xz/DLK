{"dta.poem.8227": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Damendienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will deinem Wink mich weihn,", "tokens": ["Ich", "will", "dei\u00b7nem", "Wink", "mich", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An Festen und hohen Tagen", "tokens": ["An", "Fes\u00b7ten", "und", "ho\u00b7hen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sollst du meine K\u00f6nigin sein!", "tokens": ["Sollst", "du", "mei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Deiner Launen geheimste und k\u00fchnste", "tokens": ["Dei\u00b7ner", "Lau\u00b7nen", "ge\u00b7heims\u00b7te", "und", "k\u00fchns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "KON", "ADJA"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Gehorsam erf\u00fcll' ich dir;", "tokens": ["Ge\u00b7hor\u00b7sam", "er\u00b7f\u00fcll'", "ich", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch leid ich in diesem Dienste", "tokens": ["Doch", "leid", "ich", "in", "die\u00b7sem", "Diens\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Keinen Andern neben mir.", "tokens": ["Kei\u00b7nen", "An\u00b7dern", "ne\u00b7ben", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So lang ich dir diene in Ehren,", "tokens": ["So", "lang", "ich", "dir", "die\u00b7ne", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Geh\u00f6ret dein L\u00e4cheln mein;", "tokens": ["Ge\u00b7h\u00f6\u00b7ret", "dein", "L\u00e4\u00b7cheln", "mein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Deinen Hofstaat will ich vermehren;", "tokens": ["Dei\u00b7nen", "Hof\u00b7staat", "will", "ich", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch der Erste will ich sein.", "tokens": ["Doch", "der", "Ers\u00b7te", "will", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}