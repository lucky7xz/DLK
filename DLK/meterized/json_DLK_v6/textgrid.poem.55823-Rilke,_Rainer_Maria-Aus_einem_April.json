{"textgrid.poem.55823": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Aus einem April", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder duftet der Wald.", "tokens": ["Wie\u00b7der", "duf\u00b7tet", "der", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es heben die schwebenden Lerchen", "tokens": ["Es", "he\u00b7ben", "die", "schwe\u00b7ben\u00b7den", "Ler\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "mit sich den Himmel empor, der unseren Schultern schwer war;", "tokens": ["mit", "sich", "den", "Him\u00b7mel", "em\u00b7por", ",", "der", "un\u00b7se\u00b7ren", "Schul\u00b7tern", "schwer", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "zwar sah man noch durch die \u00c4ste den Tag, wie er leer war, \u2013", "tokens": ["zwar", "sah", "man", "noch", "durch", "die", "\u00c4s\u00b7te", "den", "Tag", ",", "wie", "er", "leer", "war", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "$("], "meter": "-+-+--+--+--++", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "aber nach langen, regnenden Nachmittagen", "tokens": ["a\u00b7ber", "nach", "lan\u00b7gen", ",", "reg\u00b7nen\u00b7den", "Nach\u00b7mit\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "kommen die gold\u00fcbersonnten", "tokens": ["kom\u00b7men", "die", "gold\u00b7\u00fc\u00b7ber\u00b7sonn\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "neueren Stunden,", "tokens": ["neu\u00b7e\u00b7ren", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "vor denen fl\u00fcchtend an fernen H\u00e4userfronten", "tokens": ["vor", "de\u00b7nen", "fl\u00fcch\u00b7tend", "an", "fer\u00b7nen", "H\u00e4u\u00b7ser\u00b7fron\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "alle die wunden", "tokens": ["al\u00b7le", "die", "wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Fenster furchtsam mit Fl\u00fcgeln schlagen.", "tokens": ["Fens\u00b7ter", "furcht\u00b7sam", "mit", "Fl\u00fc\u00b7geln", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann wird es still. Sogar der Regen geht leiser", "tokens": ["Dann", "wird", "es", "still", ".", "So\u00b7gar", "der", "Re\u00b7gen", "geht", "lei\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$.", "ADV", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00fcber der Steine ruhig dunkelnden Glanz.", "tokens": ["\u00fc\u00b7ber", "der", "Stei\u00b7ne", "ru\u00b7hig", "dun\u00b7keln\u00b7den", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Alle Ger\u00e4usche ducken sich ganz", "tokens": ["Al\u00b7le", "Ge\u00b7r\u00e4u\u00b7sche", "du\u00b7cken", "sich", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "in die gl\u00e4nzenden Knospen der Reiser.", "tokens": ["in", "die", "gl\u00e4n\u00b7zen\u00b7den", "Knos\u00b7pen", "der", "Rei\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}