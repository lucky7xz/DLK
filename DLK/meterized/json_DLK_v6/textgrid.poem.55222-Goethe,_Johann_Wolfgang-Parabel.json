{"textgrid.poem.55222": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Parabel", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Stadt, wo Parit\u00e4t", "tokens": ["In", "ei\u00b7ner", "Stadt", ",", "wo", "Pa\u00b7ri\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch in der alten Ordnung steht,", "tokens": ["Noch", "in", "der", "al\u00b7ten", "Ord\u00b7nung", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da, wo sich n\u00e4mlich Katholiken", "tokens": ["Da", ",", "wo", "sich", "n\u00e4m\u00b7lich", "Ka\u00b7tho\u00b7li\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PRF", "ADV", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Protestanten ineinander schicken", "tokens": ["Und", "Pro\u00b7tes\u00b7tan\u00b7ten", "in\u00b7ein\u00b7an\u00b7der", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und, wie's von V\u00e4tern war erprobt,", "tokens": ["Und", ",", "wie's", "von", "V\u00e4\u00b7tern", "war", "er\u00b7probt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jeder Gott auf seine Weise lobt,", "tokens": ["Je\u00b7der", "Gott", "auf", "sei\u00b7ne", "Wei\u00b7se", "lobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Da lebten wir Kinder Lutheraner", "tokens": ["Da", "leb\u00b7ten", "wir", "Kin\u00b7der", "Lu\u00b7the\u00b7ra\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Von etwas Predigt und Gesang,", "tokens": ["Von", "et\u00b7was", "Pre\u00b7digt", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Waren aber dem Kling und Klang", "tokens": ["Wa\u00b7ren", "a\u00b7ber", "dem", "Kling", "und", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Der Katholiken nur zugetaner:", "tokens": ["Der", "Ka\u00b7tho\u00b7li\u00b7ken", "nur", "zu\u00b7ge\u00b7ta\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Denn alles war doch gar zu sch\u00f6n,", "tokens": ["Denn", "al\u00b7les", "war", "doch", "gar", "zu", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bunter und lustiger anzusehn.", "tokens": ["Bun\u00b7ter", "und", "lus\u00b7ti\u00b7ger", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVIZU", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dieweil nun Affe, Mensch und Kind", "tokens": ["Die\u00b7weil", "nun", "Af\u00b7fe", ",", "Mensch", "und", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Nachahmung geboren sind,", "tokens": ["Zur", "Nac\u00b7hah\u00b7mung", "ge\u00b7bo\u00b7ren", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erfanden wir, die Zeit zu k\u00fcrzen,", "tokens": ["Er\u00b7fan\u00b7den", "wir", ",", "die", "Zeit", "zu", "k\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein auserlesnes Pfaffenspiel:", "tokens": ["Ein", "au\u00b7ser\u00b7les\u00b7nes", "Pfaf\u00b7fen\u00b7spiel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Chorrock, der uns wohlgefiel,", "tokens": ["Zum", "Chor\u00b7rock", ",", "der", "uns", "wohl\u00b7ge\u00b7fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gaben die Schwestern ihre Sch\u00fcrzen;", "tokens": ["Ga\u00b7ben", "die", "Schwes\u00b7tern", "ih\u00b7re", "Sch\u00fcr\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Handt\u00fccher, mit Wirkwerk sch\u00f6n verziert,", "tokens": ["Hand\u00b7t\u00fc\u00b7cher", ",", "mit", "Wirk\u00b7werk", "sch\u00f6n", "ver\u00b7ziert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wurden zur Stola travestiert;", "tokens": ["Wur\u00b7den", "zur", "Sto\u00b7la", "tra\u00b7ves\u00b7tiert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Die M\u00fctze mu\u00dfte den Bischof zieren", "tokens": ["Die", "M\u00fct\u00b7ze", "mu\u00df\u00b7te", "den", "Bi\u00b7schof", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Von Goldpapier mit vielen Tieren.", "tokens": ["Von", "Gold\u00b7pa\u00b7pier", "mit", "vie\u00b7len", "Tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So zogen wir nun im Ornat", "tokens": ["So", "zo\u00b7gen", "wir", "nun", "im", "Or\u00b7nat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch Haus und Garten fr\u00fch und spat", "tokens": ["Durch", "Haus", "und", "Gar\u00b7ten", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wiederholten ohne Schonen", "tokens": ["Und", "wie\u00b7der\u00b7hol\u00b7ten", "oh\u00b7ne", "Scho\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die s\u00e4mtlichen heiligen Funktionen;", "tokens": ["Die", "s\u00e4mt\u00b7li\u00b7chen", "hei\u00b7li\u00b7gen", "Funk\u00b7ti\u00b7o\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch fehlte noch das beste St\u00fcck.", "tokens": ["Doch", "fehl\u00b7te", "noch", "das", "bes\u00b7te", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir wu\u00dften wohl, ein pr\u00e4chtig L\u00e4uten", "tokens": ["Wir", "wu\u00df\u00b7ten", "wohl", ",", "ein", "pr\u00e4ch\u00b7tig", "L\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Habe hier am meisten zu bedeuten;", "tokens": ["Ha\u00b7be", "hier", "am", "meis\u00b7ten", "zu", "be\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und nun beg\u00fcnstigt' uns das Gl\u00fcck:", "tokens": ["Und", "nun", "be\u00b7g\u00fcns\u00b7tigt'", "uns", "das", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn auf dem Boden hing ein Strick.", "tokens": ["Denn", "auf", "dem", "Bo\u00b7den", "hing", "ein", "Strick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir sind entz\u00fcckt, und wie wir diesen", "tokens": ["Wir", "sind", "ent\u00b7z\u00fcckt", ",", "und", "wie", "wir", "die\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "PWAV", "PPER", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zum Glockenstrang sogleich erkiesen,", "tokens": ["Zum", "Glo\u00b7cken\u00b7strang", "sog\u00b7leich", "er\u00b7kie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ruht er nicht einen Augenblick:", "tokens": ["Ruht", "er", "nicht", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Denn wechselnd eilten wir Geschwister,", "tokens": ["Denn", "wech\u00b7selnd", "eil\u00b7ten", "wir", "Ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Einer ward um den andern K\u00fcster,", "tokens": ["Ei\u00b7ner", "ward", "um", "den", "an\u00b7dern", "K\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Ein jedes dr\u00e4ngte sich hinzu.", "tokens": ["Ein", "je\u00b7des", "dr\u00e4ng\u00b7te", "sich", "hin\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Das ging nun allerliebst vonstatten,", "tokens": ["Das", "ging", "nun", "al\u00b7ler\u00b7liebst", "von\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und weil wir keine Glocken hatten,", "tokens": ["Und", "weil", "wir", "kei\u00b7ne", "Glo\u00b7cken", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "So sangen wir ", "tokens": ["So", "san\u00b7gen", "wir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Vergessen wie die \u00e4ltste Sage", "tokens": ["Ver\u00b7ges\u00b7sen", "wie", "die", "\u00e4lts\u00b7te", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War der unschuld'ge Kinderscherz;", "tokens": ["War", "der", "un\u00b7schuld'\u00b7ge", "Kin\u00b7der\u00b7scherz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Doch grade diese letzten Tage", "tokens": ["Doch", "gra\u00b7de", "die\u00b7se", "letz\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fiel er mit einmal mir aufs Herz:", "tokens": ["Fiel", "er", "mit", "ein\u00b7mal", "mir", "aufs", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sind sie ja, nach allen St\u00fccken,", "tokens": ["Da", "sind", "sie", "ja", ",", "nach", "al\u00b7len", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die neupoetischen Katholiken!", "tokens": ["Die", "neu\u00b7po\u00b7e\u00b7ti\u00b7schen", "Ka\u00b7tho\u00b7li\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}