{"dta.poem.565": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Schaut sch\u00f6ne Rarit\u00e4t! schaut sch\u00f6ne Spiele-Werk!", "tokens": ["Schaut", "sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "!", "schaut", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7Werk", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$.", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr Leute kommt herbey/ hier giebt es was zu sehen:", "tokens": ["Ihr", "Leu\u00b7te", "kommt", "her\u00b7bey", "/", "hier", "giebt", "es", "was", "zu", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Rarit\u00e4t ist wehrt/ da\u00df jederman drauf merck/", "tokens": ["Die", "Ra\u00b7ri\u00b7t\u00e4t", "ist", "wehrt", "/", "da\u00df", "je\u00b7der\u00b7man", "drauf", "merck", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$(", "KOUS", "PIS", "PAV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und beydes Spei\u00df und Tranck la\u00df\u2019 auf dem Tische stehen.", "tokens": ["Und", "bey\u00b7des", "Spei\u00df", "und", "Tranck", "la\u00df'", "auf", "dem", "Ti\u00b7sche", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Herr Br\u00e4utigam kommt her und schaut", "tokens": ["Herr", "Br\u00e4u\u00b7ti\u00b7gam", "kommt", "her", "und", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit eurer lieben Jungfer Braut.", "tokens": ["Mit", "eu\u00b7rer", "lie\u00b7ben", "Jung\u00b7fer", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiel-Wercke/ ", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spiel\u00b7\u00b7Wer\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "ne ", "tokens": ["ne"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "ne Spiele-Wercke.", "tokens": ["ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7cke", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}