{"dta.poem.18420": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "General Ginckel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe land und reich der Jrren eingenommen/", "tokens": ["Ich", "ha\u00b7be", "land", "und", "reich", "der", "Jr\u00b7ren", "ein\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihre starcke macht den stoppeln gleich zerstreut/", "tokens": ["Und", "ih\u00b7re", "star\u00b7cke", "macht", "den", "stop\u00b7peln", "gleich", "zer\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein K\u00f6nig hat durch mich ein K\u00f6nigreich bekommen;", "tokens": ["Mein", "K\u00f6\u00b7nig", "hat", "durch", "mich", "ein", "K\u00f6\u00b7nig\u00b7reich", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer fordert gr\u00f6\u00dfre treu und gr\u00f6\u00dfre tapfferkeit?", "tokens": ["Wer", "for\u00b7dert", "gr\u00f6\u00df\u00b7re", "treu", "und", "gr\u00f6\u00df\u00b7re", "tapf\u00b7fer\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}