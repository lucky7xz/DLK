{"textgrid.poem.30762": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccke l\u00e4st sich nicht beherrschen von dem Alter oder Zeit.", "tokens": ["Gl\u00fc\u00b7cke", "l\u00e4st", "sich", "nicht", "be\u00b7herr\u00b7schen", "von", "dem", "Al\u00b7ter", "o\u00b7der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Manchem bringt es schone Fr\u00fcchte, wann er noch auf Stecken reit;", "tokens": ["Man\u00b7chem", "bringt", "es", "scho\u00b7ne", "Fr\u00fcch\u00b7te", ",", "wann", "er", "noch", "auf", "Ste\u00b7cken", "reit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Manchem hebt es an zu bl\u00fchen, wann er schon an Kr\u00fccken schleicht;", "tokens": ["Man\u00b7chem", "hebt", "es", "an", "zu", "bl\u00fc\u00b7hen", ",", "wann", "er", "schon", "an", "Kr\u00fc\u00b7cken", "schleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Manchem ist es immer kummen; manchen hat es nie erreicht.", "tokens": ["Man\u00b7chem", "ist", "es", "im\u00b7mer", "kum\u00b7men", ";", "man\u00b7chen", "hat", "es", "nie", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVINF", "$.", "PIS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wer nur so viel an sich findet, da\u00df er weiter nichts begehrt,", "tokens": ["Wer", "nur", "so", "viel", "an", "sich", "fin\u00b7det", ",", "da\u00df", "er", "wei\u00b7ter", "nichts", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "APPR", "PRF", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Als von oben ihm geordnet, den hat Gl\u00fccke nie gef\u00e4hrt.", "tokens": ["Als", "von", "o\u00b7ben", "ihm", "ge\u00b7ord\u00b7net", ",", "den", "hat", "Gl\u00fc\u00b7cke", "nie", "ge\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "PPER", "VVPP", "$,", "ART", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}