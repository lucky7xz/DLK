{"textgrid.poem.44647": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Xenien", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlich sei mir gegr\u00fc\u00dft, polarischer Feuerl\u00e4nder,", "tokens": ["Freund\u00b7lich", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "po\u00b7la\u00b7ri\u00b7scher", "Feu\u00b7er\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Immer reizend und neu, singend dein alt Pescher\u00e4h!", "tokens": ["Im\u00b7mer", "rei\u00b7zend", "und", "neu", ",", "sin\u00b7gend", "dein", "alt", "Pe\u00b7sche\u00b7r\u00e4h", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADJD", "$,", "ADJD", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}