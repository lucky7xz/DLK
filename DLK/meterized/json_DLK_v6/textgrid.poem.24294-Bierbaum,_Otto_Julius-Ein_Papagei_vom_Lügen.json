{"textgrid.poem.24294": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ein Papagei vom L\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Christ, der Muselman, der Jude und der Heide:", "tokens": ["Der", "Christ", ",", "der", "Mu\u00b7sel\u00b7man", ",", "der", "Ju\u00b7de", "und", "der", "Hei\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie beugen sich dem Geist der L\u00fcge, wie die Weide", "tokens": ["Sie", "beu\u00b7gen", "sich", "dem", "Geist", "der", "L\u00fc\u00b7ge", ",", "wie", "die", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Himmelswind sich beugt; es l\u00fcgt das Kind, der Mann,", "tokens": ["Dem", "Him\u00b7mels\u00b7wind", "sich", "beugt", ";", "es", "l\u00fcgt", "das", "Kind", ",", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der B\u00e4nker, Bettler l\u00fcgt; der H\u00f6fling, der Tyrann;", "tokens": ["Der", "B\u00e4n\u00b7ker", ",", "Bett\u00b7ler", "l\u00fcgt", ";", "der", "H\u00f6f\u00b7ling", ",", "der", "Ty\u00b7rann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVFIN", "$.", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Held; der Journalist; der Forscher; der Soldat;", "tokens": ["Der", "Held", ";", "der", "Jour\u00b7na\u00b7list", ";", "der", "For\u00b7scher", ";", "der", "Sol\u00b7dat", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es l\u00fcgt das Parlament, sowie der Potentat;", "tokens": ["Es", "l\u00fcgt", "das", "Par\u00b7la\u00b7ment", ",", "so\u00b7wie", "der", "Po\u00b7ten\u00b7tat", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auf seiner Kanzel l\u00fcgt (und wie!) der Theolog;", "tokens": ["Auf", "sei\u00b7ner", "Kan\u00b7zel", "l\u00fcgt", "(", "und", "wie", "!", ")", "der", "Theo\u00b7log", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PWAV", "$.", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Der Sauhirt l\u00fcgt daf\u00fcr an seinem Schweinetrog;", "tokens": ["Der", "Sau\u00b7hirt", "l\u00fcgt", "da\u00b7f\u00fcr", "an", "sei\u00b7nem", "Schwei\u00b7ne\u00b7trog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und gar das Weib: o Gott! \u2013: es l\u00fcgt selbst, wenn es lacht", "tokens": ["Und", "gar", "das", "Weib", ":", "o", "Gott", "!", "\u2013", ":", "es", "l\u00fcgt", "selbst", ",", "wenn", "es", "lacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$.", "FM", "NN", "$.", "$(", "$.", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "(und wenn es weint, erst recht); wenn sichs die Haare macht;", "tokens": ["(", "und", "wenn", "es", "weint", ",", "erst", "recht", ")", ";", "wenn", "sichs", "die", "Haa\u00b7re", "macht", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$(", "$.", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenns kocht, wenns n\u00e4ht, wenns spinnt; wenns ha\u00dft, wenns liebt; wenns ruht;", "tokens": ["Wenns", "kocht", ",", "wenns", "n\u00e4ht", ",", "wenns", "spinnt", ";", "wenns", "ha\u00dft", ",", "wenns", "liebt", ";", "wenns", "ruht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "VVFIN", "$,", "KOUS", "VVFIN", "$.", "KOUS", "VVFIN", "$,", "KOUS", "VVFIN", "$.", "KOUS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es l\u00fcgt, wenns schweigt, und wenns sich schwatzend g\u00fctlich tut;", "tokens": ["Es", "l\u00fcgt", ",", "wenns", "schweigt", ",", "und", "wenns", "sich", "schwat\u00b7zend", "g\u00fct\u00b7lich", "tut", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "$,", "KON", "KOUS", "PRF", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und kurz und krumm: Ihr Volk, zweibeinig ungefiedert,", "tokens": ["Und", "kurz", "und", "krumm", ":", "Ihr", "Volk", ",", "zwei\u00b7bei\u00b7nig", "un\u00b7ge\u00b7fie\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$.", "PPOSAT", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Seid all dem L\u00fcgengott vervettert und verbiedert.", "tokens": ["Seid", "all", "dem", "L\u00fc\u00b7gen\u00b7gott", "ver\u00b7vet\u00b7tert", "und", "ver\u00b7bie\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PIAT", "ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur Dichter reden wahr, \u2013 denn allen ihren L\u00fcgen", "tokens": ["Nur", "Dich\u00b7ter", "re\u00b7den", "wahr", ",", "\u2013", "denn", "al\u00b7len", "ih\u00b7ren", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PTKVZ", "$,", "$(", "KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorsenden sie das Wort: Glaubt ja nichts: wir betr\u00fcgen.", "tokens": ["Vor\u00b7sen\u00b7den", "sie", "das", "Wort", ":", "Glaubt", "ja", "nichts", ":", "wir", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$.", "VVFIN", "ADV", "PIS", "$.", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Auf diese Art allein bringt man euch \u2013 Wahrheit bei.", "tokens": ["Auf", "die\u00b7se", "Art", "al\u00b7lein", "bringt", "man", "euch", "\u2013", "Wahr\u00b7heit", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "PIS", "PPER", "$(", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Gottlob, da\u00df ich kein Mensch, nein, blo\u00df ein Papagei ...", "tokens": ["Gott\u00b7lob", ",", "da\u00df", "ich", "kein", "Mensch", ",", "nein", ",", "blo\u00df", "ein", "Pa\u00b7pa\u00b7gei", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PIAT", "NN", "$,", "PTKANT", "$,", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}