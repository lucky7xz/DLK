{"textgrid.poem.52057": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man dich sieht als wie ein Ungeheuer kommen,", "tokens": ["Wenn", "man", "dich", "sieht", "als", "wie", "ein", "Un\u00b7ge\u00b7heu\u00b7er", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor dem die Th\u00fcren, B\u00e4nck und Tische gantz verstummen,", "tokens": ["Vor", "dem", "die", "Th\u00fc\u00b7ren", ",", "B\u00e4nck", "und", "Ti\u00b7sche", "gantz", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$,", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da flucht der Koch, ja selbst die Br\u00e4ter schrumpffen ein,", "tokens": ["Da", "flucht", "der", "Koch", ",", "ja", "selbst", "die", "Br\u00e4\u00b7ter", "schrumpf\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn du solst seine Furcht und ihr Erschreckn\u00fc\u00df seyn.", "tokens": ["Denn", "du", "solst", "sei\u00b7ne", "Furcht", "und", "ihr", "Er\u00b7schreck\u00b7n\u00fc\u00df", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und nicht umbsonst; denn so du frist, so kan dein Magen", "tokens": ["Und", "nicht", "um\u00b7bsonst", ";", "denn", "so", "du", "frist", ",", "so", "kan", "dein", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "$.", "KON", "ADV", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mehr Fleisch, als Pohlen vor auff Schweidnitz trieb, ertragen,", "tokens": ["Mehr", "Fleisch", ",", "als", "Poh\u00b7len", "vor", "auff", "Schweid\u00b7nitz", "trieb", ",", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "NN", "APPR", "APPR", "NE", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn du sa\u00fcffst, so rinnt durch deiner Gurgel Schlauch", "tokens": ["Und", "wenn", "du", "sa\u00fcffst", ",", "so", "rinnt", "durch", "dei\u00b7ner", "Gur\u00b7gel", "Schlauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Fa\u00df von Heidelberg in den so tieffen Bauch.", "tokens": ["Das", "Fa\u00df", "von", "Hei\u00b7del\u00b7berg", "in", "den", "so", "tief\u00b7fen", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als weit du reichst, wo du die Taffel kanst bebr\u00fctten,", "tokens": ["Als", "weit", "du", "reichst", ",", "wo", "du", "die", "Taf\u00b7fel", "kanst", "be\u00b7br\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sieht es w\u00fcster aus, als in den Bauerh\u00fctten:", "tokens": ["Da", "sieht", "es", "w\u00fcs\u00b7ter", "aus", ",", "als", "in", "den", "Bau\u00b7er\u00b7h\u00fct\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die bey uns Wirth und Vieh und K\u00f6rner ledig seyn,", "tokens": ["Die", "bey", "uns", "Wirth", "und", "Vieh", "und", "K\u00f6r\u00b7ner", "le\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "KON", "NN", "KON", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was nicht der Schlund, das ziehn drauff beyde Diebs\u00e4ck ein.", "tokens": ["Was", "nicht", "der", "Schlund", ",", "das", "ziehn", "drauff", "bey\u00b7de", "Die\u00b7bs\u00e4ck", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "$,", "PDS", "VVFIN", "PAV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wann du als eine Bach im br\u00fcchiglosen Wasen,", "tokens": ["Wann", "du", "als", "ei\u00b7ne", "Bach", "im", "br\u00fc\u00b7chig\u00b7lo\u00b7sen", "Wa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den die Vollbr\u00e4tigkeit vergruntzt und auffgeblasen:", "tokens": ["Den", "die", "Voll\u00b7br\u00e4\u00b7tig\u00b7keit", "ver\u00b7gruntzt", "und", "auff\u00b7ge\u00b7bla\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "So st\u00fcrtzt dein satter Wanst den Vorrath wieder aus,", "tokens": ["So", "st\u00fcrtzt", "dein", "sat\u00b7ter", "Wanst", "den", "Vor\u00b7rath", "wie\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit dem dein Wirth versieht die Mast Schwein unterm Hau\u00df.", "tokens": ["Mit", "dem", "dein", "Wirth", "ver\u00b7sieht", "die", "Mast", "Schwein", "un\u00b7term", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es hat sich sehr genarrt, der dich zum Hunger leiden", "tokens": ["Es", "hat", "sich", "sehr", "ge\u00b7narrt", ",", "der", "dich", "zum", "Hun\u00b7ger", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In unsre Pest gelegt, du bist zu unbescheiden:", "tokens": ["In", "uns\u00b7re", "Pest", "ge\u00b7legt", ",", "du", "bist", "zu", "un\u00b7be\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob die Besatzung gleich mit uns umschlo\u00dfen ist,", "tokens": ["Ob", "die", "Be\u00b7sat\u00b7zung", "gleich", "mit", "uns", "um\u00b7schlo\u00b7\u00dfen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sperrst du dich wie ein Wolff, wo du es rauchen siehst.", "tokens": ["Sperrst", "du", "dich", "wie", "ein", "Wolff", ",", "wo", "du", "es", "rau\u00b7chen", "siehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KOKOM", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Als du dich in den Tisch, da, wo ich war, gefunden,", "tokens": ["Als", "du", "dich", "in", "den", "Tisch", ",", "da", ",", "wo", "ich", "war", ",", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "$,", "KOUS", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Kr\u00fcg und Sch\u00fc\u00dfeln leer, als weit ich sahe, stunden,", "tokens": ["Und", "Kr\u00fcg", "und", "Sch\u00fc\u00b7\u00dfeln", "leer", ",", "als", "weit", "ich", "sa\u00b7he", ",", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein lieber Polyphem! Da nahm ich meinen Bauch", "tokens": ["Mein", "lie\u00b7ber", "Po\u00b7ly\u00b7phem", "!", "Da", "nahm", "ich", "mei\u00b7nen", "Bauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gieng davon und dacht: Er frist dich endlich auch.", "tokens": ["Und", "gieng", "da\u00b7von", "und", "dacht", ":", "Er", "frist", "dich", "end\u00b7lich", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "KON", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}