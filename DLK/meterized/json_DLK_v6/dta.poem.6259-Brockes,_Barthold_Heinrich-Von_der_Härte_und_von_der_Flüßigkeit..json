{"dta.poem.6259": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sind die C\u00f6rper auch von gr\u00f6ssrer H\u00e4rtigkeit,", "tokens": ["Es", "sind", "die", "C\u00f6r\u00b7per", "auch", "von", "gr\u00f6ss\u00b7rer", "H\u00e4r\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und festerer Beschaffenheit,", "tokens": ["Und", "fes\u00b7te\u00b7rer", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie gef\u00fcget sind, aus Theilchen, welche fest,", "tokens": ["Wenn", "sie", "ge\u00b7f\u00fc\u00b7get", "sind", ",", "aus", "Theil\u00b7chen", ",", "wel\u00b7che", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "APPR", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sich am besten in sich sencken,", "tokens": ["Die", "sich", "am", "bes\u00b7ten", "in", "sich", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKA", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und mit einander sich verschr\u00e4ncken,", "tokens": ["Und", "mit", "ein\u00b7an\u00b7der", "sich", "ver\u00b7schr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df keines zwischen sich ein leeres Pl\u00e4tzgen l\u00e4st.", "tokens": ["Da\u00df", "kei\u00b7nes", "zwi\u00b7schen", "sich", "ein", "lee\u00b7res", "Pl\u00e4tz\u00b7gen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die, deren C\u00f6rperchen, so klein,", "tokens": ["Die", ",", "de\u00b7ren", "C\u00f6r\u00b7per\u00b7chen", ",", "so", "klein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PRELAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Und sich am mindsten \u00e4hnlich seyn,", "tokens": ["Und", "sich", "am", "minds\u00b7ten", "\u00e4hn\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "ADJA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So, da\u00df auf manche Art die mancherley Gestalten", "tokens": ["So", ",", "da\u00df", "auf", "man\u00b7che", "Art", "die", "man\u00b7cher\u00b7ley", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "PIAT", "NN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Geschickt sind, sich zu binden, sich zu f\u00fcgen,", "tokens": ["Ge\u00b7schickt", "sind", ",", "sich", "zu", "bin\u00b7den", ",", "sich", "zu", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRF", "PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und fest zusammen sich zu halten;", "tokens": ["Und", "fest", "zu\u00b7sam\u00b7men", "sich", "zu", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sind mit der gr\u00f6sten M\u00fch\u2019 zu brechen, zu zerspalten.", "tokens": ["Sind", "mit", "der", "gr\u00f6s\u00b7ten", "M\u00fch'", "zu", "bre\u00b7chen", ",", "zu", "zer\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die, deren kleine Theil\u2019hingegen,", "tokens": ["Die", ",", "de\u00b7ren", "klei\u00b7ne", "Theil'\u00b7hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Indem sie steiffer sind und gl\u00e4tter,", "tokens": ["In\u00b7dem", "sie", "steif\u00b7fer", "sind", "und", "gl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So fest sich nicht zusammen legen;", "tokens": ["So", "fest", "sich", "nicht", "zu\u00b7sam\u00b7men", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Dieselben werden insgemein", "tokens": ["Die\u00b7sel\u00b7ben", "wer\u00b7den", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Zwar hart, doch auch zerbrechlich seyn.", "tokens": ["Zwar", "hart", ",", "doch", "auch", "zer\u00b7brech\u00b7lich", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}