{"dta.poem.4138": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "September.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hirschf\u00e4nger, B\u00fcchse, Netz und gr\u00fcnes Kleid,", "tokens": ["Hirschf\u00e4n\u00b7ger", ",", "B\u00fcch\u00b7se", ",", "Netz", "und", "gr\u00fc\u00b7nes", "Kleid", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ein Ro\u00df, zu jedem kecken Sprung bereit,", "tokens": ["Ein", "Ro\u00df", ",", "zu", "je\u00b7dem", "ke\u00b7cken", "Sprung", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PIAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verehr' ich dir, und w\u00fcnsche Gl\u00fcck zum Fange.", "tokens": ["Ver\u00b7ehr'", "ich", "dir", ",", "und", "w\u00fcn\u00b7sche", "Gl\u00fcck", "zum", "Fan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "KON", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Frisch auf! Um das Revier sei mir nicht bange:", "tokens": ["Frisch", "auf", "!", "Um", "das", "Re\u00b7vier", "sei", "mir", "nicht", "ban\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "KOUI", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich habe Eichenw\u00e4lder tief und breit,", "tokens": ["Ich", "ha\u00b7be", "Ei\u00b7chen\u00b7w\u00e4l\u00b7der", "tief", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Bahnen rings durchhauen f\u00fcr die Waid,", "tokens": ["Mit", "Bah\u00b7nen", "rings", "durch\u00b7hau\u00b7en", "f\u00fcr", "die", "Waid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Hirsch' und Rehe, wie ich sie verlange.", "tokens": ["Und", "Hirsch'", "und", "Re\u00b7he", ",", "wie", "ich", "sie", "ver\u00b7lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Hut geschm\u00fcckt mit einem gr\u00fcnen Reise,", "tokens": ["Den", "Hut", "ge\u00b7schm\u00fcckt", "mit", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die H\u00e4nde purpurroth von edlem Schwei\u00dfe,", "tokens": ["Die", "H\u00e4n\u00b7de", "pur\u00b7pur\u00b7roth", "von", "ed\u00b7lem", "Schwei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wagen krachend unter ihrer Last:", "tokens": ["Die", "Wa\u00b7gen", "kra\u00b7chend", "un\u00b7ter", "ih\u00b7rer", "Last", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So ziehe heim mit deinen Jagdgesellen,", "tokens": ["So", "zie\u00b7he", "heim", "mit", "dei\u00b7nen", "Jagd\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du nicht erst ein Wort noch zu bestellen", "tokens": ["Wenn", "du", "nicht", "erst", "ein", "Wort", "noch", "zu", "be\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier bei der sch\u00f6nen F\u00f6rsterstochter hast.", "tokens": ["Hier", "bei", "der", "sch\u00f6\u00b7nen", "F\u00f6rs\u00b7ter\u00b7stoch\u00b7ter", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}