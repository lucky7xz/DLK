{"textgrid.poem.48181": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonntagsruhe, Dorfesstille,", "tokens": ["Sonn\u00b7tags\u00b7ru\u00b7he", ",", "Dor\u00b7fes\u00b7stil\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind und Knecht und Magd sind aus,", "tokens": ["Kind", "und", "Knecht", "und", "Magd", "sind", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterm Herde nur die Grille", "tokens": ["Un\u00b7term", "Her\u00b7de", "nur", "die", "Gril\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Musizieret durch das Haus.", "tokens": ["Mu\u00b7si\u00b7zie\u00b7ret", "durch", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00fcr und Fenster blieben offen,", "tokens": ["T\u00fcr", "und", "Fens\u00b7ter", "blie\u00b7ben", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es schweigen Luft und Wind,", "tokens": ["Denn", "es", "schwei\u00b7gen", "Luft", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In uns schweigen Wunsch und Hoffen,", "tokens": ["In", "uns", "schwei\u00b7gen", "Wunsch", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil wir ganz im Gl\u00fccke sind.", "tokens": ["Weil", "wir", "ganz", "im", "Gl\u00fc\u00b7cke", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Felder rings \u2013 ein Gottessegen", "tokens": ["Fel\u00b7der", "rings", "\u2013", "ein", "Got\u00b7tes\u00b7se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcgel auf- und niederw\u00e4rts,", "tokens": ["H\u00fc\u00b7gel", "auf", "und", "nie\u00b7der\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf stillen Gnadenwegen", "tokens": ["Und", "auf", "stil\u00b7len", "Gna\u00b7den\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stieg auch uns er in das Herz.", "tokens": ["Stieg", "auch", "uns", "er", "in", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}