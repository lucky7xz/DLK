{"textgrid.poem.44967": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: An Schwindelei sind alle krank", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Schwindelei sind alle krank", "tokens": ["An", "Schwin\u00b7de\u00b7lei", "sind", "al\u00b7le", "krank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollen mich an Gaukler mahnen,", "tokens": ["Und", "wol\u00b7len", "mich", "an", "Gauk\u00b7ler", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur gaukelt einer von der Bank,", "tokens": ["Nur", "gau\u00b7kelt", "ei\u00b7ner", "von", "der", "Bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der andre treibts auf ebnen Bahnen.", "tokens": ["Der", "and\u00b7re", "treibts", "auf", "eb\u00b7nen", "Bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}