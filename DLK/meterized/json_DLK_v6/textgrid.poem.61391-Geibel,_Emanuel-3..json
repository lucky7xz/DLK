{"textgrid.poem.61391": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Runenstein in der Sommernacht,", "tokens": ["Am", "Ru\u00b7nen\u00b7stein", "in", "der", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da spielen die Wasserfrauen;", "tokens": ["Da", "spie\u00b7len", "die", "Was\u00b7ser\u00b7frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Wasser klingt, es singt die Luft,", "tokens": ["Das", "Was\u00b7ser", "klingt", ",", "es", "singt", "die", "Luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond steht hoch im Blauen.", "tokens": ["Der", "Mond", "steht", "hoch", "im", "Blau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das pl\u00e4tschert und lacht, das wogt und taucht", "tokens": ["Das", "pl\u00e4t\u00b7schert", "und", "lacht", ",", "das", "wogt", "und", "taucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "PDS", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Lilien auf und nieder,", "tokens": ["Wie", "Li\u00b7li\u00b7en", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es schwimmt auf der Flut das goldne Haar,", "tokens": ["Es", "schwimmt", "auf", "der", "Flut", "das", "gold\u00b7ne", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es schimmern die wei\u00dfen Glieder.", "tokens": ["Es", "schim\u00b7mern", "die", "wei\u00b7\u00dfen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit schilfigem Bart der Meermann bl\u00e4st", "tokens": ["Mit", "schil\u00b7fi\u00b7gem", "Bart", "der", "Meer\u00b7mann", "bl\u00e4st"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die gewundene Muschelposaune,", "tokens": ["Die", "ge\u00b7wun\u00b7de\u00b7ne", "Mu\u00b7schel\u00b7po\u00b7sau\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Nixen schlingen den Reigen dazu,", "tokens": ["Die", "Ni\u00b7xen", "schlin\u00b7gen", "den", "Rei\u00b7gen", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie sind in der besten Laune.", "tokens": ["Sie", "sind", "in", "der", "bes\u00b7ten", "Lau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da schreit die j\u00fcngste und kichert drauf:", "tokens": ["Da", "schreit", "die", "j\u00fcngs\u00b7te", "und", "ki\u00b7chert", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbei seht, was ich fand in der Welle!", "tokens": ["\u00bb", "ei", "seht", ",", "was", "ich", "fand", "in", "der", "Wel\u00b7le", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein blinkendes, winkendes Totengebein,", "tokens": ["Ein", "blin\u00b7ken\u00b7des", ",", "win\u00b7ken\u00b7des", "To\u00b7ten\u00b7ge\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie Silber gl\u00e4nzt es so helle.", "tokens": ["Wie", "Sil\u00b7ber", "gl\u00e4nzt", "es", "so", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich stie\u00df mit dem Fu\u00df ans Korallenriff", "tokens": ["Ich", "stie\u00df", "mit", "dem", "Fu\u00df", "ans", "Ko\u00b7ral\u00b7len\u00b7riff"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Beim lustigen Untertauchen,", "tokens": ["Beim", "lus\u00b7ti\u00b7gen", "Un\u00b7ter\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da lag's in den \u00c4sten, ich zog es hervor;", "tokens": ["Da", "lag's", "in", "den", "\u00c4s\u00b7ten", ",", "ich", "zog", "es", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nun sagt, wie k\u00f6nnen wir's brauchen?\u00ab", "tokens": ["Nun", "sagt", ",", "wie", "k\u00f6n\u00b7nen", "wir's", "brau\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "VMFIN", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Neugierig beschaut der Schwarm den Fund,", "tokens": ["Neu\u00b7gie\u00b7rig", "be\u00b7schaut", "der", "Schwarm", "den", "Fund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die K\u00f6nigin spricht mit Lachen:", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "spricht", "mit", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbdas beinerne Ding ist h\u00fcbsch und fein,", "tokens": ["\u00bb", "das", "bei\u00b7ner\u00b7ne", "Ding", "ist", "h\u00fcbsch", "und", "fein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eine Harfe wolln wir draus machen.", "tokens": ["Ei\u00b7ne", "Har\u00b7fe", "wolln", "wir", "draus", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Komm, Schilfbart, alter Musikant,", "tokens": ["Komm", ",", "Schilf\u00b7bart", ",", "al\u00b7ter", "Mu\u00b7si\u00b7kant", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft von solchen Dingen;", "tokens": ["Du", "wei\u00dft", "von", "sol\u00b7chen", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich schenk' einen Schwertfisch dir zum Ro\u00df,", "tokens": ["Ich", "schenk'", "ei\u00b7nen", "Schwert\u00b7fisch", "dir", "zum", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kannst du's zustande bringen.\u00ab", "tokens": ["Kannst", "du's", "zu\u00b7stan\u00b7de", "brin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Meermann kommt, er nimmt das Gebein,", "tokens": ["Der", "Meer\u00b7mann", "kommt", ",", "er", "nimmt", "das", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er f\u00fcgt es mit langem Gekl\u00fcgel,", "tokens": ["Er", "f\u00fcgt", "es", "mit", "lan\u00b7gem", "Ge\u00b7kl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er macht aus den Fingern die Wirbel gut,", "tokens": ["Er", "macht", "aus", "den", "Fin\u00b7gern", "die", "Wir\u00b7bel", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Aus dem Brustbein macht er den B\u00fcgel.", "tokens": ["Aus", "dem", "Brust\u00b7bein", "macht", "er", "den", "B\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Er nimmt von der K\u00f6nigin goldenem Haar", "tokens": ["Er", "nimmt", "von", "der", "K\u00f6\u00b7ni\u00b7gin", "gol\u00b7de\u00b7nem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und spannt es dr\u00fcber als Saiten;", "tokens": ["Und", "spannt", "es", "dr\u00fc\u00b7ber", "als", "Sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "KOUS", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ei wie so wundersam durch die Nacht", "tokens": ["Ei", "wie", "so", "wun\u00b7der\u00b7sam", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die T\u00f6ne schwellen und gleiten!", "tokens": ["Die", "T\u00f6\u00b7ne", "schwel\u00b7len", "und", "glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Nun schl\u00e4gt er die Harfe wohl auf und ab,", "tokens": ["Nun", "schl\u00e4gt", "er", "die", "Har\u00b7fe", "wohl", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da lassen die Wellen das Rauschen,", "tokens": ["Da", "las\u00b7sen", "die", "Wel\u00b7len", "das", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Wind h\u00e4lt leise den Odem an", "tokens": ["Der", "Wind", "h\u00e4lt", "lei\u00b7se", "den", "O\u00b7dem", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schlummert ein im Lauschen.", "tokens": ["Und", "schlum\u00b7mert", "ein", "im", "Lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die M\u00f6wen setzen sich nieder am Strand,", "tokens": ["Die", "M\u00f6\u00b7wen", "set\u00b7zen", "sich", "nie\u00b7der", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Goldfischlein steigen vom Grunde,", "tokens": ["Gold\u00b7fisc\u00b7hlein", "stei\u00b7gen", "vom", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Es horcht die Luft, es horcht das Meer", "tokens": ["Es", "horcht", "die", "Luft", ",", "es", "horcht", "das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bezaubert in der Runde;", "tokens": ["Be\u00b7zau\u00b7bert", "in", "der", "Run\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Meermann harft und singt darein,", "tokens": ["Der", "Meer\u00b7mann", "harft", "und", "singt", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er f\u00fchlt nicht M\u00fch' noch Sorgen;", "tokens": ["Er", "f\u00fchlt", "nicht", "M\u00fch'", "noch", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Nixen schlingen den Reigen dazu", "tokens": ["Die", "Ni\u00b7xen", "schlin\u00b7gen", "den", "Rei\u00b7gen", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PAV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bis an den roten Morgen.", "tokens": ["Bis", "an", "den", "ro\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}