{"dta.poem.10180": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bluhmen im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es lag die Erde noch vom strengen Frost gedr\u00fcckt,", "tokens": ["Es", "lag", "die", "Er\u00b7de", "noch", "vom", "stren\u00b7gen", "Frost", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Reiff und Schnee bedeckt, und auch zugleich", "tokens": ["Vom", "Reiff", "und", "Schnee", "be\u00b7deckt", ",", "und", "auch", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "VVPP", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An Fenster-Scheiben war der feuchte Dunst verdickt.", "tokens": ["An", "Fens\u00b7ter\u00b7Schei\u00b7ben", "war", "der", "feuch\u00b7te", "Dunst", "ver\u00b7dickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein nett formiret Eis, halb f\u00fcrchterlich, halb sch\u00f6n,", "tokens": ["Ein", "nett", "for\u00b7mi\u00b7ret", "Eis", ",", "halb", "f\u00fcrch\u00b7ter\u00b7lich", ",", "halb", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "War fr\u00fch in meinem Zimmer", "tokens": ["War", "fr\u00fch", "in", "mei\u00b7nem", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In einem wei\u00dflich grauen Schimmer", "tokens": ["In", "ei\u00b7nem", "wei\u00df\u00b7lich", "grau\u00b7en", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Am Fenster \u00fcberall zu sehn,", "tokens": ["Am", "Fens\u00b7ter", "\u00fc\u00b7be\u00b7rall", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Woraus ein\u2019 allgemein\u2019 und greise D\u00e4mmerung,", "tokens": ["Wo\u00b7raus", "ein'", "all\u00b7ge\u00b7mein'", "und", "grei\u00b7se", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.9": {"text": "Die Lust und Schau\u2019r zugleich in uns erweckt, entsprung.", "tokens": ["Die", "Lust", "und", "Schau'r", "zu\u00b7gleich", "in", "uns", "er\u00b7weckt", ",", "ent\u00b7sprung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "APPR", "PPER", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch brach, zu gleicher Zeit, ein lieblich rother Schein", "tokens": ["Doch", "brach", ",", "zu", "glei\u00b7cher", "Zeit", ",", "ein", "lieb\u00b7lich", "ro\u00b7ther", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Aus den vom Morgen-Roth bestrahlten Fenster-Scheiben", "tokens": ["Aus", "den", "vom", "Mor\u00b7gen\u00b7Roth", "be\u00b7strahl\u00b7ten", "Fens\u00b7ter\u00b7Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Rachbars, voller Glantz durch Eis und Glas herein.", "tokens": ["Des", "Rach\u00b7bars", ",", "vol\u00b7ler", "Glantz", "durch", "Eis", "und", "Glas", "her\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Auge ward mit Lust gewahr.", "tokens": ["Mein", "Au\u00b7ge", "ward", "mit", "Lust", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An denen zarten Eises Spitzen,", "tokens": ["An", "de\u00b7nen", "zar\u00b7ten", "Ei\u00b7ses", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein r\u00f6thlich angenehmes blitzen.", "tokens": ["Ein", "r\u00f6th\u00b7lich", "an\u00b7ge\u00b7neh\u00b7mes", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Offt zeigt uns das gefrohrne Glas,", "tokens": ["Offt", "zeigt", "uns", "das", "ge\u00b7frohr\u00b7ne", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von mannichfaltgen Creaturen,", "tokens": ["Von", "man\u00b7nich\u00b7falt\u00b7gen", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Gew\u00e4chsen,, Pflantzen, Stauden, Gras,", "tokens": ["Ge\u00b7w\u00e4ch\u00b7sen", ",", ",", "Pflant\u00b7zen", ",", "Stau\u00b7den", ",", "Gras", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So manche zierliche Figuren.", "tokens": ["So", "man\u00b7che", "zier\u00b7li\u00b7che", "Fi\u00b7gu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein mir fiel zugleich noch etwas ins Gesicht.", "tokens": ["Al\u00b7lein", "mir", "fiel", "zu\u00b7gleich", "noch", "et\u00b7was", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor diesen Fenstern stand im gr\u00fcnenden Gepr\u00e4nge", "tokens": ["Vor", "die\u00b7sen", "Fens\u00b7tern", "stand", "im", "gr\u00fc\u00b7nen\u00b7den", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von wahren Bluhmen eine Menge,", "tokens": ["Von", "wah\u00b7ren", "Bluh\u00b7men", "ei\u00b7ne", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die theils aus Wasser blos, und theils auch aus der Erden,", "tokens": ["Die", "theils", "aus", "Was\u00b7ser", "blos", ",", "und", "theils", "auch", "aus", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "ADV", "$,", "KON", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch W\u00e4rm\u2019, auch selbst im Frost, hervor getrieben", "tokens": ["Durch", "W\u00e4rm'", ",", "auch", "selbst", "im", "Frost", ",", "her\u00b7vor", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "ADV", "ADV", "APPRART", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bey mancher Hyacinth, auch Tulpen und Terzetten,", "tokens": ["Bey", "man\u00b7cher", "Hya\u00b7cin\u00b7th", ",", "auch", "Tul\u00b7pen", "und", "Ter\u00b7zet\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "War, nebst dem gr\u00fcnen Laub, auch Wunder-sch\u00f6n", "tokens": ["War", ",", "nebst", "dem", "gr\u00fc\u00b7nen", "Laub", ",", "auch", "Wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Pracht von Lilien-Convallien zu sehn.", "tokens": ["Die", "Pracht", "von", "Li\u00b7li\u00b7en\u00b7Con\u00b7val\u00b7li\u00b7en", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich stutzt\u2019, als ich daselbst auf einem Grunde", "tokens": ["Ich", "stutzt'", ",", "als", "ich", "da\u00b7selbst", "auf", "ei\u00b7nem", "Grun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Von Schnee und Eis, der Bluhmen bunte Pracht,", "tokens": ["Von", "Schnee", "und", "Eis", ",", "der", "Bluh\u00b7men", "bun\u00b7te", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Trotz scharffer K\u00e4lte strenger Macht,", "tokens": ["Trotz", "scharf\u00b7fer", "K\u00e4l\u00b7te", "stren\u00b7ger", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "So gar durch ihren Feind annoch erhoben, funde:", "tokens": ["So", "gar", "durch", "ih\u00b7ren", "Feind", "an\u00b7noch", "er\u00b7ho\u00b7ben", ",", "fun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und ich zugleich den Winter und den Lentzen,", "tokens": ["Und", "ich", "zu\u00b7gleich", "den", "Win\u00b7ter", "und", "den", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "In fast nicht unterschiednen Grentzen,", "tokens": ["In", "fast", "nicht", "un\u00b7ter\u00b7schied\u00b7nen", "Grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "In weiss-und buntem Schimmer gl\u00e4ntzen,", "tokens": ["In", "weiss\u00b7\u00b7und", "bun\u00b7tem", "Schim\u00b7mer", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und gleichsam auf einmahl zugleich durch Africa,", "tokens": ["Und", "gleich\u00b7sam", "auf", "ein\u00b7mahl", "zu\u00b7gleich", "durch", "A\u00b7fri\u00b7ca", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Zumahl es eben an-zu schneyen fieng,", "tokens": ["Zu\u00b7mahl", "es", "e\u00b7ben", "an\u00b7zu", "schne\u00b7yen", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Und ein schnell Flocken-Heer wild durch einander gieng;", "tokens": ["Und", "ein", "schnell", "Flo\u00b7cken\u00b7Heer", "wild", "durch", "ein\u00b7an\u00b7der", "gieng", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In Rova Zembla sah.", "tokens": ["In", "Ro\u00b7va", "Zem\u00b7bla", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hiedurch ward ich, wie billig, sehr ger\u00fchrt.", "tokens": ["Hie\u00b7durch", "ward", "ich", ",", "wie", "bil\u00b7lig", ",", "sehr", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und, wie uns das Gesch\u00f6pf mit Recht zum Sch\u00f6pfer leitet,", "tokens": ["Und", ",", "wie", "uns", "das", "Ge\u00b7sch\u00f6pf", "mit", "Recht", "zum", "Sch\u00f6p\u00b7fer", "lei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ART", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum grossen Sch\u00f6pfer auch gef\u00fchret.", "tokens": ["Zum", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "auch", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wandte mich zu Dem, der alles zubereitet,", "tokens": ["Ich", "wand\u00b7te", "mich", "zu", "Dem", ",", "der", "al\u00b7les", "zu\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stellte mir,", "tokens": ["Und", "stell\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bey diesem, Lentz und Frost vereinenden, Gesicht,", "tokens": ["Bey", "die\u00b7sem", ",", "Lentz", "und", "Frost", "ver\u00b7ei\u00b7nen\u00b7den", ",", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "NN", "KON", "NN", "VVPP", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Wesen, welches nirgend nicht,", "tokens": ["Sein", "We\u00b7sen", ",", "wel\u00b7ches", "nir\u00b7gend", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In der Allgegenwart, mir f\u00fcr.", "tokens": ["In", "der", "All\u00b7ge\u00b7gen\u00b7wart", ",", "mir", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "APPR", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "O un\u00fcmschr\u00e4ncktes All! rieff ich, von Ehr-Furcht", "tokens": ["O", "un\u00b7\u00fcm\u00b7schr\u00e4nck\u00b7tes", "All", "!", "rieff", "ich", ",", "von", "Ehr\u00b7Furcht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor dessen Angesicht sich auf der Welt", "tokens": ["Vor", "des\u00b7sen", "An\u00b7ge\u00b7sicht", "sich", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So S\u00fcd-Lands Glut, als Nordens Eis,", "tokens": ["So", "S\u00fcd\u00b7Lands", "Glut", ",", "als", "Nor\u00b7dens", "Eis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "$,", "KOUS", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Best\u00e4ndig gegenw\u00e4rtig stellt!", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "stellt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der, wie das Feld sich hier mit Schnee beziehet,", "tokens": ["Der", ",", "wie", "das", "Feld", "sich", "hier", "mit", "Schnee", "be\u00b7zie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein anders dort in schw\u00fclen Strahlen gl\u00fchet,", "tokens": ["Ein", "an\u00b7ders", "dort", "in", "schw\u00fc\u00b7len", "Strah\u00b7len", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie da der Mandel-Baum in s\u00fcsser R\u00f6the bl\u00fchet,", "tokens": ["Wie", "da", "der", "Man\u00b7del\u00b7Baum", "in", "s\u00fcs\u00b7ser", "R\u00f6\u00b7the", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie hier gekeltert wird, auf einmahl \u00fcbersiehet.", "tokens": ["Wie", "hier", "ge\u00b7kel\u00b7tert", "wird", ",", "auf", "ein\u00b7mahl", "\u00fc\u00b7ber\u00b7sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "VAFIN", "$,", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wenig man", "tokens": ["So", "we\u00b7nig", "man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Auch einigen Begriff sich machen kann", "tokens": ["Auch", "ei\u00b7ni\u00b7gen", "Be\u00b7griff", "sich", "ma\u00b7chen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PRF", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von der Allgegenwart; so zeiget dieses Bild,", "tokens": ["Von", "der", "All\u00b7ge\u00b7gen\u00b7wart", ";", "so", "zei\u00b7get", "die\u00b7ses", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Das mein Gem\u00fcth mit tieffen dencken f\u00fcllt,", "tokens": ["Das", "mein", "Ge\u00b7m\u00fcth", "mit", "tief\u00b7fen", "den\u00b7cken", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.