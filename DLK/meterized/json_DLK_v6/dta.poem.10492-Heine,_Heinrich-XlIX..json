{"dta.poem.10492": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich auf dem Lager liege,", "tokens": ["Wenn", "ich", "auf", "dem", "La\u00b7ger", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In Nacht und Kissen geh\u00fcllt,", "tokens": ["In", "Nacht", "und", "Kis\u00b7sen", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So schwebt mir vor ein s\u00fc\u00dfes,", "tokens": ["So", "schwebt", "mir", "vor", "ein", "s\u00fc\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Anmuthig liebes Bild.", "tokens": ["An\u00b7mut\u00b7hig", "lie\u00b7bes", "Bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wenn mir der stille Schlummer", "tokens": ["Wenn", "mir", "der", "stil\u00b7le", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geschlossen die Augen kaum,", "tokens": ["Ge\u00b7schlos\u00b7sen", "die", "Au\u00b7gen", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So schleicht das Bild sich leise", "tokens": ["So", "schleicht", "das", "Bild", "sich", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinein in meinen Traum.", "tokens": ["Hin\u00b7ein", "in", "mei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch mit dem Traum des Morgens", "tokens": ["Doch", "mit", "dem", "Traum", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zerrinnt es nimmermehr;", "tokens": ["Zer\u00b7rinnt", "es", "nim\u00b7mer\u00b7mehr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann trag' ich es im Herzen", "tokens": ["Dann", "trag'", "ich", "es", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ganzen Tag umher.", "tokens": ["Den", "gan\u00b7zen", "Tag", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}