{"dta.poem.4213": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vern\u00fcnftiges Erwegen \u00fcber g\u00f6ttliche  \n Werke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So oft ich Deine Wunder-Werke,", "tokens": ["So", "oft", "ich", "Dei\u00b7ne", "Wun\u00b7der\u00b7Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in denselben einen Strahl,", "tokens": ["Und", "in", "den\u00b7sel\u00b7ben", "ei\u00b7nen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O GOtt! von Deiner Weisheit, St\u00e4rke", "tokens": ["O", "Gott", "!", "von", "Dei\u00b7ner", "Weis\u00b7heit", ",", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$.", "APPR", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lieb\u2019 und Majest\u00e4t bemerke;", "tokens": ["Und", "Lieb'", "und", "Ma\u00b7jes\u00b7t\u00e4t", "be\u00b7mer\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So deucht mich, da\u00df ich aus der Zahl", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "ich", "aus", "der", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der unvern\u00fcnft\u2019gen Thiere steige,", "tokens": ["Der", "un\u00b7ver\u00b7n\u00fcnft'\u00b7gen", "Thie\u00b7re", "stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df, in ihrer Sch\u00f6nheit Pracht,", "tokens": ["Und", "da\u00df", ",", "in", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich Deine Weisheit, Lieb\u2019 und Macht", "tokens": ["Sich", "Dei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Seelen offenbar\u2019 und zeige.", "tokens": ["Der", "See\u00b7len", "of\u00b7fen\u00b7ba\u00b7r'", "und", "zei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dann deucht mich, da\u00df ich erst in mir", "tokens": ["Dann", "deucht", "mich", ",", "da\u00df", "ich", "erst", "in", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was Menschliches empfind\u2019 und sp\u00fchr\u2019,", "tokens": ["Was", "Menschli\u00b7ches", "emp\u00b7find'", "und", "sp\u00fchr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Ja GOttes Bild in mir empfinde,", "tokens": ["Ja", "Got\u00b7tes", "Bild", "in", "mir", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn ich den Geist, durch Dich mit Dir,", "tokens": ["Wenn", "ich", "den", "Geist", ",", "durch", "Dich", "mit", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "APPR", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In Deinem Werk\u2019, o HErr! verbinde,", "tokens": ["In", "Dei\u00b7nem", "Werk'", ",", "o", "Herr", "!", "ver\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da ich Dich \u00fcberall allhier", "tokens": ["Da", "ich", "Dich", "\u00fc\u00b7be\u00b7rall", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In offenbarten Spuhren finde.", "tokens": ["In", "of\u00b7fen\u00b7bar\u00b7ten", "Spuh\u00b7ren", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann facht der Creaturen Zier", "tokens": ["Dann", "facht", "der", "Crea\u00b7tu\u00b7ren", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erst die Bewunderung in mir,", "tokens": ["Erst", "die", "Be\u00b7wun\u00b7de\u00b7rung", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann die Gluht der Andacht an.", "tokens": ["Und", "dann", "die", "Gluht", "der", "An\u00b7dacht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann w\u00fcnsch\u2019 ich, w\u00fcrdig Dich zu ehren,", "tokens": ["Dann", "w\u00fcn\u00b7sch'", "ich", ",", "w\u00fcr\u00b7dig", "Dich", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und stets, so viel an mir, zu mehren", "tokens": ["Und", "stets", ",", "so", "viel", "an", "mir", ",", "zu", "meh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "APPR", "PPER", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Lob, so viel ich immer kann.", "tokens": ["Dein", "Lob", ",", "so", "viel", "ich", "im\u00b7mer", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sehne mich, Dich zu erheben,", "tokens": ["Ich", "seh\u00b7ne", "mich", ",", "Dich", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und, durch derselben Schmuck ger\u00fchrt,", "tokens": ["Und", ",", "durch", "der\u00b7sel\u00b7ben", "Schmuck", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ja gleichsam selbst zu Dir gef\u00fchrt,", "tokens": ["Ja", "gleich\u00b7sam", "selbst", "zu", "Dir", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fchl\u2019 ich ein br\u00fcnstiges Bestreben,", "tokens": ["F\u00fchl'", "ich", "ein", "br\u00fcns\u00b7ti\u00b7ges", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nach Deinem Willen hier zu leben.", "tokens": ["Nach", "Dei\u00b7nem", "Wil\u00b7len", "hier", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach gieb, da\u00df sich in meiner Brust", "tokens": ["Ach", "gieb", ",", "da\u00df", "sich", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "KOUS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dergleichen Andacht, Sucht und Lust,", "tokens": ["Derg\u00b7lei\u00b7chen", "An\u00b7dacht", ",", "Sucht", "und", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Deine Werk\u2019, oft in mir rege,", "tokens": ["Durch", "Dei\u00b7ne", "Werk'", ",", "oft", "in", "mir", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "ADJA", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da\u00df, voller Dank-Erkenntlichkeit,", "tokens": ["Da\u00df", ",", "vol\u00b7ler", "Dank\u00b7Er\u00b7kennt\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich Deine Vollenkommenheit,", "tokens": ["Ich", "Dei\u00b7ne", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Lust und Ehrfurcht preisen m\u00f6ge!", "tokens": ["In", "Lust", "und", "Ehr\u00b7furcht", "prei\u00b7sen", "m\u00f6\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach la\u00df, o HErr, mein frohes Lallen,", "tokens": ["Ach", "la\u00df", ",", "o", "Herr", ",", "mein", "fro\u00b7hes", "Lal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "FM", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus Gnaden, Dir denn auch gefallen!", "tokens": ["Aus", "Gna\u00b7den", ",", "Dir", "denn", "auch", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}