{"textgrid.poem.44498": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wenn der Vogel singen will]", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Vogel singen will,", "tokens": ["Wenn", "der", "Vo\u00b7gel", "sin\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht er einen Ast,", "tokens": ["Sucht", "er", "ei\u00b7nen", "Ast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur die Lerche tr\u00e4gt beim Sang", "tokens": ["Nur", "die", "Ler\u00b7che", "tr\u00e4gt", "beim", "Sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eigne, leichte Last.", "tokens": ["Eig\u00b7ne", ",", "leich\u00b7te", "Last", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch der Fink, die Nachtigall,", "tokens": ["Doch", "der", "Fink", ",", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst der muntre Spatz", "tokens": ["Selbst", "der", "mun\u00b7tre", "Spatz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4hlen, eh die Kehle t\u00f6nt,", "tokens": ["W\u00e4h\u00b7len", ",", "eh", "die", "Keh\u00b7le", "t\u00f6nt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den Fu\u00df den Platz.", "tokens": ["F\u00fcr", "den", "Fu\u00df", "den", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gebt mir, wo ich stehen soll,", "tokens": ["Gebt", "mir", ",", "wo", "ich", "ste\u00b7hen", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weist mir das Gebiet,", "tokens": ["Weist", "mir", "das", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich will euch wohl erfreun", "tokens": ["Und", "ich", "will", "euch", "wohl", "er\u00b7freun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch mit manchem Lied.", "tokens": ["Noch", "mit", "man\u00b7chem", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Denn in Deutschland weht der Sturm \u2013", "tokens": ["Denn", "in", "Deutschland", "weht", "der", "Sturm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "ART", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sturm, man wei\u00df, ist Wind \u2013", "tokens": ["Sturm", ",", "man", "wei\u00df", ",", "ist", "Wind", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVFIN", "$,", "VAFIN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4hnen, wenn der Ast sie schnellt,", "tokens": ["W\u00e4h\u00b7nen", ",", "wenn", "der", "Ast", "sie", "schnellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie fl\u00fcgge sind.", "tokens": ["Da\u00df", "sie", "fl\u00fcg\u00b7ge", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und hier Landes dunkelts tief,", "tokens": ["Und", "hier", "Lan\u00b7des", "dun\u00b7kelts", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nacht wie Pech und Harz,", "tokens": ["Nacht", "wie", "Pech", "und", "Harz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "KON", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In den Zweigen n\u00e4chst dem Stamm", "tokens": ["In", "den", "Zwei\u00b7gen", "n\u00e4chst", "dem", "Stamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nisten Dohlen schwarz.", "tokens": ["Nis\u00b7ten", "Doh\u00b7len", "schwarz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Kauz und Eule d\u00e4misch dumm", "tokens": ["Kauz", "und", "Eu\u00b7le", "d\u00e4\u00b7misch", "dumm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaun zum Astloch 'raus,", "tokens": ["Schaun", "zum", "Ast\u00b7loch", "'raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur der Starmatz schwatzt vom Platz,", "tokens": ["Nur", "der", "Star\u00b7matz", "schwatzt", "vom", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kanzelt f\u00fcr das Haus.", "tokens": ["Kan\u00b7zelt", "f\u00fcr", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Tiefer unten aber steigts", "tokens": ["Tie\u00b7fer", "un\u00b7ten", "a\u00b7ber", "steigts"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf vom Boden dumpf,", "tokens": ["Auf", "vom", "Bo\u00b7den", "dumpf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Fr\u00f6sche quaken laut", "tokens": ["Und", "die", "Fr\u00f6\u00b7sche", "qua\u00b7ken", "laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus verj\u00e4hrtem Sumpf.", "tokens": ["Aus", "ver\u00b7j\u00e4hr\u00b7tem", "Sumpf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Und so schweb ich ewgen Flugs", "tokens": ["Und", "so", "schweb", "ich", "ew\u00b7gen", "Flugs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Erd und Luft,", "tokens": ["Zwi\u00b7schen", "Erd", "und", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und kein Platz dem m\u00fcden Fu\u00df,", "tokens": ["Und", "kein", "Platz", "dem", "m\u00fc\u00b7den", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als dereinst die Gruft.", "tokens": ["Als", "de\u00b7reinst", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}