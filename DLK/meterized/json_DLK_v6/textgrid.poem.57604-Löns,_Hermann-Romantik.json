{"textgrid.poem.57604": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Romantik", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die alte Ritterburg,", "tokens": ["Das", "ist", "die", "al\u00b7te", "Rit\u00b7ter\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liegt in Schutt und Tr\u00fcmmern,", "tokens": ["Sie", "liegt", "in", "Schutt", "und", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der blanke Vollmond lacht hindurch,", "tokens": ["Der", "blan\u00b7ke", "Voll\u00b7mond", "lacht", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schleiereulen wimmern.", "tokens": ["Die", "Schlei\u00b7e\u00b7reu\u00b7len", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort oben hauste der R\u00e4ubergraf", "tokens": ["Dort", "o\u00b7ben", "haus\u00b7te", "der", "R\u00e4u\u00b7ber\u00b7graf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit all den z\u00fcnftigen Possen,", "tokens": ["Mit", "all", "den", "z\u00fcnf\u00b7ti\u00b7gen", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So bieder, ritterlich und brav,", "tokens": ["So", "bie\u00b7der", ",", "rit\u00b7ter\u00b7lich", "und", "brav", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nur gegen Standesgenossen.", "tokens": ["Doch", "nur", "ge\u00b7gen", "Stan\u00b7des\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "O edelromantischer Rittersport,", "tokens": ["O", "e\u00b7del\u00b7ro\u00b7man\u00b7ti\u00b7scher", "Rit\u00b7ter\u00b7sport", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Dorn und Dickicht zu lauern,", "tokens": ["In", "Dorn", "und", "Di\u00b7ckicht", "zu", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Keller und Kerker zu schleifen fort", "tokens": ["In", "Kel\u00b7ler", "und", "Ker\u00b7ker", "zu", "schlei\u00b7fen", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die wehrlosen B\u00fcrger und Bauern.", "tokens": ["Die", "wehr\u00b7lo\u00b7sen", "B\u00fcr\u00b7ger", "und", "Bau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Habt acht! es geht der Bundschuh um!", "tokens": ["Habt", "acht", "!", "es", "geht", "der", "Bund\u00b7schuh", "um", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es schlug die adligen Sch\u00e4cher", "tokens": ["Es", "schlug", "die", "ad\u00b7li\u00b7gen", "Sch\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Freiheit Evangelium,", "tokens": ["Der", "Frei\u00b7heit", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pechkranz flog auf die D\u00e4cher.", "tokens": ["Der", "Pech\u00b7kranz", "flog", "auf", "die", "D\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "O sch\u00f6ner Graf von Helfenstein,", "tokens": ["O", "sch\u00f6\u00b7ner", "Graf", "von", "Hel\u00b7fen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie niedlich war dein Springen", "tokens": ["Wie", "nied\u00b7lich", "war", "dein", "Sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei Lenzenblitz und Feuerschein", "tokens": ["Bei", "Len\u00b7zen\u00b7blitz", "und", "Feu\u00b7er\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lustigem Fl\u00f6tenklingen!", "tokens": ["Und", "lus\u00b7ti\u00b7gem", "Fl\u00f6\u00b7ten\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "O Mittelalter, gute Nacht!", "tokens": ["O", "Mit\u00b7tel\u00b7al\u00b7ter", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schade, da\u00df du verschwunden!", "tokens": ["Wie", "scha\u00b7de", ",", "da\u00df", "du", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gern h\u00e4tt' ich das Pereat ausgebracht", "tokens": ["Gern", "h\u00e4tt'", "ich", "das", "Pe\u00b7reat", "aus\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In gl\u00fchenden Versen den Hunden.", "tokens": ["In", "gl\u00fc\u00b7hen\u00b7den", "Ver\u00b7sen", "den", "Hun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Wir ziehen mit Gaudeamus vorbei,", "tokens": ["Wir", "zie\u00b7hen", "mit", "Gau\u00b7dea\u00b7mus", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schwinden im Dunkel die Tr\u00fcmmer \u2013", "tokens": ["Es", "schwin\u00b7den", "im", "Dun\u00b7kel", "die", "Tr\u00fcm\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein letzter, gellender Eulenschrei,", "tokens": ["Ein", "letz\u00b7ter", ",", "gel\u00b7len\u00b7der", "Eu\u00b7len\u00b7schrei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es klingt wie Grafengewimmer.", "tokens": ["Es", "klingt", "wie", "Gra\u00b7fen\u00b7ge\u00b7wim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}