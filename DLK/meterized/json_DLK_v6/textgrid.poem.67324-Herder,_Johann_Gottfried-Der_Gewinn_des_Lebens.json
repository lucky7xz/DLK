{"textgrid.poem.67324": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Gewinn des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am k\u00fchlen Bach, am luft'gen Baum", "tokens": ["Am", "k\u00fch\u00b7len", "Bach", ",", "am", "luft'\u00b7gen", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4um' ich nun meines Lebens Traum,", "tokens": ["Tr\u00e4um'", "ich", "nun", "mei\u00b7nes", "Le\u00b7bens", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mag nicht wissen, ob die Welt,", "tokens": ["Und", "mag", "nicht", "wis\u00b7sen", ",", "ob", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich mir tr\u00e4ume, sei bestellt;", "tokens": ["Wie", "ich", "mir", "tr\u00e4u\u00b7me", ",", "sei", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ach, ist Der wol mehr begl\u00fcckt,", "tokens": ["Denn", "ach", ",", "ist", "Der", "wol", "mehr", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "VAFIN", "ART", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der, da\u00df sie nicht so sei, erblickt?", "tokens": ["Der", ",", "da\u00df", "sie", "nicht", "so", "sei", ",", "er\u00b7blickt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich ging einmal der Weisheit nach", "tokens": ["Ich", "ging", "ein\u00b7mal", "der", "Weis\u00b7heit", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rte, was die Weisheit sprach.", "tokens": ["Und", "h\u00f6r\u00b7te", ",", "was", "die", "Weis\u00b7heit", "sprach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprach so viel- und mancherlei,", "tokens": ["Sie", "sprach", "so", "viel", "und", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "TRUNC", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was einst die Welt gewesen sei", "tokens": ["Was", "einst", "die", "Welt", "ge\u00b7we\u00b7sen", "sei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VAPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und jetzt nicht ist und, sehr verirrt,", "tokens": ["Und", "jetzt", "nicht", "ist", "und", ",", "sehr", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VAFIN", "KON", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wol nimmer, nimmer werden wird.", "tokens": ["Wol", "nim\u00b7mer", ",", "nim\u00b7mer", "wer\u00b7den", "wird", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich gr\u00e4mte mich und ging im Gram,", "tokens": ["Ich", "gr\u00e4m\u00b7te", "mich", "und", "ging", "im", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als mir der Ruhm entgegen kam.", "tokens": ["Als", "mir", "der", "Ruhm", "ent\u00b7ge\u00b7gen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdir,\u00ab sprach er, \u00bbSohn, Dir ist beschert,", "tokens": ["\u00bb", "dir", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "Sohn", ",", "Dir", "ist", "be\u00b7schert", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu r\u00e4umen weg, was Dich, beschwert.\u00ab", "tokens": ["Zu", "r\u00e4u\u00b7men", "weg", ",", "was", "Dich", ",", "be\u00b7schwert", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "PTKVZ", "$,", "PWS", "PPER", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich r\u00e4umte, wollte vor mich sehn;", "tokens": ["Ich", "r\u00e4um\u00b7te", ",", "woll\u00b7te", "vor", "mich", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allein die Felsen blieben stehn.", "tokens": ["Al\u00b7lein", "die", "Fel\u00b7sen", "blie\u00b7ben", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ermattet, ohne Gram und Zorn,", "tokens": ["Er\u00b7mat\u00b7tet", ",", "oh\u00b7ne", "Gram", "und", "Zorn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht' ich nun Rosen unterm Dorn.", "tokens": ["Sucht'", "ich", "nun", "Ro\u00b7sen", "un\u00b7term", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosen, ach! entf\u00e4rbten sich,", "tokens": ["Die", "Ro\u00b7sen", ",", "ach", "!", "ent\u00b7f\u00e4rb\u00b7ten", "sich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Dornen stachen mich;", "tokens": ["Und", "ih\u00b7re", "Dor\u00b7nen", "sta\u00b7chen", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwei Kn\u00f6spchen unter allen hier,", "tokens": ["Zwei", "Kn\u00f6spc\u00b7hen", "un\u00b7ter", "al\u00b7len", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Am k\u00fchlen Bach, am luft'gen Baum", "tokens": ["Am", "k\u00fch\u00b7len", "Bach", ",", "am", "luft'\u00b7gen", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4um' ich nun meines Lebens Traum.", "tokens": ["Tr\u00e4um'", "ich", "nun", "mei\u00b7nes", "Le\u00b7bens", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die beiden Kn\u00f6spchen pfleg' ich mir", "tokens": ["Die", "bei\u00b7den", "Kn\u00f6spc\u00b7hen", "pfleg'", "ich", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weihe sie, o Sonne, Dir!", "tokens": ["Und", "wei\u00b7he", "sie", ",", "o", "Son\u00b7ne", ",", "Dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, k\u00fchler Bach, erquicke sie!", "tokens": ["Komm", ",", "k\u00fch\u00b7ler", "Bach", ",", "er\u00b7qui\u00b7cke", "sie", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Komm, s\u00fc\u00dfes L\u00fcftchen, st\u00e4rke sie!", "tokens": ["Komm", ",", "s\u00fc\u00b7\u00dfes", "L\u00fcft\u00b7chen", ",", "st\u00e4r\u00b7ke", "sie", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}