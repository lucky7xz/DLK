{"textgrid.poem.64271": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "An Kaiser Wilhelm I", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht wie ein Herrscher unsrer Tage", "tokens": ["Nicht", "wie", "ein", "Herr\u00b7scher", "uns\u00b7rer", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht Kaiser Wilhelm in der Welt:", "tokens": ["Steht", "Kai\u00b7ser", "Wil\u00b7helm", "in", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, wie ein Bild der alten Sage:", "tokens": ["Nein", ",", "wie", "ein", "Bild", "der", "al\u00b7ten", "Sa\u00b7ge", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Friedens- wie ein Schlachtenheld!", "tokens": ["Ein", "Frie\u00b7dens", "wie", "ein", "Schlach\u00b7ten\u00b7held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihm gab der Schiedwalt des Gefechtes,", "tokens": ["Ihm", "gab", "der", "Schied\u00b7walt", "des", "Ge\u00b7fech\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gott Odhin selbst, das Zauberschwert,", "tokens": ["Gott", "Od\u00b7hin", "selbst", ",", "das", "Zau\u00b7ber\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das, wenn gez\u00fcckt zum Schutz des Rechtes,", "tokens": ["Das", ",", "wenn", "ge\u00b7z\u00fcckt", "zum", "Schutz", "des", "Rech\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie sieglos in die Scheide kehrt.", "tokens": ["Nie", "sieg\u00b7los", "in", "die", "Schei\u00b7de", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nach dem Kampf streut er den Segen", "tokens": ["Doch", "nach", "dem", "Kampf", "streut", "er", "den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Friedens aus der milden Hand", "tokens": ["Des", "Frie\u00b7dens", "aus", "der", "mil\u00b7den", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schirmt mit ehrnen Schildgehegen", "tokens": ["Und", "schirmt", "mit", "ehr\u00b7nen", "Schild\u00b7ge\u00b7he\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Marken dem bedrohten Land.", "tokens": ["Die", "Mar\u00b7ken", "dem", "be\u00b7droh\u00b7ten", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht Lorbeer nur und Laub der Eichen", "tokens": ["Nicht", "Lor\u00b7beer", "nur", "und", "Laub", "der", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "ADV", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schm\u00fcckt dieses Greises Schwert und Thron:", "tokens": ["Schm\u00fcckt", "die\u00b7ses", "Grei\u00b7ses", "Schwert", "und", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Efeufl\u00fcstern h\u00f6r' ich streichen", "tokens": ["Wie", "E\u00b7feuf\u00b7l\u00fcs\u00b7tern", "h\u00f6r'", "ich", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um ihn die Heldensage schon.", "tokens": ["Um", "ihn", "die", "Hel\u00b7den\u00b7sa\u00b7ge", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl hat gar oft den heil'gen Namen", "tokens": ["Wohl", "hat", "gar", "oft", "den", "heil'\u00b7gen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des \u00bbVaters\u00ab Schmeichelwort entweiht,", "tokens": ["Des", "\u00bb", "Va\u00b7ters", "\u00ab", "Schmei\u00b7chel\u00b7wort", "ent\u00b7weiht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit gef\u00fcllt den Flitterrahmen", "tokens": ["Da\u00b7mit", "ge\u00b7f\u00fcllt", "den", "Flit\u00b7ter\u00b7rah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verdienstebarer F\u00fcrstlichkeit: \u2013", "tokens": ["Ver\u00b7diens\u00b7te\u00b7ba\u00b7rer", "F\u00fcrst\u00b7lich\u00b7keit", ":", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Den Herzog ", "tokens": ["Den", "Her\u00b7zog"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ein dankbar Volk nennt seinen Vater", "tokens": ["Ein", "dank\u00b7bar", "Volk", "nennt", "sei\u00b7nen", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, Kaiser Wilhelm Silberbart!", "tokens": ["Dich", ",", "Kai\u00b7ser", "Wil\u00b7helm", "Sil\u00b7ber\u00b7bart", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}