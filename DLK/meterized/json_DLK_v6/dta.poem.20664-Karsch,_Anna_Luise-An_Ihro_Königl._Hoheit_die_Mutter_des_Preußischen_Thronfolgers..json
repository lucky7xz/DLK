{"dta.poem.20664": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Ihro K\u00f6nigl. Hoheit  \n die                  \n  Mutter des Preu\u00dfischen Thronfolgers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Du gleich einem andern Menschenkinde                 ", "tokens": ["Die", "Du", "gleich", "ei\u00b7nem", "an\u00b7dern", "Men\u00b7schen\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Tages Sch\u00f6nheit nicht gekannt,", "tokens": ["Des", "Ta\u00b7ges", "Sch\u00f6n\u00b7heit", "nicht", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als man zuerst die goldne F\u00fcrstenbinde", "tokens": ["Als", "man", "zu\u00b7erst", "die", "gold\u00b7ne", "F\u00fcrs\u00b7ten\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um Deine Stirne wand;", "tokens": ["Um", "Dei\u00b7ne", "Stir\u00b7ne", "wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du wurdest von zwe\u2019n Himmlischen bewundert,", "tokens": ["Du", "wur\u00b7dest", "von", "zwe'n", "Himm\u00b7li\u00b7schen", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Deinen ersten Schlaf bewacht.", "tokens": ["Die", "Dei\u00b7nen", "ers\u00b7ten", "Schlaf", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprachen von dem k\u00fcnftigen Jahrhundert,", "tokens": ["Sie", "spra\u00b7chen", "von", "dem", "k\u00fcnf\u00b7ti\u00b7gen", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von der Krone Pracht,", "tokens": ["Und", "von", "der", "Kro\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Deines Sohnes Schl\u00e4fe w\u00fcrde schm\u00fccken,", "tokens": ["Die", "Dei\u00b7nes", "Soh\u00b7nes", "Schl\u00e4\u00b7fe", "w\u00fcr\u00b7de", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn er s\u00e4\u00df auf seinem Thron,", "tokens": ["Und", "wenn", "er", "s\u00e4\u00df", "auf", "sei\u00b7nem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann segneten die V\u00f6lker mit Entz\u00fccken", "tokens": ["Dann", "seg\u00b7ne\u00b7ten", "die", "V\u00f6l\u00b7ker", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Mutter und den Sohn.", "tokens": ["Die", "Mut\u00b7ter", "und", "den", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Prinze\u00dfin, also sprachen mit einander                 ", "tokens": ["Prin\u00b7ze\u00b7\u00dfin", ",", "al\u00b7so", "spra\u00b7chen", "mit", "ein\u00b7an\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "APPR", "PRF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zwe\u2019n Engel, welche sich vergn\u00fcgt,", "tokens": ["Zwe'n", "En\u00b7gel", ",", "wel\u00b7che", "sich", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nicht in Dir ein zweyter Alexander,", "tokens": ["Da\u00df", "nicht", "in", "Dir", "ein", "zwey\u00b7ter", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "PPER", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Pyrrhus ward gewiegt;", "tokens": ["Ein", "Pyrr\u00b7hus", "ward", "ge\u00b7wiegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df nicht in Dir tief eingewickelt l\u00e4ge", "tokens": ["Da\u00df", "nicht", "in", "Dir", "tief", "ein\u00b7ge\u00b7wi\u00b7ckelt", "l\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "PPER", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Prinz, der einst voll Kriegeswut", "tokens": ["Ein", "Prinz", ",", "der", "einst", "voll", "Krie\u00b7ges\u00b7wut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Feld und Wald bezeichnete die Wege", "tokens": ["Durch", "Feld", "und", "Wald", "be\u00b7zeich\u00b7ne\u00b7te", "die", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit der Erschlagnen Blut.", "tokens": ["Mit", "der", "Er\u00b7schlag\u00b7nen", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie sahen alle Z\u00fcge, die gezwungen", "tokens": ["Sie", "sa\u00b7hen", "al\u00b7le", "Z\u00fc\u00b7ge", ",", "die", "ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Freund, Dein K\u00f6nig, muste thun,", "tokens": ["Dein", "Freund", ",", "Dein", "K\u00f6\u00b7nig", ",", "mus\u00b7te", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Schlachten, die den Feinden mi\u00dfgelungen,", "tokens": ["Und", "Schlach\u00b7ten", ",", "die", "den", "Fein\u00b7den", "mi\u00df\u00b7ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh ", "tokens": ["Eh"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Und Deinen Sohn bekleidet mit dem Panzer,", "tokens": ["Und", "Dei\u00b7nen", "Sohn", "be\u00b7klei\u00b7det", "mit", "dem", "Pan\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Heldendegen in der Hand,", "tokens": ["Den", "Hel\u00b7den\u00b7de\u00b7gen", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und voller Glut, als w\u00e4r in Ihm Sein ganzer", "tokens": ["Und", "vol\u00b7ler", "Glut", ",", "als", "w\u00e4r", "in", "Ihm", "Sein", "gan\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "KOKOM", "VAFIN", "APPR", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sonst sanfter Geist entbrannt.", "tokens": ["Sonst", "sanf\u00b7ter", "Geist", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Begleiteten Ihn durch Gefahr,", "tokens": ["Be\u00b7glei\u00b7te\u00b7ten", "Ihn", "durch", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als Er bey ", "tokens": ["Als", "Er", "bey"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "An den Sudeten war.", "tokens": ["An", "den", "Su\u00b7de\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Sie brachten diesen Stolz des Vaterlandes", "tokens": ["Sie", "brach\u00b7ten", "die\u00b7sen", "Stolz", "des", "Va\u00b7ter\u00b7lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Deine Brust, und l\u00e4cheln einst,", "tokens": ["An", "Dei\u00b7ne", "Brust", ",", "und", "l\u00e4\u00b7cheln", "einst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wegen eines sch\u00f6n verschr\u00e4nkten Bandes", "tokens": ["Wenn", "we\u00b7gen", "ei\u00b7nes", "sch\u00f6n", "ver\u00b7schr\u00e4nk\u00b7ten", "Ban\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Deine Freude weinst;", "tokens": ["Du", "Dei\u00b7ne", "Freu\u00b7de", "weinst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn Er mit Deinen Tugenden gezieret", "tokens": ["Wenn", "Er", "mit", "Dei\u00b7nen", "Tu\u00b7gen\u00b7den", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die reizendste Prinze\u00dfinn sieht,", "tokens": ["Die", "rei\u00b7zends\u00b7te", "Prin\u00b7ze\u00b7\u00dfinn", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wenn Er Sie bey Seiner Hand gef\u00fchret", "tokens": ["Und", "wenn", "Er", "Sie", "bey", "Sei\u00b7ner", "Hand", "ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An Deinen Busen zieht,", "tokens": ["An", "Dei\u00b7nen", "Bu\u00b7sen", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und Ihren Geist und Ihres Herzens Sch\u00f6ne", "tokens": ["Und", "Ih\u00b7ren", "Geist", "und", "Ih\u00b7res", "Her\u00b7zens", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr als der Wange Bl\u00fchten liebt,", "tokens": ["Mehr", "als", "der", "Wan\u00b7ge", "Bl\u00fch\u00b7ten", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Seinen ewigsichern Staaten S\u00f6hne,", "tokens": ["Und", "Sei\u00b7nen", "e\u00b7wig\u00b7si\u00b7chern", "Staa\u00b7ten", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachfolger ", "tokens": ["Nach\u00b7fol\u00b7ger"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}