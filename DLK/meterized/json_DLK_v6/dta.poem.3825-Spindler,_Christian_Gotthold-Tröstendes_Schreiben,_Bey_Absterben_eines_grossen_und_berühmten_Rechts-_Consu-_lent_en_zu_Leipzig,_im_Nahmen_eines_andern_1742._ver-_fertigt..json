{"dta.poem.3825": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00f6stendes Schreiben,  \n Bey Absterben eines grossen und ber\u00fchmten Rechts- Consu-  \n lent en zu Leipzig, im Nahmen eines andern 1742. ver-  \n fertigt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Aendert denn Natur und Himmel niemahls", "tokens": ["A\u00b7en\u00b7dert", "denn", "Na\u00b7tur", "und", "Him\u00b7mel", "nie\u00b7mahls"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Gesetze nicht?", "tokens": ["die", "Ge\u00b7set\u00b7ze", "nicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bleibt es denn beym alten Bunde, der das", "tokens": ["Bleibt", "es", "denn", "beym", "al\u00b7ten", "Bun\u00b7de", ",", "der", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,", "PRELS", "ART"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Todes-Urtheil spricht?", "tokens": ["To\u00b7des\u00b7Urt\u00b7heil", "spricht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wird denn \u00fcber jedermann hier der Urtheil-Stab", "tokens": ["Wird", "denn", "\u00fc\u00b7ber", "je\u00b7der\u00b7mann", "hier", "der", "Urt\u00b7heil\u00b7Stab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PIS", "ADV", "ART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "gebrochen?", "tokens": ["ge\u00b7bro\u00b7chen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Wird denn jedem Sarg und Grufft, Bahr und", "tokens": ["Wird", "denn", "je\u00b7dem", "Sarg", "und", "Grufft", ",", "Bahr", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "KON", "NN", "$,", "NE", "KON"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.8": {"text": "H\u00f6le zugesprochen?", "tokens": ["H\u00f6\u00b7le", "zu\u00b7ge\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Sch\u00fctzen nicht die Wissenschafften? wird man", "tokens": ["Sch\u00fct\u00b7zen", "nicht", "die", "Wis\u00b7sen\u00b7schaff\u00b7ten", "?", "wird", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "$.", "VAFIN", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "denn durch nichts befreyt?", "tokens": ["denn", "durch", "nichts", "be\u00b7freyt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Nein; der Satz ist unumst\u00f6\u00dflich: Alles stirbet mit", "tokens": ["Nein", ";", "der", "Satz", "ist", "un\u00b7um\u00b7st\u00f6\u00df\u00b7lich", ":", "Al\u00b7les", "stir\u00b7bet", "mit"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "NN", "VAFIN", "ADJD", "$.", "PIS", "VVFIN", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "der Zeit.", "tokens": ["der", "Zeit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Dieses Schicksal, dieses Ach, hat auch unser Hertz", "tokens": ["Die\u00b7ses", "Schick\u00b7sal", ",", "die\u00b7ses", "Ach", ",", "hat", "auch", "un\u00b7ser", "Hertz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "erf\u00fcllet,", "tokens": ["er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und wir sind in Trauer-Tuch, Boy und Fl\u00f6re", "tokens": ["Und", "wir", "sind", "in", "Trau\u00b7e\u00b7rTuch", ",", "Boy", "und", "Fl\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "$,", "NE", "KON", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "eingeh\u00fcllet;", "tokens": ["ein\u00b7ge\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Denn der from\u0303e ", "tokens": ["Denn", "der", "from\u0303e"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Ist uns durch den Tod entrissen; dieses schmertzt,", "tokens": ["Ist", "uns", "durch", "den", "Tod", "ent\u00b7ris\u00b7sen", ";", "die\u00b7ses", "schmertzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$.", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "und geht uns nah.", "tokens": ["und", "geht", "uns", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Seine Arbeit, seine Last, seine Sorgen, seine M\u00fche,", "tokens": ["Sei\u00b7ne", "Ar\u00b7beit", ",", "sei\u00b7ne", "Last", ",", "sei\u00b7ne", "Sor\u00b7gen", ",", "sei\u00b7ne", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Brachte h\u00f6chst erw\u00fcnschte Frucht, wenn ein Armer", "tokens": ["Brach\u00b7te", "h\u00f6chst", "er\u00b7w\u00fcnschte", "Frucht", ",", "wenn", "ein", "Ar\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "H\u00fclffe schrie;", "tokens": ["H\u00fclf\u00b7fe", "schrie", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Wem er Rettung zugesaget, war auch Ja, ein", "tokens": ["Wem", "er", "Ret\u00b7tung", "zu\u00b7ge\u00b7sa\u00b7get", ",", "war", "auch", "Ja", ",", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,", "VAFIN", "ADV", "PTKANT", "$,", "ART"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Wort, ein Mann,", "tokens": ["Wort", ",", "ein", "Mann", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Di\u00df ist, was zu seinem Ruhme ihm ein jeder zeu-", "tokens": ["Di\u00df", "ist", ",", "was", "zu", "sei\u00b7nem", "Ruh\u00b7me", "ihm", "ein", "je\u00b7der", "zeu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PPER", "ART", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.26": {"text": "gen kan.", "tokens": ["gen", "kan", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VMFIN", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.27": {"text": "So hast du denn deinem GOtt und der Welt sehr", "tokens": ["So", "hast", "du", "denn", "dei\u00b7nem", "Gott", "und", "der", "Welt", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "ART", "NN", "ADV"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "treu gedienet,", "tokens": ["treu", "ge\u00b7die\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "O! was Wunder! wenn dein Ruhm bis auf sp\u00e4te", "tokens": ["O", "!", "was", "Wun\u00b7der", "!", "wenn", "dein", "Ruhm", "bis", "auf", "sp\u00e4\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWS", "NN", "$.", "KOUS", "PPOSAT", "NN", "APPR", "APPR", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Zeiten gr\u00fcnet.", "tokens": ["Zei\u00b7ten", "gr\u00fc\u00b7net", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Dieses Lob ist \u00e4cht und t\u00fcchtig und braucht frem-", "tokens": ["Die\u00b7ses", "Lob", "ist", "\u00e4cht", "und", "t\u00fcch\u00b7tig", "und", "braucht", "frem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KON", "VVFIN", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.32": {"text": "der Farben nicht,", "tokens": ["der", "Far\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Weil von ihm die fromme Wahrheit dieses Lob", "tokens": ["Weil", "von", "ihm", "die", "from\u00b7me", "Wahr\u00b7heit", "die\u00b7ses", "Lob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "PDAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.34": {"text": "mit Rechte spricht.", "tokens": ["mit", "Rech\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.35": {"text": "Nun, er ruhe sanfft und wohl, ist er uns gleich", "tokens": ["Nun", ",", "er", "ru\u00b7he", "sanfft", "und", "wohl", ",", "ist", "er", "uns", "gleich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "NN", "VVFIN", "KON", "ADV", "$,", "VAFIN", "PPER", "PPER", "ADV"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.36": {"text": "itzt entzogen,", "tokens": ["itzt", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "O! so baut ihm doch das Hertz tausend Danck", "tokens": ["O", "!", "so", "baut", "ihm", "doch", "das", "Hertz", "tau\u00b7send", "Danck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "CARD", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "und Ehren-Bogen.", "tokens": ["und", "Eh\u00b7ren\u00b7Bo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Dort ist nun sein Lohn der Treue, wo er jubiliren kan,", "tokens": ["Dort", "ist", "nun", "sein", "Lohn", "der", "Treu\u00b7e", ",", "wo", "er", "ju\u00b7bi\u00b7li\u00b7ren", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+-+---+", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Dort wird man ihn wieder sehen in dem frohen Ca-", "tokens": ["Dort", "wird", "man", "ihn", "wie\u00b7der", "se\u00b7hen", "in", "dem", "fro\u00b7hen", "Ca"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ADV", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "naan.", "tokens": ["naan", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}}}}}