{"textgrid.poem.65529": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2. Bav und Mav", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bav oder Mav!", "tokens": ["Bav", "o\u00b7der", "Mav", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Es sch\u00fcttle sich, wen's traf.", "tokens": ["Es", "sch\u00fctt\u00b7le", "sich", ",", "wen's", "traf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zeichne sie zum Kennen,", "tokens": ["Zeich\u00b7ne", "sie", "zum", "Ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Brauchst sie nicht zu nennen.", "tokens": ["Brauchst", "sie", "nicht", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}