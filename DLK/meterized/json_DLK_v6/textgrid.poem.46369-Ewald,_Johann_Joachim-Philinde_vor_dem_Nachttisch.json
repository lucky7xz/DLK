{"textgrid.poem.46369": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Philinde vor dem Nachttisch", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Reize nahet euch! Philind ist schon erwacht,", "tokens": ["Ihr", "Rei\u00b7ze", "na\u00b7het", "euch", "!", "Phi\u00b7lind", "ist", "schon", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Liebesg\u00f6tter schl\u00fcpft in ihre Morgentracht,", "tokens": ["Ihr", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "schl\u00fcpft", "in", "ih\u00b7re", "Mor\u00b7gen\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Naht euch ihr J\u00fcnglinge! doch nehmet euch in acht,", "tokens": ["Naht", "euch", "ihr", "J\u00fcng\u00b7lin\u00b7ge", "!", "doch", "neh\u00b7met", "euch", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "CARD", "$,"], "meter": "++-++--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df euch der Gott der aus dem Busen lacht,", "tokens": ["Da\u00df", "euch", "der", "Gott", "der", "aus", "dem", "Bu\u00b7sen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie mir nicht tiefe Wunden macht!", "tokens": ["Wie", "mir", "nicht", "tie\u00b7fe", "Wun\u00b7den", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}