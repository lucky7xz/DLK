{"textgrid.poem.35023": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horch, klopfte es nicht an die Pforte?", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, klopfte es nicht an die Pforte?", "tokens": ["Horch", ",", "klopf\u00b7te", "es", "nicht", "an", "die", "Pfor\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer naht, von Himmelsduft umrauscht?", "tokens": ["Wer", "naht", ",", "von", "Him\u00b7mels\u00b7duft", "um\u00b7rauscht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woher des Trostes s\u00fc\u00dfe Worte,", "tokens": ["Wo\u00b7her", "des", "Tros\u00b7tes", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf die mein Herz voll Andacht lauscht?", "tokens": ["Auf", "die", "mein", "Herz", "voll", "An\u00b7dacht", "lauscht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer neigt, wenn alle Sterne sanken,", "tokens": ["Wer", "neigt", ",", "wenn", "al\u00b7le", "Ster\u00b7ne", "san\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit mildem Licht und stiller Huld", "tokens": ["Mit", "mil\u00b7dem", "Licht", "und", "stil\u00b7ler", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich zu dem Staub- und Erdenkranken?", "tokens": ["Sich", "zu", "dem", "Staub", "und", "Er\u00b7den\u00b7kran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist der Engel der Geduld.", "tokens": ["Es", "ist", "der", "En\u00b7gel", "der", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbo la\u00df den Gram nicht m\u00e4chtig werden,", "tokens": ["\u00bb", "o", "la\u00df", "den", "Gram", "nicht", "m\u00e4ch\u00b7tig", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "ART", "NN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du tiefbetr\u00fcbtes Menschenkind!", "tokens": ["Du", "tief\u00b7be\u00b7tr\u00fcb\u00b7tes", "Men\u00b7schen\u00b7kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wi\u00df', da\u00df die Leiden dieser Erden", "tokens": ["Wi\u00df'", ",", "da\u00df", "die", "Lei\u00b7den", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Des Himmels beste Gaben sind", "tokens": ["Des", "Him\u00b7mels", "bes\u00b7te", "Ga\u00b7ben", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df, wenn Sorgen Dich umwogen", "tokens": ["Und", "da\u00df", ",", "wenn", "Sor\u00b7gen", "Dich", "um\u00b7wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "$,", "KOUS", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Dich umh\u00fcllt des Zweifels Nacht,", "tokens": ["Und", "Dich", "um\u00b7h\u00fcllt", "des", "Zwei\u00b7fels", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort an dem glanzumfloss'nen Bogen", "tokens": ["Dort", "an", "dem", "glanz\u00b7um\u00b7floss'\u00b7nen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein treues Vaterauge wacht!\u00ab", "tokens": ["Ein", "treu\u00b7es", "Va\u00b7te\u00b7rau\u00b7ge", "wacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbo la\u00df Dir nicht zu Herzen steigen", "tokens": ["\u00bb", "o", "la\u00df", "Dir", "nicht", "zu", "Her\u00b7zen", "stei\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "VVIMP", "PPER", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die langverhaltne Thr\u00e4nenfluth!", "tokens": ["Die", "lang\u00b7ver\u00b7halt\u00b7ne", "Thr\u00e4\u00b7nen\u00b7fluth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wi\u00df, da\u00df grad in den schmerzensreichen", "tokens": ["Wi\u00df", ",", "da\u00df", "grad", "in", "den", "schmer\u00b7zens\u00b7rei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Geschicken tiefe Weisheit ruht,", "tokens": ["Ge\u00b7schi\u00b7cken", "tie\u00b7fe", "Weis\u00b7heit", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df, wenn sonst Dir Nichts verbliebe,", "tokens": ["Und", "da\u00df", ",", "wenn", "sonst", "Dir", "Nichts", "ver\u00b7blie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "ADV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hoffnung doch Dir immer lacht,", "tokens": ["Die", "Hoff\u00b7nung", "doch", "Dir", "im\u00b7mer", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da \u00fcber Dich in ew'ger Liebe", "tokens": ["Da", "\u00fc\u00b7ber", "Dich", "in", "ew'\u00b7ger", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein treues Vaterauge wacht!\u00ab", "tokens": ["Ein", "treu\u00b7es", "Va\u00b7te\u00b7rau\u00b7ge", "wacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbo wolle nie Dich einsam f\u00fchlen!", "tokens": ["\u00bb", "o", "wol\u00b7le", "nie", "Dich", "ein\u00b7sam", "f\u00fch\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VMFIN", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obgleich kein Aug' sie wandeln sah,", "tokens": ["Ob\u00b7gleich", "kein", "Aug'", "sie", "wan\u00b7deln", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sorgenhei\u00dfe Stirn zu k\u00fchlen", "tokens": ["Die", "sor\u00b7gen\u00b7hei\u00b7\u00dfe", "Stirn", "zu", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Himmelsboten immer da.", "tokens": ["Sind", "Him\u00b7mels\u00b7bo\u00b7ten", "im\u00b7mer", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer gern dem eignen Herzen glaubte,", "tokens": ["Wer", "gern", "dem", "eig\u00b7nen", "Her\u00b7zen", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der kennt des Pulses heilige Macht.", "tokens": ["Der", "kennt", "des", "Pul\u00b7ses", "hei\u00b7li\u00b7ge", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Drum wi\u00df, das \u00fcber Deinem Haupte", "tokens": ["Drum", "wi\u00df", ",", "das", "\u00fc\u00b7ber", "Dei\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein treues Vaterauge wacht!\u00ab", "tokens": ["Ein", "treu\u00b7es", "Va\u00b7te\u00b7rau\u00b7ge", "wacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbdrum f\u00fcge Dich in Gottes Walten", "tokens": ["\u00bb", "drum", "f\u00fc\u00b7ge", "Dich", "in", "Got\u00b7tes", "Wal\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PAV", "VVFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und trag Dein Leid getrost und still.", "tokens": ["Und", "trag", "Dein", "Leid", "ge\u00b7trost", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es mu\u00df im Dunkel sich gestalten,", "tokens": ["Es", "mu\u00df", "im", "Dun\u00b7kel", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was er zum Lichte f\u00fchren will.", "tokens": ["Was", "er", "zum", "Lich\u00b7te", "f\u00fch\u00b7ren", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann bringt der Glaube reichen Segen,", "tokens": ["Dann", "bringt", "der", "Glau\u00b7be", "rei\u00b7chen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ob ihn der Zweifler auch verlacht,", "tokens": ["Ob", "ihn", "der", "Zweif\u00b7ler", "auch", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df \u00fcber allen Deinen Wegen", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "al\u00b7len", "Dei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein treues Vaterauge wacht!\u00ab", "tokens": ["Ein", "treu\u00b7es", "Va\u00b7te\u00b7rau\u00b7ge", "wacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}