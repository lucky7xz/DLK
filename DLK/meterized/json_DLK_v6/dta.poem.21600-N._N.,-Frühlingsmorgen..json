{"dta.poem.21600": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsmorgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Leuchtend brach der Strahl der Sonne,", "tokens": ["Leuch\u00b7tend", "brach", "der", "Strahl", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den wei\u00dfen Nebelfluthen,", "tokens": ["Aus", "den", "wei\u00b7\u00dfen", "Ne\u00b7bel\u00b7flu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich heut\u2019 am fr\u00fchen Morgen", "tokens": ["Als", "ich", "heut'", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die thaubenetzte Wiese", "tokens": ["Durch", "die", "thau\u00b7be\u00b7netz\u00b7te", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kummervollen Herzens hinschlich;", "tokens": ["Kum\u00b7mer\u00b7vol\u00b7len", "Her\u00b7zens", "hin\u00b7schlich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und die morgenfrische Erde", "tokens": ["Und", "die", "mor\u00b7gen\u00b7fri\u00b7sche", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Streckte alle ihre Glieder,", "tokens": ["Streck\u00b7te", "al\u00b7le", "ih\u00b7re", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bl\u00e4tter, Bl\u00fcthen, Halme, Gr\u00e4ser \u2014", "tokens": ["Bl\u00e4t\u00b7ter", ",", "Bl\u00fc\u00b7then", ",", "Hal\u00b7me", ",", "Gr\u00e4\u00b7ser"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Alle durstend ihm entgegen.", "tokens": ["Al\u00b7le", "durs\u00b7tend", "ihm", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wenn also Deiner Liebe", "tokens": ["Ach", ",", "wenn", "al\u00b7so", "Dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seligs\u00fc\u00dfer Strahl doch endlich", "tokens": ["Se\u00b7lig\u00b7s\u00fc\u00b7\u00dfer", "Strahl", "doch", "end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Segnend auf mich niederthaute,", "tokens": ["Seg\u00b7nend", "auf", "mich", "nie\u00b7dert\u00b7hau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene Nebel hell durchbrechend,", "tokens": ["Je\u00b7ne", "Ne\u00b7bel", "hell", "durch\u00b7bre\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die von allen Seiten tr\u00fcbe", "tokens": ["Die", "von", "al\u00b7len", "Sei\u00b7ten", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Lebens Pfad umflie\u00dfen \u2014", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Pfad", "um\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich endlich, gleich der Erde,", "tokens": ["Wenn", "ich", "end\u00b7lich", ",", "gleich", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die im Glanz der Sonne badet,", "tokens": ["Die", "im", "Glanz", "der", "Son\u00b7ne", "ba\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In dem Glanze Deiner Liebe", "tokens": ["In", "dem", "Glan\u00b7ze", "Dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meine Seele baden d\u00fcrfte!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ba\u00b7den", "d\u00fcrf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}