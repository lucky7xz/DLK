{"textgrid.poem.45142": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Nieder\u00f6streichisch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00f6t dich! n\u00f6t dich!", "tokens": ["N\u00f6t", "dich", "!", "n\u00f6t", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Machs wie die Madam Rettich,", "tokens": ["Machs", "wie", "die", "Ma\u00b7dam", "Ret\u00b7tich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wenn ihr bei Saphir singt und deklamiert,", "tokens": ["Wenn", "ihr", "bei", "Sa\u00b7phir", "singt", "und", "de\u00b7kla\u00b7miert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Werdet ihr daf\u00fcr mit Lob beschmiert.", "tokens": ["Wer\u00b7det", "ihr", "da\u00b7f\u00fcr", "mit", "Lob", "be\u00b7schmiert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}