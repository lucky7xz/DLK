{"textgrid.poem.55805": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und ich seh' die Stunde kommen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ich seh' die Stunde kommen", "tokens": ["Und", "ich", "seh'", "die", "Stun\u00b7de", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiter, ruhig, klar,", "tokens": ["Hei\u00b7ter", ",", "ru\u00b7hig", ",", "klar", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo im Strom der Zeit verschwommen,", "tokens": ["Wo", "im", "Strom", "der", "Zeit", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mein H\u00f6chstes war,", "tokens": ["Was", "mein", "H\u00f6chs\u00b7tes", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wo, von eisenharten Jochen", "tokens": ["Wo", ",", "von", "ei\u00b7sen\u00b7har\u00b7ten", "Jo\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rettungslos zermalmt,", "tokens": ["Ret\u00b7tungs\u00b7los", "zer\u00b7malmt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Meine Seele \u2013 kerngebrochen \u2013", "tokens": ["Mei\u00b7ne", "See\u00b7le", "\u2013", "kern\u00b7ge\u00b7bro\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tief im Staub verqualmt,", "tokens": ["Tief", "im", "Staub", "ver\u00b7qualmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wo der Feuerbrand verglommen,", "tokens": ["Wo", "der", "Feu\u00b7er\u00b7brand", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den mein Herz gebar \u2013", "tokens": ["Den", "mein", "Herz", "ge\u00b7bar", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich seh' die Stunde kommen,", "tokens": ["Und", "ich", "seh'", "die", "Stun\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heiter, ruhig, klar.", "tokens": ["Hei\u00b7ter", ",", "ru\u00b7hig", ",", "klar", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}