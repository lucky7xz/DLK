{"dta.poem.2385": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "JehIch hab' Hertzwerther Freund das Werkchen durchge-", "tokens": ["Jeh", "Ich", "hab'", "Hert\u00b7zwert\u00b7her", "Freund", "das", "Werk\u00b7chen", "durch\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "lesen/", "tokens": ["le\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Das Himmelsvolle Buch! das wundersch\u00f6ne Wesen.", "tokens": ["Das", "Him\u00b7mels\u00b7vol\u00b7le", "Buch", "!", "das", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sage die\u00df davon/ da\u00df mich es wol erquikkt/", "tokens": ["Und", "sa\u00b7ge", "die\u00df", "da\u00b7von", "/", "da\u00df", "mich", "es", "wol", "er\u00b7quikkt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PAV", "$(", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df mir es meinen Geist im Lesen gantz entz\u00fckt.", "tokens": ["Da\u00df", "mir", "es", "mei\u00b7nen", "Geist", "im", "Le\u00b7sen", "gantz", "ent\u00b7z\u00fckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist ein solches Werk da Kunst und Himmelsflammen/", "tokens": ["Es", "ist", "ein", "sol\u00b7ches", "Werk", "da", "Kunst", "und", "Him\u00b7mels\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da tieffer Wortverstand verbr\u00fcdert stehn beysammen.", "tokens": ["Da", "tief\u00b7fer", "Wort\u00b7ver\u00b7stand", "ver\u00b7br\u00fc\u00b7dert", "stehn", "bey\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es wil und mu\u00df nicht nur zur Lust gelesen seyn/", "tokens": ["Es", "wil", "und", "mu\u00df", "nicht", "nur", "zur", "Lust", "ge\u00b7le\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie manches andre Buch; es dringt zum Hertzen ein;", "tokens": ["Wie", "man\u00b7ches", "and\u00b7re", "Buch", ";", "es", "dringt", "zum", "Hert\u00b7zen", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erhitzt den kalten Sinn den h\u00f6chsten Gott zu lieben.", "tokens": ["Er\u00b7hitzt", "den", "kal\u00b7ten", "Sinn", "den", "h\u00f6chs\u00b7ten", "Gott", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und reitzet ware Lust das Christenthum zu \u00fcben.", "tokens": ["Und", "reit\u00b7zet", "wa\u00b7re", "Lust", "das", "Chris\u00b7ten\u00b7thum", "zu", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Di\u00df ist sein theurer Nutz. Dr\u00fcm schweig Heliodor/", "tokens": ["Di\u00df", "ist", "sein", "theu\u00b7rer", "Nutz", ".", "Dr\u00fcm", "schweig", "He\u00b7li\u00b7o\u00b7dor", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "NN", "VVFIN", "NE", "$("], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Schweig Sidney/ Pona schweig hier geht euch einer", "tokens": ["Schweig", "Sid\u00b7ney", "/", "Po\u00b7na", "schweig", "hier", "geht", "euch", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "NE", "VVFIN", "ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "vor/", "tokens": ["vor", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "+", "measure": "single.up"}, "line.15": {"text": "In eurer Schreibensahrt. Hier habt ihr euren Meister/", "tokens": ["In", "eu\u00b7rer", "Schrei\u00b7ben\u00b7sahrt", ".", "Hier", "habt", "ihr", "eu\u00b7ren", "Meis\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jhr \u00e4dles Schreibervolk/ ihr tieffgesinnten Geister.", "tokens": ["Ihr", "\u00e4d\u00b7les", "Schrei\u00b7ber\u00b7volk", "/", "ihr", "tieff\u00b7ge\u00b7sinn\u00b7ten", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jhr k\u00fcrtzet zwar mit Lust die lange Tageszeit/", "tokens": ["Ihr", "k\u00fcrt\u00b7zet", "zwar", "mit", "Lust", "die", "lan\u00b7ge", "Ta\u00b7ges\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hier aber ist der Nutz bey sch\u00f6ner Liebligkeit.", "tokens": ["Hier", "a\u00b7ber", "ist", "der", "Nutz", "bey", "sch\u00f6\u00b7ner", "Lieb\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Fahrt fort geehrter Herr/ mit solchen werthen Sachen", "tokens": ["Fahrt", "fort", "ge\u00b7ehr\u00b7ter", "Herr", "/", "mit", "sol\u00b7chen", "wert\u00b7hen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "ADJA", "NN", "$(", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Hertz/ Sinn und Gem\u00fcht\u2019 als wie verz\u00fckket machen/", "tokens": ["Die", "Hertz", "/", "Sinn", "und", "Ge\u00b7m\u00fcht'", "als", "wie", "ver\u00b7z\u00fck\u00b7ket", "ma\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "KOUS", "KOKOM", "VVFIN", "VVINF", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Und last das sch\u00f6ne Werk einst kommen an den Tag/", "tokens": ["Und", "last", "das", "sch\u00f6\u00b7ne", "Werk", "einst", "kom\u00b7men", "an", "den", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Damit der Mensch die Zeit mit Nutz vertreiben mag.", "tokens": ["Da\u00b7mit", "der", "Mensch", "die", "Zeit", "mit", "Nutz", "ver\u00b7trei\u00b7ben", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "ART", "NN", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}