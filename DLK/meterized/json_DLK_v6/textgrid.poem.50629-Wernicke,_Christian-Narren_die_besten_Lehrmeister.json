{"textgrid.poem.50629": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Narren die besten Lehrmeister", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein rechter Geck ist mehr, als mancher Schulfuchs, werth,", "tokens": ["Ein", "rech\u00b7ter", "Geck", "ist", "mehr", ",", "als", "man\u00b7cher", "Schul\u00b7fuchs", ",", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,", "KOUS", "PIAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und man lernt mehr von ihm, als auf der hohen Schule", "tokens": ["Und", "man", "lernt", "mehr", "von", "ihm", ",", "als", "auf", "der", "ho\u00b7hen", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Verkehrt, was ansteht, zeigt. Die Weisheit ", "tokens": ["Ver\u00b7kehrt", ",", "was", "an\u00b7steht", ",", "zeigt", ".", "Die", "Weis\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "PWS", "VVFIN", "$,", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In vielen Worten,", "tokens": ["In", "vie\u00b7len", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}}}}