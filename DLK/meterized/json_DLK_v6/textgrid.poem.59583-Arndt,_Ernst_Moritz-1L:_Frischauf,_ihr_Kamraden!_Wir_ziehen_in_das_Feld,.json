{"textgrid.poem.59583": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frischauf, ihr Kamraden! Wir ziehen in das Feld,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischauf, ihr Kamraden! Wir ziehen in das Feld,", "tokens": ["Frisc\u00b7hauf", ",", "ihr", "Kam\u00b7ra\u00b7den", "!", "Wir", "zie\u00b7hen", "in", "das", "Feld", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir haben unser Herz auf Franzosen gestellt,", "tokens": ["Wir", "ha\u00b7ben", "un\u00b7ser", "Herz", "auf", "Fran\u00b7zo\u00b7sen", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wehr und der Mut sind geschliffen und blank,", "tokens": ["Die", "Wehr", "und", "der", "Mut", "sind", "ge\u00b7schlif\u00b7fen", "und", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "VVPP", "KON", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drum her mit Franzosen! Die Zeit wird uns lang,", "tokens": ["Drum", "her", "mit", "Fran\u00b7zo\u00b7sen", "!", "Die", "Zeit", "wird", "uns", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APZR", "APPR", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Hurra, ihr Kamraden! Hurra, ihr Soldaten!", "tokens": ["Hur\u00b7ra", ",", "ihr", "Kam\u00b7ra\u00b7den", "!", "Hur\u00b7ra", ",", "ihr", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Heraus alle, welchen die Freiheit gef\u00e4llt!", "tokens": ["He\u00b7raus", "al\u00b7le", ",", "wel\u00b7chen", "die", "Frei\u00b7heit", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PWAT", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gott schuf f\u00fcr die Freiheit, die Tugend die Welt.", "tokens": ["Gott", "schuf", "f\u00fcr", "die", "Frei\u00b7heit", ",", "die", "Tu\u00b7gend", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Freiheit sie lebe, das k\u00f6stlichste Gut!", "tokens": ["Die", "Frei\u00b7heit", "sie", "le\u00b7be", ",", "das", "k\u00f6st\u00b7lichs\u00b7te", "Gut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "F\u00fcr Freiheit, ihr Br\u00fcder, da schonet kein Blut!", "tokens": ["F\u00fcr", "Frei\u00b7heit", ",", "ihr", "Br\u00fc\u00b7der", ",", "da", "scho\u00b7net", "kein", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Hurra, ihr Kamraden! Hurra, ihr Soldaten!", "tokens": ["Hur\u00b7ra", ",", "ihr", "Kam\u00b7ra\u00b7den", "!", "Hur\u00b7ra", ",", "ihr", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Hurra! Und vertilget die teuflische Brut!", "tokens": ["Hur\u00b7ra", "!", "Und", "ver\u00b7til\u00b7get", "die", "teuf\u00b7li\u00b7sche", "Brut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Heraus auch f\u00fcr Deutschland, das heilige Land!", "tokens": ["He\u00b7raus", "auch", "f\u00fcr", "Deutschland", ",", "das", "hei\u00b7li\u00b7ge", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Vertilget den welschen, den teuflischen Tand!", "tokens": ["Ver\u00b7til\u00b7get", "den", "wel\u00b7schen", ",", "den", "teuf\u00b7li\u00b7schen", "Tand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Auf tapferer V\u00e4ter vermorschtem Gebein", "tokens": ["Auf", "tap\u00b7fe\u00b7rer", "V\u00e4\u00b7ter", "ver\u00b7morschtem", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da soll kein Franzose sein ", "tokens": ["Da", "soll", "kein", "Fran\u00b7zo\u00b7se", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPOSAT"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Hurra, ihr Kamraden! Hurra, ihr Soldaten!", "tokens": ["Hur\u00b7ra", ",", "ihr", "Kam\u00b7ra\u00b7den", "!", "Hur\u00b7ra", ",", "ihr", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Heraus f\u00fcr die Eltern, f\u00fcr Weib und f\u00fcr Kind,", "tokens": ["He\u00b7raus", "f\u00fcr", "die", "El\u00b7tern", ",", "f\u00fcr", "Weib", "und", "f\u00fcr", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr Jungfraun und Br\u00e4ute zur Rache geschwind!", "tokens": ["F\u00fcr", "Jung\u00b7fraun", "und", "Br\u00e4u\u00b7te", "zur", "Ra\u00b7che", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das st\u00e4rket die Arme, das st\u00e4hlet den Mut,", "tokens": ["Das", "st\u00e4r\u00b7ket", "die", "Ar\u00b7me", ",", "das", "st\u00e4h\u00b7let", "den", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drob streitet man fr\u00f6hlich, drob streitet man gut.", "tokens": ["Drob", "strei\u00b7tet", "man", "fr\u00f6h\u00b7lich", ",", "drob", "strei\u00b7tet", "man", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$,", "ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hurra, ihr Kamraden! Hurra, ihr Soldaten!", "tokens": ["Hur\u00b7ra", ",", "ihr", "Kam\u00b7ra\u00b7den", "!", "Hur\u00b7ra", ",", "ihr", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Das locket das Eisen zum feindlichen Blut.", "tokens": ["Das", "lo\u00b7cket", "das", "Ei\u00b7sen", "zum", "feind\u00b7li\u00b7chen", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Heraus alle! Rufet am lautesten ", "tokens": ["He\u00b7raus", "al\u00b7le", "!", "Ru\u00b7fet", "am", "lau\u00b7tes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "$.", "VVFIN", "APPRART", "ADJA"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und machet die schn\u00f6den Tyrannen zu Spott!", "tokens": ["Und", "ma\u00b7chet", "die", "schn\u00f6\u00b7den", "Ty\u00b7ran\u00b7nen", "zu", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit Schwertern und Lanzen in blutiger Jagd", "tokens": ["Mit", "Schwer\u00b7tern", "und", "Lan\u00b7zen", "in", "blu\u00b7ti\u00b7ger", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So jaget die R\u00e4uber bei Tag und bei Nacht;", "tokens": ["So", "ja\u00b7get", "die", "R\u00e4u\u00b7ber", "bei", "Tag", "und", "bei", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Hurra, ihr Kamraden! Hurra, ihr Soldaten!", "tokens": ["Hur\u00b7ra", ",", "ihr", "Kam\u00b7ra\u00b7den", "!", "Hur\u00b7ra", ",", "ihr", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Hasasa! Trarara! Die lustige Jagd!", "tokens": ["Ha\u00b7sa\u00b7sa", "!", "Tra\u00b7ra\u00b7ra", "!", "Die", "lus\u00b7ti\u00b7ge", "Jagd", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Auf! Spielet, Kanonen, zum lustigen Tanz!", "tokens": ["Auf", "!", "Spie\u00b7let", ",", "Ka\u00b7no\u00b7nen", ",", "zum", "lus\u00b7ti\u00b7gen", "Tanz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf! Blitzet, ihr S\u00e4bel, den blinkenden Glanz!", "tokens": ["Auf", "!", "Blit\u00b7zet", ",", "ihr", "S\u00e4\u00b7bel", ",", "den", "blin\u00b7ken\u00b7den", "Glanz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Auf! Wirbelt, ihr Trommeln, im Saus und im Braus!", "tokens": ["Auf", "!", "Wir\u00b7belt", ",", "ihr", "Trom\u00b7meln", ",", "im", "Saus", "und", "im", "Braus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf! Wehet, ihr Fahnen, zum Himmel hinaus!", "tokens": ["Auf", "!", "We\u00b7het", ",", "ihr", "Fah\u00b7nen", ",", "zum", "Him\u00b7mel", "hin\u00b7aus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Hurra, ihr Kamraden! ", "tokens": ["Hur\u00b7ra", ",", "ihr", "Kam\u00b7ra\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wir ziehen zum Sieg oder Tod heut hinaus.", "tokens": ["Wir", "zie\u00b7hen", "zum", "Sieg", "o\u00b7der", "Tod", "heut", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}