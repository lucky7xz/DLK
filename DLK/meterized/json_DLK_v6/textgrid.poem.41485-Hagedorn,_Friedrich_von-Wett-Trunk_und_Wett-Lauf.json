{"textgrid.poem.41485": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wett-Trunk und Wett-Lauf", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaub, Anacharsis hatte Recht,", "tokens": ["Glaub", ",", "A\u00b7nach\u00b7ar\u00b7sis", "hat\u00b7te", "Recht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, weil er sich zuerst bezecht,", "tokens": ["Der", ",", "weil", "er", "sich", "zu\u00b7erst", "be\u00b7zecht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begehrte, da\u00df man ihm des Wett-Trunks Preis ertheilte:", "tokens": ["Be\u00b7gehr\u00b7te", ",", "da\u00df", "man", "ihm", "des", "Wet\u00b7tTrunks", "Preis", "er\u00b7theil\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was, sprach er, trug nicht der den Lohn", "tokens": ["Was", ",", "sprach", "er", ",", "trug", "nicht", "der", "den", "Lohn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKNEG", "ART", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Wett-Lauf jederzeit davon,", "tokens": ["Im", "Wet\u00b7tLauf", "je\u00b7der\u00b7zeit", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der dessen Ziel zuerst ereilte?", "tokens": ["Der", "des\u00b7sen", "Ziel", "zu\u00b7erst", "er\u00b7eil\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRELAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund, schien der Syracuser Wein", "tokens": ["Freund", ",", "schien", "der", "Sy\u00b7ra\u00b7cu\u00b7ser", "Wein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir gestern gleich zu stark zu sein,", "tokens": ["Dir", "ge\u00b7stern", "gleich", "zu", "stark", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKA", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der dich noch eh', als mich, durch seine Kraft erhitzet;", "tokens": ["Der", "dich", "noch", "eh'", ",", "als", "mich", ",", "durch", "sei\u00b7ne", "Kraft", "er\u00b7hit\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sch\u00e4me dich der Z\u00fcge nicht:", "tokens": ["So", "sch\u00e4\u00b7me", "dich", "der", "Z\u00fc\u00b7ge", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wei\u00dft, was Anacharsis spricht,", "tokens": ["Du", "wei\u00dft", ",", "was", "A\u00b7nach\u00b7ar\u00b7sis", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was er spricht, ist was dich sch\u00fctzet.", "tokens": ["Und", "was", "er", "spricht", ",", "ist", "was", "dich", "sch\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}