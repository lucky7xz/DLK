{"dta.poem.11118": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen flucher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blitz, hagel, donner, bley und gantze sack voll teuffel", "tokens": ["Blitz", ",", "ha\u00b7gel", ",", "don\u00b7ner", ",", "bley", "und", "gant\u00b7ze", "sack", "voll", "teuf\u00b7fel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN", "ADJD", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sind deine waffen stets, damit du um dich schl\u00e4gst.", "tokens": ["Sind", "dei\u00b7ne", "waf\u00b7fen", "stets", ",", "da\u00b7mit", "du", "um", "dich", "schl\u00e4gst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo nimmst du alles her? Ich glaube sonder zweiffel,", "tokens": ["Wo", "nimmst", "du", "al\u00b7les", "her", "?", "Ich", "glau\u00b7be", "son\u00b7der", "zweif\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "PPER", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df du das erst im mund\u2019, das letzt im hertzen tr\u00e4gst.", "tokens": ["Da\u00df", "du", "das", "erst", "im", "mund'", ",", "das", "letzt", "im", "hert\u00b7zen", "tr\u00e4gst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punc