{"dta.poem.6598": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kan demnach recht klar und deutlich sehn,", "tokens": ["Man", "kan", "dem\u00b7nach", "recht", "klar", "und", "deut\u00b7lich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PAV", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was C\u00f6rper, die man f\u00fchlt, und woraus sie bestehn,", "tokens": ["Was", "C\u00f6r\u00b7per", ",", "die", "man", "f\u00fchlt", ",", "und", "wo\u00b7raus", "sie", "be\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$,", "PRELS", "PIS", "VVFIN", "$,", "KON", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die uns so mancherley Bewegungen erwecken,", "tokens": ["Die", "uns", "so", "man\u00b7cher\u00b7ley", "Be\u00b7we\u00b7gun\u00b7gen", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die warm sind und die kalt, hart, fl\u00fcssig, die im Schmecken", "tokens": ["Die", "warm", "sind", "und", "die", "kalt", ",", "hart", ",", "fl\u00fcs\u00b7sig", ",", "die", "im", "Schme\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "KON", "ART", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und im Geruch Vergn\u00fcgen bringen:", "tokens": ["Und", "im", "Ge\u00b7ruch", "Ver\u00b7gn\u00fc\u00b7gen", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sichtbar sind, auch die, woraus die T\u00f6n\u2019 entspringen.", "tokens": ["Die", "sicht\u00b7bar", "sind", ",", "auch", "die", ",", "wo\u00b7raus", "die", "T\u00f6n'", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "ADV", "ART", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es sind nur Nahmen blo\u00df allein,", "tokens": ["Es", "sind", "nur", "Nah\u00b7men", "blo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So dem Gef\u00fchl gegeben seyn,", "tokens": ["So", "dem", "Ge\u00b7f\u00fchl", "ge\u00b7ge\u00b7ben", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die der so zarten C\u00f6rperlein", "tokens": ["Die", "der", "so", "zar\u00b7ten", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verschiednen Druck, verschiedentlich Bewegen,", "tokens": ["Ver\u00b7schied\u00b7nen", "Druck", ",", "ver\u00b7schie\u00b7dent\u00b7lich", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In uns erregen.", "tokens": ["In", "uns", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}