{"textgrid.poem.49682": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Reserve", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n ist's, auf dem Platz der Stadt", "tokens": ["Wie", "sch\u00f6n", "ist's", ",", "auf", "dem", "Platz", "der", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Uniform herumspazieren,", "tokens": ["In", "U\u00b7ni\u00b7form", "her\u00b7um\u00b7spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die W\u00fcrde zeigen, die man hat,", "tokens": ["Die", "W\u00fcr\u00b7de", "zei\u00b7gen", ",", "die", "man", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und exquisiteste Manieren.", "tokens": ["Und", "ex\u00b7qui\u00b7si\u00b7tes\u00b7te", "Ma\u00b7nie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Der Helmbusch schwankt, der S\u00e4bel klirrt,", "tokens": ["Der", "Helm\u00b7busch", "schwankt", ",", "der", "S\u00e4\u00b7bel", "klirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kriegerisch ist der Assesser,", "tokens": ["Und", "krie\u00b7ge\u00b7risch", "ist", "der", "As\u00b7ses\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz martialisch angeschirrt.", "tokens": ["Ganz", "mar\u00b7ti\u00b7a\u00b7lisch", "an\u00b7ge\u00b7schirrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, so gef\u00e4llt er jedem besser.", "tokens": ["Ja", ",", "so", "ge\u00b7f\u00e4llt", "er", "je\u00b7dem", "bes\u00b7ser", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ging ihm leider im B\u00fcro", "tokens": ["Zwar", "ging", "ihm", "lei\u00b7der", "im", "B\u00fc\u00b7ro"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Magen etwas in die Weite,", "tokens": ["Der", "Ma\u00b7gen", "et\u00b7was", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf dem Drehstuhl der Popo", "tokens": ["Und", "auf", "dem", "Dreh\u00b7stuhl", "der", "Po\u00b7po"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu unsoldatisch in die Breite.", "tokens": ["Zu", "un\u00b7sol\u00b7da\u00b7tisch", "in", "die", "Brei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch macht die edle M\u00e4nnerzier", "tokens": ["Doch", "macht", "die", "ed\u00b7le", "M\u00e4n\u00b7ner\u00b7zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Troddel, Portepee und Klunker", "tokens": ["Mit", "Trod\u00b7del", ",", "Por\u00b7te\u00b7pee", "und", "Klun\u00b7ker"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Staatsanwalt zum Kavalier,", "tokens": ["Den", "Staats\u00b7an\u00b7walt", "zum", "Ka\u00b7va\u00b7lier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Richter selbst zu einem Junker.", "tokens": ["Den", "Rich\u00b7ter", "selbst", "zu", "ei\u00b7nem", "Jun\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur manchmal gibt es ein Problem.", "tokens": ["Nur", "manch\u00b7mal", "gibt", "es", "ein", "Prob\u00b7lem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Spiel wird ernst. Man soll sich schie\u00dfen;", "tokens": ["Das", "Spiel", "wird", "ernst", ".", "Man", "soll", "sich", "schie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PIS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da l\u00e4\u00dft das heldische System", "tokens": ["Da", "l\u00e4\u00dft", "das", "hel\u00b7di\u00b7sche", "Sys\u00b7tem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich nicht mehr ungetr\u00fcbt genie\u00dfen.", "tokens": ["Sich", "nicht", "mehr", "un\u00b7ge\u00b7tr\u00fcbt", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}