{"textgrid.poem.32270": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die St\u00e4rke des Weins", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wein ist st\u00e4rker als das Wasser:", "tokens": ["Wein", "ist", "st\u00e4r\u00b7ker", "als", "das", "Was\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dies gestehn auch seine Hasser.", "tokens": ["Dies", "ge\u00b7stehn", "auch", "sei\u00b7ne", "Has\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wasser rei\u00dft wohl Eichen um,", "tokens": ["Was\u00b7ser", "rei\u00dft", "wohl", "Ei\u00b7chen", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und hat H\u00e4user umgerissen:", "tokens": ["Und", "hat", "H\u00e4u\u00b7ser", "um\u00b7ge\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihr wundert euch darum,", "tokens": ["Und", "ihr", "wun\u00b7dert", "euch", "da\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df der Wein mich umgerissen?", "tokens": ["Da\u00df", "der", "Wein", "mich", "um\u00b7ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}