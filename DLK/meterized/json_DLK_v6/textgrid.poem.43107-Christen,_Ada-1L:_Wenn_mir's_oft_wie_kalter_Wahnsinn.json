{"textgrid.poem.43107": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn mir's oft wie kalter Wahnsinn", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mir's oft wie kalter Wahnsinn", "tokens": ["Wenn", "mir's", "oft", "wie", "kal\u00b7ter", "Wahn\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das \u00f6de Denken rinnt,", "tokens": ["Durch", "das", "\u00f6\u00b7de", "Den\u00b7ken", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Seele, Hilfe suchend,", "tokens": ["Wenn", "die", "See\u00b7le", ",", "Hil\u00b7fe", "su\u00b7chend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Unm\u00f6gliche ersinnt;", "tokens": ["Das", "Un\u00b7m\u00f6g\u00b7li\u00b7che", "er\u00b7sinnt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn aus abgrundtiefen Schmerzen", "tokens": ["Wenn", "aus", "ab\u00b7grund\u00b7tie\u00b7fen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie empor zum Himmel schreit:", "tokens": ["Sie", "em\u00b7por", "zum", "Him\u00b7mel", "schreit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchl ich ganz und voll den Fluch erst,", "tokens": ["F\u00fchl", "ich", "ganz", "und", "voll", "den", "Fluch", "erst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "ADJD", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der da hei\u00dft \u00bb", "tokens": ["Der", "da", "hei\u00dft", "\u00bb"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}