{"textgrid.poem.46302": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorspruch 1", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O W\u00fcrtzburg du ein Lustgart bist/", "tokens": ["O", "W\u00fcrtz\u00b7burg", "du", "ein", "Lust\u00b7gart", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De\u00dfgleich\u0113 kaum auff Erden ist.", "tokens": ["De\u00df\u00b7gleich\u0113", "kaum", "auff", "Er\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Nam heist W\u00fcrtzburg recht vnd wol.", "tokens": ["Dein", "Nam", "heist", "W\u00fcrtz\u00b7burg", "recht", "vnd", "wol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weil du Gew\u00fcrtz vnd Blumen vol.", "tokens": ["Weil", "du", "Ge\u00b7w\u00fcrtz", "vnd", "Blu\u00b7men", "vol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen deine Tugendt sein/", "tokens": ["Die", "Blu\u00b7men", "dei\u00b7ne", "Tu\u00b7gendt", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gepflantzt dir in dein Hertz hinein.", "tokens": ["Ge\u00b7pflantzt", "dir", "in", "dein", "Hertz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd jetzt durch die sechs Kinderlehr/", "tokens": ["Vnd", "jetzt", "durch", "die", "sechs", "Kin\u00b7der\u00b7lehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "CARD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird alle Tugendt wachsen mehr.", "tokens": ["Wird", "al\u00b7le", "Tu\u00b7gendt", "wach\u00b7sen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja deines G\u00e4rtners M\u00fch vnd Flei\u00df/", "tokens": ["Ja", "dei\u00b7nes", "G\u00e4rt\u00b7ners", "M\u00fch", "vnd", "Flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich machen wird zum Paradei\u00df.", "tokens": ["Dich", "ma\u00b7chen", "wird", "zum", "Pa\u00b7ra\u00b7dei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}