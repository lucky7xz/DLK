{"textgrid.poem.49908": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Pauline", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Neune, die zu ewgen T\u00e4nzen", "tokens": ["Die", "Neu\u00b7ne", ",", "die", "zu", "ew\u00b7gen", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich schwesterlich die Stirne kr\u00e4nzen,", "tokens": ["Sich", "schwes\u00b7ter\u00b7lich", "die", "Stir\u00b7ne", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sollen, hei\u00dft's, im D\u00e4mmerscheine", "tokens": ["Sie", "sol\u00b7len", ",", "hei\u00dft's", ",", "im", "D\u00e4m\u00b7mer\u00b7schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "NE", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dichterischen Wunderhaine", "tokens": ["Der", "dich\u00b7te\u00b7ri\u00b7schen", "Wun\u00b7der\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gar manches Mal dir gern begegnen", "tokens": ["Gar", "man\u00b7ches", "Mal", "dir", "gern", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dich mit ihren Gaben segnen;", "tokens": ["Und", "dich", "mit", "ih\u00b7ren", "Ga\u00b7ben", "seg\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur da\u00df du, was sie dir vertrauten,", "tokens": ["Nur", "da\u00df", "du", ",", "was", "sie", "dir", "ver\u00b7trau\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit keiner Silbe l\u00e4\u00dft verlauten.", "tokens": ["Mit", "kei\u00b7ner", "Sil\u00b7be", "l\u00e4\u00dft", "ver\u00b7lau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u2013 Ob etwa ", "tokens": ["\u2013", "Ob", "et\u00b7wa"], "token_info": ["punct", "word", "word"], "pos": ["$(", "KOUS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Dir dieses Schweigen auferlegen?", "tokens": ["Dir", "die\u00b7ses", "Schwei\u00b7gen", "auf\u00b7er\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich wei\u00df, ein solcher Schatz, verschlossen,", "tokens": ["Ich", "wei\u00df", ",", "ein", "sol\u00b7cher", "Schatz", ",", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "PIAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wird doppelt wonnig erst genossen,", "tokens": ["Wird", "dop\u00b7pelt", "won\u00b7nig", "erst", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Unendlich scheint er sich zu f\u00fcllen,", "tokens": ["Un\u00b7end\u00b7lich", "scheint", "er", "sich", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Indem wir ihn der Welt verh\u00fcllen.", "tokens": ["In\u00b7dem", "wir", "ihn", "der", "Welt", "ver\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Drum, was die Freunde sagen m\u00f6chten,", "tokens": ["Drum", ",", "was", "die", "Freun\u00b7de", "sa\u00b7gen", "m\u00f6ch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Es ziemt sich kaum mit dir zu rechten;", "tokens": ["Es", "ziemt", "sich", "kaum", "mit", "dir", "zu", "rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "W\u00fcnscht mancher doch ein gleiches Gl\u00fcck", "tokens": ["W\u00fcnscht", "man\u00b7cher", "doch", "ein", "glei\u00b7ches", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Unmutig oft sich selbst zur\u00fcck!", "tokens": ["Un\u00b7mu\u00b7tig", "oft", "sich", "selbst", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PRF", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}