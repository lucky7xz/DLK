{"textgrid.poem.53013": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Thirsis h\u00f6rt den Damon an Horatzens Seite singen", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entferne dich, verha\u00dfter Reimer-Schwarm,", "tokens": ["Ent\u00b7fer\u00b7ne", "dich", ",", "ver\u00b7ha\u00df\u00b7ter", "Rei\u00b7mer\u00b7Schwarm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verst\u00f6hre nicht die heilge Stille,", "tokens": ["Ver\u00b7st\u00f6h\u00b7re", "nicht", "die", "heil\u00b7ge", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ehrfurchtsw\u00fcrdig sich um das bepalmte Haupt", "tokens": ["Die", "ehr\u00b7furchts\u00b7w\u00fcr\u00b7dig", "sich", "um", "das", "be\u00b7palm\u00b7te", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Sternen nahen Pindus ziehet.", "tokens": ["Des", "Ster\u00b7nen", "na\u00b7hen", "Pin\u00b7dus", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Flieh, Battus Brut, von dem geweihten Fus,", "tokens": ["Flieh", ",", "Bat\u00b7tus", "Brut", ",", "von", "dem", "ge\u00b7weih\u00b7ten", "Fus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und scheue des Apollo Rache.", "tokens": ["Und", "scheu\u00b7e", "des", "A\u00b7pol\u00b7lo", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Mein stoltzes Ohr, zu hoch f\u00fcr dein Geheul,", "tokens": ["Mein", "stolt\u00b7zes", "Ohr", ",", "zu", "hoch", "f\u00fcr", "dein", "Ge\u00b7heul", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sucht auf den sonnenhellen H\u00f6hen", "tokens": ["Sucht", "auf", "den", "son\u00b7nen\u00b7hel\u00b7len", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ewge Harmonie des g\u00f6ttlichen Gesangs,", "tokens": ["Die", "ew\u00b7ge", "Har\u00b7mo\u00b7nie", "des", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sangs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wodurch der weise Nebenbuhler", "tokens": ["Wo\u00b7durch", "der", "wei\u00b7se", "Ne\u00b7ben\u00b7buh\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des unerme\u00dflichen Thebanschen Pindars", "tokens": ["Des", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7chen", "The\u00b7ban\u00b7schen", "Pin\u00b7dars"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das herrschend kluge Rom entz\u00fcckt.", "tokens": ["Das", "herr\u00b7schend", "klu\u00b7ge", "Rom", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "ADJA", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erscheine mir, du Priester des Apoll,", "tokens": ["Er\u00b7schei\u00b7ne", "mir", ",", "du", "Pries\u00b7ter", "des", "A\u00b7poll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du Erbe der Thebanschen Leyer.", "tokens": ["Du", "Er\u00b7be", "der", "The\u00b7ban\u00b7schen", "Le\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Erschein und sing in der gelehrten Wuth", "tokens": ["Er\u00b7schein", "und", "sing", "in", "der", "ge\u00b7lehr\u00b7ten", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Helden, Riesen, oder G\u00f6ttern:", "tokens": ["Von", "Hel\u00b7den", ",", "Rie\u00b7sen", ",", "o\u00b7der", "G\u00f6t\u00b7tern", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nicht, so preise nur die Ruh und Lalagen", "tokens": ["Wo", "nicht", ",", "so", "prei\u00b7se", "nur", "die", "Ruh", "und", "La\u00b7la\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Auf deiner sanftged\u00e4mpften Zitter.", "tokens": ["Auf", "dei\u00b7ner", "sanft\u00b7ge\u00b7d\u00e4mpf\u00b7ten", "Zit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6r ich dich nicht? T\u00e4uscht mich die Zauberey", "tokens": ["H\u00f6r", "ich", "dich", "nicht", "?", "T\u00e4uscht", "mich", "die", "Zau\u00b7be\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PRF", "PTKNEG", "$.", "NN", "PPER", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von deinen Jonisch stoltzen Tr\u00e4umen?", "tokens": ["Von", "dei\u00b7nen", "Jo\u00b7nisch", "stolt\u00b7zen", "Tr\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie oder reisset mich dein unbekannter Geist", "tokens": ["Wie", "o\u00b7der", "reis\u00b7set", "mich", "dein", "un\u00b7be\u00b7kann\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Dunst der weisen Rasereyen", "tokens": ["Vom", "Dunst", "der", "wei\u00b7sen", "Ra\u00b7se\u00b7rey\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Berauscht, entz\u00fcndt, aus der gemeinen Welt", "tokens": ["Be\u00b7rauscht", ",", "ent\u00b7z\u00fcndt", ",", "aus", "der", "ge\u00b7mei\u00b7nen", "Welt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ins Reich der fabelhaften Schatten?", "tokens": ["Ins", "Reich", "der", "fa\u00b7bel\u00b7haf\u00b7ten", "Schat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja Flaccus k\u00f6mmt, der gantze H\u00f6mus schallt", "tokens": ["Ja", "Flac\u00b7cus", "k\u00f6mmt", ",", "der", "gant\u00b7ze", "H\u00f6\u00b7mus", "schallt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von den unsterblichen Ges\u00e4ngen.", "tokens": ["Von", "den", "uns\u00b7terb\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommen \u00fcberall aus dem gelehrten Hain", "tokens": ["Es", "kom\u00b7men", "\u00fc\u00b7be\u00b7rall", "aus", "dem", "ge\u00b7lehr\u00b7ten", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch die unentweihten Schatten", "tokens": ["Und", "durch", "die", "un\u00b7ent\u00b7weih\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die keuschen Nymphen schon mit frohen Reihn", "tokens": ["Die", "keu\u00b7schen", "Nym\u00b7phen", "schon", "mit", "fro\u00b7hen", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihn zu empfangen hergeeilet.", "tokens": ["Ihn", "zu", "emp\u00b7fan\u00b7gen", "her\u00b7ge\u00b7ei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er jauchzt daher vom Bacchus gantz erf\u00fcllt;", "tokens": ["Er", "jauchzt", "da\u00b7her", "vom", "Bac\u00b7chus", "gantz", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPRART", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Macht der feuerreichen Gottheit", "tokens": ["Die", "Macht", "der", "feu\u00b7er\u00b7rei\u00b7chen", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Treibt ihn in neuer Wuth durch Felsen, Wald und Kluft.", "tokens": ["Treibt", "ihn", "in", "neu\u00b7er", "Wuth", "durch", "Fel\u00b7sen", ",", "Wald", "und", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er singt, was nie ein Mund gesungen.", "tokens": ["Er", "singt", ",", "was", "nie", "ein", "Mund", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welt h\u00f6rt ihn den w\u00fcrdigen August", "tokens": ["Die", "Welt", "h\u00f6rt", "ihn", "den", "w\u00fcr\u00b7di\u00b7gen", "Au\u00b7gust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis zu der G\u00f6tter Rath erheben.", "tokens": ["Bis", "zu", "der", "G\u00f6t\u00b7ter", "Rath", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er schweifft umher mit Libers Priesterin", "tokens": ["Er", "schweifft", "um\u00b7her", "mit", "Li\u00b7bers", "Pries\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In den schlaflosen tollen N\u00e4chten;", "tokens": ["In", "den", "schlaf\u00b7lo\u00b7sen", "tol\u00b7len", "N\u00e4ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er stutzt und sieht, wie sie, verwundrungsvoll", "tokens": ["Er", "stutzt", "und", "sieht", ",", "wie", "sie", ",", "ver\u00b7wund\u00b7rungs\u00b7voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Th\u00e4ler, Ufer, leere W\u00e4lder,", "tokens": ["Die", "Th\u00e4\u00b7ler", ",", "U\u00b7fer", ",", "lee\u00b7re", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jauchzt und folgt dir, der Najaden Gott,", "tokens": ["Und", "jauchzt", "und", "folgt", "dir", ",", "der", "Na\u00b7ja\u00b7den", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch tausend r\u00fchmliche Gefahren.", "tokens": ["Durch", "tau\u00b7send", "r\u00fchm\u00b7li\u00b7che", "Ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Welch deutscher Mund singt neben dir, Horatz,", "tokens": ["Welch", "deut\u00b7scher", "Mund", "singt", "ne\u00b7ben", "dir", ",", "Ho\u00b7ratz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,", "NE", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wer dr\u00fcckt mit noch verwegnern Solen,", "tokens": ["Wer", "dr\u00fcckt", "mit", "noch", "ver\u00b7weg\u00b7nern", "So\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O gl\u00fccklich k\u00fchner Geist, als du selbst, deine Spur,", "tokens": ["O", "gl\u00fcck\u00b7lich", "k\u00fch\u00b7ner", "Geist", ",", "als", "du", "selbst", ",", "dei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf diesen nie bestiegnen Felsen?", "tokens": ["Auf", "die\u00b7sen", "nie", "be\u00b7stieg\u00b7nen", "Fel\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was wagt er sich in seiner frechen Wuth", "tokens": ["Was", "wagt", "er", "sich", "in", "sei\u00b7ner", "fre\u00b7chen", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht vor Verw\u00fcstung anzurichten?", "tokens": ["Nicht", "vor", "Ver\u00b7w\u00fcs\u00b7tung", "an\u00b7zu\u00b7rich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wohin, wohin, o Freund, o k\u00fchner Geist?", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "o", "Freund", ",", "o", "k\u00fch\u00b7ner", "Geist", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "FM", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erstaunst du nicht vor diesen Kl\u00fcften,", "tokens": ["Er\u00b7staunst", "du", "nicht", "vor", "die\u00b7sen", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "