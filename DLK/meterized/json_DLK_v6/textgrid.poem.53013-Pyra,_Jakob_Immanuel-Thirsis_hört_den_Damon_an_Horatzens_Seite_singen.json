als", "stren\u00b7gen", "Schlu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Trojens Untergang vermelden?", "tokens": ["Von", "Tro\u00b7jens", "Un\u00b7ter\u00b7gang", "ver\u00b7mel\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Steigt, steigt zugleich durch die bestirnte Luft", "tokens": ["Steigt", ",", "steigt", "zu\u00b7gleich", "durch", "die", "bes\u00b7tirn\u00b7te", "Luft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Horatz und du, o deutscher Flaccus,", "tokens": ["Ho\u00b7ratz", "und", "du", ",", "o", "deut\u00b7scher", "Flac\u00b7cus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und setzt der Doris Bild bey Ariadnens Krantz.", "tokens": ["Und", "setzt", "der", "Do\u00b7ris", "Bild", "bey", "A\u00b7riad\u00b7nens", "Krantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich will hier in den Th\u00e4lern bleiben,", "tokens": ["Ich", "will", "hier", "in", "den", "Th\u00e4\u00b7lern", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihrer bl\u00fchenden erhobnen Schilderey", "tokens": ["Und", "ih\u00b7rer", "bl\u00fc\u00b7hen\u00b7den", "er\u00b7hob\u00b7nen", "Schil\u00b7de\u00b7rey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sanften Lieder Ehre opfern.", "tokens": ["Der", "sanf\u00b7ten", "Lie\u00b7der", "Eh\u00b7re", "op\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}