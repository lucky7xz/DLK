{"textgrid.poem.33093": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es liegt der Herbst auf allen Wegen,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt der Herbst auf allen Wegen,", "tokens": ["Es", "liegt", "der", "Herbst", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In hundert Farben prangt sein Kleid,", "tokens": ["In", "hun\u00b7dert", "Far\u00b7ben", "prangt", "sein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie seine Trauer, seinen Segen", "tokens": ["Wie", "sei\u00b7ne", "Trau\u00b7er", ",", "sei\u00b7nen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er um sich streut zu gleicher Zeit.", "tokens": ["Er", "um", "sich", "streut", "zu", "glei\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es rauscht der Fu\u00df im welken Laube,", "tokens": ["Es", "rauscht", "der", "Fu\u00df", "im", "wel\u00b7ken", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was bl\u00fcht' und gr\u00fcnte, ward ein Traum \u2013", "tokens": ["Was", "bl\u00fcht'", "und", "gr\u00fcn\u00b7te", ",", "ward", "ein", "Traum", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein am Stocke winkt die Traube", "tokens": ["Al\u00b7lein", "am", "Sto\u00b7cke", "winkt", "die", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und goldne Frucht schm\u00fcckt rings den Baum.", "tokens": ["Und", "gold\u00b7ne", "Frucht", "schm\u00fcckt", "rings", "den", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So nimmt und gibt mit vollen H\u00e4nden", "tokens": ["So", "nimmt", "und", "gibt", "mit", "vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herbst, ein Dieb und eine Fee;", "tokens": ["Der", "Herbst", ",", "ein", "Dieb", "und", "ei\u00b7ne", "Fee", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllung kann allein ", "tokens": ["Er\u00b7f\u00fcl\u00b7lung", "kann", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch sie umf\u00e4ngt ein tiefes Weh! \u2013", "tokens": ["Doch", "sie", "um\u00b7f\u00e4ngt", "ein", "tie\u00b7fes", "Weh", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O, Herbst der Seele! deine Fr\u00fcchte,", "tokens": ["O", ",", "Herbst", "der", "See\u00b7le", "!", "dei\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind auch Gewinn sie, oder Raub?", "tokens": ["Sind", "auch", "Ge\u00b7winn", "sie", ",", "o\u00b7der", "Raub", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PPER", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der W\u00fcnsche Bl\u00fcthe ist zunichte,", "tokens": ["Der", "W\u00fcn\u00b7sche", "Bl\u00fc\u00b7the", "ist", "zu\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hoffnung Gr\u00fcn ein welkes Laub.", "tokens": ["Der", "Hoff\u00b7nung", "Gr\u00fcn", "ein", "wel\u00b7kes", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu schwer erkauft, um zu begl\u00fccken,", "tokens": ["Zu", "schwer", "er\u00b7kauft", ",", "um", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O, Seelenherbst, ist deine Zier!", "tokens": ["O", ",", "See\u00b7len\u00b7herbst", ",", "ist", "dei\u00b7ne", "Zier", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Saft der Traube kann entz\u00fccken,", "tokens": ["Der", "Saft", "der", "Trau\u00b7be", "kann", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch keine Wonne str\u00f6mt aus ", "tokens": ["Doch", "kei\u00b7ne", "Won\u00b7ne", "str\u00f6mt", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Die Weisheit, wie die Frucht sie nennen,", "tokens": ["Die", "Weis\u00b7heit", ",", "wie", "die", "Frucht", "sie", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie pre\u00dft mir bittre Thr\u00e4nen aus,", "tokens": ["Sie", "pre\u00dft", "mir", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihres Kernes herbem Brennen", "tokens": ["Und", "ih\u00b7res", "Ker\u00b7nes", "her\u00b7bem", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entkeimet nie ein Fr\u00fchlingsstrau\u00df!", "tokens": ["Ent\u00b7kei\u00b7met", "nie", "ein", "Fr\u00fch\u00b7lings\u00b7strau\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}