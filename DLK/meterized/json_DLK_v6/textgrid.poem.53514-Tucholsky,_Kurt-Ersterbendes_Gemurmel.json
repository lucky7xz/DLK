{"textgrid.poem.53514": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ersterbendes Gemurmel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allherbstlich, wenn die braunen Bl\u00e4tter fallen,", "tokens": ["All\u00b7herbst\u00b7lich", ",", "wenn", "die", "brau\u00b7nen", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "f\u00e4llt auch dem Dichter dies und jenes ein.", "tokens": ["f\u00e4llt", "auch", "dem", "Dich\u00b7ter", "dies", "und", "je\u00b7nes", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PDS", "KON", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er sieht, wie Wolken sich zusammenballen,", "tokens": ["Er", "sieht", ",", "wie", "Wol\u00b7ken", "sich", "zu\u00b7sam\u00b7men\u00b7bal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "er h\u00f6rt der V\u00f6lker wilde Streiterein . . .", "tokens": ["er", "h\u00f6rt", "der", "V\u00f6l\u00b7ker", "wil\u00b7de", "Strei\u00b7te\u00b7rein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der deutsche Dichter kratzt sich an den Waden", "tokens": ["Der", "deut\u00b7sche", "Dich\u00b7ter", "kratzt", "sich", "an", "den", "Wa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und f\u00e4ngt sich still den letzten Sommerfloh;", "tokens": ["und", "f\u00e4ngt", "sich", "still", "den", "letz\u00b7ten", "Som\u00b7mer\u00b7floh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und denkt: du k\u00f6nntst dich auch mal wieder baden", "tokens": ["und", "denkt", ":", "du", "k\u00f6nntst", "dich", "auch", "mal", "wie\u00b7der", "ba\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und \u00fcberhaupt und so . . .", "tokens": ["und", "\u00fc\u00b7ber\u00b7haupt", "und", "so", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin ein Preu\u00dfe. Pfui auf die Verneinung!", "tokens": ["Ich", "bin", "ein", "Preu\u00b7\u00dfe", ".", "Pfui", "auf", "die", "Ver\u00b7nei\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich lob die positive Position.", "tokens": ["Ich", "lob", "die", "po\u00b7si\u00b7ti\u00b7ve", "Po\u00b7si\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ich besitz das Recht der freien Meinung", "tokens": ["Und", "ich", "be\u00b7sitz", "das", "Recht", "der", "frei\u00b7en", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in Wort und Bild und auch im Grammophon.", "tokens": ["in", "Wort", "und", "Bild", "und", "auch", "im", "Gram\u00b7mo\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sage, was ich will, und sag es feste,", "tokens": ["Ich", "sa\u00b7ge", ",", "was", "ich", "will", ",", "und", "sag", "es", "fes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "KON", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "am Stammtisch sag ichs und im Wahlb\u00fcro.", "tokens": ["am", "Stamm\u00b7tisch", "sag", "ichs", "und", "im", "Wahl\u00b7b\u00fc\u00b7ro", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Stolz sag ichs und mit einer weiten Geste:", "tokens": ["Stolz", "sag", "ichs", "und", "mit", "ei\u00b7ner", "wei\u00b7ten", "Ges\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bb . . . und \u00fcberhaupt und so . . . \u00ab", "tokens": ["\u00bb", ".", ".", ".", "und", "\u00fc\u00b7ber\u00b7haupt", "und", "so", ".", ".", ".", "\u00ab"], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "$.", "$.", "$.", "KON", "ADV", "KON", "ADV", "$.", "$.", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wohnte schon in vielen, vielen Zimmern,", "tokens": ["Ich", "wohn\u00b7te", "schon", "in", "vie\u00b7len", ",", "vie\u00b7len", "Zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "am Meer, in Bukarest, in Gro\u00dfenhain;", "tokens": ["am", "Meer", ",", "in", "Bu\u00b7ka\u00b7rest", ",", "in", "Gro\u00b7\u00dfen\u00b7hain", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NE", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und immer h\u00f6rt ich eine J\u00f6hre wimmern,", "tokens": ["und", "im\u00b7mer", "h\u00f6rt", "ich", "ei\u00b7ne", "J\u00f6h\u00b7re", "wim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann mach ich mir so allerhand Gedanken,", "tokens": ["Dann", "mach", "ich", "mir", "so", "al\u00b7ler\u00b7hand", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "zum Beispiel \u00fcber unsern Reventlow \u2013", "tokens": ["zum", "Bei\u00b7spiel", "\u00fc\u00b7ber", "un\u00b7sern", "Re\u00b7vent\u00b7low", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die kleinen Kinder haut man auf den blanken", "tokens": ["Die", "klei\u00b7nen", "Kin\u00b7der", "haut", "man", "auf", "den", "blan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und \u00fcberhaupt und so . . .", "tokens": ["und", "\u00fc\u00b7ber\u00b7haupt", "und", "so", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}