{"dta.poem.7486": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich thue Jederm\u00e4nniglich zu wissen,", "tokens": ["Ich", "thue", "Je\u00b7der\u00b7m\u00e4n\u00b7nig\u00b7lich", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich den finstern Unmut sehr bereue", "tokens": ["Da\u00df", "ich", "den", "fins\u00b7tern", "Un\u00b7mut", "sehr", "be\u00b7re\u00b7ue"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und mich von Herzen meines Lebens freue,", "tokens": ["Und", "mich", "von", "Her\u00b7zen", "mei\u00b7nes", "Le\u00b7bens", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich erl\u00f6st von allen K\u00fcmmernissen.", "tokens": ["Da\u00df", "ich", "er\u00b7l\u00f6st", "von", "al\u00b7len", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein liebes Fischchen hat nun angebissen", "tokens": ["Mein", "lie\u00b7bes", "Fischchen", "hat", "nun", "an\u00b7ge\u00b7bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schw\u00f6rt mir \u00fcber alle Ma\u00dfen Treue,", "tokens": ["Und", "schw\u00f6rt", "mir", "\u00fc\u00b7ber", "al\u00b7le", "Ma\u00b7\u00dfen", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es herzt und herzt und herzt mich stets aufs Neue,", "tokens": ["Es", "herzt", "und", "herzt", "und", "herzt", "mich", "stets", "aufs", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVPP", "KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dr\u00fcckt mich schmeichelnd in die Sophakissen.", "tokens": ["Und", "dr\u00fcckt", "mich", "schmei\u00b7chelnd", "in", "die", "Sop\u00b7ha\u00b7kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich lad' Euch, meine Freunde, s\u00e4mmtlich ein,", "tokens": ["Ich", "lad'", "Euch", ",", "mei\u00b7ne", "Freun\u00b7de", ",", "s\u00e4mmt\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir eine frohe Stunde 'mal zu schenken;", "tokens": ["Mir", "ei\u00b7ne", "fro\u00b7he", "Stun\u00b7de", "'mal", "zu", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch la\u00dft mir dann die tolle Frage sein:", "tokens": ["Doch", "la\u00dft", "mir", "dann", "die", "tol\u00b7le", "Fra\u00b7ge", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wann wir uns wohl zu ehlichen gedenken?", "tokens": ["Wann", "wir", "uns", "wohl", "zu", "eh\u00b7li\u00b7chen", "ge\u00b7den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lange noch der ganze Himmel mein,", "tokens": ["So", "lan\u00b7ge", "noch", "der", "gan\u00b7ze", "Him\u00b7mel", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will ich mich nicht auf Haus und Hof beschr\u00e4nken.", "tokens": ["Will", "ich", "mich", "nicht", "auf", "Haus", "und", "Hof", "be\u00b7schr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}