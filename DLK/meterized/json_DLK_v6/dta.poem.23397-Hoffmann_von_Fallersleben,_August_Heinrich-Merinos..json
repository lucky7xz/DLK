{"dta.poem.23397": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Merinos.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und f\u00fchrt' ich von Lieb' und von Demuth ein Fuder,", "tokens": ["Und", "f\u00fchrt'", "ich", "von", "Lieb'", "und", "von", "De\u00b7muth", "ein", "Fu\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wollt' ich nicht sein so ein L\u00e4mmelbruder,", "tokens": ["Und", "wollt'", "ich", "nicht", "sein", "so", "ein", "L\u00e4m\u00b7mel\u00b7bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VAINF", "ADV", "ART", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ei so taugt' ich nicht halb mal so viel doch wie sie,", "tokens": ["Ei", "so", "taugt'", "ich", "nicht", "halb", "mal", "so", "viel", "doch", "wie", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "ADV", "ADV", "ADV", "ADV", "KOKOM", "PPER", "$,"], "meter": "+-+--+--+-++", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und ich w\u00e4r nichts n\u00fctz' in der Monarchie.", "tokens": ["Und", "ich", "w\u00e4r", "nichts", "n\u00fctz'", "in", "der", "Mon\u00b7ar\u00b7chie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und h\u00e4tt' ich auch Alles den Armen gegeben", "tokens": ["Und", "h\u00e4tt'", "ich", "auch", "Al\u00b7les", "den", "Ar\u00b7men", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIS", "ART", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und m\u00fc\u00dft' ich nun selber von Almosen leben,", "tokens": ["Und", "m\u00fc\u00dft'", "ich", "nun", "sel\u00b7ber", "von", "Al\u00b7mo\u00b7sen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und wollt' ich kein L\u00e4mmelbruder nicht sein,", "tokens": ["Und", "wollt'", "ich", "kein", "L\u00e4m\u00b7mel\u00b7bru\u00b7der", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So hielten sie Alles f\u00fcr Trug und f\u00fcr Schein.", "tokens": ["So", "hiel\u00b7ten", "sie", "Al\u00b7les", "f\u00fcr", "Trug", "und", "f\u00fcr", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und sollt' ich auch gar mit den englischen Schaaren", "tokens": ["Und", "sollt'", "ich", "auch", "gar", "mit", "den", "eng\u00b7li\u00b7schen", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Leibhaftig vor ihnen gen Himmel auffahren,", "tokens": ["Leib\u00b7haf\u00b7tig", "vor", "ih\u00b7nen", "gen", "Him\u00b7mel", "auf\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "++--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "So glaubten sie alle, sie alle daran nie,", "tokens": ["So", "glaub\u00b7ten", "sie", "al\u00b7le", ",", "sie", "al\u00b7le", "da\u00b7ran", "nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "PPER", "PIS", "PAV", "ADV", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn ich w\u00e4re kein L\u00e4mmelbruder wie sie.", "tokens": ["Wenn", "ich", "w\u00e4\u00b7re", "kein", "L\u00e4m\u00b7mel\u00b7bru\u00b7der", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PIAT", "NN", "KOKOM", "PPER", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}}}}