{"dta.poem.13049": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso machet den Beschlu\u00df. Nun auf den Rath zu kom\u0303en", "tokens": ["\u201e", "so", "ma\u00b7chet", "den", "Be\u00b7schlu\u00df", ".", "Nun", "auf", "den", "Rath", "zu", "kom\u0303en"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eden ihr von mir verlangt: Ich habe wahr genommen,", "tokens": ["\u201e", "den", "ihr", "von", "mir", "ver\u00b7langt", ":", "Ich", "ha\u00b7be", "wahr", "ge\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPER", "APPR", "PPER", "VVPP", "$.", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "425\u201dDa\u00df alles strittig sey; was jede bey der Sach", "tokens": ["\"", "Da\u00df", "al\u00b7les", "strit\u00b7tig", "sey", ";", "was", "je\u00b7de", "bey", "der", "Sach"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PIS", "ADJD", "VAFIN", "$.", "PWS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201esich vor Versprechungen zu diesem Vorzug mach.", "tokens": ["\u201e", "sich", "vor", "Ver\u00b7spre\u00b7chun\u00b7gen", "zu", "die\u00b7sem", "Vor\u00b7zug", "mach", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "APPR", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eman will die K\u00f6niginn unwiedersprechlich ehren,", "tokens": ["\u201e", "man", "will", "die", "K\u00f6\u00b7ni\u00b7ginn", "un\u00b7wie\u00b7der\u00b7sprech\u00b7lich", "eh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund ihrer Thaten Ruhm durch diesen Streit vermehren?", "tokens": ["\u201e", "und", "ih\u00b7rer", "Tha\u00b7ten", "Ruhm", "durch", "die\u00b7sen", "Streit", "ver\u00b7meh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}