{"textgrid.poem.24807": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O la\u00df mich ruhen in deinem Arm", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00df mich ruhen in deinem Arm", "tokens": ["O", "la\u00df", "mich", "ru\u00b7hen", "in", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und tief in die Augen dir schaun!", "tokens": ["Und", "tief", "in", "die", "Au\u00b7gen", "dir", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das l\u00f6st mir vom Herzen den nagenden Harm,", "tokens": ["Das", "l\u00f6st", "mir", "vom", "Her\u00b7zen", "den", "na\u00b7gen\u00b7den", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und herab in die Seele f\u00fchl' ich es warm", "tokens": ["Und", "her\u00b7ab", "in", "die", "See\u00b7le", "f\u00fchl'", "ich", "es", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie aus dem Himmel mir taun.", "tokens": ["Wie", "aus", "dem", "Him\u00b7mel", "mir", "taun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Reich her, reich her den g\u00f6ttlichen Trank,", "tokens": ["Reich", "her", ",", "reich", "her", "den", "g\u00f6tt\u00b7li\u00b7chen", "Trank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der von den Lippen dir quillt!", "tokens": ["Der", "von", "den", "Lip\u00b7pen", "dir", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ich d\u00fcrste und schmachte matt und krank;", "tokens": ["Ich", "d\u00fcrs\u00b7te", "und", "schmach\u00b7te", "matt", "und", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erst wenn ich an deinen Busen sank,", "tokens": ["Erst", "wenn", "ich", "an", "dei\u00b7nen", "Bu\u00b7sen", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wird all mein Sehnen gestillt!", "tokens": ["Wird", "all", "mein", "Seh\u00b7nen", "ge\u00b7stillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "O mehr noch! Was sch\u00fcttelst du l\u00e4chelnd dein Haupt?", "tokens": ["O", "mehr", "noch", "!", "Was", "sch\u00fct\u00b7telst", "du", "l\u00e4\u00b7chelnd", "dein", "Haupt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$.", "PWS", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "----+--+--+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In K\u00fcssen gieb mir das Gl\u00fcck,", "tokens": ["In", "K\u00fcs\u00b7sen", "gieb", "mir", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das fl\u00fcchtige, das mir die Welt geraubt,", "tokens": ["Das", "fl\u00fcch\u00b7ti\u00b7ge", ",", "das", "mir", "die", "Welt", "ge\u00b7raubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und den alten Glauben, den ich geglaubt,", "tokens": ["Und", "den", "al\u00b7ten", "Glau\u00b7ben", ",", "den", "ich", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und der Kindheit Frieden zur\u00fcck!", "tokens": ["Und", "der", "Kind\u00b7heit", "Frie\u00b7den", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}