{"textgrid.poem.33951": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halb aus dem Schlummer erwacht,", "tokens": ["Halb", "aus", "dem", "Schlum\u00b7mer", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Den ich traumlos getrunken,", "tokens": ["Den", "ich", "traum\u00b7los", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ach, wie war ich versunken", "tokens": ["Ach", ",", "wie", "war", "ich", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die unendliche Nacht!", "tokens": ["In", "die", "un\u00b7end\u00b7li\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Tiefes Verd\u00e4mmern des Seins,", "tokens": ["Tie\u00b7fes", "Ver\u00b7d\u00e4m\u00b7mern", "des", "Seins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Denkend Nichts, noch empfindend!", "tokens": ["Den\u00b7kend", "Nichts", ",", "noch", "emp\u00b7fin\u00b7dend", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "ADV", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Richtig mir selber entschwindend,", "tokens": ["Rich\u00b7tig", "mir", "sel\u00b7ber", "ent\u00b7schwin\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schatte mit Schatten zu Eins!", "tokens": ["Schat\u00b7te", "mit", "Schat\u00b7ten", "zu", "Eins", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Da beschlich's mich so bang,", "tokens": ["Da", "be\u00b7schlich's", "mich", "so", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ob auch, den Bruder verdr\u00e4ngend,", "tokens": ["Ob", "auch", ",", "den", "Bru\u00b7der", "ver\u00b7dr\u00e4n\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Geist mir und Sinne verengend,", "tokens": ["Geist", "mir", "und", "Sin\u00b7ne", "ver\u00b7en\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Listig der Tod mich umschlag.", "tokens": ["Lis\u00b7tig", "der", "Tod", "mich", "um\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Schaudernd dacht' ich's, und fuhr", "tokens": ["Schau\u00b7dernd", "dacht'", "ich's", ",", "und", "fuhr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "$,", "KON", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auf, und schlo\u00df mich an's Leben,", "tokens": ["Auf", ",", "und", "schlo\u00df", "mich", "an's", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Dr\u00e4ngte in gl\u00fch'ndem Erheben", "tokens": ["Dr\u00e4ng\u00b7te", "in", "gl\u00fch'n\u00b7dem", "Er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "K\u00fchn mich an Gott und Natur.", "tokens": ["K\u00fchn", "mich", "an", "Gott", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Siehe, da hab' ich gelebt:", "tokens": ["Sie\u00b7he", ",", "da", "hab'", "ich", "ge\u00b7lebt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Was sonst, zu Tropfen zerflossen,", "tokens": ["Was", "sonst", ",", "zu", "Trop\u00b7fen", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Langsam und karg sich ergossen,", "tokens": ["Lang\u00b7sam", "und", "karg", "sich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hat mich auf einmal durchbebt.", "tokens": ["Hat", "mich", "auf", "ein\u00b7mal", "durch\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Oft noch ber\u00fchre du mich,", "tokens": ["Oft", "noch", "be\u00b7r\u00fch\u00b7re", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Tod, wenn ich in mir zerrinne,", "tokens": ["Tod", ",", "wenn", "ich", "in", "mir", "zer\u00b7rin\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ich mich wieder gewinne", "tokens": ["Bis", "ich", "mich", "wie\u00b7der", "ge\u00b7win\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Durch den Gedanken an dich!", "tokens": ["Durch", "den", "Ge\u00b7dan\u00b7ken", "an", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}