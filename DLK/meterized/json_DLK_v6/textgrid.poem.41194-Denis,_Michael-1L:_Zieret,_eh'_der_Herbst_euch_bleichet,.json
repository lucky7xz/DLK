{"textgrid.poem.41194": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zieret, eh' der Herbst euch bleichet,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieret, eh' der Herbst euch bleichet,", "tokens": ["Zie\u00b7ret", ",", "eh'", "der", "Herbst", "euch", "blei\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieret Sineds Harfenspiel,", "tokens": ["Zie\u00b7ret", "Si\u00b7neds", "Har\u00b7fen\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frische Bl\u00e4tter! die der Barde", "tokens": ["Fri\u00b7sche", "Bl\u00e4t\u00b7ter", "!", "die", "der", "Bar\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der sch\u00f6nsten Eiche schnitt,", "tokens": ["Von", "der", "sch\u00f6ns\u00b7ten", "Ei\u00b7che", "schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu Theresiens", "tokens": ["Zu", "The\u00b7re\u00b7si\u00b7ens"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ehre schnitt!", "tokens": ["Eh\u00b7re", "schnitt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "T\u00f6net, eh' der Tag sich neiget,", "tokens": ["T\u00f6\u00b7net", ",", "eh'", "der", "Tag", "sich", "nei\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den hohen F\u00fcrstensaal,", "tokens": ["Durch", "den", "ho\u00b7hen", "F\u00fcrs\u00b7ten\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frische Saiten! die der Barde", "tokens": ["Fri\u00b7sche", "Sai\u00b7ten", "!", "die", "der", "Bar\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Feierharfe kor,", "tokens": ["Sei\u00b7ner", "Fei\u00b7er\u00b7har\u00b7fe", "kor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu Theresiens", "tokens": ["Zu", "The\u00b7re\u00b7si\u00b7ens"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ehre kohr!", "tokens": ["Eh\u00b7re", "kohr", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Von der G\u00fcte sollt ihr t\u00f6nen,", "tokens": ["Von", "der", "G\u00fc\u00b7te", "sollt", "ihr", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Saiten, die der Barde kor!", "tokens": ["Sai\u00b7ten", ",", "die", "der", "Bar\u00b7de", "kor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der G\u00fcte, die die F\u00fcrstinn,", "tokens": ["Von", "der", "G\u00fc\u00b7te", ",", "die", "die", "F\u00fcrs\u00b7tinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der nahen Aehnlichkeit", "tokens": ["Zu", "der", "na\u00b7hen", "A\u00b7ehn\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Des unendlichen", "tokens": ["Des", "un\u00b7end\u00b7li\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wesens hebt.", "tokens": ["We\u00b7sens", "hebt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "G\u00fctig ist Allvater. Gnade", "tokens": ["G\u00fc\u00b7tig", "ist", "All\u00b7va\u00b7ter", ".", "Gna\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht von seinem Antlitz aus;", "tokens": ["Geht", "von", "sei\u00b7nem", "Ant\u00b7litz", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus seinen H\u00e4nden str\u00f6met", "tokens": ["Und", "aus", "sei\u00b7nen", "H\u00e4n\u00b7den", "str\u00f6\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer Segen auf die Welt.", "tokens": ["Im\u00b7mer", "Se\u00b7gen", "auf", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist Theresia", "tokens": ["Ist", "The\u00b7re\u00b7sia"], "token_info": ["word", "word"], "pos": ["VAFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nicht sein Bild?", "tokens": ["Nicht", "sein", "Bild", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wenn aus lauen Fr\u00fchlingswolken", "tokens": ["Wenn", "aus", "lau\u00b7en", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachsthum und Gedeihen fleu\u00dft,", "tokens": ["Wach\u00b7sthum", "und", "Ge\u00b7dei\u00b7hen", "fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Trinken nicht nur Eichenwipfel,", "tokens": ["Trin\u00b7ken", "nicht", "nur", "Ei\u00b7chen\u00b7wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der knospenvolle Strauch;", "tokens": ["Und", "der", "knos\u00b7pen\u00b7vol\u00b7le", "Strauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch das niedrigste", "tokens": ["Auch", "das", "nied\u00b7rigs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Veilchen trinkt.", "tokens": ["Veil\u00b7chen", "trinkt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Also breiten Gnadenquellen", "tokens": ["Al\u00b7so", "brei\u00b7ten", "Gna\u00b7den\u00b7quel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der F\u00fcrstinn weites Reich", "tokens": ["In", "der", "F\u00fcrs\u00b7tinn", "wei\u00b7tes", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich von Ihres Sitzes Stufen", "tokens": ["Sich", "von", "Ih\u00b7res", "Sit\u00b7zes", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der n\u00e4chsten Diener Schaar", "tokens": ["Durch", "der", "n\u00e4chs\u00b7ten", "Die\u00b7ner", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis zum fernesten", "tokens": ["Bis", "zum", "fer\u00b7nes\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Pfl\u00fcger aus.", "tokens": ["Pfl\u00fc\u00b7ger", "aus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "M\u00e4nnern, die mit treuem Rathe", "tokens": ["M\u00e4n\u00b7nern", ",", "die", "mit", "treu\u00b7em", "Ra\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr das allgemeine Wohl", "tokens": ["F\u00fcr", "das", "all\u00b7ge\u00b7mei\u00b7ne", "Wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Sorgen unterst\u00fctzten;", "tokens": ["Ih\u00b7re", "Sor\u00b7gen", "un\u00b7ter\u00b7st\u00fctz\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die nun Last der Jahre beugt,", "tokens": ["Die", "nun", "Last", "der", "Jah\u00b7re", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Folget reicher Lohn", "tokens": ["Fol\u00b7get", "rei\u00b7cher", "Lohn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bis ins Grab.", "tokens": ["Bis", "ins", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "M\u00e4nnern, die mit k\u00fchnem Eisen", "tokens": ["M\u00e4n\u00b7nern", ",", "die", "mit", "k\u00fch\u00b7nem", "Ei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das blutige Gew\u00fchl", "tokens": ["In", "das", "blu\u00b7ti\u00b7ge", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich f\u00fcr Ihre Rechte st\u00fcrzten;", "tokens": ["Sich", "f\u00fcr", "Ih\u00b7re", "Rech\u00b7te", "st\u00fcrz\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die nun Greisenalter dr\u00fcckt,", "tokens": ["Die", "nun", "Grei\u00b7sen\u00b7al\u00b7ter", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Folget reicher Lohn", "tokens": ["Fol\u00b7get", "rei\u00b7cher", "Lohn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bis ins Grab.", "tokens": ["Bis", "ins", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "M\u00e4nner, die f\u00fcr Sie zu sterben", "tokens": ["M\u00e4n\u00b7ner", ",", "die", "f\u00fcr", "Sie", "zu", "ster\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnschen, aber unerh\u00f6rt", "tokens": ["W\u00fcn\u00b7schen", ",", "a\u00b7ber", "un\u00b7er\u00b7h\u00f6rt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur mit schweren Wunden kehren", "tokens": ["Nur", "mit", "schwe\u00b7ren", "Wun\u00b7den", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Sturme finstrer Schlacht,", "tokens": ["Aus", "dem", "Stur\u00