{"dta.poem.16196": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "250.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer seine Schw\u00e4chen kennt, wird fremde nicht beschreien,", "tokens": ["Wer", "sei\u00b7ne", "Schw\u00e4\u00b7chen", "kennt", ",", "wird", "frem\u00b7de", "nicht", "be\u00b7schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo er Nachsicht selbst bedarf, auch gern verzeihen.", "tokens": ["Und", "wo", "er", "Nach\u00b7sicht", "selbst", "be\u00b7darf", ",", "auch", "gern", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wird er \u00fcberlaut auch Gl\u00e4nzendes nicht loben,", "tokens": ["Doch", "wird", "er", "\u00fc\u00b7berl\u00b7aut", "auch", "Gl\u00e4n\u00b7zen\u00b7des", "nicht", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ADV", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil menschliches Verdienst er kennt aus eignen Proben.", "tokens": ["Weil", "menschli\u00b7ches", "Ver\u00b7dienst", "er", "kennt", "aus", "eig\u00b7nen", "Pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gleich von Bewunderern entfernet wie von Sp\u00f6ttern,", "tokens": ["Gleich", "von", "Be\u00b7wun\u00b7de\u00b7rern", "ent\u00b7fer\u00b7net", "wie", "von", "Sp\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird er sowenig, als verdammen, auch verg\u00f6ttern.", "tokens": ["Wird", "er", "so\u00b7we\u00b7nig", ",", "als", "ver\u00b7dam\u00b7men", ",", "auch", "ver\u00b7g\u00f6t\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}