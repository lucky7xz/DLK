{"textgrid.poem.52854": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Kneiplied vom Ahasver", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der alte Ahasver", "tokens": ["Ich", "bin", "der", "al\u00b7te", "A\u00b7has\u00b7ver"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wandre hin, ich wandre her,", "tokens": ["Ich", "wand\u00b7re", "hin", ",", "ich", "wand\u00b7re", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meine Ruh ist hin, mein Herz ist schwer,", "tokens": ["Mei\u00b7ne", "Ruh", "ist", "hin", ",", "mein", "Herz", "ist", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ich finde sie nimmer und nimmermehr.", "tokens": ["Ich", "fin\u00b7de", "sie", "nim\u00b7mer", "und", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es br\u00fcllt der Sturm, es rauscht das Wehr,", "tokens": ["Es", "br\u00fcllt", "der", "Sturm", ",", "es", "rauscht", "das", "Wehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht sterben k\u00f6nnen, o Malheur!", "tokens": ["Nicht", "ster\u00b7ben", "k\u00f6n\u00b7nen", ",", "o", "Mal\u00b7heur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMINF", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Haupt ist m\u00fcd, mein Herz ist leer,", "tokens": ["Mein", "Haupt", "ist", "m\u00fcd", ",", "mein", "Herz", "ist", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin der alte Ahasver.", "tokens": ["Ich", "bin", "der", "al\u00b7te", "A\u00b7has\u00b7ver", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es brummt der Ochs, es tanzt der B\u00e4r,", "tokens": ["Es", "brummt", "der", "Ochs", ",", "es", "tanzt", "der", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich finde sie nimmer und nimmermehr,", "tokens": ["Ich", "fin\u00b7de", "sie", "nim\u00b7mer", "und", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin der ewige Hebr\u00e4'r,", "tokens": ["Ich", "bin", "der", "e\u00b7wi\u00b7ge", "He\u00b7br\u00e4'r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meine Ruh ist hin, ich streck's Gewehr.", "tokens": ["Mei\u00b7ne", "Ruh", "ist", "hin", ",", "ich", "streck's", "Ge\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mich hetzt und jagt, ich wei\u00df nicht wer,", "tokens": ["Mich", "hetzt", "und", "jagt", ",", "ich", "wei\u00df", "nicht", "wer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wandre hin, ich wandre her,", "tokens": ["Ich", "wand\u00b7re", "hin", ",", "ich", "wand\u00b7re", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu schlafen hab' ich sehr Begehr,", "tokens": ["Zu", "schla\u00b7fen", "hab'", "ich", "sehr", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin der alte Ahasver.", "tokens": ["Ich", "bin", "der", "al\u00b7te", "A\u00b7has\u00b7ver", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich komme wie von Ohngef\u00e4hr,", "tokens": ["Ich", "kom\u00b7me", "wie", "von", "Ohn\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Meine Ruh ist hin, mein Herz ist schwer.", "tokens": ["Mei\u00b7ne", "Ruh", "ist", "hin", ",", "mein", "Herz", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich fahre \u00fcber Land und Meer,", "tokens": ["Ich", "fah\u00b7re", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wandre hin, ich wandre her.", "tokens": ["Ich", "wand\u00b7re", "hin", ",", "ich", "wand\u00b7re", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein alter Magen knurret sehr,", "tokens": ["Mein", "al\u00b7ter", "Ma\u00b7gen", "knur\u00b7ret", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin der alte Ahasver,", "tokens": ["Ich", "bin", "der", "al\u00b7te", "A\u00b7has\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wandre in die Kreuz und Quer,", "tokens": ["Ich", "wand\u00b7re", "in", "die", "Kreuz", "und", "Quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich finde sie nimmer und nimmermehr.", "tokens": ["Ich", "fin\u00b7de", "sie", "nim\u00b7mer", "und", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich lehne an die Wand den Speer,", "tokens": ["Ich", "leh\u00b7ne", "an", "die", "Wand", "den", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe keine Ruhe mehr,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Ru\u00b7he", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schweife nach der Pendellehr',", "tokens": ["Ich", "schwei\u00b7fe", "nach", "der", "Pen\u00b7del\u00b7lehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meine Ruh ist hin, mein Herz ist schwer.", "tokens": ["Mei\u00b7ne", "Ruh", "ist", "hin", ",", "mein", "Herz", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Schon lang ist's, da\u00df ich \u00fcbelh\u00f6r',", "tokens": ["Schon", "lang", "ist's", ",", "da\u00df", "ich", "\u00fc\u00b7bel\u00b7h\u00f6r'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fcra\u00e7ao ist ein fein Lik\u00f6r,", "tokens": ["K\u00fc\u00b7ra\u00e7ao", "ist", "ein", "fein", "Li\u00b7k\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJD", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich finde keine Ruhe mehr,", "tokens": ["Ich", "fin\u00b7de", "kei\u00b7ne", "Ru\u00b7he", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einst war ich unterm Milit\u00e4r.", "tokens": ["Einst", "war", "ich", "un\u00b7term", "Mi\u00b7li\u00b7t\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich zahle stets, was ich verzehr'", "tokens": ["Ich", "zah\u00b7le", "stets", ",", "was", "ich", "ver\u00b7zehr'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wandre hin und wandre her,", "tokens": ["Und", "wand\u00b7re", "hin", "und", "wand\u00b7re", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin der alte Ahasver", "tokens": ["Ich", "bin", "der", "al\u00b7te", "A\u00b7has\u00b7ver"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "mea