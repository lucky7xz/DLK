{"textgrid.poem.61880": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rauher Tag will rauhe Weise;", "tokens": ["Rau\u00b7her", "Tag", "will", "rau\u00b7he", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun am Herd der Waffenschmied", "tokens": ["Nun", "am", "Herd", "der", "Waf\u00b7fen\u00b7schmied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwerter fegt, wer lauscht im Kreise", "tokens": ["Schwer\u00b7ter", "fegt", ",", "wer", "lauscht", "im", "Krei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch auf dein ged\u00e4mpftes Lied?", "tokens": ["Noch", "auf", "dein", "ge\u00b7d\u00e4mpf\u00b7tes", "Lied", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df dir's willig, Herz, gefallen,", "tokens": ["La\u00df", "dir's", "wil\u00b7lig", ",", "Herz", ",", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PIS", "ADJD", "$,", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht die Zeit doch k\u00fchnen Gang;", "tokens": ["Geht", "die", "Zeit", "doch", "k\u00fch\u00b7nen", "Gang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies Get\u00f6s auch wird verhallen,", "tokens": ["Dies", "Ge\u00b7t\u00f6s", "auch", "wird", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn dein Volk sein Ziel errang.", "tokens": ["Wenn", "dein", "Volk", "sein", "Ziel", "er\u00b7rang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Burg einst seiner Ehren", "tokens": ["Wenn", "die", "Burg", "einst", "sei\u00b7ner", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ausgebaut ins Blaue strebt,", "tokens": ["Aus\u00b7ge\u00b7baut", "ins", "Blau\u00b7e", "strebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach Ges\u00e4ngen wird's begehren,", "tokens": ["Nach", "Ge\u00b7s\u00e4n\u00b7gen", "wird's", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drauf ein Hauch des Friedens schwebt.", "tokens": ["Drauf", "ein", "Hauch", "des", "Frie\u00b7dens", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6nheit wieder vom Poeten", "tokens": ["Sch\u00f6n\u00b7heit", "wie\u00b7der", "vom", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fordert dann ein froh Geschlecht;", "tokens": ["For\u00b7dert", "dann", "ein", "froh", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fchling, Lieb' und Andacht treten", "tokens": ["Fr\u00fch\u00b7ling", ",", "Lieb'", "und", "An\u00b7dacht", "tre\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ihr uralt heilig Recht.", "tokens": ["In", "ihr", "ur\u00b7alt", "hei\u00b7lig", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und im Klange deiner Lieder,", "tokens": ["Und", "im", "Klan\u00b7ge", "dei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob dich l\u00e4ngst die Erde k\u00fchlt,", "tokens": ["Ob", "dich", "l\u00e4ngst", "die", "Er\u00b7de", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Brust der Jugend wieder", "tokens": ["Durch", "die", "Brust", "der", "Ju\u00b7gend", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt, was du einst gef\u00fchlt.", "tokens": ["Wan\u00b7delt", ",", "was", "du", "einst", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}