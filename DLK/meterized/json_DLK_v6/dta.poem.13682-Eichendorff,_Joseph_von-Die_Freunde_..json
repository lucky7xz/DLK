{"dta.poem.13682": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dir w\u00e4chst Dein Herz noch bei der W\u00e4lder Sausen,", "tokens": ["Dir", "w\u00e4chst", "Dein", "Herz", "noch", "bei", "der", "W\u00e4l\u00b7der", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich r\u00fchren noch die wilden Riesenworte,", "tokens": ["Dich", "r\u00fch\u00b7ren", "noch", "die", "wil\u00b7den", "Rie\u00b7sen\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Gott vertrau'nd, dem h\u00f6chsten Schirm' und", "tokens": ["Nur", "Gott", "ver\u00b7trau'nd", ",", "dem", "h\u00f6chs\u00b7ten", "Schirm'", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Horte \u2014", "tokens": ["Hor\u00b7te"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So magst Du bei den alten Wundern hausen.", "tokens": ["So", "magst", "Du", "bei", "den", "al\u00b7ten", "Wun\u00b7dern", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}