{"textgrid.poem.58039": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "77. Jesus zu der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Seele, la\u00df dein Sorgen,", "tokens": ["O", "See\u00b7le", ",", "la\u00df", "dein", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dies ist nur ", "tokens": ["Dies", "ist", "nur"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sorgst du, so sorg' ich nicht!", "tokens": ["Sorgst", "du", ",", "so", "sor\u00b7g'", "ich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum bleib in mir verborgen,", "tokens": ["Drum", "bleib", "in", "mir", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In meinen Scho\u00df dich senk", "tokens": ["In", "mei\u00b7nen", "Scho\u00df", "dich", "senk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und an dich selbst nicht denk!", "tokens": ["Und", "an", "dich", "selbst", "nicht", "denk", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}