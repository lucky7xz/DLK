{"textgrid.poem.64529": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "153. Der tapfre T\u00f6ffel", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Philippen, der ihm nichts gethan,", "tokens": ["Phil\u00b7ip\u00b7pen", ",", "der", "ihm", "nichts", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lief j\u00fcngst mit wutherf\u00fclltem Blicke", "tokens": ["Lief", "j\u00fcngst", "mit", "wut\u00b7her\u00b7f\u00fcll\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit geballter Faust der tapfre T\u00f6ffel an -", "tokens": ["Und", "mit", "ge\u00b7ball\u00b7ter", "Faust", "der", "tapf\u00b7re", "T\u00f6f\u00b7fel", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kam mit blut'gem Kopf zur\u00fccke;", "tokens": ["Und", "kam", "mit", "blut'\u00b7gem", "Kopf", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dar\u00fcber lachte Fritz: den wird erst T\u00f6ffel schlagen!", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "lach\u00b7te", "Fritz", ":", "den", "wird", "erst", "T\u00f6f\u00b7fel", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "$.", "ART", "VAFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O nein, er heult: ich will's der Mutter sagen.", "tokens": ["O", "nein", ",", "er", "heult", ":", "ich", "will's", "der", "Mut\u00b7ter", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}