{"dta.poem.1437": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sarch-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was hab ich hier vor mir? Di\u00df ist mein leztes Hau\u00df.", "tokens": ["Was", "hab", "ich", "hier", "vor", "mir", "?", "Di\u00df", "ist", "mein", "lez\u00b7tes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$.", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwar einen engen Platz bedeckt das schmale Dach/", "tokens": ["Zwar", "ei\u00b7nen", "en\u00b7gen", "Platz", "be\u00b7deckt", "das", "schma\u00b7le", "Dach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch Raum genung f\u00fcr mich: Di\u00df ist mein Schlaff-Ge-", "tokens": ["Doch", "Raum", "ge\u00b7nung", "f\u00fcr", "mich", ":", "Di\u00df", "ist", "mein", "Schlaff\u00b7Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "APPR", "PPER", "$.", "PDS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier ruh\u2019 ich f\u00fcr dem Sturm der Nord-Lufft sicher aus.", "tokens": ["Hier", "ruh'", "ich", "f\u00fcr", "dem", "Sturm", "der", "Nord\u00b7Lufft", "si\u00b7cher", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich st\u00f6rt kein b\u00f6ser Traum/ mein K\u00fcssen ist der Span/", "tokens": ["Mich", "st\u00f6rt", "kein", "b\u00f6\u00b7ser", "Traum", "/", "mein", "K\u00fcs\u00b7sen", "ist", "der", "Span", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo aller Span und Streit der Welt ist abgethan.", "tokens": ["Wo", "al\u00b7ler", "Span", "und", "Streit", "der", "Welt", "ist", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "KON", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Di\u00df ist mein Schnecken-Hau\u00df und sichrer Auffenthalt/", "tokens": ["Di\u00df", "ist", "mein", "Schne\u00b7cken\u00b7Hau\u00df", "und", "sich\u00b7rer", "Auf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Erd und Himmel z\u00fcrnt/ wenn Blitz und Donner", "tokens": ["Wenn", "Erd", "und", "Him\u00b7mel", "z\u00fcrnt", "/", "wenn", "Blitz", "und", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$(", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Di\u00df ist mein Garten-Saal; wenns Schnee und Hagel streut/", "tokens": ["Di\u00df", "ist", "mein", "Gar\u00b7ten\u00b7Saal", ";", "wenns", "Schnee", "und", "Ha\u00b7gel", "streut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bin ich wohl bedeckt; bin ich erbla\u00dft und kalt/", "tokens": ["So", "bin", "ich", "wohl", "be\u00b7deckt", ";", "bin", "ich", "er\u00b7bla\u00dft", "und", "kalt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$.", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie bald werd ich auffs neu in stetem Sommer bl\u00fchn/", "tokens": ["Wie", "bald", "werd", "ich", "auffs", "neu", "in", "ste\u00b7tem", "Som\u00b7mer", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPRART", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als reine Lilien und zarter Gelsamin.", "tokens": ["Als", "rei\u00b7ne", "Li\u00b7li\u00b7en", "und", "zar\u00b7ter", "Gel\u00b7sa\u00b7min", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Di\u00df ist der Nachen/ der zur Gl\u00fcckes-Insul l\u00e4ufft/", "tokens": ["Di\u00df", "ist", "der", "Na\u00b7chen", "/", "der", "zur", "Gl\u00fc\u00b7ckes\u00b7In\u00b7sul", "l\u00e4ufft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Br\u00fccke/ drauff ich kan mit sichrem Fusse stehn/", "tokens": ["Die", "Br\u00fc\u00b7cke", "/", "drauff", "ich", "kan", "mit", "sich\u00b7rem", "Fus\u00b7se", "stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PAV", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Pforte/ da ich durch ins Paradi\u00df soll gehn/", "tokens": ["Die", "Pfor\u00b7te", "/", "da", "ich", "durch", "ins", "Pa\u00b7ra\u00b7di\u00df", "soll", "gehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "APPR", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df ist der Kasten/ den die S\u00fcndflutt nicht ers\u00e4usst/", "tokens": ["Di\u00df", "ist", "der", "Kas\u00b7ten", "/", "den", "die", "S\u00fcnd\u00b7flutt", "nicht", "er\u00b7s\u00e4usst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein JEsus schleust nach mir die Th\u00fcre selber zu/", "tokens": ["Mein", "Je\u00b7sus", "schleust", "nach", "mir", "die", "Th\u00fc\u00b7re", "sel\u00b7ber", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und weckt mich/ wenn es Zeit/ zur Himmels-Lust und Ruh!", "tokens": ["Und", "weckt", "mich", "/", "wenn", "es", "Zeit", "/", "zur", "Him\u00b7mels\u00b7Lust", "und", "Ruh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "NN", "$(", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}