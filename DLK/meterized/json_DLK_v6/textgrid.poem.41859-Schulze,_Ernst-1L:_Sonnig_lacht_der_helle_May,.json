{"textgrid.poem.41859": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sonnig lacht der helle May,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonnig lacht der helle May,", "tokens": ["Son\u00b7nig", "lacht", "der", "hel\u00b7le", "May", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fcchtig rinnt der Quell vorbey,", "tokens": ["Fl\u00fcch\u00b7tig", "rinnt", "der", "Quell", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von Blumen prangt die Weide;", "tokens": ["Und", "von", "Blu\u00b7men", "prangt", "die", "Wei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueppig bl\u00fcht die ein' empor,", "tokens": ["Uep\u00b7pig", "bl\u00fcht", "die", "ein'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die andre schaut hervor", "tokens": ["Und", "die", "and\u00b7re", "schaut", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIS", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00fcchtern aus dem gr\u00fcnen Kleide:", "tokens": ["Sch\u00fcch\u00b7tern", "aus", "dem", "gr\u00fc\u00b7nen", "Klei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also sendet mild und rein", "tokens": ["Al\u00b7so", "sen\u00b7det", "mild", "und", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren lauen Sonnenschein", "tokens": ["Ih\u00b7ren", "lau\u00b7en", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In das Herz die Liebe nieder,", "tokens": ["In", "das", "Herz", "die", "Lie\u00b7be", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in sehnsuchtsvoller Brust", "tokens": ["Und", "in", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wechseln fl\u00fcchtig Leid und Lust,", "tokens": ["Wech\u00b7seln", "fl\u00fcch\u00b7tig", "Leid", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig keimen neue Lieder.", "tokens": ["E\u00b7wig", "kei\u00b7men", "neu\u00b7e", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welkt, ihr Blumen, gern dahin,", "tokens": ["Welkt", ",", "ihr", "Blu\u00b7men", ",", "gern", "da\u00b7hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn euch gr\u00fc\u00dft die Sch\u00e4ferin", "tokens": ["Denn", "euch", "gr\u00fc\u00dft", "die", "Sch\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich im Vor\u00fcberwallen.", "tokens": ["Freund\u00b7lich", "im", "Vor\u00b7\u00fc\u00b7berw\u00b7al\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieder, fordert keinen Dank,", "tokens": ["Lie\u00b7der", ",", "for\u00b7dert", "kei\u00b7nen", "Dank", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn auch nur Minuten lang", "tokens": ["Wenn", "auch", "nur", "Mi\u00b7nu\u00b7ten", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Liebsten ihr gefallen.", "tokens": ["Mei\u00b7ner", "Liebs\u00b7ten", "ihr", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}