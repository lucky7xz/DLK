{"textgrid.poem.64860": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, glaube nicht, da\u00df du mein Herz geknickt", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, glaube nicht, da\u00df du mein Herz geknickt", "tokens": ["Nein", ",", "glau\u00b7be", "nicht", ",", "da\u00df", "du", "mein", "Herz", "ge\u00b7knickt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit deines Zornes wilden Donnerschl\u00e4gen;", "tokens": ["Mit", "dei\u00b7nes", "Zor\u00b7nes", "wil\u00b7den", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es pocht so stolz, wie stolz mein Auge blickt!", "tokens": ["Es", "pocht", "so", "stolz", ",", "wie", "stolz", "mein", "Au\u00b7ge", "blickt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Dichterherzen ist das Ungl\u00fcck Segen.", "tokens": ["Dem", "Dich\u00b7ter\u00b7her\u00b7zen", "ist", "das", "Un\u00b7gl\u00fcck", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur des gemeinen Lebens dumpfer Harm", "tokens": ["Nur", "des", "ge\u00b7mei\u00b7nen", "Le\u00b7bens", "dum\u00b7pfer", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kann schn\u00f6d' entkr\u00e4ftend an der Seele nagen,", "tokens": ["Kann", "schn\u00f6d'", "ent\u00b7kr\u00e4f\u00b7tend", "an", "der", "See\u00b7le", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch eines gro\u00dfen Schmerzes starker Arm", "tokens": ["Doch", "ei\u00b7nes", "gro\u00b7\u00dfen", "Schmer\u00b7zes", "star\u00b7ker", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird in die Reih'n der Himmlischen sie tragen.", "tokens": ["Wird", "in", "die", "Reih'n", "der", "Himm\u00b7li\u00b7schen", "sie", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nie f\u00fchlt' ich mich in fr\u00fch'rer Wonnezeit", "tokens": ["Nie", "f\u00fchlt'", "ich", "mich", "in", "fr\u00fch'\u00b7rer", "Won\u00b7ne\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ganz entr\u00fcckt den niedrigen Bereichen,", "tokens": ["So", "ganz", "ent\u00b7r\u00fcckt", "den", "nied\u00b7ri\u00b7gen", "Be\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als jetzt, nachdem das Ungl\u00fcck mich geweiht", "tokens": ["Als", "jetzt", ",", "nach\u00b7dem", "das", "Un\u00b7gl\u00fcck", "mich", "ge\u00b7weiht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu einer Schmerzenhoheit sonder Gleichen.", "tokens": ["Zu", "ei\u00b7ner", "Schmer\u00b7zen\u00b7ho\u00b7heit", "son\u00b7der", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein einsam Herz thront k\u00f6niglich und hehr,", "tokens": ["Mein", "ein\u00b7sam", "Herz", "thront", "k\u00f6\u00b7nig\u00b7lich", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stolz von dem Rest der Menschen abgeschnitten;", "tokens": ["Stolz", "von", "dem", "Rest", "der", "Men\u00b7schen", "ab\u00b7ge\u00b7schnit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "F\u00fcr meines Gleichen acht' ich sie nicht mehr,", "tokens": ["F\u00fcr", "mei\u00b7nes", "Glei\u00b7chen", "acht'", "ich", "sie", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn Keiner hat so viel wie ich gelitten.", "tokens": ["Denn", "Kei\u00b7ner", "hat", "so", "viel", "wie", "ich", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "PIAT", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}