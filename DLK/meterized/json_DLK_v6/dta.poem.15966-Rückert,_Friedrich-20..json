{"dta.poem.15966": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Zweifel treibt dich an, der Zweifel macht dich stocken,", "tokens": ["Der", "Zwei\u00b7fel", "treibt", "dich", "an", ",", "der", "Zwei\u00b7fel", "macht", "dich", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er dient zu hemmen dich, und vorw\u00e4rts dich zu locken.", "tokens": ["Er", "dient", "zu", "hem\u00b7men", "dich", ",", "und", "vor\u00b7w\u00e4rts", "dich", "zu", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVFIN", "PPER", "$,", "KON", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der vorw\u00e4rts treibende nie ruhende ist gut,", "tokens": ["Der", "vor\u00b7w\u00e4rts", "trei\u00b7ben\u00b7de", "nie", "ru\u00b7hen\u00b7de", "ist", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "ADJA", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schlimm ist der stockende verstockte Zweifelmut.", "tokens": ["Schlimm", "ist", "der", "sto\u00b7cken\u00b7de", "ver\u00b7stock\u00b7te", "Zwei\u00b7fel\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df etwas Gutes sei und Sch\u00f6nes zu erstreben,", "tokens": ["Da\u00df", "et\u00b7was", "Gu\u00b7tes", "sei", "und", "Sch\u00f6\u00b7nes", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Guten wird darob kein Zweifel sich erheben.", "tokens": ["Dem", "Gu\u00b7ten", "wird", "da\u00b7rob", "kein", "Zwei\u00b7fel", "sich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df etwas Gutes schon erstrebt und Sch\u00f6nes sei,", "tokens": ["Da\u00df", "et\u00b7was", "Gu\u00b7tes", "schon", "er\u00b7strebt", "und", "Sch\u00f6\u00b7nes", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVPP", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Besten wohnt darob der gr\u00f6\u00dfte Zweifel bei.", "tokens": ["Dem", "Bes\u00b7ten", "wohnt", "da\u00b7rob", "der", "gr\u00f6\u00df\u00b7te", "Zwei\u00b7fel", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zu immer h\u00f6hern H\u00f6hn giebt dir der Zweifel Schwung,", "tokens": ["Zu", "im\u00b7mer", "h\u00f6\u00b7hern", "H\u00f6hn", "giebt", "dir", "der", "Zwei\u00b7fel", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch in den Abgrund st\u00fcrzt dich die Verzweifelung.", "tokens": ["Doch", "in", "den", "Ab\u00b7grund", "st\u00fcrzt", "dich", "die", "Ver\u00b7zwei\u00b7fe\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}