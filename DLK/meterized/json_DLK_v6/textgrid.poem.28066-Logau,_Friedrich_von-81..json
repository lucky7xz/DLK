{"textgrid.poem.28066": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ware Schand, ob wo au\u00df Assurs geilen H\u00e4nden", "tokens": ["Es", "wa\u00b7re", "Schand", ",", "ob", "wo", "au\u00df", "As\u00b7surs", "gei\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "KOUS", "PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein sch\u00f6nes Weib kam weg mit Ehren ohne sch\u00e4nden.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Weib", "kam", "weg", "mit", "Eh\u00b7ren", "oh\u00b7ne", "sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist noch heute Brauch, da\u00df der zu Schanden k\u00fcmmt,", "tokens": ["Es", "ist", "noch", "heu\u00b7te", "Brauch", ",", "da\u00df", "der", "zu", "Schan\u00b7den", "k\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$,", "KOUS", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich zu Ehren h\u00e4lt und nicht zu Schanden stimmt.", "tokens": ["Der", "sich", "zu", "Eh\u00b7ren", "h\u00e4lt", "und", "nicht", "zu", "Schan\u00b7den", "stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}