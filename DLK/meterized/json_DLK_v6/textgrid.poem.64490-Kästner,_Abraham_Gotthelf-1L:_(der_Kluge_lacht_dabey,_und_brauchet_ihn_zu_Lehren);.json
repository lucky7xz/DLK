{"textgrid.poem.64490": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: (der Kluge lacht dabey, und brauchet ihn zu Lehren);", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "(der Kluge lacht dabey, und brauchet ihn zu Lehren);", "tokens": ["(", "der", "Klu\u00b7ge", "lacht", "da\u00b7bey", ",", "und", "brau\u00b7chet", "ihn", "zu", "Leh\u00b7ren", ")", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PAV", "$,", "KON", "VVFIN", "PPER", "APPR", "NN", "$(", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch den, als er an eines Teiches Rand", "tokens": ["Auch", "den", ",", "als", "er", "an", "ei\u00b7nes", "Tei\u00b7ches", "Rand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey muntern, jungen G\u00e4nschen stand:", "tokens": ["Bey", "mun\u00b7tern", ",", "jun\u00b7gen", "G\u00e4n\u00b7schen", "stand", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbihr lieben Dingerchen, jetzt seyd ihr artig, klein,", "tokens": ["\u00bb", "ihr", "lie\u00b7ben", "Din\u00b7ge\u00b7rchen", ",", "jetzt", "seyd", "ihr", "ar\u00b7tig", ",", "klein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald werdet ihr nur gro\u00dfe G\u00e4nse seyn.\u00ab", "tokens": ["Bald", "wer\u00b7det", "ihr", "nur", "gro\u00b7\u00dfe", "G\u00e4n\u00b7se", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn ich mit kleinen M\u00e4dchen spielte.", "tokens": ["Wenn", "ich", "mit", "klei\u00b7nen", "M\u00e4d\u00b7chen", "spiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}