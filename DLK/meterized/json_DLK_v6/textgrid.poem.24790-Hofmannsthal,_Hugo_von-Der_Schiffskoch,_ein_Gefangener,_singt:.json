{"textgrid.poem.24790": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schiffskoch, ein Gefangener, singt:", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh, geschieden von den Meinigen,", "tokens": ["Weh", ",", "ge\u00b7schie\u00b7den", "von", "den", "Mei\u00b7ni\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "ART", "PPOSS", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Lieg ich hier seit vielen Wochen;", "tokens": ["Lieg", "ich", "hier", "seit", "vie\u00b7len", "Wo\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach und denen, die mich peinigen,", "tokens": ["Ach", "und", "de\u00b7nen", ",", "die", "mich", "pei\u00b7ni\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PDS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mu\u00df ich Mahl- um Mahlzeit kochen.", "tokens": ["Mu\u00df", "ich", "Mahl", "um", "Mahl\u00b7zeit", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "TRUNC", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne purpurflossige Fische,", "tokens": ["Sch\u00f6\u00b7ne", "pur\u00b7pur\u00b7flos\u00b7si\u00b7ge", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die sie mir lebendig brachten,", "tokens": ["Die", "sie", "mir", "le\u00b7ben\u00b7dig", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schauen aus gebrochenen Augen,", "tokens": ["Schau\u00b7en", "aus", "ge\u00b7bro\u00b7che\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sanfte Tiere mu\u00df ich schlachten.", "tokens": ["Sanf\u00b7te", "Tie\u00b7re", "mu\u00df", "ich", "schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stille Tiere mu\u00df ich schlachten,", "tokens": ["Stil\u00b7le", "Tie\u00b7re", "mu\u00df", "ich", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Fr\u00fcchte mu\u00df ich sch\u00e4len", "tokens": ["Sch\u00f6\u00b7ne", "Fr\u00fcch\u00b7te", "mu\u00df", "ich", "sch\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und f\u00fcr sie, die mich verachten,", "tokens": ["Und", "f\u00fcr", "sie", ",", "die", "mich", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Feurige Gew\u00fcrze w\u00e4hlen.", "tokens": ["Feu\u00b7ri\u00b7ge", "Ge\u00b7w\u00fcr\u00b7ze", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie ich gebeugt beim Licht in", "tokens": ["Und", "wie", "ich", "ge\u00b7beugt", "beim", "Licht", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVPP", "APPRART", "NN", "APPR"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "S\u00fc\u00df- und scharfen D\u00fcften w\u00fchle,", "tokens": ["S\u00fc\u00df", "und", "schar\u00b7fen", "D\u00fcf\u00b7ten", "w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigen auf ins Herz der Freiheit", "tokens": ["Stei\u00b7gen", "auf", "ins", "Herz", "der", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungeheuere Gef\u00fchle!", "tokens": ["Un\u00b7ge\u00b7heu\u00b7e\u00b7re", "Ge\u00b7f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Weh, geschieden von den Meinigen,", "tokens": ["Weh", ",", "ge\u00b7schie\u00b7den", "von", "den", "Mei\u00b7ni\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "ART", "PPOSS", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Lieg ich hier seit wieviel Wochen!", "tokens": ["Lieg", "ich", "hier", "seit", "wie\u00b7viel", "Wo\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "Ach und denen, die mich peinigen,", "tokens": ["Ach", "und", "de\u00b7nen", ",", "die", "mich", "pei\u00b7ni\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PDS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mu\u00df ich Mahl- um Mahlzeit kochen!", "tokens": ["Mu\u00df", "ich", "Mahl", "um", "Mahl\u00b7zeit", "ko\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "TRUNC", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}