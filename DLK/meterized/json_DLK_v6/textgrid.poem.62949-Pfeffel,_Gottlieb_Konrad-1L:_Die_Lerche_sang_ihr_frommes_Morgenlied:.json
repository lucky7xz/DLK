{"textgrid.poem.62949": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lerche sang ihr frommes Morgenlied:", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche sang ihr frommes Morgenlied:", "tokens": ["Die", "Ler\u00b7che", "sang", "ihr", "from\u00b7mes", "Mor\u00b7gen\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Weihrauch flog es durch die L\u00fcfte", "tokens": ["Wie", "Weih\u00b7rauch", "flog", "es", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis zum saphirnen Thron, an dem das erste Glied", "tokens": ["Bis", "zum", "sa\u00b7phir\u00b7nen", "Thron", ",", "an", "dem", "das", "ers\u00b7te", "Glied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wesenkette h\u00e4ngt. Die schwarzen Kl\u00fcfte", "tokens": ["Der", "We\u00b7sen\u00b7ket\u00b7te", "h\u00e4ngt", ".", "Die", "schwar\u00b7zen", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Felsen hallten es zur\u00fcck.", "tokens": ["Der", "Fel\u00b7sen", "hall\u00b7ten", "es", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Rohrspatz, welchen die Musik", "tokens": ["Ein", "Rohrs\u00b7patz", ",", "wel\u00b7chen", "die", "Mu\u00b7sik"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus seinem tr\u00e4gen Schlummer weckte,", "tokens": ["Aus", "sei\u00b7nem", "tr\u00e4\u00b7gen", "Schlum\u00b7mer", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fuhr auf: verw\u00fcnschte Muckerin,", "tokens": ["Fuhr", "auf", ":", "ver\u00b7w\u00fcnschte", "Mu\u00b7cke\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ich wollte, da sie flugs verreckte!", "tokens": ["Ich", "woll\u00b7te", ",", "da", "sie", "flugs", "ver\u00b7reck\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So fluchet er. Die Lerche h\u00f6ret ihn", "tokens": ["So", "flu\u00b7chet", "er", ".", "Die", "Ler\u00b7che", "h\u00f6\u00b7ret", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und l\u00e4\u00dft den dummen L\u00e4strer toben;", "tokens": ["Und", "l\u00e4\u00dft", "den", "dum\u00b7men", "L\u00e4st\u00b7rer", "to\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie schwingt sich in des Aethers lichte Flur", "tokens": ["Sie", "schwingt", "sich", "in", "des", "A\u00b7e\u00b7thers", "lich\u00b7te", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und ruft im Fliehn: vergieb, o Vater der Natur!", "tokens": ["Und", "ruft", "im", "Fliehn", ":", "ver\u00b7gieb", ",", "o", "Va\u00b7ter", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "VVFIN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hier war es S\u00fcnde, dich zu loben.", "tokens": ["Hier", "war", "es", "S\u00fcn\u00b7de", ",", "dich", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}