{"textgrid.poem.38694": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Andenken", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich denke dein,", "tokens": ["Ich", "den\u00b7ke", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wann durch den Hain", "tokens": ["Wann", "durch", "den", "Hain"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Nachtigallen", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Akkorde schallen.", "tokens": ["Ak\u00b7kor\u00b7de", "schal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wann denkst du mein?", "tokens": ["Wann", "denkst", "du", "mein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich denke dein,", "tokens": ["Ich", "den\u00b7ke", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im D\u00e4mmerschein", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Abendhelle", "tokens": ["Der", "A\u00b7bend\u00b7hel\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text