{"textgrid.poem.34177": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Selbstvernichtung in der Selbsterhaltung", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du verl\u00e4ugnest dich selbst? Warum denn? Ich will mich behaupten,", "tokens": ["Du", "ver\u00b7l\u00e4ug\u00b7nest", "dich", "selbst", "?", "Wa\u00b7rum", "denn", "?", "Ich", "will", "mich", "be\u00b7haup\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PWAV", "ADV", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und man duldet mich nicht, zeig' ich mein wahres Gesicht!", "tokens": ["Und", "man", "dul\u00b7det", "mich", "nicht", ",", "zeig'", "ich", "mein", "wah\u00b7res", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber, behauptest du dich, indem du dich heuchelnd vernichtest?", "tokens": ["A\u00b7ber", ",", "be\u00b7haup\u00b7test", "du", "dich", ",", "in\u00b7dem", "du", "dich", "heu\u00b7chelnd", "ver\u00b7nich\u00b7test", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Lebst du noch selber? Es spukt dann ja ein Schatten f\u00fcr dich!", "tokens": ["Lebst", "du", "noch", "sel\u00b7ber", "?", "Es", "spukt", "dann", "ja", "ein", "Schat\u00b7ten", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}