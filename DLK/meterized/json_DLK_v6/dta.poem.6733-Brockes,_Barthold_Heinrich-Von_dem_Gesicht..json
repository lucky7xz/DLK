{"dta.poem.6733": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als zarte F\u00e4den, d\u00fcnne D\u00fcnste,", "tokens": ["Als", "zar\u00b7te", "F\u00e4\u00b7den", ",", "d\u00fcn\u00b7ne", "D\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auswertig zarte Rind\u2019, unsichtbare Gespinste,", "tokens": ["Aus\u00b7wer\u00b7tig", "zar\u00b7te", "Rind'", ",", "un\u00b7sicht\u00b7ba\u00b7re", "Ge\u00b7spins\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die T\u00f6ne, Licht und Farben uns formiren,", "tokens": ["Die", "T\u00f6\u00b7ne", ",", "Licht", "und", "Far\u00b7ben", "uns", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dadurch, da\u00df sie ohn\u2019 da\u00df wir es versp\u00fcren,", "tokens": ["Da\u00b7durch", ",", "da\u00df", "sie", "ohn'", "da\u00df", "wir", "es", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Das Werckzeug blo\u00df allein ber\u00fchren.", "tokens": ["Das", "Wer\u00b7ck\u00b7zeug", "blo\u00df", "al\u00b7lein", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}}}}}