{"textgrid.poem.57515": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, unser Vater stirbt! Es sinket Haupt und Herz;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, unser Vater stirbt! Es sinket Haupt und Herz;", "tokens": ["Ach", ",", "un\u00b7ser", "Va\u00b7ter", "stirbt", "!", "Es", "sin\u00b7ket", "Haupt", "und", "Herz", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Glieder beben uns; o ungemeiner Schmerz!", "tokens": ["Die", "Glie\u00b7der", "be\u00b7ben", "uns", ";", "o", "un\u00b7ge\u00b7mei\u00b7ner", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verh\u00e4ngni\u00df! halte still mit deinen Donnerschl\u00e4gen;", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df", "!", "hal\u00b7te", "still", "mit", "dei\u00b7nen", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie sind f\u00fcr uns zu hart, o Herr! la\u00df dich bewegen!", "tokens": ["Sie", "sind", "f\u00fcr", "uns", "zu", "hart", ",", "o", "Herr", "!", "la\u00df", "dich", "be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "$,", "FM", "NN", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch du h\u00f6rest nicht. Man \u00f6ffnet schon das Grab;", "tokens": ["Je\u00b7doch", "du", "h\u00f6\u00b7rest", "nicht", ".", "Man", "\u00f6ff\u00b7net", "schon", "das", "Grab", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$.", "PIS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man senkt des Vaters Leib und unsre Lust hinab.", "tokens": ["Man", "senkt", "des", "Va\u00b7ters", "Leib", "und", "uns\u00b7re", "Lust", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O st\u00fcrben wir mit ihm! so d\u00fcrften unsre Klagen", "tokens": ["O", "st\u00fcr\u00b7ben", "wir", "mit", "ihm", "!", "so", "d\u00fcrf\u00b7ten", "uns\u00b7re", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "$.", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihn nicht voll Gram und Leid und Schmerz zu Grabe tragen.", "tokens": ["Ihn", "nicht", "voll", "Gram", "und", "Leid", "und", "Schmerz", "zu", "Gra\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "NN", "KON", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O z\u00fcrnendes Geschick! warum bist du entbrannt?", "tokens": ["O", "z\u00fcr\u00b7nen\u00b7des", "Ge\u00b7schick", "!", "wa\u00b7rum", "bist", "du", "ent\u00b7brannt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und warum rei\u00dfest du, mit zornerf\u00fcllter Hand,", "tokens": ["Und", "wa\u00b7rum", "rei\u00b7\u00dfest", "du", ",", "mit", "zor\u00b7ner\u00b7f\u00fcll\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht solche von der Welt, die durch verderbte Sitten,", "tokens": ["Nicht", "sol\u00b7che", "von", "der", "Welt", ",", "die", "durch", "ver\u00b7derb\u00b7te", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fast st\u00fcndlich Pflicht, Vernunft und Tugend \u00fcberschritten?", "tokens": ["Fast", "st\u00fcnd\u00b7lich", "Pflicht", ",", "Ver\u00b7nunft", "und", "Tu\u00b7gend", "\u00fc\u00b7bersc\u00b7hrit\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Gott ein Scheusal sind; die nur der Welt zur Qual,", "tokens": ["Die", "Gott", "ein", "Scheu\u00b7sal", "sind", ";", "die", "nur", "der", "Welt", "zur", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$.", "ART", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Frommen zum Verdru\u00df, und ihrer Kinder Zahl", "tokens": ["Den", "From\u00b7men", "zum", "Ver\u00b7dru\u00df", ",", "und", "ih\u00b7rer", "Kin\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur zum Verderb gelebt: indem ihr b\u00f6ses Leben", "tokens": ["Nur", "zum", "Ver\u00b7derb", "ge\u00b7lebt", ":", "in\u00b7dem", "ihr", "b\u00f6\u00b7ses", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$.", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein allzudeutlich Bild der Bosheit abgegeben.", "tokens": ["Ein", "all\u00b7zu\u00b7deut\u00b7lich", "Bild", "der", "Bos\u00b7heit", "ab\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum stirbt dieses Haupt, des Adels Schmuck und Ruhm?", "tokens": ["Wa\u00b7rum", "stirbt", "die\u00b7ses", "Haupt", ",", "des", "A\u00b7dels", "Schmuck", "und", "Ruhm", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum stirbt diese Brust, der Tugend Eigenthum?", "tokens": ["Wa\u00b7rum", "stirbt", "die\u00b7se", "Brust", ",", "der", "Tu\u00b7gend", "Ei\u00b7gen\u00b7thum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warum weicht dieser Geist, ein Muster des Verstandes?", "tokens": ["Wa\u00b7rum", "weicht", "die\u00b7ser", "Geist", ",", "ein", "Mus\u00b7ter", "des", "Ver\u00b7stan\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum stirbt ", "tokens": ["Wa\u00b7rum", "stirbt"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der Vater, der sein Haus mit solchem Witz regiert,", "tokens": ["Der", "Va\u00b7ter", ",", "der", "sein", "Haus", "mit", "sol\u00b7chem", "Witz", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der uns mit Wort und That zur Tugend angef\u00fchrt!", "tokens": ["Der", "uns", "mit", "Wort", "und", "That", "zur", "Tu\u00b7gend", "an\u00b7ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Warum wird solch ein Mann, den alle preisen m\u00fcssen,", "tokens": ["Wa\u00b7rum", "wird", "solch", "ein", "Mann", ",", "den", "al\u00b7le", "prei\u00b7sen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIAT", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So fr\u00fch, so unverhofft der Unterwelt entrissen?", "tokens": ["So", "fr\u00fch", ",", "so", "un\u00b7ver\u00b7hofft", "der", "Un\u00b7ter\u00b7welt", "ent\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Tag! der unsre Lust, als d\u00fcnnes Glas, zerschl\u00e4gt;", "tokens": ["O", "Tag", "!", "der", "uns\u00b7re", "Lust", ",", "als", "d\u00fcn\u00b7nes", "Glas", ",", "zer\u00b7schl\u00e4gt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "PPOSAT", "NN", "$,", "KOUS", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Glockenschlag! der mehr ein kindlich Ohr bewegt,", "tokens": ["O", "Glo\u00b7cken\u00b7schlag", "!", "der", "mehr", "ein", "kind\u00b7lich", "Ohr", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ADV", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als wenn ein starker Knall aus finstern Wolken br\u00fcllet,", "tokens": ["Als", "wenn", "ein", "star\u00b7ker", "Knall", "aus", "fins\u00b7tern", "Wol\u00b7ken", "br\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Stadt, das Feld, den Wald mit Bangigkeit erf\u00fcllet;", "tokens": ["Die", "Stadt", ",", "das", "Feld", ",", "den", "Wald", "mit", "Ban\u00b7gig\u00b7keit", "er\u00b7f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja Th\u00fcrme niederwirft und Eichen niederschmei\u00dft,", "tokens": ["Ja", "Th\u00fcr\u00b7me", "nie\u00b7der\u00b7wirft", "und", "Ei\u00b7chen", "nie\u00b7der\u00b7schmei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den allerst\u00e4rksten Stamm aus seinen Wurzeln rei\u00dft,", "tokens": ["Den", "al\u00b7ler\u00b7st\u00e4rks\u00b7ten", "Stamm", "aus", "sei\u00b7nen", "Wur\u00b7zeln", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den dicken Strumpf zermalmt, die festen Aeste splittert,", "tokens": ["Den", "di\u00b7cken", "Strumpf", "zer\u00b7malmt", ",", "die", "fes\u00b7ten", "A\u00b7es\u00b7te", "split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und durch sein Krachen macht, da\u00df Grund und Boden sch\u00fcttert.", "tokens": ["Und", "durch", "sein", "Kra\u00b7chen", "macht", ",", "da\u00df", "Grund", "und", "Bo\u00b7den", "sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Geliebtes Vaterherz! wir schauen noch zur\u00fcck:", "tokens": ["Ge\u00b7lieb\u00b7tes", "Va\u00b7ter\u00b7herz", "!", "wir", "schau\u00b7en", "noch", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie treulich sorgtest du f\u00fcr deiner Kinder Gl\u00fcck!", "tokens": ["Wie", "treu\u00b7lich", "sorg\u00b7test", "du", "f\u00fcr", "dei\u00b7ner", "Kin\u00b7der", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir sehn dich itzo noch, mit ernstlichem Bem\u00fchen", "tokens": ["Wir", "sehn", "dich", "it\u00b7zo", "noch", ",", "mit", "ernst\u00b7li\u00b7chem", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und v\u00e4terlicher Zucht, uns zu der Weisheit ziehen.", "tokens": ["Und", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "Zucht", ",", "uns", "zu", "der", "Weis\u00b7heit", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du sollst uns k\u00fcnftig auch, durch deiner Tugend Schein,", "tokens": ["Du", "sollst", "uns", "k\u00fcnf\u00b7tig", "auch", ",", "durch", "dei\u00b7ner", "Tu\u00b7gend", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Leitstern auf der Bahn zum wahren Lobe seyn;", "tokens": ["Ein", "Leits\u00b7tern", "auf", "der", "Bahn", "zum", "wah\u00b7ren", "Lo\u00b7be", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Beyspiel soll uns noch, zur Folge deiner Ahnen,", "tokens": ["Dein", "Bey\u00b7spiel", "soll", "uns", "noch", ",", "zur", "Fol\u00b7ge", "dei\u00b7ner", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch da das Grab dich deckt, die rechten Wege bahnen.", "tokens": ["Auch", "da", "das", "Grab", "dich", "deckt", ",", "die", "rech\u00b7ten", "We\u00b7ge", "bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie w\u00fcrdig hat dich sonst der Hauptmannsstab geschm\u00fcckt;", "tokens": ["Wie", "w\u00fcr\u00b7dig", "hat", "dich", "sonst", "der", "Haupt\u00b7manns\u00b7stab", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie tapfer hat dein Arm das scharfe Schwert gez\u00fcckt,", "tokens": ["Wie", "tap\u00b7fer", "hat", "dein", "Arm", "das", "schar\u00b7fe", "Schwert", "ge\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Vaterland besch\u00fctzt, dem F\u00fcrsten treu gedienet,", "tokens": ["Das", "Va\u00b7ter\u00b7land", "be\u00b7sch\u00fctzt", ",", "dem", "F\u00fcrs\u00b7ten", "treu", "ge\u00b7die\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Billigkeit zu trotz, sich keiner That erk\u00fchnet!", "tokens": ["Der", "Bil\u00b7lig\u00b7keit", "zu", "trotz", ",", "sich", "kei\u00b7ner", "That", "er\u00b7k\u00fch\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nie Grausamkeit und Wuth in Kriegeszeit ver\u00fcbt;", "tokens": ["Nie", "Grau\u00b7sam\u00b7keit", "und", "Wuth", "in", "Krie\u00b7ges\u00b7zeit", "ver\u00b7\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch mitten in der Schlacht Erbarmen ausge\u00fcbt;", "tokens": ["Auch", "mit\u00b7ten", "in", "der", "Schlacht", "Er\u00b7bar\u00b7men", "aus\u00b7ge\u00b7\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und nicht, wie Barbarn thun, durch Eisen, Brand und Morden,", "tokens": ["Und", "nicht", ",", "wie", "Bar\u00b7barn", "thun", ",", "durch", "Ei\u00b7sen", ",", "Brand", "und", "Mor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "NN", "VVINF", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ergrimmten B\u00e4ren zwar, nicht Helden gleich geworden.", "tokens": ["Er\u00b7grimm\u00b7ten", "B\u00e4\u00b7ren", "zwar", ",", "nicht", "Hel\u00b7den", "gleich", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PTKNEG", "NN", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wie ein junger Baum, der keinen G\u00e4rtner hat,", "tokens": ["So", "wie", "ein", "jun\u00b7ger", "Baum", ",", "der", "kei\u00b7nen", "G\u00e4rt\u00b7ner", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ohn alle Wartung steht; krumm, ungestalt und matt,", "tokens": ["Ohn", "al\u00b7le", "War\u00b7tung", "steht", ";", "krumm", ",", "un\u00b7ge\u00b7stalt", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$.", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verwildert und verwirrt den schwachen Gipfel neiget,", "tokens": ["Ver\u00b7wil\u00b7dert", "und", "ver\u00b7wirrt", "den", "schwa\u00b7chen", "Gip\u00b7fel", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil ihn ein jeder Wind bis an die Erde beuget;", "tokens": ["Weil", "ihn", "ein", "je\u00b7der", "Wind", "bis", "an", "die", "Er\u00b7de", "beu\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wo er im Kothe liegt, verfaulet und verdirbt,", "tokens": ["Wo", "er", "im", "Ko\u00b7the", "liegt", ",", "ver\u00b7fau\u00b7let", "und", "ver\u00b7dirbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn Rinde, Saft und Mark vor Feuchtigkeit erstirbt:", "tokens": ["Wenn", "Rin\u00b7de", ",", "Saft", "und", "Mark", "vor", "Feuch\u00b7tig\u00b7keit", "er\u00b7stirbt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So kann es uns ergehn, da wir best\u00fcrzt empfunden,", "tokens": ["So", "kann", "es", "uns", "er\u00b7gehn", ",", "da", "wir", "be\u00b7st\u00fcrzt", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch diesen Todesfall sey unsre Zucht verschwunden.", "tokens": ["Durch", "die\u00b7sen", "To\u00b7des\u00b7fall", "sey", "uns\u00b7re", "Zucht", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ihr Freunde! nehmt euch doch verla\u00dfner Weysen an,", "tokens": ["Ihr", "Freun\u00b7de", "!", "nehmt", "euch", "doch", "ver\u00b7la\u00df\u00b7ner", "Wey\u00b7sen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ersetzet, was uns fehlt, thut das, was der gethan,", "tokens": ["Er\u00b7set\u00b7zet", ",", "was", "uns", "fehlt", ",", "thut", "das", ",", "was", "der", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PDS", "$,", "PRELS", "ART", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der uns so weit gebracht; und zeigt an eurer G\u00fcte,", "tokens": ["Der", "uns", "so", "weit", "ge\u00b7bracht", ";", "und", "zeigt", "an", "eu\u00b7rer", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$.", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo nicht ein v\u00e4terlich, doch ein geneigt Gem\u00fcthe.", "tokens": ["Wo", "nicht", "ein", "v\u00e4\u00b7ter\u00b7lich", ",", "doch", "ein", "ge\u00b7neigt", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "ADJD", "$,", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und, du Allm\u00e4chtiger! der du der Weysen pflegst,", "tokens": ["Und", ",", "du", "All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "!", "der", "du", "der", "Wey\u00b7sen", "pflegst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "NN", "$.", "ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie zarten M\u00fcttern gleich, in Liebesarmen tr\u00e4gst,", "tokens": ["Sie", "zar\u00b7ten", "M\u00fct\u00b7tern", "gleich", ",", "in", "Lie\u00b7be\u00b7sar\u00b7men", "tr\u00e4gst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Entzeuch uns nicht die Huld! Denn wo wir die nur haben:", "tokens": ["Ent\u00b7zeuch", "uns", "nicht", "die", "Huld", "!", "Denn", "wo", "wir", "die", "nur", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "$.", "KON", "PWAV", "PPER", "ART", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ists, als h\u00e4tten wir den Vater nicht begraben.", "tokens": ["So", "ists", ",", "als", "h\u00e4t\u00b7ten", "wir", "den", "Va\u00b7ter", "nicht", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Indessen ruhe wohl, du hochgesch\u00e4tztes Haupt!", "tokens": ["In\u00b7des\u00b7sen", "ru\u00b7he", "wohl", ",", "du", "hoch\u00b7ge\u00b7sch\u00e4tz\u00b7tes", "Haupt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dir ein fr\u00fcher Tod des Alters Krone raubt;", "tokens": ["Da", "dir", "ein", "fr\u00fc\u00b7her", "Tod", "des", "Al\u00b7ters", "Kro\u00b7ne", "raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird ein Sternenglanz, in Salems g\u00fcldnen Zimmern,", "tokens": ["So", "wird", "ein", "Ster\u00b7nen\u00b7glanz", ",", "in", "Sa\u00b7lems", "g\u00fcld\u00b7nen", "Zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als deiner Tugend Lohn, auf deiner Scheitel schimmern.", "tokens": ["Als", "dei\u00b7ner", "Tu\u00b7gend", "Lohn", ",", "auf", "dei\u00b7ner", "Schei\u00b7tel", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach nimm den letzten Dank f\u00fcr deine Sorgen an!", "tokens": ["Ach", "nimm", "den", "letz\u00b7ten", "Dank", "f\u00fcr", "dei\u00b7ne", "Sor\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn da man deinen Leib nicht ferner ehren kann,", "tokens": ["Denn", "da", "man", "dei\u00b7nen", "Leib", "nicht", "fer\u00b7ner", "eh\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Soll weder Tod noch Staub die Kindespflicht verwehren:", "tokens": ["Soll", "we\u00b7der", "Tod", "noch", "Staub", "die", "Kin\u00b7des\u00b7pflicht", "ver\u00b7weh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "NN", "ADV", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir k\u00fcssen deine Gruft, und netzen sie mit Z\u00e4hren.", "tokens": ["Wir", "k\u00fcs\u00b7sen", "dei\u00b7ne", "Gruft", ",", "und", "net\u00b7zen", "sie", "mit", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}