{"textgrid.poem.61232": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schleicht um Busch und Halde", "tokens": ["Es", "schleicht", "um", "Busch", "und", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sonnenstrahl so matt,", "tokens": ["Der", "Son\u00b7nen\u00b7strahl", "so", "matt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im herbstlich stillen Walde", "tokens": ["Im", "herbst\u00b7lich", "stil\u00b7len", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00e4llt langsam Blatt um Blatt.", "tokens": ["F\u00e4llt", "lang\u00b7sam", "Blatt", "um", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Welt versinkt in Todesruh',", "tokens": ["Die", "Welt", "ver\u00b7sinkt", "in", "To\u00b7des\u00b7ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was ist's denn mehr? Auch du, auch du,", "tokens": ["Was", "ist's", "denn", "mehr", "?", "Auch", "du", ",", "auch", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "$.", "ADV", "PPER", "$,", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Herz, du findest balde", "tokens": ["Mein", "Herz", ",", "du", "fin\u00b7dest", "bal\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die rechte Lagerstatt.", "tokens": ["Die", "rech\u00b7te", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du brachst am Lebenssteige", "tokens": ["Du", "brachst", "am", "Le\u00b7bens\u00b7stei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fr\u00fcchte, die er bot,", "tokens": ["Die", "Fr\u00fcch\u00b7te", ",", "die", "er", "bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Jugend Rosenzweige,", "tokens": ["Der", "Ju\u00b7gend", "Ro\u00b7sen\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Minne Himmelsbrot.", "tokens": ["Der", "Min\u00b7ne", "Him\u00b7mels\u00b7brot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch endlich wird des Windes Raub", "tokens": ["Doch", "end\u00b7lich", "wird", "des", "Win\u00b7des", "Raub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die letzte Lieb, das letzte Laub -", "tokens": ["Die", "letz\u00b7te", "Lieb", ",", "das", "letz\u00b7te", "Laub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So neige dich, o neige", "tokens": ["So", "nei\u00b7ge", "dich", ",", "o", "nei\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich l\u00e4chelnd in den Tod.", "tokens": ["Dich", "l\u00e4\u00b7chelnd", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}