{"textgrid.poem.67282": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei mir gegr\u00fc\u00dfet, Wappen Hamonia's,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dfet, Wappen Hamonia's,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "Wap\u00b7pen", "Ha\u00b7mo\u00b7nia's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Maal der Urzeit! D\u00fcstere Mitternacht", "tokens": ["Du", "Maal", "der", "Ur\u00b7zeit", "!", "D\u00fcs\u00b7te\u00b7re", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gebahr den sch\u00f6nsten Morgen, prangend", "tokens": ["Ge\u00b7bahr", "den", "sch\u00f6ns\u00b7ten", "Mor\u00b7gen", ",", "pran\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ragen ergl\u00fchend die freien Th\u00fcrne!", "tokens": ["Ra\u00b7gen", "er\u00b7gl\u00fc\u00b7hend", "die", "frei\u00b7en", "Th\u00fcr\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Werft euch auf's Antlitz, B\u00fcrger, es hebe sich", "tokens": ["Werft", "euch", "auf's", "Ant\u00b7litz", ",", "B\u00fcr\u00b7ger", ",", "es", "he\u00b7be", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Dankes Opfer! Siehe, die himmlische,", "tokens": ["Des", "Dan\u00b7kes", "Op\u00b7fer", "!", "Sie\u00b7he", ",", "die", "himm\u00b7li\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "VVIMP", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Rettung Stunde, schwebt als Erstlings-", "tokens": ["Der", "Ret\u00b7tung", "Stun\u00b7de", ",", "schwebt", "als", "Erst\u00b7lings"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "KOKOM", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tochter des sch\u00f6nsten der Lenze nieder.", "tokens": ["Toch\u00b7ter", "des", "sch\u00f6ns\u00b7ten", "der", "Len\u00b7ze", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Bekr\u00e4nzt mit Bl\u00fcthen strahlender Hoffnungen,", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "Bl\u00fc\u00b7then", "strah\u00b7len\u00b7der", "Hoff\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O jede Knospe reife zu Edens Frucht!", "tokens": ["O", "je\u00b7de", "Knos\u00b7pe", "rei\u00b7fe", "zu", "E\u00b7dens", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch den nur kr\u00f6nt Allvaters Segen,", "tokens": ["Doch", "den", "nur", "kr\u00f6nt", "All\u00b7va\u00b7ters", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihn ergreift mit des Strebens Inbrunst.", "tokens": ["Der", "ihn", "er\u00b7greift", "mit", "des", "Stre\u00b7bens", "In\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die weisen V\u00e4ter gaben dem Hansa-Schild", "tokens": ["Die", "wei\u00b7sen", "V\u00e4\u00b7ter", "ga\u00b7ben", "dem", "Han\u00b7sa\u00b7Schild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zween k\u00fchne W\u00e4chter, deutend mit Seherblick", "tokens": ["Zween", "k\u00fch\u00b7ne", "W\u00e4ch\u00b7ter", ",", "deu\u00b7tend", "mit", "Se\u00b7her\u00b7blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf unsern Tag \u2013 Nun gilt's! du L\u00f6wen-", "tokens": ["Auf", "un\u00b7sern", "Tag", "\u2013", "Nun", "gilt's", "!", "du", "L\u00f6\u00b7wen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "$.", "PPER", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rachen, ihr Klaun und du Flamm' im Auge!", "tokens": ["Ra\u00b7chen", ",", "ihr", "Klaun", "und", "du", "Flamm'", "im", "Au\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "PPER", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Auch mich, wo sie sich st\u00fcrzt in des Elbgotts Bett,", "tokens": ["Auch", "mich", ",", "wo", "sie", "sich", "st\u00fcrzt", "in", "des", "Elb\u00b7gotts", "Bett", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "ART", "NE", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Gebahr die Alster-Nymphe. Mir zucket schon", "tokens": ["Ge\u00b7bahr", "die", "Als\u00b7ter\u00b7Nym\u00b7phe", ".", "Mir", "zu\u00b7cket", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Arm; schon blitzt, gewetzt, das gute", "tokens": ["Mein", "Arm", ";", "schon", "blitzt", ",", "ge\u00b7wetzt", ",", "das", "gu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVFIN", "$,", "VVPP", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwert, und ich spotte des Silberhaares.", "tokens": ["Schwert", ",", "und", "ich", "spot\u00b7te", "des", "Sil\u00b7ber\u00b7haa\u00b7res", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}