{"textgrid.poem.33368": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An Alexinger", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier schick' ich dir den selt'nen Freund zur\u00fccke,", "tokens": ["Hier", "schick'", "ich", "dir", "den", "selt'\u00b7nen", "Freund", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem nie ein Freund auf Erden glich,", "tokens": ["Dem", "nie", "ein", "Freund", "auf", "Er\u00b7den", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, wenn er bei dir war \u2013 so wenig als die Ficke", "tokens": ["Der", ",", "wenn", "er", "bei", "dir", "war", "\u2013", "so", "we\u00b7nig", "als", "die", "Fi\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$(", "ADV", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von deinem Rock \u2013 von deiner Seite wich,", "tokens": ["Von", "dei\u00b7nem", "Rock", "\u2013", "von", "dei\u00b7ner", "Sei\u00b7te", "wich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der, wenn er oft in heissen Pr\u00fcfungstagen", "tokens": ["Der", ",", "wenn", "er", "oft", "in", "heis\u00b7sen", "Pr\u00fc\u00b7fungs\u00b7ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein schweres Dichterkreuz, woran", "tokens": ["Dein", "schwe\u00b7res", "Dich\u00b7ter\u00b7kreuz", ",", "wo\u00b7ran"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du nun als Heiland h\u00e4ngst, den steilen Berg hinan", "tokens": ["Du", "nun", "als", "Hei\u00b7land", "h\u00e4ngst", ",", "den", "stei\u00b7len", "Berg", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "KOUS", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bis hin zur Sch\u00e4delstatt des Ruhms getragen,", "tokens": ["Bis", "hin", "zur", "Sch\u00e4\u00b7del\u00b7statt", "des", "Ruhms", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den blut'gen Schwei\u00df, der dir dabei entrann,", "tokens": ["Den", "blut'\u00b7gen", "Schwei\u00df", ",", "der", "dir", "da\u00b7bei", "ent\u00b7rann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie Sankt Veronika, dir von der Stirne wischte,", "tokens": ["Wie", "Sankt", "Ve\u00b7ro\u00b7ni\u00b7ka", ",", "dir", "von", "der", "Stir\u00b7ne", "wischte", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den treuen Freund, in dessen Schoos,", "tokens": ["Den", "treu\u00b7en", "Freund", ",", "in", "des\u00b7sen", "Schoos", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn dir das Schicksal Schmerz in deine Tage mischte,", "tokens": ["Wenn", "dir", "das", "Schick\u00b7sal", "Schmerz", "in", "dei\u00b7ne", "Ta\u00b7ge", "mischte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So manche Thr\u00e4ne deines Kummers flo\u00df,", "tokens": ["So", "man\u00b7che", "Thr\u00e4\u00b7ne", "dei\u00b7nes", "Kum\u00b7mers", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der jeden deiner Seufzer h\u00f6rte,", "tokens": ["Der", "je\u00b7den", "dei\u00b7ner", "Seuf\u00b7zer", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So willig stets an deine Lippe kam,", "tokens": ["So", "wil\u00b7lig", "stets", "an", "dei\u00b7ne", "Lip\u00b7pe", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und all' den Unrath von dir nahm,", "tokens": ["Und", "all'", "den", "Un\u00b7rath", "von", "dir", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der den Kopf so manchen Tag beschwerte;", "tokens": ["Der", "den", "Kopf", "so", "man\u00b7chen", "Tag", "be\u00b7schwer\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Den Freund, der, wenn was zu vergessen war,", "tokens": ["Den", "Freund", ",", "der", ",", "wenn", "was", "zu", "ver\u00b7ges\u00b7sen", "war", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "KOUS", "PIS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Bei dir den treuen Mentor machte,", "tokens": ["Bei", "dir", "den", "treu\u00b7en", "Men\u00b7tor", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "In manchen F\u00e4llen f\u00fcr dich dachte,", "tokens": ["In", "man\u00b7chen", "F\u00e4l\u00b7len", "f\u00fcr", "dich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wo du vergassest, und vor dem sogar", "tokens": ["Wo", "du", "ver\u00b7gas\u00b7sest", ",", "und", "vor", "dem", "so\u00b7gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KON", "APPR", "ART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Kein Fleckchen an dir sicher war;", "tokens": ["Kein", "Fleck\u00b7chen", "an", "dir", "si\u00b7cher", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Kurz, den Getreuen, der sich w\u00fcrdig machte,", "tokens": ["Kurz", ",", "den", "Ge\u00b7treu\u00b7en", ",", "der", "sich", "w\u00fcr\u00b7dig", "mach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "Am sch\u00f6nsten M\u00e4dchenbusen nun,", "tokens": ["Am", "sch\u00f6ns\u00b7ten", "M\u00e4d\u00b7chen\u00b7bu\u00b7sen", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "F\u00fcr seine Dienste auszuruhn,", "tokens": ["F\u00fcr", "sei\u00b7ne", "Diens\u00b7te", "aus\u00b7zu\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und den du, ob du gleich ihm viel zu danken hast,", "tokens": ["Und", "den", "du", ",", "ob", "du", "gleich", "ihm", "viel", "zu", "dan\u00b7ken", "hast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "$,", "KOUS", "PPER", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So sch\u00e4ndlich j\u00fcngst bei mir verga\u00df'st,", "tokens": ["So", "sch\u00e4nd\u00b7lich", "j\u00fcngst", "bei", "mir", "ver\u00b7ga\u00df'st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Den so verdienten Freund, o Lieber! schicke", "tokens": ["Den", "so", "ver\u00b7dien\u00b7ten", "Freund", ",", "o", "Lie\u00b7ber", "!", "schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "FM", "ADJD", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Ich dir \u2013 in deinem ", "tokens": ["Ich", "dir", "\u2013", "in", "dei\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "PPER", "$(", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}}}}}