{"textgrid.poem.24831": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sagt: \u00bbUm Freuden, die erstarben,", "tokens": ["Ihr", "sagt", ":", "\u00bb", "Um", "Freu\u00b7den", ",", "die", "er\u00b7star\u00b7ben", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "KOUI", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum dies jahrelange Leid?", "tokens": ["Wa\u00b7rum", "dies", "jah\u00b7re\u00b7lan\u00b7ge", "Leid", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedwede Wunde mu\u00df vernarben,", "tokens": ["Jed\u00b7we\u00b7de", "Wun\u00b7de", "mu\u00df", "ver\u00b7nar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und jeden Kummer stillt die Zeit.\u00ab", "tokens": ["Und", "je\u00b7den", "Kum\u00b7mer", "stillt", "die", "Zeit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein! Scheucht, wenn ihr verm\u00f6gt, den euern;", "tokens": ["Nein", "!", "Scheucht", ",", "wenn", "ihr", "ver\u00b7m\u00f6gt", ",", "den", "eu\u00b7ern", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch treu bewahr' ich meinen Gram,", "tokens": ["Doch", "treu", "be\u00b7wahr'", "ich", "mei\u00b7nen", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der stets mir frisch das Bild der Teuern", "tokens": ["Der", "stets", "mir", "frisch", "das", "Bild", "der", "Teu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erh\u00e4lt, wie da ich Abschied nahm.", "tokens": ["Er\u00b7h\u00e4lt", ",", "wie", "da", "ich", "Ab\u00b7schied", "nahm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00df ist die Trauer im Gem\u00fcte,", "tokens": ["S\u00fc\u00df", "ist", "die", "Trau\u00b7er", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die von vergangnen Wonnen spricht:", "tokens": ["Die", "von", "ver\u00b7gang\u00b7nen", "Won\u00b7nen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O raubt die D\u00fcfte nicht der Bl\u00fcte,", "tokens": ["O", "raubt", "die", "D\u00fcf\u00b7te", "nicht", "der", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herzen seinen Kummer nicht!", "tokens": ["Dem", "Her\u00b7zen", "sei\u00b7nen", "Kum\u00b7mer", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag ewig bluten meine Wunde,", "tokens": ["Mag", "e\u00b7wig", "blu\u00b7ten", "mei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn, von dem Schmerze neu belebt,", "tokens": ["Wenn", ",", "von", "dem", "Schmer\u00b7ze", "neu", "be\u00b7lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nur die Erinnrung jeder Stunde,", "tokens": ["Nur", "die", "E\u00b7rinn\u00b7rung", "je\u00b7der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "In der sie mein war, mich umschwebt.", "tokens": ["In", "der", "sie", "mein", "war", ",", "mich", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "VAFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}