{"textgrid.poem.47160": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, geliebtes Angesicht,", "tokens": ["La\u00df", ",", "ge\u00b7lieb\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df uns nicht verzagen,", "tokens": ["La\u00df", "uns", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df der Liebe Jugendlicht", "tokens": ["Da\u00df", "der", "Lie\u00b7be", "Ju\u00b7gend\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lischt in kurzen Tagen.", "tokens": ["Lischt", "in", "kur\u00b7zen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ew'ge Jugend ist durch dich", "tokens": ["Ew'\u00b7ge", "Ju\u00b7gend", "ist", "durch", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf in mir gegangen;", "tokens": ["Auf", "in", "mir", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mag denn nur die ird'sche sich", "tokens": ["Mag", "denn", "nur", "die", "ird'\u00b7sche", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "ADJA", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehlen von den Wangen!", "tokens": ["Steh\u00b7len", "von", "den", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dieses Leben, das du mir", "tokens": ["Die\u00b7ses", "Le\u00b7ben", ",", "das", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebend hast gegeben,", "tokens": ["Lie\u00b7bend", "hast", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Liebend wieder geb' ich dir", "tokens": ["Lie\u00b7bend", "wie\u00b7der", "geb'", "ich", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und versch\u00f6nt das Leben.", "tokens": ["Und", "ver\u00b7sch\u00f6nt", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Jeder Blitz aus deinem Licht,", "tokens": ["Je\u00b7der", "Blitz", "aus", "dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Sch\u00f6nheitsfunken,", "tokens": ["Je\u00b7der", "Sch\u00f6n\u00b7heits\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In das Dunkel ist er nicht,", "tokens": ["In", "das", "Dun\u00b7kel", "ist", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern hier versunken;", "tokens": ["Son\u00b7dern", "hier", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "In die fr\u00fchlingshelle Brust", "tokens": ["In", "die", "fr\u00fch\u00b7lings\u00b7hel\u00b7le", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stieg er leis' hernieder,", "tokens": ["Stieg", "er", "leis'", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ward ein stiller Keim der Lust", "tokens": ["Ward", "ein", "stil\u00b7ler", "Keim", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Baum der Lieder.", "tokens": ["An", "dem", "Baum", "der", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Liebste! dieses Fr\u00fchlings Glanz,", "tokens": ["Liebs\u00b7te", "!", "die\u00b7ses", "Fr\u00fch\u00b7lings", "Glanz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich dir verdanke,", "tokens": ["Den", "ich", "dir", "ver\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Freudig deinem Haupt zum Kranz", "tokens": ["Freu\u00b7dig", "dei\u00b7nem", "Haupt", "zum", "Kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Opfr' er jede Ranke.", "tokens": ["Opf\u00b7r'", "er", "je\u00b7de", "Ran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wann in meines Auges Glanz", "tokens": ["Wann", "in", "mei\u00b7nes", "Au\u00b7ges", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du nicht mehr mein Lieben", "tokens": ["Du", "nicht", "mehr", "mein", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ADV", "PPOSAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lesen kannst, so lies es ganz", "tokens": ["Le\u00b7sen", "kannst", ",", "so", "lies", "es", "ganz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch im Lied geschrieben.", "tokens": ["Noch", "im", "Lied", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wann kein andrer Spiegel dir", "tokens": ["Wann", "kein", "an\u00b7drer", "Spie\u00b7gel", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will die Jugend zeigen,", "tokens": ["Will", "die", "Ju\u00b7gend", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In des Liedes Spiegel hier", "tokens": ["In", "des", "Lie\u00b7des", "Spie\u00b7gel", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist sie noch dein eigen.", "tokens": ["Ist", "sie", "noch", "dein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}