{"textgrid.poem.49556": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bef\u00fcrchte nicht, da\u00df dir Aristons Hertz entflieht", "tokens": ["Be\u00b7f\u00fcrch\u00b7te", "nicht", ",", "da\u00df", "dir", "A\u00b7ris\u00b7tons", "Hertz", "ent\u00b7flieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Z\u00e4rtlichkeit mit Unbestand bezahle.", "tokens": ["Und", "dei\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7keit", "mit", "Un\u00b7be\u00b7stand", "be\u00b7zah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man wird zwar ungetreu, wenn man dich, Isse, sieht,", "tokens": ["Man", "wird", "zwar", "un\u00b7ge\u00b7treu", ",", "wenn", "man", "dich", ",", "Is\u00b7se", ",", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PIS", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch andern nur; doch nur zum allerlezten mahle.", "tokens": ["Doch", "an\u00b7dern", "nur", ";", "doch", "nur", "zum", "al\u00b7ler\u00b7lez\u00b7ten", "mah\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "$.", "ADV", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}