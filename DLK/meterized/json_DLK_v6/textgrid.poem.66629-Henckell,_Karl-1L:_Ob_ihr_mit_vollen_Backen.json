{"textgrid.poem.66629": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob ihr mit vollen Backen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ihr mit vollen Backen", "tokens": ["Ob", "ihr", "mit", "vol\u00b7len", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trompetet Lug und Neid,", "tokens": ["Trom\u00b7pe\u00b7tet", "Lug", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "In t\u00fcckischen Attacken", "tokens": ["In", "t\u00fc\u00b7cki\u00b7schen", "At\u00b7ta\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Garstige Granaten speit,", "tokens": ["Gars\u00b7ti\u00b7ge", "Gra\u00b7na\u00b7ten", "speit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Ihr Helden von der Feder,", "tokens": ["Ihr", "Hel\u00b7den", "von", "der", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom kritischen Katheder,", "tokens": ["Vom", "kri\u00b7ti\u00b7schen", "Ka\u00b7the\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mir sitzt der Schalk im Nacken", "tokens": ["Mir", "sitzt", "der", "Schalk", "im", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Lust und Leid.", "tokens": ["In", "Lust", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr seid die alte Meute,", "tokens": ["Ihr", "seid", "die", "al\u00b7te", "Meu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schwarze Meute ja,", "tokens": ["Die", "schwar\u00b7ze", "Meu\u00b7te", "ja", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber l\u00e4ute, l\u00e4ute", "tokens": ["Ich", "a\u00b7ber", "l\u00e4u\u00b7te", ",", "l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein hell Halleluja.", "tokens": ["Mein", "hell", "Hal\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von meinem Fensterbrette", "tokens": ["Von", "mei\u00b7nem", "Fens\u00b7ter\u00b7bret\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Pfeif' ich die Fr\u00fchlingsmette,", "tokens": ["Pfeif'", "ich", "die", "Fr\u00fch\u00b7lings\u00b7met\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geburtstag feir' ich heute,", "tokens": ["Ge\u00b7burts\u00b7tag", "feir'", "ich", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was wollt ihr da?", "tokens": ["Was", "wollt", "ihr", "da", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mir wirft die Post der Gaben", "tokens": ["Mir", "wirft", "die", "Post", "der", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So viele auf den Tisch:", "tokens": ["So", "vie\u00b7le", "auf", "den", "Tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die \u00bbHerzli\u00ab sind erhaben,", "tokens": ["Die", "\u00bb", "Herz\u00b7li", "\u00ab", "sind", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$(", "NE", "$(", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kuchen s\u00fc\u00df und frisch!", "tokens": ["Die", "Ku\u00b7chen", "s\u00fc\u00df", "und", "frisch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Freundschaft streut mir Gr\u00fc\u00dfe", "tokens": ["Die", "Freund\u00b7schaft", "streut", "mir", "Gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Blumen vor die F\u00fc\u00dfe,", "tokens": ["Und", "Blu\u00b7men", "vor", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Liebe will mich laben", "tokens": ["Die", "Lie\u00b7be", "will", "mich", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verschwenderisch.", "tokens": ["Ver\u00b7schwen\u00b7de\u00b7risch", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Die Sonne steigt im Osten,", "tokens": ["Die", "Son\u00b7ne", "steigt", "im", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Amsel ladet hell:", "tokens": ["Die", "Am\u00b7sel", "la\u00b7det", "hell", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbvom Lichtquell la\u00df uns kosten,", "tokens": ["\u00bb", "vom", "Licht\u00b7quell", "la\u00df", "uns", "kos\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frischauf, frischauf, Gesell!", "tokens": ["Frisc\u00b7hauf", ",", "frisc\u00b7hauf", ",", "Ge\u00b7sell", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach all den toten Tagen", "tokens": ["Nach", "all", "den", "to\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir m\u00f6gen's wohl vertragen,", "tokens": ["Wir", "m\u00f6\u00b7gen's", "wohl", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die weichen Veilchen spro\u00dften", "tokens": ["Die", "wei\u00b7chen", "Veil\u00b7chen", "spro\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Am jungen Quell.", "tokens": ["Am", "jun\u00b7gen", "Quell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und la\u00df dich's nicht verdrie\u00dfen", "tokens": ["Und", "la\u00df", "dich's", "nicht", "ver\u00b7drie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und la\u00df dich's nicht gereun,", "tokens": ["Und", "la\u00df", "dich's", "nicht", "ge\u00b7reun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Guten auszugie\u00dfen", "tokens": ["Den", "Gu\u00b7ten", "aus\u00b7zu\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ges\u00e4nge, die dich freun!", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", ",", "die", "dich", "freun", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Merker und die Mucker,", "tokens": ["Die", "Mer\u00b7ker", "und", "die", "Mu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das sind die \u00e4rmsten Schlucker,", "tokens": ["Das", "sind", "die", "\u00e4rms\u00b7ten", "Schlu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die lieben in Verlie\u00dfen", "tokens": ["Die", "lie\u00b7ben", "in", "Ver\u00b7lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Licht zu scheun.\u00ab", "tokens": ["Das", "Licht", "zu", "scheun", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die ihr mit plumpen R\u00fcsseln", "tokens": ["Die", "ihr", "mit", "plum\u00b7pen", "R\u00fcs\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich schnuppernd untersucht,", "tokens": ["Mich", "schnup\u00b7pernd", "un\u00b7ter\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit gelben Himmelsschl\u00fcsseln", "tokens": ["Mit", "gel\u00b7ben", "Him\u00b7mels\u00b7schl\u00fcs\u00b7seln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jag' ich euch in die Flucht.", "tokens": ["Jag'", "ich", "euch", "in", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr seid die schwarze Meute,", "tokens": ["Ihr", "seid", "die", "schwar\u00b7ze", "Meu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich aber l\u00e4ute, l\u00e4ute", "tokens": ["Ich", "a\u00b7ber", "l\u00e4u\u00b7te", ",", "l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und samml' auf Silbersch\u00fcsseln", "tokens": ["Und", "samml'", "auf", "Sil\u00b7ber\u00b7sch\u00fcs\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Goldsaftige Frucht.", "tokens": ["Gol\u00b7dsaf\u00b7ti\u00b7ge", "Frucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}