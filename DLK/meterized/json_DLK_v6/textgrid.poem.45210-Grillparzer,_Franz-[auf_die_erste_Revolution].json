{"textgrid.poem.45210": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[auf die erste Revolution]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die erste Revolution", "tokens": ["Auf", "die", "ers\u00b7te", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Kamen wieder die Bourbons.", "tokens": ["Ka\u00b7men", "wie\u00b7der", "die", "Bour\u00b7bons", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf unsre allgemeine zweite,", "tokens": ["Auf", "uns\u00b7re", "all\u00b7ge\u00b7mei\u00b7ne", "zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommen wohl wieder die alten Leute.", "tokens": ["Kom\u00b7men", "wohl", "wie\u00b7der", "die", "al\u00b7ten", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}