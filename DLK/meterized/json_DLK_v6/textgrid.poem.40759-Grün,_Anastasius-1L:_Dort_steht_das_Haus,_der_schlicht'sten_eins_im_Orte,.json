{"textgrid.poem.40759": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort steht das Haus, der schlicht'sten eins im Orte,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort steht das Haus, der schlicht'sten eins im Orte,", "tokens": ["Dort", "steht", "das", "Haus", ",", "der", "schlicht'\u00b7sten", "eins", "im", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich wie Kriegerreihn an Haltung gleichen;", "tokens": ["Die", "sich", "wie", "Krie\u00b7ger\u00b7reihn", "an", "Hal\u00b7tung", "glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOKOM", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur tr\u00e4gt's die Marmortafel ob der Pforte,", "tokens": ["Nur", "tr\u00e4gt's", "die", "Mar\u00b7mor\u00b7ta\u00b7fel", "ob", "der", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Heldenbrust das Ehrenzeichen.", "tokens": ["Wie", "ei\u00b7ne", "Hel\u00b7den\u00b7brust", "das", "Eh\u00b7ren\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein kahler Ziegelbau mit Riegelw\u00e4nden", "tokens": ["Ein", "kah\u00b7ler", "Zie\u00b7gel\u00b7bau", "mit", "Rie\u00b7gel\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und steilem Giebeldach nach Landessitte;", "tokens": ["Und", "stei\u00b7lem", "Gie\u00b7bel\u00b7dach", "nach", "Lan\u00b7des\u00b7sit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir aber ist's, als ob an allen Enden", "tokens": ["Dir", "a\u00b7ber", "ist's", ",", "als", "ob", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein milder Glorienschein den Bau umglitte.", "tokens": ["Ein", "mil\u00b7der", "Glo\u00b7ri\u00b7en\u00b7schein", "den", "Bau", "um\u00b7glit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gemeines Weinlaub will zum Simse klettern,", "tokens": ["Ge\u00b7mei\u00b7nes", "Wein\u00b7laub", "will", "zum", "Sim\u00b7se", "klet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mauern Risse doch verbirgt's in Ranken;", "tokens": ["Der", "Mau\u00b7ern", "Ris\u00b7se", "doch", "ver\u00b7birgt's", "in", "Ran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wird's zum Lorber, sch\u00f6n mit heil'gen Bl\u00e4ttern", "tokens": ["So", "wird's", "zum", "Lor\u00b7ber", ",", "sch\u00f6n", "mit", "heil'\u00b7gen", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Haupt umh\u00fcllend Furchen der Gedanken.", "tokens": ["Am", "Haupt", "um\u00b7h\u00fcl\u00b7lend", "Fur\u00b7chen", "der", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Holz der Treppen, ausgetreten, enge,", "tokens": ["Das", "Holz", "der", "Trep\u00b7pen", ",", "aus\u00b7ge\u00b7tre\u00b7ten", ",", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besch\u00e4mt den Marmorbau vor Tempelhallen;", "tokens": ["Be\u00b7sch\u00e4mt", "den", "Mar\u00b7mor\u00b7bau", "vor", "Tem\u00b7pel\u00b7hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo g\u00e4b's so edler Waller fromm Gedr\u00e4nge,", "tokens": ["Wo", "g\u00e4b's", "so", "ed\u00b7ler", "Wal\u00b7ler", "fromm", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie hier vor uns empor und nach uns wallen!", "tokens": ["Wie", "hier", "vor", "uns", "em\u00b7por", "und", "nach", "uns", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPER", "PTKVZ", "KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir treten ein. Uns will's die Brust umschn\u00fcren,", "tokens": ["Wir", "tre\u00b7ten", "ein", ".", "Uns", "will's", "die", "Brust", "um\u00b7schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob wir bang im Saal des K\u00f6nigs st\u00e4nden;", "tokens": ["Als", "ob", "wir", "bang", "im", "Saal", "des", "K\u00f6\u00b7nigs", "st\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Andacht und Demut will das Herz uns r\u00fchren,", "tokens": ["An\u00b7dacht", "und", "De\u00b7mut", "will", "das", "Herz", "uns", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob wir uns in Gottes Kirche f\u00e4nden.", "tokens": ["Als", "ob", "wir", "uns", "in", "Got\u00b7tes", "Kir\u00b7che", "f\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir stehn am Pult, wo Er gedacht, gedichtet.", "tokens": ["Wir", "stehn", "am", "Pult", ",", "wo", "Er", "ge\u00b7dacht", ",", "ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All' was des Sch\u00f6nen, Gro\u00dfen er gesonnen,", "tokens": ["All'", "was", "des", "Sch\u00f6\u00b7nen", ",", "Gro\u00b7\u00dfen", "er", "ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "ART", "NN", "$,", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat \u00fcbermannt uns jetzt und aufgerichtet,", "tokens": ["Hat", "\u00fc\u00b7berm\u00b7annt", "uns", "jetzt", "und", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPER", "ADV", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Getr\u00e4nkt, gel\u00e4utert aus kristallnem Bronnen.", "tokens": ["Ge\u00b7tr\u00e4nkt", ",", "ge\u00b7l\u00e4u\u00b7tert", "aus", "kris\u00b7tall\u00b7nem", "Bron\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das Schweigen herrscht, wo einst sein Wort geklungen.", "tokens": ["Das", "Schwei\u00b7gen", "herrscht", ",", "wo", "einst", "sein", "Wort", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr als die\u00df Wort, nicht frei von ird'scher Fehle,", "tokens": ["Mehr", "als", "die\u00df", "Wort", ",", "nicht", "frei", "von", "ird'\u00b7scher", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PDS", "NN", "$,", "PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat uns des Schweigens Geisterbann bezwungen,", "tokens": ["Hat", "uns", "des", "Schwei\u00b7gens", "Geis\u00b7ter\u00b7bann", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fromm Gel\u00f6bni\u00df keimt aus unsrer Seele.", "tokens": ["Und", "fromm", "Ge\u00b7l\u00f6b\u00b7ni\u00df", "keimt", "aus", "uns\u00b7rer", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Hier d\u00fcnkt uns doppelt arm jed' \u00e4rmlich Streben,", "tokens": ["Hier", "d\u00fcnkt", "uns", "dop\u00b7pelt", "arm", "jed'", "\u00e4rm\u00b7lich", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gro\u00df k\u00f6nnen Wen'ge, gut sein kann der Kleinste;", "tokens": ["Gro\u00df", "k\u00f6n\u00b7nen", "Wen'\u00b7ge", ",", "gut", "sein", "kann", "der", "Kleins\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "VAINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Ortes Weihe adelt uns das Leben,", "tokens": ["Des", "Or\u00b7tes", "Wei\u00b7he", "a\u00b7delt", "uns", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie geadelt hier selbst das Gemeinste:", "tokens": ["Wie", "sie", "ge\u00b7a\u00b7delt", "hier", "selbst", "das", "Ge\u00b7meins\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Der Tropfen, der aus seiner Feder spritzte,", "tokens": ["Der", "Trop\u00b7fen", ",", "der", "aus", "sei\u00b7ner", "Fe\u00b7der", "spritz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Spur, die in die Dielen er getreten,", "tokens": ["Die", "Spur", ",", "die", "in", "die", "Die\u00b7len", "er", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Strich, den dort er in die Scheiben ritzte,", "tokens": ["Der", "Strich", ",", "den", "dort", "er", "in", "die", "Schei\u00b7ben", "ritz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sind uns Feuerstapfen des Propheten.", "tokens": ["Sie", "sind", "uns", "Feu\u00b7er\u00b7stap\u00b7fen", "des", "Pro\u00b7phe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Selbst hier das Spinngeh\u00e4ng', \u2013 wer m\u00f6cht' es missen!", "tokens": ["Selbst", "hier", "das", "Spinn\u00b7ge\u00b7h\u00e4ng'", ",", "\u2013", "wer", "m\u00f6cht'", "es", "mis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns will der Ueberflei\u00df der Magd mi\u00dffallen,", "tokens": ["Uns", "will", "der", "Ue\u00b7ber\u00b7flei\u00df", "der", "Magd", "mi\u00df\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die weg den Staub gefegt, der \u2013 k\u00f6nnt ihr's wissen?", "tokens": ["Die", "weg", "den", "Staub", "ge\u00b7fegt", ",", "der", "\u2013", "k\u00f6nnt", "ih\u00b7r's", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,", "PRELS", "$(", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Sohlen des Unsterblichen entfallen!", "tokens": ["Den", "Soh\u00b7len", "des", "U\u00b7nsterb\u00b7li\u00b7chen", "ent\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "So wirkt der Todte noch! \u2013 \u00bbWelch froh Getriebe", "tokens": ["So", "wirkt", "der", "Tod\u00b7te", "noch", "!", "\u2013", "\u00bb", "Welch", "froh", "Ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "$(", "$(", "PIAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umgab sein Leben erst!\u00ab \u2013 \u2013 O th\u00f6richt W\u00e4hnen!", "tokens": ["Um\u00b7gab", "sein", "Le\u00b7ben", "erst", "!", "\u00ab", "\u2013", "\u2013", "O", "th\u00f6\u00b7richt", "W\u00e4h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$.", "$(", "$(", "$(", "NE", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl schritt hier an der Muse Hand die Liebe,", "tokens": ["Wohl", "schritt", "hier", "an", "der", "Mu\u00b7se", "Hand", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus sel'gen Tr\u00e4umen stieg ein g\u00f6ttlich Sehnen.", "tokens": ["Aus", "sel'\u00b7gen", "Tr\u00e4u\u00b7men", "stieg", "ein", "g\u00f6tt\u00b7lich", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Doch hielten Einkehr auch viel dunkle Stunden", "tokens": ["Doch", "hiel\u00b7ten", "Ein\u00b7kehr", "a