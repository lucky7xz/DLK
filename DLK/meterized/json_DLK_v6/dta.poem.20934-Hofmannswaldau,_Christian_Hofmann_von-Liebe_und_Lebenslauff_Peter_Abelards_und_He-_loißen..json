{"dta.poem.20934": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Liebe  \n und Lebenslauff  \n Peter Abelards und He-  \n loi\u00dfen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Freund/ den Noth ber\u00fchmt/ Verlust hat gro\u00df ge-", "tokens": ["Ein", "Freund", "/", "den", "Noth", "be\u00b7r\u00fchmt", "/", "Ver\u00b7lust", "hat", "gro\u00df", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "ADJD", "$(", "NN", "VAFIN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "macht/", "tokens": ["macht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dr\u00fcckt seine Freundin noch alhier an Brust und Armen", "tokens": ["Dr\u00fcckt", "sei\u00b7ne", "Freun\u00b7din", "noch", "al\u00b7hier", "an", "Brust", "und", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lieb und Vertrauligkeit/ so Tod und Grab verlacht/", "tokens": ["Lieb", "und", "Ver\u00b7trau\u00b7lig\u00b7keit", "/", "so", "Tod", "und", "Grab", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "ADV", "NN", "KON", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Heist die Verliebten Zwey auch in dem Grab erwarmen.", "tokens": ["Heist", "die", "Ver\u00b7lieb\u00b7ten", "Zwey", "auch", "in", "dem", "Grab", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "CARD", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein edles Leben macht auch einen edlen Todt/", "tokens": ["Ein", "ed\u00b7les", "Le\u00b7ben", "macht", "auch", "ei\u00b7nen", "ed\u00b7len", "Todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Getreue Liebe will auch aus dem Grab entspriessen/", "tokens": ["Ge\u00b7treu\u00b7e", "Lie\u00b7be", "will", "auch", "aus", "dem", "Grab", "ent\u00b7spries\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zum Zeugni\u00df da\u00df Sie nun besiegen Todt und Noth/", "tokens": ["Zum", "Zeug\u00b7ni\u00df", "da\u00df", "Sie", "nun", "be\u00b7sie\u00b7gen", "Todt", "und", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "PPER", "ADV", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wollen sie sich hier auch in der Asche k\u00e4ssen.", "tokens": ["So", "wol\u00b7len", "sie", "sich", "hier", "auch", "in", "der", "A\u00b7sche", "k\u00e4s\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}