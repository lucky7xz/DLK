{"textgrid.poem.53777": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Prolet vor Gericht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stehst du in dem Menschenschrank?", "tokens": ["Stehst", "du", "in", "dem", "Men\u00b7schen\u00b7schrank", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die da wolln dich strafen.", "tokens": ["Die", "da", "wolln", "dich", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du bist m\u00fcde, bleich und krank;", "tokens": ["Du", "bist", "m\u00fc\u00b7de", ",", "bleich", "und", "krank", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "die sind voller Tatendrang,", "tokens": ["die", "sind", "vol\u00b7ler", "Ta\u00b7ten\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "satt und ausgeschlafen.", "tokens": ["satt", "und", "aus\u00b7ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zum Justizwerk, wohl vertraut,", "tokens": ["Zum", "Jus\u00b7tiz\u00b7werk", ",", "wohl", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "wird man sich vereinen:", "tokens": ["wird", "man", "sich", "ver\u00b7ei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Junge! Wehr dich deiner Haut!", "tokens": ["Jun\u00b7ge", "!", "Wehr", "dich", "dei\u00b7ner", "Haut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dreie gegen einen!", "tokens": ["Drei\u00b7e", "ge\u00b7gen", "ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Direktor, fein mit Ei,", "tokens": ["Der", "Di\u00b7rek\u00b7tor", ",", "fein", "mit", "Ei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "hackt mit kurzen Fragen.", "tokens": ["hackt", "mit", "kur\u00b7zen", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf die schlimmste Pflaumerei", "tokens": ["Auf", "die", "schlimms\u00b7te", "Pflau\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "darfst du gar nichts sagen.", "tokens": ["darfst", "du", "gar", "nichts", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Spitzel kann mit Vorbehalt", "tokens": ["Spit\u00b7zel", "kann", "mit", "Vor\u00b7be\u00b7halt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "unter Schutz erscheinen.", "tokens": ["un\u00b7ter", "Schutz", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Protokoll und Staatsanwalt:", "tokens": ["Pro\u00b7to\u00b7koll", "und", "Staats\u00b7an\u00b7walt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcnfe gegen einen!", "tokens": ["F\u00fcn\u00b7fe", "ge\u00b7gen", "ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Staatsanwalt und Pl\u00e4doyer,", "tokens": ["Staats\u00b7an\u00b7walt", "und", "Pl\u00e4\u00b7do\u00b7yer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommst du noch nach Hause?", "tokens": ["Kommst", "du", "noch", "nach", "Hau\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Antrag. Die Justiz-AG", "tokens": ["An\u00b7trag", ".", "Die", "Jus\u00b7ti\u00b7zAG"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "macht erst Fr\u00fchst\u00fcckspause.", "tokens": ["macht", "erst", "Fr\u00fch\u00b7st\u00fccks\u00b7pau\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vier Jahr Zuchthaus.", "tokens": ["Vier", "Jahr", "Zucht\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "\u00bbabf\u00fchrn den . . . !\u00ab", "tokens": ["\u00bb", "ab\u00b7f\u00fchrn", "den", ".", ".", ".", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "ART", "$.", "$.", "$.", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Leis zwei Frauen weinen.", "tokens": ["Leis", "zwei", "Frau\u00b7en", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wirst du je sie wiedersehn?", "tokens": ["Wirst", "du", "je", "sie", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Alle gegen einen \u2013!", "tokens": ["Al\u00b7le", "ge\u00b7gen", "ei\u00b7nen", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "ART", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In Zellen bricht man euer Leben", "tokens": ["In", "Zel\u00b7len", "bricht", "man", "eu\u00b7er", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr etwas, das ihr niemals saht:", "tokens": ["f\u00fcr", "et\u00b7was", ",", "das", "ihr", "nie\u00b7mals", "saht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Freiheit m\u00fc\u00dft ihr T\u00fcten kleben,", "tokens": ["F\u00fcr", "Frei\u00b7heit", "m\u00fc\u00dft", "ihr", "T\u00fc\u00b7ten", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein jeglicher ein Volkssoldat.", "tokens": ["ein", "jeg\u00b7li\u00b7cher", "ein", "Volks\u00b7sol\u00b7dat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herauf ihr! Aus den Kohlenzechen!", "tokens": ["Her\u00b7auf", "ihr", "!", "Aus", "den", "Koh\u00b7len\u00b7ze\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Baut in Betrieben Stein auf Stein!", "tokens": ["Baut", "in", "Be\u00b7trie\u00b7ben", "Stein", "auf", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es kommt der Tag, da wir uns r\u00e4chen:", "tokens": ["Es", "kommt", "der", "Tag", ",", "da", "wir", "uns", "r\u00e4\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da werdet ihr die Richter sein \u2013!", "tokens": ["Da", "wer\u00b7det", "ihr", "die", "Rich\u00b7ter", "sein", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VAINF", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}