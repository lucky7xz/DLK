{"textgrid.poem.60819": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Corneille", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht der Trag\u00f6die Sch\u00f6pfer in mir! Der bed\u00fcrftigen Sprache", "tokens": ["Seht", "der", "Tra\u00b7g\u00f6\u00b7die", "Sch\u00f6p\u00b7fer", "in", "mir", "!", "Der", "be\u00b7d\u00fcrf\u00b7ti\u00b7gen", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Gab ich zuerst Reichtum, Leben und Redegewalt.", "tokens": ["Gab", "ich", "zu\u00b7erst", "Reich\u00b7tum", ",", "Le\u00b7ben", "und", "Re\u00b7de\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "R\u00fcckw\u00e4rts lie\u00df ich die griechische Fabel, und reine Geschichte", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "lie\u00df", "ich", "die", "grie\u00b7chi\u00b7sche", "Fa\u00b7bel", ",", "und", "rei\u00b7ne", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Stellt ich zuerst rein dar, ohne gemeinere Form:", "tokens": ["Stellt", "ich", "zu\u00b7erst", "rein", "dar", ",", "oh\u00b7ne", "ge\u00b7mei\u00b7ne\u00b7re", "Form", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "KOUI", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Roms Herrschaft, Aufschwung und Verfall und verfeinerte Staatskunst", "tokens": ["Roms", "Herr\u00b7schaft", ",", "Auf\u00b7schwung", "und", "Ver\u00b7fall", "und", "ver\u00b7fei\u00b7ner\u00b7te", "Staats\u00b7kunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Zeigt ich, und zeigte sie wahr, aber mit W\u00fcrde zugleich;", "tokens": ["Zeigt", "ich", ",", "und", "zeig\u00b7te", "sie", "wahr", ",", "a\u00b7ber", "mit", "W\u00fcr\u00b7de", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ADJD", "$,", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Denn mir schien's, als wolle der Mensch in erhabenen Stunden", "tokens": ["Denn", "mir", "schien's", ",", "als", "wol\u00b7le", "der", "Mensch", "in", "er\u00b7ha\u00b7be\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ohne Kontrast anschaun gro\u00dfe Naturen allein.", "tokens": ["Oh\u00b7ne", "Kont\u00b7rast", "an\u00b7schaun", "gro\u00b7\u00dfe", "Na\u00b7tu\u00b7ren", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}