{"textgrid.poem.60364": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du schon je dich ganz allein gefunden,", "tokens": ["Hast", "du", "schon", "je", "dich", "ganz", "al\u00b7lein", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lieblos und ohne Gott auf einer Heide,", "tokens": ["Lieb\u00b7los", "und", "oh\u00b7ne", "Gott", "auf", "ei\u00b7ner", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wunden schn\u00f6den Mi\u00dfgeschicks verbunden", "tokens": ["Die", "Wun\u00b7den", "schn\u00f6\u00b7den", "Mi\u00df\u00b7ge\u00b7schicks", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit stolzer Stille, zornig dumpfem Leide?", "tokens": ["Mit", "stol\u00b7zer", "Stil\u00b7le", ",", "zor\u00b7nig", "dum\u00b7pfem", "Lei\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "War jede frohe Hoffnung dir entschwunden,", "tokens": ["War", "je\u00b7de", "fro\u00b7he", "Hoff\u00b7nung", "dir", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie einem J\u00e4ger an der Bergesscheide", "tokens": ["Wie", "ei\u00b7nem", "J\u00e4\u00b7ger", "an", "der", "Ber\u00b7ges\u00b7schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stirbt das Gebell von den verlornen Hunden,", "tokens": ["Stirbt", "das", "Ge\u00b7bell", "von", "den", "ver\u00b7lor\u00b7nen", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie's V\u00f6glein zieht, da\u00df es den Winter meide?", "tokens": ["Wie's", "V\u00f6\u00b7glein", "zieht", ",", "da\u00df", "es", "den", "Win\u00b7ter", "mei\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Warst du auf einer Heide ", "tokens": ["Warst", "du", "auf", "ei\u00b7ner", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wei\u00dft du auch, wie's einen dann bezwingt,", "tokens": ["So", "wei\u00dft", "du", "auch", ",", "wie's", "ei\u00b7nen", "dann", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er umarmend st\u00fcrzt an einen Stein;", "tokens": ["Da\u00df", "er", "um\u00b7ar\u00b7mend", "st\u00fcrzt", "an", "ei\u00b7nen", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df er, von seiner Einsamkeit erschreckt,", "tokens": ["Da\u00df", "er", ",", "von", "sei\u00b7ner", "Ein\u00b7sam\u00b7keit", "er\u00b7schreckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entsetzt empor vom starren Felsen springt", "tokens": ["Ent\u00b7setzt", "em\u00b7por", "vom", "star\u00b7ren", "Fel\u00b7sen", "springt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bang dem Winde nach die Arme streckt.", "tokens": ["Und", "bang", "dem", "Win\u00b7de", "nach", "die", "Ar\u00b7me", "streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}