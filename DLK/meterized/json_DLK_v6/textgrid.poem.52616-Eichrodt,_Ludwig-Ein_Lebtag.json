{"textgrid.poem.52616": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lebtag", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir lagern in dem gr\u00fcnen Gras,", "tokens": ["Wir", "la\u00b7gern", "in", "dem", "gr\u00fc\u00b7nen", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir ruhn im goldnen Sonnenschein,", "tokens": ["Wir", "ruhn", "im", "gold\u00b7nen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es blitzt der Wein im gr\u00fcnen Glas,", "tokens": ["Es", "blitzt", "der", "Wein", "im", "gr\u00fc\u00b7nen", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es blitzt vom Aug der goldne Wein.", "tokens": ["Es", "blitzt", "vom", "Aug", "der", "gold\u00b7ne", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Abwechselnd K\u00fcsse geben", "tokens": ["Ab\u00b7wech\u00b7selnd", "K\u00fcs\u00b7se", "ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Dem Liebchen und den Reben,", "tokens": ["Dem", "Lieb\u00b7chen", "und", "den", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Herzen und die Becher voll,", "tokens": ["Die", "Her\u00b7zen", "und", "die", "Be\u00b7cher", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ja \u2013 ja das hei\u00df ich Leben!", "tokens": ["Ja", "\u2013", "ja", "das", "hei\u00df", "ich", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADV", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Uns blaut der Himmel ins Gesicht,", "tokens": ["Uns", "blaut", "der", "Him\u00b7mel", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der laute Strom erquickt das Ohr,", "tokens": ["Der", "lau\u00b7te", "Strom", "er\u00b7quickt", "das", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns malt der Ferne Zauberlicht", "tokens": ["Uns", "malt", "der", "Fer\u00b7ne", "Zau\u00b7ber\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar wunderselge Tr\u00e4ume vor.", "tokens": ["Gar", "wun\u00b7der\u00b7sel\u00b7ge", "Tr\u00e4u\u00b7me", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So Arm in Arm geschlungen,", "tokens": ["So", "Arm", "in", "Arm", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Lieb und Wein durchklungen,", "tokens": ["Von", "Lieb", "und", "Wein", "durch\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sei durch den gr\u00fcnen hallenden Wald", "tokens": ["Sei", "durch", "den", "gr\u00fc\u00b7nen", "hal\u00b7len\u00b7den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der Freude Preis gesungen.", "tokens": ["Der", "Freu\u00b7de", "Preis", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt ihr das milde S\u00e4useln schon?", "tokens": ["H\u00f6rt", "ihr", "das", "mil\u00b7de", "S\u00e4u\u00b7seln", "schon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es k\u00fcndet schattenvolle Ruh,", "tokens": ["Es", "k\u00fcn\u00b7det", "schat\u00b7ten\u00b7vol\u00b7le", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wispert durch die Buchenkron", "tokens": ["Es", "wis\u00b7pert", "durch", "die", "Bu\u00b7chen\u00b7kron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fc\u00dft im Gras die Blumen zu,", "tokens": ["Und", "k\u00fc\u00dft", "im", "Gras", "die", "Blu\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es rauschet auf und nieder", "tokens": ["Es", "rau\u00b7schet", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie singendes Gefieder,", "tokens": ["Wie", "sin\u00b7gen\u00b7des", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das sind der Tagesk\u00f6nigin", "tokens": ["Das", "sind", "der", "Ta\u00b7ges\u00b7k\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schauers\u00fc\u00dfe Schlummerlieder.", "tokens": ["Schau\u00b7er\u00b7s\u00fc\u00b7\u00dfe", "Schlum\u00b7mer\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und vor uns gl\u00fcht der Himmel auf,", "tokens": ["Und", "vor", "uns", "gl\u00fcht", "der", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch wir sind rosig \u00fcberhaucht,", "tokens": ["Auch", "wir", "sind", "ro\u00b7sig", "\u00fc\u00b7ber\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vollendet ist der Sonnenlauf!", "tokens": ["Voll\u00b7en\u00b7det", "ist", "der", "Son\u00b7nen\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der gro\u00dfe Stern ins Meer getaucht.", "tokens": ["Der", "gro\u00b7\u00dfe", "Stern", "ins", "Meer", "ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die letzten Perlenfunken", "tokens": ["Die", "letz\u00b7ten", "Per\u00b7len\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Scheidegru\u00df getrunken!", "tokens": ["Zum", "Schei\u00b7de\u00b7gru\u00df", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcllt an, f\u00fcllt an, uns hat das Licht", "tokens": ["F\u00fcllt", "an", ",", "f\u00fcllt", "an", ",", "uns", "hat", "das", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Auf Wiedersehn gewunken.", "tokens": ["Auf", "Wie\u00b7der\u00b7sehn", "ge\u00b7wun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schaut um und auf! im tiefen Blau", "tokens": ["Schaut", "um", "und", "auf", "!", "im", "tie\u00b7fen", "Blau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der goldne Brachmond schwimmt heran,", "tokens": ["Der", "gold\u00b7ne", "Brach\u00b7mond", "schwimmt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wandelt durch die Sternenau", "tokens": ["Er", "wan\u00b7delt", "durch", "die", "Ster\u00b7ne\u00b7nau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So leuchtende verschwiegne Bahn.", "tokens": ["So", "leuch\u00b7ten\u00b7de", "ver\u00b7schwieg\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steht auf, steht auf, Genossen!", "tokens": ["Steht", "auf", ",", "steht", "auf", ",", "Ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Nacht ist ausgegossen \u2013", "tokens": ["Die", "Nacht", "ist", "aus\u00b7ge\u00b7gos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den k\u00fchnen W\u00fcnschen in der Brust", "tokens": ["Den", "k\u00fch\u00b7nen", "W\u00fcn\u00b7schen", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Herzen aufgeschlossen!", "tokens": ["Die", "Her\u00b7zen", "auf\u00b7ge\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}