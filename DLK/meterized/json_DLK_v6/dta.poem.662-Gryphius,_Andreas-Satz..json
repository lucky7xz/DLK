{"dta.poem.662": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.85", "vi:0.14"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Ephraim! ", "tokens": ["Ach", "Eph\u00b7raim", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ach ja! er ist es doch! er ist es den ich liebe!", "tokens": ["Ach", "ja", "!", "er", "ist", "es", "doch", "!", "er", "ist", "es", "den", "ich", "lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PPER", "VAFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PPER", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob ich jhn gleich sehr offt/ vmb seine schuldt betr\u00fcbe.", "tokens": ["Ob", "ich", "jhn", "gleich", "sehr", "offt", "/", "vmb", "sei\u00b7ne", "schuldt", "be\u00b7tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "$(", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob wol mein grim/", "tokens": ["Ob", "wol", "mein", "grim", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sich vber seine missethat erhitzt: ", "tokens": ["Sich", "vber", "sei\u00b7ne", "mis\u00b7se\u00b7that", "er\u00b7hitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ob er gleich zimblich vnsanfft jtzund sitzt;", "tokens": ["Ob", "er", "gleich", "zim\u00b7blich", "vn\u00b7sanfft", "jt\u00b7zund", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dennoch bleibt er meine wo\u00f1e/ v\u00f1 mein au\u00dferwehltes kindt:", "tokens": ["Den\u00b7noch", "bleibt", "er", "mei\u00b7ne", "wo\u00f1e", "/", "v\u00f1", "mein", "au\u00b7\u00dfer\u00b7wehl\u00b7tes", "kindt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Vnd mein Sohn/ der Hertz vnd Ohren/ nimmermehr ver-", "tokens": ["Vnd", "mein", "Sohn", "/", "der", "Hertz", "vnd", "Oh\u00b7ren", "/", "nim\u00b7mer\u00b7mehr", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "NN", "KON", "NN", "$(", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "schlossen findt.", "tokens": ["schlos\u00b7sen", "findt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}