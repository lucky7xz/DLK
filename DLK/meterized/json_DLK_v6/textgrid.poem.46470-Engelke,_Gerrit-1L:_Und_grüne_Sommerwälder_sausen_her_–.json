{"textgrid.poem.46470": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und gr\u00fcne Sommerw\u00e4lder sausen her \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und gr\u00fcne Sommerw\u00e4lder sausen her \u2013", "tokens": ["Und", "gr\u00fc\u00b7ne", "Som\u00b7mer\u00b7w\u00e4l\u00b7der", "sau\u00b7sen", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Menschheit wimmelt froh in Wegen \u2013", "tokens": ["Und", "Menschheit", "wim\u00b7melt", "froh", "in", "We\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "NN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wolken baden tief im Blau \u2013", "tokens": ["Wol\u00b7ken", "ba\u00b7den", "tief", "im", "Blau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, alles mir entgegen!", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "mir", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonnevoll, strombeklommen \u2013", "tokens": ["Son\u00b7ne\u00b7voll", ",", "strom\u00b7be\u00b7klom\u00b7men", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ach Ich!", "tokens": ["Ach", "Ich", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Mir schaukelt Leben zu vom Strand, vom Kahn \u2013", "tokens": ["Mir", "schau\u00b7kelt", "Le\u00b7ben", "zu", "vom", "Strand", ",", "vom", "Kahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "APPRART", "NN", "$,", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu mir kommt Flu\u00df, und Flu\u00df und See und Meer \u2013", "tokens": ["Zu", "mir", "kommt", "Flu\u00df", ",", "und", "Flu\u00df", "und", "See", "und", "Meer", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "$,", "KON", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schiffsgewimmel \u2013 Qualmh\u00e4fen \u2013 Ozean \u2013", "tokens": ["Schiffs\u00b7ge\u00b7wim\u00b7mel", "\u2013", "Qualm\u00b7h\u00e4\u00b7fen", "\u2013", "O\u00b7ze\u00b7an", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NE", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Immer mehr, immer mehr!", "tokens": ["Im\u00b7mer", "mehr", ",", "im\u00b7mer", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "O' Ich! O Ich!", "tokens": ["O'", "Ich", "!", "O", "Ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "$.", "NE", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und Nordlandk\u00fcsten \u2013 S\u00fcdseepalmenw\u00e4lder \u2013", "tokens": ["Und", "Nord\u00b7land\u00b7k\u00fcs\u00b7ten", "\u2013", "S\u00fcd\u00b7see\u00b7pal\u00b7men\u00b7w\u00e4l\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Sternschw\u00e4rme \u2013 Indiens Wunderst\u00e4dte \u2013", "tokens": ["Und", "Stern\u00b7schw\u00e4r\u00b7me", "\u2013", "In\u00b7di\u00b7ens", "Wun\u00b7der\u00b7st\u00e4d\u00b7te", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "NE", "NN", "$("], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und tausend Lichter aus der Welt", "tokens": ["Und", "tau\u00b7send", "Lich\u00b7ter", "aus", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Taumeln, rauschen in ungeheurem wilden Wirbel", "tokens": ["Tau\u00b7meln", ",", "rau\u00b7schen", "in", "un\u00b7ge\u00b7heu\u00b7rem", "wil\u00b7den", "Wir\u00b7bel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "In eine weite Seele!", "tokens": ["In", "ei\u00b7ne", "wei\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich! Ich!", "tokens": ["Ich", "!", "Ich", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PPER", "$.", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Meine Blicke sind Sterne \u2013 Flut braust im Ohr!", "tokens": ["Mei\u00b7ne", "Bli\u00b7cke", "sind", "Ster\u00b7ne", "\u2013", "Flut", "braust", "im", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$(", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und eine Seele gl\u00fcht weltdurchstr\u00f6mt und dr\u00f6hnt", "tokens": ["Und", "ei\u00b7ne", "See\u00b7le", "gl\u00fcht", "welt\u00b7durch\u00b7str\u00f6mt", "und", "dr\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und kann nicht d\u00e4mpfen den rasenden Chor \u2013", "tokens": ["Und", "kann", "nicht", "d\u00e4mp\u00b7fen", "den", "ra\u00b7sen\u00b7den", "Chor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich berste \u2013 ersticke \u2013 blicke", "tokens": ["Ich", "bers\u00b7te", "\u2013", "er\u00b7sti\u00b7cke", "\u2013", "bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hoch:", "tokens": ["Hoch", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Der du Alles ins Dasein rei\u00dft,", "tokens": ["Der", "du", "Al\u00b7les", "ins", "Da\u00b7sein", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "L\u00f6se, dunkel durchschwingender Lebensgeist,", "tokens": ["L\u00f6\u00b7se", ",", "dun\u00b7kel", "durch\u00b7schwin\u00b7gen\u00b7der", "Le\u00b7bens\u00b7geist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aus dem l\u00e4hmenden Drange zu metallischem Klang,", "tokens": ["Aus", "dem", "l\u00e4h\u00b7men\u00b7den", "Dran\u00b7ge", "zu", "me\u00b7tal\u00b7li\u00b7schem", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aus dem chaotischen Orgeln zum Weltgesang", "tokens": ["Aus", "dem", "chao\u00b7ti\u00b7schen", "Or\u00b7geln", "zum", "Welt\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Die lebensbest\u00fcrzte Seele", "tokens": ["Die", "le\u00b7bens\u00b7be\u00b7st\u00fcrz\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "F\u00fcr Alles und Alle!", "tokens": ["F\u00fcr", "Al\u00b7les", "und", "Al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}