{"dta.poem.7951": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "In Varnhagens Stammbuch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Als Ph\u00f6bus stark mit Mauern, Th\u00fcrmen, Gittern", "tokens": ["Als", "Ph\u00f6\u00b7bus", "stark", "mit", "Mau\u00b7ern", ",", "Th\u00fcr\u00b7men", ",", "Git\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "NE", "ADJD", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die K\u00f6nigsburg von Nisa half bereiten,", "tokens": ["Die", "K\u00f6\u00b7nigs\u00b7burg", "von", "Ni\u00b7sa", "half", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da legt\u2019 er seiner Lyra goldne Saiten", "tokens": ["Da", "legt'", "er", "sei\u00b7ner", "Ly\u00b7ra", "gold\u00b7ne", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einen Mauerstein mit leisem Sch\u00fcttern.", "tokens": ["Auf", "ei\u00b7nen", "Mau\u00b7er\u00b7stein", "mit", "lei\u00b7sem", "Sch\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Zinne konnte nicht so sehr verwittern,", "tokens": ["Die", "Zin\u00b7ne", "konn\u00b7te", "nicht", "so", "sehr", "ver\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df nicht den Marmor noch in sp\u00e4ten Zeiten,", "tokens": ["Da\u00df", "nicht", "den", "Mar\u00b7mor", "noch", "in", "sp\u00e4\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst bei des Fingers leichtem Dr\u00fcbergleiten,", "tokens": ["Selbst", "bei", "des", "Fin\u00b7gers", "leich\u00b7tem", "Dr\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Durchklungen h\u00e4tt\u2019 ein sanft melodisch Zittern.", "tokens": ["Durch\u00b7klun\u00b7gen", "h\u00e4tt'", "ein", "sanft", "me\u00b7lo\u00b7disch", "Zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So legt\u2019 auch ich auf dies Ged\u00e4chtni\u00dfblatt,", "tokens": ["So", "legt'", "auch", "ich", "auf", "dies", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das du wohl \u00f6fters, bl\u00e4tternd, wirst ber\u00fchren,", "tokens": ["Das", "du", "wohl", "\u00f6f\u00b7ters", ",", "bl\u00e4t\u00b7ternd", ",", "wirst", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "$,", "VVPP", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Saitenspiel, auch gab es einen Ton:", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", ",", "auch", "gab", "es", "ei\u00b7nen", "Ton", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dennoch zweifl\u2019 ich, ob an dieser Statt", "tokens": ["Und", "den\u00b7noch", "zweifl'", "ich", ",", "ob", "an", "die\u00b7ser", "Statt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du jemals einen Nachklang werdest sp\u00fcren,", "tokens": ["Du", "je\u00b7mals", "ei\u00b7nen", "Nach\u00b7klang", "wer\u00b7dest", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ich bin Ph\u00f6bus nicht, noch Ph\u00f6bus Sohn.", "tokens": ["Denn", "ich", "bin", "Ph\u00f6\u00b7bus", "nicht", ",", "noch", "Ph\u00f6\u00b7bus", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NE", "PTKNEG", "$,", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}