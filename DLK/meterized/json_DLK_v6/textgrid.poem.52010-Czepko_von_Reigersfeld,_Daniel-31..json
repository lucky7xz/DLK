{"textgrid.poem.52010": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die zwischen innen dort ein junges Mensch erkannt,", "tokens": ["Die", "zwi\u00b7schen", "in\u00b7nen", "dort", "ein", "jun\u00b7ges", "Mensch", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hin in ihren Schoos mit ihren H\u00e4nden rei\u00dfen,", "tokens": ["Und", "hin", "in", "ih\u00b7ren", "Schoos", "mit", "ih\u00b7ren", "H\u00e4n\u00b7den", "rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan ich als Landsleut euch ihr Spie\u00df Gesellen weisen,", "tokens": ["Kan", "ich", "als", "Lands\u00b7leut", "euch", "ihr", "Spie\u00df", "Ge\u00b7sel\u00b7len", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "PPER", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn schauet, beyde ziehn hin in ihr Vaterland.", "tokens": ["Denn", "schau\u00b7et", ",", "bey\u00b7de", "ziehn", "hin", "in", "ihr", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}