{"dta.poem.11242": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die wohlfeilen k\u00e4mme.  \n U. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jns k\u00fcnfftige wird man die k\u00e4mme wohlfeil kauffen,", "tokens": ["Ins", "k\u00fcnff\u00b7ti\u00b7ge", "wird", "man", "die", "k\u00e4m\u00b7me", "wohl\u00b7feil", "kauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Weil man die h\u00f6rner sieht auf allen gassen lauffen.", "tokens": ["Weil", "man", "die", "h\u00f6r\u00b7ner", "sieht", "auf", "al\u00b7len", "gas\u00b7sen", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}