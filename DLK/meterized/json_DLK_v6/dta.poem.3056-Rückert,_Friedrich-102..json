{"dta.poem.3056": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du solang erhofft, wann es nun endlich kam,", "tokens": ["Was", "du", "so\u00b7lang", "er\u00b7hofft", ",", "wann", "es", "nun", "end\u00b7lich", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "VVPP", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie schnell ist es vorbei, und ewig bleibt der Gram,", "tokens": ["Wie", "schnell", "ist", "es", "vor\u00b7bei", ",", "und", "e\u00b7wig", "bleibt", "der", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df es nie wieder kommt, weil's da nun einmal war;", "tokens": ["Da\u00df", "es", "nie", "wie\u00b7der", "kommt", ",", "weil's", "da", "nun", "ein\u00b7mal", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PWS", "ADV", "ADV", "ADV", "VAFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Doch sterbend l\u00e4\u00dft es dir ein Kind, das es gebar:", "tokens": ["Doch", "ster\u00b7bend", "l\u00e4\u00dft", "es", "dir", "ein", "Kind", ",", "das", "es", "ge\u00b7bar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein neues Hoffen, das zu seiner Zeit gebiert", "tokens": ["Ein", "neu\u00b7es", "Hof\u00b7fen", ",", "das", "zu", "sei\u00b7ner", "Zeit", "ge\u00b7biert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein neues wieder und sein Leben dran verliert.", "tokens": ["Ein", "neu\u00b7es", "wie\u00b7der", "und", "sein", "Le\u00b7ben", "dran", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "KON", "PPOSAT", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das sind die Hoffnungen, verloren wie geboren,", "tokens": ["Das", "sind", "die", "Hoff\u00b7nun\u00b7gen", ",", "ver\u00b7lo\u00b7ren", "wie", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VVPP", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die uns unvermerkt das Leben geht verloren.", "tokens": ["Durch", "die", "uns", "un\u00b7ver\u00b7merkt", "das", "Le\u00b7ben", "geht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das sind die Hoffnungen, geboren wie verloren,", "tokens": ["Das", "sind", "die", "Hoff\u00b7nun\u00b7gen", ",", "ge\u00b7bo\u00b7ren", "wie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VVPP", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die das Menschenherz ist immer neugeboren.", "tokens": ["Durch", "die", "das", "Men\u00b7schen\u00b7herz", "ist", "im\u00b7mer", "neu\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}