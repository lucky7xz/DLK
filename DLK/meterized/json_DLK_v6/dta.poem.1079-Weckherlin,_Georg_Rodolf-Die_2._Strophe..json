{"dta.poem.1079": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 2. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie ein Kaufman/ dessen Sin", "tokens": ["Gleich", "wie", "ein", "Kauf\u00b7man", "/", "des\u00b7sen", "Sin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$(", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach anders nichts dan reichthumb", "tokens": ["Nach", "an\u00b7ders", "nichts", "dan", "reicht\u00b7humb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIS", "ADV", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "trachtet/", "tokens": ["trach\u00b7tet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Das er alle arbeit verachtet/", "tokens": ["Das", "er", "al\u00b7le", "ar\u00b7beit", "ver\u00b7ach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Halb verblindet durch den gewin/", "tokens": ["Halb", "ver\u00b7blin\u00b7det", "durch", "den", "ge\u00b7win", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wan er nach vil m\u00fch vnd gefahr", "tokens": ["Wan", "er", "nach", "vil", "m\u00fch", "vnd", "ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADV", "ADJD", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Kan das erw\u00fcnschte gestad gr\u00fcssen/", "tokens": ["Kan", "das", "er\u00b7w\u00fcnschte", "ge\u00b7stad", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VVFIN", "ADJD", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Vnd numehr mit k\u00f6stlicher wahr", "tokens": ["Vnd", "nu\u00b7mehr", "mit", "k\u00f6st\u00b7li\u00b7cher", "wahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Seinen geitzigen hunger b\u00fcssen/", "tokens": ["Sei\u00b7nen", "geit\u00b7zi\u00b7gen", "hun\u00b7ger", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Da in der kleinoten vnzahl", "tokens": ["Da", "in", "der", "klei\u00b7no\u00b7ten", "vn\u00b7zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Sein aug vnd gem\u00fcht sich bew\u00f6gen/", "tokens": ["Sein", "aug", "vnd", "ge\u00b7m\u00fcht", "sich", "be\u00b7w\u00f6\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVPP", "PRF", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Das Er nicht waist/ arm in der wahl/", "tokens": ["Das", "Er", "nicht", "waist", "/", "arm", "in", "der", "wahl", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVFIN", "$(", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Was zu nemen/ was weg zul\u00f6gen.", "tokens": ["Was", "zu", "ne\u00b7men", "/", "was", "weg", "zu\u00b7l\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "$(", "PWS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}