{"dta.poem.11171": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den tapffern Churf\u00fcrsten  \n Johann George den  III.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist der dritte printz von drey Johann Georgen,", "tokens": ["Du", "bist", "der", "drit\u00b7te", "printz", "von", "drey", "Jo\u00b7hann", "Ge\u00b7or\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "CARD", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und darffst von keinem nichts, als nur den nahmen, borgen.", "tokens": ["Und", "darffst", "von", "kei\u00b7nem", "nichts", ",", "als", "nur", "den", "nah\u00b7men", ",", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "APPR", "PIAT", "PIS", "$,", "KOUS", "ADV", "ART", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer deine thaten liest, der stimmt gar gerne bey,", "tokens": ["Wer", "dei\u00b7ne", "tha\u00b7ten", "liest", ",", "der", "stimmt", "gar", "ger\u00b7ne", "bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "VVFIN", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Wittekind vor dir ein bloses kind nur sey.", "tokens": ["Da\u00df", "Wit\u00b7te\u00b7kind", "vor", "dir", "ein", "blo\u00b7ses", "kind", "nur", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "w