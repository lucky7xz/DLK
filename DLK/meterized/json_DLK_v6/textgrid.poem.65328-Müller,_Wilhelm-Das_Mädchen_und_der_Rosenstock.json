{"textgrid.poem.65328": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen und der Rosenstock", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei M\u00fctterchen, warum giebst du denn nimmer", "tokens": ["Ei", "M\u00fct\u00b7ter\u00b7chen", ",", "wa\u00b7rum", "giebst", "du", "denn", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+-+-+++", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dein sch\u00f6nes Kind heraus aus deiner Kammer?", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Kind", "he\u00b7raus", "aus", "dei\u00b7ner", "Kam\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es spinnt doch lange schon am Hochzeitlinnen", "tokens": ["Es", "spinnt", "doch", "lan\u00b7ge", "schon", "am", "Hoch\u00b7zeit\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4dchen ist ja auch kein Rosenst\u00f6ckchen,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "ist", "ja", "auch", "kein", "Ro\u00b7sen\u00b7st\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das man so lange stellt in's Fenstereckchen,", "tokens": ["Das", "man", "so", "lan\u00b7ge", "stellt", "in's", "Fens\u00b7ter\u00b7eck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis aufgebrochen sind die letzten Kn\u00f6spchen.", "tokens": ["Bis", "auf\u00b7ge\u00b7bro\u00b7chen", "sind", "die", "letz\u00b7ten", "Kn\u00f6spc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gelt, m\u00f6chtest wohl hier hinter deinen Scheiben", "tokens": ["Gelt", ",", "m\u00f6ch\u00b7test", "wohl", "hier", "hin\u00b7ter", "dei\u00b7nen", "Schei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sch\u00f6ne Stubenbl\u00fcmchen immer haben,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Stu\u00b7ben\u00b7bl\u00fcm\u00b7chen", "im\u00b7mer", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit dein Fenster h\u00fcbsch in Ehren bleibe?", "tokens": ["Da\u00b7mit", "dein", "Fens\u00b7ter", "h\u00fcbsch", "in", "Eh\u00b7ren", "blei\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}