{"textgrid.poem.29709": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "124.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutius ist eine Biene, fleucht herum auff allem s\u00fcssen;", "tokens": ["Mu\u00b7tius", "ist", "ei\u00b7ne", "Bie\u00b7ne", ",", "fleucht", "he\u00b7rum", "auff", "al\u00b7lem", "s\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "VVFIN", "PTKVZ", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ist nicht stoltz, was nur begegnet zu behertzen, zu bek\u00fcssen.", "tokens": ["Ist", "nicht", "stoltz", ",", "was", "nur", "be\u00b7geg\u00b7net", "zu", "be\u00b7hert\u00b7zen", ",", "zu", "be\u00b7k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,", "PRELS", "ADV", "VVPP", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}