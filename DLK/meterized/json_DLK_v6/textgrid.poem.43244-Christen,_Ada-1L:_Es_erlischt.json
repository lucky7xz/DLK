{"textgrid.poem.43244": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es erlischt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es erlischt", "tokens": ["Es", "er\u00b7lischt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Jeder Schmerz,", "tokens": ["Je\u00b7der", "Schmerz", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Jedes Leid", "tokens": ["Je\u00b7des", "Leid"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ist hinweggewischt,", "tokens": ["Ist", "hin\u00b7weg\u00b7ge\u00b7wischt", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der Erde Gew\u00fchl,", "tokens": ["Der", "Er\u00b7de", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Welt versinkt", "tokens": ["Die", "Welt", "ver\u00b7sinkt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In Einem Gef\u00fchl", "tokens": ["In", "Ei\u00b7nem", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Der Seligkeit ...", "tokens": ["Der", "Se\u00b7lig\u00b7keit", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Jeder Blick, er trinkt", "tokens": ["Je\u00b7der", "Blick", ",", "er", "trinkt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Die rauschende Zeit,", "tokens": ["Die", "rau\u00b7schen\u00b7de", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Wenn Auge und Herz", "tokens": ["Wenn", "Au\u00b7ge", "und", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "An dem Zeiger h\u00e4ngt", "tokens": ["An", "dem", "Zei\u00b7ger", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.13": {"text": "Und alles Leben", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Zusammendr\u00e4ngt", "tokens": ["Zu\u00b7sam\u00b7men\u00b7dr\u00e4ngt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "In den Einen", "tokens": ["In", "den", "Ei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Einzigen Gedanken:", "tokens": ["Ein\u00b7zi\u00b7gen", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Bald ist er da! ...", "tokens": ["Bald", "ist", "er", "da", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}