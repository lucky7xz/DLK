{"textgrid.poem.46949": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr deutschen W\u00e4lder rauscht in euren Frischen,", "tokens": ["Ihr", "deut\u00b7schen", "W\u00e4l\u00b7der", "rauscht", "in", "eu\u00b7ren", "Fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sch\u00fcttelt eure Locken unverwirret;", "tokens": ["Und", "sch\u00fct\u00b7telt", "eu\u00b7re", "Lo\u00b7cken", "un\u00b7ver\u00b7wir\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Taub' ist's, die in euren Schatten girret;", "tokens": ["Die", "Taub'", "ist's", ",", "die", "in", "eu\u00b7ren", "Schat\u00b7ten", "gir\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Geier, der sie scheucht, hat ausgekrischen.", "tokens": ["Der", "Gei\u00b7er", ",", "der", "sie", "scheucht", ",", "hat", "aus\u00b7ge\u00b7kri\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ihr, o deutsche Str\u00f6me, braust dazwischen;", "tokens": ["Und", "ihr", ",", "o", "deut\u00b7sche", "Str\u00f6\u00b7me", ",", "braust", "da\u00b7zwi\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "$,", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr d\u00fcrft die Silbergleise ungeirret", "tokens": ["Ihr", "d\u00fcrft", "die", "Sil\u00b7berg\u00b7lei\u00b7se", "un\u00b7ge\u00b7ir\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nun wieder ziehn; die Rosse sind entschirret,", "tokens": ["Nun", "wie\u00b7der", "ziehn", ";", "die", "Ros\u00b7se", "sind", "ent\u00b7schir\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die streitig machten eure Flut den Fischen.", "tokens": ["Die", "strei\u00b7tig", "mach\u00b7ten", "eu\u00b7re", "Flut", "den", "Fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr deutschen Auen, k\u00fcnftig unzertreten,", "tokens": ["Ihr", "deut\u00b7schen", "Au\u00b7en", ",", "k\u00fcnf\u00b7tig", "un\u00b7zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ihr sollt jetzt Scharen tragen dichter \u00c4hren,", "tokens": ["Ihr", "sollt", "jetzt", "Scha\u00b7ren", "tra\u00b7gen", "dich\u00b7ter", "\u00c4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nicht starre Saaten mehr von Speer und Spie\u00dfen;", "tokens": ["Nicht", "star\u00b7re", "Saa\u00b7ten", "mehr", "von", "Speer", "und", "Spie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und nicht der Tod als Schnitter sei gebeten,", "tokens": ["Und", "nicht", "der", "Tod", "als", "Schnit\u00b7ter", "sei", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "KOUS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und nicht die Ernte soll von Blut und Z\u00e4hren,", "tokens": ["Und", "nicht", "die", "Ern\u00b7te", "soll", "von", "Blut", "und", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Vom Tau des Friedens soll sie \u00fcberflie\u00dfen.", "tokens": ["Vom", "Tau", "des", "Frie\u00b7dens", "soll", "sie", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}