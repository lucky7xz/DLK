{"textgrid.poem.40504": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon gl\u00e4nzt der Mond im Meeresplan", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon gl\u00e4nzt der Mond im Meeresplan", "tokens": ["Schon", "gl\u00e4nzt", "der", "Mond", "im", "Mee\u00b7res\u00b7plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch fern ist das Schiff vom Hafen!", "tokens": ["Noch", "fern", "ist", "das", "Schiff", "vom", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Mitternacht bricht m\u00e4hlich an,", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "bricht", "m\u00e4h\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Passagiere schlafen.", "tokens": ["Die", "Pas\u00b7sa\u00b7gie\u00b7re", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Wacht am Maste schielt hinein", "tokens": ["Die", "Wacht", "am", "Mas\u00b7te", "schielt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Mond und Sternenkreise,", "tokens": ["In", "Mond", "und", "Ster\u00b7nen\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis \u00fcberblendet vom Strahlenschein", "tokens": ["Bis", "\u00fc\u00b7berb\u00b7len\u00b7det", "vom", "Strah\u00b7len\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPRART", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Das Aug' sich geschlossen leise.", "tokens": ["Das", "Aug'", "sich", "ge\u00b7schlos\u00b7sen", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Steuermann belauscht zuviel", "tokens": ["Der", "Steu\u00b7er\u00b7mann", "be\u00b7lauscht", "zu\u00b7viel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Meeres Pl\u00e4tschern und Klingen,", "tokens": ["Des", "Mee\u00b7res", "Pl\u00e4t\u00b7schern", "und", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis ihn die Wellen mit listigem Spiel", "tokens": ["Bis", "ihn", "die", "Wel\u00b7len", "mit", "lis\u00b7ti\u00b7gem", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "In Schlummer hin\u00fcbersingen.", "tokens": ["In", "Schlum\u00b7mer", "hin\u00b7\u00fc\u00b7ber\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Kapit\u00e4n guckt auch zu tief", "tokens": ["Der", "Ka\u00b7pi\u00b7t\u00e4n", "guckt", "auch", "zu", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Glas nach Ankergr\u00fcnden,", "tokens": ["Ins", "Glas", "nach", "An\u00b7ker\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis er ganz sanft im Herrn entschlief,", "tokens": ["Bis", "er", "ganz", "sanft", "im", "Herrn", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bevor er sie konnte finden.", "tokens": ["Be\u00b7vor", "er", "sie", "konn\u00b7te", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Weh dir, verlass'nes armes Schiff!", "tokens": ["Weh", "dir", ",", "ver\u00b7lass'\u00b7nes", "ar\u00b7mes", "Schiff", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weh allen Passagieren!", "tokens": ["Weh", "al\u00b7len", "Pas\u00b7sa\u00b7gie\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer wird durch Sandbank, Sturm und Riff", "tokens": ["Wer", "wird", "durch", "Sand\u00b7bank", ",", "Sturm", "und", "Riff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch nun zum Hafen f\u00fchren?", "tokens": ["Euch", "nun", "zum", "Ha\u00b7fen", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da nahm eine lose Welle das Wort:", "tokens": ["Da", "nahm", "ei\u00b7ne", "lo\u00b7se", "Wel\u00b7le", "das", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Schwestern, was kann's verschlagen!", "tokens": ["Ihr", "Schwes\u00b7tern", ",", "was", "kann's", "ver\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir schieben zum Spa\u00df am Schifflein fort,", "tokens": ["Wir", "schie\u00b7ben", "zum", "Spa\u00df", "am", "Schif\u00b7flein", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "La\u00dft sehn, wie weit wir's tragen?", "tokens": ["La\u00dft", "sehn", ",", "wie", "weit", "wir's", "tra\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PWAV", "ADJD", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da dachte Boreas: Fast ist's Zeit,", "tokens": ["Da", "dach\u00b7te", "Bo\u00b7re\u00b7as", ":", "Fast", "ist's", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$.", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu ruhn von dem vielen Bewegen!", "tokens": ["Zu", "ruhn", "von", "dem", "vie\u00b7len", "Be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Will mich einmal gem\u00e4chlich breit", "tokens": ["Will", "mich", "ein\u00b7mal", "ge\u00b7m\u00e4ch\u00b7lich", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Rast in die Segel legen.", "tokens": ["Zur", "Rast", "in", "die", "Se\u00b7gel", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Hei, wie das Schiff durch die Fluthen scho\u00df,", "tokens": ["Hei", ",", "wie", "das", "Schiff", "durch", "die", "Flut\u00b7hen", "scho\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Getrieben von Wind und Wellen!", "tokens": ["Ge\u00b7trie\u00b7ben", "von", "Wind", "und", "Wel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch weh, nun geht's auf den Felsen los,", "tokens": ["Doch", "weh", ",", "nun", "geht's", "auf", "den", "Fel\u00b7sen", "los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hilf Gott, nun mu\u00df es zerschellen!", "tokens": ["Hilf", "Gott", ",", "nun", "mu\u00df", "es", "zer\u00b7schel\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Den Blinden und Lahmen im Wege pflegt", "tokens": ["Den", "Blin\u00b7den", "und", "Lah\u00b7men", "im", "We\u00b7ge", "pflegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu weichen ein Mann von Sitte!", "tokens": ["Zu", "wei\u00b7chen", "ein", "Mann", "von", "Sit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So denkt der Felsen und bewegt", "tokens": ["So", "denkt", "der", "Fel\u00b7sen", "und", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck sich um sechs Schritte.", "tokens": ["Zu\u00b7r\u00fcck", "sich", "um", "sechs", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vorbei das Schiff durch die Fluthen scho\u00df,", "tokens": ["Vor\u00b7bei", "das", "Schiff", "durch", "die", "Flut\u00b7hen", "scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Getrieben von Wind und Wellen;", "tokens": ["Ge\u00b7trie\u00b7ben", "von", "Wind", "und", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch nun geht's grad' auf den Hafen los,", "tokens": ["Doch", "nun", "geht's", "grad'", "auf", "den", "Ha\u00b7fen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nun wird's an der K\u00fcste zerschellen!", "tokens": ["Nun", "wird's", "an", "der", "K\u00fcs\u00b7te", "zer\u00b7schel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Den Ankern ward es zeitlang fast,", "tokens": ["Den", "An\u00b7kern", "ward", "es", "zeit\u00b7lang", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die m\u00fc\u00dfig am Borde hingen;", "tokens": ["Die", "m\u00fc\u00b7\u00dfig", "am", "Bor\u00b7de", "hin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sagte einer: Ihr Br\u00fcder, la\u00dft", "tokens": ["Da", "sag\u00b7te", "ei\u00b7ner", ":", "Ihr", "Br\u00fc\u00b7der", ",", "la\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "$.", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Bad' ins Meer uns springen!", "tokens": ["Zum", "Bad'", "ins", "Meer", "uns", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Gesagt, gethan! Er h\u00fcpft vom Bord!", "tokens": ["Ge\u00b7sagt", ",", "ge\u00b7than", "!", "Er", "h\u00fcpft", "vom", "Bord", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Volk im Schiff erwachte;", "tokens": ["Das", "Volk", "im", "Schiff", "er\u00b7wach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lagen vor Anker mitten im Port!", "tokens": ["Sie", "la\u00b7gen", "vor", "An\u00b7ker", "mit\u00b7ten", "im", "Port", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "APPRART", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie freundlich das Ufer lachte!", "tokens": ["Wie", "freund\u00b7lich", "das", "U\u00b7fer", "lach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Sie stiegen ans Land, gar inniglich", "tokens": ["Sie", "stie\u00b7gen", "ans", "Land", ",", "gar", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entz\u00fcckt von des Schiffs Regierern.", "tokens": ["Ent\u00b7z\u00fcckt", "von", "des", "Schiffs", "Re\u00b7gie\u00b7rern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gott wolle meine Freund' und mich", "tokens": ["Gott", "wol\u00b7le", "mei\u00b7ne", "Freund'", "und", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bewahren vor solchen F\u00fchrern!", "tokens": ["Be\u00b7wah\u00b7ren", "vor", "sol\u00b7chen", "F\u00fch\u00b7rern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Doch woll' er meinen Freunden und mir", "tokens": ["Doch", "woll'", "er", "mei\u00b7nen", "Freun\u00b7den", "und", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "KON", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Solche Wellen und Winde geben,", "tokens": ["Sol\u00b7che", "Wel\u00b7len", "und", "Win\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und solche Felsen und Anker daf\u00fcr,", "tokens": ["Und", "sol\u00b7che", "Fel\u00b7sen", "und", "An\u00b7ker", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zur See und auch im Leben!", "tokens": ["Zur", "See", "und", "auch", "im", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}