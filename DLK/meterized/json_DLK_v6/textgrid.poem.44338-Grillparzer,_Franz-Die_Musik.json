{"textgrid.poem.44338": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die Musik", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, o K\u00f6nigin!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "o", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der strahlenden Herrscherstirn,", "tokens": ["Mit", "der", "strah\u00b7len\u00b7den", "Herr\u00b7schers\u00b7tirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit dem lieblich t\u00f6nenden Munde", "tokens": ["Mit", "dem", "lieb\u00b7lich", "t\u00f6\u00b7nen\u00b7den", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und dem Wahnsinn spr\u00fchenden Blick,", "tokens": ["Und", "dem", "Wahn\u00b7sinn", "spr\u00fc\u00b7hen\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Schwingend das zarte Plektron,", "tokens": ["Schwin\u00b7gend", "das", "zar\u00b7te", "Plek\u00b7tron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ein m\u00e4chtiger Szepter in deiner Hand.", "tokens": ["Ein", "m\u00e4ch\u00b7ti\u00b7ger", "Szep\u00b7ter", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Sei mir gegr\u00fc\u00dfet, Herrlichste", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "Herr\u00b7lichs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Unter den herrlichen Schwestern!", "tokens": ["Un\u00b7ter", "den", "herr\u00b7li\u00b7chen", "Schwes\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Lieblich sind sie, die Huldinnen alle,", "tokens": ["Lieb\u00b7lich", "sind", "sie", ",", "die", "Hul\u00b7din\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die am Throne des Lichts gezeugt,", "tokens": ["Die", "am", "Thro\u00b7ne", "des", "Lichts", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von unsterblichen M\u00fcttern geboren,", "tokens": ["Von", "uns\u00b7terb\u00b7li\u00b7chen", "M\u00fct\u00b7tern", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gerne nieder zur Erde steigen;", "tokens": ["Ger\u00b7ne", "nie\u00b7der", "zur", "Er\u00b7de", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Boten einer vergangenen,", "tokens": ["Bo\u00b7ten", "ei\u00b7ner", "ver\u00b7gan\u00b7ge\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Verk\u00fcnder einer k\u00fcnftigen Welt.", "tokens": ["Ver\u00b7k\u00fcn\u00b7der", "ei\u00b7ner", "k\u00fcnf\u00b7ti\u00b7gen", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Lieblich sind sie, die Huldinnen alle,", "tokens": ["Lieb\u00b7lich", "sind", "sie", ",", "die", "Hul\u00b7din\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn sie, der Sterblichkeit Nebelkleid", "tokens": ["Wenn", "sie", ",", "der", "Sterb\u00b7lich\u00b7keit", "Ne\u00b7bel\u00b7kleid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um die leuchtenden Schultern geworfen,", "tokens": ["Um", "die", "leuch\u00b7ten\u00b7den", "Schul\u00b7tern", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wie Apollon unter den Hirten", "tokens": ["Wie", "A\u00b7pol\u00b7lon", "un\u00b7ter", "den", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "In dem Kreise der Menschen weilen;", "tokens": ["In", "dem", "Krei\u00b7se", "der", "Men\u00b7schen", "wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und in der Fremde rauhen Boden", "tokens": ["Und", "in", "der", "Frem\u00b7de", "rau\u00b7hen", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Palmenreiser der Heimat pflanzen;", "tokens": ["Pal\u00b7men\u00b7rei\u00b7ser", "der", "Hei\u00b7mat", "pflan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Menschen \u00e4hnlich und dennoch G\u00f6tter,", "tokens": ["Men\u00b7schen", "\u00e4hn\u00b7lich", "und", "den\u00b7noch", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADV", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Beide Welten liebend verbinden,", "tokens": ["Bei\u00b7de", "Wel\u00b7ten", "lie\u00b7bend", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Hernieder zur Erde den Himmel ziehn", "tokens": ["Her\u00b7nie\u00b7der", "zur", "Er\u00b7de", "den", "Him\u00b7mel", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und den Menschen zu G\u00f6ttern erh\u00f6hn.", "tokens": ["Und", "den", "Men\u00b7schen", "zu", "G\u00f6t\u00b7tern", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Lieblich sind sie, die Huldinnen alle,", "tokens": ["Lieb\u00b7lich", "sind", "sie", ",", "die", "Hul\u00b7din\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch wie die Rose unter den Blumen", "tokens": ["Doch", "wie", "die", "Ro\u00b7se", "un\u00b7ter", "den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Strahlst du hervor aus dem Chore der Schwestern.", "tokens": ["Strahlst", "du", "her\u00b7vor", "aus", "dem", "Cho\u00b7re", "der", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Als das Recht von der Erde verschwunden,", "tokens": ["Als", "das", "Recht", "von", "der", "Er\u00b7de", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und die Unschuld gen Himmel geflohn,", "tokens": ["Und", "die", "Un\u00b7schuld", "gen", "Him\u00b7mel", "ge\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dienen lernte die freie Geb\u00e4rde,", "tokens": ["Die\u00b7nen", "lern\u00b7te", "die", "frei\u00b7e", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "L\u00fcgen das Aug, des Himmels Bild,", "tokens": ["L\u00fc\u00b7gen", "das", "Aug", ",", "des", "Him\u00b7mels", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und das Wort, das heilige, wahre,", "tokens": ["Und", "das", "Wort", ",", "das", "hei\u00b7li\u00b7ge", ",", "wah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sich in sch\u00e4ndende Fesseln schlug:", "tokens": ["Sich", "in", "sch\u00e4n\u00b7den\u00b7de", "Fes\u00b7seln", "schlug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Da wardst du von den G\u00f6ttern gesendet,", "tokens": ["Da", "wardst", "du", "von", "den", "G\u00f6t\u00b7tern", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als Vertraute f\u00fcr bessere Seelen,", "tokens": ["Als", "Ver\u00b7trau\u00b7te", "f\u00fcr", "bes\u00b7se\u00b7re", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Deine Sprach ihrem Munde zu leihn.", "tokens": ["Dei\u00b7ne", "Sprach", "ih\u00b7rem", "Mun\u00b7de", "zu", "leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Freudig eilten sie dir entgegen,", "tokens": ["Freu\u00b7dig", "eil\u00b7ten", "sie", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Sanken vertrauend in deinen Arm,", "tokens": ["San\u00b7ken", "ver\u00b7trau\u00b7end", "in", "dei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Und Lieb und Hoffnung, und Scham und Reue", "tokens": ["Und", "Lieb", "und", "Hoff\u00b7nung", ",", "und", "Scham", "und", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Fl\u00fcsterten leis in deinen Busen,", "tokens": ["Fl\u00fcs\u00b7ter\u00b7ten", "leis", "in", "dei\u00b7nen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Was sie erreicht und was sie verloren,", "tokens": ["Was", "sie", "er\u00b7reicht", "und", "was", "sie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Was sie getr\u00e4umt und wie sie gef\u00fchlt.", "tokens": ["Was", "sie", "ge\u00b7tr\u00e4umt", "und", "wie", "sie", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Seitdem stehst du dem Menschen zur Seite,", "tokens": ["Seit\u00b7dem", "stehst", "du", "dem", "Men\u00b7schen", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Eine helfende Tr\u00f6sterin!", "tokens": ["Ei\u00b7ne", "hel\u00b7fen\u00b7de", "Tr\u00f6s\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wo er weilt und wo er wandelt,", "tokens": ["Wo", "er", "weilt", "und", "wo", "er", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Ungl\u00fccks g\u00e4hnendem Absturz,", "tokens": ["An", "des", "Un\u00b7gl\u00fccks", "g\u00e4h\u00b7nen\u00b7dem", "Ab\u00b7sturz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf der Freude Blumenh\u00f6hn,", "tokens": ["Auf", "der", "Freu\u00b7de", "Blu\u00b7men\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcberall t\u00f6nt deine Stimm ihm entgegen,", "tokens": ["\u00dc\u00b7be\u00b7rall", "t\u00f6nt", "dei\u00b7ne", "Stimm", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wie ein Ruf aus besseren Welten,", "tokens": ["Wie", "ein", "Ruf", "aus", "bes\u00b7se\u00b7ren", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Klagend, tr\u00f6stend, freundlich erhebend,", "tokens": ["Kla\u00b7gend", ",", "tr\u00f6s\u00b7tend", ",", "freund\u00b7lich", "er\u00b7he\u00b7bend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Von der Wiege bis ins Grab.", "tokens": ["Von", "der", "Wie\u00b7ge", "bis", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sanft stehst du an der Wiege des Knaben,", "tokens": ["Sanft", "stehst", "du", "an", "der", "Wie\u00b7ge", "des", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-",