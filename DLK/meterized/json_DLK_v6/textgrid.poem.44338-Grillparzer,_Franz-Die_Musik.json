: "iambic.tetra"}, "line.6": {"text": "Was der Dichter zu laut verr\u00e4t,", "tokens": ["Was", "der", "Dich\u00b7ter", "zu", "laut", "ver\u00b7r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Lispelt vernehmlich dein Saitenspiel.", "tokens": ["Lis\u00b7pelt", "ver\u00b7nehm\u00b7lich", "dein", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Sei die Dichtkunst noch so gepriesen,", "tokens": ["Sei", "die", "Dicht\u00b7kunst", "noch", "so", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Sie spricht doch nur der Menschen Sprache,", "tokens": ["Sie", "spricht", "doch", "nur", "der", "Men\u00b7schen", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du sprichst, wie man im Himmel spricht!", "tokens": ["Du", "sprichst", ",", "wie", "man", "im", "Him\u00b7mel", "spricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Darum sei mir dreimal gesegnet,", "tokens": ["Da\u00b7rum", "sei", "mir", "drei\u00b7mal", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hohe, strahlende K\u00f6nigin!", "tokens": ["Ho\u00b7he", ",", "strah\u00b7len\u00b7de", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ewig soll meine Lippe dich preisen,", "tokens": ["E\u00b7wig", "soll", "mei\u00b7ne", "Lip\u00b7pe", "dich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und in den Klang meiner Weihges\u00e4nge", "tokens": ["Und", "in", "den", "Klang", "mei\u00b7ner", "Weih\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mische sich jauchzend der Jubel der Welt!", "tokens": ["Mi\u00b7sche", "sich", "jauch\u00b7zend", "der", "Ju\u00b7bel", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}