{"textgrid.poem.41954": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6re mich an! Still weil' ich im frostigen Hauch des Oktobers,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re mich an! Still weil' ich im frostigen Hauch des Oktobers,", "tokens": ["H\u00f6\u00b7re", "mich", "an", "!", "Still", "weil'", "ich", "im", "fros\u00b7ti\u00b7gen", "Hauch", "des", "Ok\u00b7to\u00b7bers", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Liebchen, vor deinem Gemach; \u00f6ffne mir freundlich das Haus.", "tokens": ["Lieb\u00b7chen", ",", "vor", "dei\u00b7nem", "Ge\u00b7mach", ";", "\u00f6ff\u00b7ne", "mir", "freund\u00b7lich", "das", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Liebchen, o h\u00f6rst du mich nicht? Umsonst! schon liegst du entschlummert,", "tokens": ["Lieb\u00b7chen", ",", "o", "h\u00f6rst", "du", "mich", "nicht", "?", "Um\u00b7sonst", "!", "schon", "liegst", "du", "ent\u00b7schlum\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "VVFIN", "PPER", "PRF", "PTKNEG", "$.", "NN", "$.", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+-++-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Und ein gaukelnder Traum k\u00fcsset dir friedlich die Stirn.", "tokens": ["Und", "ein", "gau\u00b7keln\u00b7der", "Traum", "k\u00fcs\u00b7set", "dir", "fried\u00b7lich", "die", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "O der gl\u00fcckliche Traum! Er darf dich sicher umflattern,", "tokens": ["O", "der", "gl\u00fcck\u00b7li\u00b7che", "Traum", "!", "Er", "darf", "dich", "si\u00b7cher", "um\u00b7flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und die entfesselte Brust g\u00f6nnt ihm das l\u00fcsterne Spiel.", "tokens": ["Und", "die", "ent\u00b7fes\u00b7sel\u00b7te", "Brust", "g\u00f6nnt", "ihm", "das", "l\u00fcs\u00b7ter\u00b7ne", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dankbar schenkt er daf\u00fcr dir die zartesten seiner Gebilde;", "tokens": ["Dank\u00b7bar", "schenkt", "er", "da\u00b7f\u00fcr", "dir", "die", "zar\u00b7tes\u00b7ten", "sei\u00b7ner", "Ge\u00b7bil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "PPER", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ich auch, wenn mir ein Gott l\u00e4chelte, nahe dir jetzt.", "tokens": ["Ich", "auch", ",", "wenn", "mir", "ein", "Gott", "l\u00e4\u00b7chel\u00b7te", ",", "na\u00b7he", "dir", "jetzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Schmeichelnd sprichst du es aus, was so oft auf der Wange die Scham mir", "tokens": ["Schmei\u00b7chelnd", "sprichst", "du", "es", "aus", ",", "was", "so", "oft", "auf", "der", "Wan\u00b7ge", "die", "Scham", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "PPER"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "K\u00fcndete, was in der Brust sch\u00fcchtern die Scham mir verschwieg.", "tokens": ["K\u00fcn\u00b7de\u00b7te", ",", "was", "in", "der", "Brust", "sch\u00fcch\u00b7tern", "die", "Scham", "mir", "ver\u00b7schwieg", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Aber entflieh, zu reizender Wahn! schweig, t\u00e4uschende Hoffnung!", "tokens": ["A\u00b7ber", "ent\u00b7flieh", ",", "zu", "rei\u00b7zen\u00b7der", "Wahn", "!", "schweig", ",", "t\u00e4u\u00b7schen\u00b7de", "Hoff\u00b7nung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$.", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Zeigte sie gestern nicht noch, da\u00df sie mich nimmer geliebt?", "tokens": ["Zeig\u00b7te", "sie", "ge\u00b7stern", "nicht", "noch", ",", "da\u00df", "sie", "mich", "nim\u00b7mer", "ge\u00b7liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+---+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Leise betrat und leicht sie des Gartens falbe Geb\u00fcsche,", "tokens": ["Lei\u00b7se", "be\u00b7trat", "und", "leicht", "sie", "des", "Gar\u00b7tens", "fal\u00b7be", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Nahete still, und es schlug laut mir im Busen das Herz.", "tokens": ["Na\u00b7he\u00b7te", "still", ",", "und", "es", "schlug", "laut", "mir", "im", "Bu\u00b7sen", "das", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Ach, da stahl sich ein Andrer hinzu, und, wehe, sie gr\u00fc\u00dft' ihn,", "tokens": ["Ach", ",", "da", "stahl", "sich", "ein", "A\u00b7ndrer", "hin\u00b7zu", ",", "und", ",", "we\u00b7he", ",", "sie", "gr\u00fc\u00dft'", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,", "KON", "$,", "ADJD", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Freundlich und schmiegte sich schnell an den gebotenen Arm.", "tokens": ["Freund\u00b7lich", "und", "schmieg\u00b7te", "sich", "schnell", "an", "den", "ge\u00b7bo\u00b7te\u00b7nen", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Einig wallten sie nun, versenkt in leises Gefl\u00fcster:", "tokens": ["Ei\u00b7nig", "wall\u00b7ten", "sie", "nun", ",", "ver\u00b7senkt", "in", "lei\u00b7ses", "Ge\u00b7fl\u00fcs\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Doch mir kochte die Brust hei\u00dfer im siedenden Zorn.", "tokens": ["Doch", "mir", "koch\u00b7te", "die", "Brust", "hei\u00b7\u00dfer", "im", "sie\u00b7den\u00b7den", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "APPRART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Ha, ich sah es zu gut, Verr\u00e4therin, wie er die Hand dir", "tokens": ["Ha", ",", "ich", "sah", "es", "zu", "gut", ",", "Ver\u00b7r\u00e4\u00b7the\u00b7rin", ",", "wie", "er", "die", "Hand", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$,", "NN", "$,", "PWAV", "PPER", "ART", "NN", "PPER"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Dr\u00fcckt' und den kosenden Mund traulich zum Ohre dir bog!", "tokens": ["Dr\u00fcckt'", "und", "den", "ko\u00b7sen\u00b7den", "Mund", "trau\u00b7lich", "zum", "Oh\u00b7re", "dir", "bog", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Geh nur, ich liebe dich nicht! Ich glaub' ich liebte dich nimmer;", "tokens": ["Geh", "nur", ",", "ich", "lie\u00b7be", "dich", "nicht", "!", "Ich", "glaub'", "ich", "lieb\u00b7te", "dich", "nim\u00b7mer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Stets noch fand ich dich falsch, wenn du am z\u00e4rtlichsten schienst.", "tokens": ["Stets", "noch", "fand", "ich", "dich", "falsch", ",", "wenn", "du", "am", "z\u00e4rt\u00b7lichs\u00b7ten", "schienst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,", "KOUS", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Mich nur wolltest du fesseln und nicht dich selber, nur Herrschsucht", "tokens": ["Mich", "nur", "woll\u00b7test", "du", "fes\u00b7seln", "und", "nicht", "dich", "sel\u00b7ber", ",", "nur", "Herrschsucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "VVINF", "KON", "PTKNEG", "PPER", "ADV", "$,", "ADV", "NN"], "meter": "--+--+-++-+--+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Leitete dich, des Gef\u00fchls spottete listig die Kunst.", "tokens": ["Lei\u00b7te\u00b7te", "dich", ",", "des", "Ge\u00b7f\u00fchls", "spot\u00b7te\u00b7te", "lis\u00b7tig", "die", "Kunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.25": {"text": "H\u00e4tte dich weniger doch die Natur holdselig gebildet,", "tokens": ["H\u00e4t\u00b7te", "dich", "we\u00b7ni\u00b7ger", "doch", "die", "Na\u00b7tur", "hold\u00b7se\u00b7lig", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+--+-+-", "measure": "iambic.septa.invert"}, "line.26": {"text": "H\u00e4tte sie minderen Reiz doch in den strahlenden Blick,", "tokens": ["H\u00e4t\u00b7te", "sie", "min\u00b7de\u00b7ren", "Reiz", "doch", "in", "den", "strah\u00b7len\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Mindere Zaubergewalt in das schmeichelnde Wort der Beth\u00f6rung,", "tokens": ["Min\u00b7de\u00b7re", "Zau\u00b7ber\u00b7ge\u00b7walt", "in", "das", "schmei\u00b7cheln\u00b7de", "Wort", "der", "Be\u00b7th\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Mindere Grazie doch dir in die Seele gelegt!", "tokens": ["Min\u00b7de\u00b7re", "Gra\u00b7zie", "doch", "dir", "in", "die", "See\u00b7le", "ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.29": {"text": "Oeffne die Th\u00fcre mir nur; schnell ist dir Alles verziehen:", "tokens": ["Oeff\u00b7ne", "die", "Th\u00fc\u00b7re", "mir", "nur", ";", "schnell", "ist", "dir", "Al\u00b7les", "ver\u00b7zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$.", "ADJD", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.30": {"text": "S\u00fcndigen darfst du so oft, als du zu k\u00fcssen vermagst!", "tokens": ["S\u00fcn\u00b7di\u00b7gen", "darfst", "du", "so", "oft", ",", "als", "du", "zu", "k\u00fcs\u00b7sen", "ver\u00b7magst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}