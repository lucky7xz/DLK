{"textgrid.poem.66793": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Pump von Pumpsack ist \u2013 auf Taille! \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Pump von Pumpsack ist \u2013 auf Taille! \u2013", "tokens": ["Herr", "Pump", "von", "Pum\u00b7psack", "ist", "\u2013", "auf", "Tail\u00b7le", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "APPR", "NN", "VAFIN", "$(", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein eleganter Offizier,", "tokens": ["Ein", "e\u00b7leg\u00b7an\u00b7ter", "Of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verachtet g\u00e4nzlich die Kanaille", "tokens": ["Ver\u00b7ach\u00b7tet", "g\u00e4nz\u00b7lich", "die", "Ka\u00b7nail\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sauerkraut und Lagerbier.", "tokens": ["Von", "Sau\u00b7er\u00b7kraut", "und", "La\u00b7ger\u00b7bier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sind auch gef\u00e4lscht nur seine Waden,", "tokens": ["Sind", "auch", "ge\u00b7f\u00e4lscht", "nur", "sei\u00b7ne", "Wa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4r' ihm P\u00f6belkost gesund,", "tokens": ["Und", "w\u00e4r'", "ihm", "P\u00f6\u00b7bel\u00b7kost", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er l\u00e4\u00dft sich zu Champagner laden,", "tokens": ["Er", "l\u00e4\u00dft", "sich", "zu", "Cham\u00b7pag\u00b7ner", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zugleich ein Leutnant und ein Hund.", "tokens": ["Zu\u00b7gleich", "ein", "Leut\u00b7nant", "und", "ein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Wie schneidig wei\u00df sich einzuf\u00fchren", "tokens": ["Wie", "schnei\u00b7dig", "wei\u00df", "sich", "ein\u00b7zu\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Pumpsack bei der noblen Welt!", "tokens": ["Von", "Pum\u00b7psack", "bei", "der", "nob\u00b7len", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor milit\u00e4rischen All\u00fcren", "tokens": ["Vor", "mi\u00b7li\u00b7t\u00e4\u00b7ri\u00b7schen", "Al\u00b7l\u00fc\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wird windelweich das h\u00e4rtste Geld.", "tokens": ["Wird", "win\u00b7del\u00b7weich", "das", "h\u00e4rts\u00b7te", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kommerzienrats sind ganz ersch\u00fcttert,", "tokens": ["Kom\u00b7mer\u00b7zi\u00b7en\u00b7rats", "sind", "ganz", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zu engagieren solchen Bund,", "tokens": ["Zu", "en\u00b7ga\u00b7gie\u00b7ren", "sol\u00b7chen", "Bund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Herr Pump von Pumpsack wird gef\u00fcttert,", "tokens": ["Herr", "Pump", "von", "Pum\u00b7psack", "wird", "ge\u00b7f\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zugleich ein Leutnant und ein Hund.", "tokens": ["Zu\u00b7gleich", "ein", "Leut\u00b7nant", "und", "ein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Ein Pump von Pumpsack auf dem Balle \u2013", "tokens": ["Ein", "Pump", "von", "Pum\u00b7psack", "auf", "dem", "Bal\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Fr\u00e4ulein Gretchen Kotillon \u2013", "tokens": ["Mit", "Fr\u00e4u\u00b7lein", "Gret\u00b7chen", "Ko\u00b7til\u00b7lon", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lohnt sich schon in jedem Falle", "tokens": ["Da", "lohnt", "sich", "schon", "in", "je\u00b7dem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die exquisiteste Bouillon.", "tokens": ["Die", "ex\u00b7qui\u00b7si\u00b7tes\u00b7te", "Bou\u00b7il\u00b7lon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schon macht der Premier Attacke,", "tokens": ["Schon", "macht", "der", "Pre\u00b7mier", "At\u00b7ta\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Kein Dolch sticht ihm die Seele wund,", "tokens": ["Kein", "Dolch", "sticht", "ihm", "die", "See\u00b7le", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er siegt auf seinem Bettelsacke,", "tokens": ["Er", "siegt", "auf", "sei\u00b7nem", "Bet\u00b7tel\u00b7sa\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zugleich ein Leutnant und ein Hund.", "tokens": ["Zu\u00b7gleich", "ein", "Leut\u00b7nant", "und", "ein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Pump findet Gretchen etwas schlapp zwar,", "tokens": ["Pump", "fin\u00b7det", "Gret\u00b7chen", "et\u00b7was", "schlapp", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Apathisch leider zum Exze\u00df,", "tokens": ["A\u00b7pat\u00b7hisch", "lei\u00b7der", "zum", "Ex\u00b7ze\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihr Herz kommt nimmermehr in Trab zwar,", "tokens": ["Ihr", "Herz", "kommt", "nim\u00b7mer\u00b7mehr", "in", "Trab", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschweige in Galopp ... indes", "tokens": ["Ge\u00b7schwei\u00b7ge", "in", "Ga\u00b7lopp", "...", "in\u00b7des"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "NE", "$(", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Herr Pump von Pumpsack ist zufrieden", "tokens": ["Herr", "Pump", "von", "Pum\u00b7psack", "ist", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem realen Hintergrund,", "tokens": ["Mit", "dem", "re\u00b7a\u00b7len", "Hin\u00b7ter\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den ihm sein Achselstern beschieden \u2013", "tokens": ["Den", "ihm", "sein", "Ach\u00b7sels\u00b7tern", "be\u00b7schie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Zugleich ein Leutnant und ein Hund.", "tokens": ["Zu\u00b7gleich", "ein", "Leut\u00b7nant", "und", "ein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Der liebe Gott ist heute praktisch", "tokens": ["Der", "lie\u00b7be", "Gott", "ist", "heu\u00b7te", "prak\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcgt famos sich in die Zeit,", "tokens": ["Und", "f\u00fcgt", "fa\u00b7mos", "sich", "in", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Pumpsackens Ehe ist nun faktisch", "tokens": ["Pum\u00b7psa\u00b7ckens", "E\u00b7he", "ist", "nun", "fak\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Namen der Dreifaltigkeit.", "tokens": ["Im", "Na\u00b7men", "der", "Drei\u00b7fal\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "\u00bbmein Sohn ", "tokens": ["\u00bb", "mein", "Sohn"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Prahlt Frau Kommerzienr\u00e4tin Schund,", "tokens": ["Prahlt", "Frau", "Kom\u00b7mer\u00b7zi\u00b7en\u00b7r\u00e4\u00b7tin", "Schund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Herr Pump von Pumpsack \u00fcbt Ballettsport,", "tokens": ["Herr", "Pump", "von", "Pum\u00b7psack", "\u00fcbt", "Bal\u00b7letts\u00b7port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Zugleich ein Leutnant und ein Hund.", "tokens": ["Zu\u00b7gleich", "ein", "Leut\u00b7nant", "und", "ein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}}}}