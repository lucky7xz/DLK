{"dta.poem.9824": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wider diejenigen/ die immer wider den  \n Adam schreyen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeiht dem Adam/ ihr ver\u00e4chter/", "tokens": ["Ver\u00b7zeiht", "dem", "A\u00b7dam", "/", "ihr", "ver\u00b7\u00e4ch\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Even er gefolget hat.", "tokens": ["Da\u00df", "E\u00b7ven", "er", "ge\u00b7fol\u00b7get", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn was er f\u00fcr die mutter that/", "tokens": ["Denn", "was", "er", "f\u00fcr", "die", "mut\u00b7ter", "that", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das thun wir t\u00e4glich f\u00fcr die t\u00f6chter.", "tokens": ["Das", "thun", "wir", "t\u00e4g\u00b7lich", "f\u00fcr", "die", "t\u00f6ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}