rd", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er schwankt' und glitt im rauchenden Blut.", "tokens": ["Er", "schwankt'", "und", "glitt", "im", "rau\u00b7chen\u00b7den", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Noch einmal hub er sein bla\u00df Gesicht:", "tokens": ["Noch", "ein\u00b7mal", "hub", "er", "sein", "bla\u00df", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbherr Gott im Himmel, du h\u00e4ltst Gericht!", "tokens": ["\u00bb", "herr", "Gott", "im", "Him\u00b7mel", ",", "du", "h\u00e4ltst", "Ge\u00b7richt", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "Wohl um das Gold erschluget ihr mich;", "tokens": ["Wohl", "um", "das", "Gold", "er\u00b7schlu\u00b7get", "ihr", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Weh euch! Ihr seid verloren wie ich.", "tokens": ["Weh", "euch", "!", "Ihr", "seid", "ver\u00b7lo\u00b7ren", "wie", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "VVPP", "KOKOM", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Auch ich, ich wollte den Schatz allein", "tokens": ["Auch", "ich", ",", "ich", "woll\u00b7te", "den", "Schatz", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPER", "VMFIN", "ART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und mischt' euch t\u00f6dliches Gift an den Wein.\u00ab", "tokens": ["Und", "mischt'", "euch", "t\u00f6d\u00b7li\u00b7ches", "Gift", "an", "den", "Wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}