{"textgrid.poem.61885": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die Goldgr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie waren gezogen \u00fcber das Meer,", "tokens": ["Sie", "wa\u00b7ren", "ge\u00b7zo\u00b7gen", "\u00fc\u00b7ber", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach Gl\u00fcck und Gold stand ihr Begehr,", "tokens": ["Nach", "Gl\u00fcck", "und", "Gold", "stand", "ihr", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drei wilde Gesellen, vom Wetter gebr\u00e4unt,", "tokens": ["Drei", "wil\u00b7de", "Ge\u00b7sel\u00b7len", ",", "vom", "Wet\u00b7ter", "ge\u00b7br\u00e4unt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und kannten sich wohl und waren sich freund.", "tokens": ["Und", "kann\u00b7ten", "sich", "wohl", "und", "wa\u00b7ren", "sich", "freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "KON", "VAFIN", "PRF", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie hatten gegraben Tag und Nacht,", "tokens": ["Sie", "hat\u00b7ten", "ge\u00b7gra\u00b7ben", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Flusse die Grube, im Berge den Schacht,", "tokens": ["Am", "Flus\u00b7se", "die", "Gru\u00b7be", ",", "im", "Ber\u00b7ge", "den", "Schacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "In Sonnengluten und Regengebraus", "tokens": ["In", "Son\u00b7nen\u00b7glu\u00b7ten", "und", "Re\u00b7gen\u00b7ge\u00b7braus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei Durst und Hunger hielten sie aus.", "tokens": ["Bei", "Durst", "und", "Hun\u00b7ger", "hiel\u00b7ten", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und endlich, endlich, nach Monden voll Schwei\u00df,", "tokens": ["Und", "end\u00b7lich", ",", "end\u00b7lich", ",", "nach", "Mon\u00b7den", "voll", "Schwei\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "APPR", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sahn aus der Tiefe sie winken den Preis,", "tokens": ["Da", "sahn", "aus", "der", "Tie\u00b7fe", "sie", "win\u00b7ken", "den", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da gl\u00fcht es sie an durch das Dunkel so hold,", "tokens": ["Da", "gl\u00fcht", "es", "sie", "an", "durch", "das", "Dun\u00b7kel", "so", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit Blicken der Schlange, das feurige Gold.", "tokens": ["Mit", "Bli\u00b7cken", "der", "Schlan\u00b7ge", ",", "das", "feu\u00b7ri\u00b7ge", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Sie brachen es los aus dem finsteren Raum,", "tokens": ["Sie", "bra\u00b7chen", "es", "los", "aus", "dem", "fins\u00b7te\u00b7ren", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und als sie's fa\u00dften, sie hoben es kaum,", "tokens": ["Und", "als", "sie's", "fa\u00df\u00b7ten", ",", "sie", "ho\u00b7ben", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und als sie's wogen, sie jauchzten zugleich:", "tokens": ["Und", "als", "sie's", "wo\u00b7gen", ",", "sie", "jauchz\u00b7ten", "zu\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbnun sind wir geborgen, nun sind wir reich!\u00ab", "tokens": ["\u00bb", "nun", "sind", "wir", "ge\u00b7bor\u00b7gen", ",", "nun", "sind", "wir", "reich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie lachten und kreischten mit jubelndem Schall,", "tokens": ["Sie", "lach\u00b7ten", "und", "kreischten", "mit", "ju\u00b7beln\u00b7dem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie tanzten im Kreis um das blanke Metall,", "tokens": ["Sie", "tanz\u00b7ten", "im", "Kreis", "um", "das", "blan\u00b7ke", "Me\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und h\u00e4tte der Stolz nicht bez\u00e4hmt ihr Gel\u00fcst,", "tokens": ["Und", "h\u00e4t\u00b7te", "der", "Stolz", "nicht", "be\u00b7z\u00e4hmt", "ihr", "Ge\u00b7l\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie h\u00e4tten's mit br\u00fcnstiger Lippe gek\u00fc\u00dft.", "tokens": ["Sie", "h\u00e4t\u00b7ten's", "mit", "br\u00fcns\u00b7ti\u00b7ger", "Lip\u00b7pe", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Sprach Tom, der J\u00e4ger: \u00bbNun la\u00dft uns ruhn!", "tokens": ["Sprach", "Tom", ",", "der", "J\u00e4\u00b7ger", ":", "\u00bb", "Nun", "la\u00dft", "uns", "ruhn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zeit ist's, auf das M\u00fchsal uns g\u00fctlich zu tun.", "tokens": ["Zeit", "ist's", ",", "auf", "das", "M\u00fch\u00b7sal", "uns", "g\u00fct\u00b7lich", "zu", "tun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPR", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Geh, Sam, und hol' uns Speisen und Wein,", "tokens": ["Geh", ",", "Sam", ",", "und", "hol'", "uns", "Spei\u00b7sen", "und", "Wein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ein lustiges Fest mu\u00df gefeiert sein.\u00ab", "tokens": ["Ein", "lus\u00b7ti\u00b7ges", "Fest", "mu\u00df", "ge\u00b7fei\u00b7ert", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Wie trunken schlenderte Sam dahin", "tokens": ["Wie", "trun\u00b7ken", "schlen\u00b7der\u00b7te", "Sam", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "NE", "PAV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Flecken hinab mit verzaubertem Sinn;", "tokens": ["Zum", "Fle\u00b7cken", "hin\u00b7ab", "mit", "ver\u00b7zau\u00b7ber\u00b7tem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sein Haupt umnebelnd beschlichen ihn sacht", "tokens": ["Sein", "Haupt", "um\u00b7ne\u00b7belnd", "be\u00b7schli\u00b7chen", "ihn", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gedanken, wie er sie nimmer gedacht.", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "wie", "er", "sie", "nim\u00b7mer", "ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die andern sa\u00dfen am Bergeshang,", "tokens": ["Die", "an\u00b7dern", "sa\u00b7\u00dfen", "am", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie pr\u00fcften das Erz, und es blitzt' und es klang.", "tokens": ["Sie", "pr\u00fcf\u00b7ten", "das", "Erz", ",", "und", "es", "blitzt'", "und", "es", "klang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sprach Will, der Rote: \u00bbDas Gold ist fein;", "tokens": ["Sprach", "Will", ",", "der", "Ro\u00b7te", ":", "\u00bb", "Das", "Gold", "ist", "fein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ART", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur schade, da\u00df wir es teilen zu drein!\u00ab", "tokens": ["Nur", "scha\u00b7de", ",", "da\u00df", "wir", "es", "tei\u00b7len", "zu", "drein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVINF", "PTKZU", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbdu meinst?\u00ab \u2013 \u00bbJe nun, ich meine nur so.", "tokens": ["\u00bb", "du", "meinst", "?", "\u00ab", "\u2013", "\u00bb", "Je", "nun", ",", "ich", "mei\u00b7ne", "nur", "so", "."], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "$.", "$(", "$(", "$(", "ADV", "ADV", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwei w\u00fcrden des Schatzes besser froh\u00ab \u2013", "tokens": ["Zwei", "w\u00fcr\u00b7den", "des", "Schat\u00b7zes", "bes\u00b7ser", "froh", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbdoch wenn \u2013\u00ab \u2013 \u00bbWenn was?\u00ab \u2013 \u00bbNun, nehmen wir an,", "tokens": ["\u00bb", "doch", "wenn", "\u2013", "\u00ab", "\u2013", "\u00bb", "Wenn", "was", "?", "\u00ab", "\u2013", "\u00bb", "Nun", ",", "neh\u00b7men", "wir", "an", ","], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "$(", "$(", "$(", "$(", "KOUS", "PIS", "$.", "$(", "$(", "$(", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sam w\u00e4re nicht da\u00ab \u2013 \u00bbJa, freilich, dann \u2013 \u2013\u00ab", "tokens": ["Sam", "w\u00e4\u00b7re", "nicht", "da", "\u00ab", "\u2013", "\u00bb", "Ja", ",", "frei\u00b7lich", ",", "dann", "\u2013", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "$(", "$(", "$(", "PTKANT", "$,", "ADV", "$,", "ADV", "$(", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Sie schwiegen lang; die Sonne glomm", "tokens": ["Sie", "schwie\u00b7gen", "lang", ";", "die", "Son\u00b7ne", "glomm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und glei\u00dft' um das Gold; da murmelte Tom:", "tokens": ["Und", "glei\u00dft'", "um", "das", "Gold", ";", "da", "mur\u00b7mel\u00b7te", "Tom", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbsiehst du die Schlucht dort unten?\u00ab \u2013 \u00bbWarum?\u00ab \u2013", "tokens": ["\u00bb", "siehst", "du", "die", "Schlucht", "dort", "un\u00b7ten", "?", "\u00ab", "\u2013", "\u00bb", "Wa\u00b7rum", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$.", "$(", "$(", "$(", "PWAV", "$.", "$(", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00bbihr Schatten ist tief, und die Felsen sind stumm.\u00ab \u2013", "tokens": ["\u00bb", "ihr", "Schat\u00b7ten", "ist", "tief", ",", "und", "die", "Fel\u00b7sen", "sind", "stumm", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$.", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbversteh' ich dich recht?\u00ab \u2013 \u00bbWas fragst du noch viel!", "tokens": ["\u00bb", "ver\u00b7steh'", "ich", "dich", "recht", "?", "\u00ab", "\u2013", "\u00bb", "Was", "fragst", "du", "noch", "viel", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PRF", "ADJD", "$.", "$(", "$(", "$(", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir dachten es beide und f\u00fchren's ans Ziel.", "tokens": ["Wir", "dach\u00b7ten", "es", "bei\u00b7de", "und", "f\u00fch\u00b7ren's", "ans", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein t\u00fcchtiger Sto\u00df und ein Grab im Gestein,", "tokens": ["Ein", "t\u00fcch\u00b7ti\u00b7ger", "Sto\u00df", "und", "ein", "Grab", "im", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So ist es getan, und wir teilen allein.\u00ab", "tokens": ["So", "ist", "es", "ge\u00b7tan", ",", "und", "wir", "tei\u00b7len", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "KON", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Sie schwiegen aufs neu'. Es vergl\u00fchte der Tag,", "tokens": ["Sie", "schwie\u00b7gen", "aufs", "neu'", ".", "Es", "ver\u00b7gl\u00fch\u00b7te", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie Blut auf dem Golde das Sp\u00e4trot lag;", "tokens": ["Wie", "Blut", "auf", "dem", "Gol\u00b7de", "das", "Sp\u00e4t\u00b7rot", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da kam er zur\u00fcck, ihr junger Geno\u00df,", "tokens": ["Da", "kam", "er", "zu\u00b7r\u00fcck", ",", "ihr", "jun\u00b7ger", "Ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von bleicher Stirne der Schwei\u00df ihm f