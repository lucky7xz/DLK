{"textgrid.poem.40458": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gemeinde Gabelbach", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stolz ragt aus den Th\u00fcringer Tannen", "tokens": ["Stolz", "ragt", "aus", "den", "Th\u00fc\u00b7rin\u00b7ger", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Kikelhahns Haupt in das Land,", "tokens": ["Des", "Ki\u00b7kel\u00b7hahns", "Haupt", "in", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Von Goethe in klassischem Griechisch", "tokens": ["Von", "Goe\u00b7the", "in", "klas\u00b7si\u00b7schem", "Grie\u00b7chisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Vom Wald, wo das Lied er gesungen:", "tokens": ["Vom", "Wald", ",", "wo", "das", "Lied", "er", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u00bbob allen Wipfeln ist Ruh'\u00ab,", "tokens": ["\u00bb", "ob", "al\u00b7len", "Wip\u00b7feln", "ist", "Ruh'", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PIAT", "NN", "VAFIN", "NN", "$(", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da schaut eine kleine Gemeinde", "tokens": ["Da", "schaut", "ei\u00b7ne", "klei\u00b7ne", "Ge\u00b7mein\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Stillfriedlich den Welth\u00e4ndeln zu.", "tokens": ["Still\u00b7fried\u00b7lich", "den", "Welt\u00b7h\u00e4n\u00b7deln", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und f\u00e4rbt die gerodete Stelle", "tokens": ["Und", "f\u00e4rbt", "die", "ge\u00b7ro\u00b7de\u00b7te", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sich abendgoldsonnig und klar,", "tokens": ["Sich", "a\u00b7bend\u00b7gold\u00b7son\u00b7nig", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sitzen sie all' an der Quelle", "tokens": ["Da", "sit\u00b7zen", "sie", "all'", "an", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringen ein Rauchopfer dar.", "tokens": ["Und", "brin\u00b7gen", "ein", "Rau\u00b7ch\u00b7op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit Zweifeln und Sorgen sich qu\u00e4len", "tokens": ["Mit", "Zwei\u00b7feln", "und", "Sor\u00b7gen", "sich", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hat dortlands gr\u00fcndlich ein End',", "tokens": ["Hat", "dort\u00b7lands", "gr\u00fcnd\u00b7lich", "ein", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Treu sorgt f\u00fcr das Heil seiner Seelen", "tokens": ["Treu", "sorgt", "f\u00fcr", "das", "Heil", "sei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein trefflicher Sup'rintendent.", "tokens": ["Ein", "treff\u00b7li\u00b7cher", "Sup'\u00b7rin\u00b7ten\u00b7dent", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gerechtigkeit liebt man und \u00fcbt man,", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "liebt", "man", "und", "\u00fcbt", "man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "KON", "VVFIN", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sp\u00e4t nur verl\u00e4\u00dft ihren Sitz,", "tokens": ["Und", "sp\u00e4t", "nur", "ver\u00b7l\u00e4\u00dft", "ih\u00b7ren", "Sitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wenn sie ankam, Tagfahrt zu halten,", "tokens": ["Wenn", "sie", "an\u00b7kam", ",", "Tag\u00b7fahrt", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Ilmenauer Justiz.", "tokens": ["Die", "Il\u00b7me\u00b7nau\u00b7er", "Jus\u00b7tiz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Es werden gediegener Schulzen", "tokens": ["Es", "wer\u00b7den", "ge\u00b7die\u00b7ge\u00b7ner", "Schul\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verdienste, wie selten, geehrt;", "tokens": ["Ver\u00b7diens\u00b7te", ",", "wie", "sel\u00b7ten", ",", "ge\u00b7ehrt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "$,", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Steindenkm\u00e4ler zeigen dem Wandrer,", "tokens": ["Stein\u00b7denk\u00b7m\u00e4\u00b7ler", "zei\u00b7gen", "dem", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie hoch der Gemeinde sie wert.", "tokens": ["Wie", "hoch", "der", "Ge\u00b7mein\u00b7de", "sie", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Die Stra\u00dfen sind bestens in Ordnung,", "tokens": ["Die", "Stra\u00b7\u00dfen", "sind", "bes\u00b7tens", "in", "Ord\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Beschirmt von der Berggeister Huld,", "tokens": ["Be\u00b7schirmt", "von", "der", "Berg\u00b7geis\u00b7ter", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wer auf den Holzwegen stolpert,", "tokens": ["Und", "wer", "auf", "den", "Holz\u00b7we\u00b7gen", "stol\u00b7pert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ist meistenteils selber dran schuld.", "tokens": ["Ist", "meis\u00b7ten\u00b7teils", "sel\u00b7ber", "dran", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PAV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbder w\u00fcrzigste Harzduft der Tannen", "tokens": ["\u00bb", "der", "w\u00fcr\u00b7zigs\u00b7te", "Harz\u00b7duft", "der", "Tan\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erquicke die sangfrohe Schar;", "tokens": ["Er\u00b7qui\u00b7cke", "die", "sang\u00b7fro\u00b7he", "Schar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Goethischem Bergbau vermelde", "tokens": ["Von", "Goe\u00b7thi\u00b7schem", "Berg\u00b7bau", "ver\u00b7mel\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Recht viel noch der Bergmeister Mahr.", "tokens": ["Recht", "viel", "noch", "der", "Berg\u00b7meis\u00b7ter", "Mahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Dem Butterbrot stets sich geselle", "tokens": ["Dem", "But\u00b7ter\u00b7brot", "stets", "sich", "ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Belag von ger\u00e4ucherter Wurst,", "tokens": ["Be\u00b7lag", "von", "ge\u00b7r\u00e4u\u00b7cher\u00b7ter", "Wurst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Niemalen versiege die Quelle", "tokens": ["Nie\u00b7ma\u00b7len", "ver\u00b7sie\u00b7ge", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und nie der gemeindliche Durst:", "tokens": ["Und", "nie", "der", "ge\u00b7meind\u00b7li\u00b7che", "Durst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Dies w\u00fcnscht seinen Th\u00fcringer Freunden", "tokens": ["Dies", "w\u00fcnscht", "sei\u00b7nen", "Th\u00fc\u00b7rin\u00b7ger", "Freun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der ferne Gemeindepoet,", "tokens": ["Der", "fer\u00b7ne", "Ge\u00b7mein\u00b7de\u00b7poet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den zweimal im Gabelbachhause", "tokens": ["Den", "zwei\u00b7mal", "im", "Ga\u00b7bel\u00b7bach\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Kikelhahn sanft angekr\u00e4ht.\u00ab", "tokens": ["Der", "Ki\u00b7kel\u00b7hahn", "sanft", "an\u00b7ge\u00b7kr\u00e4ht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}