}, "line.9": {"text": "Ein Gl\u00fcck, wer ", "tokens": ["Ein", "Gl\u00fcck", ",", "wer"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PWS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Wie doch im Grund er sollte,", "tokens": ["Wie", "doch", "im", "Grund", "er", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Den letzten Tag ersehnen seiner Lieben,", "tokens": ["Den", "letz\u00b7ten", "Tag", "er\u00b7seh\u00b7nen", "sei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Um dann, zur\u00fcckgeblieben", "tokens": ["Um", "dann", ",", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUI", "ADV", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Arm und beraubt, zu sehen,", "tokens": ["Arm", "und", "be\u00b7raubt", ",", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.14": {"text": "Wie von der Schwelle das geliebte Wesen", "tokens": ["Wie", "von", "der", "Schwel\u00b7le", "das", "ge\u00b7lieb\u00b7te", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Von hinnen wird getragen,", "tokens": ["Von", "hin\u00b7nen", "wird", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Mit dem vereint er lebte manches Jahr,", "tokens": ["Mit", "dem", "ver\u00b7eint", "er", "leb\u00b7te", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ade ihm sagen, jeder Hoffnung baar,", "tokens": ["A\u00b7de", "ihm", "sa\u00b7gen", ",", "je\u00b7der", "Hoff\u00b7nung", "baar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Ihm wieder zu begegnen", "tokens": ["Ihm", "wie\u00b7der", "zu", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "In dieser ird'schen Welt;", "tokens": ["In", "die\u00b7ser", "ird'\u00b7schen", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Und dann, auf Erden einsam und verlassen", "tokens": ["Und", "dann", ",", "auf", "Er\u00b7den", "ein\u00b7sam", "und", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Umblickend, in gewohnter Stund' und St\u00e4tte", "tokens": ["Um\u00b7bli\u00b7ckend", ",", "in", "ge\u00b7wohn\u00b7ter", "Stund'", "und", "St\u00e4t\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Zu denken Dessen, dem er einst gesellt?", "tokens": ["Zu", "den\u00b7ken", "Des\u00b7sen", ",", "dem", "er", "einst", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMINF", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wie, o Natur, wie bringst du's \u00fcbers Herz,", "tokens": ["Wie", ",", "o", "Na\u00b7tur", ",", "wie", "bringst", "du's", "\u00fc\u00b7bers", "Herz", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "NN", "$,", "PWAV", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Grausam hinwegzurei\u00dfen", "tokens": ["Grau\u00b7sam", "hin\u00b7weg\u00b7zu\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.25": {"text": "Den Freund aus Freundesarmen,", "tokens": ["Den", "Freund", "aus", "Freun\u00b7des\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Geschwister von Geschwistern,", "tokens": ["Ge\u00b7schwis\u00b7ter", "von", "Ge\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Die Kinder von den Eltern,", "tokens": ["Die", "Kin\u00b7der", "von", "den", "El\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Sein Lieb vom Liebenden, da\u00df Eins erlischt", "tokens": ["Sein", "Lieb", "vom", "Lie\u00b7ben\u00b7den", ",", "da\u00df", "Eins", "er\u00b7lischt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und weiter lebt das Andre? Mu\u00dftest du", "tokens": ["Und", "wei\u00b7ter", "lebt", "das", "And\u00b7re", "?", "Mu\u00df\u00b7test", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "PIS", "$.", "VMFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Zum Leiden und zum Lieben", "tokens": ["Zum", "Lei\u00b7den", "und", "zum", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Die Kraft uns leihn, da\u00df, was wir hei\u00df geliebt,", "tokens": ["Die", "Kraft", "uns", "leihn", ",", "da\u00df", ",", "was", "wir", "hei\u00df", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "KOUS", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Wir \u00fcberleben? Doch Natur von je", "tokens": ["Wir", "\u00fc\u00b7berl\u00b7e\u00b7ben", "?", "Doch", "Na\u00b7tur", "von", "je"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Gehorchte andern Trieben,", "tokens": ["Ge\u00b7horch\u00b7te", "an\u00b7dern", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Und wenig gilt i