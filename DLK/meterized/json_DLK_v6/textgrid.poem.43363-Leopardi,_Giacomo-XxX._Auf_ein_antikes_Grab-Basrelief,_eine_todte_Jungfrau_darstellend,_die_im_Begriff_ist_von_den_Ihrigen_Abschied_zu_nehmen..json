pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Mit schwarzem Flor umkleiden,", "tokens": ["Mit", "schwar\u00b7zem", "Flor", "um\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Mit Grau'n ihn so umgeben,", "tokens": ["Mit", "Grau'n", "ihn", "so", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Da\u00df uns mit Furcht und Beben", "tokens": ["Da\u00df", "uns", "mit", "Furcht", "und", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Mehr als die Brandung schreckt der sichre Hafen?", "tokens": ["Mehr", "als", "die", "Bran\u00b7dung", "schreckt", "der", "sich\u00b7re", "Ha\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zwar, wenn dies bittre Sterben", "tokens": ["Zwar", ",", "wenn", "dies", "bitt\u00b7re", "Ster\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Loos ist, das du Allen", "tokens": ["Ein", "Loos", "ist", ",", "das", "du", "Al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00e4ngt, die ohne Wissen du und Willen",