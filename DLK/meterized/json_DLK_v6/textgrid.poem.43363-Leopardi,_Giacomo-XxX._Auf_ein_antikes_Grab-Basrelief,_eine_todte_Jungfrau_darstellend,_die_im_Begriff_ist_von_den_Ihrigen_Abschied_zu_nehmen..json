{"textgrid.poem.43363": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxX. Auf ein antikes Grab-Basrelief, eine todte Jungfrau darstellend, die im Begriff ist von den Ihrigen Abschied zu nehmen.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo eilst du hin? Wer ruft dich", "tokens": ["Wo", "eilst", "du", "hin", "?", "Wer", "ruft", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinweg von deinen Lieben,", "tokens": ["Hin\u00b7weg", "von", "dei\u00b7nen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du holde M\u00e4dchenblume?", "tokens": ["Du", "hol\u00b7de", "M\u00e4d\u00b7chen\u00b7blu\u00b7me", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Willst du allein dein v\u00e4terliches Haus", "tokens": ["Willst", "du", "al\u00b7lein", "dein", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So fr\u00fch verlassen? Kehrst zu dieser Schwelle", "tokens": ["So", "fr\u00fch", "ver\u00b7las\u00b7sen", "?", "Kehrst", "zu", "die\u00b7ser", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "$.", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du je zur\u00fcck und wird ein Wiedersehen", "tokens": ["Du", "je", "zu\u00b7r\u00fcck", "und", "wird", "ein", "Wie\u00b7der\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKVZ", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erfreun, die heut in Thr\u00e4nen dich umstehen?", "tokens": ["Er\u00b7freun", ",", "die", "heut", "in", "Thr\u00e4\u00b7nen", "dich", "um\u00b7ste\u00b7hen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Aug' ist trocken, muthig die Geberde,", "tokens": ["Dein", "Aug'", "ist", "tro\u00b7cken", ",", "mut\u00b7hig", "die", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dennoch bist du traurig. Ob willkommen,", "tokens": ["Und", "den\u00b7noch", "bist", "du", "trau\u00b7rig", ".", "Ob", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$.", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob unerw\u00fcnscht die Reise dir erschiene,", "tokens": ["Ob", "un\u00b7er\u00b7w\u00fcnscht", "die", "Rei\u00b7se", "dir", "er\u00b7schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob dir das Ziel mi\u00dff\u00e4llt \u2013", "tokens": ["Ob", "dir", "das", "Ziel", "mi\u00df\u00b7f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus deiner ernsten Miene", "tokens": ["Aus", "dei\u00b7ner", "erns\u00b7ten", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verr\u00e4th sich's kaum. Ach, zweifelnd und beklommen", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "sich's", "kaum", ".", "Ach", ",", "zwei\u00b7felnd", "und", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "$.", "ITJ", "$,", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Schwankt mir das Herz, und wohl in aller Welt", "tokens": ["Schwankt", "mir", "das", "Herz", ",", "und", "wohl", "in", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wei\u00df Niemand, ob sich gn\u00e4dig dir der Himmel,", "tokens": ["Wei\u00df", "Nie\u00b7mand", ",", "ob", "sich", "gn\u00e4\u00b7dig", "dir", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PRF", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ob grausam wollt' erweisen,", "tokens": ["Ob", "grau\u00b7sam", "wollt'", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ob man dich soll beklagen oder preisen.", "tokens": ["Ob", "man", "dich", "soll", "be\u00b7kla\u00b7gen", "o\u00b7der", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dich ruft der Tod; schon bei des Tags Beginn", "tokens": ["Dich", "ruft", "der", "Tod", ";", "schon", "bei", "des", "Tags", "Be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die letzte Stunde! Zum verlassnen Neste", "tokens": ["Die", "letz\u00b7te", "Stun\u00b7de", "!", "Zum", "ver\u00b7lass\u00b7nen", "Nes\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kehrst du nicht mehr. F\u00fcr immer", "tokens": ["Kehrst", "du", "nicht", "mehr", ".", "F\u00fcr", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$.", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Musst du die theuren Eltern", "tokens": ["Musst", "du", "die", "theu\u00b7ren", "El\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verlassen. Unterirdisch", "tokens": ["Ver\u00b7las\u00b7sen", ".", "Un\u00b7ter\u00b7ir\u00b7disch"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist deiner Reise Ziel;", "tokens": ["Ist", "dei\u00b7ner", "Rei\u00b7se", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort wirst du nun verweilen f\u00fcrderhin.", "tokens": ["Dort", "wirst", "du", "nun", "ver\u00b7wei\u00b7len", "f\u00fcr\u00b7der\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Gl\u00fcck vielleicht! Und doch, wer still bei sich", "tokens": ["Ein", "Gl\u00fcck", "viel\u00b7leicht", "!", "Und", "doch", ",", "wer", "still", "bei", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "KON", "ADV", "$,", "PWS", "ADJD", "APPR", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dein irdisch Loos betrachtet, seufzt um dich.", "tokens": ["Dein", "ir\u00b7disch", "Loos", "be\u00b7trach\u00b7tet", ",", "seufzt", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Niemals das Licht zu schauen", "tokens": ["Nie\u00b7mals", "das", "Licht", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "War wohl das Beste. Doch einmal geboren,", "tokens": ["War", "wohl", "das", "Bes\u00b7te", ".", "Doch", "ein\u00b7mal", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da Sch\u00f6nheit erst sich k\u00f6niglich entfaltet", "tokens": ["Da", "Sch\u00f6n\u00b7heit", "erst", "sich", "k\u00f6\u00b7nig\u00b7lich", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Wuchs und Angesicht", "tokens": ["In", "Wuchs", "und", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schon die Welt von ferne", "tokens": ["Und", "schon", "die", "Welt", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beginnt sich ihrer jungen Macht zu beugen,", "tokens": ["Be\u00b7ginnt", "sich", "ih\u00b7rer", "jun\u00b7gen", "Macht", "zu", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Beim Aufbl\u00fchn jeder Hoffnung, da noch nicht", "tokens": ["Beim", "Auf\u00b7bl\u00fchn", "je\u00b7der", "Hoff\u00b7nung", ",", "da", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "KOUS", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit d\u00fcstrer Blitze flammender Gewalt", "tokens": ["Mit", "d\u00fcst\u00b7rer", "Blit\u00b7ze", "flam\u00b7men\u00b7der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wahrheit die freudenhelle Stirn getroffen,", "tokens": ["Wahr\u00b7heit", "die", "freu\u00b7den\u00b7hel\u00b7le", "Stirn", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Gleich einem Rauche, der im Tageslicht", "tokens": ["Gleich", "ei\u00b7nem", "Rau\u00b7che", ",", "der", "im", "Ta\u00b7ges\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein windbewegtes W\u00f6lkchen aufw\u00e4rts wallt,", "tokens": ["Ein", "wind\u00b7be\u00b7weg\u00b7tes", "W\u00f6lk\u00b7chen", "auf\u00b7w\u00e4rts", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So, gleich wie nie entstanden, zu verschweben", "tokens": ["So", ",", "gleich", "wie", "nie", "ent\u00b7stan\u00b7den", ",", "zu", "ver\u00b7schwe\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "KOKOM", "ADV", "VVPP", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und k\u00fcnft'ge Lebensf\u00fclle", "tokens": ["Und", "k\u00fcnft'\u00b7ge", "Le\u00b7bens\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Zu tauschen mit des Grabes dunkler Stille,", "tokens": ["Zu", "tau\u00b7schen", "mit", "des", "Gra\u00b7bes", "dunk\u00b7ler", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Das ist's \u2013 mag es dem Geist", "tokens": ["Das", "ist's", "\u2013", "mag", "es", "dem", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$(", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Auch eine Wohlthat scheinen \u2013,", "tokens": ["Auch", "ei\u00b7ne", "Wohlt\u00b7hat", "schei\u00b7nen", "\u2013", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Was auch dem Muthigsten das Herz zerrei\u00dft.", "tokens": ["Was", "auch", "dem", "Mut\u00b7higs\u00b7ten", "das", "Herz", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Mutter, von deinen Kindern", "tokens": ["Mut\u00b7ter", ",", "von", "dei\u00b7nen", "Kin\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Gef\u00fcrchtet, die du fr\u00fch schon weinen lehrst,", "tokens": ["Ge\u00b7f\u00fcrch\u00b7tet", ",", "die", "du", "fr\u00fch", "schon", "wei\u00b7nen", "lehrst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Natur du grause, die du nur geb\u00e4rst", "tokens": ["Na\u00b7tur", "du", "grau\u00b7se", ",", "die", "du", "nur", "ge\u00b7b\u00e4rst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und n\u00e4hrst, um deine eigne Brut zu t\u00f6dten:", "tokens": ["Und", "n\u00e4hrst", ",", "um", "dei\u00b7ne", "eig\u00b7ne", "Brut", "zu", "t\u00f6d\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn Scheiden vor der Zeit", "tokens": ["Wenn", "Schei\u00b7den", "vor", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein \u00dcbel ist, wie kannst du es erw\u00e4hlen", "tokens": ["Ein", "\u00dc\u00b7bel", "ist", ",", "wie", "kannst", "du", "es", "er\u00b7w\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den schuldlos jungen Seelen?", "tokens": ["Den", "schuld\u00b7los", "jun\u00b7gen", "See\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Und ist's ein Gl\u00fcck, warum", "tokens": ["Und", "ist's", "ein", "Gl\u00fcck", ",", "wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mu\u00df als das schwerste Leid", "tokens": ["Mu\u00df", "als", "das", "schwers\u00b7te", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line