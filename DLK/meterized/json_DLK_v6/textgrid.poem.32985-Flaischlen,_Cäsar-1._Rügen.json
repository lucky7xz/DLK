{"textgrid.poem.32985": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1. R\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief und still", "tokens": ["Tief", "und", "still"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "in grauem Regen", "tokens": ["in", "grau\u00b7em", "Re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "liegen Wald und", "tokens": ["lie\u00b7gen", "Wald", "und"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "KON"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "liegen Wiesen ...", "tokens": ["lie\u00b7gen", "Wie\u00b7sen", "..."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "tief und still", "tokens": ["tief", "und", "still"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "mit m\u00fcden, schweren", "tokens": ["mit", "m\u00fc\u00b7den", ",", "schwe\u00b7ren"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "$,", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wellen", "tokens": ["Wel\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "schleppt das Meer zum Strand ...", "tokens": ["schleppt", "das", "Meer", "zum", "Strand", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "graue M\u00f6wen", "tokens": ["grau\u00